      "utterance": [
        {
          "id": "MDRW2100024924.1.1",
          "form": "요즘 비가 너무 자주와요",
          "original_form": "요즘 비가 너무 자주와요",
          "speaker_id": "2",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024924.1.2",
          "form": "오늘 호의주의보네요",
          "original_form": "오늘 호의주의보네요",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024924.1.3",
          "form": "주말에 빨래해서 다행쓰",
          "original_form": "주말에 빨래해서 다행쓰",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024924.1.4",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024924.1.5",
          "form": "빨래 요즘 잘 안 말라서",
          "original_form": "빨래 요즘 잘 안 말라서",
          "speaker_id": "2",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100024924.1.6",
          "form": "입을 옷이 없다죠 ㅎ",
          "original_form": "입을 옷이 없다죠 ㅎ",
          "speaker_id": "2",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100024924.1.7",
          "form": "잘못 말리면 답없고",
          "original_form": "잘못 말리면 답없고",
          "speaker_id": "1",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100024924.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024924.1.9",
          "form": "건조기 사고 싶다가도 돈없어서 포기하고 그렇네요",
          "original_form": "건조기 사고 싶다가도 돈없어서 포기하고 그렇네요",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100024924.1.10",
          "form": "저희집엔 일체형인데 잘못쓰면 옷이 망가져서",
          "original_form": "저희집엔 일체형인데 잘못쓰면 옷이 망가져서",
          "speaker_id": "1",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100024924.1.11",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024924.1.12",
          "form": "저 아시는분 니트 엄청 줄었더라고요",
          "original_form": "저 아시는분 니트 엄청 줄었더라고요",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024924.1.13",
          "form": "아직은 날씨가 더워서 반팔 입고 다녀야겠어요 근데 저녁은 춥고",
          "original_form": "아직은 날씨가 더워서 반팔 입고 다녀야겠어요 근데 저녁은 춥고",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024924.1.14",
          "form": "반팔과 가디건이죠!",
          "original_form": "반팔과 가디건이죠!",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024924.1.15",
          "form": "그렇죠!",
          "original_form": "그렇죠!",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100024924.1.16",
          "form": "어떤 계절 좋아하세요~?",
          "original_form": "어떤 계절 좋아하세요~?",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100024924.1.17",
          "form": "전 겨울이요",
          "original_form": "전 겨울이요",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100024924.1.18",
          "form": "여름은 더워서 땀나고 비도 자주오고",
          "original_form": "여름은 더워서 땀나고 비도 자주오고",
          "speaker_id": "1",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100024924.1.19",
          "form": "별로",
          "original_form": "별로",
          "speaker_id": "1",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100024924.1.20",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100024924.1.21",
          "form": "겨울이랑 봄 좋아해요",
          "original_form": "겨울이랑 봄 좋아해요",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100024924.1.22",
          "form": "겨울 눈 오는거 보는건 좋은데ㅜ",
          "original_form": "겨울 눈 오는거 보는건 좋은데ㅜ",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100024924.1.23",
          "form": "운전하면 막막",
          "original_form": "운전하면 막막",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100024924.1.24",
          "form": "겨울에 노곤노곤함이 좋아요",
          "original_form": "겨울에 노곤노곤함이 좋아요",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100024924.1.25",
          "form": "비오는 날 운전도 최악",
          "original_form": "비오는 날 운전도 최악",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100024924.1.26",
          "form": "맞아요 그 따뜻한 느낌도 좋아요",
          "original_form": "맞아요 그 따뜻한 느낌도 좋아요",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100024924.1.27",
          "form": "눈올때 장작타는소리 틀어놓고 그러면 좋더라고요",
          "original_form": "눈올때 장작타는소리 틀어놓고 그러면 좋더라고요",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100024924.1.28",
          "form": "비오는 날도 진짜 목숨 걸고 운전한다죠 ㅋㅋ",
          "original_form": "비오는 날도 진짜 목숨 걸고 운전한다죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100024924.1.29",
          "form": "향초피우는 것도 좋아해요",
          "original_form": "향초피우는 것도 좋아해요",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100024924.1.30",
          "form": "기본 주차를 못해서 운전안하구오 ㅋㅋ",
          "original_form": "기본 주차를 못해서 운전안하구오 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100024924.1.31",
          "form": "맞아요 향초 피우는것도 좋아요!!! 무드등도",
          "original_form": "맞아요 향초 피우는것도 좋아요!!! 무드등도",
          "speaker_id": "2",
          "time": "20210901 09:20"
        },
        {
          "id": "MDRW2100024924.1.32",
          "form": "아 주차 전 후방카메라 없으면 주차 절대못해요 ㅋㅋㅋ",
          "original_form": "아 주차 전 후방카메라 없으면 주차 절대못해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:20"
        },
        {
          "id": "MDRW2100024924.1.33",
          "form": "비오늘날 향초는 나름 제습효과도 있더라구요",
          "original_form": "비오늘날 향초는 나름 제습효과도 있더라구요",
          "speaker_id": "1",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100024924.1.34",
          "form": "오 정말요?",
          "original_form": "오 정말요?",
          "speaker_id": "2",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100024924.1.35",
          "form": "오늘 틀어봐야겠네요",
          "original_form": "오늘 틀어봐야겠네요",
          "speaker_id": "2",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100024924.1.36",
          "form": "비가 엄청 쏟아지고 있어서요",
          "original_form": "비가 엄청 쏟아지고 있어서요",
          "speaker_id": "2",
          "time": "20210901 09:25"
        },
        {
          "id": "MDRW2100024924.1.37",
          "form": "여긴 부슬부슬내려요",
          "original_form": "여긴 부슬부슬내려요",
          "speaker_id": "1",
          "time": "20210901 09:39"
        },
        {
          "id": "MDRW2100024924.1.38",
          "form": "비가 너무 계속 오니까 몸도 처지고 별로네요",
          "original_form": "비가 너무 계속 오니까 몸도 처지고 별로네요",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100024924.1.39",
          "form": "적당히 오면 좋은데 길어지니깐 기분도 우울 하고요 ㅎ",
          "original_form": "적당히 오면 좋은데 길어지니깐 기분도 우울 하고요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100024924.1.40",
          "form": "비오는날엔 집콕이 최곤데 ㅜㅠ",
          "original_form": "비오는날엔 집콕이 최곤데 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 10:30"
        }
      ]
    }
  ]
