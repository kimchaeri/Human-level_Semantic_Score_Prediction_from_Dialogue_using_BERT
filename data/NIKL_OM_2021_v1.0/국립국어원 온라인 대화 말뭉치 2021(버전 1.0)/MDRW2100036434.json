      "utterance": [
        {
          "id": "MDRW2100036434.1.1",
          "form": "있잖아 내가 산 거 기억해?",
          "original_form": "있잖아 내가 산 거 기억해?",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036434.1.2",
          "form": "응 혹시",
          "original_form": "응 혹시",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036434.1.3",
          "form": "나 북트롤리 샀다고 햤잖아",
          "original_form": "나 북트롤리 샀다고 햤잖아",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100036434.1.4",
          "form": "타이머 콘센트 ??",
          "original_form": "타이머 콘센트 ??",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.5",
          "form": "아냐ㅠㅠ",
          "original_form": "아냐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.6",
          "form": "아ㅏ 응 그거도 말했었지",
          "original_form": "아ㅏ 응 그거도 말했었지",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.7",
          "form": "그거 후기 좀 알려줘!",
          "original_form": "그거 후기 좀 알려줘!",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.8",
          "form": "북트롤리 근데 그 나무를 가공하고 있는걸까?",
          "original_form": "북트롤리 근데 그 나무를 가공하고 있는걸까?",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.9",
          "form": "감감무소식이네",
          "original_form": "감감무소식이네",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.10",
          "form": "세상ㅇ에 아직 안온거야?",
          "original_form": "세상ㅇ에 아직 안온거야?",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.11",
          "form": "지금 내 방에 책으로 가득찼는데",
          "original_form": "지금 내 방에 책으로 가득찼는데",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.12",
          "form": "언재오는지 알 수 있을까?",
          "original_form": "언재오는지 알 수 있을까?",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.13",
          "form": "아직 중국에 있을까?",
          "original_form": "아직 중국에 있을까?",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.14",
          "form": "정말 속이 터져.",
          "original_form": "정말 속이 터져.",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.15",
          "form": "배송을 그렇게 느리게하면 어떡해",
          "original_form": "배송을 그렇게 느리게하면 어떡해",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.16",
          "form": "그러니까. 근데 또 물어봤다가 내 물건을 취소하면 어쩌지",
          "original_form": "그러니까. 근데 또 물어봤다가 내 물건을 취소하면 어쩌지",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.17",
          "form": "그러게 너무 기다리기 힘들겠다",
          "original_form": "그러게 너무 기다리기 힘들겠다",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036434.1.18",
          "form": "나는 그 물건이 정말 필요해",
          "original_form": "나는 그 물건이 정말 필요해",
          "speaker_id": "2",
          "time": "20210830 23:03"
        }
      ]
