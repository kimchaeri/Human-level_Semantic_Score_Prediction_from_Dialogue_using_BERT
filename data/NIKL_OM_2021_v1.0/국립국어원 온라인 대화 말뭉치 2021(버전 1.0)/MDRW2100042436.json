      "utterance": [
        {
          "id": "MDRW2100042436.1.1",
          "form": "나는 그 화분 숙제라고 생각했거든ㅋㅋㅋ",
          "original_form": "나는 그 화분 숙제라고 생각했거든ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042436.1.2",
          "form": "아 어",
          "original_form": "아 어",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042436.1.3",
          "form": "그 선생님한테 학교생활이 어떠냐 친구들 뭐 얘기 하고 갈 때",
          "original_form": "그 선생님한테 학교생활이 어떠냐 친구들 뭐 얘기 하고 갈 때",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042436.1.4",
          "form": "뭐 못 끼거나 그렇지 않냐 뭐",
          "original_form": "뭐 못 끼거나 그렇지 않냐 뭐",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042436.1.5",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042436.1.6",
          "form": "이렇게 얘기하니까 선생님이",
          "original_form": "이렇게 얘기하니까 선생님이",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042436.1.7",
          "form": "애들이 훨씬 잘한다고",
          "original_form": "애들이 훨씬 잘한다고",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042436.1.8",
          "form": "어머님 말처럼 애들이 다 자기 주장만 내세우면",
          "original_form": "어머님 말처럼 애들이 다 자기 주장만 내세우면",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042436.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042436.1.10",
          "form": "어떻게 될까요",
          "original_form": "어떻게 될까요",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042436.1.11",
          "form": "아 그렇지 그렇지",
          "original_form": "아 그렇지 그렇지",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042436.1.12",
          "form": "들어주는 친구도 중요하지",
          "original_form": "들어주는 친구도 중요하지",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042436.1.13",
          "form": "애들 마다 성향이 다르고 역할도 다 다르고",
          "original_form": "애들 마다 성향이 다르고 역할도 다 다르고",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042436.1.14",
          "form": "이렇게 별 탈 없이 지내는 것만으로도",
          "original_form": "이렇게 별 탈 없이 지내는 것만으로도",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042436.1.15",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042436.1.16",
          "form": "많이 칭찬해주시는 건 어떨까요",
          "original_form": "많이 칭찬해주시는 건 어떨까요",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042436.1.17",
          "form": "오선생님 정말 말 예쁘게 한다",
          "original_form": "오선생님 정말 말 예쁘게 한다",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042436.1.18",
          "form": "이렇게 얘기하데요 ㅋㅋㅋㅋㅋ",
          "original_form": "이렇게 얘기하데요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042436.1.19",
          "form": "반성했겠네",
          "original_form": "반성했겠네",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042436.1.20",
          "form": "나 쫌 무안했잖아요ㅋㅋㅋㅋㅋ",
          "original_form": "나 쫌 무안했잖아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042436.1.21",
          "form": "그러니까요 소심하다 이런 거 잘 못한다 막했는데ㅋㅋㅋㅋㅋ",
          "original_form": "그러니까요 소심하다 이런 거 잘 못한다 막했는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:10"
        }
      ]
