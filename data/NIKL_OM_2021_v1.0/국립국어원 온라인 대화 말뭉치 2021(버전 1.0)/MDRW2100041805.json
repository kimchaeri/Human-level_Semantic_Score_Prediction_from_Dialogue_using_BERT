      "utterance": [
        {
          "id": "MDRW2100041805.1.1",
          "form": "쉽지 않다요~",
          "original_form": "쉽지 않다요~",
          "speaker_id": "1",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100041805.1.2",
          "form": "어 그래 가지고 그렇게 있으니까 우리도 쫌 별론거야",
          "original_form": "어 그래 가지고 그렇게 있으니까 우리도 쫌 별론거야",
          "speaker_id": "2",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041805.1.3",
          "form": "그래서 우리 신랑도 참다가 name3 아빠한테",
          "original_form": "그래서 우리 신랑도 참다가 &name3& 아빠한테",
          "speaker_id": "2",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041805.1.4",
          "form": "형님 요새 수도 주안이랑 안놀고 말도 안하고 소율이 이하고 둘이 놀아서",
          "original_form": "형님 요새 수도 주안이랑 안놀고 말도 안하고 소율이 이하고 둘이 놀아서",
          "speaker_id": "2",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041805.1.5",
          "form": "맨날 혼자 속상하게 집에 온다",
          "original_form": "맨날 혼자 속상하게 집에 온다",
          "speaker_id": "2",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041805.1.6",
          "form": "뭐 이런저런 얘기를 이제 다했어",
          "original_form": "뭐 이런저런 얘기를 이제 다했어",
          "speaker_id": "2",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041805.1.7",
          "form": "그래서 얘들이 너무 자주 붙어 있으니까 쫌 더 많이 싸우는 거 같아서 캠핑을 취소하기로 했거든",
          "original_form": "그래서 얘들이 너무 자주 붙어 있으니까 쫌 더 많이 싸우는 거 같아서 캠핑을 취소하기로 했거든",
          "speaker_id": "2",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100041805.1.8",
          "form": "원래 이번 주 도 같이 캠핑을 가기로 돼 있었어",
          "original_form": "원래 이번 주 도 같이 캠핑을 가기로 돼 있었어",
          "speaker_id": "2",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100041805.1.9",
          "form": "아고 그래됐구만요~",
          "original_form": "아고 그래됐구만요~",
          "speaker_id": "1",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100041805.1.10",
          "form": "나 둘찌.받아와서ㅠ",
          "original_form": "나 둘찌.받아와서ㅠ",
          "speaker_id": "1",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100041805.1.11",
          "form": "이라고 일단락됐는데 수가 다른학원을 가는거구만요~",
          "original_form": "이라고 일단락됐는데 수가 다른학원을 가는거구만요~",
          "speaker_id": "1",
          "time": "20210906 16:27"
        },
        {
          "id": "MDRW2100041805.1.12",
          "form": "아예 만날수 없게;;",
          "original_form": "아예 만날수 없게;;",
          "speaker_id": "1",
          "time": "20210906 16:27"
        },
        {
          "id": "MDRW2100041805.1.13",
          "form": "아 그렇지 이제 서로 거리 두기를 하자라고 얘기 했는데",
          "original_form": "아 그렇지 이제 서로 거리 두기를 하자라고 얘기 했는데",
          "speaker_id": "2",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100041805.1.14",
          "form": "name3엄마가 태권도 마치고 아예 다른 학원을 보내버리는 거지",
          "original_form": "&name3&엄마가 태권도 마치고 아예 다른 학원을 보내버리는 거지",
          "speaker_id": "2",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100041805.1.15",
          "form": "그래서 우리 신랑은",
          "original_form": "그래서 우리 신랑은",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100041805.1.16",
          "form": "자기 아들 버릇없는 거 생각 안하고",
          "original_form": "자기 아들 버릇없는 거 생각 안하고",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100041805.1.17",
          "form": "우리 원 잘못한 것만 얘기한다고",
          "original_form": "우리 원 잘못한 것만 얘기한다고",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100041805.1.18",
          "form": "화가 났지 뭐",
          "original_form": "화가 났지 뭐",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100041805.1.19",
          "form": "좀 글네요...;;",
          "original_form": "좀 글네요...;;",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041805.1.20",
          "form": "애들 어떻게 노는지 알텐데..뭐 그렇게 까지;;",
          "original_form": "애들 어떻게 노는지 알텐데..뭐 그렇게 까지;;",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041805.1.21",
          "form": "슬픈 현실이구만요~",
          "original_form": "슬픈 현실이구만요~",
          "speaker_id": "1",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100041805.1.22",
          "form": "이게 어른이 맘상해서 개입되면 이래 되는듯요~",
          "original_form": "이게 어른이 맘상해서 개입되면 이래 되는듯요~",
          "speaker_id": "1",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041805.1.23",
          "form": "애들은 이카다가도 잘 노는데..",
          "original_form": "애들은 이카다가도 잘 노는데..",
          "speaker_id": "1",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100041805.1.24",
          "form": "그니까 어른들이 서로 마음이 상하기 시작하니",
          "original_form": "그니까 어른들이 서로 마음이 상하기 시작하니",
          "speaker_id": "2",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041805.1.25",
          "form": "결론은 안 보게 되는 것 같다",
          "original_form": "결론은 안 보게 되는 것 같다",
          "speaker_id": "2",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041805.1.26",
          "form": "맞아요 name3한테도 캤을 거예요",
          "original_form": "맞아요 &name3&한테도 캤을 거예요",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.27",
          "form": "놀지 말라고",
          "original_form": "놀지 말라고",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.28",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.29",
          "form": "같이 자기도 하고 했었는데",
          "original_form": "같이 자기도 하고 했었는데",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.30",
          "form": "애들 싸움이 어른 싸움 되는,,",
          "original_form": "애들 싸움이 어른 싸움 되는,,",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.31",
          "form": "거의 매일 여기 놀이터와서 놀고",
          "original_form": "거의 매일 여기 놀이터와서 놀고",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.32",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.33",
          "form": "먹고 했는데",
          "original_form": "먹고 했는데",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.34",
          "form": "그냥 어정쩡한 사이면 모르겠는데",
          "original_form": "그냥 어정쩡한 사이면 모르겠는데",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.35",
          "form": "가족끼리 캠핑도 가고 했는데",
          "original_form": "가족끼리 캠핑도 가고 했는데",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.36",
          "form": "그 전화두번에 끝났음",
          "original_form": "그 전화두번에 끝났음",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.37",
          "form": "의아하네 응,,",
          "original_form": "의아하네 응,,",
          "speaker_id": "1",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041805.1.38",
          "form": "name3엄마는 게임이랑 카톡지운거에",
          "original_form": "&name3&엄마는 게임이랑 카톡지운거에",
          "speaker_id": "2",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041805.1.39",
          "form": "그래서 애들 땜에 만난 사이는",
          "original_form": "그래서 애들 땜에 만난 사이는",
