      "utterance": [
        {
          "id": "MDRW2100022571.1.1",
          "form": "주제 고르기가 진짜 애매하네 식음료냐 쇼핑과상품이냐",
          "original_form": "주제 고르기가 진짜 애매하네 식음료냐 쇼핑과상품이냐",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022571.1.2",
          "form": "아이스크림이랑 초코렛만 생각햇어요 ㅋㅋㅋㅋ",
          "original_form": "아이스크림이랑 초코렛만 생각햇어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.3",
          "form": "네 초코파이도 민트초코맛 있더라니까요?",
          "original_form": "네 초코파이도 민트초코맛 있더라니까요?",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.4",
          "form": "아 저 오예스먹어봤어요 ㅋㅋㅋㅋ",
          "original_form": "아 저 오예스먹어봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.5",
          "form": "카페음료, 아이스크림 등은 그래도 민초맛 잘 살려낼 거 같은데",
          "original_form": "카페음료, 아이스크림 등은 그래도 민초맛 잘 살려낼 거 같은데",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.6",
          "form": "그건 괜찮던데요!",
          "original_form": "그건 괜찮던데요!",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.7",
          "form": "과자는 흉내만 냈을 거 같아서 더 반감 들엉 ㅗㅠ",
          "original_form": "과자는 흉내만 냈을 거 같아서 더 반감 들엉 ㅗㅠ",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.8",
          "form": "오 맛이 괜찮아요 ?",
          "original_form": "오 맛이 괜찮아요 ?",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.9",
          "form": "진짜 민초 구현해냈나요",
          "original_form": "진짜 민초 구현해냈나요",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.10",
          "form": "앜ㅋㅋㅋㅋ근데 맛없게만드는건 좀 유죄 ㅠ",
          "original_form": "앜ㅋㅋㅋㅋ근데 맛없게만드는건 좀 유죄 ㅠ",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.11",
          "form": "ㅋㅋㅋㅋ아 오예스도 먹어보고 싶네요",
          "original_form": "ㅋㅋㅋㅋ아 오예스도 먹어보고 싶네요",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.12",
          "form": "사실 아이스크림은 못따라와요 ㅠㅠ",
          "original_form": "사실 아이스크림은 못따라와요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.13",
          "form": "미니 오예스도 되게 아기자기하게 귀여웠어요",
          "original_form": "미니 오예스도 되게 아기자기하게 귀여웠어요",
          "speaker_id": "1",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022571.1.14",
          "form": "먹어봤어요?",
          "original_form": "먹어봤어요?",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022571.1.15",
          "form": "네네 ㅋㅋㅋㅋ맛나요",
          "original_form": "네네 ㅋㅋㅋㅋ맛나요",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022571.1.16",
          "form": "최애과자있어요?",
          "original_form": "최애과자있어요?",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022571.1.17",
          "form": "오예스가 해태건가요?",
          "original_form": "오예스가 해태건가요?",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022571.1.18",
          "form": "저는 최애과자는 잘 모르겠어요",
          "original_form": "저는 최애과자는 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022571.1.19",
          "form": "아 생각났어요!!",
          "original_form": "아 생각났어요!!",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022571.1.20",
          "form": "저 허니버터칩 좋아해요!!",
          "original_form": "저 허니버터칩 좋아해요!!",
          "speaker_id": "1",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022571.1.21",
          "form": "크 허니버터칩 맛아요",
          "original_form": "크 허니버터칩 맛아요",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022571.1.22",
          "form": "과자 중에 허니버터칩 최고에요",
          "original_form": "과자 중에 허니버터칩 최고에요",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022571.1.23",
          "form": "맥주안주로딱입니다",
          "original_form": "맥주안주로딱입니다",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022571.1.24",
          "form": "그거 유행 끝나고도 쭉 먹었어요",
          "original_form": "그거 유행 끝나고도 쭉 먹었어요",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022571.1.25",
          "form": "저두요 ㅋㅋㅋㅋ다양한 맛으로도 나왓더라구요",
          "original_form": "저두요 ㅋㅋㅋㅋ다양한 맛으로도 나왓더라구요",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022571.1.26",
          "form": "앗 그래요~?",
          "original_form": "앗 그래요~?",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022571.1.27",
          "form": "허너버터 아몬드도 맛있던데 헤",
          "original_form": "허너버터 아몬드도 맛있던데 헤",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022571.1.28",
          "form": "네네 트러플 뭐이런거두잇구 ㅋㅋㅋㅋ",
          "original_form": "네네 트러플 뭐이런거두잇구 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022571.1.29",
          "form": "님은 쿠크다스 좋아하시죠?^^;; ㅎㅎ",
          "original_form": "님은 쿠크다스 좋아하시죠?^^;; ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022571.1.30",
          "form": "맞아요 저도 그거 좋아해욬ㅋㅋㅋㅋ",
          "original_form": "맞아요 저도 그거 좋아해욬ㅋㅋㅋㅋ",
