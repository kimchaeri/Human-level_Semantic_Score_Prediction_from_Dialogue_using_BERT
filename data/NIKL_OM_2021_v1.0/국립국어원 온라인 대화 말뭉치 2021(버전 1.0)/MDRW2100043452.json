      "utterance": [
        {
          "id": "MDRW2100043452.1.1",
          "form": "ㅋㅋㅋ글쎄",
          "original_form": "ㅋㅋㅋ글쎄",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043452.1.2",
          "form": "요즘은 옷 구매욕은 별로 없고",
          "original_form": "요즘은 옷 구매욕은 별로 없고",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043452.1.3",
          "form": "일억이 생긴다면 뭘 사실까요",
          "original_form": "일억이 생긴다면 뭘 사실까요",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043452.1.4",
          "form": "집이요",
          "original_form": "집이요",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043452.1.5",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043452.1.6",
          "form": "일억갖고 집을 어케삽니까",
          "original_form": "일억갖고 집을 어케삽니까",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043452.1.7",
          "form": "집말고 딴거 사라하면 뭐사실지?",
          "original_form": "집말고 딴거 사라하면 뭐사실지?",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043452.1.8",
          "form": "저축해서 집 사는데 보탭니다",
          "original_form": "저축해서 집 사는데 보탭니다",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043452.1.9",
          "form": "절약해야죠",
          "original_form": "절약해야죠",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043452.1.10",
          "form": "재미가 없으시군요",
          "original_form": "재미가 없으시군요",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043452.1.11",
          "form": "절약안돼 집못사 하면 뭘 사시겠습니까",
          "original_form": "절약안돼 집못사 하면 뭘 사시겠습니까",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043452.1.12",
          "form": "언니는 뭘 사고 싶나요",
          "original_form": "언니는 뭘 사고 싶나요",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043452.1.13",
          "form": "일억이 있다면 일단 저도 집살때 보탤거같지만",
          "original_form": "일억이 있다면 일단 저도 집살때 보탤거같지만",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043452.1.14",
          "form": "일단 주식으로 갈라넣든가",
          "original_form": "일단 주식으로 갈라넣든가",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043452.1.15",
          "form": "아님 동생 명품가방 하나 사줄까~",
          "original_form": "아님 동생 명품가방 하나 사줄까~",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043452.1.16",
          "form": "아 삼성전자주식",
          "original_form": "아 삼성전자주식",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043452.1.17",
          "form": "와~",
          "original_form": "와~",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043452.1.18",
          "form": "감동이네요",
          "original_form": "감동이네요",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043452.1.19",
          "form": "그러나 전 정말 명품은 크게 관심이 없습니당",
          "original_form": "그러나 전 정말 명품은 크게 관심이 없습니당",
          "speaker_id": "2",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043452.1.20",
          "form": "샤넬 재테크하라구",
          "original_form": "샤넬 재테크하라구",
          "speaker_id": "1",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043452.1.21",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043452.1.22",
          "form": "그럼 매장 가서 줄서고 그래야ㅋㅋ",
          "original_form": "그럼 매장 가서 줄서고 그래야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043452.1.23",
          "form": "ㅋㅋ맞아 요새 명품매장 줄선댔지",
          "original_form": "ㅋㅋ맞아 요새 명품매장 줄선댔지",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043452.1.24",
          "form": "응 아주 난리라고 하더라",
          "original_form": "응 아주 난리라고 하더라",
          "speaker_id": "2",
          "time": "20210907 19:13"
        }
      ]
