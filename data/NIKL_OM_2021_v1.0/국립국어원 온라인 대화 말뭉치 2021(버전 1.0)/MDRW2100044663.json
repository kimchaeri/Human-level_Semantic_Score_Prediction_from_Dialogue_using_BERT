      "utterance": [
        {
          "id": "MDRW2100044663.1.1",
          "form": "님 다여트 해요?",
          "original_form": "님 다여트 해요?",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044663.1.2",
          "form": "운동안하고 살빼는법있어요?",
          "original_form": "운동안하고 살빼는법있어요?",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044663.1.3",
          "form": "안먹음 돼요",
          "original_form": "안먹음 돼요",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044663.1.4",
          "form": "먹으면서 살빼는법 있어요?",
          "original_form": "먹으면서 살빼는법 있어요?",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044663.1.5",
          "form": "시술 어때요?",
          "original_form": "시술 어때요?",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044663.1.6",
          "form": "시술 어떤거있어요?",
          "original_form": "시술 어떤거있어요?",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044663.1.7",
          "form": "지방흡입요",
          "original_form": "지방흡입요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044663.1.8",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044663.1.9",
          "form": "무서운데ㅠㅠ",
          "original_form": "무서운데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044663.1.10",
          "form": "괜찮을까여",
          "original_form": "괜찮을까여",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044663.1.11",
          "form": "흠...무섭긴해요",
          "original_form": "흠...무섭긴해요",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044663.1.12",
          "form": "역시 운동이 답일까요",
          "original_form": "역시 운동이 답일까요",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044663.1.13",
          "form": "맞아요ㅠ 휴 그치만 살쪄서 옷이안맞아서 걱정이네영",
          "original_form": "맞아요ㅠ 휴 그치만 살쪄서 옷이안맞아서 걱정이네영",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044663.1.14",
          "form": "운동 조금 귀찮은데ㅠㅠ",
          "original_form": "운동 조금 귀찮은데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044663.1.15",
          "form": "주산 어떨까여",
          "original_form": "주산 어떨까여",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044663.1.16",
          "form": "오 살빼는 쥬사가 있나여?",
          "original_form": "오 살빼는 쥬사가 있나여?",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044663.1.17",
          "form": "주사",
          "original_form": "주사",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044663.1.18",
          "form": "지방분해??",
          "original_form": "지방분해??",
          "speaker_id": "1",
          "time": "20210908 00:02"
        }
      ]
