      "utterance": [
        {
          "id": "MDRW2100024028.1.1",
          "form": "자주먹진 못하는데 단새우 초밥도 좋아요",
          "original_form": "자주먹진 못하는데 단새우 초밥도 좋아요",
          "speaker_id": "1",
          "time": "20210830 21:24"
        },
        {
          "id": "MDRW2100024028.1.2",
          "form": "와 새우 초밥 너무 좋죠 ㅠㅠ 단새우 초밥은 먹어본적 없지만 새우초밥 굉장히 좋아해요",
          "original_form": "와 새우 초밥 너무 좋죠 ㅠㅠ 단새우 초밥은 먹어본적 없지만 새우초밥 굉장히 좋아해요",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100024028.1.3",
          "form": "내일은 뭐 드실건가요??",
          "original_form": "내일은 뭐 드실건가요??",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100024028.1.4",
          "form": "글쎄요 아직 못 정했어요",
          "original_form": "글쎄요 아직 못 정했어요",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024028.1.5",
          "form": "뭐드실거예요?",
          "original_form": "뭐드실거예요?",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024028.1.6",
          "form": "내일은 계란말이를 해보고 싶어요 ㅎㅎ",
          "original_form": "내일은 계란말이를 해보고 싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024028.1.7",
          "form": "저는 시골에 살아서 배달을 못해먹거든요 해먹을 수있다면 초밥을 먹고싶네요 ㅎㅎ",
          "original_form": "저는 시골에 살아서 배달을 못해먹거든요 해먹을 수있다면 초밥을 먹고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024028.1.8",
          "form": "앗 그러시구나...배달되는 음식 메뉴있나요?",
          "original_form": "앗 그러시구나...배달되는 음식 메뉴있나요?",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100024028.1.9",
          "form": "아니요 허허 여기는 아무것ㄷ없어요",
          "original_form": "아니요 허허 여기는 아무것ㄷ없어요",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024028.1.10",
          "form": "채식좋아하시나요?",
          "original_form": "채식좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024028.1.11",
          "form": "일주일에 하루는 채식으로만 먹어요",
          "original_form": "일주일에 하루는 채식으로만 먹어요",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024028.1.12",
          "form": "호불호로 따지자면 불호긴해요...",
          "original_form": "호불호로 따지자면 불호긴해요...",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024028.1.13",
          "form": "그러시군요 저도 환경생각해서 채식하고 있어요! 나중에 지구가 없어지면 인간탓이니까요 ㅠㅠ",
          "original_form": "그러시군요 저도 환경생각해서 채식하고 있어요! 나중에 지구가 없어지면 인간탓이니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024028.1.14",
          "form": "저도 고기를 굉장히 좋아해요 토마호크가 먹고싶은 참이에요 지금 ㅎㅎ",
          "original_form": "저도 고기를 굉장히 좋아해요 토마호크가 먹고싶은 참이에요 지금 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024028.1.15",
          "form": "그죠 저도 환경때문에 주에 한번이라도...짧게 해보고있어요",
          "original_form": "그죠 저도 환경때문에 주에 한번이라도...짧게 해보고있어요",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024028.1.16",
          "form": "참 채소가 비싸고 씁쓸한게 많죠...",
          "original_form": "참 채소가 비싸고 씁쓸한게 많죠...",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024028.1.17",
          "form": "생으로 먹으면 소화도힘들고",
          "original_form": "생으로 먹으면 소화도힘들고",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024028.1.18",
          "form": "맞아요 정말 소화 어려운게 힘든것 같아요",
          "original_form": "맞아요 정말 소화 어려운게 힘든것 같아요",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024028.1.19",
          "form": "저는 그래서 월남쌈을 주로 해먹었어요!",
          "original_form": "저는 그래서 월남쌈을 주로 해먹었어요!",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024028.1.20",
          "form": "몇개만 넣어서 거의 소스맛으로 먹기도 해서요 ㅋㅋ",
          "original_form": "몇개만 넣어서 거의 소스맛으로 먹기도 해서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024028.1.21",
          "form": "저도 월남쌈 자주 먹어요ㅋㅋㅋㅋ",
          "original_form": "저도 월남쌈 자주 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100024028.1.22",
          "form": "편하죠....맛있고",
          "original_form": "편하죠....맛있고",
          "speaker_id": "1",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100024028.1.23",
          "form": "과일은 좋아하시나요?",
          "original_form": "과일은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024028.1.24",
          "form": "맞아요! 과일 저는 배 좋아해요",
          "original_form": "맞아요! 과일 저는 배 좋아해요",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024028.1.25",
          "form": "복숭아 좋아하시나요?",
          "original_form": "복숭아 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024028.1.26",
          "form": "좋아하신다면 딱복 물복 중 뭘 선호하시나요?",
          "original_form": "좋아하신다면 딱복 물복 중 뭘 선호하시나요?",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024028.1.27",
          "form": "복숭아좋죠",
          "original_form": "복숭아좋죠",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024028.1.28",
          "form": "저는 물복이요!",
          "original_form": "저는 물복이요!",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024028.1.29",
          "form": "뭐좋아하세요?",
          "original_form": "뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024028.1.30",
          "form": "저도 물복이여 진짜 너무 맛있어요",
          "original_form": "저도 물복이여 진짜 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024028.1.31",
          "form": "요슴 품종도 다야해서 더 좋은 것 같아요",
          "original_form": "요슴 품종도 다야해서 더 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210830 22:12"
        }
      ]
