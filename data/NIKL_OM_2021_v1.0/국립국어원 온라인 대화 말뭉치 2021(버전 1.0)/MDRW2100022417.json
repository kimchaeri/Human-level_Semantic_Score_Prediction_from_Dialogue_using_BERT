      "utterance": [
        {
          "id": "MDRW2100022417.1.1",
          "form": "아 그거 맛있어???",
          "original_form": "아 그거 맛있어???",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022417.1.2",
          "form": "었어?",
          "original_form": "었어?",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022417.1.3",
          "form": "나 한번도 안먹어봄",
          "original_form": "나 한번도 안먹어봄",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022417.1.4",
          "form": "나는맛있었음",
          "original_form": "나는맛있었음",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022417.1.5",
          "form": "다시나왔으면좋겠음",
          "original_form": "다시나왔으면좋겠음",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022417.1.6",
          "form": "근데너매운더싫어해서",
          "original_form": "근데너매운더싫어해서",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022417.1.7",
          "form": "안좋아할수도",
          "original_form": "안좋아할수도",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022417.1.8",
          "form": "아 그래? 오 그럴 수도 있겠다",
          "original_form": "아 그래? 오 그럴 수도 있겠다",
          "speaker_id": "2",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022417.1.9",
          "form": "아니 근데 롯데리아 단종 된 거 다시 나온 적 없지 않아?",
          "original_form": "아니 근데 롯데리아 단종 된 거 다시 나온 적 없지 않아?",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022417.1.10",
          "form": "뭐단종됐었지?",
          "original_form": "뭐단종됐었지?",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022417.1.11",
          "form": "강정버거도 단종되었을걸",
          "original_form": "강정버거도 단종되었을걸",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022417.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022417.1.13",
          "form": "az도",
          "original_form": "az도",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022417.1.14",
          "form": "이제없지않아?",
          "original_form": "이제없지않아?",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022417.1.15",
          "form": "은근단종된거많네",
          "original_form": "은근단종된거많네",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022417.1.16",
          "form": "어 AZ가 없어짐??????",
          "original_form": "어 AZ가 없어짐??????",
          "speaker_id": "2",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022417.1.17",
          "form": "뭐여",
          "original_form": "뭐여",
          "speaker_id": "2",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022417.1.18",
          "form": "그럼 뭐먹어 뮤ㅗㄴ데",
          "original_form": "그럼 뭐먹어 뮤ㅗㄴ데",
          "speaker_id": "2",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022417.1.19",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022417.1.20",
          "form": "너az좋아함?",
          "original_form": "너az좋아함?",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022417.1.21",
          "form": "아 괜찮다고 생각했었음",
          "original_form": "아 괜찮다고 생각했었음",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022417.1.22",
          "form": "근데 갑자기 사라졌더니까 당황함",
          "original_form": "근데 갑자기 사라졌더니까 당황함",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022417.1.23",
          "form": "저번달에단종됐다는대",
          "original_form": "저번달에단종됐다는대",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022417.1.24",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022417.1.25",
          "form": "롯데리아는",
          "original_form": "롯데리아는",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022417.1.26",
          "form": "맛있는거다",
          "original_form": "맛있는거다",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022417.1.27",
          "form": "단종시켜",
          "original_form": "단종시켜",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022417.1.28",
          "form": "그니까 걔네 왜 그런대 정말",
          "original_form": "그니까 걔네 왜 그런대 정말",
          "speaker_id": "2",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022417.1.29",
          "form": "디저트만 남겨두고 다 버릴 생각인가",
          "original_form": "디저트만 남겨두고 다 버릴 생각인가",
          "speaker_id": "2",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022417.1.30",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022417.1.31",
          "form": "너네집에서",
          "original_form": "너네집에서",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022417.1.32",
