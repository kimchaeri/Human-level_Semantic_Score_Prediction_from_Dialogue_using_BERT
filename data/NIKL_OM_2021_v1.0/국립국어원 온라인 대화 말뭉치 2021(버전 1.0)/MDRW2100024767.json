      "utterance": [
        {
          "id": "MDRW2100024767.1.1",
          "form": "지금은 배달문화가 바껴서 안줄것같아",
          "original_form": "지금은 배달문화가 바껴서 안줄것같아",
          "speaker_id": "2",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024767.1.2",
          "form": "이런 그렇구만",
          "original_form": "이런 그렇구만",
          "speaker_id": "1",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024767.1.3",
          "form": "으악 진짜 머지 않은 날에 탕수육 먹어야겠어",
          "original_form": "으악 진짜 머지 않은 날에 탕수육 먹어야겠어",
          "speaker_id": "1",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024767.1.4",
          "form": "너랑 얘기하니까 더 먹고싶어진다",
          "original_form": "너랑 얘기하니까 더 먹고싶어진다",
          "speaker_id": "1",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024767.1.5",
          "form": "탕수육소스에",
          "original_form": "탕수육소스에",
          "speaker_id": "2",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024767.1.6",
          "form": "군만두찍으면",
          "original_form": "군만두찍으면",
          "speaker_id": "2",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024767.1.7",
          "form": "존맛이야",
          "original_form": "존맛이야",
          "speaker_id": "2",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024767.1.8",
          "form": "헐 진짜? 나 지금까지 군만두는 짜장에만 찍었는데",
          "original_form": "헐 진짜? 나 지금까지 군만두는 짜장에만 찍었는데",
          "speaker_id": "1",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024767.1.9",
          "form": "탕수육소스에 찍을 생각은 못 해봤다",
          "original_form": "탕수육소스에 찍을 생각은 못 해봤다",
          "speaker_id": "1",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024767.1.10",
          "form": "응 진짜 바삭바삭하고 달달해",
          "original_form": "응 진짜 바삭바삭하고 달달해",
          "speaker_id": "2",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024767.1.11",
          "form": "같은 튀김인데 왜 그럴 생각을 못 했을까",
          "original_form": "같은 튀김인데 왜 그럴 생각을 못 했을까",
          "speaker_id": "1",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100024767.1.12",
          "form": "니 말 듣고 무릎 탁 침 ㅋㅋㅋ 꼭 해볼게",
          "original_form": "니 말 듣고 무릎 탁 침 ㅋㅋㅋ 꼭 해볼게",
          "speaker_id": "1",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100024767.1.13",
          "form": "토달볶은  금지야",
          "original_form": "토달볶은  금지야",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024767.1.14",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024767.1.15",
          "form": "아 왜 토마토가 어때서",
          "original_form": "아 왜 토마토가 어때서",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024767.1.16",
          "form": "토마토랑 달걀은 괴식이다 고식",
          "original_form": "토마토랑 달걀은 괴식이다 고식",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024767.1.17",
          "form": "괴식이라니 얼마나 건강에 좋은데",
          "original_form": "괴식이라니 얼마나 건강에 좋은데",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024767.1.18",
          "form": "이단이다!!!",
          "original_form": "이단이다!!!",
          "speaker_id": "1",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024767.1.19",
          "form": "콩이랑 초코를 먹는 느낌일듯",
          "original_form": "콩이랑 초코를 먹는 느낌일듯",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024767.1.20",
          "form": "아 그건 진짜 괴식이다",
          "original_form": "아 그건 진짜 괴식이다",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024767.1.21",
          "form": "굳이 왜가   입에서    맴돈다",
          "original_form": "굳이 왜가   입에서    맴돈다",
          "speaker_id": "2",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024767.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024767.1.23",
          "form": "토달볶이 그런 느낌이구나 진짜 웃기네",
          "original_form": "토달볶이 그런 느낌이구나 진짜 웃기네",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024767.1.24",
          "form": "다음에 콩초코 만들어서 선물해줄게",
          "original_form": "다음에 콩초코 만들어서 선물해줄게",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024767.1.25",
          "form": "계란은 많이 먹으면 좋지",
          "original_form": "계란은 많이 먹으면 좋지",
          "speaker_id": "2",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024767.1.26",
          "form": "콩초코라니 윙치킨다음으로",
          "original_form": "콩초코라니 윙치킨다음으로",
          "speaker_id": "2",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024767.1.27",
          "form": "어ㅇ이없네",
          "original_form": "어ㅇ이없네",
          "speaker_id": "2",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024767.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ윙치킨이래",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ윙치킨이래",
