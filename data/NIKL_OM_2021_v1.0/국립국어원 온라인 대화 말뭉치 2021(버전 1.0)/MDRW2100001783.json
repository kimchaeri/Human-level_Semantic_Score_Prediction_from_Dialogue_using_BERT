      "utterance": [
        {
          "id": "MDRW2100001783.1.1",
          "form": "name2님도 슬기로운 의사생활 보시나요?",
          "original_form": "&name2&님도 슬기로운 의사생활 보시나요?",
          "speaker_id": "1",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100001783.1.2",
          "form": "몇번 봤어요 조정석 역시 연기 잘하는듯해요",
          "original_form": "몇번 봤어요 조정석 역시 연기 잘하는듯해요",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100001783.1.3",
          "form": "다들 연기잘하더라구요ㅎ",
          "original_form": "다들 연기잘하더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100001783.1.4",
          "form": "그 안경쓴 여자분 이름이?",
          "original_form": "그 안경쓴 여자분 이름이?",
          "speaker_id": "2",
          "time": "20210819 21:13"
        },
        {
          "id": "MDRW2100001783.1.5",
          "form": "앗 저도 잘모르겠어요 드라마에서 처음보는얼굴인데",
          "original_form": "앗 저도 잘모르겠어요 드라마에서 처음보는얼굴인데",
          "speaker_id": "1",
          "time": "20210819 21:13"
        },
        {
          "id": "MDRW2100001783.1.6",
          "form": "전미도네요",
          "original_form": "전미도네요",
          "speaker_id": "2",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100001783.1.7",
          "form": "신인같은데 나이는 좀 있더라구요 83인가",
          "original_form": "신인같은데 나이는 좀 있더라구요 83인가",
          "speaker_id": "1",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100001783.1.8",
          "form": "42세래요",
          "original_form": "42세래요",
          "speaker_id": "2",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100001783.1.9",
          "form": "헉 83아니예요?ㅎ 80이였나보네요",
          "original_form": "헉 83아니예요?ㅎ 80이였나보네요",
          "speaker_id": "1",
          "time": "20210819 21:15"
        },
        {
          "id": "MDRW2100001783.1.10",
          "form": "뮤지컬로 데뷔했네요",
          "original_form": "뮤지컬로 데뷔했네요",
          "speaker_id": "2",
          "time": "20210819 21:15"
        },
        {
          "id": "MDRW2100001783.1.11",
          "form": "결혼도 했던데요",
          "original_form": "결혼도 했던데요",
          "speaker_id": "1",
          "time": "20210819 21:15"
        },
        {
          "id": "MDRW2100001783.1.12",
          "form": "82년생 맞아요 프로필 42세라고ㅋㅋ",
          "original_form": "82년생 맞아요 프로필 42세라고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:16"
        },
        {
          "id": "MDRW2100001783.1.13",
          "form": "노래도 그럼잘할텐데 드라마는 컨셉이라 엄청 음치로 나오더라고요",
          "original_form": "노래도 그럼잘할텐데 드라마는 컨셉이라 엄청 음치로 나오더라고요",
          "speaker_id": "1",
          "time": "20210819 21:16"
        },
        {
          "id": "MDRW2100001783.1.14",
          "form": "컨셉인가봐요",
          "original_form": "컨셉인가봐요",
          "speaker_id": "2",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001783.1.15",
          "form": "그거보셨어요? 고요속의 외침 퀴즈하는거",
          "original_form": "그거보셨어요? 고요속의 외침 퀴즈하는거",
          "speaker_id": "1",
          "time": "20210819 21:19"
        },
        {
          "id": "MDRW2100001783.1.16",
          "form": "나영석 피디가 하는거였는데 진짜 그거보고 울면서 봤어요 넘웃겨서",
          "original_form": "나영석 피디가 하는거였는데 진짜 그거보고 울면서 봤어요 넘웃겨서",
          "speaker_id": "1",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100001783.1.17",
          "form": "완전 웃겼어요",
          "original_form": "완전 웃겼어요",
          "speaker_id": "2",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100001783.1.18",
          "form": "본거 또보고 몇번을 봐도 웃기더라구요ㅎㅎ",
          "original_form": "본거 또보고 몇번을 봐도 웃기더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001783.1.19",
          "form": "특히 정경호가 은근 애교가 많은듯요",
          "original_form": "특히 정경호가 은근 애교가 많은듯요",
          "speaker_id": "1",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001783.1.20",
          "form": "계속 노래가 멤돈다고",
          "original_form": "계속 노래가 멤돈다고",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001783.1.21",
          "form": "그니까요 약간 허당끼도 있고",
          "original_form": "그니까요 약간 허당끼도 있고",
          "speaker_id": "1",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001783.1.22",
          "form": "신서유기에서도 고요속의 외침이 제일 재밌더라고요",
          "original_form": "신서유기에서도 고요속의 외침이 제일 재밌더라고요",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001783.1.23",
          "form": "그래요? 함 찾아봐야겠네요",
          "original_form": "그래요? 함 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210819 21:22"
        }
      ]
    }
  ]
