      "utterance": [
        {
          "id": "MDRW2100004930.1.1",
          "form": "안되는건 또 뭐죠",
          "original_form": "안되는건 또 뭐죠",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004930.1.2",
          "form": "애도 생각하면 더 슬프네요",
          "original_form": "애도 생각하면 더 슬프네요",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004930.1.3",
          "form": "아내분이 고집이 있으시군요",
          "original_form": "아내분이 고집이 있으시군요",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004930.1.4",
          "form": "아이가 있어서 저도 슬퍼요",
          "original_form": "아이가 있어서 저도 슬퍼요",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004930.1.5",
          "form": "오늘도 아빠를 세상에서 가장 사랑해 하는데",
          "original_form": "오늘도 아빠를 세상에서 가장 사랑해 하는데",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004930.1.6",
          "form": "지금 같이 지내고 있지만 언젠가 떨어져야 한다는 생각에",
          "original_form": "지금 같이 지내고 있지만 언젠가 떨어져야 한다는 생각에",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004930.1.7",
          "form": "항상 슬퍼요ㅠ",
          "original_form": "항상 슬퍼요ㅠ",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004930.1.8",
          "form": "애기는 몇살이에요??",
          "original_form": "애기는 몇살이에요??",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004930.1.9",
          "form": "아직 멀었어요. 힘내세요",
          "original_form": "아직 멀었어요. 힘내세요",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004930.1.10",
          "form": "이제4살이에요ㅎㅎ",
          "original_form": "이제4살이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004930.1.11",
          "form": "한달남았네요ㅎ",
          "original_form": "한달남았네요ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004930.1.12",
          "form": "저희아기랑 한살차이네요",
          "original_form": "저희아기랑 한살차이네요",
          "speaker_id": "2",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004930.1.13",
          "form": "아직 너무 애긴대  ㅜ",
          "original_form": "아직 너무 애긴대  ㅜ",
          "speaker_id": "2",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004930.1.14",
          "form": "우리애가 1월까지만해도 말을 못했어요ㅎ",
          "original_form": "우리애가 1월까지만해도 말을 못했어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004930.1.15",
          "form": "처음부터요???",
          "original_form": "처음부터요???",
          "speaker_id": "2",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004930.1.16",
          "form": "근데 어린이집가고서부터는 우와ㅎ",
          "original_form": "근데 어린이집가고서부터는 우와ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004930.1.17",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004930.1.18",
          "form": "단어정도? 엄마?아빠?",
          "original_form": "단어정도? 엄마?아빠?",
          "speaker_id": "1",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004930.1.19",
          "form": "또래보다 진짜 느렸었어요ㅠㅠ",
          "original_form": "또래보다 진짜 느렸었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004930.1.20",
          "form": "저희집애도 그런데 금방 늘더라고요",
          "original_form": "저희집애도 그런데 금방 늘더라고요",
          "speaker_id": "2",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004930.1.21",
          "form": "걱정마세요",
          "original_form": "걱정마세요",
          "speaker_id": "2",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004930.1.22",
          "form": "지금은 엄청나요ㅎㅎㅎㅎ",
          "original_form": "지금은 엄청나요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004930.1.23",
          "form": "하루는 아이가 이렇게 말하더라구요ㅎ\r\n아빠! 저 핸드폰으로 유튜브 보면 안될까요? 제가 킬께요",
          "original_form": "하루는 아이가 이렇게 말하더라구요ㅎ\r\n아빠! 저 핸드폰으로 유튜브 보면 안될까요? 제가 킬께요",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004930.1.24",
          "form": "금방 배울때죠",
          "original_form": "금방 배울때죠",
          "speaker_id": "2",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004930.1.25",
          "form": "ㅎㅎㅎㅎㅎㅎㅎㅎㅎ어찌나 웃었던지ㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎㅎㅎ어찌나 웃었던지ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004930.1.26",
          "form": "엄청 길게 말하네요",
          "original_form": "엄청 길게 말하네요",
          "speaker_id": "2",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004930.1.27",
          "form": "아무래도 어린이집영향이 큰것같애요ㅎ",
          "original_form": "아무래도 어린이집영향이 큰것같애요ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004930.1.28",
          "form": "그동안 코로나때문에 가정보육을 자주해서 그런게 아닐까 싶어요ㅎ",
          "original_form": "그동안 코로나때문에 가정보육을 자주해서 그런게 아닐까 싶어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004930.1.29",
          "form": "결혼해서 가장좋은건 아이가 행복을 주는거네요ㅎ",
          "original_form": "결혼해서 가장좋은건 아이가 행복을 주는거네요ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004930.1.30",
          "form": "맞아요 결혼후 아이때메 더 행복한것 같아요",
          "original_form": "맞아요 결혼후 아이때메 더 행복한것 같아요",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100004930.1.31",
          "form": "아이가 결혼생활의 선물이죠ㅎ",
          "original_form": "아이가 결혼생활의 선물이죠ㅎ",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100004930.1.32",
          "form": "맞아요 아이가 주는 기쁨이 크네요 ㅎ",
          "original_form": "맞아요 아이가 주는 기쁨이 크네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100004930.1.33",
          "form": "결혼생활중 아이가 있고 없는 차이가 크죠",
          "original_form": "결혼생활중 아이가 있고 없는 차이가 크죠",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100004930.1.34",
          "form": "아이가 잘때 제일 좋아요",
          "original_form": "아이가 잘때 제일 좋아요",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100004930.1.35",
          "form": "아시죠? 그 잘때 뭐라뭐라하는거ㅎㅎ",
          "original_form": "아시죠? 그 잘때 뭐라뭐라하는거ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100004930.1.36",
          "form": "그때 아빠 사랑해 하는데 엄청 울었었어요ㅎㅎ",
          "original_form": "그때 아빠 사랑해 하는데 엄청 울었었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100004930.1.37",
          "form": "아이가.엄청 사랑스럽네요 ㅎ",
          "original_form": "아이가.엄청 사랑스럽네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100004930.1.38",
          "form": "전 잠자기 전이 지옥이라 너무 힘들어요",
          "original_form": "전 잠자기 전이 지옥이라 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004930.1.39",
          "form": "자고 있을때가 천사죠",
          "original_form": "자고 있을때가 천사죠",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004930.1.40",
          "form": "두명이라 더더욱 그럿겟어요ㅎ",
          "original_form": "두명이라 더더욱 그럿겟어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004930.1.41",
          "form": "전 맨날 이야기해줘요ㅎ",
          "original_form": "전 맨날 이야기해줘요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004930.1.42",
          "form": "네 패턴도 바껴서 시간이 금방가네요",
          "original_form": "네 패턴도 바껴서 시간이 금방가네요",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100004930.1.43",
          "form": "다정한 아빠네요 ㅎ",
          "original_form": "다정한 아빠네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100004930.1.44",
          "form": "무서운이야기ㅎㅎㅎ 맨날 지어내고ㅎ 결론은 똑같은거 엄마말 아빠말 잘안들으면 도깨비나 귀신이 잡아간다",
          "original_form": "무서운이야기ㅎㅎㅎ 맨날 지어내고ㅎ 결론은 똑같은거 엄마말 아빠말 잘안들으면 도깨비나 귀신이 잡아간다",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100004930.1.45",
          "form": "다정하다고하기보단 아이가 너무 좋아서 그래요ㅎ 내 자식이니깐요 ㅡㅎ",
          "original_form": "다정하다고하기보단 아이가 너무 좋아서 그래요ㅎ 내 자식이니깐요 ㅡㅎ",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100004930.1.46",
          "form": "저도 공룡나온다고 하고 재워요",
          "original_form": "저도 공룡나온다고 하고 재워요",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100004930.1.47",
          "form": "집집마다  다 똑같나봐요",
          "original_form": "집집마다  다 똑같나봐요",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100004930.1.48",
          "form": "오 공룡좋네요ㅎㅎ",
          "original_form": "오 공룡좋네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100004930.1.49",
          "form": "팁좀 공유해주세요ㅎ",
          "original_form": "팁좀 공유해주세요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100004930.1.50",
          "form": "육아팁ㅎ",
          "original_form": "육아팁ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100004930.1.51",
          "form": "그냥 공룡나온다고 하면 재울때 멀리 안가고 제옆에 눕더라고요 ㅎ",
          "original_form": "그냥 공룡나온다고 하면 재울때 멀리 안가고 제옆에 눕더라고요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:25"
        }
      ]
    }
