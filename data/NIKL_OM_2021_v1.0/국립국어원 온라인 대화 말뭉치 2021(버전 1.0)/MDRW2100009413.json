      "utterance": [
        {
          "id": "MDRW2100009413.1.1",
          "form": "여행계획있으세요?",
          "original_form": "여행계획있으세요?",
          "speaker_id": "1",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009413.1.2",
          "form": "전 당분간은 없어요~ name1님은요?",
          "original_form": "전 당분간은 없어요~ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009413.1.3",
          "form": "저두요ㅠㅠ 코로나때문에",
          "original_form": "저두요ㅠㅠ 코로나때문에",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009413.1.4",
          "form": "코로나 이전에 계획하던 여행 있으셨나요?",
          "original_form": "코로나 이전에 계획하던 여행 있으셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009413.1.5",
          "form": "일본여행 비행기표도 취소했답니다ㅠㅠ",
          "original_form": "일본여행 비행기표도 취소했답니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009413.1.6",
          "form": "여행경험있으신가요?",
          "original_form": "여행경험있으신가요?",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009413.1.7",
          "form": "헉 아쉽다ㅜㅜ",
          "original_form": "헉 아쉽다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009413.1.8",
          "form": "넵넵 여행 자주 다녔었어요~",
          "original_form": "넵넵 여행 자주 다녔었어요~",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009413.1.9",
          "form": "국내여행추천해주세요~",
          "original_form": "국내여행추천해주세요~",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009413.1.10",
          "form": "남자친구랑요!",
          "original_form": "남자친구랑요!",
          "speaker_id": "1",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009413.1.11",
          "form": "속초 추천해요",
          "original_form": "속초 추천해요",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009413.1.12",
          "form": "관광시장이 인가 많더라구요",
          "original_form": "관광시장이 인가 많더라구요",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009413.1.13",
          "form": "바다가좋죠!",
          "original_form": "바다가좋죠!",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009413.1.14",
          "form": "속초 대표음식은 뭔가요??",
          "original_form": "속초 대표음식은 뭔가요??",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009413.1.15",
          "form": "저는 최근에 막국수가 맛있었어요",
          "original_form": "저는 최근에 막국수가 맛있었어요",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009413.1.16",
          "form": "여행 가면 맛집 좋아하시나봐요",
          "original_form": "여행 가면 맛집 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009413.1.17",
          "form": "넵 ㅎㅎ 막국수가 또 맛있죠",
          "original_form": "넵 ㅎㅎ 막국수가 또 맛있죠",
          "speaker_id": "1",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009413.1.18",
          "form": "해외여행가보셨나요?",
          "original_form": "해외여행가보셨나요?",
          "speaker_id": "1",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009413.1.19",
          "form": "넵 그런데 일본은 못 가봤어요",
          "original_form": "넵 그런데 일본은 못 가봤어요",
          "speaker_id": "2",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009413.1.20",
          "form": "일본은 어떻게 계획하게 되셨나요?",
          "original_form": "일본은 어떻게 계획하게 되셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009413.1.21",
          "form": "어디갔다오셨어요?",
          "original_form": "어디갔다오셨어요?",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009413.1.22",
          "form": "일본은 친한친구랑 성인기념여행이요!",
          "original_form": "일본은 친한친구랑 성인기념여행이요!",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009413.1.23",
          "form": "저는 괌이 가장 기억에 남아요",
          "original_form": "저는 괌이 가장 기억에 남아요",
          "speaker_id": "2",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009413.1.24",
          "form": "일본 꼭 가보고 싶어요 ~",
          "original_form": "일본 꼭 가보고 싶어요 ~",
          "speaker_id": "2",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009413.1.25",
          "form": "괌! 꿈이죠ㅠㅠㅠ",
          "original_form": "괌! 꿈이죠ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 23:29"
        }
      ]
    }
