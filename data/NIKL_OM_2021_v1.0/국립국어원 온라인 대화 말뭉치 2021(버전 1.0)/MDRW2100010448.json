      "utterance": [
        {
          "id": "MDRW2100010448.1.1",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "1",
          "time": "20210823 18:23"
        },
        {
          "id": "MDRW2100010448.1.2",
          "form": "애인도 없어요ㅎㅎ",
          "original_form": "애인도 없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010448.1.3",
          "form": "앗 죄송합니다ㅜㅜ",
          "original_form": "앗 죄송합니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010448.1.4",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010448.1.5",
          "form": "아뇨 결혼은 안하고 결혼전제로 만나고있어요",
          "original_form": "아뇨 결혼은 안하고 결혼전제로 만나고있어요",
          "speaker_id": "1",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010448.1.6",
          "form": "얼마나되셨어요?",
          "original_form": "얼마나되셨어요?",
          "speaker_id": "2",
          "time": "20210823 18:24"
        },
        {
          "id": "MDRW2100010448.1.7",
          "form": "곧 6주년이에요",
          "original_form": "곧 6주년이에요",
          "speaker_id": "1",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010448.1.8",
          "form": "엄청나네요!",
          "original_form": "엄청나네요!",
          "speaker_id": "2",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010448.1.9",
          "form": "어떻게 만나셨어요?",
          "original_form": "어떻게 만나셨어요?",
          "speaker_id": "2",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010448.1.10",
          "form": "학교에서 선후배관계였다가 자연스레 만나게됐어요",
          "original_form": "학교에서 선후배관계였다가 자연스레 만나게됐어요",
          "speaker_id": "1",
          "time": "20210823 18:25"
        },
        {
          "id": "MDRW2100010448.1.11",
          "form": "낭만적이네요ㅜㅜ",
          "original_form": "낭만적이네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010448.1.12",
          "form": "저는 비혼으로 살다죽을 거같아요",
          "original_form": "저는 비혼으로 살다죽을 거같아요",
          "speaker_id": "2",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010448.1.13",
          "form": "왜용?",
          "original_form": "왜용?",
          "speaker_id": "1",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100010448.1.14",
          "form": "남이랑 같이 살 자신도 없고 혼자가 편해요",
          "original_form": "남이랑 같이 살 자신도 없고 혼자가 편해요",
          "speaker_id": "2",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010448.1.15",
          "form": "연애도 안하세용?",
          "original_form": "연애도 안하세용?",
          "speaker_id": "1",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010448.1.16",
          "form": "안하다보니 굳이 해야되는지도 모르겠고 혼자 노는게 익숙해요",
          "original_form": "안하다보니 굳이 해야되는지도 모르겠고 혼자 노는게 익숙해요",
          "speaker_id": "2",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010448.1.17",
          "form": "혼자만의 재미가 있죠ㅎㅎ 나중에 친구들 결혼하면 외롭지않을까요",
          "original_form": "혼자만의 재미가 있죠ㅎㅎ 나중에 친구들 결혼하면 외롭지않을까요",
          "speaker_id": "1",
          "time": "20210823 18:28"
        },
        {
          "id": "MDRW2100010448.1.18",
          "form": "그때가면 저도 찾아서 하겠죠 뭐ㅋㅋ 전 일이랑 결혼할래요ㅋㅋ",
          "original_form": "그때가면 저도 찾아서 하겠죠 뭐ㅋㅋ 전 일이랑 결혼할래요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:28"
        },
        {
          "id": "MDRW2100010448.1.19",
          "form": "최고네요ㅋㅋ",
          "original_form": "최고네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:29"
