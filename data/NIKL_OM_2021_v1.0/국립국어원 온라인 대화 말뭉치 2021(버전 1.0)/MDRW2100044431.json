        },
        {
          "id": "MDRW2100044431.1.2",
          "form": "뭐먹고싶어요?",
          "original_form": "뭐먹고싶어요?",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044431.1.3",
          "form": "저요.. 곱창 ?",
          "original_form": "저요.. 곱창 ?",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044431.1.4",
          "form": "ㅋㅋ야채곱창?",
          "original_form": "ㅋㅋ야채곱창?",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
