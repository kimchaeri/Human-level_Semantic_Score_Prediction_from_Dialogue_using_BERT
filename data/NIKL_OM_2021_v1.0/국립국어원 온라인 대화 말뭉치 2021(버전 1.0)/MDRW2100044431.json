          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044431.1.6",
          "form": "소곱창이요",
          "original_form": "소곱창이요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044431.1.7",
          "form": "근대 야곱도 땡겨요",
          "original_form": "근대 야곱도 땡겨요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044431.1.8",
          "form": "ㅋㅋ당면많이드릴까요??",
          "original_form": "ㅋㅋ당면많이드릴까요??",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044431.1.9",
          "form": "완전이요",
          "original_form": "완전이요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044431.1.10",
          "form": "깻잎도요",
          "original_form": "깻잎도요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044431.1.11",
