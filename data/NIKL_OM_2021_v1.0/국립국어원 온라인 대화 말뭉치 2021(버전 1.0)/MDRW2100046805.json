      "utterance": [
        {
          "id": "MDRW2100046805.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 14:41"
        },
        {
          "id": "MDRW2100046805.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211105 14:41"
        },
        {
          "id": "MDRW2100046805.1.3",
          "form": "요즘 즐겨보는 영화나 드라마 있으신가요!",
          "original_form": "요즘 즐겨보는 영화나 드라마 있으신가요!",
          "speaker_id": "2",
          "time": "20211105 14:42"
        },
        {
          "id": "MDRW2100046805.1.4",
          "form": "오징어게임봤어요",
          "original_form": "오징어게임봤어요",
          "speaker_id": "1",
          "time": "20211105 14:42"
        },
        {
          "id": "MDRW2100046805.1.5",
          "form": "역시! 요즘 제일 핫한 콘텐츠를 보셨군요",
          "original_form": "역시! 요즘 제일 핫한 콘텐츠를 보셨군요",
          "speaker_id": "2",
          "time": "20211105 14:42"
        },
        {
          "id": "MDRW2100046805.1.6",
          "form": "네 제일 핫하긴 하죠",
          "original_form": "네 제일 핫하긴 하죠",
          "speaker_id": "1",
          "time": "20211105 14:42"
        },
        {
          "id": "MDRW2100046805.1.7",
          "form": "저는 내용이 다소 충격적이라.. 무서워서 보다가 중단했어요",
          "original_form": "저는 내용이 다소 충격적이라.. 무서워서 보다가 중단했어요",
          "speaker_id": "2",
          "time": "20211105 14:43"
        },
        {
          "id": "MDRW2100046805.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20211105 14:43"
        },
        {
          "id": "MDRW2100046805.1.9",
          "form": "그럴수도 있겠네요",
          "original_form": "그럴수도 있겠네요",
          "speaker_id": "1",
          "time": "20211105 14:43"
        },
        {
          "id": "MDRW2100046805.1.10",
          "form": "무궁화꽃이 피었습니다 게임에서부터 사람이 죽어서",
          "original_form": "무궁화꽃이 피었습니다 게임에서부터 사람이 죽어서",
          "speaker_id": "2",
          "time": "20211105 14:43"
        },
        {
          "id": "MDRW2100046805.1.11",
          "form": "깜짝놀랐네요 ㅋㅋ",
          "original_form": "깜짝놀랐네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 14:44"
        },
        {
          "id": "MDRW2100046805.1.12",
          "form": "저도 깜놀했어요 ㅋ",
          "original_form": "저도 깜놀했어요 ㅋ",
          "speaker_id": "1",
          "time": "20211105 14:44"
        },
        {
          "id": "MDRW2100046805.1.13",
          "form": "끝까지 다 보신건가요?",
          "original_form": "끝까지 다 보신건가요?",
          "speaker_id": "2",
          "time": "20211105 14:44"
        },
        {
          "id": "MDRW2100046805.1.14",
          "form": "네 다봤어요",
          "original_form": "네 다봤어요",
          "speaker_id": "1",
          "time": "20211105 14:44"
        },
        {
          "id": "MDRW2100046805.1.15",
          "form": "저는 이병헌이 충격이었다는",
          "original_form": "저는 이병헌이 충격이었다는",
          "speaker_id": "1",
          "time": "20211105 14:44"
        },
        {
          "id": "MDRW2100046805.1.16",
          "form": "전혀 몰랐는데",
          "original_form": "전혀 몰랐는데",
          "speaker_id": "1",
          "time": "20211105 14:44"
        },
        {
          "id": "MDRW2100046805.1.17",
          "form": "다른 사람들은",
          "original_form": "다른 사람들은",
          "speaker_id": "1",
          "time": "20211105 14:44"
        },
        {
          "id": "MDRW2100046805.1.18",
          "form": "알았다던데",
          "original_form": "알았다던데",
          "speaker_id": "1",
          "time": "20211105 14:45"
        },
        {
          "id": "MDRW2100046805.1.19",
          "form": "앗ㅋㅋ 저는 목소리 듣고 알았어요",
          "original_form": "앗ㅋㅋ 저는 목소리 듣고 알았어요",
          "speaker_id": "2",
          "time": "20211105 14:45"
        },
        {
          "id": "MDRW2100046805.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20211105 14:45"
        },
        {
          "id": "MDRW2100046805.1.21",
          "form": "영어 목소리가 누가봐도 이병헌",
          "original_form": "영어 목소리가 누가봐도 이병헌",
          "speaker_id": "2",
          "time": "20211105 14:45"
        },
        {
          "id": "MDRW2100046805.1.22",
          "form": "저는 전혀 몰랐어요",
          "original_form": "저는 전혀 몰랐어요",
          "speaker_id": "1",
          "time": "20211105 14:45"
        },
        {
          "id": "MDRW2100046805.1.23",
          "form": "저는 그 미스터션샤인에서 질리도록 이병헌 목소리를 들었어서",
          "original_form": "저는 그 미스터션샤인에서 질리도록 이병헌 목소리를 들었어서",
          "speaker_id": "2",
          "time": "20211105 14:45"
        },
        {
          "id": "MDRW2100046805.1.24",
          "form": "금방 알아챘나봐요",
          "original_form": "금방 알아챘나봐요",
          "speaker_id": "2",
          "time": "20211105 14:45"
        }
      ]
