      "utterance": [
        {
          "id": "MDRW2100025044.1.1",
          "form": "강동이면",
          "original_form": "강동이면",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100025044.1.2",
          "form": "하남이랑 가깝지 않나요?",
          "original_form": "하남이랑 가깝지 않나요?",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025044.1.3",
          "form": "하남스타필드에도 맛집이 많은,,,",
          "original_form": "하남스타필드에도 맛집이 많은,,,",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025044.1.4",
          "form": "네 ㅋㅋㅋㅋ 저어릴때 하남 살았었어요",
          "original_form": "네 ㅋㅋㅋㅋ 저어릴때 하남 살았었어요",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025044.1.5",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025044.1.6",
          "form": "그당시엔...아무것도없어서",
          "original_form": "그당시엔...아무것도없어서",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025044.1.7",
          "form": "롯데리아하나있었는데",
          "original_form": "롯데리아하나있었는데",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025044.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025044.1.9",
          "form": "요샌 정말많이발전했더라구요",
          "original_form": "요샌 정말많이발전했더라구요",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025044.1.10",
          "form": "그럼 롯데리아가 맛집이죠..!",
          "original_form": "그럼 롯데리아가 맛집이죠..!",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025044.1.11",
          "form": "저살때는 대형마트도없고",
          "original_form": "저살때는 대형마트도없고",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025044.1.12",
          "form": "영화관도 서울로 나들이가서보고그랬는데",
          "original_form": "영화관도 서울로 나들이가서보고그랬는데",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025044.1.13",
          "form": "ㅋㅋㄱㅋ스타필드가 살던동네 바로옆에 생겨서 깜짝놀랐어요",
          "original_form": "ㅋㅋㄱㅋ스타필드가 살던동네 바로옆에 생겨서 깜짝놀랐어요",
          "speaker_id": "2",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025044.1.14",
          "form": "요샌 코스트코도 들어왔더라구요",
          "original_form": "요샌 코스트코도 들어왔더라구요",
          "speaker_id": "2",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025044.1.15",
          "form": "서울 바로옆인데도 그렇군여..",
          "original_form": "서울 바로옆인데도 그렇군여..",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025044.1.16",
          "form": "오 코스트코도 맛집이져..",
          "original_form": "오 코스트코도 맛집이져..",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025044.1.17",
          "form": "가끔 가서 피자사먹고...",
          "original_form": "가끔 가서 피자사먹고...",
          "speaker_id": "2",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025044.1.18",
          "form": "헐 근데 그러면",
          "original_form": "헐 근데 그러면",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025044.1.19",
          "form": "치즈피자맛집..",
          "original_form": "치즈피자맛집..",
          "speaker_id": "2",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025044.1.20",
          "form": "한지역에 스타필드랑 코스트코가 다 있는 거예요?",
          "original_form": "한지역에 스타필드랑 코스트코가 다 있는 거예요?",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025044.1.21",
          "form": "천국이다",
          "original_form": "천국이다",
          "speaker_id": "1",
          "time": "20210831 22:43"
        },
        {
          "id": "MDRW2100025044.1.22",
          "form": "네 그렇더라구요",
          "original_form": "네 그렇더라구요",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100025044.1.23",
          "form": "일산도 그렇죠 참",
          "original_form": "일산도 그렇죠 참",
          "speaker_id": "1",
          "time": "20210831 22:44"
        }
      ]
