      "utterance": [
        {
          "id": "MDRW2100011527.1.1",
          "form": "거기 비와?",
          "original_form": "거기 비와?",
          "speaker_id": "2",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011527.1.2",
          "form": "엄청 쏟아지다가 막 그쳤어",
          "original_form": "엄청 쏟아지다가 막 그쳤어",
          "speaker_id": "1",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011527.1.3",
          "form": "습하니까 덥다",
          "original_form": "습하니까 덥다",
          "speaker_id": "1",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011527.1.4",
          "form": "응응 습해서 에어컨을 켜야겠음...",
          "original_form": "응응 습해서 에어컨을 켜야겠음...",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011527.1.5",
          "form": "응 나도 켰다ㅋ",
          "original_form": "응 나도 켰다ㅋ",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011527.1.6",
          "form": "복직용 옷 아직우 여름옷 사야겠지?",
          "original_form": "복직용 옷 아직우 여름옷 사야겠지?",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011527.1.7",
          "form": "아직은",
          "original_form": "아직은",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011527.1.8",
          "form": "응응 9월은 여름 옷 입을거같아~",
          "original_form": "응응 9월은 여름 옷 입을거같아~",
          "speaker_id": "2",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011527.1.9",
          "form": "살찌니까 옷 보기도 싫더라구 ㅜㅜ 그래도 안입을수없으니 샀어ㅋㅋ",
          "original_form": "살찌니까 옷 보기도 싫더라구 ㅜㅜ 그래도 안입을수없으니 샀어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011527.1.10",
          "form": "임부복 오해받을지도ㅋㅋ",
          "original_form": "임부복 오해받을지도ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011527.1.11",
          "form": "옷이라도 편해야...학교서 숨이라도 쉰다..ㅎ 잘했어!",
          "original_form": "옷이라도 편해야...학교서 숨이라도 쉰다..ㅎ 잘했어!",
          "speaker_id": "2",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100011527.1.12",
          "form": "ㅋㅋㅋ 고마워ㅠ 숨은 잘쉴듯",
          "original_form": "ㅋㅋㅋ 고마워ㅠ 숨은 잘쉴듯",
          "speaker_id": "1",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011527.1.13",
          "form": "날씨가 흐렸다 맑았다 난리났네",
          "original_form": "날씨가 흐렸다 맑았다 난리났네",
          "speaker_id": "1",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011527.1.14",
          "form": "응응 아직 반팔입지? 잘때드",
          "original_form": "응응 아직 반팔입지? 잘때드",
          "speaker_id": "2",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011527.1.15",
          "form": "응 바지도 반바지입어ㅋ",
          "original_form": "응 바지도 반바지입어ㅋ",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011527.1.16",
          "form": "맞어 막땀도 줄줄 나더라구 감기 걸릴까봐 걱정",
          "original_form": "맞어 막땀도 줄줄 나더라구 감기 걸릴까봐 걱정",
          "speaker_id": "2",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011527.1.17",
          "form": "환절기라ㅜ조심해야해",
          "original_form": "환절기라ㅜ조심해야해",
          "speaker_id": "1",
