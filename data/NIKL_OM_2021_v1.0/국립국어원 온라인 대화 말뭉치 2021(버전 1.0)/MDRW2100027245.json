      "utterance": [
        {
          "id": "MDRW2100027245.1.1",
          "form": "곰표맥주 드셔보셨나요?",
          "original_form": "곰표맥주 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027245.1.2",
          "form": "아뇨 아직 못 먹어봤어요 ㅠㅠ",
          "original_form": "아뇨 아직 못 먹어봤어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027245.1.3",
          "form": "드셔보셨어요??",
          "original_form": "드셔보셨어요??",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027245.1.4",
          "form": "네 요새는 구하기 어렵진 않더라구요ㅎㅎ",
          "original_form": "네 요새는 구하기 어렵진 않더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027245.1.5",
          "form": "오 ㅎㅎ 다행이네요",
          "original_form": "오 ㅎㅎ 다행이네요",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027245.1.6",
          "form": "어떤 맛인가요?? 궁금하네요",
          "original_form": "어떤 맛인가요?? 궁금하네요",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027245.1.7",
          "form": "과일맛이에요 블랑이랑 비슷한맛!",
          "original_form": "과일맛이에요 블랑이랑 비슷한맛!",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027245.1.8",
          "form": "다른 콜라보 제품 산 적 있으신가요???",
          "original_form": "다른 콜라보 제품 산 적 있으신가요???",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027245.1.9",
          "form": "오 맛있을 것 같아요!",
          "original_form": "오 맛있을 것 같아요!",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027245.1.10",
          "form": "네 저 진로이즈백 마셔봤어요 ㅎㅎ",
          "original_form": "네 저 진로이즈백 마셔봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027245.1.11",
          "form": "진로 두꺼비 너무 귀엽더라구요",
          "original_form": "진로 두꺼비 너무 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100027245.1.12",
          "form": "진로이즈백 귀엽죠 감자칩도 나온거 보셨나요?ㅋㅋㅋ",
          "original_form": "진로이즈백 귀엽죠 감자칩도 나온거 보셨나요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100027245.1.13",
          "form": "헐 진짜네요 ㅋㅋㅋㅋㅋ",
          "original_form": "헐 진짜네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027245.1.14",
          "form": "너무 귀여워요...",
          "original_form": "너무 귀여워요...",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027245.1.15",
          "form": "굿즈가 엄청 다양하네요!",
          "original_form": "굿즈가 엄청 다양하네요!",
          "speaker_id": "2",
          "time": "20210902 13:09"
        },
        {
          "id": "MDRW2100027245.1.16",
          "form": "진로가 이런걸 잘하나봐요!",
          "original_form": "진로가 이런걸 잘하나봐요!",
          "speaker_id": "1",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100027245.1.17",
          "form": "그러게요!",
          "original_form": "그러게요!",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027245.1.18",
          "form": "보니까 키링이 제일 갖고싶네요 ㅎㅎ",
          "original_form": "보니까 키링이 제일 갖고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027245.1.19",
          "form": "키링이 굿즈로 제일 사기 좋은거 같아요",
          "original_form": "키링이 굿즈로 제일 사기 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210902 13:43"
        },
        {
          "id": "MDRW2100027245.1.20",
          "form": "그쵸 저금통도 너무 귀엽네요 ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 저금통도 너무 귀엽네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027245.1.21",
          "form": "전 편의점을 자주 가는 편인데 요새 편의점에 이런 콜라보 상품이 많은 거 같아요",
          "original_form": "전 편의점을 자주 가는 편인데 요새 편의점에 이런 콜라보 상품이 많은 거 같아요",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027245.1.22",
          "form": "맞아요 편의점에 많아서 더 좋은 것 같아용",
          "original_form": "맞아요 편의점에 많아서 더 좋은 것 같아용",
          "speaker_id": "2",
          "time": "20210902 14:56"
        },
        {
          "id": "MDRW2100027245.1.23",
          "form": "몰랐는데 진로랑 김치 불닭볶음면이랑 콜라보 한 적이 있네요",
          "original_form": "몰랐는데 진로랑 김치 불닭볶음면이랑 콜라보 한 적이 있네요",
          "speaker_id": "2",
          "time": "20210902 14:57"
        },
        {
          "id": "MDRW2100027245.1.24",
          "form": "헐 신기하네요",
          "original_form": "헐 신기하네요",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027245.1.25",
          "form": "이런거 만드는 사람들 창의력이 대단한거 가탕요",
          "original_form": "이런거 만드는 사람들 창의력이 대단한거 가탕요",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027245.1.26",
          "form": "맞아요 머리 아플 것 같아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 머리 아플 것 같아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:11"
        }
      ]
    }
