      "utterance": [
        {
          "id": "MDRW2100003021.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100003021.1.2",
          "form": "넵 name1님~~ 간만에 이야기 하는 기분이네여 ㅎㅎ",
          "original_form": "넵 &name1&님~~ 간만에 이야기 하는 기분이네여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100003021.1.3",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100003021.1.4",
          "form": "아이는 놀고 있나요?",
          "original_form": "아이는 놀고 있나요?",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100003021.1.5",
          "form": "자나요?",
          "original_form": "자나요?",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100003021.1.6",
          "form": "저희 애들은 어릴때 너무 늦게자서 힘들었어요",
          "original_form": "저희 애들은 어릴때 너무 늦게자서 힘들었어요",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100003021.1.7",
          "form": "층간소음이 늦게자면 밤에 발생 하니까요",
          "original_form": "층간소음이 늦게자면 밤에 발생 하니까요",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100003021.1.8",
          "form": "넘 힘들더라구요",
          "original_form": "넘 힘들더라구요",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100003021.1.9",
          "form": "저희는 주택이라 ㅎㅎㅎ",
          "original_form": "저희는 주택이라 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100003021.1.10",
          "form": "그런 걱정은 없어요",
          "original_form": "그런 걱정은 없어요",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100003021.1.11",
          "form": "오 너무 이상적이네요",
          "original_form": "오 너무 이상적이네요",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100003021.1.12",
          "form": "전 아파트만 살아서 힘들었어요",
          "original_form": "전 아파트만 살아서 힘들었어요",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100003021.1.13",
          "form": "에궁 그러셨구낭  ~~",
          "original_form": "에궁 그러셨구낭  ~~",
          "speaker_id": "2",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100003021.1.14",
          "form": "이제 다 크셔서 다행일거 같은 느낌 ? 이네요 ㅎㅎ",
          "original_form": "이제 다 크셔서 다행일거 같은 느낌 ? 이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100003021.1.15",
          "form": "근데 저 아들놈이 나이값 못하고",
          "original_form": "근데 저 아들놈이 나이값 못하고",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100003021.1.16",
          "form": "뜁니다 ㅡㅡ",
          "original_form": "뜁니다 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100003021.1.17",
          "form": "몸은 어떠세요?",
          "original_form": "몸은 어떠세요?",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100003021.1.18",
          "form": "비도오고 힘들지 않으세요?",
          "original_form": "비도오고 힘들지 않으세요?",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100003021.1.19",
          "form": "아 저는 코로나때문에   무기력한거 빼곤 괜찮아요 ㅎㅎ",
          "original_form": "아 저는 코로나때문에   무기력한거 빼곤 괜찮아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100003021.1.20",
          "form": "name1님은  비오면 몸 아프시군요 ㅠㅠ",
          "original_form": "&name1&님은  비오면 몸 아프시군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100003021.1.21",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100003021.1.22",
          "form": "전 무릎 수술해서 좀 그래요",
          "original_form": "전 무릎 수술해서 좀 그래요",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100003021.1.23",
          "form": "예전에 맨홀에 빠진적 있어요 ㅋㅋ",
          "original_form": "예전에 맨홀에 빠진적 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100003021.1.24",
          "form": "아공  무릎 수술  ㅜㅜ",
          "original_form": "아공  무릎 수술  ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100003021.1.25",
          "form": "수술한자리는 날씨 안좋으면 바로 티난다 하더라구요",
          "original_form": "수술한자리는 날씨 안좋으면 바로 티난다 하더라구요",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100003021.1.26",
          "form": "네 티나요 땡겨요",
          "original_form": "네 티나요 땡겨요",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100003021.1.27",
          "form": "집안일 조금만 하셔야 겠어요 ㅜㅜ",
          "original_form": "집안일 조금만 하셔야 겠어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100003021.1.28",
          "form": "아프면 더하기 싫은게 집안일",
          "original_form": "아프면 더하기 싫은게 집안일",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100003021.1.29",
          "form": "맨날 하기 싫긴해요",
          "original_form": "맨날 하기 싫긴해요",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100003021.1.30",
          "form": "밤이 한가하시죠?",
          "original_form": "밤이 한가하시죠?",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100003021.1.31",
          "form": "항상 낮엔 뭔가 여유가 없어요",
          "original_form": "항상 낮엔 뭔가 여유가 없어요",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100003021.1.32",
          "form": "ㅎㅎ그런거 같아요 오롯이 ~ 혼자만의 시간 ㅎㅎㅎ",
          "original_form": "ㅎㅎ그런거 같아요 오롯이 ~ 혼자만의 시간 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100003021.1.33",
          "form": "네 엄마들의 자유시간이죠",
          "original_form": "네 엄마들의 자유시간이죠",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100003021.1.34",
          "form": "이때 만화도 보고 핸드폰으로 막 여가보내요",
          "original_form": "이때 만화도 보고 핸드폰으로 막 여가보내요",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100003021.1.35",
          "form": "ㅎㅎㅎ 동감이요",
          "original_form": "ㅎㅎㅎ 동감이요",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100003021.1.36",
          "form": "카카오 페이지도 보고요",
          "original_form": "카카오 페이지도 보고요",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100003021.1.37",
          "form": "아 게임도 하고요",
          "original_form": "아 게임도 하고요",
          "speaker_id": "1",
          "time": "20210821 22:54"
        }
      ]
