        },
        {
          "id": "MDRW2100019112.1.2",
          "form": "안냥하세요",
          "original_form": "안냥하세요",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019112.1.3",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019112.1.4",
          "form": "전 야채곱창이요 ㅋㅋ",
          "original_form": "전 야채곱창이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019112.1.5",
          "form": "곱창 좋아하는 분들 많더라구요 제친구들만 해도 ㅋㅋㅋㅋ",
          "original_form": "곱창 좋아하는 분들 많더라구요 제친구들만 해도 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019112.1.6",
          "form": "맛 미쳤어요",
          "original_form": "맛 미쳤어요",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019112.1.7",
          "form": "헠핰 또먹고싶다",
          "original_form": "헠핰 또먹고싶다",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019112.1.8",
          "form": "name1님은 name1를 좋아하시나요",
          "original_form": "&name1&님은 &name1&를 좋아하시나요",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100019112.1.9",
          "form": "네 너무 좋아해요 ㅋㅋㅋ",
          "original_form": "네 너무 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019112.1.10",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "1",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019112.1.11",
          "form": "배떡 먹어봤어요",
          "original_form": "배떡 먹어봤어요",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019112.1.12",
          "form": "근데 엽떡 로제가 더 맛있다규 해서",
          "original_form": "근데 엽떡 로제가 더 맛있다규 해서",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019112.1.13",
          "form": "먹어보려구오",
          "original_form": "먹어보려구오",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019112.1.14",
          "form": "오그래요? 엽떡도 궁금하네요 안 먹어봤는데",
          "original_form": "오그래요? 엽떡도 궁금하네요 안 먹어봤는데",
          "speaker_id": "1",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019112.1.15",
          "form": "어디거 먹어보셨나요",
          "original_form": "어디거 먹어보셨나요",
          "speaker_id": "2",
          "time": "20210827 19:16"
        },
        {
          "id": "MDRW2100019112.1.16",
          "form": "전 신전 배떡 스쿨푸드 먹어본거같아요",
          "original_form": "전 신전 배떡 스쿨푸드 먹어본거같아요",
          "speaker_id": "1",
          "time": "20210827 19:16"
        },
        {
          "id": "MDRW2100019112.1.17",
          "form": "오오 뭐가 제일 낫나요",
          "original_form": "오오 뭐가 제일 낫나요",
          "speaker_id": "2",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019112.1.18",
          "form": "신전도 있는지 몰랐네용",
          "original_form": "신전도 있는지 몰랐네용",
          "speaker_id": "2",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019112.1.19",
          "form": "전 신전이 양도 적당하고 꾸덕해거",
          "original_form": "전 신전이 양도 적당하고 꾸덕해거",
          "speaker_id": "1",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019112.1.20",
          "form": "좋더라구요",
          "original_form": "좋더라구요",
          "speaker_id": "1",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019112.1.21",
          "form": "신전 거 먹어볼게요",
          "original_form": "신전 거 먹어볼게요",
