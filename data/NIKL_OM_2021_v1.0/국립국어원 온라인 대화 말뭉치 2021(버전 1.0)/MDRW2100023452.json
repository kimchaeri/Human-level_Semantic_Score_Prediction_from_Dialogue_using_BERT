      "utterance": [
        {
          "id": "MDRW2100023452.1.1",
          "form": "이런게 조은건...어쩔수업어",
          "original_form": "이런게 조은건...어쩔수업어",
          "speaker_id": "2",
          "time": "20210830 03:22"
        },
        {
          "id": "MDRW2100023452.1.2",
          "form": "그래서자주",
          "original_form": "그래서자주",
          "speaker_id": "1",
          "time": "20210830 03:23"
        },
        {
          "id": "MDRW2100023452.1.3",
          "form": "그 뭐냐",
          "original_form": "그 뭐냐",
          "speaker_id": "2",
          "time": "20210830 03:23"
        },
        {
          "id": "MDRW2100023452.1.4",
          "form": "중고거래에서",
          "original_form": "중고거래에서",
          "speaker_id": "1",
          "time": "20210830 03:23"
        },
        {
          "id": "MDRW2100023452.1.5",
          "form": "사기를당하는걸수도있어",
          "original_form": "사기를당하는걸수도있어",
          "speaker_id": "1",
          "time": "20210830 03:23"
        },
        {
          "id": "MDRW2100023452.1.6",
          "form": "바이린?거기서 옷 산다는건 어떻해됐어",
          "original_form": "바이린?거기서 옷 산다는건 어떻해됐어",
          "speaker_id": "2",
          "time": "20210830 03:23"
        },
        {
          "id": "MDRW2100023452.1.7",
          "form": "나 한번빠께 사기 안당햇어",
          "original_form": "나 한번빠께 사기 안당햇어",
          "speaker_id": "2",
          "time": "20210830 03:23"
        },
        {
          "id": "MDRW2100023452.1.8",
          "form": "공짜를좋아해서 아낀돈으로 사기꾼들에게",
          "original_form": "공짜를좋아해서 아낀돈으로 사기꾼들에게",
          "speaker_id": "1",
          "time": "20210830 03:23"
        },
        {
          "id": "MDRW2100023452.1.9",
          "form": "옷은 아직",
          "original_form": "옷은 아직",
          "speaker_id": "1",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.10",
          "form": "거기서말고",
          "original_form": "거기서말고",
          "speaker_id": "1",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.11",
          "form": "넌 정말",
          "original_form": "넌 정말",
          "speaker_id": "2",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.12",
          "form": "나쁜 아이야",
          "original_form": "나쁜 아이야",
          "speaker_id": "2",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.13",
          "form": "퍼빗에서 블라우스하나샀어",
          "original_form": "퍼빗에서 블라우스하나샀어",
          "speaker_id": "1",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.14",
          "form": "워치 스트랩도 하나 사고",
          "original_form": "워치 스트랩도 하나 사고",
          "speaker_id": "1",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.15",
          "form": "퍼빗은 맨날",
          "original_form": "퍼빗은 맨날",
          "speaker_id": "2",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.16",
          "form": "오늘 지름신 미쳤다리",
          "original_form": "오늘 지름신 미쳤다리",
          "speaker_id": "1",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.17",
          "form": "지네 옷이 거래처 최상이고",
          "original_form": "지네 옷이 거래처 최상이고",
          "speaker_id": "2",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.18",
          "form": "ㄴㄴ성공한거 좀 있었음",
          "original_form": "ㄴㄴ성공한거 좀 있었음",
          "speaker_id": "1",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.19",
          "form": "옷감 5번 넘게 태스트 해보고",
          "original_form": "옷감 5번 넘게 태스트 해보고",
          "speaker_id": "2",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.20",
          "form": "여리여ㅕ리핏아고",
          "original_form": "여리여ㅕ리핏아고",
          "speaker_id": "2",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.21",
          "form": "옛날 그 카라틴 디자인이 다시 나왔길래",
          "original_form": "옛날 그 카라틴 디자인이 다시 나왔길래",
          "speaker_id": "1",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.22",
          "form": "샀어",
          "original_form": "샀어",
          "speaker_id": "1",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.23",
          "form": "그래서 킹받ㄱ아",
          "original_form": "그래서 킹받ㄱ아",
          "speaker_id": "2",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.24",
          "form": "아니근데내가조아서산건데 초칠래!?",
          "original_form": "아니근데내가조아서산건데 초칠래!?",
          "speaker_id": "1",
          "time": "20210830 03:24"
        },
        {
          "id": "MDRW2100023452.1.25",
          "form": "오 워치 스트랩... 워치 잘 이용해?",
          "original_form": "오 워치 스트랩... 워치 잘 이용해?",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.26",
          "form": "미안해...",
          "original_form": "미안해...",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.27",
          "form": "내가직업없다고꼽주는거지지금?",
          "original_form": "내가직업없다고꼽주는거지지금?",
          "speaker_id": "1",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.28",
          "form": "사실 나도 사고싶은데",
          "original_form": "사실 나도 사고싶은데",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.29",
          "form": "질투가 나서그랬어",
          "original_form": "질투가 나서그랬어",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.30",
          "form": "난 그런거 하나 살때마다",
          "original_form": "난 그런거 하나 살때마다",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.31",
          "form": "손 벌벌ㄹㄹ",
          "original_form": "손 벌벌ㄹㄹ",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.32",
          "form": "할인정보 찾아보고",
          "original_form": "할인정보 찾아보고",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.33",
          "form": "그냥 시계차고싶을때 쓰기 좋은거같아",
          "original_form": "그냥 시계차고싶을때 쓰기 좋은거같아",
          "speaker_id": "1",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.34",
          "form": "적립방법 찾아보고",
          "original_form": "적립방법 찾아보고",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.35",
          "form": "에누리에서 검색해서 최저가 찾아보고",
          "original_form": "에누리에서 검색해서 최저가 찾아보고",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.36",
          "form": "우리나이가 이제",
          "original_form": "우리나이가 이제",
          "speaker_id": "1",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.37",
          "form": "그럭개 살아서 그래",
          "original_form": "그럭개 살아서 그래",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.38",
          "form": "ost는 쓰기그렇잖아",
          "original_form": "ost는 쓰기그렇잖아",
          "speaker_id": "1",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.39",
          "form": "근데 워치는 그냥가격은 평준화돼잇고",
          "original_form": "근데 워치는 그냥가격은 평준화돼잇고",
          "speaker_id": "1",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.40",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.41",
          "form": "ost 그거",
          "original_form": "ost 그거",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.42",
          "form": "부자들도쓰니까",
          "original_form": "부자들도쓰니까",
          "speaker_id": "1",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.43",
          "form": "프로포즈 링으로 쓰는 사람도 잇대",
          "original_form": "프로포즈 링으로 쓰는 사람도 잇대",
          "speaker_id": "2",
          "time": "20210830 03:25"
        },
        {
          "id": "MDRW2100023452.1.44",
          "form": "워치 그거 머",
          "original_form": "워치 그거 머",
