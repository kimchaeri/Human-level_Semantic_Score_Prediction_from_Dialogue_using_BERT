      "utterance": [
        {
          "id": "MDRW2100013375.1.1",
          "form": "좋아하시는 게임 있으신가요?",
          "original_form": "좋아하시는 게임 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100013375.1.2",
          "form": "오버워치도 좋아하구요",
          "original_form": "오버워치도 좋아하구요",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100013375.1.3",
          "form": "스팀게임 스위치게임 다 좋아해요",
          "original_form": "스팀게임 스위치게임 다 좋아해요",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100013375.1.4",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100013375.1.5",
          "form": "스위치 게임 재밌는 것같아요",
          "original_form": "스위치 게임 재밌는 것같아요",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100013375.1.6",
          "form": "폰게임은 안하시는 편인가요?",
          "original_form": "폰게임은 안하시는 편인가요?",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100013375.1.7",
          "form": "네 폰게임은 안해요",
          "original_form": "네 폰게임은 안해요",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100013375.1.8",
          "form": "스위치게임 하시나요?",
          "original_form": "스위치게임 하시나요?",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100013375.1.9",
          "form": "없어서 잘 못해요ㅠ",
          "original_form": "없어서 잘 못해요ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100013375.1.10",
          "form": "친구 집에 놀러갔을때",
          "original_form": "친구 집에 놀러갔을때",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100013375.1.11",
          "form": "한번만ㅠ 하고 하는 편이에요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "한번만ㅠ 하고 하는 편이에요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100013375.1.12",
          "form": "아ㅋㅋㅋㅋ 간편하게 한두시간 한두판 짧게해도 재밌는게 스위치 게임 같아요",
          "original_form": "아ㅋㅋㅋㅋ 간편하게 한두시간 한두판 짧게해도 재밌는게 스위치 게임 같아요",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100013375.1.13",
          "form": "맞아요 스위치 게임 중에 뭘 제일 좋아하세요?",
          "original_form": "맞아요 스위치 게임 중에 뭘 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100013375.1.14",
          "form": "지금은 동숲을 주로 하고 있어요",
          "original_form": "지금은 동숲을 주로 하고 있어요",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100013375.1.15",
          "form": "헉 저두 동숲 좋아해요...",
          "original_form": "헉 저두 동숲 좋아해요...",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100013375.1.16",
          "form": "포켓캠프로 그냥 만족..하고 있슴다",
          "original_form": "포켓캠프로 그냥 만족..하고 있슴다",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100013375.1.17",
          "form": "포켓캠프 하시다가 스위치동숲으로 넘어오시죠!",
          "original_form": "포켓캠프 하시다가 스위치동숲으로 넘어오시죠!",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100013375.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ그것도 좋을 것같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그것도 좋을 것같아요",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100013375.1.19",
          "form": "왠지 포켓캠프는 현질을 유도하는 것같단말이죠...?",
          "original_form": "왠지 포켓캠프는 현질을 유도하는 것같단말이죠...?",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100013375.1.20",
          "form": "현질유도가 있구나...",
          "original_form": "현질유도가 있구나...",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100013375.1.21",
          "form": "모바일은 그런점이 싫어요",
          "original_form": "모바일은 그런점이 싫어요",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100013375.1.22",
          "form": "그쵸ㅠㅋㅋ 근데 또 예쁜 한정판 의상이나 머리나 그런게ㅜ나오면....",
          "original_form": "그쵸ㅠㅋㅋ 근데 또 예쁜 한정판 의상이나 머리나 그런게ㅜ나오면....",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100013375.1.23",
          "form": "흑흑 텅장",
          "original_form": "흑흑 텅장",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100013375.1.24",
          "form": "스위치를 삽시다! 사서 마이디자인 하셔요ㅠㅠ",
          "original_form": "스위치를 삽시다! 사서 마이디자인 하셔요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100013375.1.25",
          "form": "후하후하 미래의 나에게 맡기겠어요",
          "original_form": "후하후하 미래의 나에게 맡기겠어요",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100013375.1.26",
          "form": "열심히 돈 벌어라",
          "original_form": "열심히 돈 벌어라",
          "speaker_id": "2",
          "time": "20210825 23:42"
        }
      ]
    }
  ]
