      "utterance": [
        {
          "id": "MDRW2100042901.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 11:00"
        },
        {
          "id": "MDRW2100042901.1.2",
          "form": "좋아하는 유튜버가 누군가요?",
          "original_form": "좋아하는 유튜버가 누군가요?",
          "speaker_id": "1",
          "time": "20210907 11:00"
        },
        {
          "id": "MDRW2100042901.1.3",
          "form": "저는 자주는 안보는데 입짧은 햇님 좋아해요",
          "original_form": "저는 자주는 안보는데 입짧은 햇님 좋아해요",
          "speaker_id": "2",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100042901.1.4",
          "form": "오오 저도 햇님 좋아해요",
          "original_form": "오오 저도 햇님 좋아해요",
          "speaker_id": "1",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100042901.1.5",
          "form": "웃을때 너무 귀여우세요",
          "original_form": "웃을때 너무 귀여우세요",
          "speaker_id": "1",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100042901.1.6",
          "form": "먹방 자주 보세요?",
          "original_form": "먹방 자주 보세요?",
          "speaker_id": "2",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100042901.1.7",
          "form": "종종 봐요 대리 만족으로 ㅎㅎ",
          "original_form": "종종 봐요 대리 만족으로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100042901.1.8",
          "form": "나도 저렇게 먹고 싶다,, 하면성ㅅ",
          "original_form": "나도 저렇게 먹고 싶다,, 하면성ㅅ",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100042901.1.9",
          "form": "하면서요",
          "original_form": "하면서요",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100042901.1.10",
          "form": "우아 저도요 !!",
          "original_form": "우아 저도요 !!",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100042901.1.11",
          "form": "대식가들 너무 부러우ㅜ요",
          "original_form": "대식가들 너무 부러우ㅜ요",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100042901.1.12",
          "form": "마자요 ㅠㅠㅠㅠ",
          "original_form": "마자요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100042901.1.13",
          "form": "저도 딱 식탐은 많은데 배가 안 따라주는.. ㅠㅠ맨날 남겨요",
          "original_form": "저도 딱 식탐은 많은데 배가 안 따라주는.. ㅠㅠ맨날 남겨요",
          "speaker_id": "1",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100042901.1.14",
          "form": "저도요!!",
          "original_form": "저도요!!",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100042901.1.15",
          "form": "그걸로 돈도 벌고 너무 부러워요",
          "original_form": "그걸로 돈도 벌고 너무 부러워요",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100042901.1.16",
          "form": "ㅠㅠ그쵸",
          "original_form": "ㅠㅠ그쵸",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100042901.1.17",
          "form": "저는 쿠킹 영상도 좋아해요",
          "original_form": "저는 쿠킹 영상도 좋아해요",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100042901.1.18",
          "form": "오오 !! 저도요! 요리 유투브도 자주봐요 ㅋㅋ",
          "original_form": "오오 !! 저도요! 요리 유투브도 자주봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100042901.1.19",
          "form": "그쵸그쵸 ㅋㅋ 힐링이예요",
          "original_form": "그쵸그쵸 ㅋㅋ 힐링이예요",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100042901.1.20",
          "form": "친해지고 싶어요",
          "original_form": "친해지고 싶어요",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100042901.1.21",
          "form": "쿠킹 유튜버 분이랑 ㅋㅋ",
          "original_form": "쿠킹 유튜버 분이랑 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100042901.1.22",
          "form": "오오 따라서 베이킹도 하시나요??",
          "original_form": "오오 따라서 베이킹도 하시나요??",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100042901.1.23",
          "form": "아네요 베이킹 하는것도 봐요 ㅋㅋ",
          "original_form": "아네요 베이킹 하는것도 봐요 ㅋㅋ",
