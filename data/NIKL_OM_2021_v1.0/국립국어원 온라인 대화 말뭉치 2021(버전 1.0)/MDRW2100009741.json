      "utterance": [
        {
          "id": "MDRW2100009741.1.1",
          "form": "운동 하고 계신거 있나요?",
          "original_form": "운동 하고 계신거 있나요?",
          "speaker_id": "2",
          "time": "20210823 09:19"
        },
        {
          "id": "MDRW2100009741.1.2",
          "form": "저는 홈트하다가 포기했어요",
          "original_form": "저는 홈트하다가 포기했어요",
          "speaker_id": "2",
          "time": "20210823 09:20"
        },
        {
          "id": "MDRW2100009741.1.3",
          "form": "저는 매일 퇴근하고",
          "original_form": "저는 매일 퇴근하고",
          "speaker_id": "1",
          "time": "20210823 09:21"
        },
        {
          "id": "MDRW2100009741.1.4",
          "form": "g x 하고있어요",
          "original_form": "g x 하고있어요",
          "speaker_id": "1",
          "time": "20210823 09:22"
        },
        {
          "id": "MDRW2100009741.1.5",
          "form": "GX면 같이 하는 운동 맞죠?",
          "original_form": "GX면 같이 하는 운동 맞죠?",
          "speaker_id": "2",
          "time": "20210823 09:23"
        },
        {
          "id": "MDRW2100009741.1.6",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210823 09:23"
        },
        {
          "id": "MDRW2100009741.1.7",
          "form": "저는 사람 모이는게 무서워서 시도도 못해봤어요ㅠ",
          "original_form": "저는 사람 모이는게 무서워서 시도도 못해봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210823 09:23"
        },
        {
          "id": "MDRW2100009741.1.8",
          "form": "아무래도 운동은 강제성이 있어야 꾸준히 하는것같아서",
          "original_form": "아무래도 운동은 강제성이 있어야 꾸준히 하는것같아서",
          "speaker_id": "1",
          "time": "20210823 09:24"
        },
        {
          "id": "MDRW2100009741.1.9",
          "form": "아무래도 코로나때문에",
          "original_form": "아무래도 코로나때문에",
          "speaker_id": "1",
          "time": "20210823 09:24"
        },
        {
          "id": "MDRW2100009741.1.10",
          "form": "걱정되지만",
          "original_form": "걱정되지만",
          "speaker_id": "1",
          "time": "20210823 09:24"
        },
        {
          "id": "MDRW2100009741.1.11",
          "form": "또 코로나때문에 사람이 별로없어요",
          "original_form": "또 코로나때문에 사람이 별로없어요",
          "speaker_id": "1",
          "time": "20210823 09:25"
        },
        {
          "id": "MDRW2100009741.1.12",
          "form": "운동도하고 최근에 식단조절도 시작했어요",
          "original_form": "운동도하고 최근에 식단조절도 시작했어요",
          "speaker_id": "1",
          "time": "20210823 09:26"
        },
        {
          "id": "MDRW2100009741.1.13",
          "form": "다이어트좀 해보려고",
          "original_form": "다이어트좀 해보려고",
          "speaker_id": "1",
          "time": "20210823 09:26"
        },
        {
          "id": "MDRW2100009741.1.14",
          "form": "홈트는 뭐하셨어요?",
          "original_form": "홈트는 뭐하셨어요?",
          "speaker_id": "1",
          "time": "20210823 09:26"
        },
        {
          "id": "MDRW2100009741.1.15",
          "form": "식단 조절까지 하면서 운동하시면 힘들텐데 의지가 대단하시네요",
          "original_form": "식단 조절까지 하면서 운동하시면 힘들텐데 의지가 대단하시네요",
          "speaker_id": "2",
          "time": "20210823 09:26"
        },
        {
          "id": "MDRW2100009741.1.16",
          "form": "유튜브 보면서 따라했었어요",
          "original_form": "유튜브 보면서 따라했었어요",
          "speaker_id": "2",
          "time": "20210823 09:26"
        },
        {
          "id": "MDRW2100009741.1.17",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210823 09:28"
        },
        {
          "id": "MDRW2100009741.1.18",
          "form": "다이어트는 운동보다 식단이 더 힘들대요",
          "original_form": "다이어트는 운동보다 식단이 더 힘들대요",
          "speaker_id": "1",
          "time": "20210823 09:28"
        },
        {
          "id": "MDRW2100009741.1.19",
          "form": "운동은 선택일 수 있지만 식단은 인생의 낙을 제한하는 거라...",
          "original_form": "운동은 선택일 수 있지만 식단은 인생의 낙을 제한하는 거라...",
          "speaker_id": "2",
          "time": "20210823 09:29"
        },
        {
          "id": "MDRW2100009741.1.20",
          "form": "맞아요 식욕은 도저히 참기 어려워요",
          "original_form": "맞아요 식욕은 도저히 참기 어려워요",
          "speaker_id": "1",
          "time": "20210823 09:33"
        },
        {
          "id": "MDRW2100009741.1.21",
          "form": "그래도 건강을 위해서 시도해야지",
          "original_form": "그래도 건강을 위해서 시도해야지",
          "speaker_id": "1",
          "time": "20210823 09:33"
        },
        {
          "id": "MDRW2100009741.1.22",
          "form": "디자인 공부하시면 패션에도 관심 많으시겧어요",
          "original_form": "디자인 공부하시면 패션에도 관심 많으시겧어요",
          "speaker_id": "1",
          "time": "20210823 09:34"
        },
        {
          "id": "MDRW2100009741.1.23",
          "form": "패션 관련한 디자인은 아닌지라 가끔 패션쇼 보는게 고작이에요ㅎㅎ",
          "original_form": "패션 관련한 디자인은 아닌지라 가끔 패션쇼 보는게 고작이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 09:36"
        },
        {
          "id": "MDRW2100009741.1.24",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210823 09:40"
        },
        {
          "id": "MDRW2100009741.1.25",
          "form": "옷 코디할때 어떤 스타일이세요?",
          "original_form": "옷 코디할때 어떤 스타일이세요?",
          "speaker_id": "1",
          "time": "20210823 09:41"
        },
        {
          "id": "MDRW2100009741.1.26",
          "form": "단정 깔끔? 개성? 화려함?",
          "original_form": "단정 깔끔? 개성? 화려함?",
          "speaker_id": "1",
          "time": "20210823 09:41"
        },
        {
          "id": "MDRW2100009741.1.27",
          "form": "저는 차려입을 때는 깔끔한거 선호하고",
          "original_form": "저는 차려입을 때는 깔끔한거 선호하고",
          "speaker_id": "2",
          "time": "20210823 09:42"
        },
        {
          "id": "MDRW2100009741.1.28",
          "form": "평상시에는 그냥 츄리닝 입고 다녀요ㅋㅋ",
          "original_form": "평상시에는 그냥 츄리닝 입고 다녀요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:42"
        },
        {
          "id": "MDRW2100009741.1.29",
          "form": "ㅋㅋㅋ 저도 집주변 다닐때는 츄리닝에 슬리퍼",
          "original_form": "ㅋㅋㅋ 저도 집주변 다닐때는 츄리닝에 슬리퍼",
          "speaker_id": "1",
          "time": "20210823 09:43"
        },
        {
          "id": "MDRW2100009741.1.30",
          "form": "회사갈때는 근무복",
          "original_form": "회사갈때는 근무복",
          "speaker_id": "1",
          "time": "20210823 09:43"
        }
      ]
