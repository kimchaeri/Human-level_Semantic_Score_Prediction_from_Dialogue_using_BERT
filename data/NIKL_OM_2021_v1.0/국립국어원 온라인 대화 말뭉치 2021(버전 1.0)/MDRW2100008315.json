      "utterance": [
        {
          "id": "MDRW2100008315.1.1",
          "form": "넵!! 저도요 ㅎㅎ",
          "original_form": "넵!! 저도요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008315.1.2",
          "form": "오 날씨군요",
          "original_form": "오 날씨군요",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008315.1.3",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "1",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008315.1.4",
          "form": "비가많이와서 시원하이 좋아진거같아요",
          "original_form": "비가많이와서 시원하이 좋아진거같아요",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100008315.1.5",
          "form": "그러게요 선선해졌어요",
          "original_form": "그러게요 선선해졌어요",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008315.1.6",
          "form": "저는 겨울보다 차라리여름이 좋은데 name1님은 어떠세요",
          "original_form": "저는 겨울보다 차라리여름이 좋은데 &name1&님은 어떠세요",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008315.1.7",
          "form": "해도길도 옷도 간편하고 빨래도 잘마르고",
          "original_form": "해도길도 옷도 간편하고 빨래도 잘마르고",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008315.1.8",
          "form": "넘좋지않나요",
          "original_form": "넘좋지않나요",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008315.1.9",
          "form": "저도 여름파예요 올 여름만 빼고 ㅋㅋㅋ",
          "original_form": "저도 여름파예요 올 여름만 빼고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008315.1.10",
          "form": "여름 하면 떠오르는 그 느낌이 좋아요",
          "original_form": "여름 하면 떠오르는 그 느낌이 좋아요",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008315.1.11",
          "form": "오 역시 겨울보단 여름이죠",
          "original_form": "오 역시 겨울보단 여름이죠",
          "speaker_id": "2",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008315.1.12",
          "form": "너무 추우면 아무것도 못하겠어서 ㅋㅋㅋ 해도 빨리 지고요",
          "original_form": "너무 추우면 아무것도 못하겠어서 ㅋㅋㅋ 해도 빨리 지고요",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008315.1.13",
          "form": "추운건 진짜 넘 힘들어요 서있기만 해도",
          "original_form": "추운건 진짜 넘 힘들어요 서있기만 해도",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008315.1.14",
          "form": "몸이 쉽게 아파져요ㅋㅋ",
          "original_form": "몸이 쉽게 아파져요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008315.1.15",
          "form": "너무 추우면 좀 우울해지지 않나요 뭔가 축 쳐져요 ㅠㅠ",
          "original_form": "너무 추우면 좀 우울해지지 않나요 뭔가 축 쳐져요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008315.1.16",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008315.1.17",
          "form": "병원가기도 추워서 싫고ㅜ",
          "original_form": "병원가기도 추워서 싫고ㅜ",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008315.1.18",
          "form": "옷짐도쉽게늘어나요",
          "original_form": "옷짐도쉽게늘어나요",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008315.1.19",
          "form": "겨울엔 그냥 전기장판안에 있다가 밖에 나갈땐 롱패딩만 입어여 ㅋㅋㅋ",
          "original_form": "겨울엔 그냥 전기장판안에 있다가 밖에 나갈땐 롱패딩만 입어여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008315.1.20",
          "form": "빨래도 안마르고ㅜ",
          "original_form": "빨래도 안마르고ㅜ",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008315.1.21",
          "form": "전기장판은 전자파때문에 몸이 쳐지지않나요",
          "original_form": "전기장판은 전자파때문에 몸이 쳐지지않나요",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008315.1.22",
          "form": "전자파 안나오는거 사면 돼요 ㅋㅋ",
          "original_form": "전자파 안나오는거 사면 돼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008315.1.23",
          "form": "제 친구는 온수매트를 구했더라구요",
          "original_form": "제 친구는 온수매트를 구했더라구요",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008315.1.24",
          "form": "온수매트도 좋죠 ㅋㅋㅋ",
          "original_form": "온수매트도 좋죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008315.1.25",
          "form": "안나오는 제품도 있군요",
          "original_form": "안나오는 제품도 있군요",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008315.1.26",
          "form": "저는 그냥 남는 전기매트깔아서 겨울버텨냈어요 ㅎㅎ",
          "original_form": "저는 그냥 남는 전기매트깔아서 겨울버텨냈어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008315.1.27",
          "form": "여름  막바지 되니까 태풍이 오네요 세상에",
          "original_form": "여름  막바지 되니까 태풍이 오네요 세상에",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008315.1.28",
          "form": "그러게요 날씨 급변하는게 무서워요 ㅋㅋ",
          "original_form": "그러게요 날씨 급변하는게 무서워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008315.1.29",
          "form": "어제 방에 비가 새서ㅋㅋ",
          "original_form": "어제 방에 비가 새서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008315.1.30",
          "form": "당황했었어요 ㅈ",
          "original_form": "당황했었어요 ㅈ",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008315.1.31",
          "form": "태풍올라오면 심해질것같은데 걱정이에요",
          "original_form": "태풍올라오면 심해질것같은데 걱정이에요",
          "speaker_id": "2",
          "time": "20210822 15:10"
        }
      ]
