      "utterance": [
        {
          "id": "MDRW2100023557.1.1",
          "form": "정말 일하기싫다",
          "original_form": "정말 일하기싫다",
          "speaker_id": "1",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100023557.1.2",
          "form": "진짜싫다",
          "original_form": "진짜싫다",
          "speaker_id": "2",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100023557.1.3",
          "form": "일은 왜해야할까",
          "original_form": "일은 왜해야할까",
          "speaker_id": "2",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100023557.1.4",
          "form": "이제 이만큼 발전했으면 인간을 놔주라 로봇을 써주라",
          "original_form": "이제 이만큼 발전했으면 인간을 놔주라 로봇을 써주라",
          "speaker_id": "2",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100023557.1.5",
          "form": "점심드셨는데 일안하세요??",
          "original_form": "점심드셨는데 일안하세요??",
          "speaker_id": "2",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100023557.1.6",
          "form": "일하기싫어서 미루는 중",
          "original_form": "일하기싫어서 미루는 중",
          "speaker_id": "1",
          "time": "20210830 14:08"
        },
        {
          "id": "MDRW2100023557.1.7",
          "form": "솔직히 주 4일해줘라",
          "original_form": "솔직히 주 4일해줘라",
          "speaker_id": "1",
          "time": "20210830 14:08"
        },
        {
          "id": "MDRW2100023557.1.8",
          "form": "아니면 주 5일 5시간근무",
          "original_form": "아니면 주 5일 5시간근무",
          "speaker_id": "1",
          "time": "20210830 14:08"
        },
        {
          "id": "MDRW2100023557.1.9",
          "form": "ㅁㅈ..나도 해야하는데 ㅠㅠ",
          "original_form": "ㅁㅈ..나도 해야하는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023557.1.10",
          "form": "월화 하고 수 쉬고 목금",
          "original_form": "월화 하고 수 쉬고 목금",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023557.1.11",
          "form": "안정적억..",
          "original_form": "안정적억..",
          "speaker_id": "2",
          "time": "20210830 14:21"
        },
        {
          "id": "MDRW2100023557.1.12",
          "form": "어 인정",
          "original_form": "어 인정",
          "speaker_id": "1",
          "time": "20210830 15:01"
        },
        {
          "id": "MDRW2100023557.1.13",
          "form": "가운데 쉬어줘야해",
          "original_form": "가운데 쉬어줘야해",
          "speaker_id": "1",
          "time": "20210830 15:01"
        },
        {
          "id": "MDRW2100023557.1.14",
          "form": "그니까 지금도 시간 너뮤 안가.. ㅠ 내일부터는 진짜 바쁜데..",
          "original_form": "그니까 지금도 시간 너뮤 안가.. ㅠ 내일부터는 진짜 바쁜데..",
          "speaker_id": "2",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100023557.1.15",
          "form": "일 넘 많아..",
          "original_form": "일 넘 많아..",
          "speaker_id": "2",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100023557.1.16",
          "form": "나도 내일무터 마감..",
          "original_form": "나도 내일무터 마감..",
          "speaker_id": "1",
          "time": "20210830 15:37"
        },
        {
          "id": "MDRW2100023557.1.17",
          "form": "근데 오늘도 겁나 바빠",
          "original_form": "근데 오늘도 겁나 바빠",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100023557.1.18",
          "form": "쉬었어서",
          "original_form": "쉬었어서",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100023557.1.19",
          "form": "대리가 일 잘 처리안해놔서 더욱..",
          "original_form": "대리가 일 잘 처리안해놔서 더욱..",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100023557.1.20",
          "form": "헐랭 왜그랬대 ㅠㅠ",
          "original_form": "헐랭 왜그랬대 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100023557.1.21",
          "form": "정퇴는 가능?",
          "original_form": "정퇴는 가능?",
          "speaker_id": "2",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100023557.1.22",
          "form": "어어 정퇴는 ㄱㅏ능 ㅠㅠ 빡세게 해야해",
          "original_form": "어어 정퇴는 ㄱㅏ능 ㅠㅠ 빡세게 해야해",
          "speaker_id": "1",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100023557.1.23",
          "form": "아마..계속..계속..?",
          "original_form": "아마..계속..계속..?",
          "speaker_id": "1",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023557.1.24",
          "form": "돈이나 더 줬으면 좋겠다",
          "original_form": "돈이나 더 줬으면 좋겠다",
          "speaker_id": "1",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023557.1.25",
          "form": "ㅠㅠ인정",
          "original_form": "ㅠㅠ인정",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023557.1.26",
          "form": "돈더줘라..상여도줘라..추석인데",
          "original_form": "돈더줘라..상여도줘라..추석인데",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100023557.1.27",
          "form": "너네 떡값은나오니?",
          "original_form": "너네 떡값은나오니?",
          "speaker_id": "1",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100023557.1.28",
          "form": "우리는 안나와",
          "original_form": "우리는 안나와",
          "speaker_id": "1",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100023557.1.29",
          "form": "우리도 안줄걸...그냥 참치캔..?",
          "original_form": "우리도 안줄걸...그냥 참치캔..?",
          "speaker_id": "2",
          "time": "20210830 16:13"
        },
        {
          "id": "MDRW2100023557.1.30",
          "form": "우리는 멸친데",
          "original_form": "우리는 멸친데",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023557.1.31",
          "form": "참치캔이 나은 것 같아",
          "original_form": "참치캔이 나은 것 같아",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023557.1.32",
          "form": "머야",
          "original_form": "머야",
          "speaker_id": "2",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023557.1.33",
          "form": "멸치조은데",
          "original_form": "멸치조은데",
          "speaker_id": "2",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023557.1.34",
          "form": "마감이라 진짜바빠",
          "original_form": "마감이라 진짜바빠",
          "speaker_id": "1",
          "time": "20210901 16:43"
        }
      ]
    }
