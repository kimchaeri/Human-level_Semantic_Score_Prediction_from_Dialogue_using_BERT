      "utterance": [
        {
          "id": "MDRW2100043395.1.1",
          "form": "불닭볶음면 먹고싶어요",
          "original_form": "불닭볶음면 먹고싶어요",
          "speaker_id": "2",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100043395.1.2",
          "form": "치즈뿌려서",
          "original_form": "치즈뿌려서",
          "speaker_id": "2",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100043395.1.3",
          "form": "누누님 이건 비밀인데 한 턴씩 빠르게 주고받기에 세미 스무고개가 엄청 좋은 거 아시나요",
          "original_form": "누누님 이건 비밀인데 한 턴씩 빠르게 주고받기에 세미 스무고개가 엄청 좋은 거 아시나요",
          "speaker_id": "1",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100043395.1.4",
          "form": "전자레인ㅣ에 녹이고",
          "original_form": "전자레인ㅣ에 녹이고",
          "speaker_id": "2",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100043395.1.5",
          "form": "세미 스무고개가 뭔가요?",
          "original_form": "세미 스무고개가 뭔가요?",
          "speaker_id": "2",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100043395.1.6",
          "form": "예를 들어 서로 음식 메뉴 맞추기를 한다 치면",
          "original_form": "예를 들어 서로 음식 메뉴 맞추기를 한다 치면",
          "speaker_id": "1",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100043395.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100043395.1.8",
          "form": "매운음식인가요?\n아니요 안 매운 음식ㅇ이ㅖ요\n한식인가요\n네 한식입니다\n\n이런 식으로 팟팟팟 주고받으면 진짜 빨리 차거든요! 해보실래요?!",
          "original_form": "매운음식인가요?\n아니요 안 매운 음식ㅇ이ㅖ요\n한식인가요\n네 한식입니다\n\n이런 식으로 팟팟팟 주고받으면 진짜 빨리 차거든요! 해보실래요?!",
          "speaker_id": "1",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100043395.1.9",
          "form": "질문 답변 한 줄씩 핑퐁하면 끝내줘요(...)",
          "original_form": "질문 답변 한 줄씩 핑퐁하면 끝내줘요(...)",
          "speaker_id": "1",
          "time": "20210907 18:45"
        },
        {
          "id": "MDRW2100043395.1.10",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043395.1.11",
          "form": "콜?!",
          "original_form": "콜?!",
          "speaker_id": "1",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043395.1.12",
          "form": "제가 맞춰봐도 될까요",
          "original_form": "제가 맞춰봐도 될까요",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043395.1.13",
          "form": "뜨거운 음식인가요",
          "original_form": "뜨거운 음식인가요",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043395.1.14",
          "form": "좋아요! 판 갈겠슴다 후후",
          "original_form": "좋아요! 판 갈겠슴다 후후",
          "speaker_id": "1",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043395.1.15",
          "form": "헉 저 저녁 후다닥 먹고 와도 될까요",
          "original_form": "헉 저 저녁 후다닥 먹고 와도 될까요",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043395.1.16",
          "form": "삼겹살은 당연히",
          "original_form": "삼겹살은 당연히",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043395.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043395.1.18",
          "form": "다녀오세요~",
          "original_form": "다녀오세요~",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043395.1.19",
          "form": "그리고 이벤트 내일 오전에 조기종료래료(...)",
          "original_form": "그리고 이벤트 내일 오전에 조기종료래료(...)",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043395.1.20",
          "form": "오늘 최대한 같이 채우실래요?",
          "original_form": "오늘 최대한 같이 채우실래요?",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043395.1.21",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043395.1.22",
          "form": "왜 갑자기",
          "original_form": "왜 갑자기",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043395.1.23",
          "form": "종료...?",
          "original_form": "종료...?",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043395.1.24",
          "form": "제출한 갯수 확인 페이지 들어가보세요! 낼까지래요",
          "original_form": "제출한 갯수 확인 페이지 들어가보세요! 낼까지래요",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043395.1.25",
          "form": "헉 전지금 113개 제출했어요",
          "original_form": "헉 전지금 113개 제출했어요",
          "speaker_id": "2",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043395.1.26",
          "form": "생각보다 자금이 많이 들어갔나봐욬ㅋㅋ",
          "original_form": "생각보다 자금이 많이 들어갔나봐욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043395.1.27",
          "form": "저 저녁 먹고 왔어요!\n그러니까요 ㅋㅋㅋㅋ 예산이 동났나봐요\n몇 개까지 채우실 생각이세요?",
          "original_form": "저 저녁 먹고 왔어요!\n그러니까요 ㅋㅋㅋㅋ 예산이 동났나봐요\n몇 개까지 채우실 생각이세요?",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043395.1.28",
          "form": "전 지금 210개 했어요",
          "original_form": "전 지금 210개 했어요",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043395.1.29",
          "form": "저 150개요!!",
          "original_form": "저 150개요!!",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043395.1.30",
          "form": "헉 그럼 40여개 쯤 스피드런 같이 해요!",
          "original_form": "헉 그럼 40여개 쯤 스피드런 같이 해요!",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043395.1.31",
          "form": "몇시까지 가능하세요?????!!!!!!!!?!?!",
          "original_form": "몇시까지 가능하세요?????!!!!!!!!?!?!",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043395.1.32",
          "form": "아니다 언제부터 가능하세요!!",
          "original_form": "아니다 언제부터 가능하세요!!",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043395.1.33",
          "form": "저중간중간 들어와서 하구",
          "original_form": "저중간중간 들어와서 하구",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043395.1.34",
          "form": "지금도 하고계시나요ㅋㅋ",
          "original_form": "지금도 하고계시나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043395.1.35",
          "form": "저 여러명 걸쳐있어서",
          "original_form": "저 여러명 걸쳐있어서",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043395.1.36",
          "form": "네! 저는 열심히 마지막 날을 불태우려구요",
          "original_form": "네! 저는 열심히 마지막 날을 불태우려구요",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043395.1.37",
          "form": "목표치 끝나셨으면 먼저 가셔도되어용",
          "original_form": "목표치 끝나셨으면 먼저 가셔도되어용",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043395.1.38",
          "form": "앗 그래서",
          "original_form": "앗 그래서",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043395.1.39",
          "form": "다들 스무고개 하자 해 보세요ㅋㅋㅋ편해요ㅋㅋㅋㅋ",
          "original_form": "다들 스무고개 하자 해 보세요ㅋㅋㅋ편해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043395.1.40",
          "form": "오늘저녁은 어떤것이죠",
          "original_form": "오늘저녁은 어떤것이죠",
          "speaker_id": "2",
          "time": "20210907 19:40"
        }
      ]
    }
