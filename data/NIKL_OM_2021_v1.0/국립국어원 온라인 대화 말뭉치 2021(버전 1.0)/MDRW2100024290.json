      "utterance": [
        {
          "id": "MDRW2100024290.1.1",
          "form": "나혼자산다 팬이라서",
          "original_form": "나혼자산다 팬이라서",
          "speaker_id": "2",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024290.1.2",
          "form": "저희 아들은...ㅈ많이 부족해서",
          "original_form": "저희 아들은...ㅈ많이 부족해서",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024290.1.3",
          "form": "제가 끼고 살아야 할듯요",
          "original_form": "제가 끼고 살아야 할듯요",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024290.1.4",
          "form": "집안일도 가르쳐야하는데ㅠㅠ",
          "original_form": "집안일도 가르쳐야하는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024290.1.5",
          "form": "돈열심히버셔야겠어요",
          "original_form": "돈열심히버셔야겠어요",
          "speaker_id": "2",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024290.1.6",
          "form": "나혼자산다ㅋㅋㅋ재밌죠?ㅋㅋㅋ그런가보면1인가족이 트렌드인듯",
          "original_form": "나혼자산다ㅋㅋㅋ재밌죠?ㅋㅋㅋ그런가보면1인가족이 트렌드인듯",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024290.1.7",
          "form": "결혼 전 독립해서 살아보고 싶었는데 아쉬워요",
          "original_form": "결혼 전 독립해서 살아보고 싶었는데 아쉬워요",
          "speaker_id": "1",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100024290.1.8",
          "form": "집안일에 치여서ㅋㅋㅋ결혼도 안했을듯ㅋ",
          "original_form": "집안일에 치여서ㅋㅋㅋ결혼도 안했을듯ㅋ",
          "speaker_id": "1",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100024290.1.9",
          "form": "ㅋㅋㅋ 집안일은 해도해도끝이없어요",
          "original_form": "ㅋㅋㅋ 집안일은 해도해도끝이없어요",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024290.1.10",
          "form": "안해도해도",
          "original_form": "안해도해도",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024290.1.11",
          "form": "ㅋㅋㄱ",
          "original_form": "ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024290.1.12",
          "form": "엄마랑살때가그립죠",
          "original_form": "엄마랑살때가그립죠",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024290.1.13",
          "form": "결혼해도집안일하는건아니지만ㅋ",
          "original_form": "결혼해도집안일하는건아니지만ㅋ",
          "speaker_id": "1",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024290.1.14",
          "form": "집안일은 내일로",
          "original_form": "집안일은 내일로",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024290.1.15",
          "form": "국룰아닌가요",
          "original_form": "국룰아닌가요",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024290.1.16",
          "form": "육아에 집안일까지 슈퍼우먼이세요",
          "original_form": "육아에 집안일까지 슈퍼우먼이세요",
          "speaker_id": "1",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024290.1.17",
          "form": "거기에 부업까지 진짜 존경스럽습니다",
          "original_form": "거기에 부업까지 진짜 존경스럽습니다",
          "speaker_id": "1",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024290.1.18",
          "form": "블로그에다이어트까지",
          "original_form": "블로그에다이어트까지",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024290.1.19",
          "form": "집안일이 젤 뒷전이죠",
          "original_form": "집안일이 젤 뒷전이죠",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024290.1.20",
          "form": "ㅋㅋㅋ집안일은 내일로",
          "original_form": "ㅋㅋㅋ집안일은 내일로",
          "speaker_id": "1",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100024290.1.21",
          "form": "저녁식사는 마라탕",
          "original_form": "저녁식사는 마라탕",
          "speaker_id": "2",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024290.1.22",
          "form": "말아먹었어요",
          "original_form": "말아먹었어요",
          "speaker_id": "2",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024290.1.23",
          "form": "국룰이라하셨죠?ㅋㅋ동의",
          "original_form": "국룰이라하셨죠?ㅋㅋ동의",
          "speaker_id": "1",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024290.1.24",
          "form": "합니다",
          "original_form": "합니다",
          "speaker_id": "1",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024290.1.25",
          "form": "집안일은 내일로",
          "original_form": "집안일은 내일로",
          "speaker_id": "2",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024290.1.26",
          "form": "저는 오징어 먹았어요ㅋㅋ엄마가 요리ㅋㅋ",
          "original_form": "저는 오징어 먹았어요ㅋㅋ엄마가 요리ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024290.1.27",
          "form": "오늘은 일단쉬자",
          "original_form": "오늘은 일단쉬자",
          "speaker_id": "2",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024290.1.28",
          "form": "엄마가 해주는요리",
          "original_form": "엄마가 해주는요리",
          "speaker_id": "2",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024290.1.29",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "2",
          "time": "20210831 00:18"
        },
        {
          "id": "MDRW2100024290.1.30",
          "form": "육아는 미루지 못해 속상해요",
          "original_form": "육아는 미루지 못해 속상해요",
          "speaker_id": "1",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024290.1.31",
          "form": "요새는 친정가도",
          "original_form": "요새는 친정가도",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024290.1.32",
          "form": "엄마는 둘찌만보고",
          "original_form": "엄마는 둘찌만보고",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024290.1.33",
          "form": "상차릴생각도안해요",
          "original_form": "상차릴생각도안해요",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024290.1.34",
          "form": "미룰수있다면 미루고 싶네요 육아야 말로ㅋ",
          "original_form": "미룰수있다면 미루고 싶네요 육아야 말로ㅋ",
          "speaker_id": "1",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100024290.1.35",
          "form": "아이가 뒤집기를 아직 안하는군요",
          "original_form": "아이가 뒤집기를 아직 안하는군요",
          "speaker_id": "1",
          "time": "20210831 01:08"
        }
