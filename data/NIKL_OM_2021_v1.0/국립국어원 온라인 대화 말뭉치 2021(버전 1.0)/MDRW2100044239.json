      "utterance": [
        {
          "id": "MDRW2100044239.1.1",
          "form": "모든 디저트는 저의 사랑이죠",
          "original_form": "모든 디저트는 저의 사랑이죠",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044239.1.2",
          "form": "전 타르트 좋아하는데",
          "original_form": "전 타르트 좋아하는데",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044239.1.3",
          "form": "아직 맛집을 못찾았어요",
          "original_form": "아직 맛집을 못찾았어요",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044239.1.4",
          "form": "타르트! 음.. 타르트는",
          "original_form": "타르트! 음.. 타르트는",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044239.1.5",
          "form": "어디였더라",
          "original_form": "어디였더라",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044239.1.6",
          "form": "타르타르도 무난했지만 타르트에 금칠 했나 싶어서 다른가게를 갔었는데",
          "original_form": "타르타르도 무난했지만 타르트에 금칠 했나 싶어서 다른가게를 갔었는데",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044239.1.7",
          "form": "이름이 갑자기 떠오르질 않네요",
          "original_form": "이름이 갑자기 떠오르질 않네요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044239.1.8",
          "form": "그쵸 타르타르는 너무 비싸요 ㅠ",
          "original_form": "그쵸 타르타르는 너무 비싸요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044239.1.9",
          "form": "논현동에 리츠빈이라는 카페가 있는데",
          "original_form": "논현동에 리츠빈이라는 카페가 있는데",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044239.1.10",
          "form": "그 근처에 타르트 가게가 하나 있어요",
          "original_form": "그 근처에 타르트 가게가 하나 있어요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044239.1.11",
          "form": "통유리창으로 되어있는데",
          "original_form": "통유리창으로 되어있는데",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044239.1.12",
          "form": "가게가 되게 좁고 긴 편이거든오",
          "original_form": "가게가 되게 좁고 긴 편이거든오",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044239.1.13",
          "form": "오 왠지 맛집의 기운이 느껴지는데요",
          "original_form": "오 왠지 맛집의 기운이 느껴지는데요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044239.1.14",
          "form": "어쩌다 한번 가본 곳인데",
          "original_form": "어쩌다 한번 가본 곳인데",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044239.1.15",
          "form": "디저트 종류도 다양하고",
          "original_form": "디저트 종류도 다양하고",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044239.1.16",
          "form": "상당히 맛있었어요",
          "original_form": "상당히 맛있었어요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044239.1.17",
          "form": "이름은 기억 안나시죠?",
          "original_form": "이름은 기억 안나시죠?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044239.1.18",
          "form": "글쎄 그게 제일 문제네요 ;;",
          "original_form": "글쎄 그게 제일 문제네요 ;;",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044239.1.19",
          "form": "아쉽네요 ㅋㅋ큐ㅠㅠㅠ",
          "original_form": "아쉽네요 ㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044239.1.20",
          "form": "쎄쎄종인가?",
          "original_form": "쎄쎄종인가?",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044239.1.21",
          "form": "지도에는 온통 식당만 나와서",
          "original_form": "지도에는 온통 식당만 나와서",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044239.1.22",
          "form": "찾기 넘 힘든거같아요",
          "original_form": "찾기 넘 힘든거같아요",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044239.1.23",
          "form": "아무리 검색해도 보이지 않네요 ㅠ",
          "original_form": "아무리 검색해도 보이지 않네요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044239.1.24",
          "form": "은근 찾아가는 사람들이 많았어서",
          "original_form": "은근 찾아가는 사람들이 많았어서",
          "speaker_id": "2",
          "time": "20210907 22:19"
        }
      ]
    }
