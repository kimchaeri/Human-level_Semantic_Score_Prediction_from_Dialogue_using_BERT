      "utterance": [
        {
          "id": "MDRW2100045737.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 06:29"
        },
        {
          "id": "MDRW2100045737.1.2",
          "form": "아니요.. 인좋아합니다",
          "original_form": "아니요.. 인좋아합니다",
          "speaker_id": "1",
          "time": "20210908 06:29"
        },
        {
          "id": "MDRW2100045737.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 06:29"
        },
        {
          "id": "MDRW2100045737.1.4",
          "form": "아뇨., .ㅋㅋㅋㅋ",
          "original_form": "아뇨., .ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:30"
        },
        {
          "id": "MDRW2100045737.1.5",
          "form": "동생은 열심히 하던데 전 잘 안맞네요ㅠㅜ",
          "original_form": "동생은 열심히 하던데 전 잘 안맞네요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210908 06:30"
        },
        {
          "id": "MDRW2100045737.1.6",
          "form": "네.. 가만히 있는게 너무 좋아요",
          "original_form": "네.. 가만히 있는게 너무 좋아요",
          "speaker_id": "1",
          "time": "20210908 06:31"
        },
        {
          "id": "MDRW2100045737.1.7",
          "form": "몸이 썩는것 같아서 큰일입니다",
          "original_form": "몸이 썩는것 같아서 큰일입니다",
          "speaker_id": "1",
          "time": "20210908 06:31"
        },
        {
          "id": "MDRW2100045737.1.8",
          "form": "저도요ㅋㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045737.1.9",
          "form": "그냥 숨만 쉽니다",
          "original_form": "그냥 숨만 쉽니다",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045737.1.10",
          "form": "저는 침대에 누워있는걸 좋아하는데 양심상 다리 들었다 내렸다 하요",
          "original_form": "저는 침대에 누워있는걸 좋아하는데 양심상 다리 들었다 내렸다 하요",
          "speaker_id": "1",
          "time": "20210908 06:33"
        },
        {
          "id": "MDRW2100045737.1.11",
          "form": "근데 진짜 몸이 점점 안좋아지는 거 같긴해요ㅜㅜ",
          "original_form": "근데 진짜 몸이 점점 안좋아지는 거 같긴해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045737.1.12",
          "form": "근육이 없으니까 자세도 틀어지고...",
          "original_form": "근육이 없으니까 자세도 틀어지고...",
          "speaker_id": "2",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045737.1.13",
          "form": "맞아요.. 근육이 없으니까 운동도 못하는",
          "original_form": "맞아요.. 근육이 없으니까 운동도 못하는",
          "speaker_id": "1",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045737.1.14",
          "form": "이상한 일이",
          "original_form": "이상한 일이",
          "speaker_id": "1",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045737.1.15",
          "form": "그니까요ㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045737.1.16",
          "form": "완전 기현상",
          "original_form": "완전 기현상",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045737.1.17",
          "form": "무릎도 너무 안좋고",
          "original_form": "무릎도 너무 안좋고",
          "speaker_id": "1",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045737.1.18",
          "form": "아무것도 안하는데 왜 무릎은 안좋은지 모르겠네요 ㅎㅎ",
          "original_form": "아무것도 안하는데 왜 무릎은 안좋은지 모르겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045737.1.19",
          "form": "아 완전요...",
          "original_form": "아 완전요...",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045737.1.20",
          "form": "근데 그거 운동부족이어도 아프대요",
          "original_form": "근데 그거 운동부족이어도 아프대요",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045737.1.21",
          "form": "저 고삼때 병원갔는데 운동부족이랬어요",
          "original_form": "저 고삼때 병원갔는데 운동부족이랬어요",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045737.1.22",
          "form": "결국 운동부족이어서 그렇군요 ㅋㅋ",
          "original_form": "결국 운동부족이어서 그렇군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045737.1.23",
          "form": "근데 또 무릎강화운동을 하기 위해선 근육이필요한",
          "original_form": "근데 또 무릎강화운동을 하기 위해선 근육이필요한",
          "speaker_id": "1",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045737.1.24",
          "form": "그죠ㅠㅠ",
          "original_form": "그죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045737.1.25",
          "form": "아이러니...",
          "original_form": "아이러니...",
          "speaker_id": "2",
          "time": "20210908 06:38"
        }
      ]
    }
