      "utterance": [
        {
          "id": "MDRW2100005424.1.1",
          "form": "님",
          "original_form": "님",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005424.1.2",
          "form": "설거지 몇일분",
          "original_form": "설거지 몇일분",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005424.1.3",
          "form": "소유하고있어요?",
          "original_form": "소유하고있어요?",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005424.1.4",
          "form": "저 방금다했는디",
          "original_form": "저 방금다했는디",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005424.1.5",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005424.1.6",
          "form": "요즘 박박해요",
          "original_form": "요즘 박박해요",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005424.1.7",
          "form": "배신감",
          "original_form": "배신감",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005424.1.8",
          "form": "당신은더러워요",
          "original_form": "당신은더러워요",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005424.1.9",
          "form": "전 오늘치 밀렸어요",
          "original_form": "전 오늘치 밀렸어요",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005424.1.10",
          "form": "하루치라고요",
          "original_form": "하루치라고요",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.11",
          "form": "당신은역겨워요",
          "original_form": "당신은역겨워요",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.12",
          "form": "빨래는 일주일치",
          "original_form": "빨래는 일주일치",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.13",
          "form": "입니다",
          "original_form": "입니다",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.14",
          "form": "당신을경멸합니다",
          "original_form": "당신을경멸합니다",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.15",
          "form": "달달하다",
          "original_form": "달달하다",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.16",
          "form": "일주일밀리면",
          "original_form": "일주일밀리면",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.17",
          "form": "비난의 맛",
          "original_form": "비난의 맛",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.18",
          "form": "입을옷이",
          "original_form": "입을옷이",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.19",
          "form": "있긴해요?",
          "original_form": "있긴해요?",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.20",
          "form": "밖을 안나가잖아요...",
          "original_form": "밖을 안나가잖아요...",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.21",
          "form": "재택 2주째",
          "original_form": "재택 2주째",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.22",
          "form": "그렇긴한디...",
          "original_form": "그렇긴한디...",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005424.1.23",
          "form": "입을속옷이있긴해요?",
          "original_form": "입을속옷이있긴해요?",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.24",
          "form": "빨래도 운동복 잠옷 수건",
          "original_form": "빨래도 운동복 잠옷 수건",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.25",
          "form": "뿐이에요",
          "original_form": "뿐이에요",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.26",
          "form": "빤쓰몇개야",
          "original_form": "빤쓰몇개야",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.27",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.28",
          "form": "드로즈 왕창 사놔서",
          "original_form": "드로즈 왕창 사놔서",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.29",
          "form": "바지도 안입어서",
          "original_form": "바지도 안입어서",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.30",
          "form": "빨래도줄엇어요",
          "original_form": "빨래도줄엇어요",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.31",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.32",
          "form": "금지어",
          "original_form": "금지어",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.33",
          "form": "빤쑤부자",
          "original_form": "빤쑤부자",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.34",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005424.1.36",
          "form": "너무많이 필터링되는 거 아니냐",
          "original_form": "너무많이 필터링되는 거 아니냐",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005424.1.37",
          "form": "강제로 고운말을쓰게하다니...!!",
          "original_form": "강제로 고운말을쓰게하다니...!!",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005424.1.38",
          "form": "핏물 빤쓰빨이",
          "original_form": "핏물 빤쓰빨이",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005424.1.39",
          "form": "빤쑤가어땨서!!!",
          "original_form": "빤쑤가어땨서!!!",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005424.1.40",
          "form": "자주 해서 이렇게 됐습니다",
          "original_form": "자주 해서 이렇게 됐습니다",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005424.1.41",
          "form": "그렇구나...",
          "original_form": "그렇구나...",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005424.1.42",
          "form": "그래고 자주빠세요",
          "original_form": "그래고 자주빠세요",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005424.1.43",
          "form": "저 매일 갈아입는다고요",
          "original_form": "저 매일 갈아입는다고요",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005424.1.44",
          "form": "빤쓰",
          "original_form": "빤쓰",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005424.1.45",
          "form": "그럼 빨랴도 자주하세요...",
          "original_form": "그럼 빨랴도 자주하세요...",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005424.1.46",
          "form": "싫.어.",
          "original_form": "싫.어.",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005424.1.47",
          "form": "근데 어차피 요즘 비와서",
          "original_form": "근데 어차피 요즘 비와서",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005424.1.48",
          "form": "낼 빨래날인디",
          "original_form": "낼 빨래날인디",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005424.1.49",
          "form": "큰일이야",
          "original_form": "큰일이야",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005424.1.50",
          "form": "안타깝네요",
          "original_form": "안타깝네요",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005424.1.51",
          "form": "제 지역은 내일 비안와요",
          "original_form": "제 지역은 내일 비안와요",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005424.1.52",
          "form": "빨래할수있어요",
          "original_form": "빨래할수있어요",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005424.1.53",
          "form": "저도 사실 3일밀려서",
          "original_form": "저도 사실 3일밀려서",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005424.1.54",
          "form": "빨래할동안만 고향체인지",
          "original_form": "빨래할동안만 고향체인지",
          "speaker_id": "1",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005424.1.55",
          "form": "평샹체인지하새요",
          "original_form": "평샹체인지하새요",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005424.1.56",
          "form": "선넘네",
          "original_form": "선넘네",
          "speaker_id": "1",
          "time": "20210821 22:25"
        }
      ]
