      "utterance": [
        {
          "id": "MDRW2100023258.1.1",
          "form": "가장 좋았던 여행지는 오디세요?",
          "original_form": "가장 좋았던 여행지는 오디세요?",
          "speaker_id": "1",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100023258.1.2",
          "form": "저 아일랜드랑 미얀마요",
          "original_form": "저 아일랜드랑 미얀마요",
          "speaker_id": "2",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023258.1.3",
          "form": "미얀마사람들이 참 순박한데",
          "original_form": "미얀마사람들이 참 순박한데",
          "speaker_id": "2",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023258.1.4",
          "form": "요즘 난리네요 ㅠ",
          "original_form": "요즘 난리네요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023258.1.5",
          "form": "오오~~ 둘다 가본적 없는 나라에요",
          "original_form": "오오~~ 둘다 가본적 없는 나라에요",
          "speaker_id": "1",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023258.1.6",
          "form": "미얀마에서는 어떤 여행일정 보내셨는지 궁금해요",
          "original_form": "미얀마에서는 어떤 여행일정 보내셨는지 궁금해요",
          "speaker_id": "1",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023258.1.7",
          "form": "미얀마는... 사원같은게 많나요?",
          "original_form": "미얀마는... 사원같은게 많나요?",
          "speaker_id": "1",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023258.1.8",
          "form": "종교가...... 불교?",
          "original_form": "종교가...... 불교?",
          "speaker_id": "1",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023258.1.9",
          "form": "인가요? 미얀마 국교가?",
          "original_form": "인가요? 미얀마 국교가?",
          "speaker_id": "1",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023258.1.10",
          "form": "불교국가예여",
          "original_form": "불교국가예여",
          "speaker_id": "2",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023258.1.11",
          "form": "90프로이상이",
          "original_form": "90프로이상이",
          "speaker_id": "2",
          "time": "20210829 23:32"
        },
        {
          "id": "MDRW2100023258.1.12",
          "form": "거기서 어떤 여해일정 보내셨어요?",
          "original_form": "거기서 어떤 여해일정 보내셨어요?",
          "speaker_id": "1",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100023258.1.13",
          "form": "거기 더워서 수영도 하고",
          "original_form": "거기 더워서 수영도 하고",
          "speaker_id": "2",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100023258.1.14",
          "form": "불교유적지 주로 다녔어요",
          "original_form": "불교유적지 주로 다녔어요",
          "speaker_id": "2",
          "time": "20210829 23:37"
        },
        {
          "id": "MDRW2100023258.1.15",
          "form": "아~ 가는곳마다 불교사원이었겠네요",
          "original_form": "아~ 가는곳마다 불교사원이었겠네요",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100023258.1.16",
          "form": "먹거리는 뭐가 있었어여?",
          "original_form": "먹거리는 뭐가 있었어여?",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100023258.1.17",
          "form": "생각이 잘 안나여",
          "original_form": "생각이 잘 안나여",
          "speaker_id": "2",
          "time": "20210830 00:03"
        },
        {
          "id": "MDRW2100023258.1.18",
          "form": "약간 음식이 한국인 입맛에 잘 맞았어요",
          "original_form": "약간 음식이 한국인 입맛에 잘 맞았어요",
          "speaker_id": "2",
          "time": "20210830 00:03"
        },
        {
          "id": "MDRW2100023258.1.19",
          "form": "오~ 미얀마는 주변에서도 가봤다고 못들어봐서 되게 신기하네요ㅜㅎㅎ",
          "original_form": "오~ 미얀마는 주변에서도 가봤다고 못들어봐서 되게 신기하네요ㅜㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:08"
        },
        {
          "id": "MDRW2100023258.1.20",
          "form": "아일랜드도요!",
          "original_form": "아일랜드도요!",
          "speaker_id": "1",
          "time": "20210830 00:09"
        },
        {
          "id": "MDRW2100023258.1.21",
          "form": "어쩌다가 여행지로 선택하신거에요?",
          "original_form": "어쩌다가 여행지로 선택하신거에요?",
          "speaker_id": "1",
          "time": "20210830 00:09"
        },
        {
          "id": "MDRW2100023258.1.22",
          "form": "아일랜드는 한번 가보고 좋아서 다시갔구",
          "original_form": "아일랜드는 한번 가보고 좋아서 다시갔구",
          "speaker_id": "2",
          "time": "20210830 00:09"
        },
        {
          "id": "MDRW2100023258.1.23",
          "form": "미얀마는 친구 추천으로요",
          "original_form": "미얀마는 친구 추천으로요",
          "speaker_id": "2",
          "time": "20210830 00:10"
        },
        {
          "id": "MDRW2100023258.1.24",
          "form": "아일랜드는 뭐 보셨어요?",
          "original_form": "아일랜드는 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210830 00:13"
        },
        {
          "id": "MDRW2100023258.1.25",
          "form": "더블린에서",
          "original_form": "더블린에서",
          "speaker_id": "2",
          "time": "20210830 00:13"
        },
        {
          "id": "MDRW2100023258.1.26",
          "form": "해리포터 도서관 봤고요",
          "original_form": "해리포터 도서관 봤고요",
          "speaker_id": "2",
          "time": "20210830 00:13"
        },
        {
          "id": "MDRW2100023258.1.27",
          "form": "오 거기 해리포터 도서관이 있나요??",
          "original_form": "오 거기 해리포터 도서관이 있나요??",
          "speaker_id": "1",
          "time": "20210830 00:15"
        },
        {
          "id": "MDRW2100023258.1.28",
          "form": "해리포터에서 아일랜드가 나왔었던거 같긴하네요",
          "original_form": "해리포터에서 아일랜드가 나왔었던거 같긴하네요",
          "speaker_id": "1",
          "time": "20210830 00:15"
        },
        {
          "id": "MDRW2100023258.1.29",
          "form": "그리고 절벽?? 클리프라고 있구요",
          "original_form": "그리고 절벽?? 클리프라고 있구요",
          "speaker_id": "2",
          "time": "20210830 00:16"
        },
        {
          "id": "MDRW2100023258.1.30",
          "form": "바람이 많이 불어서 날아가지 않게 조심",
          "original_form": "바람이 많이 불어서 날아가지 않게 조심",
          "speaker_id": "2",
          "time": "20210830 00:16"
        },
        {
          "id": "MDRW2100023258.1.31",
          "form": "조심하셨어요? ㅎㅎ",
          "original_form": "조심하셨어요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:17"
        },
        {
          "id": "MDRW2100023258.1.32",
          "form": "아일랜드도 주변에서 갔다고 들은적이 없어서",
          "original_form": "아일랜드도 주변에서 갔다고 들은적이 없어서",
          "speaker_id": "1",
          "time": "20210830 00:17"
        },
        {
          "id": "MDRW2100023258.1.33",
          "form": "되게 새로워요~",
          "original_form": "되게 새로워요~",
          "speaker_id": "1",
          "time": "20210830 00:17"
        },
        {
          "id": "MDRW2100023258.1.34",
          "form": "새로운 여행지를 알게된듯한 ㅎ",
          "original_form": "새로운 여행지를 알게된듯한 ㅎ",
          "speaker_id": "1",
          "time": "20210830 00:17"
        }
      ]
