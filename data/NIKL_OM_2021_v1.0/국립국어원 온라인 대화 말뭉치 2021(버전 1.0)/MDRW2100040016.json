      "utterance": [
        {
          "id": "MDRW2100040016.1.1",
          "form": "다른 건 딱히 주인공이 되고 싶진 않은데",
          "original_form": "다른 건 딱히 주인공이 되고 싶진 않은데",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100040016.1.2",
          "form": "꽃남만큼은 달걀도 맞을 수 있다!",
          "original_form": "꽃남만큼은 달걀도 맞을 수 있다!",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100040016.1.3",
          "form": "심지어 잘나가는 모델이 금잔디 짝사랑? 하지 않낭ㅅ?",
          "original_form": "심지어 잘나가는 모델이 금잔디 짝사랑? 하지 않낭ㅅ?",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100040016.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ앗 이부분은 뭔가 정확히 기억이 안나요 ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ앗 이부분은 뭔가 정확히 기억이 안나요 ㅜ",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100040016.1.5",
          "form": "짝사랑이라고 하니 제 머릿속엔 오로지 명예소방관만이 떠오릅니다..",
          "original_form": "짝사랑이라고 하니 제 머릿속엔 오로지 명예소방관만이 떠오릅니다..",
          "speaker_id": "2",
          "time": "20210904 00:38"
        },
        {
          "id": "MDRW2100040016.1.6",
          "form": "아놔 명예소방관",
          "original_form": "아놔 명예소방관",
          "speaker_id": "1",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100040016.1.7",
          "form": "그그 금잔디가",
          "original_form": "그그 금잔디가",
          "speaker_id": "1",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100040016.1.8",
          "form": "죽으려는 형 구해줘서",
          "original_form": "죽으려는 형 구해줘서",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100040016.1.9",
          "form": "그 동생이 금잔디 꼬시는데",
          "original_form": "그 동생이 금잔디 꼬시는데",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100040016.1.10",
          "form": "알고보니 구준표 엄마의 사주를 받은..",
          "original_form": "알고보니 구준표 엄마의 사주를 받은..",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100040016.1.11",
          "form": "아아아아ㅏ~~~ 기억났어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아아아아ㅏ~~~ 기억났어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100040016.1.12",
          "form": "근데 찐 사랑하게 되어버린",
          "original_form": "근데 찐 사랑하게 되어버린",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100040016.1.13",
          "form": "그 모델 말입니가?",
          "original_form": "그 모델 말입니가?",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100040016.1.14",
          "form": "맞어요!!! 구준표 기억 상실증에",
          "original_form": "맞어요!!! 구준표 기억 상실증에",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100040016.1.15",
          "form": "한 몫하는 고녀석이요!!",
          "original_form": "한 몫하는 고녀석이요!!",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100040016.1.16",
          "form": "ㅠ.^ 금잔디도 납치하는..",
          "original_form": "ㅠ.^ 금잔디도 납치하는..",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100040016.1.17",
          "form": "완전 밷 가이",
          "original_form": "완전 밷 가이",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100040016.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ아ㅜ기억상실증하니가 생각났는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아ㅜ기억상실증하니가 생각났는데",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100040016.1.19",
          "form": "그쯤 여자 하나 더 나오자낳요",
          "original_form": "그쯤 여자 하나 더 나오자낳요",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
