        {
          "id": "MDRW2100018335.1.1",
          "form": "저녙은 그래도 탄수화물 최소화하고\r\n맛있는거 해줄게",
          "original_form": "저녙은 그래도 탄수화물 최소화하고\r\n맛있는거 해줄게",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018335.1.2",
          "form": "그래도 탄수화물이 맛있긴 한데",
          "original_form": "그래도 탄수화물이 맛있긴 한데",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018335.1.3",
          "form": "그건 당연한거지만 ㅎㅎㅎ",
          "original_form": "그건 당연한거지만 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018335.1.4",
          "form": "탄수화물이 없다니 ㅜㅜ",
          "original_form": "탄수화물이 없다니 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100018335.1.5",
          "form": "그대신 꼬기꼬기 마니 먹음대징 ㅎㅎ",
          "original_form": "그대신 꼬기꼬기 마니 먹음대징 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018335.1.6",
          "form": "탄수화물 +고기가 더맛니긴 하잖아",
          "original_form": "탄수화물 +고기가 더맛니긴 하잖아",
          "speaker_id": "1",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018335.1.7",
          "form": "그래도 탄수화물은 좀 쥴여보자 ㅎㅎ",
          "original_form": "그래도 탄수화물은 좀 쥴여보자 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018335.1.8",
          "form": "웅웅 메뉴는 뭐야?",
          "original_form": "웅웅 메뉴는 뭐야?",
          "speaker_id": "1",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100018335.1.9",
          "form": "삼결 숙주볶음",
          "original_form": "삼결 숙주볶음",
          "speaker_id": "2",
          "time": "20210826 23:59"
        },
        {
          "id": "MDRW2100018335.1.10",
          "form": "살찌는거 아냐?",
          "original_form": "살찌는거 아냐?",
