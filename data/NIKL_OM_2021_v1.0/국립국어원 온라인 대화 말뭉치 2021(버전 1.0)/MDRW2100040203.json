      "utterance": [
        {
          "id": "MDRW2100040203.1.1",
          "form": "안녕하세요~~~",
          "original_form": "안녕하세요~~~",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100040203.1.2",
          "form": "저는 보름에 한 번",
          "original_form": "저는 보름에 한 번",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100040203.1.3",
          "form": "엄마가 머리 다듬어주세요!",
          "original_form": "엄마가 머리 다듬어주세요!",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100040203.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100040203.1.5",
          "form": "어머니가 다듬어주세요? 우와... 미용사신가요?",
          "original_form": "어머니가 다듬어주세요? 우와... 미용사신가요?",
          "speaker_id": "1",
          "time": "20210904 13:48"
        },
        {
          "id": "MDRW2100040203.1.6",
          "form": "아뇨아눀ㅋㅋㅋ 엄마가",
          "original_form": "아뇨아눀ㅋㅋㅋ 엄마가",
          "speaker_id": "2",
          "time": "20210904 13:49"
        },
        {
          "id": "MDRW2100040203.1.7",
          "form": "유튜브 보면서 독학하시고",
          "original_form": "유튜브 보면서 독학하시고",
          "speaker_id": "2",
          "time": "20210904 13:49"
        },
        {
          "id": "MDRW2100040203.1.8",
          "form": "저 해주시는거예요!!!",
          "original_form": "저 해주시는거예요!!!",
          "speaker_id": "2",
          "time": "20210904 13:49"
        },
        {
          "id": "MDRW2100040203.1.9",
          "form": "코로나 때문에 미용실 가기 겁나니까",
          "original_form": "코로나 때문에 미용실 가기 겁나니까",
          "speaker_id": "2",
          "time": "20210904 13:49"
        },
        {
          "id": "MDRW2100040203.1.10",
          "form": "비용도 아낄 겸,,, 감염위험도 줄일 겸,,,",
          "original_form": "비용도 아낄 겸,,, 감염위험도 줄일 겸,,,",
          "speaker_id": "2",
          "time": "20210904 13:49"
        },
        {
          "id": "MDRW2100040203.1.11",
          "form": "어머..대박대박...!!금손이신가보다...........ㅋㅋㅋㅋㅋ유투브독학...!!우와 금손금손",
          "original_form": "어머..대박대박...!!금손이신가보다...........ㅋㅋㅋㅋㅋ유투브독학...!!우와 금손금손",
          "speaker_id": "1",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100040203.1.12",
          "form": "앜ㅋㅋㅋ 저도 저희 엄마가 대단하긴 해요...",
          "original_form": "앜ㅋㅋㅋ 저도 저희 엄마가 대단하긴 해요...",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100040203.1.13",
          "form": "name1님은 머리 새로 하셨나요?!",
          "original_form": "&name1&님은 머리 새로 하셨나요?!",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100040203.1.14",
          "form": "저는 새로 하고 싶은데.. 엄두가 안나요.. 무슨 머리하지 싶어서요 ㅋㅋ 파마할까.. 싶기도 한데 후.. 파마 망한 적이 많아서 ㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "저는 새로 하고 싶은데.. 엄두가 안나요.. 무슨 머리하지 싶어서요 ㅋㅋ 파마할까.. 싶기도 한데 후.. 파마 망한 적이 많아서 ㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100040203.1.15",
          "form": "일단 앞머리도 너무 많이 길러놔서 앞머리부터 혼자 자를까 했는데",
          "original_form": "일단 앞머리도 너무 많이 길러놔서 앞머리부터 혼자 자를까 했는데",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040203.1.16",
          "form": "저번에 혼자 잘랐다가 폭망해서....",
          "original_form": "저번에 혼자 잘랐다가 폭망해서....",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040203.1.17",
          "form": "그냥 집게핀 꽂고 있어요^^ 간편",
          "original_form": "그냥 집게핀 꽂고 있어요^^ 간편",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100040203.1.18",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100040203.1.19",
          "form": "저는 머리 길었을 때도 똥머리 같은 걸 혼자 못묶어서",
          "original_form": "저는 머리 길었을 때도 똥머리 같은 걸 혼자 못묶어서",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100040203.1.20",
          "form": "엄두도 안내는데... name1님은 그래도 도전정신이 강하시네요! ",
          "original_form": "엄두도 안내는데... &name1&님은 그래도 도전정신이 강하시네요! ",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100040203.1.21",
          "form": "그런가요?! ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런가요?! ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100040203.1.22",
          "form": "요즘 머리 피부 다 손보고 싶어서 안달이에요",
          "original_form": "요즘 머리 피부 다 손보고 싶어서 안달이에요",
          "speaker_id": "1",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100040203.1.23",
          "form": "앗 그쵸그쵸 ㅋㅋㅋㅌㅌㅌ",
          "original_form": "앗 그쵸그쵸 ㅋㅋㅋㅌㅌㅌ",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100040203.1.24",
          "form": "저는 요즘 귀를 뚫고싶은데",
          "original_form": "저는 요즘 귀를 뚫고싶은데",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100040203.1.25",
          "form": "염증날까봐 걱정이에요!",
          "original_form": "염증날까봐 걱정이에요!",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100040203.1.26",
          "form": "아~ 피어싱하실건가요?",
          "original_form": "아~ 피어싱하실건가요?",
          "speaker_id": "1",
          "time": "20210906 19:04"
        },
        {
          "id": "MDRW2100040203.1.27",
          "form": "아직은 안했는데 해보고 싶더라구요!",
          "original_form": "아직은 안했는데 해보고 싶더라구요!",
          "speaker_id": "2",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100040203.1.28",
          "form": "name1님은 귀 뚫으셨나요?",
          "original_form": "&name1&님은 귀 뚫으셨나요?",
          "speaker_id": "2",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100040203.1.29",
          "form": "저는 귓볼만 뚫었다가 지금은 막혔어요!!ㅎㅎㅎㅎ 귓바퀴에 하나해보고싶어요..",
          "original_form": "저는 귓볼만 뚫었다가 지금은 막혔어요!!ㅎㅎㅎㅎ 귓바퀴에 하나해보고싶어요..",
          "speaker_id": "1",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100040203.1.30",
          "form": "우와 !!! 역시 멋쟁이시네요,,,",
          "original_form": "우와 !!! 역시 멋쟁이시네요,,,",
          "speaker_id": "2",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100040203.1.31",
          "form": "저는 아직 귀를 안뚫었거든요ㅠ",
          "original_form": "저는 아직 귀를 안뚫었거든요ㅠ",
          "speaker_id": "2",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100040203.1.32",
          "form": "아~ㅋㅋㅋ 막상 뚫으려니 귀찮으셔서 그런거죠?ㅋㅋㅋ 아니면 무서워서...?!?!",
          "original_form": "아~ㅋㅋㅋ 막상 뚫으려니 귀찮으셔서 그런거죠?ㅋㅋㅋ 아니면 무서워서...?!?!",
          "speaker_id": "1",
          "time": "20210906 19:28"
        },
        {
          "id": "MDRW2100040203.1.33",
          "form": "좀 답답한 이야기지만",
          "original_form": "좀 답답한 이야기지만",
