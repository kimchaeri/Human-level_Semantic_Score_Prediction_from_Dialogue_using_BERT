      "utterance": [
        {
          "id": "MDRW2100023821.1.1",
          "form": "오즈의 마법사도 재밌게 봤어요",
          "original_form": "오즈의 마법사도 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210830 18:06"
        },
        {
          "id": "MDRW2100023821.1.2",
          "form": "동화책으로만본거같아요!",
          "original_form": "동화책으로만본거같아요!",
          "speaker_id": "1",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023821.1.3",
          "form": "그것도 시리즈가있던거같은데!",
          "original_form": "그것도 시리즈가있던거같은데!",
          "speaker_id": "1",
          "time": "20210830 18:07"
        },
        {
          "id": "MDRW2100023821.1.4",
          "form": "네 생각보다 되게 길어요ㅋㅋㅋㅋ",
          "original_form": "네 생각보다 되게 길어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100023821.1.5",
          "form": "어릴때 만화로본기억은있는데 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어릴때 만화로본기억은있는데 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100023821.1.6",
          "form": "막 에메랄드 도시같은곳에 가고",
          "original_form": "막 에메랄드 도시같은곳에 가고",
          "speaker_id": "1",
          "time": "20210830 18:10"
        },
        {
          "id": "MDRW2100023821.1.7",
          "form": "네네 도로시가 돌아간 뒤에도 문제가 생기는 후속? 2편?도 있고 그래욬ㅋㅋㅋ",
          "original_form": "네네 도로시가 돌아간 뒤에도 문제가 생기는 후속? 2편?도 있고 그래욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:11"
        },
        {
          "id": "MDRW2100023821.1.8",
          "form": "전 빨간머리앤 시리즈 좋아했어요!",
          "original_form": "전 빨간머리앤 시리즈 좋아했어요!",
          "speaker_id": "1",
          "time": "20210830 18:18"
        },
        {
          "id": "MDRW2100023821.1.9",
          "form": "저도 그거 봤어요!",
          "original_form": "저도 그거 봤어요!",
          "speaker_id": "2",
          "time": "20210830 18:30"
        },
        {
          "id": "MDRW2100023821.1.10",
          "form": "책도 만화도 모두 사랑스럽죠 ㅜㅜ  너무좋아서 원서까지 읽었어요 ㅋㅋㅋ",
          "original_form": "책도 만화도 모두 사랑스럽죠 ㅜㅜ  너무좋아서 원서까지 읽었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100023821.1.11",
          "form": "원서요? 와 대단하세요",
          "original_form": "원서요? 와 대단하세요",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100023821.1.12",
          "form": "원서는 굉장히.. 기독교적이에요 ㅋㅋ",
          "original_form": "원서는 굉장히.. 기독교적이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100023821.1.13",
          "form": "오... 그건 몰랐는데요",
          "original_form": "오... 그건 몰랐는데요",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100023821.1.14",
          "form": "성경을 알아야 알아먹을 내용들이 꽤 나와요",
          "original_form": "성경을 알아야 알아먹을 내용들이 꽤 나와요",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100023821.1.15",
          "form": "성경 읽으신 적 있나요?",
          "original_form": "성경 읽으신 적 있나요?",
          "speaker_id": "2",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100023821.1.16",
          "form": "네 어릴때 교회에 다녔거든요!",
          "original_form": "네 어릴때 교회에 다녔거든요!",
          "speaker_id": "1",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100023821.1.17",
          "form": "어릴때 교회다녔어서 성경 읽어봤어요!",
          "original_form": "어릴때 교회다녔어서 성경 읽어봤어요!",
          "speaker_id": "1",
          "time": "20210831 00:30"
        }
      ]
