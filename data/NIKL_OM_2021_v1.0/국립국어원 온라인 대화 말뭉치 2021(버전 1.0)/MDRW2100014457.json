      "utterance": [
        {
          "id": "MDRW2100014457.1.1",
          "form": "요즘엔 뭐 많이 보시나요?",
          "original_form": "요즘엔 뭐 많이 보시나요?",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014457.1.2",
          "form": "전 펜트 하우스요 ㅋㅋ",
          "original_form": "전 펜트 하우스요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014457.1.3",
          "form": "전지적 참견시점하고",
          "original_form": "전지적 참견시점하고",
          "speaker_id": "1",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014457.1.4",
          "form": "으아 전 얘기만 들어봤어요",
          "original_form": "으아 전 얘기만 들어봤어요",
          "speaker_id": "2",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014457.1.5",
          "form": "전참시 저도 좋아해요ㅋㅋ",
          "original_form": "전참시 저도 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014457.1.6",
          "form": "아저도 얘기만 듣다 발을 들여서 ㅋㅋ",
          "original_form": "아저도 얘기만 듣다 발을 들여서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014457.1.7",
          "form": "펜하는 다음 시즌도 있나요?",
          "original_form": "펜하는 다음 시즌도 있나요?",
          "speaker_id": "2",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014457.1.8",
          "form": "최근에 유세윤도 웃겼는데ㅋ",
          "original_form": "최근에 유세윤도 웃겼는데ㅋ",
          "speaker_id": "2",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014457.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014457.1.10",
          "form": "우세윤 ㅋㅋ",
          "original_form": "우세윤 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014457.1.11",
          "form": "둘이 너무 웃겨요",
          "original_form": "둘이 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014457.1.12",
          "form": "궁합이 저런거구나 싶어요ㅋㅋ",
          "original_form": "궁합이 저런거구나 싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014457.1.13",
          "form": "저도 참 어른이 저리노네 하며 잼났어요",
          "original_form": "저도 참 어른이 저리노네 하며 잼났어요",
          "speaker_id": "1",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014457.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014457.1.15",
          "form": "둘리 아주 영혼의 짝이예요",
          "original_form": "둘리 아주 영혼의 짝이예요",
          "speaker_id": "1",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014457.1.16",
          "form": "둘이",
          "original_form": "둘이",
          "speaker_id": "1",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014457.1.17",
          "form": "펜하 2해요곧 끝나요",
          "original_form": "펜하 2해요곧 끝나요",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014457.1.18",
          "form": "드라마 뭐보세요?",
          "original_form": "드라마 뭐보세요?",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014457.1.19",
          "form": "전 드라마 안 본지 오래예요",
          "original_form": "전 드라마 안 본지 오래예요",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014457.1.20",
          "form": "예능만 주로 봅니다ㅋㅋ",
          "original_form": "예능만 주로 봅니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014457.1.21",
          "form": "드라마는 계속 길게 봐야해서",
          "original_form": "드라마는 계속 길게 봐야해서",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014457.1.22",
          "form": "저도 저것만봐요",
          "original_form": "저도 저것만봐요",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014457.1.23",
          "form": "도깨비 때가 젤좋았어요",
          "original_form": "도깨비 때가 젤좋았어요",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014457.1.24",
          "form": "예능만 보게돼요",
          "original_form": "예능만 보게돼요",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014457.1.25",
          "form": "만날 죽다 살던데",
          "original_form": "만날 죽다 살던데",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014457.1.26",
          "form": "맞아요 도깨비",
          "original_form": "맞아요 도깨비",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014457.1.27",
          "form": "아 이동욱 ㅋㅋ",
          "original_form": "아 이동욱 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014457.1.28",
          "form": "공유도 좋지만요 ㅋㅋ",
          "original_form": "공유도 좋지만요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:22"
        }
      ]
    }
