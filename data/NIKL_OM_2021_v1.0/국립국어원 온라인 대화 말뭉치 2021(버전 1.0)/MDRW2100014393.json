      "utterance": [
        {
          "id": "MDRW2100014393.1.1",
          "form": "맞아요. 직장 근처면 마치고 바로 달려갈 수 있죠",
          "original_form": "맞아요. 직장 근처면 마치고 바로 달려갈 수 있죠",
          "speaker_id": "1",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014393.1.2",
          "form": "그니까요 \r\n집 들어가는 순간 나오기 싫어지더라구요",
          "original_form": "그니까요 \r\n집 들어가는 순간 나오기 싫어지더라구요",
          "speaker_id": "2",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014393.1.3",
          "form": "집 들어간 뒤에 나오는사람은 진짜 독한사람이에요ㅋㅋㅋ",
          "original_form": "집 들어간 뒤에 나오는사람은 진짜 독한사람이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014393.1.4",
          "form": "ㅋㅋㅋ마자요 진짜ㅋㅋㄱ\r\n그사람은 뭘해도 성공할사람ㅋㅋ",
          "original_form": "ㅋㅋㅋ마자요 진짜ㅋㅋㄱ\r\n그사람은 뭘해도 성공할사람ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014393.1.5",
          "form": "일 끝나고 집 들어가면 나올땐 다음날이죠",
          "original_form": "일 끝나고 집 들어가면 나올땐 다음날이죠",
          "speaker_id": "1",
          "time": "20210825 18:54"
        },
        {
          "id": "MDRW2100014393.1.6",
          "form": "그쵸ㅋㅋㅋ집 들어가면 나오는거 아니죠ㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋ집 들어가면 나오는거 아니죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014393.1.7",
          "form": "지금 일은 하고계신게 있나요?",
          "original_form": "지금 일은 하고계신게 있나요?",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014393.1.8",
          "form": "네  직장 다니고있어요!",
          "original_form": "네  직장 다니고있어요!",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014393.1.9",
          "form": "어떤 직종이세요?",
          "original_form": "어떤 직종이세요?",
          "speaker_id": "1",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014393.1.10",
          "form": "저는 식품품질관리 하고있어요",
          "original_form": "저는 식품품질관리 하고있어요",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014393.1.11",
          "form": "어떤 일 하시나요?",
          "original_form": "어떤 일 하시나요?",
          "speaker_id": "2",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014393.1.12",
          "form": "저는 카페알바하고있어요",
          "original_form": "저는 카페알바하고있어요",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014393.1.13",
          "form": "주말 오픈이라 거의 청소가 메인이네요",
          "original_form": "주말 오픈이라 거의 청소가 메인이네요",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014393.1.14",
          "form": "주말이면 손님 많이 오겠네요",
          "original_form": "주말이면 손님 많이 오겠네요",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014393.1.15",
          "form": "프랜차이즈 카페인가요?",
          "original_form": "프랜차이즈 카페인가요?",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014393.1.16",
          "form": "작은 동네 카페입니다",
          "original_form": "작은 동네 카페입니다",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014393.1.17",
          "form": "진짜 청소하는게 거의  업무예요ㅎㅎ",
          "original_form": "진짜 청소하는게 거의  업무예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014393.1.18",
          "form": "저는 동네 카페가 더  분위기 있고 좋더라구요",
          "original_form": "저는 동네 카페가 더  분위기 있고 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014393.1.19",
          "form": "청소할 게 엄청 많으신가봐요",
          "original_form": "청소할 게 엄청 많으신가봐요",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014393.1.20",
          "form": "아침에 청소하고 오전엔 요즘 손님이 거의 없어서ㅋㅋ",
          "original_form": "아침에 청소하고 오전엔 요즘 손님이 거의 없어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014393.1.21",
          "form": "그쵸 코로나땜에..",
          "original_form": "그쵸 코로나땜에..",
          "speaker_id": "2",
