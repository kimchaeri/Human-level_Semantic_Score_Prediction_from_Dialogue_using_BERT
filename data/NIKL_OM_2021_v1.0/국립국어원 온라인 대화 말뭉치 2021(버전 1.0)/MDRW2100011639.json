      "utterance": [
        {
          "id": "MDRW2100011639.1.1",
          "form": "오늘 뭘 먹었나요",
          "original_form": "오늘 뭘 먹었나요",
          "speaker_id": "1",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011639.1.2",
          "form": "안녕하세요 name1님^^",
          "original_form": "안녕하세요 &name1&님^^",
          "speaker_id": "2",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011639.1.3",
          "form": "떡볶이랑 설빙먹음",
          "original_form": "떡볶이랑 설빙먹음",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011639.1.4",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011639.1.5",
          "form": "무슨 떡볶이 먹었어?",
          "original_form": "무슨 떡볶이 먹었어?",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011639.1.6",
          "form": "사람들이 떡군이네 맛있다해서",
          "original_form": "사람들이 떡군이네 맛있다해서",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011639.1.7",
          "form": "시켜먹어봤어",
          "original_form": "시켜먹어봤어",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011639.1.8",
          "form": "너는 점심뭐먹어성",
          "original_form": "너는 점심뭐먹어성",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011639.1.9",
          "form": "먹었어",
          "original_form": "먹었어",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011639.1.10",
          "form": "난 라면 먹었어",
          "original_form": "난 라면 먹었어",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011639.1.11",
          "form": "진라면",
          "original_form": "진라면",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011639.1.12",
          "form": "매운맛",
          "original_form": "매운맛",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011639.1.13",
          "form": "난 라면 끓일때 스프 먼저 넣는데",
          "original_form": "난 라면 끓일때 스프 먼저 넣는데",
          "speaker_id": "1",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011639.1.14",
          "form": "넌 어때?",
          "original_form": "넌 어때?",
          "speaker_id": "1",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011639.1.15",
          "form": "물부터 끓여야지",
          "original_form": "물부터 끓여야지",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011639.1.16",
          "form": "무조건 조리법대로",
          "original_form": "무조건 조리법대로",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011639.1.17",
          "form": "아 그건 당연하지;;",
          "original_form": "아 그건 당연하지;;",
          "speaker_id": "1",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011639.1.18",
          "form": "아..그렇구나^^",
          "original_form": "아..그렇구나^^",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011639.1.19",
          "form": "물 없이 스프만 넣니???",
          "original_form": "물 없이 스프만 넣니???",
          "speaker_id": "1",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100011639.1.20",
          "form": "그럴수도있지",
          "original_form": "그럴수도있지",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011639.1.21",
          "form": "편견없는사람이 있을수도있잖아",
          "original_form": "편견없는사람이 있을수도있잖아",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011639.1.22",
          "form": "ㅉ...ㅉ",
          "original_form": "ㅉ...ㅉ",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011639.1.23",
          "form": "난 면부터넣는데...-_-",
          "original_form": "난 면부터넣는데...-_-",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011639.1.24",
          "form": "스프가 다 타겠구나 친구야",
          "original_form": "스프가 다 타겠구나 친구야",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011639.1.25",
          "form": "난아니라고",
          "original_form": "난아니라고",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011639.1.26",
          "form": "근데 스프부터 넣으면",
          "original_form": "근데 스프부터 넣으면",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011639.1.27",
          "form": "수증기 땜에 뜨거워서",
          "original_form": "수증기 땜에 뜨거워서",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011639.1.28",
          "form": "서면맛집은 뭐가있니",
          "original_form": "서면맛집은 뭐가있니",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011639.1.29",
          "form": "그 위에 스프 넣는게 편하긴 해",
          "original_form": "그 위에 스프 넣는게 편하긴 해",
          "speaker_id": "1",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011639.1.30",
          "form": "조리법에서 시작해서 산으로가는중",
          "original_form": "조리법에서 시작해서 산으로가는중",
          "speaker_id": "2",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011639.1.31",
          "form": "그래 니말이맞아",
          "original_form": "그래 니말이맞아",
          "speaker_id": "2",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011639.1.32",
          "form": "서면에 맛집은",
          "original_form": "서면에 맛집은",
          "speaker_id": "1",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011639.1.33",
          "form": "별로 없는듯",
          "original_form": "별로 없는듯",
          "speaker_id": "1",
          "time": "20210824 14:48"
        }
      ]
    }
  ]
