      "utterance": [
        {
          "id": "MDRW2100019702.1.1",
          "form": "반려동물 있으신가요?",
          "original_form": "반려동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019702.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100019702.1.3",
          "form": "현재는 키우는 동물이 없어요ㅠ",
          "original_form": "현재는 키우는 동물이 없어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019702.1.4",
          "form": "name1님은 있으세요?",
          "original_form": "&name1&님은 있으세요?",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019702.1.5",
          "form": "저도요 ㅜ",
          "original_form": "저도요 ㅜ",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019702.1.6",
          "form": "전 키워본적조차 없어요",
          "original_form": "전 키워본적조차 없어요",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019702.1.7",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019702.1.8",
          "form": "저는 되게 많이 키워보긴했는데",
          "original_form": "저는 되게 많이 키워보긴했는데",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019702.1.9",
          "form": "크면서 미안해서 못키우겠더라고요ㅠ",
          "original_form": "크면서 미안해서 못키우겠더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100019702.1.10",
          "form": "아 그래요? ㅠㅠ 전 그마음을 잘 모르겠어요 경험이없는지라",
          "original_form": "아 그래요? ㅠㅠ 전 그마음을 잘 모르겠어요 경험이없는지라",
          "speaker_id": "1",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100019702.1.11",
          "form": "저는 몇년전에 기니피그를 키웠었는데 죽어서 넘 슬펐던 기억이있어여",
          "original_form": "저는 몇년전에 기니피그를 키웠었는데 죽어서 넘 슬펐던 기억이있어여",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019702.1.12",
          "form": "그래서 이제는 잘 못키우겠더라고용ㅎㅎ",
          "original_form": "그래서 이제는 잘 못키우겠더라고용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019702.1.13",
          "form": "아 너무 마음아팠을거같아요 ㅠㅠ",
          "original_form": "아 너무 마음아팠을거같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019702.1.14",
          "form": "제 친구도 그런말 하더라구요 ㅜㅜ",
          "original_form": "제 친구도 그런말 하더라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019702.1.15",
          "form": "주변에 보면 그런사람 많더라고요!",
          "original_form": "주변에 보면 그런사람 많더라고요!",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019702.1.16",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019702.1.17",
          "form": "ㅜㅜ 좋아하는 동물은 있으세요?",
          "original_form": "ㅜㅜ 좋아하는 동물은 있으세요?",
          "speaker_id": "1",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019702.1.18",
          "form": "강아지랑 토끼좋아해요!!",
          "original_form": "강아지랑 토끼좋아해요!!",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019702.1.19",
          "form": "저도 둘다 좋아해요 ㅠㅜ 너무 귀엽죠",
          "original_form": "저도 둘다 좋아해요 ㅠㅜ 너무 귀엽죠",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019702.1.20",
          "form": "맞아요!!! 보고만 있어도 힐링이에요",
          "original_form": "맞아요!!! 보고만 있어도 힐링이에요",
          "speaker_id": "2",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019702.1.21",
          "form": "요새는 동물 영상도 많아서 좋더라구요",
          "original_form": "요새는 동물 영상도 많아서 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 21:40"
        }
