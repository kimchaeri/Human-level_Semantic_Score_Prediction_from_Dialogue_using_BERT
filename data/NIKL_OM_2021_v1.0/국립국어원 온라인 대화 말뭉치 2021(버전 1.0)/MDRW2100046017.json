      "utterance": [
        {
          "id": "MDRW2100046017.1.1",
          "form": "이것저것 많이 샀지ㅋ",
          "original_form": "이것저것 많이 샀지ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046017.1.2",
          "form": "많이샀네",
          "original_form": "많이샀네",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046017.1.3",
          "form": "기저귀 바우처가 아직남아서 얼른써야함",
          "original_form": "기저귀 바우처가 아직남아서 얼른써야함",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046017.1.4",
          "form": "아 얼른사야겠네",
          "original_form": "아 얼른사야겠네",
          "speaker_id": "2",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100046017.1.5",
          "form": "응 근데요즘 기저귀가 핫딜이 잘안떠ㅠㅠ",
          "original_form": "응 근데요즘 기저귀가 핫딜이 잘안떠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100046017.1.6",
          "form": "에헤이 세일끝나기전에 떠야하는데ㅠㅠ",
          "original_form": "에헤이 세일끝나기전에 떠야하는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100046017.1.7",
          "form": "그러이 기저귀 뜯을때마다 쫄림",
          "original_form": "그러이 기저귀 뜯을때마다 쫄림",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046017.1.8",
          "form": "그맘알지ㅋㅋ어여사라",
          "original_form": "그맘알지ㅋㅋ어여사라",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046017.1.9",
          "form": "핫딜뜨면왕창 사야지",
          "original_form": "핫딜뜨면왕창 사야지",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046017.1.10",
          "form": "어여 뜨길ㅋ",
          "original_form": "어여 뜨길ㅋ",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046017.1.11",
          "form": "맥스드라이만 안떠ㅠㅠ",
          "original_form": "맥스드라이만 안떠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046017.1.12",
          "form": "다른것도한번 써봐",
          "original_form": "다른것도한번 써봐",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046017.1.13",
          "form": "한번도 안사봐서ㅋ 안사지네 선뜻",
          "original_form": "한번도 안사봐서ㅋ 안사지네 선뜻",
