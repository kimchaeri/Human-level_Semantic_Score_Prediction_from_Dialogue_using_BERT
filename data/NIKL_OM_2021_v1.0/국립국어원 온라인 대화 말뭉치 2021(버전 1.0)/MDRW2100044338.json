      "utterance": [
        {
          "id": "MDRW2100044338.1.1",
          "form": "맛있게 먹었다니 다행이네",
          "original_form": "맛있게 먹었다니 다행이네",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044338.1.2",
          "form": "엄마가 해주는 나물 반찬은 진짜 맛있는 듯",
          "original_form": "엄마가 해주는 나물 반찬은 진짜 맛있는 듯",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044338.1.3",
          "form": "내가  나물 반찬은 잘하지",
          "original_form": "내가  나물 반찬은 잘하지",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044338.1.4",
          "form": "진짜로 엄마가 하는 나물 반찬은 파는 것보다 더 맛있어",
          "original_form": "진짜로 엄마가 하는 나물 반찬은 파는 것보다 더 맛있어",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044338.1.5",
          "form": "너네 외할머니가 그렇게 잘했어",
          "original_form": "너네 외할머니가 그렇게 잘했어",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044338.1.6",
          "form": "어쩐지 이",
          "original_form": "어쩐지 이",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044338.1.7",
          "form": "엄마 반찬 솜씨를 배워야 되는데",
          "original_form": "엄마 반찬 솜씨를 배워야 되는데",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044338.1.8",
          "form": "빨리 배워",
          "original_form": "빨리 배워",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044338.1.9",
          "form": "근데 내가 하면 그 맛이 안 난단 말이야",
          "original_form": "근데 내가 하면 그 맛이 안 난단 말이야",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044338.1.10",
          "form": "그것도 많이 해봐 야 나는 거야",
          "original_form": "그것도 많이 해봐 야 나는 거야",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044338.1.11",
          "form": "근데 내가 요리에는 이 경 취미가 없어서",
          "original_form": "근데 내가 요리에는 이 경 취미가 없어서",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044338.1.12",
          "form": "그래도 해봐야지",
          "original_form": "그래도 해봐야지",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044338.1.13",
          "form": "많이 해봐야 잘하지",
          "original_form": "많이 해봐야 잘하지",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044338.1.14",
          "form": "나중에 시간되면 할게요",
          "original_form": "나중에 시간되면 할게요",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044338.1.15",
          "form": "그때가 언제인지 모르겠네",
          "original_form": "그때가 언제인지 모르겠네",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044338.1.16",
          "form": "언젠가 그때 겠지",
          "original_form": "언젠가 그때 겠지",
          "speaker_id": "1",
          "time": "20210907 22:23"
        }
