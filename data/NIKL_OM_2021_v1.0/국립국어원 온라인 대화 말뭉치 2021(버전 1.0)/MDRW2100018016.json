      "utterance": [
        {
          "id": "MDRW2100018016.1.1",
          "form": "날씨가 제법 쌀쌀해 졌어요",
          "original_form": "날씨가 제법 쌀쌀해 졌어요",
          "speaker_id": "1",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100018016.1.2",
          "form": "그러게요. 쌀쌀하네요",
          "original_form": "그러게요. 쌀쌀하네요",
          "speaker_id": "2",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100018016.1.3",
          "form": "환절기 건강 유의 하세요",
          "original_form": "환절기 건강 유의 하세요",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018016.1.4",
          "form": "그래도 오늘은 덥지 않았어요?",
          "original_form": "그래도 오늘은 덥지 않았어요?",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018016.1.5",
          "form": "저는 쌀쌀했던거 같은데 지역이 어디세요?",
          "original_form": "저는 쌀쌀했던거 같은데 지역이 어디세요?",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018016.1.6",
          "form": "저는 경기도 입니다",
          "original_form": "저는 경기도 입니다",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018016.1.7",
          "form": "여기는 부산이예요",
          "original_form": "여기는 부산이예요",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018016.1.8",
          "form": "우와 부산 너무 좋아요",
          "original_form": "우와 부산 너무 좋아요",
          "speaker_id": "1",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018016.1.9",
          "form": "살기엔 좋아요",
          "original_form": "살기엔 좋아요",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100018016.1.10",
          "form": "너무 부러워요",
          "original_form": "너무 부러워요",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018016.1.11",
          "form": "작년에 송도 갔는데 너무 좋았어요",
          "original_form": "작년에 송도 갔는데 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018016.1.12",
          "form": "부산은 따뜻한 편이죠?",
          "original_form": "부산은 따뜻한 편이죠?",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018016.1.13",
          "form": "아 송도요? ㅋ 근데 부산 살면 오히려 여름에 바다 안가요",
          "original_form": "아 송도요? ㅋ 근데 부산 살면 오히려 여름에 바다 안가요",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018016.1.14",
          "form": "그럼 겨울 바다에 가시나요?",
          "original_form": "그럼 겨울 바다에 가시나요?",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018016.1.15",
          "form": "여름 빼고 ㅋ  여름엔 너무 사람이 많아서요",
          "original_form": "여름 빼고 ㅋ  여름엔 너무 사람이 많아서요",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018016.1.16",
          "form": "저같은 사람들이 가서 그런가바요ㅋㅋ",
          "original_form": "저같은 사람들이 가서 그런가바요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018016.1.17",
          "form": "부산 사람은 언제든 갈 수 있자나요ㅋ",
          "original_form": "부산 사람은 언제든 갈 수 있자나요ㅋ",
