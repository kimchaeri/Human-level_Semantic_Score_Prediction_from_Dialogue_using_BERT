      "utterance": [
        {
          "id": "MDRW2100022740.1.1",
          "form": "근데 자기들이 대중에 키치는 영향좀",
          "original_form": "근데 자기들이 대중에 키치는 영향좀",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022740.1.2",
          "form": "생각해줬으면 좋겠다",
          "original_form": "생각해줬으면 좋겠다",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022740.1.3",
          "form": "맞아ㅠㅠㅠ",
          "original_form": "맞아ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.4",
          "form": "그 펫키지 댓글반응 보려고",
          "original_form": "그 펫키지 댓글반응 보려고",
          "speaker_id": "1",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.6",
          "form": "유튜브 클립영상 들어갔는데",
          "original_form": "유튜브 클립영상 들어갔는데",
          "speaker_id": "1",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.7",
          "form": "누가ㅋㅋㅋㅋ",
          "original_form": "누가ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.8",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.9",
          "form": "태연이 키우는개 얼마에요?",
          "original_form": "태연이 키우는개 얼마에요?",
          "speaker_id": "1",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.11",
          "form": "이렇게 써놓은거보고",
          "original_form": "이렇게 써놓은거보고",
          "speaker_id": "1",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.12",
          "form": "기함했다 진짜",
          "original_form": "기함했다 진짜",
          "speaker_id": "1",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.13",
          "form": "하.....",
          "original_form": "하.....",
          "speaker_id": "2",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.14",
          "form": "근데 진짜 한 생명을",
          "original_form": "근데 진짜 한 생명을",
          "speaker_id": "2",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.15",
          "form": "저렇게 돈으로만 생각한다는게",
          "original_form": "저렇게 돈으로만 생각한다는게",
          "speaker_id": "2",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.16",
          "form": "너무 소름끼쳐",
          "original_form": "너무 소름끼쳐",
          "speaker_id": "2",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022740.1.17",
          "form": "저러다 사람 때려놓고 얼마에요?",
          "original_form": "저러다 사람 때려놓고 얼마에요?",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022740.1.18",
          "form": "내말이..진짜 유해한문환데",
          "original_form": "내말이..진짜 유해한문환데",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022740.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022740.1.20",
          "form": "물어보는건 시간 문제 아니냐...",
          "original_form": "물어보는건 시간 문제 아니냐...",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022740.1.21",
          "form": "그건이미 옛날부터 있엇지않나",
          "original_form": "그건이미 옛날부터 있엇지않나",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022740.1.22",
          "form": "여자보고 얼마면돼?",
          "original_form": "여자보고 얼마면돼?",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022740.1.23",
          "form": "그렇긴 한데",
          "original_form": "그렇긴 한데",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022740.1.24",
          "form": "이...gg ㄹ ㅏ ㄹ",
          "original_form": "이...gg ㄹ ㅏ ㄹ",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022740.1.25",
          "form": "으",
          "original_form": "으",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022740.1.26",
          "form": "ㅅ...ㅂ",
          "original_form": "ㅅ...ㅂ",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022740.1.27",
          "form": "암튼 우리나라 유기동물 너무 심함",
          "original_form": "암튼 우리나라 유기동물 너무 심함",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022740.1.28",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022740.1.29",
          "form": "코로나 끝나면 또 우르르 버려질꺼라며",
          "original_form": "코로나 끝나면 또 우르르 버려질꺼라며",
          "speaker_id": "2",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022740.1.30",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022740.1.31",
          "form": "지금 집에만있으니까",
          "original_form": "지금 집에만있으니까",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022740.1.32",
          "form": "심심하다고 막 입양하는건가?",
          "original_form": "심심하다고 막 입양하는건가?",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022740.1.33",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022740.1.34",
          "form": "외롭다고 막 입양한다더라",
          "original_form": "외롭다고 막 입양한다더라",
          "speaker_id": "2",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022740.1.35",
          "form": "으..",
          "original_form": "으..",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022740.1.36",
          "form": "근데 이런거 관련해서 좀 어떻게 했으면 좋겠다",
          "original_form": "근데 이런거 관련해서 좀 어떻게 했으면 좋겠다",
          "speaker_id": "2",
          "time": "20210829 17:36"
        }
