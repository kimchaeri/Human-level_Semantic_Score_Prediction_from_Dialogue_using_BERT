      "utterance": [
        {
          "id": "MDRW2100001963.1.1",
          "form": "요즘 하는 일은 어때",
          "original_form": "요즘 하는 일은 어때",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001963.1.2",
          "form": "요새 별로 안바쁜데",
          "original_form": "요새 별로 안바쁜데",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001963.1.3",
          "form": "좀 어려운일 해서 신경쓸게 많다",
          "original_form": "좀 어려운일 해서 신경쓸게 많다",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001963.1.4",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001963.1.5",
          "form": "많이 어렵나",
          "original_form": "많이 어렵나",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001963.1.6",
          "form": "많이는 아닌데",
          "original_form": "많이는 아닌데",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001963.1.7",
          "form": "잘 해야돼서 ㅋㅋ",
          "original_form": "잘 해야돼서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001963.1.8",
          "form": "우리 딸은 잘 하니까 ㅋㅋ",
          "original_form": "우리 딸은 잘 하니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001963.1.9",
          "form": "ㅋㅋ 그렇지요",
          "original_form": "ㅋㅋ 그렇지요",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001963.1.10",
          "form": "잘하니까 더 부담이 있지만 ㅠㅋㅋ",
          "original_form": "잘하니까 더 부담이 있지만 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001963.1.11",
          "form": "넘 부담 갖지마라",
          "original_form": "넘 부담 갖지마라",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001963.1.12",
          "form": "옹 ㅜㅜ그러려고 한다",
          "original_form": "옹 ㅜㅜ그러려고 한다",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001963.1.13",
          "form": "구래 부담을 떨쳐버리고",
          "original_form": "구래 부담을 떨쳐버리고",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001963.1.14",
          "form": "할수 있는 만큼만",
          "original_form": "할수 있는 만큼만",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001963.1.15",
          "form": "옹옹! 할 수 있는 만큼만 열심히",
          "original_form": "옹옹! 할 수 있는 만큼만 열심히",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001963.1.16",
          "form": "그래 그거다",
          "original_form": "그래 그거다",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001963.1.17",
