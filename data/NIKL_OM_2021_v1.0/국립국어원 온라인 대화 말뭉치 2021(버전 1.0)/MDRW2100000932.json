      "utterance": [
        {
          "id": "MDRW2100000932.1.1",
          "form": "싸이월드 다시 들어가봤나?",
          "original_form": "싸이월드 다시 들어가봤나?",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000932.1.2",
          "form": "에 지금도 들어갈수있나?",
          "original_form": "에 지금도 들어갈수있나?",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000932.1.3",
          "form": "아니 내 계정 왜 살려내?ㅠ 다시 없애줬으면 좋겠댜",
          "original_form": "아니 내 계정 왜 살려내?ㅠ 다시 없애줬으면 좋겠댜",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000932.1.4",
          "form": "아직 준비중인줄 알았는데",
          "original_form": "아직 준비중인줄 알았는데",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000932.1.5",
          "form": "어플로 다시 태어난 더 아니가?",
          "original_form": "어플로 다시 태어난 더 아니가?",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.6",
          "form": "모르겠자 다시 준비중인가",
          "original_form": "모르겠자 다시 준비중인가",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.7",
          "form": "근데 내 건 없애줬으면 좋겠다ㅠ",
          "original_form": "근데 내 건 없애줬으면 좋겠다ㅠ",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.8",
          "form": "뭔가 들어갈 수 있으면",
          "original_form": "뭔가 들어갈 수 있으면",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.9",
          "form": "커뮤니티에 난리날꺼같은데",
          "original_form": "커뮤니티에 난리날꺼같은데",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.10",
          "form": "우리가 모를리가없다",
          "original_form": "우리가 모를리가없다",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.11",
          "form": "한메일이라서 나는 아이디찾기도 안되고 내 정보만 남잖아ㅠㅠ",
          "original_form": "한메일이라서 나는 아이디찾기도 안되고 내 정보만 남잖아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.12",
          "form": "최든에 커뮤니티에서 뭐 본 거 같은데",
          "original_form": "최든에 커뮤니티에서 뭐 본 거 같은데",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.13",
          "form": "내가 그 글을 눌러본 게 아니라서 잘 모르겜ㅅ네",
          "original_form": "내가 그 글을 눌러본 게 아니라서 잘 모르겜ㅅ네",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.14",
          "form": "한번 검색해봐야겠네",
          "original_form": "한번 검색해봐야겠네",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.15",
          "form": "나는 싸이월드에 뭐 많이 안남겨서",
          "original_form": "나는 싸이월드에 뭐 많이 안남겨서",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.16",
          "form": "내싸이보다 더 문제인건",
          "original_form": "내싸이보다 더 문제인건",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.17",
          "form": "나도 그냥 다이어리 이런거",
          "original_form": "나도 그냥 다이어리 이런거",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.18",
          "form": "엄마 싸이월드임",
          "original_form": "엄마 싸이월드임",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.19",
          "form": "공유다이어리!",
          "original_form": "공유다이어리!",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000932.1.20",
          "form": "엄마 싸이월드에 뭐 했는데?",
          "original_form": "엄마 싸이월드에 뭐 했는데?",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.21",
          "form": "거기 내사진 걍 한가득",
          "original_form": "거기 내사진 걍 한가득",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.22",
          "form": "니 사진있나?",
          "original_form": "니 사진있나?",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.23",
          "form": "아 그러면 나도",
          "original_form": "아 그러면 나도",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.24",
          "form": "물론 애기때사진이지만",
          "original_form": "물론 애기때사진이지만",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.25",
          "form": "나도 엄마 싸이랑 고모 싸이월드",
          "original_form": "나도 엄마 싸이랑 고모 싸이월드",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.26",
          "form": "한 초등학생?",
          "original_form": "한 초등학생?",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.27",
          "form": "완전 다채로운 표정 가득한데",
          "original_form": "완전 다채로운 표정 가득한데",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.28",
          "form": "나 목욕하는 사진도 있는 거 같은데",
          "original_form": "나 목욕하는 사진도 있는 거 같은데",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.29",
          "form": "완전 어린이라서",
          "original_form": "완전 어린이라서",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ안돼 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ안돼 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.31",
          "form": "싸이월드 부활 그냥 하지말지ㅠ",
          "original_form": "싸이월드 부활 그냥 하지말지ㅠ",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000932.1.32",
          "form": "프라이버시를 지켜주세요",
          "original_form": "프라이버시를 지켜주세요",
          "speaker_id": "2",
          "time": "20210819 20:42"
        }
      ]
    }
