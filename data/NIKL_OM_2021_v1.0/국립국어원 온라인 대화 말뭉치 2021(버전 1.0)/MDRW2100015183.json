      "utterance": [
        {
          "id": "MDRW2100015183.1.1",
          "form": "ㅠㅠ건강이 최고잖아요",
          "original_form": "ㅠㅠ건강이 최고잖아요",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015183.1.2",
          "form": "몸조리 잘하세요 백신ㅠ",
          "original_form": "몸조리 잘하세요 백신ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015183.1.3",
          "form": "네엡 ㅎ",
          "original_form": "네엡 ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015183.1.4",
          "form": "그래서 주제도 미용과 건강으루!",
          "original_form": "그래서 주제도 미용과 건강으루!",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015183.1.5",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015183.1.6",
          "form": "건강이ㅠ참",
          "original_form": "건강이ㅠ참",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015183.1.7",
          "form": "최곤데 힘들죠",
          "original_form": "최곤데 힘들죠",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015183.1.8",
          "form": "인정 ㅠㅜ",
          "original_form": "인정 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015183.1.9",
          "form": "ㅠㅠ아프지마세요",
          "original_form": "ㅠㅠ아프지마세요",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015183.1.10",
          "form": "네엡 걱정 감사합니다! ㅎ",
          "original_form": "네엡 걱정 감사합니다! ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015183.1.11",
          "form": "저도 백신을 맞아야할텐데ㅠ",
          "original_form": "저도 백신을 맞아야할텐데ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015183.1.12",
          "form": "사전예약은 하셨어요????",
          "original_form": "사전예약은 하셨어요????",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015183.1.13",
          "form": "아직이요ㅎ",
          "original_form": "아직이요ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015183.1.14",
          "form": "미루다보니 아직안했네요",
          "original_form": "미루다보니 아직안했네요",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015183.1.15",
          "form": "ㅎㄹㄹ",
          "original_form": "ㅎㄹㄹ",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015183.1.16",
          "form": "얼른 예약하세요!",
          "original_form": "얼른 예약하세요!",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015183.1.17",
          "form": "좀이라도 빨리 맞아야 6시이후에도 3명이서 만나죠 ㅋㅋ",
          "original_form": "좀이라도 빨리 맞아야 6시이후에도 3명이서 만나죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015183.1.18",
          "form": "ㅋㅋㅋ 요즘",
          "original_form": "ㅋㅋㅋ 요즘",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015183.1.19",
          "form": "2인밖에안되나요?",
          "original_form": "2인밖에안되나요?",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015183.1.20",
          "form": "네 아직 4단계라",
          "original_form": "네 아직 4단계라",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015183.1.21",
          "form": "6시 이전은 4명까지 집합되고 6~9시까지 2명이요",
          "original_form": "6시 이전은 4명까지 집합되고 6~9시까지 2명이요",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015183.1.22",
          "form": "6시 이후에도 백신 맞은 사람있으면 4명까지 추가로 되요",
          "original_form": "6시 이후에도 백신 맞은 사람있으면 4명까지 추가로 되요",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015183.1.23",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015183.1.24",
          "form": "넷다 맞아야하나요?",
          "original_form": "넷다 맞아야하나요?",
          "speaker_id": "1",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015183.1.25",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210826 09:10"
        },
        {
          "id": "MDRW2100015183.1.26",
          "form": "두 명만 맞으면 되요",
          "original_form": "두 명만 맞으면 되요",
          "speaker_id": "2",
          "time": "20210826 09:10"
        },
        {
          "id": "MDRW2100015183.1.27",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100015183.1.28",
          "form": "꽤 합리적이네요",
          "original_form": "꽤 합리적이네요",
          "speaker_id": "1",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100015183.1.29",
          "form": "ㅋㅋㅋㅋ name1님 동네는 4단계 아니신거예요???",
          "original_form": "ㅋㅋㅋㅋ &name1&님 동네는 4단계 아니신거예요???",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100015183.1.30",
          "form": "처음 들어보신것처럼 말씀하셔서",
          "original_form": "처음 들어보신것처럼 말씀하셔서",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100015183.1.31",
          "form": "4단계맞죠 서울이니",
          "original_form": "4단계맞죠 서울이니",
          "speaker_id": "1",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100015183.1.32",
          "form": "그런세부사항은 잘몰랐네요ㅋㅋㅋ",
          "original_form": "그런세부사항은 잘몰랐네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100015183.1.33",
          "form": "취준하시느라 속세와는 관심이 멀어지셨군요 ㄷ",
          "original_form": "취준하시느라 속세와는 관심이 멀어지셨군요 ㄷ",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100015183.1.34",
          "form": "어제는 백신맞고 죽을 것 같았는데 이제 좀 살 것 같네여",
          "original_form": "어제는 백신맞고 죽을 것 같았는데 이제 좀 살 것 같네여",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100015183.1.35",
          "form": "ㅋㅋㅋ 맞아요그거죠ㅋㅋ",
          "original_form": "ㅋㅋㅋ 맞아요그거죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100015183.1.36",
          "form": "좀괜찮나요ㅠ",
          "original_form": "좀괜찮나요ㅠ",
          "speaker_id": "1",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100015183.1.37",
          "form": "2차는 언제옝ᆢ",
          "original_form": "2차는 언제옝ᆢ",
          "speaker_id": "1",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100015183.1.38",
          "form": "예요",
          "original_form": "예요",
          "speaker_id": "1",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100015183.1.39",
          "form": "점심먹고나서 많이 괜찮아졌어요 ㅋㅋ",
          "original_form": "점심먹고나서 많이 괜찮아졌어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100015183.1.40",
          "form": "아직 2차일정은 안 나왔습니다!",
          "original_form": "아직 2차일정은 안 나왔습니다!",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100015183.1.41",
          "form": "다행이네요ㄷㄷ",
          "original_form": "다행이네요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
