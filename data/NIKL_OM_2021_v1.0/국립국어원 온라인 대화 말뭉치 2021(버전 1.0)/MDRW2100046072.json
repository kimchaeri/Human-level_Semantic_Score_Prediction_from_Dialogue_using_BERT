      "utterance": [
        {
          "id": "MDRW2100046072.1.1",
          "form": "근데 빨간지프는",
          "original_form": "근데 빨간지프는",
          "speaker_id": "2",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046072.1.2",
          "form": "너무 예쁘긴 해요...",
          "original_form": "너무 예쁘긴 해요...",
          "speaker_id": "2",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046072.1.3",
          "form": "ganzi",
          "original_form": "ganzi",
          "speaker_id": "1",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046072.1.4",
          "form": "진짜요ㅠㅠㅠㅠㅠ",
          "original_form": "진짜요ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046072.1.5",
          "form": "죵나 예뻐요",
          "original_form": "죵나 예뻐요",
          "speaker_id": "2",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046072.1.6",
          "form": "한번",
          "original_form": "한번",
          "speaker_id": "1",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046072.1.7",
          "form": "시승이라도해봤음좋겠네용 ㅋㅋ",
          "original_form": "시승이라도해봤음좋겠네용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.8",
          "form": "맞아요ㅋㅋㅋ 나중에 해봐요!",
          "original_form": "맞아요ㅋㅋㅋ 나중에 해봐요!",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.9",
          "form": "아 근데 오토바이는 진짜",
          "original_form": "아 근데 오토바이는 진짜",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.10",
          "form": "무서운 거 같아요",
          "original_form": "무서운 거 같아요",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.11",
          "form": "맞이요",
          "original_form": "맞이요",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.12",
          "form": "핫딜 안 하는 사람이 이해안가는 것처럼",
          "original_form": "핫딜 안 하는 사람이 이해안가는 것처럼",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.13",
          "form": "쫄보라 절대 탈생각없음",
          "original_form": "쫄보라 절대 탈생각없음",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.14",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.15",
          "form": "오토바이 타는 사람도 이해 안 돼요ㅠㅠ",
          "original_form": "오토바이 타는 사람도 이해 안 돼요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.16",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.17",
          "form": "막 안전용품 사서 타던데",
          "original_form": "막 안전용품 사서 타던데",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.18",
          "form": "그래봤자ㅠㅠ",
          "original_form": "그래봤자ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.19",
          "form": "저는 전에",
          "original_form": "저는 전에",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046072.1.20",
          "form": "전동킥보드탓다가너무무서워서진짜",
          "original_form": "전동킥보드탓다가너무무서워서진짜",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046072.1.21",
          "form": "ㅋㅋㅋ...",
          "original_form": "ㅋㅋㅋ...",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046072.1.22",
          "form": "맞아요ㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046072.1.23",
          "form": "그거 지지할 데도 없고",
          "original_form": "그거 지지할 데도 없고",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046072.1.24",
          "form": "넘어지면 끝이에요",
          "original_form": "넘어지면 끝이에요",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046072.1.25",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046072.1.26",
          "form": "얼굴 갈림",
          "original_form": "얼굴 갈림",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046072.1.27",
          "form": "느리게가는데도넘무섭던데",
          "original_form": "느리게가는데도넘무섭던데",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046072.1.28",
          "form": "다른사람들은 쌩쌩잘타더라구요",
          "original_form": "다른사람들은 쌩쌩잘타더라구요",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046072.1.29",
          "form": "싱기..",
          "original_form": "싱기..",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046072.1.30",
          "form": "신기방기...",
          "original_form": "신기방기...",
          "speaker_id": "2",
          "time": "20210908 10:07"
        }
      ]
    }
