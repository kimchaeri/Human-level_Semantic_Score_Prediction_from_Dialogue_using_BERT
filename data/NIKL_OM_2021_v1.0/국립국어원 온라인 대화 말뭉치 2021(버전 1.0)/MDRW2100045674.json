      "utterance": [
        {
          "id": "MDRW2100045674.1.1",
          "form": "떡볶이 치즈밥 야기했오요",
          "original_form": "떡볶이 치즈밥 야기했오요",
          "speaker_id": "1",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045674.1.2",
          "form": "그럼 이번엔 초밥 이야기해볼까요?",
          "original_form": "그럼 이번엔 초밥 이야기해볼까요?",
          "speaker_id": "2",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045674.1.3",
          "form": "꺄악 초밥 짱맛",
          "original_form": "꺄악 초밥 짱맛",
          "speaker_id": "1",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045674.1.4",
          "form": "회전초밥집 가고싶네요ㅠㅠㅠ",
          "original_form": "회전초밥집 가고싶네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045674.1.5",
          "form": "아 저두요 회전초밥집 가면 아직 오고있는데 일어나서 가져오는거 국룰이죠?",
          "original_form": "아 저두요 회전초밥집 가면 아직 오고있는데 일어나서 가져오는거 국룰이죠?",
          "speaker_id": "1",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045674.1.6",
          "form": "아 당근이죠~~~ 그거 어느 세월에 기다려요!",
          "original_form": "아 당근이죠~~~ 그거 어느 세월에 기다려요!",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045674.1.7",
          "form": "너무 느려요 그거ㅋㅋㅋㅋ",
          "original_form": "너무 느려요 그거ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045674.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸! 저만 답답한게 아니었어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쵸! 저만 답답한게 아니었어요",
          "speaker_id": "1",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045674.1.9",
          "form": "그리고 맛없는거만 계속 돌고",
          "original_form": "그리고 맛없는거만 계속 돌고",
          "speaker_id": "1",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045674.1.10",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045674.1.11",
          "form": "저거 어느 세월에 없어지지 싶은ㅋㅋㅋ",
          "original_form": "저거 어느 세월에 없어지지 싶은ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045674.1.12",
          "form": "긍까요",
          "original_form": "긍까요",
          "speaker_id": "1",
          "time": "20210908 05:18"
        },
        {
          "id": "MDRW2100045674.1.13",
          "form": "소고기 초밥도 먹고 싶어요",
          "original_form": "소고기 초밥도 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210908 05:18"
        },
        {
          "id": "MDRW2100045674.1.14",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210908 05:18"
        },
        {
          "id": "MDRW2100045674.1.15",
          "form": "네ㅠㅠㅠㅠㅠㅠ진짜 맛있어요",
          "original_form": "네ㅠㅠㅠㅠㅠㅠ진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210908 05:18"
        },
        {
          "id": "MDRW2100045674.1.16",
          "form": "소고기는 뭔들!",
          "original_form": "소고기는 뭔들!",
          "speaker_id": "2",
          "time": "20210908 05:18"
        },
        {
          "id": "MDRW2100045674.1.17",
          "form": "초밥중에 무슨 초밥이 제일 많이 팔릴까요?",
          "original_form": "초밥중에 무슨 초밥이 제일 많이 팔릴까요?",
          "speaker_id": "1",
          "time": "20210908 05:19"
        },
        {
          "id": "MDRW2100045674.1.18",
          "form": "흠...너무 어려운데요?! 찍어보자면 보편적 취향의 연어?",
          "original_form": "흠...너무 어려운데요?! 찍어보자면 보편적 취향의 연어?",
          "speaker_id": "2",
          "time": "20210908 05:20"
        },
        {
          "id": "MDRW2100045674.1.19",
          "form": "저는 광어 할래요!",
          "original_form": "저는 광어 할래요!",
          "speaker_id": "1",
          "time": "20210908 05:20"
        },
        {
          "id": "MDRW2100045674.1.20",
          "form": "생각보다 연어가 기름지다고 생각하는 분들도 계시더라구요",
          "original_form": "생각보다 연어가 기름지다고 생각하는 분들도 계시더라구요",
          "speaker_id": "1",
          "time": "20210908 05:21"
        },
        {
          "id": "MDRW2100045674.1.21",
          "form": "아아 오 광어가 정답일 것 같아요",
          "original_form": "아아 오 광어가 정답일 것 같아요",
          "speaker_id": "2",
          "time": "20210908 05:21"
        },
        {
          "id": "MDRW2100045674.1.22",
          "form": "바로 설득당했어요ㅋㅋㅋㅋ",
          "original_form": "바로 설득당했어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:21"
        },
        {
          "id": "MDRW2100045674.1.23",
