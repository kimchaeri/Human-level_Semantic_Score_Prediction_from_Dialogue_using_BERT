      "utterance": [
        {
          "id": "MDRW2100043400.1.1",
          "form": "뜨거운음식인가요?",
          "original_form": "뜨거운음식인가요?",
          "speaker_id": "2",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043400.1.2",
          "form": "제가 오늘 먹을 저녁 맞춰보세요 후후",
          "original_form": "제가 오늘 먹을 저녁 맞춰보세요 후후",
          "speaker_id": "1",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043400.1.3",
          "form": "네 뜨거운 음식입니다!",
          "original_form": "네 뜨거운 음식입니다!",
          "speaker_id": "1",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043400.1.4",
          "form": "맵나요?",
          "original_form": "맵나요?",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100043400.1.5",
          "form": "음.. 사람마다 다를 것 같은데 일단 빨간 음식이예요",
          "original_form": "음.. 사람마다 다를 것 같은데 일단 빨간 음식이예요",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100043400.1.6",
          "form": "떡볶이!",
          "original_form": "떡볶이!",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043400.1.7",
          "form": "땡~ 떡볶이 아닙니다~~~",
          "original_form": "땡~ 떡볶이 아닙니다~~~",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043400.1.8",
          "form": "음 빨간음식",
          "original_form": "음 빨간음식",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043400.1.9",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043400.1.10",
          "form": "뢔면인가요?",
          "original_form": "뢔면인가요?",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043400.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ라면도 아닙니다! 아쉽습니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ라면도 아닙니다! 아쉽습니다",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043400.1.12",
          "form": "낫 뢔면",
          "original_form": "낫 뢔면",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043400.1.13",
          "form": "아 뭘까요",
          "original_form": "아 뭘까요",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043400.1.14",
          "form": "밀가루인가요?",
          "original_form": "밀가루인가요?",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043400.1.15",
          "form": "후후후 막 음식 관련 뭐든 물어보세요",
          "original_form": "후후후 막 음식 관련 뭐든 물어보세요",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043400.1.16",
          "form": "밀가루 안 들어가요!",
          "original_form": "밀가루 안 들어가요!",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043400.1.17",
          "form": "아악 더 모르겠어요",
          "original_form": "아악 더 모르겠어요",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043400.1.18",
          "form": "고기인가요?",
          "original_form": "고기인가요?",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043400.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043400.1.20",
          "form": "헉 네 고기 들어갑니다!",
          "original_form": "헉 네 고기 들어갑니다!",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043400.1.21",
          "form": "헐 혹이",
          "original_form": "헐 혹이",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043400.1.22",
          "form": "치킨?",
          "original_form": "치킨?",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043400.1.23",
          "form": "아 치킨은 밀가루있구나",
          "original_form": "아 치킨은 밀가루있구나",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043400.1.24",
          "form": "허어억 아닙니다! 맛있겠다",
          "original_form": "허어억 아닙니다! 맛있겠다",
          "speaker_id": "1",
          "time": "20210907 18:55"
        }
      ]
