      "utterance": [
        {
          "id": "MDRW2100000367.1.1",
          "form": "name2님 한식 vs 중식 vs 일식 어떤게좋으세요? ㅎㅎ",
          "original_form": "&name2&님 한식 vs 중식 vs 일식 어떤게좋으세요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000367.1.2",
          "form": "다 좋아요. 그렇지만 굳이 고르자면 일식?",
          "original_form": "다 좋아요. 그렇지만 굳이 고르자면 일식?",
          "speaker_id": "2",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000367.1.3",
          "form": "초밥 너무 좋아하거든요.",
          "original_form": "초밥 너무 좋아하거든요.",
          "speaker_id": "2",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000367.1.4",
          "form": "헉 저두 초밥 사랑해용",
          "original_form": "헉 저두 초밥 사랑해용",
          "speaker_id": "1",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000367.1.5",
          "form": "근데 막 잘먹진않아서 연어랑 광어만 ㅋㅋㅋ좋아해요",
          "original_form": "근데 막 잘먹진않아서 연어랑 광어만 ㅋㅋㅋ좋아해요",
          "speaker_id": "1",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000367.1.6",
          "form": "저는 단새우초밥 너무 좋아해요! 연어도 좋고",
          "original_form": "저는 단새우초밥 너무 좋아해요! 연어도 좋고",
          "speaker_id": "2",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000367.1.7",
          "form": "요즘엔 세상이 좋아져서 배달 너무 잘 오더라고요?",
          "original_form": "요즘엔 세상이 좋아져서 배달 너무 잘 오더라고요?",
          "speaker_id": "2",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000367.1.8",
          "form": "맞아용 코로나때매 식당가기두 애매한데",
          "original_form": "맞아용 코로나때매 식당가기두 애매한데",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000367.1.9",
          "form": "배달이 넘 잘돼있어서 금방오구 좋더라구용 ㅋㅋㅋ",
          "original_form": "배달이 넘 잘돼있어서 금방오구 좋더라구용 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000367.1.10",
          "form": "그래서 배민도 매월 vip에요!!",
          "original_form": "그래서 배민도 매월 vip에요!!",
          "speaker_id": "1",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000367.1.11",
          "form": "그러게요! 배민원이라는 것도 생겨서",
          "original_form": "그러게요! 배민원이라는 것도 생겨서",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000367.1.12",
          "form": "오래 기다리지도 않고요",
          "original_form": "오래 기다리지도 않고요",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000367.1.13",
          "form": "헉 배민원이 뭐에요??? 처음들어봐용",
          "original_form": "헉 배민원이 뭐에요??? 처음들어봐용",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000367.1.14",
          "form": "쿠팡 치타배달처럼",
          "original_form": "쿠팡 치타배달처럼",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000367.1.15",
          "form": "한 집만 배달하는 서비스요!",
          "original_form": "한 집만 배달하는 서비스요!",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000367.1.16",
          "form": "헐 우와 그런게있어요??? 배민 들어가면 보이나용?? ㅋㅋㅋㅋ",
          "original_form": "헐 우와 그런게있어요??? 배민 들어가면 보이나용?? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000367.1.17",
          "form": "지금 접속해봅니다...돈 더 비싸구 그런가??",
          "original_form": "지금 접속해봅니다...돈 더 비싸구 그런가??",
          "speaker_id": "1",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000367.1.18",
          "form": "아니요 똑같아요!",
          "original_form": "아니요 똑같아요!",
          "speaker_id": "2",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100000367.1.19",
          "form": "쿠팡잇츠에 자극받아서 하는 서비스인듯!",
          "original_form": "쿠팡잇츠에 자극받아서 하는 서비스인듯!",
          "speaker_id": "2",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100000367.1.20",
          "form": "와 그렇군요 배달잘알이셨네용 ㅋㅋㅋㅋ",
          "original_form": "와 그렇군요 배달잘알이셨네용 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100000367.1.21",
          "form": "오늘 저녁은 뭐드셨나용 ㅋㅋ",
          "original_form": "오늘 저녁은 뭐드셨나용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100000367.1.22",
          "form": "부찌 먹었어요! 부대찌개",
          "original_form": "부찌 먹었어요! 부대찌개",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100000367.1.23",
          "form": "반조리로 된 거 사 봤는데 맛있네요",
          "original_form": "반조리로 된 거 사 봤는데 맛있네요",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100000367.1.24",
          "form": "요리 좋아하세요?",
          "original_form": "요리 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100000367.1.25",
          "form": "요리하는게 재밌긴한데 맛이없어서 즐기진않아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "요리하는게 재밌긴한데 맛이없어서 즐기진않아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:44"
        },
        {
          "id": "MDRW2100000367.1.26",
          "form": "분명 백종원레시피였는데..",
          "original_form": "분명 백종원레시피였는데..",
          "speaker_id": "1",
          "time": "20210819 19:44"
        },
        {
          "id": "MDRW2100000367.1.27",
          "form": "name2님은용?!",
          "original_form": "&name2&님은용?!",
          "speaker_id": "1",
          "time": "20210819 19:44"
        },
        {
          "id": "MDRW2100000367.1.28",
          "form": "저는 재미도 못느끼는 것 같아요",
          "original_form": "저는 재미도 못느끼는 것 같아요",
          "speaker_id": "2",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000367.1.29",
          "form": "기껏해야 반조리?",
          "original_form": "기껏해야 반조리?",
          "speaker_id": "2",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000367.1.30",
          "form": "저는 설거지가 더 재밌어요 차라리!",
          "original_form": "저는 설거지가 더 재밌어요 차라리!",
