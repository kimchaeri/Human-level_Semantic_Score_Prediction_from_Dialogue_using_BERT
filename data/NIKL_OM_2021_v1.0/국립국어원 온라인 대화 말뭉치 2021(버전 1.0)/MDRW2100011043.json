      "utterance": [
        {
          "id": "MDRW2100011043.1.1",
          "form": "너 다이어트 해본적있어?",
          "original_form": "너 다이어트 해본적있어?",
          "speaker_id": "2",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100011043.1.2",
          "form": "다이어트 시작하려는데 어떤 음식이 좋을까",
          "original_form": "다이어트 시작하려는데 어떤 음식이 좋을까",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011043.1.3",
          "form": "나 오늘 북촌화로에 가봤어",
          "original_form": "나 오늘 북촌화로에 가봤어",
          "speaker_id": "1",
          "time": "20210824 00:18"
        },
        {
          "id": "MDRW2100011043.1.4",
          "form": "오 그게 식당이름이야?",
          "original_form": "오 그게 식당이름이야?",
          "speaker_id": "2",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.5",
          "form": "뭘파는데??",
          "original_form": "뭘파는데??",
          "speaker_id": "2",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.6",
          "form": "응. 대구에 있어",
          "original_form": "응. 대구에 있어",
          "speaker_id": "1",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.7",
          "form": "고깃집이야",
          "original_form": "고깃집이야",
          "speaker_id": "1",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.8",
          "form": "점심땐 점심특선을 팔아.",
          "original_form": "점심땐 점심특선을 팔아.",
          "speaker_id": "1",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.9",
          "form": "음 맛있겠다",
          "original_form": "음 맛있겠다",
          "speaker_id": "2",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.10",
          "form": "사실 석경을 가고싶었는데 줄이 너무 길어서 저기로 갔어.",
          "original_form": "사실 석경을 가고싶었는데 줄이 너무 길어서 저기로 갔어.",
          "speaker_id": "1",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.11",
          "form": "갈비찜이 부드러워",
          "original_form": "갈비찜이 부드러워",
          "speaker_id": "1",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.12",
          "form": "그런 식당에서 파는 점심특선 정말 가성비 좋은 것 같아",
          "original_form": "그런 식당에서 파는 점심특선 정말 가성비 좋은 것 같아",
          "speaker_id": "2",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.13",
          "form": "근데 빨간찜갈비는 아냐.",
          "original_form": "근데 빨간찜갈비는 아냐.",
          "speaker_id": "1",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.14",
          "form": "석경 점심특선 맛이씾",
          "original_form": "석경 점심특선 맛이씾",
          "speaker_id": "2",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.15",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "1",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.16",
          "form": "맛있지",
          "original_form": "맛있지",
          "speaker_id": "2",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.17",
          "form": "간장찜갈비야",
          "original_form": "간장찜갈비야",
          "speaker_id": "1",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.18",
          "form": "거긴 정말 사람이 너무많아",
          "original_form": "거긴 정말 사람이 너무많아",
          "speaker_id": "2",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.19",
          "form": "근데.수성구 석경은 맛이 다르대",
          "original_form": "근데.수성구 석경은 맛이 다르대",
          "speaker_id": "1",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011043.1.20",
          "form": "우와 그것도 맛있겠다",
          "original_form": "우와 그것도 맛있겠다",
          "speaker_id": "2",
          "time": "20210824 00:32"
        },
        {
          "id": "MDRW2100011043.1.21",
          "form": "더 맛있대?",
          "original_form": "더 맛있대?",
          "speaker_id": "2",
          "time": "20210824 00:32"
        },
        {
          "id": "MDRW2100011043.1.22",
          "form": "석경 이사가고 난 후로 불편해",
          "original_form": "석경 이사가고 난 후로 불편해",
          "speaker_id": "1",
          "time": "20210824 00:32"
        },
        {
          "id": "MDRW2100011043.1.23",
          "form": "차댈데가 더 없어",
          "original_form": "차댈데가 더 없어",
          "speaker_id": "1",
          "time": "20210824 00:32"
        },
        {
          "id": "MDRW2100011043.1.24",
          "form": "석경 수성구점은 된찌도 맛업ㄱ대",
          "original_form": "석경 수성구점은 된찌도 맛업ㄱ대",
          "speaker_id": "1",
          "time": "20210824 00:32"
        },
        {
          "id": "MDRW2100011043.1.25",
          "form": "맞아맞아 나는 차가 없지만",
          "original_form": "맞아맞아 나는 차가 없지만",
          "speaker_id": "2",
          "time": "20210824 00:32"
        },
        {
          "id": "MDRW2100011043.1.26",
          "form": "그리고 파김치도 없대",
          "original_form": "그리고 파김치도 없대",
          "speaker_id": "1",
          "time": "20210824 00:32"
        },
        {
          "id": "MDRW2100011043.1.27",
          "form": "그런 것 같더라",
          "original_form": "그런 것 같더라",
          "speaker_id": "2",
          "time": "20210824 00:32"
        },
        {
          "id": "MDRW2100011043.1.28",
          "form": "두개는 필수야.",
          "original_form": "두개는 필수야.",
          "speaker_id": "1",
          "time": "20210824 00:32"
        },
        {
          "id": "MDRW2100011043.1.29",
          "form": "맞아맞아.",
          "original_form": "맞아맞아.",
          "speaker_id": "1",
          "time": "20210824 00:32"
        },
        {
          "id": "MDRW2100011043.1.30",
          "form": "생각래보니까 또 먹고싶다.",
          "original_form": "생각래보니까 또 먹고싶다.",
          "speaker_id": "1",
          "time": "20210824 00:32"
        }
      ]
    }
  ]
