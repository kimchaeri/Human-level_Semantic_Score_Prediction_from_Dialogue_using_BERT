      "utterance": [
        {
          "id": "MDRW2100045953.1.1",
          "form": "상받은거던데요",
          "original_form": "상받은거던데요",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045953.1.2",
          "form": "예전에는 녹차 카스테라만먹다가",
          "original_form": "예전에는 녹차 카스테라만먹다가",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045953.1.3",
          "form": "오 저는 빵에 건포도 들어가는걸 싫어하요 ㅠㅠ",
          "original_form": "오 저는 빵에 건포도 들어가는걸 싫어하요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045953.1.4",
          "form": "저도 그거 싫어해서 안먹었는데",
          "original_form": "저도 그거 싫어해서 안먹었는데",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045953.1.5",
          "form": "이건 생크림인가",
          "original_form": "이건 생크림인가",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045953.1.6",
          "form": "그게 맛나더라구요",
          "original_form": "그게 맛나더라구요",
          "speaker_id": "2",
          "time": "20210908 09:03"
        },
        {
          "id": "MDRW2100045953.1.7",
          "form": "오오 생크림은 좋아요 ㅋㅋㅋㅋ",
          "original_form": "오오 생크림은 좋아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045953.1.8",
          "form": "녹차 카스테라 맛잇을거가탕요",
          "original_form": "녹차 카스테라 맛잇을거가탕요",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045953.1.9",
          "form": "허니 카스테라도 맛나구요",
          "original_form": "허니 카스테라도 맛나구요",
          "speaker_id": "2",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045953.1.10",
          "form": "아침먹고왔는데",
          "original_form": "아침먹고왔는데",
          "speaker_id": "2",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045953.1.11",
          "form": "배고파지네요",
          "original_form": "배고파지네요",
          "speaker_id": "2",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045953.1.12",
          "form": "이제 점심 먹을시간을 기다려야겟어요 ㅋㅋ",
          "original_form": "이제 점심 먹을시간을 기다려야겟어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045953.1.13",
          "form": "그때까지 어찌 참죠?ㅎ",
          "original_form": "그때까지 어찌 참죠?ㅎ",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045953.1.14",
          "form": "그러게욬ㅋㅋㅋㅋ 저는 아침도 아직이라",
          "original_form": "그러게욬ㅋㅋㅋㅋ 저는 아침도 아직이라",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045953.1.15",
          "form": "오늘 점심은 머드세요?",
          "original_form": "오늘 점심은 머드세요?",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045953.1.16",
          "form": "아마 짜파게티 먹을거같아요!!!!!",
          "original_form": "아마 짜파게티 먹을거같아요!!!!!",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045953.1.17",
          "form": "저는ㅈ구내식당 ㅋ",
          "original_form": "저는ㅈ구내식당 ㅋ",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045953.1.18",
          "form": "오오 구내식당 밥 잘나와요?",
          "original_form": "오오 구내식당 밥 잘나와요?",
