      "utterance": [
        {
          "id": "MDRW2100011688.1.1",
          "form": "제주도라도 다시 가고싶네",
          "original_form": "제주도라도 다시 가고싶네",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011688.1.2",
          "form": "제주도도 좋지",
          "original_form": "제주도도 좋지",
          "speaker_id": "1",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011688.1.3",
          "form": "미국 어디가고싶은데?",
          "original_form": "미국 어디가고싶은데?",
          "speaker_id": "1",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011688.1.4",
          "form": "제주도 너무 좋았거든",
          "original_form": "제주도 너무 좋았거든",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011688.1.5",
          "form": "미국..어디든",
          "original_form": "미국..어디든",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011688.1.6",
          "form": "지금은 어디든 다 좋을듯",
          "original_form": "지금은 어디든 다 좋을듯",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011688.1.7",
          "form": "애가 가고싶어 한다",
          "original_form": "애가 가고싶어 한다",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011688.1.8",
          "form": "난 싱가폴 생각난다",
          "original_form": "난 싱가폴 생각난다",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011688.1.9",
          "form": "또 가고싶데?",
          "original_form": "또 가고싶데?",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011688.1.10",
          "form": "좋았제",
          "original_form": "좋았제",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011688.1.11",
          "form": "너무 좋았다 코로나 직전이라",
          "original_form": "너무 좋았다 코로나 직전이라",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011688.1.12",
          "form": "어 미국여행",
          "original_form": "어 미국여행",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011688.1.13",
          "form": "부럽네",
          "original_form": "부럽네",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011688.1.14",
          "form": "미국가면 이제 어느정도 알아듣지싶다",
          "original_form": "미국가면 이제 어느정도 알아듣지싶다",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011688.1.15",
          "form": "큰 건이는 자유의 여신상 보고싶데",
          "original_form": "큰 건이는 자유의 여신상 보고싶데",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011688.1.16",
          "form": "대단한데~",
          "original_form": "대단한데~",
          "speaker_id": "1",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011688.1.17",
          "form": "어 당연하지 보고싶지",
          "original_form": "어 당연하지 보고싶지",
          "speaker_id": "2",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011688.1.18",
          "form": "나는 미국보다 유럽",
          "original_form": "나는 미국보다 유럽",
          "speaker_id": "1",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011688.1.19",
          "form": "물어보니 미국 네바다 가고싶데",
          "original_form": "물어보니 미국 네바다 가고싶데",
          "speaker_id": "2",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011688.1.20",
          "form": "유럽의 골목",
          "original_form": "유럽의 골목",
          "speaker_id": "1",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011688.1.21",
          "form": "네바다는 왜?",
          "original_form": "네바다는 왜?",
          "speaker_id": "1",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011688.1.22",
          "form": "미스터리써클 많은곳이라고 하네",
          "original_form": "미스터리써클 많은곳이라고 하네",
          "speaker_id": "2",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011688.1.23",
          "form": "미스터리 써클?",
          "original_form": "미스터리 써클?",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011688.1.24",
          "form": "네바다에 뭐있어?",
          "original_form": "네바다에 뭐있어?",
