      "utterance": [
        {
          "id": "MDRW2100011095.1.1",
          "form": "퇴근은 하셨어요?",
          "original_form": "퇴근은 하셨어요?",
          "speaker_id": "1",
          "time": "20210824 00:52"
        },
        {
          "id": "MDRW2100011095.1.2",
          "form": "아니요 ㅎㅎ 야간이라서",
          "original_form": "아니요 ㅎㅎ 야간이라서",
          "speaker_id": "2",
          "time": "20210824 00:54"
        },
        {
          "id": "MDRW2100011095.1.3",
          "form": "내일 아침 퇴근입니다ㅋㅋ",
          "original_form": "내일 아침 퇴근입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:54"
        },
        {
          "id": "MDRW2100011095.1.4",
          "form": "아 야간이면 몇시퇴근이예요?",
          "original_form": "아 야간이면 몇시퇴근이예요?",
          "speaker_id": "1",
          "time": "20210824 00:54"
        },
        {
          "id": "MDRW2100011095.1.5",
          "form": "원래는 9시요 ㅎㅎ",
          "original_form": "원래는 9시요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:55"
        },
        {
          "id": "MDRW2100011095.1.6",
          "form": "오늘은 당직이라서 5시까지 밤새고 자러가요",
          "original_form": "오늘은 당직이라서 5시까지 밤새고 자러가요",
          "speaker_id": "2",
          "time": "20210824 00:55"
        },
        {
          "id": "MDRW2100011095.1.7",
          "form": "아까 5시나오셨는데 근무시간이 8 시간이 아닌가요?",
          "original_form": "아까 5시나오셨는데 근무시간이 8 시간이 아닌가요?",
          "speaker_id": "1",
          "time": "20210824 00:56"
        },
        {
          "id": "MDRW2100011095.1.8",
          "form": "아 야간은 6-9인데",
          "original_form": "아 야간은 6-9인데",
          "speaker_id": "2",
          "time": "20210824 00:57"
        },
        {
          "id": "MDRW2100011095.1.9",
          "form": "중간에 무급으로 휴게시간이 네시간정도",
          "original_form": "중간에 무급으로 휴게시간이 네시간정도",
          "speaker_id": "2",
          "time": "20210824 00:57"
        },
        {
          "id": "MDRW2100011095.1.10",
          "form": "빠지니까",
          "original_form": "빠지니까",
          "speaker_id": "2",
          "time": "20210824 00:57"
        },
        {
          "id": "MDRW2100011095.1.11",
          "form": "11시간이지 싶네요",
          "original_form": "11시간이지 싶네요",
          "speaker_id": "2",
          "time": "20210824 00:58"
        },
        {
          "id": "MDRW2100011095.1.12",
          "form": "힘들겠어요 밤낮 바뀌면",
          "original_form": "힘들겠어요 밤낮 바뀌면",
          "speaker_id": "1",
          "time": "20210824 00:58"
        },
        {
          "id": "MDRW2100011095.1.13",
          "form": "경찰들 근무하는거랑 비슷한가봐요",
          "original_form": "경찰들 근무하는거랑 비슷한가봐요",
          "speaker_id": "1",
          "time": "20210824 00:58"
        },
        {
          "id": "MDRW2100011095.1.14",
          "form": "네 그래도 잘 맞는 편이라서",
          "original_form": "네 그래도 잘 맞는 편이라서",
          "speaker_id": "2",
          "time": "20210824 00:58"
        },
        {
          "id": "MDRW2100011095.1.15",
          "form": "눈붙이면 잘자요ㅎㅎ",
          "original_form": "눈붙이면 잘자요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:58"
        },
        {
          "id": "MDRW2100011095.1.16",
          "form": "다행이네요 혼자 계시는거예요? 아님여러명 같이 근무?",
          "original_form": "다행이네요 혼자 계시는거예요? 아님여러명 같이 근무?",
          "speaker_id": "1",
          "time": "20210824 00:59"
        },
        {
          "id": "MDRW2100011095.1.17",
          "form": "일할때는 공익까지 4명정도 있고",
          "original_form": "일할때는 공익까지 4명정도 있고",
          "speaker_id": "2",
          "time": "20210824 01:08"
        },
        {
          "id": "MDRW2100011095.1.18",
          "form": "당직은 혼자예요 ㅎㅎ",
          "original_form": "당직은 혼자예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:08"
        },
        {
          "id": "MDRW2100011095.1.19",
          "form": "아 글쿤요 근데 지하철 새벽운행도안하는데 그시간에 무슨일하나요?",
          "original_form": "아 글쿤요 근데 지하철 새벽운행도안하는데 그시간에 무슨일하나요?",
          "speaker_id": "1",
          "time": "20210824 01:09"
        },
        {
          "id": "MDRW2100011095.1.20",
          "form": "역사 내 작업도 있기도 하고 음",
          "original_form": "역사 내 작업도 있기도 하고 음",
          "speaker_id": "2",
          "time": "20210824 01:11"
        },
        {
          "id": "MDRW2100011095.1.21",
          "form": "크게 일은 없어요 사실",
          "original_form": "크게 일은 없어요 사실",
          "speaker_id": "2",
          "time": "20210824 01:12"
        },
        {
          "id": "MDRW2100011095.1.22",
          "form": "경보만 안울리면ㅎㅎ",
          "original_form": "경보만 안울리면ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:12"
        },
        {
          "id": "MDRW2100011095.1.23",
          "form": "ㅎㅎ 그럼 주간근무보단 훨편하겠네요",
          "original_form": "ㅎㅎ 그럼 주간근무보단 훨편하겠네요",
          "speaker_id": "1",
          "time": "20210824 01:12"
        },
        {
          "id": "MDRW2100011095.1.24",
          "form": "네 그쵸ㅋㅋ 근데 막차 전까지 취객이 ㅎㅎ",
          "original_form": "네 그쵸ㅋㅋ 근데 막차 전까지 취객이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:13"
        },
        {
          "id": "MDRW2100011095.1.25",
          "form": "아 그게문제ㅎ",
          "original_form": "아 그게문제ㅎ",
