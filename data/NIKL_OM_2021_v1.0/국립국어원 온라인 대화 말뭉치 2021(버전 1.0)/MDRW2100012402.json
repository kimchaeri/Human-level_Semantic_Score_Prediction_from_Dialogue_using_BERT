      "utterance": [
        {
          "id": "MDRW2100012402.1.1",
          "form": "최근에 산 물건 있으세요?",
          "original_form": "최근에 산 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012402.1.2",
          "form": "동물의숲 타이틀을 샀어요",
          "original_form": "동물의숲 타이틀을 샀어요",
          "speaker_id": "2",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100012402.1.3",
          "form": "게임 좋아하시나봐요",
          "original_form": "게임 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100012402.1.4",
          "form": "게임좋아하죠!",
          "original_form": "게임좋아하죠!",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100012402.1.5",
          "form": "타이틀들은 중고로 살만해서 중고거래로 샀어요",
          "original_form": "타이틀들은 중고로 살만해서 중고거래로 샀어요",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100012402.1.6",
          "form": "저는 오늘 감자칩 거래해요",
          "original_form": "저는 오늘 감자칩 거래해요",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100012402.1.7",
          "form": "과자가 질려가지구",
          "original_form": "과자가 질려가지구",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100012402.1.8",
          "form": "와 그럼 몇개씩 거래하나요?",
          "original_form": "와 그럼 몇개씩 거래하나요?",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100012402.1.9",
          "form": "2개요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "2개요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100012402.1.10",
          "form": "감자 2개랑 식물보호제 같이 팔았어요",
          "original_form": "감자 2개랑 식물보호제 같이 팔았어요",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100012402.1.11",
          "form": "되게 일상적이고 신기하네요ㅋㅋㅋㅋㅋ",
          "original_form": "되게 일상적이고 신기하네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100012402.1.12",
          "form": "묶음상품같기도하구",
          "original_form": "묶음상품같기도하구",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100012402.1.13",
          "form": "중고거래 많이하세요?",
          "original_form": "중고거래 많이하세요?",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100012402.1.14",
          "form": "네 판매가 잘 되면 자주할 수 있을텐데요",
          "original_form": "네 판매가 잘 되면 자주할 수 있을텐데요",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100012402.1.15",
          "form": "연락은 뜨문뜨문 와요",
          "original_form": "연락은 뜨문뜨문 와요",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100012402.1.16",
          "form": "그래도 팔리긴 하니까 좋아요",
          "original_form": "그래도 팔리긴 하니까 좋아요",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100012402.1.17",
          "form": "구매하신 물품도 있으세요?",
          "original_form": "구매하신 물품도 있으세요?",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100012402.1.18",
          "form": "학용품 같은 거 샀었어요!",
          "original_form": "학용품 같은 거 샀었어요!",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100012402.1.19",
          "form": "되게 싸게 올라와서요",
          "original_form": "되게 싸게 올라와서요",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100012402.1.20",
          "form": "쏠쏠 하시겠다",
          "original_form": "쏠쏠 하시겠다",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100012402.1.21",
          "form": "싸게사시고 안쓰는거 파시구",
          "original_form": "싸게사시고 안쓰는거 파시구",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100012402.1.22",
          "form": "온도 높으실거 같아요",
          "original_form": "온도 높으실거 같아요",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100012402.1.23",
          "form": "45도 정도 돼요 ㅋㅋㅋㅋ",
          "original_form": "45도 정도 돼요 ㅋㅋㅋㅋ",
