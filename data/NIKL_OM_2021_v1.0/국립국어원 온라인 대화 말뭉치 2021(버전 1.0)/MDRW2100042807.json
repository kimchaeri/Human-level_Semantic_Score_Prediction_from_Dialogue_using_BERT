      "utterance": [
        {
          "id": "MDRW2100042807.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042807.1.2",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042807.1.3",
          "form": "아뇨 오늘 맞으러가요!",
          "original_form": "아뇨 오늘 맞으러가요!",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042807.1.4",
          "form": "헉 대박",
          "original_form": "헉 대박",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042807.1.5",
          "form": "잘 맞고 오세요!",
          "original_form": "잘 맞고 오세요!",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042807.1.6",
          "form": "화이자 맞으시나요?",
          "original_form": "화이자 맞으시나요?",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042807.1.7",
          "form": "그건 잘 모르겠어요 화이자 또는 모더나라고 적혀있더라구요",
          "original_form": "그건 잘 모르겠어요 화이자 또는 모더나라고 적혀있더라구요",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042807.1.8",
          "form": "쿠브어플에 그렇게 나오긴하는데 저는 문자도 이번주였나 저번주였나 한번 더 오더라구요",
          "original_form": "쿠브어플에 그렇게 나오긴하는데 저는 문자도 이번주였나 저번주였나 한번 더 오더라구요",
          "speaker_id": "1",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042807.1.9",
          "form": "거기에 화이자라고 적혀있긴 했네요",
          "original_form": "거기에 화이자라고 적혀있긴 했네요",
          "speaker_id": "1",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042807.1.10",
          "form": "저는 이번주 목요일에 맞아요 ㄷㄷㄷ",
          "original_form": "저는 이번주 목요일에 맞아요 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042807.1.11",
          "form": "아 저도 카톡으로 왔네요",
          "original_form": "아 저도 카톡으로 왔네요",
          "speaker_id": "2",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042807.1.12",
          "form": "화이자 맞는대요",
          "original_form": "화이자 맞는대요",
          "speaker_id": "2",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042807.1.13",
          "form": "10시에 맞으시나요??",
          "original_form": "10시에 맞으시나요??",
          "speaker_id": "1",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042807.1.14",
          "form": "아뇨 오후 세시요 ㅎㅎ",
          "original_form": "아뇨 오후 세시요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042807.1.15",
          "form": "오 아직 여유있네요 ㅋㅋㅋ",
          "original_form": "오 아직 여유있네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100042807.1.16",
          "form": "맞고나면 하루정도 못씻으니까 맞기 전에 샤워하고 가는게 좋대요!",
          "original_form": "맞고나면 하루정도 못씻으니까 맞기 전에 샤워하고 가는게 좋대요!",
          "speaker_id": "1",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100042807.1.17",
          "form": "헐 그렇군요 그건 생각 못했어요",
          "original_form": "헐 그렇군요 그건 생각 못했어요",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042807.1.18",
          "form": "씻구 가야겠네요",
          "original_form": "씻구 가야겠네요",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042807.1.19",
          "form": "약은 사놓으셨나요?",
          "original_form": "약은 사놓으셨나요?",
          "speaker_id": "2",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100042807.1.20",
          "form": "네 저 엄빠가 사놓으신거 있는데",
          "original_form": "네 저 엄빠가 사놓으신거 있는데",
          "speaker_id": "1",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100042807.1.21",
          "form": "열나는거 말고 통증?같은 증상에는 이부프로펜 계열이 좋다고해서 주사맞고나면 추가로 사려구요",
          "original_form": "열나는거 말고 통증?같은 증상에는 이부프로펜 계열이 좋다고해서 주사맞고나면 추가로 사려구요",
          "speaker_id": "1",
          "time": "20210907 10:22"
        },
        {
          "id": "MDRW2100042807.1.22",
          "form": "헐 그렇군요 근데 이부프로펜 먹어도 되나요? 아세트아미노펜만 먹어야된다 들었거든요",
          "original_form": "헐 그렇군요 근데 이부프로펜 먹어도 되나요? 아세트아미노펜만 먹어야된다 들었거든요",
          "speaker_id": "2",
          "time": "20210907 10:23"
        },
        {
          "id": "MDRW2100042807.1.23",
          "form": "아세트가 그 타이레놀인가요?",
          "original_form": "아세트가 그 타이레놀인가요?",
          "speaker_id": "1",
          "time": "20210907 10:28"
        },
        {
          "id": "MDRW2100042807.1.24",
          "form": "네 타이레놀이요!",
          "original_form": "네 타이레놀이요!",
          "speaker_id": "2",
          "time": "20210907 10:29"
        }
      ]
    }
