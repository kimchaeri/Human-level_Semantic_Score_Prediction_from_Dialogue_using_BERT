        },
        {
          "id": "MDRW2100010618.1.2",
          "form": "햄버거 좋아하시나요?",
          "original_form": "햄버거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 20:06"
        },
        {
          "id": "MDRW2100010618.1.3",
          "form": "안녕하세요. 편하게 음슴체 쓰셔도 되요",
          "original_form": "안녕하세요. 편하게 음슴체 쓰셔도 되요",
          "speaker_id": "2",
          "time": "20210823 20:06"
        },
        {
          "id": "MDRW2100010618.1.4",
          "form": "완전 좋아해여ㅋㅋ",
          "original_form": "완전 좋아해여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:06"
        },
        {
          "id": "MDRW2100010618.1.5",
          "form": "저도 완전 좋아해요~~~~~",
          "original_form": "저도 완전 좋아해요~~~~~",
          "speaker_id": "1",
          "time": "20210823 20:06"
        },
        {
          "id": "MDRW2100010618.1.6",
          "form": "버거킹와퍼도 좋고",
          "original_form": "버거킹와퍼도 좋고",
          "speaker_id": "2",
          "time": "20210823 20:06"
        },
        {
          "id": "MDRW2100010618.1.7",
          "form": "맘터도 좋아해요",
          "original_form": "맘터도 좋아해요",
          "speaker_id": "2",
          "time": "20210823 20:06"
        },
        {
          "id": "MDRW2100010618.1.8",
          "form": "저 맘스터치 딥치즈버거 젤 조아해요",
          "original_form": "저 맘스터치 딥치즈버거 젤 조아해요",
          "speaker_id": "1",
          "time": "20210823 20:06"
        },
        {
          "id": "MDRW2100010618.1.9",
          "form": "아 상상만해도 맛있겠다",
          "original_form": "아 상상만해도 맛있겠다",
          "speaker_id": "2",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010618.1.10",
          "form": "진짜 넘맛있어요 맘터",
          "original_form": "진짜 넘맛있어요 맘터",
          "speaker_id": "1",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010618.1.11",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "2",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010618.1.12",
          "form": "저녁먹었는데 또 먹고싶네",
          "original_form": "저녁먹었는데 또 먹고싶네",
          "speaker_id": "2",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010618.1.13",
          "form": "ㅈ저는 치즈랑 치킨 좋아해요 ㅋㅋㅋ",
          "original_form": "ㅈ저는 치즈랑 치킨 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010618.1.14",
          "form": "무슨 치킨 젤 좋아해여",
          "original_form": "무슨 치킨 젤 좋아해여",
          "speaker_id": "2",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010618.1.15",
          "form": "저녁 뭐드셧어용?",
          "original_form": "저녁 뭐드셧어용?",
          "speaker_id": "1",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010618.1.16",
          "form": "저 뿌링클이랑 푸라닭 고.추마요 굽네 고.추바사삭 좋아해용",
          "original_form": "저 뿌링클이랑 푸라닭 고.추마요 굽네 고.추바사삭 좋아해용",
          "speaker_id": "1",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010618.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010618.1.18",
          "form": "뭘 좀 아시는군여",
          "original_form": "뭘 좀 아시는군여",
          "speaker_id": "2",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010618.1.19",
          "form": "저녁 떡볶이 먹었어여",
          "original_form": "저녁 떡볶이 먹었어여",
          "speaker_id": "2",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010618.1.20",
          "form": "로~제 떡볶이",
          "original_form": "로~제 떡볶이",
          "speaker_id": "2",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010618.1.21",
          "form": "로~제 떡봈이 어디서 시키셧나여",
          "original_form": "로~제 떡봈이 어디서 시키셧나여",
          "speaker_id": "1",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100010618.1.22",
          "form": "ㅋㅋㅋ 근처 떡볶이집에서 새로 팔길래 시켰는데",
          "original_form": "ㅋㅋㅋ 근처 떡볶이집에서 새로 팔길래 시켰는데",
          "speaker_id": "2",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100010618.1.23",
          "form": "로제는 배떡이 맛있는거같아요",
          "original_form": "로제는 배떡이 맛있는거같아요",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010618.1.24",
          "form": "여기건 묽어서 아쉬웠으여 ㅠㅠ",
          "original_form": "여기건 묽어서 아쉬웠으여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010618.1.25",
          "form": "진짜 로제는 배떡이 제일 맛난거같아여",
          "original_form": "진짜 로제는 배떡이 제일 맛난거같아여",
          "speaker_id": "1",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010618.1.26",
          "form": "저도 삼첩분식 먹아봤는데 별로엿어요ㅠ",
          "original_form": "저도 삼첩분식 먹아봤는데 별로엿어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010618.1.27",
          "form": "깻잎넣고 분모자넣소",
          "original_form": "깻잎넣고 분모자넣소",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010618.1.28",
          "form": "당면도 넣고 으으 또 먹고싶다",
          "original_form": "당면도 넣고 으으 또 먹고싶다",
          "speaker_id": "2",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010618.1.29",
          "form": "저는 우동먹엇어요",
          "original_form": "저는 우동먹엇어요",
