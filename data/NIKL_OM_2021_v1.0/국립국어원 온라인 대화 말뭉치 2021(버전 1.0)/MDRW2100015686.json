      "utterance": [
        {
          "id": "MDRW2100015686.1.1",
          "form": "지금 대체로 만족하는 편이에요",
          "original_form": "지금 대체로 만족하는 편이에요",
          "speaker_id": "2",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015686.1.2",
          "form": "아 부럽다",
          "original_form": "아 부럽다",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015686.1.3",
          "form": "저는 지금 이직고민중이라 ㅠ",
          "original_form": "저는 지금 이직고민중이라 ㅠ",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015686.1.4",
          "form": "name1은 불만족 상태이신가봐요 ㅠㅠ",
          "original_form": "&name1&은 불만족 상태이신가봐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015686.1.5",
          "form": "직장 내 동료간 갈등 있는 게 아니면 이직 하지마세요..",
          "original_form": "직장 내 동료간 갈등 있는 게 아니면 이직 하지마세요..",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015686.1.6",
          "form": "비슷비슷합니다..",
          "original_form": "비슷비슷합니다..",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015686.1.7",
          "form": "그런가요??",
          "original_form": "그런가요??",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015686.1.8",
          "form": "사실 일은 괜찮은데 제 커리어에 도움이 안될거같아서",
          "original_form": "사실 일은 괜찮은데 제 커리어에 도움이 안될거같아서",
          "speaker_id": "1",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015686.1.9",
          "form": "고민이네요",
          "original_form": "고민이네요",
          "speaker_id": "1",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015686.1.10",
          "form": "요즘 코로나라서 일자리 구하기도 쉽지 않더라구요",
          "original_form": "요즘 코로나라서 일자리 구하기도 쉽지 않더라구요",
          "speaker_id": "2",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015686.1.11",
          "form": "그건 진짜 맞아요",
          "original_form": "그건 진짜 맞아요",
          "speaker_id": "1",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015686.1.12",
          "form": "그 직종에서 커리어 쌓는 게 중요하더라도 당분간은 관망해보세요",
          "original_form": "그 직종에서 커리어 쌓는 게 중요하더라도 당분간은 관망해보세요",
          "speaker_id": "2",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015686.1.13",
          "form": "네 경력쌓는게 더 좋을거같긴한데",
          "original_form": "네 경력쌓는게 더 좋을거같긴한데",
          "speaker_id": "1",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015686.1.14",
          "form": "월급도 마음에 안들어서 ㅋㅋ",
          "original_form": "월급도 마음에 안들어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015686.1.15",
          "form": "진짜 확실히 더 나은 곳 아니면...",
          "original_form": "진짜 확실히 더 나은 곳 아니면...",
          "speaker_id": "2",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015686.1.16",
          "form": "저 아는 분 이직하겠다고 회사 박차고 나왔는데 그 뒤로 감감무소식이에요...",
          "original_form": "저 아는 분 이직하겠다고 회사 박차고 나왔는데 그 뒤로 감감무소식이에요...",
          "speaker_id": "2",
          "time": "20210826 00:20"
        },
        {
          "id": "MDRW2100015686.1.17",
          "form": "그쵸...ㅠㅠ",
          "original_form": "그쵸...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015686.1.18",
          "form": "직장 먼저 구하고 퇴사하지... 참 ㅠㅠㅠ",
          "original_form": "직장 먼저 구하고 퇴사하지... 참 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015686.1.19",
          "form": "근데 다니면서 이직하는건 진짜 보통 독하지않고서야 ㅠㅠ",
          "original_form": "근데 다니면서 이직하는건 진짜 보통 독하지않고서야 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015686.1.20",
          "form": "집오면 힘들어서",
          "original_form": "집오면 힘들어서",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015686.1.21",
          "form": "이직준비 에바더라고요 ㅋㅋ사실 해봤는데",
          "original_form": "이직준비 에바더라고요 ㅋㅋ사실 해봤는데",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015686.1.22",
          "form": "맞아요 준비하는 것도 일이에요 ㅜㅜ",
          "original_form": "맞아요 준비하는 것도 일이에요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015686.1.23",
          "form": "된다는 보장도 없구...",
          "original_form": "된다는 보장도 없구...",
          "speaker_id": "2",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015686.1.24",
          "form": "일갔다오면 쉬어야해서 ㅠㅠ",
          "original_form": "일갔다오면 쉬어야해서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015686.1.25",
          "form": "그쵸 ㅠㅠ",
          "original_form": "그쵸 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015686.1.26",
          "form": "나중에 실업급여 받을 때가 있다면 그때가 적기인 것 같아요",
          "original_form": "나중에 실업급여 받을 때가 있다면 그때가 적기인 것 같아요",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015686.1.27",
          "form": "오!!  좀더 고민해봐야겠네요 ㅎㅎ",
          "original_form": "오!!  좀더 고민해봐야겠네요 ㅎㅎ",
