      "utterance": [
        {
          "id": "MDRW2100019306.1.1",
          "form": "출첵 말하는거 맞지?",
          "original_form": "출첵 말하는거 맞지?",
          "speaker_id": "1",
          "time": "20210827 17:12"
        },
        {
          "id": "MDRW2100019306.1.2",
          "form": "카페우딜도배장같아",
          "original_form": "카페우딜도배장같아",
          "speaker_id": "2",
          "time": "20210827 17:12"
        },
        {
          "id": "MDRW2100019306.1.3",
          "form": "아니아니 ㅋㅋㅋ",
          "original_form": "아니아니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:12"
        },
        {
          "id": "MDRW2100019306.1.4",
          "form": "베스킨 31일날사면 받는스템프말한.ㄴ디유",
          "original_form": "베스킨 31일날사면 받는스템프말한.ㄴ디유",
          "speaker_id": "2",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019306.1.5",
          "form": "헉! 그건 뭐죠?",
          "original_form": "헉! 그건 뭐죠?",
          "speaker_id": "1",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019306.1.6",
          "form": "ㅋㅋㅋ그럼 전 안모았네유..",
          "original_form": "ㅋㅋㅋ그럼 전 안모았네유..",
          "speaker_id": "1",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019306.1.7",
          "form": "네에?",
          "original_form": "네에?",
          "speaker_id": "2",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019306.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019306.1.9",
          "form": "아냐 31일날사이즈햇으몀 찍혓을수잇어",
          "original_form": "아냐 31일날사이즈햇으몀 찍혓을수잇어",
          "speaker_id": "2",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019306.1.10",
          "form": "ㅋㅋㅋㅋ어차피 뭐 저번달에 배라 한번샀는데?",
          "original_form": "ㅋㅋㅋㅋ어차피 뭐 저번달에 배라 한번샀는데?",
          "speaker_id": "1",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019306.1.11",
          "form": "허플에서",
          "original_form": "허플에서",
          "speaker_id": "2",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019306.1.12",
          "form": "못봤어 찍힌거!",
          "original_form": "못봤어 찍힌거!",
          "speaker_id": "1",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100019306.1.13",
          "form": "한번봐봐 저번꺼",
          "original_form": "한번봐봐 저번꺼",
          "speaker_id": "2",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100019306.1.14",
          "form": "찍혓으면 해피오더로 사전예약하면 자동으로찍혀",
          "original_form": "찍혓으면 해피오더로 사전예약하면 자동으로찍혀",
          "speaker_id": "2",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100019306.1.15",
          "form": "2 4 6 7 에 따라 혜택잇음",
          "original_form": "2 4 6 7 에 따라 혜택잇음",
          "speaker_id": "2",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100019306.1.16",
          "form": "오 대박쓰! 제발 찍혀있기를ㅋㅋㅋ",
          "original_form": "오 대박쓰! 제발 찍혀있기를ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100019306.1.17",
          "form": "난 올해까지 3년째 챔피언될라하고잇지 ㅋㅋㅋㅋㅋ",
          "original_form": "난 올해까지 3년째 챔피언될라하고잇지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100019306.1.18",
          "form": "챔피언 혜택은 무엇이죠?!!",
          "original_form": "챔피언 혜택은 무엇이죠?!!",
          "speaker_id": "1",
          "time": "20210827 17:17"
        },
        {
          "id": "MDRW2100019306.1.19",
          "form": "아무거나 고르는 아이스크림케익입니다",
          "original_form": "아무거나 고르는 아이스크림케익입니다",
          "speaker_id": "2",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100019306.1.20",
          "form": "재작년작년다 서울에서 바꿔먹은듯 ㅋㅋㅋ",
          "original_form": "재작년작년다 서울에서 바꿔먹은듯 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100019306.1.21",
          "form": "우와 대박ㅋㅋㅋㅋ 멋있다",
          "original_form": "우와 대박ㅋㅋㅋㅋ 멋있다",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100019306.1.22",
          "form": "정말로 현소네",
          "original_form": "정말로 현소네",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100019306.1.23",
          "form": "2번이면 싱글레귤러 2개여 ㅋㅋㅋㅋ그대는",
          "original_form": "2번이면 싱글레귤러 2개여 ㅋㅋㅋㅋ그대는",
          "speaker_id": "2",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019306.1.24",
          "form": "그대 스마일캐쉬쓰는가?",
          "original_form": "그대 스마일캐쉬쓰는가?",
          "speaker_id": "2",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019306.1.25",
          "form": "스캐는 지마켓으로 전환하는거지,",
          "original_form": "스캐는 지마켓으로 전환하는거지,",
          "speaker_id": "1",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019306.1.26",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019306.1.27",
          "form": "지마켓에서 등록하고 요기요에서 해피오더에서랑 결제할때 다쓸수잇음!",
          "original_form": "지마켓에서 등록하고 요기요에서 해피오더에서랑 결제할때 다쓸수잇음!",
          "speaker_id": "2",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019306.1.28",
          "form": "헐! 현명한 소비 카페 뭔가 바꼈나?",
          "original_form": "헐! 현명한 소비 카페 뭔가 바꼈나?",
          "speaker_id": "1",
          "time": "20210830 13:10"
        },
        {
