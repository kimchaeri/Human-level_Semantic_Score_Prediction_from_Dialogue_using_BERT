      "utterance": [
        {
          "id": "MDRW2100003910.1.1",
          "form": "최근에 구매 하신 게 있나요?",
          "original_form": "최근에 구매 하신 게 있나요?",
          "speaker_id": "2",
          "time": "20210821 13:37"
        },
        {
          "id": "MDRW2100003910.1.2",
          "form": "저 가방이요",
          "original_form": "저 가방이요",
          "speaker_id": "1",
          "time": "20210821 13:40"
        },
        {
          "id": "MDRW2100003910.1.3",
          "form": "어떤 가방 사셨나요?",
          "original_form": "어떤 가방 사셨나요?",
          "speaker_id": "2",
          "time": "20210821 13:41"
        },
        {
          "id": "MDRW2100003910.1.4",
          "form": "뭐사셨어요?",
          "original_form": "뭐사셨어요?",
          "speaker_id": "1",
          "time": "20210821 13:41"
        },
        {
          "id": "MDRW2100003910.1.5",
          "form": "아들 가방이요",
          "original_form": "아들 가방이요",
          "speaker_id": "1",
          "time": "20210821 13:41"
        },
        {
          "id": "MDRW2100003910.1.6",
          "form": "가방없이 살다 급 사달래서요",
          "original_form": "가방없이 살다 급 사달래서요",
          "speaker_id": "1",
          "time": "20210821 13:41"
        },
        {
          "id": "MDRW2100003910.1.7",
          "form": "전 약국에서 식염수 샀어요 요즘 마스크로 트러블이 많거든요",
          "original_form": "전 약국에서 식염수 샀어요 요즘 마스크로 트러블이 많거든요",
          "speaker_id": "2",
          "time": "20210821 13:42"
        },
        {
          "id": "MDRW2100003910.1.8",
          "form": "친구들이 들고 다니니 사고 싶었나봐요",
          "original_form": "친구들이 들고 다니니 사고 싶었나봐요",
          "speaker_id": "2",
          "time": "20210821 13:43"
        },
        {
          "id": "MDRW2100003910.1.9",
          "form": "트러블 장난 아니예요",
          "original_form": "트러블 장난 아니예요",
          "speaker_id": "1",
          "time": "20210821 13:43"
        },
        {
          "id": "MDRW2100003910.1.10",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210821 13:43"
        },
        {
          "id": "MDRW2100003910.1.11",
          "form": "아주 까다로워요",
          "original_form": "아주 까다로워요",
          "speaker_id": "1",
          "time": "20210821 13:44"
        },
        {
          "id": "MDRW2100003910.1.12",
          "form": "마스크를 안 낄수도 없고 난감해요",
          "original_form": "마스크를 안 낄수도 없고 난감해요",
          "speaker_id": "2",
          "time": "20210821 13:44"
        },
        {
          "id": "MDRW2100003910.1.13",
          "form": "자제분이 몇살이세요?",
          "original_form": "자제분이 몇살이세요?",
          "speaker_id": "2",
          "time": "20210821 13:45"
        },
        {
          "id": "MDRW2100003910.1.14",
          "form": "맞아요 저 뾰루지 났어요",
          "original_form": "맞아요 저 뾰루지 났어요",
          "speaker_id": "1",
          "time": "20210821 13:47"
        },
        {
          "id": "MDRW2100003910.1.15",
          "form": "19이요",
          "original_form": "19이요",
          "speaker_id": "1",
          "time": "20210821 13:47"
        },
        {
          "id": "MDRW2100003910.1.16",
          "form": "취향을 모르겠어요",
          "original_form": "취향을 모르겠어요",
          "speaker_id": "1",
          "time": "20210821 13:48"
        },
        {
          "id": "MDRW2100003910.1.17",
          "form": "한창 마음이 갈대같겠네요",
          "original_form": "한창 마음이 갈대같겠네요",
          "speaker_id": "2",
          "time": "20210821 13:51"
        },
        {
          "id": "MDRW2100003910.1.18",
          "form": "네 그래서 한참 검색후  샀어요",
          "original_form": "네 그래서 한참 검색후  샀어요",
          "speaker_id": "1",
          "time": "20210821 13:51"
        },
        {
          "id": "MDRW2100003910.1.19",
          "form": "화장품은 잘사는데 게을러서 잘안써요 ㅋㅋ",
          "original_form": "화장품은 잘사는데 게을러서 잘안써요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:52"
        },
        {
          "id": "MDRW2100003910.1.20",
          "form": "아까워요",
          "original_form": "아까워요",
          "speaker_id": "1",
          "time": "20210821 13:52"
        },
        {
          "id": "MDRW2100003910.1.21",
          "form": "전 피부가 예민해서 화장품을 고를때 힘들어요 ㅠㅠ",
          "original_form": "전 피부가 예민해서 화장품을 고를때 힘들어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003910.1.22",
          "form": "남자애들은 다 사줘야하나봐요 혼자서는 무관심이던데",
          "original_form": "남자애들은 다 사줘야하나봐요 혼자서는 무관심이던데",
          "speaker_id": "2",
          "time": "20210821 13:54"
        },
        {
          "id": "MDRW2100003910.1.23",
          "form": "남동생 있거든요 ㅋㅋㅋ",
          "original_form": "남동생 있거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 13:54"
        },
        {
          "id": "MDRW2100003910.1.24",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003910.1.25",
          "form": "맞아요 ㅎㅎ뭘 살때 다 사줘야해요",
          "original_form": "맞아요 ㅎㅎ뭘 살때 다 사줘야해요",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003910.1.26",
          "form": "무심하고 그래서 더 힘들어요 ㅎㅎ",
          "original_form": "무심하고 그래서 더 힘들어요 ㅎㅎ",
