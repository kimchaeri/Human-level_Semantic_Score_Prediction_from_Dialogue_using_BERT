      "utterance": [
        {
          "id": "MDRW2100043346.1.1",
          "form": "화이트하임 좋아하시나요?",
          "original_form": "화이트하임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043346.1.2",
          "form": "네네ㅎㅎ 근데 최애는 감자칩이에요",
          "original_form": "네네ㅎㅎ 근데 최애는 감자칩이에요",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043346.1.3",
          "form": "어떤 감자칩이 최애감자칩인가요??",
          "original_form": "어떤 감자칩이 최애감자칩인가요??",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100043346.1.4",
          "form": "역시 포카칩이죠, 근데 칩포테토나 생생감자칩도 좋아해요",
          "original_form": "역시 포카칩이죠, 근데 칩포테토나 생생감자칩도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043346.1.5",
          "form": "포카칩 오리지널 vs 어니언",
          "original_form": "포카칩 오리지널 vs 어니언",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043346.1.6",
          "form": "닥전입니다",
          "original_form": "닥전입니다",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100043346.1.7",
          "form": "힝ㅠ 전 어니언인데",
          "original_form": "힝ㅠ 전 어니언인데",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100043346.1.8",
          "form": "어제도 포카칩 어니언맛 먹었어요",
          "original_form": "어제도 포카칩 어니언맛 먹었어요",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100043346.1.9",
          "form": "오리지널 감자맛을 좋아하시는군요!",
          "original_form": "오리지널 감자맛을 좋아하시는군요!",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100043346.1.10",
          "form": "네넹ㅋㅋㅋ 어니언도 좋아하는데 찐사랑은 오리지널이에요",
          "original_form": "네넹ㅋㅋㅋ 어니언도 좋아하는데 찐사랑은 오리지널이에요",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100043346.1.11",
          "form": "생물 감자도 좋아하세요?",
          "original_form": "생물 감자도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100043346.1.12",
          "form": "네네, 찐감자 구운감자 튀긴감자 다 좋아요",
          "original_form": "네네, 찐감자 구운감자 튀긴감자 다 좋아요",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100043346.1.13",
          "form": "튀긴감자 정말 최고에요",
          "original_form": "튀긴감자 정말 최고에요",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100043346.1.14",
          "form": "배불러도 들어가요",
          "original_form": "배불러도 들어가요",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100043346.1.15",
          "form": "어제도 감자튀김 갈릭버터소스에 찍어먹었어요 ",
          "original_form": "어제도 감자튀김 갈릭버터소스에 찍어먹었어요 ",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100043346.1.16",
          "form": "크으으으으",
          "original_form": "크으으으으",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100043346.1.17",
          "form": "듣기만해도 존맛",
          "original_form": "듣기만해도 존맛",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100043346.1.18",
          "form": "이에요",
          "original_form": "이에요",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100043346.1.19",
          "form": "허니버터시즈닝도 솔솔",
          "original_form": "허니버터시즈닝도 솔솔",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100043346.1.20",
          "form": "청년다방 허니갈릭버터감자튀김이 갑자기생각나네요",
          "original_form": "청년다방 허니갈릭버터감자튀김이 갑자기생각나네요",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100043346.1.21",
          "form": "마약을 드셨네요",
          "original_form": "마약을 드셨네요",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100043346.1.22",
          "form": "맞아요, 그거 먹고 반해서 소스를 샀어요",
          "original_form": "맞아요, 그거 먹고 반해서 소스를 샀어요",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100043346.1.23",
          "form": "현명하시네요",
          "original_form": "현명하시네요",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100043346.1.24",
          "form": "이제 튀긴감자에서 못벗어나겠네요ㅋㅋ",
          "original_form": "이제 튀긴감자에서 못벗어나겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100043346.1.25",
          "form": "이제 감튀 무한구매의 길로...",
          "original_form": "이제 감튀 무한구매의 길로...",
