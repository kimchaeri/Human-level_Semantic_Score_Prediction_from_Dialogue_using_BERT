      "utterance": [
        {
          "id": "MDRW2100012337.1.1",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012337.1.2",
          "form": "산거야 판거야?",
          "original_form": "산거야 판거야?",
          "speaker_id": "1",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012337.1.3",
          "form": "사기도 하고 팔기도 했어",
          "original_form": "사기도 하고 팔기도 했어",
          "speaker_id": "2",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100012337.1.4",
          "form": "산거는 뭐삿어?",
          "original_form": "산거는 뭐삿어?",
          "speaker_id": "1",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012337.1.5",
          "form": "나 마카 샀엉",
          "original_form": "나 마카 샀엉",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012337.1.6",
          "form": "마카? 미술용품 말하는거야?",
          "original_form": "마카? 미술용품 말하는거야?",
          "speaker_id": "1",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012337.1.7",
          "form": "웅ㅇ웅 맞아 싸게 팔아주셨어",
          "original_form": "웅ㅇ웅 맞아 싸게 팔아주셨어",
          "speaker_id": "2",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012337.1.8",
          "form": "얼마에 삿어?",
          "original_form": "얼마에 삿어?",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012337.1.9",
          "form": "정확하게 기억은 안나는데 6만원 정도에 샀어",
          "original_form": "정확하게 기억은 안나는데 6만원 정도에 샀어",
          "speaker_id": "2",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012337.1.10",
          "form": "그렇구나.",
          "original_form": "그렇구나.",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100012337.1.11",
          "form": "판거는 뭐 팔았어?",
          "original_form": "판거는 뭐 팔았어?",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012337.1.12",
          "form": "코난 굿즈 팔았어!",
          "original_form": "코난 굿즈 팔았어!",
          "speaker_id": "2",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012337.1.13",
          "form": "얼마에 팔았어?",
          "original_form": "얼마에 팔았어?",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012337.1.14",
          "form": "8000원?",
          "original_form": "8000원?",
          "speaker_id": "2",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012337.1.15",
          "form": "싸게 판거야?",
          "original_form": "싸게 판거야?",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012337.1.16",
          "form": "당연하지 일본 직구상품이었다구!",
          "original_form": "당연하지 일본 직구상품이었다구!",
          "speaker_id": "2",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100012337.1.17",
          "form": "그렇구나 어떤사람이 사갔어?",
          "original_form": "그렇구나 어떤사람이 사갔어?",
          "speaker_id": "1",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100012337.1.18",
          "form": "귀여운 학생이 사갔어",
          "original_form": "귀여운 학생이 사갔어",
          "speaker_id": "2",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100012337.1.19",
          "form": "오 나는 항상 아줌마들이랑만 했는데",
          "original_form": "오 나는 항상 아줌마들이랑만 했는데",
          "speaker_id": "1",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012337.1.20",
          "form": "아 진짜 넌 뭘 팔았어?",
          "original_form": "아 진짜 넌 뭘 팔았어?",
          "speaker_id": "2",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012337.1.21",
          "form": "나는 주로 안읽는 책을 팔았어",
          "original_form": "나는 주로 안읽는 책을 팔았어",
          "speaker_id": "1",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012337.1.22",
          "form": "아 그렇구낭",
          "original_form": "아 그렇구낭",
          "speaker_id": "2",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012337.1.23",
          "form": "요새 구매 희망 물건 있어?",
          "original_form": "요새 구매 희망 물건 있어?",
          "speaker_id": "2",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012337.1.24",
          "form": "나는 토익 공부중이라 토익책",
          "original_form": "나는 토익 공부중이라 토익책",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012337.1.25",
          "form": "아 토익책이 비싸.",
          "original_form": "아 토익책이 비싸.",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012337.1.26",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012337.1.27",
          "form": "아니 저렴한거 많더라",
          "original_form": "아니 저렴한거 많더라",
          "speaker_id": "1",
          "time": "20210824 21:33"
        }
      ]
    }
