      "utterance": [
        {
          "id": "MDRW2100016022.1.1",
          "form": "으음..",
          "original_form": "으음..",
          "speaker_id": "1",
          "time": "20210826 08:01"
        },
        {
          "id": "MDRW2100016022.1.2",
          "form": "해리포터는요?",
          "original_form": "해리포터는요?",
          "speaker_id": "1",
          "time": "20210826 08:01"
        },
        {
          "id": "MDRW2100016022.1.3",
          "form": "제 최애중 하나인데",
          "original_form": "제 최애중 하나인데",
          "speaker_id": "1",
          "time": "20210826 08:01"
        },
        {
          "id": "MDRW2100016022.1.4",
          "form": "해리포터는 좋아해요",
          "original_form": "해리포터는 좋아해요",
          "speaker_id": "2",
          "time": "20210826 08:02"
        },
        {
          "id": "MDRW2100016022.1.5",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210826 08:02"
        },
        {
          "id": "MDRW2100016022.1.6",
          "form": "해리포터 약간 뭐랄까",
          "original_form": "해리포터 약간 뭐랄까",
          "speaker_id": "2",
          "time": "20210826 08:02"
        },
        {
          "id": "MDRW2100016022.1.7",
          "form": "다행입니다",
          "original_form": "다행입니다",
          "speaker_id": "1",
          "time": "20210826 08:03"
        },
        {
          "id": "MDRW2100016022.1.8",
          "form": "미지의 세계 보는 기분",
          "original_form": "미지의 세계 보는 기분",
          "speaker_id": "2",
          "time": "20210826 08:03"
        },
        {
          "id": "MDRW2100016022.1.9",
          "form": "해리포터에서 좋아하는 인물은 누군가요",
          "original_form": "해리포터에서 좋아하는 인물은 누군가요",
          "speaker_id": "2",
          "time": "20210826 08:03"
        },
        {
          "id": "MDRW2100016022.1.10",
          "form": "ㅋㅋㄲㄲ",
          "original_form": "ㅋㅋㄲㄲ",
          "speaker_id": "1",
          "time": "20210826 08:03"
        },
        {
          "id": "MDRW2100016022.1.11",
          "form": "저는 해그리드요",
          "original_form": "저는 해그리드요",
          "speaker_id": "1",
          "time": "20210826 08:03"
        },
        {
          "id": "MDRW2100016022.1.12",
          "form": "그 거인아저씨",
          "original_form": "그 거인아저씨",
          "speaker_id": "2",
          "time": "20210826 08:04"
        },
        {
          "id": "MDRW2100016022.1.13",
          "form": "이유는요?",
          "original_form": "이유는요?",
          "speaker_id": "2",
          "time": "20210826 08:04"
        },
        {
          "id": "MDRW2100016022.1.14",
          "form": "론이랑 저랑 성격이 비슷..제외",
          "original_form": "론이랑 저랑 성격이 비슷..제외",
          "speaker_id": "1",
          "time": "20210826 08:04"
        },
        {
          "id": "MDRW2100016022.1.15",
          "form": "해그리드같은 분이 제가 해리처럼",
          "original_form": "해그리드같은 분이 제가 해리처럼",
          "speaker_id": "1",
          "time": "20210826 08:04"
        },
        {
          "id": "MDRW2100016022.1.16",
          "form": "쭈구리됐을때 지켜주잖아요",
          "original_form": "쭈구리됐을때 지켜주잖아요",
          "speaker_id": "1",
          "time": "20210826 08:04"
        },
        {
          "id": "MDRW2100016022.1.17",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 08:04"
        },
        {
          "id": "MDRW2100016022.1.18",
          "form": "음 약간 안정적인 사람 좋아하는구나",
          "original_form": "음 약간 안정적인 사람 좋아하는구나",
          "speaker_id": "2",
          "time": "20210826 08:04"
        },
        {
          "id": "MDRW2100016022.1.19",
          "form": "싫어하는 인물은요?",
          "original_form": "싫어하는 인물은요?",
          "speaker_id": "2",
          "time": "20210826 08:04"
        },
        {
          "id": "MDRW2100016022.1.20",
          "form": "론말구",
          "original_form": "론말구",
          "speaker_id": "2",
          "time": "20210826 08:04"
        },
        {
          "id": "MDRW2100016022.1.21",
          "form": "저는 해리사촌요",
          "original_form": "저는 해리사촌요",
          "speaker_id": "1",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100016022.1.22",
          "form": "아.. 그 머글",
          "original_form": "아.. 그 머글",
          "speaker_id": "2",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100016022.1.23",
          "form": "슬리데린 말포이도",
          "original_form": "슬리데린 말포이도",
          "speaker_id": "1",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100016022.1.24",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100016022.1.25",
          "form": "불쌍하게 여기는데",
          "original_form": "불쌍하게 여기는데",
          "speaker_id": "1",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100016022.1.26",
          "form": "말포이는  좀 재수없긴해요ㅎ",
          "original_form": "말포이는  좀 재수없긴해요ㅎ",
          "speaker_id": "2",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100016022.1.27",
          "form": "먹을걸로 장난치면 벌받아요",
          "original_form": "먹을걸로 장난치면 벌받아요",
          "speaker_id": "1",
          "time": "20210826 08:05"
        },
        {
          "id": "MDRW2100016022.1.28",
          "form": "ㅜ그쵸",
          "original_form": "ㅜ그쵸",
          "speaker_id": "1",
