      "utterance": [
        {
          "id": "MDRW2100035857.1.1",
          "form": "얼마나...심했길래......요!?",
          "original_form": "얼마나...심했길래......요!?",
          "speaker_id": "2",
          "time": "20210830 09:28"
        },
        {
          "id": "MDRW2100035857.1.2",
          "form": "머리 묶는건 저랑 비슷하신것같아요!ㅎㅎ",
          "original_form": "머리 묶는건 저랑 비슷하신것같아요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 09:28"
        },
        {
          "id": "MDRW2100035857.1.3",
          "form": "염색 금지 파마금지 고데기 금지",
          "original_form": "염색 금지 파마금지 고데기 금지",
          "speaker_id": "1",
          "time": "20210830 09:28"
        },
        {
          "id": "MDRW2100035857.1.4",
          "form": "어깨 넘으면 묶어야하고",
          "original_form": "어깨 넘으면 묶어야하고",
          "speaker_id": "1",
          "time": "20210830 09:28"
        },
        {
          "id": "MDRW2100035857.1.5",
          "form": "헐 고데기도요!?!??!?!?!?!",
          "original_form": "헐 고데기도요!?!??!?!?!?!",
          "speaker_id": "2",
          "time": "20210830 09:28"
        },
        {
          "id": "MDRW2100035857.1.6",
          "form": "네네ㅠㅠ",
          "original_form": "네네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 09:28"
        },
        {
          "id": "MDRW2100035857.1.7",
          "form": "화장하면 교문에서 잡히고",
          "original_form": "화장하면 교문에서 잡히고",
          "speaker_id": "1",
          "time": "20210830 09:28"
        },
        {
          "id": "MDRW2100035857.1.8",
          "form": "제가 다닌 학교는 염색파마금지인데 고데기는...안 잡았거든요!!!! 헐 화장도 잡았구나....",
          "original_form": "제가 다닌 학교는 염색파마금지인데 고데기는...안 잡았거든요!!!! 헐 화장도 잡았구나....",
          "speaker_id": "2",
          "time": "20210830 09:28"
        },
        {
          "id": "MDRW2100035857.1.9",
          "form": "저는 좀 억울했던게 약한 반곱슬이라",
          "original_form": "저는 좀 억울했던게 약한 반곱슬이라",
          "speaker_id": "1",
          "time": "20210830 09:29"
        },
        {
          "id": "MDRW2100035857.1.10",
          "form": "머리도 긴편이라 고데기한것같은 머리인데",
          "original_form": "머리도 긴편이라 고데기한것같은 머리인데",
          "speaker_id": "1",
          "time": "20210830 09:29"
        },
        {
          "id": "MDRW2100035857.1.11",
          "form": "아 뭔지 알아요ㅜㅜㅜㅜㅜㅜㅜ",
          "original_form": "아 뭔지 알아요ㅜㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 09:29"
        },
        {
          "id": "MDRW2100035857.1.12",
          "form": "학주선생님이 잡았어요ㅠㅠ",
          "original_form": "학주선생님이 잡았어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 09:29"
        },
        {
          "id": "MDRW2100035857.1.13",
          "form": "파마 풀고오라고ㅋㅋㅋㅋ",
          "original_form": "파마 풀고오라고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:29"
        },
        {
          "id": "MDRW2100035857.1.14",
          "form": "솔직히...자연곱슬은 알아서 구분해주셔야하는거 아니냐고요ㅠㅠㅠㅠㅠㅠ",
          "original_form": "솔직히...자연곱슬은 알아서 구분해주셔야하는거 아니냐고요ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 09:29"
        },
        {
          "id": "MDRW2100035857.1.15",
          "form": "그러게요ㅠㅠ 그래서",
          "original_form": "그러게요ㅠㅠ 그래서",
          "speaker_id": "1",
          "time": "20210830 09:29"
        },
        {
          "id": "MDRW2100035857.1.16",
          "form": "담임선생님이 해명해주셨어요...ㅋㅋㅋ",
          "original_form": "담임선생님이 해명해주셨어요...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:29"
        },
        {
          "id": "MDRW2100035857.1.17",
          "form": "다행이에요 좋은선생님이 계셔서...!!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "다행이에요 좋은선생님이 계셔서...!!ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:30"
        },
        {
