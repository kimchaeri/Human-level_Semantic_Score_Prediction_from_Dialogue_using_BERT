      "utterance": [
        {
          "id": "MDRW2100044915.1.1",
          "form": "교촌은 맛있는데 닭이 너무 작은것같지않나요 ㅋㅋㅋ",
          "original_form": "교촌은 맛있는데 닭이 너무 작은것같지않나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044915.1.2",
          "form": "양념빨인데",
          "original_form": "양념빨인데",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044915.1.3",
          "form": "넘맛있죠",
          "original_form": "넘맛있죠",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044915.1.4",
          "form": "맞아요 ㅋㅋㅋㅋ그래도 콤보를 내주는건 좋은것 같아요",
          "original_form": "맞아요 ㅋㅋㅋㅋ그래도 콤보를 내주는건 좋은것 같아요",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044915.1.5",
          "form": "닭은 솔까 넘작고 냉동느낌",
          "original_form": "닭은 솔까 넘작고 냉동느낌",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044915.1.6",
          "form": "ㅎㅓ니 너무 맛나요",
          "original_form": "ㅎㅓ니 너무 맛나요",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044915.1.7",
          "form": "맞아요ㅠㅠㅠ 근데 진짜 소스가 은근 중독",
          "original_form": "맞아요ㅠㅠㅠ 근데 진짜 소스가 은근 중독",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044915.1.8",
          "form": "그리고 가끔 생각나요 ㅋㅋㅋㅋ",
          "original_form": "그리고 가끔 생각나요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044915.1.9",
          "form": "그니까요 따라할수없어요",
          "original_form": "그니까요 따라할수없어요",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044915.1.10",
          "form": "그러니까요ㅠㅠ 진짜 ㅋㅋ",
          "original_form": "그러니까요ㅠㅠ 진짜 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044915.1.11",
          "form": "굽네는 안좋아하시나요?",
          "original_form": "굽네는 안좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044915.1.12",
          "form": "회사 야식은 굽네 볼케이노 ??",
          "original_form": "회사 야식은 굽네 볼케이노 ??",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044915.1.13",
          "form": "먹었어요",
          "original_form": "먹었어요",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044915.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ오 대박 마침 또 드셨군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ오 대박 마침 또 드셨군요",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044915.1.15",
          "form": "안매웟어여?",
          "original_form": "안매웟어여?",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044915.1.16",
          "form": "네 근데 저 원래 잘먹어서ㅋㅋ",
          "original_form": "네 근데 저 원래 잘먹어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044915.1.17",
          "form": "ㅋㅋ오 굽네중엔 그럼 볼케이노 가장 좋아하세요?",
          "original_form": "ㅋㅋ오 굽네중엔 그럼 볼케이노 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044915.1.18",
          "form": "오리지널에 소스 먹는게 베스트같아요!",
          "original_form": "오리지널에 소스 먹는게 베스트같아요!",
          "speaker_id": "2",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044915.1.19",
          "form": "오 ㅋㅋㅋㅋ저는 고 추 바사삭 좋아해요!!! 오리지날도 깔끔하고 좋죠 !!!",
          "original_form": "오 ㅋㅋㅋㅋ저는 고 추 바사삭 좋아해요!!! 오리지날도 깔끔하고 좋죠 !!!",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044915.1.20",
          "form": "예전엔 비비큐도 진짜 자주먹었어요!! ㅋㅋㅋㅋ",
          "original_form": "예전엔 비비큐도 진짜 자주먹었어요!! ㅋㅋㅋㅋ",
