      "utterance": [
        {
          "id": "MDRW2100040774.1.1",
          "form": "뭔가 인간자연수명원래30년쯤된다면서요",
          "original_form": "뭔가 인간자연수명원래30년쯤된다면서요",
          "speaker_id": "2",
          "time": "20210905 04:27"
        },
        {
          "id": "MDRW2100040774.1.2",
          "form": "인간의 장수에대한욕심이",
          "original_form": "인간의 장수에대한욕심이",
          "speaker_id": "2",
          "time": "20210905 04:27"
        },
        {
          "id": "MDRW2100040774.1.3",
          "form": "이렇게 목숨만 늘어나고",
          "original_form": "이렇게 목숨만 늘어나고",
          "speaker_id": "2",
          "time": "20210905 04:27"
        },
        {
          "id": "MDRW2100040774.1.4",
          "form": "헐 30년 밖에 안된대요?!",
          "original_form": "헐 30년 밖에 안된대요?!",
          "speaker_id": "1",
          "time": "20210905 04:27"
        },
        {
          "id": "MDRW2100040774.1.5",
          "form": "대박 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "대박 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 04:27"
        },
        {
          "id": "MDRW2100040774.1.6",
          "form": "그럼 저는 진짜 내일모레.. 죽어야하는...",
          "original_form": "그럼 저는 진짜 내일모레.. 죽어야하는...",
          "speaker_id": "1",
          "time": "20210905 04:27"
        },
        {
          "id": "MDRW2100040774.1.7",
          "form": "네네 8ㅅ8",
          "original_form": "네네 8ㅅ8",
          "speaker_id": "1",
          "time": "20210905 04:27"
        },
        {
          "id": "MDRW2100040774.1.8",
          "form": "ㅋ큐큐ㅠ",
          "original_form": "ㅋ큐큐ㅠ",
          "speaker_id": "2",
          "time": "20210905 04:27"
        },
        {
          "id": "MDRW2100040774.1.9",
          "form": "근데 의학기술덕에",
          "original_form": "근데 의학기술덕에",
          "speaker_id": "2",
          "time": "20210905 04:27"
        },
        {
          "id": "MDRW2100040774.1.10",
          "form": "120년까지사는 ...",
          "original_form": "120년까지사는 ...",
          "speaker_id": "2",
          "time": "20210905 04:27"
        },
        {
          "id": "MDRW2100040774.1.11",
          "form": "정말 대단해요 현대과학",
          "original_form": "정말 대단해요 현대과학",
          "speaker_id": "1",
          "time": "20210905 04:27"
        },
        {
          "id": "MDRW2100040774.1.12",
          "form": "그러니깐요..",
          "original_form": "그러니깐요..",
          "speaker_id": "2",
          "time": "20210905 04:28"
        },
        {
          "id": "MDRW2100040774.1.13",
          "form": "저도 뭔가 기여를 하고자 했으나",
          "original_form": "저도 뭔가 기여를 하고자 했으나",
          "speaker_id": "1",
          "time": "20210905 04:28"
        },
        {
          "id": "MDRW2100040774.1.14",
          "form": "허걱",
          "original_form": "허걱",
          "speaker_id": "2",
          "time": "20210905 04:28"
        },
        {
          "id": "MDRW2100040774.1.15",
          "form": "세상에는 너무 재밌는 게 많다...",
          "original_form": "세상에는 너무 재밌는 게 많다...",
          "speaker_id": "1",
          "time": "20210905 04:28"
        },
        {
          "id": "MDRW2100040774.1.16",
          "form": "유기화학~",
          "original_form": "유기화학~",
          "speaker_id": "2",
          "time": "20210905 04:28"
        },
        {
          "id": "MDRW2100040774.1.17",
          "form": "아맞다 유기화학",
          "original_form": "아맞다 유기화학",
          "speaker_id": "1",
          "time": "20210905 04:28"
        },
        {
          "id": "MDRW2100040774.1.18",
          "form": "와근대진짜",
          "original_form": "와근대진짜",
          "speaker_id": "2",
          "time": "20210905 04:28"
        },
        {
          "id": "MDRW2100040774.1.19",
          "form": "뭐하는거냐고 여쭤보셨는데",
          "original_form": "뭐하는거냐고 여쭤보셨는데",
          "speaker_id": "1",
          "time": "20210905 04:28"
        },
        {
          "id": "MDRW2100040774.1.20",
          "form": "앟",
          "original_form": "앟",
          "speaker_id": "2",
          "time": "20210905 04:28"
        },
        {
          "id": "MDRW2100040774.1.21",
          "form": "제가 대답을 제대로 안했어요",
          "original_form": "제가 대답을 제대로 안했어요",
          "speaker_id": "1",
          "time": "20210905 04:28"
        },
        {
          "id": "MDRW2100040774.1.22",
          "form": "큐큐ㅠㅠ큐ㅠㅋ큐",
          "original_form": "큐큐ㅠㅠ큐ㅠㅋ큐",
          "speaker_id": "2",
          "time": "20210905 04:28"
        },
        {
          "id": "MDRW2100040774.1.23",
          "form": "네네 와근데진짜!",
          "original_form": "네네 와근데진짜!",
          "speaker_id": "1",
          "time": "20210905 04:28"
