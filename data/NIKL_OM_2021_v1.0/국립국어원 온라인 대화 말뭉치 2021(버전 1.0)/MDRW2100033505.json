      "utterance": [
        {
          "id": "MDRW2100033505.1.1",
          "form": "또 극중 신부님이니까",
          "original_form": "또 극중 신부님이니까",
          "speaker_id": "2",
          "time": "20210824 08:25"
        },
        {
          "id": "MDRW2100033505.1.2",
          "form": "사람들이 경찰한테 말안해도",
          "original_form": "사람들이 경찰한테 말안해도",
          "speaker_id": "2",
          "time": "20210824 08:25"
        },
        {
          "id": "MDRW2100033505.1.3",
          "form": "신부님한테는 말해요",
          "original_form": "신부님한테는 말해요",
          "speaker_id": "2",
          "time": "20210824 08:26"
        },
        {
          "id": "MDRW2100033505.1.4",
          "form": "신부님한테는 털어놓는구나ㅋㅋ",
          "original_form": "신부님한테는 털어놓는구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 08:26"
        },
        {
          "id": "MDRW2100033505.1.5",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:26"
        },
        {
          "id": "MDRW2100033505.1.6",
          "form": "아 웃기다ㅋㅋㅋ",
          "original_form": "아 웃기다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 08:26"
        },
        {
          "id": "MDRW2100033505.1.7",
          "form": "고백성사 ㅋㅋ",
          "original_form": "고백성사 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:26"
        },
        {
          "id": "MDRW2100033505.1.8",
          "form": "그러면서 잡혀가나요",
          "original_form": "그러면서 잡혀가나요",
          "speaker_id": "1",
          "time": "20210824 08:26"
        },
        {
          "id": "MDRW2100033505.1.9",
          "form": "당신의 비밀은 나랑 당신이랑 저위의 그분밖에 모릅니다",
          "original_form": "당신의 비밀은 나랑 당신이랑 저위의 그분밖에 모릅니다",
          "speaker_id": "2",
          "time": "20210824 08:26"
        },
        {
          "id": "MDRW2100033505.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:26"
        },
        {
          "id": "MDRW2100033505.1.11",
          "form": "ㅋㅋ 아 웃기다ㅋㅋ  어떤 점을 좋아하는지 알거같아요",
          "original_form": "ㅋㅋ 아 웃기다ㅋㅋ  어떤 점을 좋아하는지 알거같아요",
          "speaker_id": "1",
          "time": "20210824 08:26"
        },
        {
          "id": "MDRW2100033505.1.12",
          "form": "신부님이 직접 체포하거나 그런건아니니까요",
          "original_form": "신부님이 직접 체포하거나 그런건아니니까요",
          "speaker_id": "2",
          "time": "20210824 08:27"
        },
        {
          "id": "MDRW2100033505.1.13",
          "form": "되게 매력있네요",
          "original_form": "되게 매력있네요",
          "speaker_id": "1",
          "time": "20210824 08:27"
        },
        {
          "id": "MDRW2100033505.1.14",
          "form": "신부님은 끼어들어서 수사만 함",
          "original_form": "신부님은 끼어들어서 수사만 함",
          "speaker_id": "2",
          "time": "20210824 08:27"
        },
        {
          "id": "MDRW2100033505.1.15",
          "form": "진짜 요새 푹 빠져있어요",
          "original_form": "진짜 요새 푹 빠져있어요",
          "speaker_id": "2",
          "time": "20210824 08:27"
        },
        {
          "id": "MDRW2100033505.1.16",
          "form": "핵심인물이네요 듣기만해도 너무 재밌어요",
          "original_form": "핵심인물이네요 듣기만해도 너무 재밌어요",
          "speaker_id": "1",
          "time": "20210824 08:27"
        },
        {
          "id": "MDRW2100033505.1.17",
          "form": "진짜 꼭 봐봐야겠어요",
          "original_form": "진짜 꼭 봐봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 08:27"
        },
        {
          "id": "MDRW2100033505.1.18",
          "form": "재밌는 드라마 알려줘서 고마워요",
          "original_form": "재밌는 드라마 알려줘서 고마워요",
