      "utterance": [
        {
          "id": "MDRW2100038064.1.1",
          "form": "일갔어유?ㅋ",
          "original_form": "일갔어유?ㅋ",
          "speaker_id": "1",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100038064.1.2",
          "form": "일하고 이제밥먹는다ㅋㅋ",
          "original_form": "일하고 이제밥먹는다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038064.1.3",
          "form": "유가네왔다",
          "original_form": "유가네왔다",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100038064.1.4",
          "form": "사람엄청나네",
          "original_form": "사람엄청나네",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100038064.1.5",
          "form": "오 유가네 좋죠ㅋㅋㅋ",
          "original_form": "오 유가네 좋죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100038064.1.6",
          "form": "맛있겠다ㅋㅋ",
          "original_form": "맛있겠다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100038064.1.7",
          "form": "어ㅋㅋ 여긴 참 맛있네ㅋ",
          "original_form": "어ㅋㅋ 여긴 참 맛있네ㅋ",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038064.1.8",
          "form": "탑텐도가야되고",
          "original_form": "탑텐도가야되고",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038064.1.9",
          "form": "ㅋㅋ맞아요ㅋㅋ탑텐가는구나ㅋㅋ",
          "original_form": "ㅋㅋ맞아요ㅋㅋ탑텐가는구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100038064.1.10",
          "form": "뭐사요?ㅋ",
          "original_form": "뭐사요?ㅋ",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100038064.1.11",
          "form": "티랑바지",
          "original_form": "티랑바지",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100038064.1.12",
          "form": "중성적인걸로다가",
          "original_form": "중성적인걸로다가",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100038064.1.13",
          "form": "침ᆢ",
          "original_form": "침ᆢ",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100038064.1.14",
          "form": "치마",
          "original_form": "치마",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100038064.1.15",
          "form": "치마ㅋ",
          "original_form": "치마ㅋ",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100038064.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100038064.1.17",
          "form": "중성적인걸로ㅋㅋ",
          "original_form": "중성적인걸로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100038064.1.18",
          "form": "둘째옷사러가요?ㅋ",
          "original_form": "둘째옷사러가요?ㅋ",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100038064.1.19",
          "form": "둘다꺼샀다",
          "original_form": "둘다꺼샀다",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100038064.1.20",
          "form": "티랑바지ㅋ",
          "original_form": "티랑바지ㅋ",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100038064.1.21",
          "form": "오 ㅋㅋㅋ 좋네요 ㅋㅋ 많이 샀어요?",
