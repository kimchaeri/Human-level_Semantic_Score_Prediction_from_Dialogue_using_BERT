          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100038064.1.22",
          "form": "거의9만원",
          "original_form": "거의9만원",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038064.1.23",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038064.1.24",
          "form": "별로안샀나?",
          "original_form": "별로안샀나?",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038064.1.25",
          "form": "ㅋㅋㅋㅋ많이 산것같은데요??ㅋ",
          "original_form": "ㅋㅋㅋㅋ많이 산것같은데요??ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038064.1.26",
          "form": "요즘 근데 옷값이 비싸니깐. 그래두 탑텐은 가성비 좋잖아유 ㅋㅋ",
          "original_form": "요즘 근데 옷값이 비싸니깐. 그래두 탑텐은 가성비 좋잖아유 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038064.1.27",
          "form": "그래 옷이 너무없네 애들옷정리하니",
          "original_form": "그래 옷이 너무없네 애들옷정리하니",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100038064.1.28",
          "form": "헌옷수거 한번 또 불러야긋다",
          "original_form": "헌옷수거 한번 또 불러야긋다",
