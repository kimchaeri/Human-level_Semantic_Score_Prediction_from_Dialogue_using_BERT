      "utterance": [
        {
          "id": "MDRW2100025164.1.1",
          "form": "잠들었네요 ㅎㅎ",
          "original_form": "잠들었네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 06:03"
        },
        {
          "id": "MDRW2100025164.1.2",
          "form": "저도 잠들었었어요:)",
          "original_form": "저도 잠들었었어요:)",
          "speaker_id": "1",
          "time": "20210901 07:57"
        },
        {
          "id": "MDRW2100025164.1.3",
          "form": "출근은 잘 하셨나요?",
          "original_form": "출근은 잘 하셨나요?",
          "speaker_id": "1",
          "time": "20210901 07:57"
        },
        {
          "id": "MDRW2100025164.1.4",
          "form": "네 출근완료",
          "original_form": "네 출근완료",
          "speaker_id": "2",
          "time": "20210901 08:23"
        },
        {
          "id": "MDRW2100025164.1.5",
          "form": "오늘우",
          "original_form": "오늘우",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100025164.1.6",
          "form": "오늘은 잘 보내셨나요?",
          "original_form": "오늘은 잘 보내셨나요?",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100025164.1.7",
          "form": "운전연습은 좀 하셨어요?",
          "original_form": "운전연습은 좀 하셨어요?",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100025164.1.8",
          "form": "네:) 저도 출근하고싶네요 ㅋㅋ",
          "original_form": "네:) 저도 출근하고싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100025164.1.9",
          "form": "회사다니고 싶으세요?",
          "original_form": "회사다니고 싶으세요?",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100025164.1.10",
          "form": "네ㅋㅋ\r\n저도 출퇴근시간 보장되는\r\n회사에 다니고 싶어요 ㅋ",
          "original_form": "네ㅋㅋ\r\n저도 출퇴근시간 보장되는\r\n회사에 다니고 싶어요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100025164.1.11",
          "form": "육아는 24시간 연중무휴라ㅠ",
          "original_form": "육아는 24시간 연중무휴라ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100025164.1.12",
          "form": "아......",
          "original_form": "아......",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100025164.1.13",
          "form": "남자아이 둘이서",
          "original_form": "남자아이 둘이서",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100025164.1.14",
          "form": "엄마를 힘들게 하나요?",
          "original_form": "엄마를 힘들게 하나요?",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100025164.1.15",
          "form": "남편분오시면 퇴근하시면 안되나요..?ㅋ",
          "original_form": "남편분오시면 퇴근하시면 안되나요..?ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100025164.1.16",
          "form": "ㅠㅠ아빠도 힘들져",
          "original_form": "ㅠㅠ아빠도 힘들져",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100025164.1.17",
          "form": "name2은 무슨 일 하세요?",
          "original_form": "&name2&은 무슨 일 하세요?",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100025164.1.18",
          "form": "저 그냥 회사다니져",
          "original_form": "저 그냥 회사다니져",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100025164.1.19",
          "form": "전문직은 아닙니다 ㅋㅋ",
          "original_form": "전문직은 아닙니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100025164.1.20",
          "form": "전문직인가...",
          "original_form": "전문직인가...",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100025164.1.21",
          "form": "잘 모르겠네여",
          "original_form": "잘 모르겠네여",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100025164.1.22",
          "form": "오잉? 잘 모르시는군용",
          "original_form": "오잉? 잘 모르시는군용",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100025164.1.23",
          "form": "지금 회사에\r\n근무하신지는 얼마나 되셨어요?",
          "original_form": "지금 회사에\r\n근무하신지는 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100025164.1.24",
          "form": "이직한지 얼마 안되서 5개월요",
          "original_form": "이직한지 얼마 안되서 5개월요",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100025164.1.25",
          "form": "아 이직하셨군용!",
          "original_form": "아 이직하셨군용!",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100025164.1.26",
          "form": "코시국에 이직 성공을 ",
          "original_form": "코시국에 이직 성공을 ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100025164.1.27",
          "form": "짝짝짝짝",
          "original_form": "짝짝짝짝",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100025164.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100025164.1.29",
          "form": "예전에 회사다니셨죠?",
          "original_form": "예전에 회사다니셨죠?",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100025164.1.30",
          "form": "일반직장인 or 전문직?",
          "original_form": "일반직장인 or 전문직?",
