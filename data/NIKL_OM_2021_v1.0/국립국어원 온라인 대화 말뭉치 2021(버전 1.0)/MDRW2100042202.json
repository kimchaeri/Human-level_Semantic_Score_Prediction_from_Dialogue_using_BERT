      "utterance": [
        {
          "id": "MDRW2100042202.1.1",
          "form": "닭요리 대회를 언니가 나갔었는데요",
          "original_form": "닭요리 대회를 언니가 나갔었는데요",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042202.1.2",
          "form": "사실은 우리 언니가 이런 걸 잘해요",
          "original_form": "사실은 우리 언니가 이런 걸 잘해요",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042202.1.3",
          "form": "요리사거든요",
          "original_form": "요리사거든요",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042202.1.4",
          "form": "그래서 이런 대회 있는 정보를 잘 알아서 접수해 줬어요",
          "original_form": "그래서 이런 대회 있는 정보를 잘 알아서 접수해 줬어요",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042202.1.5",
          "form": "와!!! 금손 유전자였군요!!",
          "original_form": "와!!! 금손 유전자였군요!!",
          "speaker_id": "1",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042202.1.6",
          "form": "여러번 대회 나갔엇는데",
          "original_form": "여러번 대회 나갔엇는데",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042202.1.7",
          "form": "대회마다 재료 주제가 있어요",
          "original_form": "대회마다 재료 주제가 있어요",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042202.1.8",
          "form": "진짜 넘 행복하겠어요 가족분들은!!",
          "original_form": "진짜 넘 행복하겠어요 가족분들은!!",
          "speaker_id": "1",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042202.1.9",
          "form": "연어요리 대회도 있고",
          "original_form": "연어요리 대회도 있고",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042202.1.10",
          "form": "돼지고기 대회도 있고 이런 식이에요",
          "original_form": "돼지고기 대회도 있고 이런 식이에요",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042202.1.11",
          "form": "전 여기저기 다니다가 닭요리 대회 때 대상탄거죠",
          "original_form": "전 여기저기 다니다가 닭요리 대회 때 대상탄거죠",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042202.1.12",
          "form": "레시피는 언니가 다 만들어줬어요 \r\n전 하기만 했죠",
          "original_form": "레시피는 언니가 다 만들어줬어요 \r\n전 하기만 했죠",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042202.1.13",
          "form": "진짜 멋있으세요!!!",
          "original_form": "진짜 멋있으세요!!!",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042202.1.14",
          "form": "레시피 따라하는것도 잘 안되는 사람이 얼마나 많은데요! ㅋㅋㅋ",
          "original_form": "레시피 따라하는것도 잘 안되는 사람이 얼마나 많은데요! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042202.1.15",
          "form": "저희 언니도 상 많이 타서 6시 내 고향에도 나오고 그랬어요",
          "original_form": "저희 언니도 상 많이 타서 6시 내 고향에도 나오고 그랬어요",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042202.1.16",
          "form": "저 대상타는건 뉴스에 단신으로 잠깐 나왔었죠",
          "original_form": "저 대상타는건 뉴스에 단신으로 잠깐 나왔었죠",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042202.1.17",
          "form": "와 저도 봤을 수도 있겠는데요! ㅎㅎ 최고심",
          "original_form": "와 저도 봤을 수도 있겠는데요! ㅎㅎ 최고심",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042202.1.18",
          "form": "이렇게 스타분을 만나게되다니 영광입니다! ㅎㅎ",
          "original_form": "이렇게 스타분을 만나게되다니 영광입니다! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042202.1.19",
          "form": "KBS주최 대회였을 거에요",
          "original_form": "KBS주최 대회였을 거에요",
