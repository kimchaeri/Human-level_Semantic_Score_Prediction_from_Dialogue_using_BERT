      "utterance": [
        {
          "id": "MDRW2100020623.1.1",
          "form": "아 이번건 예상 못했네ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 이번건 예상 못했네ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020623.1.2",
          "form": "지금은 메론약간 넣는다던데 루머인가",
          "original_form": "지금은 메론약간 넣는다던데 루머인가",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020623.1.3",
          "form": "ㅋㅋㅋㅋ 긴장하시라구요",
          "original_form": "ㅋㅋㅋㅋ 긴장하시라구요",
          "speaker_id": "1",
          "time": "20210828 10:31"
        },
        {
          "id": "MDRW2100020623.1.4",
          "form": "오랜만에 메로나도 먹어봐야겠네요",
          "original_form": "오랜만에 메로나도 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020623.1.5",
          "form": "메로나도 오리지널만 드시나요",
          "original_form": "메로나도 오리지널만 드시나요",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020623.1.6",
          "form": "카페는 즐겨가시나요",
          "original_form": "카페는 즐겨가시나요",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020623.1.7",
          "form": "요즘 맛 겁나 많아졌던데",
          "original_form": "요즘 맛 겁나 많아졌던데",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020623.1.8",
          "form": "네 카페 좋아합니다",
          "original_form": "네 카페 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020623.1.9",
          "form": "네 딴거 도전을 못해봤어묘",
          "original_form": "네 딴거 도전을 못해봤어묘",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020623.1.10",
          "form": "왜냐면 메로나는 메론맛이나서 먹는거니까!",
          "original_form": "왜냐면 메로나는 메론맛이나서 먹는거니까!",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020623.1.11",
          "form": "지조있는타입",
          "original_form": "지조있는타입",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020623.1.12",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020623.1.13",
          "form": "카페즐겨가세요",
          "original_form": "카페즐겨가세요",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020623.1.14",
          "form": "요즘은 잘 안가는데",
          "original_form": "요즘은 잘 안가는데",
          "speaker_id": "2",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020623.1.15",
          "form": "늘 가서 아아를 시킨답니다",
          "original_form": "늘 가서 아아를 시킨답니다",
          "speaker_id": "2",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020623.1.16",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "1",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020623.1.17",
          "form": "얼죽아회원이십니까",
          "original_form": "얼죽아회원이십니까",
          "speaker_id": "1",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020623.1.18",
          "form": "저는 얼죽아 회원입니다",
          "original_form": "저는 얼죽아 회원입니다",
          "speaker_id": "1",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020623.1.19",
          "form": "어휴 따뜻한거 먹으면",
          "original_form": "어휴 따뜻한거 먹으면",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020623.1.20",
          "form": "속이 느끼해요",
          "original_form": "속이 느끼해요",
          "speaker_id": "2",
          "time": "20210828 10:34"
        }
      ]
