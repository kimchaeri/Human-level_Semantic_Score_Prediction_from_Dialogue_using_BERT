        },
        {
          "id": "MDRW2100036566.1.2",
          "form": "아까 방탄소년단 브이앱 봤니",
          "original_form": "아까 방탄소년단 브이앱 봤니",
          "speaker_id": "1",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100036566.1.3",
          "form": "어떤 멤버",
          "original_form": "어떤 멤버",
          "speaker_id": "2",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100036566.1.4",
          "form": "지민이가 다녀갔어",
          "original_form": "지민이가 다녀갔어",
          "speaker_id": "1",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100036566.1.5",
          "form": "팬한테 정말 잘하네",
          "original_form": "팬한테 정말 잘하네",
          "speaker_id": "2",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036566.1.6",
          "form": "오늘 엄청 귀엽더라",
          "original_form": "오늘 엄청 귀엽더라",
          "speaker_id": "1",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036566.1.7",
          "form": "지민이는 원래 좀 귀엽지",
          "original_form": "지민이는 원래 좀 귀엽지",
          "speaker_id": "2",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036566.1.8",
          "form": "지민이 교정도 할 거래요",
          "original_form": "지민이 교정도 할 거래요",
          "speaker_id": "1",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036566.1.9",
          "form": "교정해야 되는 줄 처음 알았어",
          "original_form": "교정해야 되는 줄 처음 알았어",
          "speaker_id": "2",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036566.1.10",
          "form": "그러게 이빨도 귀엽기만 하던데",
          "original_form": "그러게 이빨도 귀엽기만 하던데",
          "speaker_id": "1",
          "time": "20210831 00:45"
        },
        {
          "id": "MDRW2100036566.1.11",
          "form": "하고 나면 더 잘 생겼을지도 몰라",
          "original_form": "하고 나면 더 잘 생겼을지도 몰라",
          "speaker_id": "2",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100036566.1.12",
          "form": "빛이 나겠네",
          "original_form": "빛이 나겠네",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100036566.1.13",
          "form": "방탄소년단은 정말 다들 대단해",
