      "utterance": [
        {
          "id": "MDRW2100037866.1.1",
          "form": "흠 결혼하셨나요",
          "original_form": "흠 결혼하셨나요",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100037866.1.2",
          "form": "네 저 결혼했어요!^^",
          "original_form": "네 저 결혼했어요!^^",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100037866.1.3",
          "form": "저는 미혼이라 할말이ㅜㅜ",
          "original_form": "저는 미혼이라 할말이ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100037866.1.4",
          "form": "어떻게 만나셨어요 ㅎㅎ",
          "original_form": "어떻게 만나셨어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100037866.1.5",
          "form": "아 ㅎㅎ 전 대학교 씨씨였어용",
          "original_form": "아 ㅎㅎ 전 대학교 씨씨였어용",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100037866.1.6",
          "form": "다른과오빠.",
          "original_form": "다른과오빠{emoji:♥}.{emoji:♥}",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100037866.1.7",
          "form": "미혼일때 많이즐기세요 ㅋㅋ",
          "original_form": "미혼일때 많이즐기세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100037866.1.8",
          "form": "음 전.. 어디서 만나야하지? 부터 고민이에요ㅋㅋ",
          "original_form": "음 전.. 어디서 만나야하지? 부터 고민이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100037866.1.9",
          "form": "여중 여고 대학도 여자만 드글드글한 과 나오고 ㅋㅋㅋ",
          "original_form": "여중 여고 대학도 여자만 드글드글한 과 나오고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100037866.1.10",
          "form": "ㅋㅋㄱㄱㄱㄱ  저도 저희과는  여자만 있는과여서",
          "original_form": "ㅋㅋㄱㄱㄱㄱ  저도 저희과는  여자만 있는과여서",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100037866.1.11",
          "form": "저도 여고 ^^;",
          "original_form": "저도 여고 ^^;",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100037866.1.12",
          "form": "대학때 ㅋㅋ괜찮은 오빠있는지나 알아볼것이지ㅋㅋ",
          "original_form": "대학때 ㅋㅋ괜찮은 오빠있는지나 알아볼것이지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100037866.1.13",
          "form": "제동생은 앱으로 이번에 남자친구 사겼어요!",
          "original_form": "제동생은 앱으로 이번에 남자친구 사겼어요!",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100037866.1.14",
          "form": "앱으로요??",
          "original_form": "앱으로요??",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100037866.1.15",
          "form": "우와 30대아줌마는 무섭네욬ㅋㅋ",
          "original_form": "우와 30대아줌마는 무섭네욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100037866.1.16",
          "form": "네네 좀신기하기도하고 ㅋㄱ",
          "original_form": "네네 좀신기하기도하고 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100037866.1.17",
          "form": "30대세요?",
          "original_form": "30대세요?",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100037866.1.18",
          "form": "네 30대에요^^  나이든 티나네요ㅠㅠ",
          "original_form": "네 30대에요^^  나이든 티나네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100037866.1.19",
          "form": "앱으로 남친이라니..ㅎㅎㅎ 왕신기해요",
          "original_form": "앱으로 남친이라니..ㅎㅎㅎ 왕신기해요",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100037866.1.20",
          "form": "착한남자만나세요 ! 요즘 뉴스보면 무서운일이 너무 많아묘~~",
          "original_form": "착한남자만나세요 ! 요즘 뉴스보면 무서운일이 너무 많아묘~~",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100037866.1.21",
          "form": "저도 제동생도 30대인걸요 ㅋ",
          "original_form": "저도 제동생도 30대인걸요 ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100037866.1.22",
          "form": "우린아직 늙지않았다며 ㅋㅋ",
          "original_form": "우린아직 늙지않았다며 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100037866.1.23",
          "form": "근데 코로나라서 뭔가 소개팅도 잘 없나봐요",
          "original_form": "근데 코로나라서 뭔가 소개팅도 잘 없나봐요",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100037866.1.24",
          "form": "전 아줌마라 흑흑ㅋㅋ\r\n미혼친구들이 젤 부러워요 ㅋㅎ",
          "original_form": "전 아줌마라 흑흑ㅋㅋ\r\n미혼친구들이 젤 부러워요 ㅋㅎ",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100037866.1.25",
          "form": "아 그러네요 코로나때문에.ㅜㅜ 자꾸 확진자만 늘고",
          "original_form": "아 그러네요 코로나때문에.ㅜㅜ 자꾸 확진자만 늘고",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100037866.1.26",
          "form": "전 결혼하시거나 육아하는 친구들이 부러워요",
          "original_form": "전 결혼하시거나 육아하는 친구들이 부러워요",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100037866.1.27",
          "form": "뭔가 이뤄냈구나? 싶은 ㅎㅎ",
          "original_form": "뭔가 이뤄냈구나? 싶은 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100037866.1.28",
          "form": "ㅎㅎㅎ 서로 가지지 못한것을 부러워하네요",
          "original_form": "ㅎㅎㅎ 서로 가지지 못한것을 부러워하네요",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100037866.1.29",
          "form": "부러워하지마세욯ㅋㄱㅋ 어느날 금방이더라구요~ㅎㅎ",
          "original_form": "부러워하지마세욯ㅋㄱㅋ 어느날 금방이더라구요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100037866.1.30",
          "form": "근데 저 주변에 이야기 하다보면 진짜 못된 남편들 너무많아요!",
          "original_form": "근데 저 주변에 이야기 하다보면 진짜 못된 남편들 너무많아요!",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100037866.1.31",
          "form": "남편감은 꼭 가정적이고 선한 남자로 고르시길..ㅎㅎㅎ",
          "original_form": "남편감은 꼭 가정적이고 선한 남자로 고르시길..ㅎㅎㅎ",
