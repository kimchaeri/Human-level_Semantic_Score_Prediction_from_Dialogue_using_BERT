      "utterance": [
        {
          "id": "MDRW2100014845.1.1",
          "form": "와 꾸준히 하는게 대단한거 같네요",
          "original_form": "와 꾸준히 하는게 대단한거 같네요",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014845.1.2",
          "form": "전 센터 안나가면 절대못해요 ㅋㅋ",
          "original_form": "전 센터 안나가면 절대못해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014845.1.3",
          "form": "살려고 하는 편이에요ㅋㅋ",
          "original_form": "살려고 하는 편이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014845.1.4",
          "form": "안 하면 허리나 여기저기 너무 아파서요",
          "original_form": "안 하면 허리나 여기저기 너무 아파서요",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014845.1.5",
          "form": "맞아요 진짜 몸이 더 아프죠 ㅠㅠ",
          "original_form": "맞아요 진짜 몸이 더 아프죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014845.1.6",
          "form": "안하면 꾸준히 하는게 좋은거같아요",
          "original_form": "안하면 꾸준히 하는게 좋은거같아요",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014845.1.7",
          "form": "저도 요가해본 적 있는데 다시 다녀볼까봐요",
          "original_form": "저도 요가해본 적 있는데 다시 다녀볼까봐요",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014845.1.8",
          "form": "네 마스크",
          "original_form": "네 마스크",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014845.1.9",
          "form": "생각보다 안갑갑해요 ㅋㅋ",
          "original_form": "생각보다 안갑갑해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014845.1.10",
          "form": "오 그렇군요 마스크 뭐 쓰고 하세요?",
          "original_form": "오 그렇군요 마스크 뭐 쓰고 하세요?",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014845.1.11",
          "form": "대부분 갑갑하니까 덴탈마스크 쓰는데",
          "original_form": "대부분 갑갑하니까 덴탈마스크 쓰는데",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014845.1.12",
          "form": "말안하니까 쌤말고 ㅋㅋ",
          "original_form": "말안하니까 쌤말고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014845.1.13",
          "form": "괜찮은거같아요",
          "original_form": "괜찮은거같아요",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014845.1.14",
          "form": "요즘 하기 좋은 운동이네요",
          "original_form": "요즘 하기 좋은 운동이네요",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014845.1.15",
          "form": "백신 접종은 아직이세요?",
          "original_form": "백신 접종은 아직이세요?",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014845.1.16",
          "form": "네 그래서 ㅠㅠ 근데 이번달에 맞아요 ㅋㅋ",
          "original_form": "네 그래서 ㅠㅠ 근데 이번달에 맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014845.1.17",
          "form": "그래서 담달부터 열심히 운동하려고요 ㅋㅋ",
          "original_form": "그래서 담달부터 열심히 운동하려고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014845.1.18",
          "form": "저도 낼모레 맞아요",
          "original_form": "저도 낼모레 맞아요",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014845.1.19",
          "form": "비상약 잘 챙겨두세요!",
          "original_form": "비상약 잘 챙겨두세요!",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014845.1.20",
          "form": "걱덩되네요 ㅠㅠ 그 타이레놀",
          "original_form": "걱덩되네요 ㅠㅠ 그 타이레놀",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014845.1.21",
          "form": "먹어야한다하더라구요",
          "original_form": "먹어야한다하더라구요",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014845.1.22",
          "form": "아프면",
          "original_form": "아프면",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014845.1.23",
          "form": "네 저는 타이레놀 사놓긴 했는데 잘 안 들어서 걱정입니다ㅋㅋ",
          "original_form": "네 저는 타이레놀 사놓긴 했는데 잘 안 들어서 걱정입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014845.1.24",
          "form": "아이고 이게  대부분 팔만 아프다고는 하는데",
          "original_form": "아이고 이게  대부분 팔만 아프다고는 하는데",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014845.1.25",
          "form": "부작용 심하면 심장내과가 있는 응급실로 뛰어가래요",
          "original_form": "부작용 심하면 심장내과가 있는 응급실로 뛰어가래요",
          "speaker_id": "2",
