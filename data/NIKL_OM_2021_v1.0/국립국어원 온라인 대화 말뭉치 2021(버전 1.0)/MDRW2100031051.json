        },
        {
          "id": "MDRW2100031051.1.2",
          "form": "아니요ㅋㅋㅋ면제입니다",
          "original_form": "아니요ㅋㅋㅋ면제입니다",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100031051.1.3",
          "form": "디피 보셨나요?",
          "original_form": "디피 보셨나요?",
          "speaker_id": "1",
          "time": "20210907 05:53"
        },
        {
          "id": "MDRW2100031051.1.4",
          "form": "아 그런가요",
          "original_form": "아 그런가요",
          "speaker_id": "2",
          "time": "20210907 06:48"
        },
        {
          "id": "MDRW2100031051.1.5",
          "form": "네 요즘 핫하죠 ㅎㅎ",
          "original_form": "네 요즘 핫하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 06:48"
        },
        {
          "id": "MDRW2100031051.1.6",
          "form": "오오 저눈 무서워서 껐어요ㅠㅠ",
          "original_form": "오오 저눈 무서워서 껐어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 07:03"
        },
        {
          "id": "MDRW2100031051.1.7",
          "form": "참고 보셔야죠",
          "original_form": "참고 보셔야죠",
          "speaker_id": "2",
          "time": "20210907 08:12"
        },
        {
          "id": "MDRW2100031051.1.8",
          "form": "ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:12"
        },
        {
          "id": "MDRW2100031051.1.9",
          "form": "끝까지 다보셨어요?",
          "original_form": "끝까지 다보셨어요?",
          "speaker_id": "1",
          "time": "20210907 08:12"
        },
        {
          "id": "MDRW2100031051.1.10",
          "form": "4회까지인 봤어요 얼른 정주행해야죠",
          "original_form": "4회까지인 봤어요 얼른 정주행해야죠",
          "speaker_id": "2",
          "time": "20210907 08:14"
        },
        {
          "id": "MDRW2100031051.1.11",
          "form": "오 마지막 2편 남았네요",
          "original_form": "오 마지막 2편 남았네요",
          "speaker_id": "1",
          "time": "20210907 08:15"
        },
        {
          "id": "MDRW2100031051.1.12",
          "form": "ㅋㅋㅋ보세요 재밌어요 진짜",
          "original_form": "ㅋㅋㅋ보세요 재밌어요 진짜",
