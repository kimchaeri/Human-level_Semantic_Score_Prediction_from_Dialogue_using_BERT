        },
        {
          "id": "MDRW2100035320.1.15",
          "form": "에고",
          "original_form": "에고",
          "speaker_id": "2",
          "time": "20210829 14:50"
        },
        {
          "id": "MDRW2100035320.1.16",
          "form": "그런일이 있군요",
