      "utterance": [
        {
          "id": "MDRW2100025625.1.1",
          "form": "구기종목 안해본지 너무 오래됐어요",
          "original_form": "구기종목 안해본지 너무 오래됐어요",
          "speaker_id": "1",
          "time": "20210901 14:28"
        },
        {
          "id": "MDRW2100025625.1.2",
          "form": "코로나 끝나면 탁구장 가고 싶습니다",
          "original_form": "코로나 끝나면 탁구장 가고 싶습니다",
          "speaker_id": "1",
          "time": "20210901 14:28"
        },
        {
          "id": "MDRW2100025625.1.3",
          "form": "탁구가 인기많더라고요",
          "original_form": "탁구가 인기많더라고요",
          "speaker_id": "2",
          "time": "20210901 14:33"
        },
        {
          "id": "MDRW2100025625.1.4",
          "form": "티비보면서 해보고싶더라고요",
          "original_form": "티비보면서 해보고싶더라고요",
          "speaker_id": "2",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025625.1.5",
          "form": "크게 어렵지 않은게 제일 장점인거 같아요",
          "original_form": "크게 어렵지 않은게 제일 장점인거 같아요",
          "speaker_id": "1",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100025625.1.6",
          "form": "탁구장이 많아서 접근성도 좋구요",
          "original_form": "탁구장이 많아서 접근성도 좋구요",
          "speaker_id": "1",
          "time": "20210901 16:39"
        },
        {
          "id": "MDRW2100025625.1.7",
          "form": "맞아요 교회에서도 많이 하죠 ㅎ",
          "original_form": "맞아요 교회에서도 많이 하죠 ㅎ",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100025625.1.8",
          "form": "학생때 진짜 많이 쳤었는데",
          "original_form": "학생때 진짜 많이 쳤었는데",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025625.1.9",
          "form": "어릴땐 다재밌었던거같아요",
          "original_form": "어릴땐 다재밌었던거같아요",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100025625.1.10",
          "form": "그건 맞죠ㅋㅋ",
          "original_form": "그건 맞죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100025625.1.11",
          "form": "엄청뛰어다니고",
          "original_form": "엄청뛰어다니고",
          "speaker_id": "2",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100025625.1.12",
          "form": "집 밖에서 노는 시간이 더 길었던거 같아요",
          "original_form": "집 밖에서 노는 시간이 더 길었던거 같아요",
          "speaker_id": "1",
          "time": "20210902 09:10"
        },
        {
          "id": "MDRW2100025625.1.13",
          "form": "매일매일 스포츠를했죠 ㅎ",
          "original_form": "매일매일 스포츠를했죠 ㅎ",
          "speaker_id": "2",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100025625.1.14",
          "form": "주로 어떤거 하셨어요?",
          "original_form": "주로 어떤거 하셨어요?",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100025625.1.15",
          "form": "고무줄이랑 술래잡기 온동네돌아다녔어요",
          "original_form": "고무줄이랑 술래잡기 온동네돌아다녔어요",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100025625.1.16",
          "form": "ㅋㅋ 저두요",
          "original_form": "ㅋㅋ 저두요",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100025625.1.17",
          "form": "어릴때 더 많이 돌아다닌거 같아요",
          "original_form": "어릴때 더 많이 돌아다닌거 같아요",
          "speaker_id": "1",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100025625.1.18",
          "form": "해지면 들어가고",
          "original_form": "해지면 들어가고",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100025625.1.19",
          "form": "맞아요오... 그립네요 진짜",
          "original_form": "맞아요오... 그립네요 진짜",
