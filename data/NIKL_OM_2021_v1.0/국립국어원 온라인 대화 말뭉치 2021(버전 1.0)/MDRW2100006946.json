      "utterance": [
        {
          "id": "MDRW2100006946.1.1",
          "form": "안녕하세요^^",
          "original_form": "안녕하세요^^",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006946.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006946.1.3",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006946.1.4",
          "form": "버거킹 버거요 ~  나무님은요?",
          "original_form": "버거킹 버거요 ~  나무님은요?",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006946.1.5",
          "form": "저는 삼계탕이요~ 레토르트 ㅋㅋ",
          "original_form": "저는 삼계탕이요~ 레토르트 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006946.1.6",
          "form": "오~~맛있었겠다",
          "original_form": "오~~맛있었겠다",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006946.1.7",
          "form": "비도오고 오늘은 딱 삼계탕이죠",
          "original_form": "비도오고 오늘은 딱 삼계탕이죠",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006946.1.8",
          "form": "맛은 그냥 평범했어요~",
          "original_form": "맛은 그냥 평범했어요~",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006946.1.9",
          "form": "건강해지는 맛이기를~~",
          "original_form": "건강해지는 맛이기를~~",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006946.1.10",
          "form": "뭔가 야식 땡기지않으시나여 ㅎ",
          "original_form": "뭔가 야식 땡기지않으시나여 ㅎ",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006946.1.11",
          "form": "진짜 비 오니까 국물 생각나서 ㅋㅋ",
          "original_form": "진짜 비 오니까 국물 생각나서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006946.1.12",
          "form": "야식 좋죠",
          "original_form": "야식 좋죠",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006946.1.13",
          "form": "맞아여  비오니까",
          "original_form": "맞아여  비오니까",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006946.1.14",
          "form": "매운 거",
          "original_form": "매운 거",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006946.1.15",
          "form": "떡볶이나 마라탕 ㅎ",
          "original_form": "떡볶이나 마라탕 ㅎ",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006946.1.16",
          "form": "급 배고파짐요 ㅎ",
          "original_form": "급 배고파짐요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100006946.1.17",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100006946.1.18",
          "form": "내가 뭐 잘못 눌렀나보다",
          "original_form": "내가 뭐 잘못 눌렀나보다",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100006946.1.19",
          "form": "먹고싶어져서요~~",
          "original_form": "먹고싶어져서요~~",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100006946.1.20",
          "form": "아 ㅎㅎ",
          "original_form": "아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100006946.1.21",
          "form": "자판이 이상한가?",
          "original_form": "자판이 이상한가?",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100006946.1.22",
          "form": "요즘은 떡볶이도 매워서 로제만 시켜요",
          "original_form": "요즘은 떡볶이도 매워서 로제만 시켜요",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100006946.1.23",
          "form": "그럴수도 있죠",
          "original_form": "그럴수도 있죠",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100006946.1.24",
          "form": "로제 소스 진짜 맛나죠",
          "original_form": "로제 소스 진짜 맛나죠",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100006946.1.25",
          "form": "배떡이 젤 나은듯",
          "original_form": "배떡이 젤 나은듯",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100006946.1.26",
          "form": "매운 거 잘 못 먹어서 로제 유행한 거 정말 다행이에요",
          "original_form": "매운 거 잘 못 먹어서 로제 유행한 거 정말 다행이에요",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100006946.1.27",
          "form": "아  잘 못드시는구나",
          "original_form": "아  잘 못드시는구나",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100006946.1.28",
          "form": "요새 다 매운데 고생하실듯",
          "original_form": "요새 다 매운데 고생하실듯",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100006946.1.29",
          "form": "네.. 마라까지는 용기가 안나요 ㅋㅋ",
          "original_form": "네.. 마라까지는 용기가 안나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100006946.1.30",
          "form": "마라탕을 즐기시다니 정말 용감하세요",
          "original_form": "마라탕을 즐기시다니 정말 용감하세요",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006946.1.31",
          "form": "매운거 먹다보니ㅈ내성이 ㅋ",
          "original_form": "매운거 먹다보니ㅈ내성이 ㅋ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006946.1.32",
          "form": "먹을수록 단련됩니당 ㅎ",
          "original_form": "먹을수록 단련됩니당 ㅎ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006946.1.33",
          "form": "도전하심이 ㄷㄷ",
          "original_form": "도전하심이 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006946.1.34",
          "form": "그쵸? 먹다보면 는다는데",
          "original_form": "그쵸? 먹다보면 는다는데",
          "speaker_id": "2",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006946.1.35",
          "form": "처음 엽떡나올때 넘 매워서 못먹었는데..",
          "original_form": "처음 엽떡나올때 넘 매워서 못먹었는데..",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100006946.1.36",
          "form": "이젠 없어서 못먹어요 ㅋㅋ",
          "original_form": "이젠 없어서 못먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100006946.1.37",
          "form": "진짜 충격이었어요 떡이랑 어묵 하나씩 먹고 젓가락 내려놨었는데 ㅋㅋ",
          "original_form": "진짜 충격이었어요 떡이랑 어묵 하나씩 먹고 젓가락 내려놨었는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100006946.1.38",
          "form": "ㅎㅎ 쥬시쿨로도 진정안되는 매운맛",
          "original_form": "ㅎㅎ 쥬시쿨로도 진정안되는 매운맛",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006946.1.39",
          "form": "어느새 제일 매운단계가 가능합니당 ㅋ",
          "original_form": "어느새 제일 매운단계가 가능합니당 ㅋ",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006946.1.40",
          "form": "헐 제일 매운단계??!!",
          "original_form": "헐 제일 매운단계??!!",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006946.1.41",
          "form": "전 입만 갖다대도 입술이 부을 것 같던데 ㄷㄷ",
          "original_form": "전 입만 갖다대도 입술이 부을 것 같던데 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100006946.1.42",
          "form": "단력하시면",
          "original_form": "단력하시면",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100006946.1.43",
          "form": "매운맛 통증이  스트레스가 풀리더라구요",
          "original_form": "매운맛 통증이  스트레스가 풀리더라구요",
          "speaker_id": "1",
          "time": "20210822 00:36"
        }
      ]
