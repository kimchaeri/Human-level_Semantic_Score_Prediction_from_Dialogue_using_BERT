      "utterance": [
        {
          "id": "MDRW2100032425.1.1",
          "form": "맞아 엄청 선선해졌어",
          "original_form": "맞아 엄청 선선해졌어",
          "speaker_id": "2",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032425.1.2",
          "form": "밤에는 춥기까지 하더라",
          "original_form": "밤에는 춥기까지 하더라",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032425.1.3",
          "form": "그러게 엊그제까지 여름이었는데",
          "original_form": "그러게 엊그제까지 여름이었는데",
          "speaker_id": "2",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032425.1.4",
          "form": "입추매직 ㅋㅋㅋ 처서 지났나?",
          "original_form": "입추매직 ㅋㅋㅋ 처서 지났나?",
          "speaker_id": "1",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032425.1.5",
          "form": "그래도 아직 낮에는 덥더라고",
          "original_form": "그래도 아직 낮에는 덥더라고",
          "speaker_id": "2",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032425.1.6",
          "form": "어제 시장 갔는데",
          "original_form": "어제 시장 갔는데",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032425.1.7",
          "form": "덥더라",
          "original_form": "덥더라",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032425.1.8",
          "form": "시장에 사람 많았어?",
          "original_form": "시장에 사람 많았어?",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032425.1.9",
          "form": "많던데",
          "original_form": "많던데",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032425.1.10",
          "form": "주말이라 그런가",
          "original_form": "주말이라 그런가",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032425.1.11",
          "form": "요즘 밤에 창문 열고 자는데 시원해서 좋아",
          "original_form": "요즘 밤에 창문 열고 자는데 시원해서 좋아",
          "speaker_id": "2",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100032425.1.12",
          "form": "일교차가 커서",
          "original_form": "일교차가 커서",
          "speaker_id": "1",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100032425.1.13",
          "form": "맞아 감기 조심해야해 환절기니까",
          "original_form": "맞아 감기 조심해야해 환절기니까",
          "speaker_id": "2",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100032425.1.14",
          "form": "금방 겨울 오겠다",
          "original_form": "금방 겨울 오겠다",
          "speaker_id": "1",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100032425.1.15",
          "form": "확 추워지긴 하던데",
          "original_form": "확 추워지긴 하던데",
          "speaker_id": "2",
          "time": "20210905 17:23"
        }
