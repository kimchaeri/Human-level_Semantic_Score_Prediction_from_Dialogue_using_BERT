      "utterance": [
        {
          "id": "MDRW2100035654.1.1",
          "form": "장화있으신가요",
          "original_form": "장화있으신가요",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035654.1.2",
          "form": "저 그",
          "original_form": "저 그",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035654.1.3",
          "form": "레인부츠??",
          "original_form": "레인부츠??",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035654.1.4",
          "form": "한 십년전인가 유행하던 장화있잖아용",
          "original_form": "한 십년전인가 유행하던 장화있잖아용",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035654.1.5",
          "form": "그거 사고 딱 한번 신었어용",
          "original_form": "그거 사고 딱 한번 신었어용",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035654.1.6",
          "form": "사고싶은데 맨날 얼마나 쓰겠나~~싶어서 미루다가 지금도 없는 사람입니다",
          "original_form": "사고싶은데 맨날 얼마나 쓰겠나~~싶어서 미루다가 지금도 없는 사람입니다",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035654.1.7",
          "form": "미루길 잘하셨어요!ㅋㅋㅋㅋ",
          "original_form": "미루길 잘하셨어요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035654.1.8",
          "form": "이거 비올때 신어봤는데",
          "original_form": "이거 비올때 신어봤는데",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035654.1.9",
          "form": "약간 사용하기 불편한가요?",
          "original_form": "약간 사용하기 불편한가요?",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035654.1.10",
          "form": "빗물 들어가니까 안에 계속 습기차서 고이고",
          "original_form": "빗물 들어가니까 안에 계속 습기차서 고이고",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035654.1.11",
          "form": "저 약간 뽀송양말에대한 로망이 있는데..ㅜ",
          "original_form": "저 약간 뽀송양말에대한 로망이 있는데..ㅜ",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035654.1.12",
          "form": "바람이 안통해서",
          "original_form": "바람이 안통해서",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035654.1.13",
          "form": "습해지겠네요",
          "original_form": "습해지겠네요",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035654.1.14",
          "form": "빗물 들어가는 순간 끝나요",
          "original_form": "빗물 들어가는 순간 끝나요",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035654.1.15",
          "form": "네네ㅠㅠ",
          "original_form": "네네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035654.1.16",
          "form": "으앙...",
          "original_form": "으앙...",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035654.1.17",
          "form": "근데 비가 또 사선으로 내리니까...",
          "original_form": "근데 비가 또 사선으로 내리니까...",
          "speaker_id": "1",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035654.1.18",
          "form": "저는 비만 안들어올수있다면",
          "original_form": "저는 비만 안들어올수있다면",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035654.1.19",
          "form": "태풍오는날은 절대...노우...",
          "original_form": "태풍오는날은 절대...노우...",
          "speaker_id": "1",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035654.1.20",
          "form": "시골에서 신는 허벅장화도 가능합니다",
          "original_form": "시골에서 신는 허벅장화도 가능합니다",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035654.1.21",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035654.1.22",
          "form": "부끄럽지 않으니까..",
          "original_form": "부끄럽지 않으니까..",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035654.1.23",
          "form": "비에 신발젖는거",
          "original_form": "비에 신발젖는거",
          "speaker_id": "1",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035654.1.24",
          "form": "엄청 싫어하시나봐요ㅠㅠ",
          "original_form": "엄청 싫어하시나봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:01"
        }
      ]
