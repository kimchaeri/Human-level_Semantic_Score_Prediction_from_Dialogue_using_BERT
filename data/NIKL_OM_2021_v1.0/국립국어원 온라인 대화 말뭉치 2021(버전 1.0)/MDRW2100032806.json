      "utterance": [
        {
          "id": "MDRW2100032806.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210905 23:58"
        },
        {
          "id": "MDRW2100032806.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032806.1.3",
          "form": "주부님이신가여",
          "original_form": "주부님이신가여",
          "speaker_id": "1",
          "time": "20210905 23:59"
        },
        {
          "id": "MDRW2100032806.1.4",
          "form": "아니요! 저는 주부는 아니에요:) name1님께서는 주부신가요?",
          "original_form": "아니요! 저는 주부는 아니에요:) &name1&님께서는 주부신가요?",
          "speaker_id": "2",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032806.1.5",
          "form": "아뇨 저도 싱글이요",
          "original_form": "아뇨 저도 싱글이요",
          "speaker_id": "1",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032806.1.6",
          "form": "요즘 이사해서 집꾸미고 이것저것 집안일 많이 해서요",
          "original_form": "요즘 이사해서 집꾸미고 이것저것 집안일 많이 해서요",
          "speaker_id": "1",
          "time": "20210906 00:00"
        },
        {
          "id": "MDRW2100032806.1.7",
          "form": "그렇군요! 저도 싱글입니다\r\n저도 이사날짜가 얼마 남지않았어요:)",
          "original_form": "그렇군요! 저도 싱글입니다\r\n저도 이사날짜가 얼마 남지않았어요:)",
          "speaker_id": "2",
          "time": "20210906 00:01"
        },
        {
          "id": "MDRW2100032806.1.8",
          "form": "오 어디로 가시나요",
          "original_form": "오 어디로 가시나요",
          "speaker_id": "1",
          "time": "20210906 00:02"
        },
        {
          "id": "MDRW2100032806.1.9",
          "form": "가족과 함께?",
          "original_form": "가족과 함께?",
          "speaker_id": "1",
          "time": "20210906 00:02"
        },
        {
          "id": "MDRW2100032806.1.10",
          "form": "넵 가족과 함께 갑니다:)\r\n멀리는 못가고 지금 사는 집 근처로만 이동할것같아요!",
          "original_form": "넵 가족과 함께 갑니다:)\r\n멀리는 못가고 지금 사는 집 근처로만 이동할것같아요!",
          "speaker_id": "2",
          "time": "20210906 00:04"
        },
        {
          "id": "MDRW2100032806.1.11",
          "form": "저는 룸메이트들과 살다가 제주에 혼자왔어오",
          "original_form": "저는 룸메이트들과 살다가 제주에 혼자왔어오",
          "speaker_id": "1",
          "time": "20210906 00:04"
        },
        {
          "id": "MDRW2100032806.1.12",
          "form": "정말 부러워요ㅠㅠ\r\n저도 자취해보고 싶은데 장소가 무려 제주라니 \r\n제주에는 다닐곳도 많지요?",
          "original_form": "정말 부러워요ㅠㅠ\r\n저도 자취해보고 싶은데 장소가 무려 제주라니 \r\n제주에는 다닐곳도 많지요?",
          "speaker_id": "2",
          "time": "20210906 00:06"
        },
        {
          "id": "MDRW2100032806.1.13",
          "form": "넹 저도 얼마안되서",
          "original_form": "넹 저도 얼마안되서",
          "speaker_id": "1",
          "time": "20210906 00:06"
        },
        {
          "id": "MDRW2100032806.1.14",
          "form": "집정리하고 이제 돌아다니려구요",
          "original_form": "집정리하고 이제 돌아다니려구요",
          "speaker_id": "1",
          "time": "20210906 00:07"
        },
        {
          "id": "MDRW2100032806.1.15",
          "form": "혼자서 집정리 하시려면 많이 힘드시겠어요\r\n룸메이트분들과 계시다가 혼자 사시는데 적적하진않으세요?",
          "original_form": "혼자서 집정리 하시려면 많이 힘드시겠어요\r\n룸메이트분들과 계시다가 혼자 사시는데 적적하진않으세요?",
          "speaker_id": "2",
          "time": "20210906 00:08"
        },
        {
          "id": "MDRW2100032806.1.16",
          "form": "혼자여서 좋은거도 많았는데",
          "original_form": "혼자여서 좋은거도 많았는데",
          "speaker_id": "1",
          "time": "20210906 00:09"
        },
        {
          "id": "MDRW2100032806.1.17",
          "form": "지금은 밥 먹는게 적적해요",
          "original_form": "지금은 밥 먹는게 적적해요",
          "speaker_id": "1",
          "time": "20210906 00:10"
        },
        {
          "id": "MDRW2100032806.1.18",
          "form": "요리하는거 좋아하거든요",
          "original_form": "요리하는거 좋아하거든요",
          "speaker_id": "1",
          "time": "20210906 00:10"
        }
      ]
