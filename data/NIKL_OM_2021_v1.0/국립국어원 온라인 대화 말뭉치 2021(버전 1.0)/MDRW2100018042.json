      "utterance": [
        {
          "id": "MDRW2100018042.1.1",
          "form": "어떤 음식 좋아하세요??",
          "original_form": "어떤 음식 좋아하세요??",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018042.1.2",
          "form": "파삭한거요...",
          "original_form": "파삭한거요...",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018042.1.3",
          "form": "튀김같은...",
          "original_form": "튀김같은...",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018042.1.4",
          "form": "튀김 너무 맛있죠 ㅠ",
          "original_form": "튀김 너무 맛있죠 ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018042.1.5",
          "form": "치킨 좋아하세요?",
          "original_form": "치킨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100018042.1.6",
          "form": "ㅋㅋㅋㅋ 세상에 튀김 싫어하는 사람 엄죠..",
          "original_form": "ㅋㅋㅋㅋ 세상에 튀김 싫어하는 사람 엄죠..",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018042.1.7",
          "form": "튀김의 연장이쥬.. ㅋㅋㅋ",
          "original_form": "튀김의 연장이쥬.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018042.1.8",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018042.1.9",
          "form": "바삭한 치킨..ㅋㅋㅋ 단짠 양념.. ㅋㅋ",
          "original_form": "바삭한 치킨..ㅋㅋㅋ 단짠 양념.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018042.1.10",
          "form": "신발을 튀겨도 맛있다는 말까지..",
          "original_form": "신발을 튀겨도 맛있다는 말까지..",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018042.1.11",
          "form": "치킨 자주먹어유?",
          "original_form": "치킨 자주먹어유?",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018042.1.12",
          "form": "자주는 아니고 가끔이요 ㅋㅋㅋ",
          "original_form": "자주는 아니고 가끔이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018042.1.13",
          "form": "예전엔 치킨이 젤 좋았는데 요샌 찜닭이나 닭갈비가 도 좋더라구요",
          "original_form": "예전엔 치킨이 젤 좋았는데 요샌 찜닭이나 닭갈비가 도 좋더라구요",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018042.1.14",
          "form": "닭을 좋아하시나바요?",
          "original_form": "닭을 좋아하시나바요?",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100018042.1.15",
          "form": "네 ㅋㅋㅋ 닭으로 한 요리들이 맛난게 많더라구요",
          "original_form": "네 ㅋㅋㅋ 닭으로 한 요리들이 맛난게 많더라구요",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100018042.1.16",
          "form": "닭 어느 부위가 좋으세요ㅡ",
          "original_form": "닭 어느 부위가 좋으세요ㅡ",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100018042.1.17",
          "form": "저는 무조건 다리살만 좋아해요 ㅋㅋㅋ",
          "original_form": "저는 무조건 다리살만 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018042.1.18",
          "form": "치킨도 퍽살은 안 먹어서요 ㅠㅠ",
          "original_form": "치킨도 퍽살은 안 먹어서요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018042.1.19",
          "form": "완전 반대네요..",
          "original_form": "완전 반대네요..",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018042.1.20",
          "form": "원래 취향이 반대인 사람이랑 같이 닭을 먹어야해요 ㅌㅋㅋㅋ",
          "original_form": "원래 취향이 반대인 사람이랑 같이 닭을 먹어야해요 ㅌㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:22"
        }
      ]
