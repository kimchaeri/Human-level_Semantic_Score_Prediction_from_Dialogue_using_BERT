      "utterance": [
        {
          "id": "MDRW2100020882.1.1",
          "form": "착 좋아하시나봐요??",
          "original_form": "착 좋아하시나봐요??",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100020882.1.2",
          "form": "*책",
          "original_form": "*책",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100020882.1.3",
          "form": "전 소설 잡지 좋아해요",
          "original_form": "전 소설 잡지 좋아해요",
          "speaker_id": "2",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100020882.1.4",
          "form": "오 추천하실 만한 소설책 있나요?",
          "original_form": "오 추천하실 만한 소설책 있나요?",
          "speaker_id": "1",
          "time": "20210829 06:35"
        },
        {
          "id": "MDRW2100020882.1.5",
          "form": "전 최근에는 재혼황후 재밌게 봤어요",
          "original_form": "전 최근에는 재혼황후 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210829 09:16"
        },
        {
          "id": "MDRW2100020882.1.6",
          "form": "오~ 저도 본적있어요!",
          "original_form": "오~ 저도 본적있어요!",
          "speaker_id": "1",
          "time": "20210830 10:32"
        },
        {
          "id": "MDRW2100020882.1.7",
          "form": "일러스트 넘 고급진것!!",
          "original_form": "일러스트 넘 고급진것!!",
          "speaker_id": "1",
          "time": "20210830 10:32"
        },
        {
          "id": "MDRW2100020882.1.8",
          "form": "맞아요 잠깐 휴재라 아쉬워요",
          "original_form": "맞아요 잠깐 휴재라 아쉬워요",
          "speaker_id": "2",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100020882.1.9",
          "form": "아 휴재이군요..",
          "original_form": "아 휴재이군요..",
          "speaker_id": "1",
          "time": "20210830 11:06"
        },
        {
          "id": "MDRW2100020882.1.10",
          "form": "드라마로나왔음좋겠어요",
          "original_form": "드라마로나왔음좋겠어요",
          "speaker_id": "1",
          "time": "20210830 11:06"
        },
        {
          "id": "MDRW2100020882.1.11",
          "form": "나온다는 얘기가 있더라고요 ㅎㅎ",
          "original_form": "나온다는 얘기가 있더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100020882.1.12",
          "form": "맞아요..! 늘그렇지만 피셜 ㅠㅠ얼른 공식 떳음좋겟어요!",
          "original_form": "맞아요..! 늘그렇지만 피셜 ㅠㅠ얼른 공식 떳음좋겟어요!",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100020882.1.13",
          "form": "배역 중요할 것 같아요",
          "original_form": "배역 중요할 것 같아요",
          "speaker_id": "2",
          "time": "20210830 12:08"
        },
        {
          "id": "MDRW2100020882.1.14",
          "form": "맞아요 진짜중요하죠 혹시 생각해두신 배우있으시려나요?",
          "original_form": "맞아요 진짜중요하죠 혹시 생각해두신 배우있으시려나요?",
          "speaker_id": "1",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100020882.1.15",
          "form": "아마 여주는 수애배우가 되지 않을까 싶기도해요",
          "original_form": "아마 여주는 수애배우가 되지 않을까 싶기도해요",
          "speaker_id": "2",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100020882.1.16",
          "form": "cf 영향이 좀 있어요",
          "original_form": "cf 영향이 좀 있어요",
          "speaker_id": "2",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100020882.1.17",
          "form": "아하! 오 고급진 황후느낌물씬나긴하네요~",
          "original_form": "아하! 오 고급진 황후느낌물씬나긴하네요~",
          "speaker_id": "1",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100020882.1.18",
          "form": "오디오북은 오디오북대로 매력이 있더라고요",
          "original_form": "오디오북은 오디오북대로 매력이 있더라고요",
          "speaker_id": "2",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100020882.1.19",
          "form": "그쵸 근데전틀어놓고 누우면 잠이 솔솔오더라구요",
          "original_form": "그쵸 근데전틀어놓고 누우면 잠이 솔솔오더라구요",
