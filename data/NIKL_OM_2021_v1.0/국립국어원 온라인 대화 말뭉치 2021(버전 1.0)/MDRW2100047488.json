      "utterance": [
        {
          "id": "MDRW2100047488.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100047488.1.2",
          "form": "맛집좋아하세요?",
          "original_form": "맛집좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100047488.1.3",
          "form": "네~~ 어디 지역가면 네이버에서 맛집 검색해서 가요",
          "original_form": "네~~ 어디 지역가면 네이버에서 맛집 검색해서 가요",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100047488.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100047488.1.5",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100047488.1.6",
          "form": "맛집은 사랑입니다",
          "original_form": "맛집은 사랑입니다",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100047488.1.7",
          "form": "자주가는 맛집 있으세요?",
          "original_form": "자주가는 맛집 있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100047488.1.8",
          "form": "음 단골은아니고",
          "original_form": "음 단골은아니고",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100047488.1.9",
          "form": "저도 나갈곳있으면 주로검색해서찾아가는편이예요ㅋㅋㅋ",
          "original_form": "저도 나갈곳있으면 주로검색해서찾아가는편이예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100047488.1.10",
          "form": "아무데나안드가고",
          "original_form": "아무데나안드가고",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100047488.1.11",
          "form": "저랑 같으시네요 ㅎ",
          "original_form": "저랑 같으시네요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100047488.1.12",
          "form": "ㅋㅋㅋㄱ 그게좋죠",
          "original_form": "ㅋㅋㅋㄱ 그게좋죠",
          "speaker_id": "1",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100047488.1.13",
          "form": "주말에는 맛집 탐방 해야 겠어요",
          "original_form": "주말에는 맛집 탐방 해야 겠어요",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100047488.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100047488.1.15",
          "form": "어디로가시게요?",
          "original_form": "어디로가시게요?",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100047488.1.16",
          "form": "그건 아직 안 정했어요 ㅎ",
          "original_form": "그건 아직 안 정했어요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100047488.1.17",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100047488.1.18",
          "form": "종류도요?",
          "original_form": "종류도요?",
          "speaker_id": "1",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100047488.1.19",
          "form": "네~맛집이야기 하니까 맛난거 먹고싶어서요",
          "original_form": "네~맛집이야기 하니까 맛난거 먹고싶어서요",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100047488.1.20",
          "form": "맛집이",
          "original_form": "맛집이",
          "speaker_id": "1",
          "time": "20210827 01:08"
        },
        {
