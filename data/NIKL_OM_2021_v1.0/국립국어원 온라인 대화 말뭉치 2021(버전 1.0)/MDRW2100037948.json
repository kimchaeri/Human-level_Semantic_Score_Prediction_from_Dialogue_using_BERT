      "utterance": [
        {
          "id": "MDRW2100037948.1.1",
          "form": "인턴이 김향기인가보다",
          "original_form": "인턴이 김향기인가보다",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037948.1.2",
          "form": "아 김향기 귀엽자나요",
          "original_form": "아 김향기 귀엽자나요",
          "speaker_id": "1",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037948.1.3",
          "form": "프로디자이너가 최다니엘 오윤아",
          "original_form": "프로디자이너가 최다니엘 오윤아",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037948.1.4",
          "form": "ㅋㅋㅋ 최다니엘 어색함",
          "original_form": "ㅋㅋㅋ 최다니엘 어색함",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100037948.1.5",
          "form": "ㅋㅋ그러게 김향기 은근슬쩍 활동이 많았네",
          "original_form": "ㅋㅋ그러게 김향기 은근슬쩍 활동이 많았네",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100037948.1.6",
          "form": "요샌 잘 안 나왔던거 같아요",
          "original_form": "요샌 잘 안 나왔던거 같아요",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100037948.1.7",
          "form": "아이, 승리호 찍었는데 오랜만에 보는거 같다",
          "original_form": "아이, 승리호 찍었는데 오랜만에 보는거 같다",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100037948.1.8",
          "form": "승리호에 김향기 나왔어요?",
          "original_form": "승리호에 김향기 나왔어요?",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037948.1.9",
          "form": "그리고 소문이 무성한 너를 닮은 사람 드라마도 있고",
          "original_form": "그리고 소문이 무성한 너를 닮은 사람 드라마도 있고",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037948.1.10",
          "form": "소문이 무성한ㅋㅋ 왜요?",
          "original_form": "소문이 무성한ㅋㅋ 왜요?",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037948.1.11",
          "form": "엉 승리호에서 업둥이 역이었다네 난 안봄",
          "original_form": "엉 승리호에서 업둥이 역이었다네 난 안봄",
          "speaker_id": "2",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100037948.1.12",
          "form": "넷플릭스가 없어 저도 못 봤어요",
          "original_form": "넷플릭스가 없어 저도 못 봤어요",
          "speaker_id": "1",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037948.1.13",
          "form": "고현정이 나오거든",
          "original_form": "고현정이 나오거든",
          "speaker_id": "2",
          "time": "20210901 21:28"
        }
