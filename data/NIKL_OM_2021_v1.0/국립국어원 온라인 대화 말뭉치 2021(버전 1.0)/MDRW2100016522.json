      "utterance": [
        {
          "id": "MDRW2100016522.1.1",
          "form": "넷플 보시나요",
          "original_form": "넷플 보시나요",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016522.1.2",
          "form": "예전에 몇번 봤는데 지금은 안보네요",
          "original_form": "예전에 몇번 봤는데 지금은 안보네요",
          "speaker_id": "2",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100016522.1.3",
          "form": "은근 볼거 없지 않아요?",
          "original_form": "은근 볼거 없지 않아요?",
          "speaker_id": "2",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100016522.1.4",
          "form": "음 맞아요ㅜ 유튭은보시나요",
          "original_form": "음 맞아요ㅜ 유튭은보시나요",
          "speaker_id": "1",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100016522.1.5",
          "form": "유툽은 자주봐요 모르는거 검색할때도 보구요ㅎ",
          "original_form": "유툽은 자주봐요 모르는거 검색할때도 보구요ㅎ",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016522.1.6",
          "form": "저는 여행유튭좋아해요",
          "original_form": "저는 여행유튭좋아해요",
          "speaker_id": "1",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016522.1.7",
          "form": "아 누구꺼 즐겨보세요?",
          "original_form": "아 누구꺼 즐겨보세요?",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016522.1.8",
          "form": "곽튜브요!",
          "original_form": "곽튜브요!",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016522.1.9",
          "form": "해외여행인가요?",
          "original_form": "해외여행인가요?",
          "speaker_id": "2",
          "time": "20210826 17:16"
        },
        {
          "id": "MDRW2100016522.1.10",
          "form": "네네 지금 러시아에요",
          "original_form": "네네 지금 러시아에요",
          "speaker_id": "1",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100016522.1.11",
          "form": "아 지금갔어요?",
          "original_form": "아 지금갔어요?",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100016522.1.12",
          "form": "네네 백신맞고가더라구요",
          "original_form": "네네 백신맞고가더라구요",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100016522.1.13",
          "form": "아 부럽네요 백신맞으면 해외갈수 있군요",
          "original_form": "아 부럽네요 백신맞으면 해외갈수 있군요",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100016522.1.14",
          "form": "그런가바요ㅠ",
          "original_form": "그런가바요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:39"
        }
