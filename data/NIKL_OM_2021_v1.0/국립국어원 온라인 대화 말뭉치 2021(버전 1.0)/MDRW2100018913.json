      "utterance": [
        {
          "id": "MDRW2100018913.1.1",
          "form": "코로나로 여행도 모임도 힘들어요",
          "original_form": "코로나로 여행도 모임도 힘들어요",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018913.1.2",
          "form": "그나마 드라이브 하네요",
          "original_form": "그나마 드라이브 하네요",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018913.1.3",
          "form": "주말에 어디 가세요?",
          "original_form": "주말에 어디 가세요?",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018913.1.4",
          "form": "산과 절이 많으시니 좋으시겠어요",
          "original_form": "산과 절이 많으시니 좋으시겠어요",
          "speaker_id": "1",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018913.1.5",
          "form": "특산품도 버섯?ㅋㅋ",
          "original_form": "특산품도 버섯?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018913.1.6",
          "form": "그렇죠 지방이라",
          "original_form": "그렇죠 지방이라",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018913.1.7",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018913.1.8",
          "form": "쌀",
          "original_form": "쌀",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018913.1.9",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018913.1.10",
          "form": "김해평야!! 호남평야!! 기억나요",
          "original_form": "김해평야!! 호남평야!! 기억나요",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018913.1.11",
          "form": "아 ㅎㅎ",
          "original_form": "아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018913.1.12",
          "form": "그런데 품질이 좋으면 비싸더라고요",
          "original_form": "그런데 품질이 좋으면 비싸더라고요",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018913.1.13",
          "form": "여긴 뭐 그정도 유명하진 않아요",
          "original_form": "여긴 뭐 그정도 유명하진 않아요",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018913.1.14",
          "form": "이천쌀도 있구나",
          "original_form": "이천쌀도 있구나",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018913.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018913.1.16",
          "form": "비싸죠 ㅎㅎ",
          "original_form": "비싸죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018913.1.17",
          "form": "회도 서울이 제일 싸대요ㅋㅋ",
          "original_form": "회도 서울이 제일 싸대요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018913.1.18",
          "form": "주말에 계획 없으세요?",
          "original_form": "주말에 계획 없으세요?",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018913.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018913.1.20",
          "form": "지방 헉 회 진짜 비싸료",
          "original_form": "지방 헉 회 진짜 비싸료",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018913.1.21",
          "form": "어디론가 가고픈데",
          "original_form": "어디론가 가고픈데",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018913.1.22",
          "form": "추천좀 해주세요",
          "original_form": "추천좀 해주세요",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018913.1.23",
          "form": "특히 바닷가 ㅡㅡ",
          "original_form": "특히 바닷가 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018913.1.24",
          "form": "저도 요새 드라이브 밖에 안해서 달리",
          "original_form": "저도 요새 드라이브 밖에 안해서 달리",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018913.1.25",
          "form": "추천도 뭣도 없네요",
          "original_form": "추천도 뭣도 없네요",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018913.1.26",
          "form": "절밑에서 산채 정식만 먹고 ㅋㅋ",
