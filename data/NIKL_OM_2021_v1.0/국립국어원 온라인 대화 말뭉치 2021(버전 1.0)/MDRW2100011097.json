      "utterance": [
        {
          "id": "MDRW2100011097.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 00:54"
        },
        {
          "id": "MDRW2100011097.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011097.1.3",
          "form": "계시는 곳도 비 많이 왔나요?",
          "original_form": "계시는 곳도 비 많이 왔나요?",
          "speaker_id": "2",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100011097.1.4",
          "form": "서울이라 비만조금오고 괜찮네요!",
          "original_form": "서울이라 비만조금오고 괜찮네요!",
          "speaker_id": "1",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100011097.1.5",
          "form": "아 저희 동네도요!",
          "original_form": "아 저희 동네도요!",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100011097.1.6",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100011097.1.7",
          "form": "봄 가을 좋아해요!",
          "original_form": "봄 가을 좋아해요!",
          "speaker_id": "1",
          "time": "20210824 11:29"
        },
        {
          "id": "MDRW2100011097.1.8",
          "form": "아 그러시구나 전 여름 좋아하는데 올여름은 좀 많이 덥더라고요",
          "original_form": "아 그러시구나 전 여름 좋아하는데 올여름은 좀 많이 덥더라고요",
          "speaker_id": "2",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100011097.1.9",
          "form": "네 진짜너무더웠어요",
          "original_form": "네 진짜너무더웠어요",
          "speaker_id": "1",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100011097.1.10",
          "form": "그쵸.. 올여름 정말 너무 힘들었네요 ㅋㅋㅋ",
          "original_form": "그쵸.. 올여름 정말 너무 힘들었네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011097.1.11",
          "form": "가을도 좋은데 참 짧아서 아쉬운거 같아요",
          "original_form": "가을도 좋은데 참 짧아서 아쉬운거 같아요",
          "speaker_id": "2",
          "time": "20210824 11:33"
        },
        {
          "id": "MDRW2100011097.1.12",
          "form": "마자요 거의 바로 겨울 되버리니까ㅠ",
          "original_form": "마자요 거의 바로 겨울 되버리니까ㅠ",
          "speaker_id": "1",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100011097.1.13",
          "form": "그쵸 ㅠㅠ 가을옷 사면 겨울 오는 느낌 ㅋㅋ",
          "original_form": "그쵸 ㅠㅠ 가을옷 사면 겨울 오는 느낌 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100011097.1.14",
          "form": "그래서 가을옷 사기 아까워요ㅎㅎ",
          "original_form": "그래서 가을옷 사기 아까워요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 11:53"
        },
        {
          "id": "MDRW2100011097.1.15",
          "form": "벌써좀 서늘하네요",
          "original_form": "벌써좀 서늘하네요",
          "speaker_id": "1",
          "time": "20210824 11:53"
        },
        {
          "id": "MDRW2100011097.1.16",
          "form": "그러게요 날씨가 애매한거 같아요 쌀쌀한데 습도는 또 높고 ㅎㅎ",
          "original_form": "그러게요 날씨가 애매한거 같아요 쌀쌀한데 습도는 또 높고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:15"
        },
        {
          "id": "MDRW2100011097.1.17",
          "form": "아 맞아요.. 살짝 찝찝하긴하네요",
          "original_form": "아 맞아요.. 살짝 찝찝하긴하네요",
          "speaker_id": "1",
          "time": "20210824 12:23"
        }
      ]
