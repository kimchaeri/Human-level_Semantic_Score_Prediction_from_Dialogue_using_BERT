        {
          "id": "MDRW2100019536.1.1",
          "form": "나는 돈까스를 좋아해",
          "original_form": "나는 돈까스를 좋아해",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019536.1.2",
          "form": "나도 오늘 돈까스 먹었어",
          "original_form": "나도 오늘 돈까스 먹었어",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019536.1.3",
          "form": "어떤 돈까스?",
          "original_form": "어떤 돈까스?",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019536.1.4",
          "form": "사보텐 알아?",
          "original_form": "사보텐 알아?",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019536.1.5",
          "form": "알아 거기 맛있지",
          "original_form": "알아 거기 맛있지",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019536.1.6",
          "form": "거기서 치즈 돈까스 먹었어",
          "original_form": "거기서 치즈 돈까스 먹었어",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019536.1.7",
          "form": "난 치즈 돈까스는 별로야",
          "original_form": "난 치즈 돈까스는 별로야",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019536.1.8",
          "form": "어떤 점이?",
          "original_form": "어떤 점이?",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019536.1.9",
          "form": "치즈가 굳잖아",
          "original_form": "치즈가 굳잖아",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019536.1.10",
          "form": "난 돈까스 나베도 조하",
          "original_form": "난 돈까스 나베도 조하",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100019536.1.11",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100019536.1.12",
          "form": "나도 그건 무척 좋아해",
          "original_form": "나도 그건 무척 좋아해",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100019536.1.13",
          "form": "난 돈까스는 늘 옳아",
          "original_form": "난 돈까스는 늘 옳아",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019536.1.14",
          "form": "매일 먹을 수 있어?",
          "original_form": "매일 먹을 수 있어?",
          "speaker_id": "2",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019536.1.15",
          "form": "그건 좀 그래",
          "original_form": "그건 좀 그래",
