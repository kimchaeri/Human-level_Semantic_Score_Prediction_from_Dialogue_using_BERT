      "utterance": [
        {
          "id": "MDRW2100042858.1.1",
          "form": "잘려면 가슴이두근거려",
          "original_form": "잘려면 가슴이두근거려",
          "speaker_id": "2",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100042858.1.2",
          "form": "금방억지로일어났다\r\n밤새가위눌리고 지금도 가위눌리고",
          "original_form": "금방억지로일어났다\r\n밤새가위눌리고 지금도 가위눌리고",
          "speaker_id": "2",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100042858.1.3",
          "form": "맞나??그래도 모르니 검사받는게ㅜ",
          "original_form": "맞나??그래도 모르니 검사받는게ㅜ",
          "speaker_id": "1",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100042858.1.4",
          "form": "나도 몸이 무겁긴하더라ㅜ",
          "original_form": "나도 몸이 무겁긴하더라ㅜ",
          "speaker_id": "1",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100042858.1.5",
          "form": "독한 감기약먹으면...약에취하잖아 그런기분",
          "original_form": "독한 감기약먹으면...약에취하잖아 그런기분",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100042858.1.6",
          "form": "응 나도 그랬다",
          "original_form": "응 나도 그랬다",
          "speaker_id": "1",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100042858.1.7",
          "form": "그 기분이 좀 오래간데ㅜ",
          "original_form": "그 기분이 좀 오래간데ㅜ",
          "speaker_id": "1",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100042858.1.8",
          "form": "딱히 아픈건아니고ㅜ",
          "original_form": "딱히 아픈건아니고ㅜ",
          "speaker_id": "1",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100042858.1.9",
          "form": "ㅇㅇ열은없는데",
          "original_form": "ㅇㅇ열은없는데",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100042858.1.10",
          "form": "정신을못차리겠네",
          "original_form": "정신을못차리겠네",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100042858.1.11",
          "form": "나중에 식은땀도 나고",
          "original_form": "나중에 식은땀도 나고",
          "speaker_id": "1",
          "time": "20210907 10:15"
        },
        {
          "id": "MDRW2100042858.1.12",
          "form": "갑자기 오후되면 힘빠진다ㅜ번아웃처럼",
          "original_form": "갑자기 오후되면 힘빠진다ㅜ번아웃처럼",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100042858.1.13",
          "form": "그래서 name3이가 이렇게 아픈거구나싶더라ㅜ",
          "original_form": "그래서 &name3&이가 이렇게 아픈거구나싶더라ㅜ",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100042858.1.14",
          "form": "우울증오면 그렇나",
          "original_form": "우울증오면 그렇나",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100042858.1.15",
          "form": "내몸의 스위치가 오프된기분",
          "original_form": "내몸의 스위치가 오프된기분",
          "speaker_id": "2",
          "time": "20210907 10:17"
        },
        {
          "id": "MDRW2100042858.1.16",
          "form": "name3이 갑자기 힘빠지잖아 그게 번아웃",
          "original_form": "&name3&이 갑자기 힘빠지잖아 그게 번아웃",
          "speaker_id": "1",
          "time": "20210907 10:17"
        },
        {
          "id": "MDRW2100042858.1.17",
          "form": "우울증도 그렇지 그래서 온 몸에 힘빠지고ㅜ",
          "original_form": "우울증도 그렇지 그래서 온 몸에 힘빠지고ㅜ",
          "speaker_id": "1",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100042858.1.18",
          "form": "쉬는방법밖에 없다ㅜ",
          "original_form": "쉬는방법밖에 없다ㅜ",
          "speaker_id": "1",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100042858.1.19",
          "form": "ㅇㅇ37.2네",
          "original_form": "ㅇㅇ37.2네",
          "speaker_id": "2",
          "time": "20210907 11:38"
        },
        {
          "id": "MDRW2100042858.1.20",
          "form": "나도 37.5는 안 넘어가고 2~4 왔다갔다하더라",
          "original_form": "나도 37.5는 안 넘어가고 2~4 왔다갔다하더라",
          "speaker_id": "1",
          "time": "20210907 11:54"
        },
        {
          "id": "MDRW2100042858.1.21",
          "form": "병원가기도 이상하고ㅜ그래서 내가 계속 몸 안 좋다고 한 것임ㅜ",
          "original_form": "병원가기도 이상하고ㅜ그래서 내가 계속 몸 안 좋다고 한 것임ㅜ",
          "speaker_id": "1",
          "time": "20210907 11:54"
        },
        {
          "id": "MDRW2100042858.1.22",
          "form": "ㅇㅇ식은땀난다",
          "original_form": "ㅇㅇ식은땀난다",
