        },
        {
          "id": "MDRW2100007965.1.6",
          "form": "높아요",
          "original_form": "높아요",
          "speaker_id": "2",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100007965.1.7",
          "form": "장마라 또 비올 수도 있겠어요",
          "original_form": "장마라 또 비올 수도 있겠어요",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100007965.1.8",
          "form": "비 좋아하세요?",
