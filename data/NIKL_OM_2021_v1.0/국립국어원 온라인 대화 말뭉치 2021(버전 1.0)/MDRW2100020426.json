      "utterance": [
        {
          "id": "MDRW2100020426.1.1",
          "form": "안녕하세용 어제 뭐 드셨어요~?",
          "original_form": "안녕하세용 어제 뭐 드셨어요~?",
          "speaker_id": "1",
          "time": "20210828 05:11"
        },
        {
          "id": "MDRW2100020426.1.2",
          "form": "카레 먹었어요~님은요?",
          "original_form": "카레 먹었어요~님은요?",
          "speaker_id": "2",
          "time": "20210828 05:11"
        },
        {
          "id": "MDRW2100020426.1.3",
          "form": "저는 비가 와서 짬뽕 먹었어요~ ㅎㅎ",
          "original_form": "저는 비가 와서 짬뽕 먹었어요~ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 05:12"
        },
        {
          "id": "MDRW2100020426.1.4",
          "form": "무슨 카레 드셨어요?",
          "original_form": "무슨 카레 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 05:12"
        },
        {
          "id": "MDRW2100020426.1.5",
          "form": "짬뽕 딱이네요~그냥 일반카레요",
          "original_form": "짬뽕 딱이네요~그냥 일반카레요",
          "speaker_id": "2",
          "time": "20210828 05:12"
        },
        {
          "id": "MDRW2100020426.1.6",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 05:13"
        },
        {
          "id": "MDRW2100020426.1.7",
          "form": "아하 저는 카레 먹을때 초콜릿 한조각 넣어서 먹는데 맛있어요!",
          "original_form": "아하 저는 카레 먹을때 초콜릿 한조각 넣어서 먹는데 맛있어요!",
          "speaker_id": "1",
          "time": "20210828 05:13"
        },
        {
          "id": "MDRW2100020426.1.8",
          "form": "커피는 잘 못마시는데 카페가는거 좋아해요!",
          "original_form": "커피는 잘 못마시는데 카페가는거 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 05:13"
        },
        {
          "id": "MDRW2100020426.1.9",
          "form": "아 진짜요?초코렛은 생각 못했어요",
          "original_form": "아 진짜요?초코렛은 생각 못했어요",
          "speaker_id": "2",
          "time": "20210828 05:13"
        },
        {
          "id": "MDRW2100020426.1.10",
          "form": "넹넹ㅎㅎ 맛이 좀 더 깊어져요",
          "original_form": "넹넹ㅎㅎ 맛이 좀 더 깊어져요",
          "speaker_id": "1",
          "time": "20210828 05:13"
        },
        {
          "id": "MDRW2100020426.1.11",
          "form": "초콜릿맛은 하나도 안나요",
          "original_form": "초콜릿맛은 하나도 안나요",
          "speaker_id": "1",
          "time": "20210828 05:13"
        },
        {
          "id": "MDRW2100020426.1.12",
          "form": "저는 커피 못 마셔요ㅜ",
          "original_form": "저는 커피 못 마셔요ㅜ",
          "speaker_id": "2",
          "time": "20210828 05:13"
        },
        {
          "id": "MDRW2100020426.1.13",
          "form": "저랑 같으시네요ㅎㅎ",
          "original_form": "저랑 같으시네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 05:13"
        },
        {
          "id": "MDRW2100020426.1.14",
          "form": "아~~다음에 해봐야겠어요",
          "original_form": "아~~다음에 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 05:14"
        },
        {
          "id": "MDRW2100020426.1.15",
          "form": "대신 프라푸치노나 스무디 주로 먹어요",
          "original_form": "대신 프라푸치노나 스무디 주로 먹어요",
          "speaker_id": "1",
          "time": "20210828 05:14"
        },
        {
          "id": "MDRW2100020426.1.16",
          "form": "저도 그래요~전 자몽티요",
          "original_form": "저도 그래요~전 자몽티요",
