      "utterance": [
        {
          "id": "MDRW2100015959.1.1",
          "form": "안녕하세용 어제 뭐드셨어요?",
          "original_form": "안녕하세용 어제 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210826 03:54"
        },
        {
          "id": "MDRW2100015959.1.2",
          "form": "전 어제 저녁으로 떡볶이해먹었어요!",
          "original_form": "전 어제 저녁으로 떡볶이해먹었어요!",
          "speaker_id": "2",
          "time": "20210826 03:54"
        },
        {
          "id": "MDRW2100015959.1.3",
          "form": "만두 사는데 덤으로 딸려왔더라고용",
          "original_form": "만두 사는데 덤으로 딸려왔더라고용",
          "speaker_id": "2",
          "time": "20210826 03:54"
        },
        {
          "id": "MDRW2100015959.1.4",
          "form": "저는 파리바게트 요기요 행사하길래",
          "original_form": "저는 파리바게트 요기요 행사하길래",
          "speaker_id": "1",
          "time": "20210826 03:55"
        },
        {
          "id": "MDRW2100015959.1.5",
          "form": "빵 먹었어요!",
          "original_form": "빵 먹었어요!",
          "speaker_id": "1",
          "time": "20210826 03:55"
        },
        {
          "id": "MDRW2100015959.1.6",
          "form": "아~~ 저도 그거 열심히 노리고 있었는데",
          "original_form": "아~~ 저도 그거 열심히 노리고 있었는데",
          "speaker_id": "2",
          "time": "20210826 03:55"
        },
        {
          "id": "MDRW2100015959.1.7",
          "form": "어제는 저녁에 맛있는 빵이 없어서 포기하구..",
          "original_form": "어제는 저녁에 맛있는 빵이 없어서 포기하구..",
          "speaker_id": "2",
          "time": "20210826 03:55"
        },
        {
          "id": "MDRW2100015959.1.8",
          "form": "오늘은 야식쿠폰이 만오천원부터로 바꼈잖아용 ㅠㅠ",
          "original_form": "오늘은 야식쿠폰이 만오천원부터로 바꼈잖아용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 03:56"
        },
        {
          "id": "MDRW2100015959.1.9",
          "form": "그래서 내일 점심에 사려고요 ㅎㅎ",
          "original_form": "그래서 내일 점심에 사려고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 03:56"
        },
        {
          "id": "MDRW2100015959.1.10",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "1",
          "time": "20210826 03:56"
        },
        {
          "id": "MDRW2100015959.1.11",
          "form": "완전 잘 알고 계시네용",
          "original_form": "완전 잘 알고 계시네용",
          "speaker_id": "1",
          "time": "20210826 03:56"
        },
        {
          "id": "MDRW2100015959.1.12",
          "form": "무슨 빵이 맛있을까요??",
          "original_form": "무슨 빵이 맛있을까요??",
          "speaker_id": "2",
          "time": "20210826 03:56"
        },
        {
          "id": "MDRW2100015959.1.13",
          "form": "맞아욬ㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 03:56"
        },
        {
          "id": "MDRW2100015959.1.14",
          "form": "그 파바는",
          "original_form": "그 파바는",
          "speaker_id": "1",
          "time": "20210826 03:56"
        },
        {
          "id": "MDRW2100015959.1.15",
          "form": "추천해주세용!!",
          "original_form": "추천해주세용!!",
          "speaker_id": "2",
          "time": "20210826 03:56"
        },
        {
          "id": "MDRW2100015959.1.16",
          "form": "장발장 빵이라고해서",
          "original_form": "장발장 빵이라고해서",
          "speaker_id": "1",
          "time": "20210826 03:56"
        },
        {
          "id": "MDRW2100015959.1.17",
          "form": "토종효모빵이랑",
          "original_form": "토종효모빵이랑",
          "speaker_id": "1",
          "time": "20210826 03:57"
        },
        {
          "id": "MDRW2100015959.1.18",
          "form": "꿀토스트가 유명하더라구요",
          "original_form": "꿀토스트가 유명하더라구요",
          "speaker_id": "1",
          "time": "20210826 03:57"
        },
        {
          "id": "MDRW2100015959.1.19",
          "form": "오오오 있으면 그 두개 꼭 먼저 담아야겠어요!!!",
          "original_form": "오오오 있으면 그 두개 꼭 먼저 담아야겠어요!!!",
          "speaker_id": "2",
          "time": "20210826 03:58"
        },
        {
          "id": "MDRW2100015959.1.20",
          "form": "꿀팁 얻어가네용ㅎㅎ",
          "original_form": "꿀팁 얻어가네용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 03:58"
        },
        {
          "id": "MDRW2100015959.1.21",
          "form": "둘다 슴슴한 맛이라 조리빵 위주로 좋아하시면",
          "original_form": "둘다 슴슴한 맛이라 조리빵 위주로 좋아하시면",
          "speaker_id": "1",
          "time": "20210826 03:58"
        },
        {
          "id": "MDRW2100015959.1.22",
          "form": "다른거 추천드려요",
          "original_form": "다른거 추천드려요",
          "speaker_id": "1",
          "time": "20210826 03:58"
        },
        {
          "id": "MDRW2100015959.1.23",
          "form": "그치만",
          "original_form": "그치만",
          "speaker_id": "1",
          "time": "20210826 03:58"
        },
        {
          "id": "MDRW2100015959.1.24",
          "form": "꼭 한번은 드셔보세요!ㅎㅎ",
          "original_form": "꼭 한번은 드셔보세요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 03:58"
        },
        {
          "id": "MDRW2100015959.1.25",
          "form": "담백하고 슴슴한 빵이 또 물리지 않는 매력이 있죠 ㅎㅎ",
          "original_form": "담백하고 슴슴한 빵이 또 물리지 않는 매력이 있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 03:59"
        },
        {
          "id": "MDRW2100015959.1.26",
          "form": "둘 다 이름만 들어도 맛있어보여요~~",
          "original_form": "둘 다 이름만 들어도 맛있어보여요~~",
          "speaker_id": "2",
          "time": "20210826 03:59"
        },
        {
          "id": "MDRW2100015959.1.27",
          "form": "ㅎㅎ 이번주 내내 하니까",
          "original_form": "ㅎㅎ 이번주 내내 하니까",
          "speaker_id": "1",
          "time": "20210826 03:59"
        },
        {
          "id": "MDRW2100015959.1.28",
          "form": "달리려구요!",
          "original_form": "달리려구요!",
          "speaker_id": "1",
          "time": "20210826 03:59"
        },
        {
          "id": "MDRW2100015959.1.29",
          "form": "저는 파리바게트 가면 빵은 안먹고 항상 샐러드만 사먹었던것같아요 ㅋㅋㅋㅋ",
          "original_form": "저는 파리바게트 가면 빵은 안먹고 항상 샐러드만 사먹었던것같아요 ㅋㅋㅋㅋ",
