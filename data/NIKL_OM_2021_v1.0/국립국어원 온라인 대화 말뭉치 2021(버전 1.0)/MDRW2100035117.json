      "utterance": [
        {
          "id": "MDRW2100035117.1.1",
          "form": "구렇겟죠!",
          "original_form": "구렇겟죠!",
          "speaker_id": "2",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.2",
          "form": "맛잇겟다ㅠㅠ",
          "original_form": "맛잇겟다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.3",
          "form": "그것도 엄마가 사오셨는데",
          "original_form": "그것도 엄마가 사오셨는데",
          "speaker_id": "1",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.4",
          "form": "마라탕도드세요!?",
          "original_form": "마라탕도드세요!?",
          "speaker_id": "2",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.5",
          "form": "너무 맛있어섴ㅋㅋㅋ",
          "original_form": "너무 맛있어섴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.6",
          "form": "네네! 먹어요",
          "original_form": "네네! 먹어요",
          "speaker_id": "1",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.7",
          "form": "와 ㅋㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.8",
          "form": "그냥라면처럼끓이는데 그맛이라니",
          "original_form": "그냥라면처럼끓이는데 그맛이라니",
          "speaker_id": "2",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.9",
          "form": "마라탕 넘 맛있는데 위생 문제 터져서 슬펐어요ㅠㅠ",
          "original_form": "마라탕 넘 맛있는데 위생 문제 터져서 슬펐어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.10",
          "form": "미고랭도 보면 안에 소스가 작게 3가지인가잇고",
          "original_form": "미고랭도 보면 안에 소스가 작게 3가지인가잇고",
          "speaker_id": "2",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.11",
          "form": "그래요??",
          "original_form": "그래요??",
          "speaker_id": "2",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.12",
          "form": "전 몰랐네요 ㅠㅠㅠㅠ",
          "original_form": "전 몰랐네요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.13",
          "form": "제가 먹은건 두가지였던것같아용",
          "original_form": "제가 먹은건 두가지였던것같아용",
          "speaker_id": "1",
          "time": "20210829 03:09"
        },
        {
          "id": "MDRW2100035117.1.14",
          "form": "전 간편화해서 나온건지",
          "original_form": "전 간편화해서 나온건지",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100035117.1.15",
          "form": "태국 가고싶네요ㅠㅠ",
          "original_form": "태국 가고싶네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100035117.1.16",
          "form": "태국엔 맛있는게 너무 많아요",
          "original_form": "태국엔 맛있는게 너무 많아요",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100035117.1.17",
          "form": "맞아요ㅠ 길거리에서",
          "original_form": "맞아요ㅠ 길거리에서",
          "speaker_id": "2",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100035117.1.18",
          "form": "물가도 저렴하고ㅋㅋㅋ",
          "original_form": "물가도 저렴하고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100035117.1.19",
          "form": "몇백원에 먹는 팟타이도 너무맛잇고ㅠ",
          "original_form": "몇백원에 먹는 팟타이도 너무맛잇고ㅠ",
          "speaker_id": "2",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100035117.1.20",
          "form": "진짜 한국에선 타이음식왜케비싼지ㅠㅠ",
          "original_form": "진짜 한국에선 타이음식왜케비싼지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100035117.1.21",
          "form": "아아 팟타이... 맛있겠다",
          "original_form": "아아 팟타이... 맛있겠다",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100035117.1.22",
          "form": "와아",
          "original_form": "와아",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100035117.1.23",
          "form": "해산물 듬뿍 들어간 요리도 맛있고",
          "original_form": "해산물 듬뿍 들어간 요리도 맛있고",
          "speaker_id": "1",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100035117.1.24",
          "form": "맞아여ㅠㅠ",
          "original_form": "맞아여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:10"
        },
        {
          "id": "MDRW2100035117.1.25",
          "form": "태국은 언제 가셨어용?",
          "original_form": "태국은 언제 가셨어용?",
          "speaker_id": "1",
          "time": "20210829 03:11"
        },
        {
          "id": "MDRW2100035117.1.26",
          "form": "좀오래되써요",
          "original_form": "좀오래되써요",
          "speaker_id": "2",
          "time": "20210829 03:11"
        },
        {
          "id": "MDRW2100035117.1.27",
          "form": "6년?",
          "original_form": "6년?",
          "speaker_id": "2",
          "time": "20210829 03:11"
        },
        {
          "id": "MDRW2100035117.1.28",
          "form": "오호 그래도 그렇게 오래되진 않으셨네용!",
          "original_form": "오호 그래도 그렇게 오래되진 않으셨네용!",
          "speaker_id": "1",
          "time": "20210829 03:11"
        },
        {
          "id": "MDRW2100035117.1.29",
          "form": "마지막여행이4년전이니ㅠㅠ",
          "original_form": "마지막여행이4년전이니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:11"
        },
        {
          "id": "MDRW2100035117.1.30",
          "form": "아아ㅠㅠ",
          "original_form": "아아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 03:11"
        },
        {
          "id": "MDRW2100035117.1.31",
          "form": "코로나가 빨리 없어져서",
          "original_form": "코로나가 빨리 없어져서",
          "speaker_id": "1",
          "time": "20210829 03:11"
        },
        {
          "id": "MDRW2100035117.1.32",
          "form": "여행다니고싶어요",
          "original_form": "여행다니고싶어요",
          "speaker_id": "1",
          "time": "20210829 03:11"
        }
      ]
    }
  ]
