      "utterance": [
        {
          "id": "MDRW2100007787.1.1",
          "form": "제동생이 파워블로그에요 ㅎ",
          "original_form": "제동생이 파워블로그에요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100007787.1.2",
          "form": "앗 진짜요?!! 대단하시네요~",
          "original_form": "앗 진짜요?!! 대단하시네요~",
          "speaker_id": "2",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100007787.1.3",
          "form": "이번 올림픽 경기 보셨나요?",
          "original_form": "이번 올림픽 경기 보셨나요?",
          "speaker_id": "2",
          "time": "20210822 10:46"
        },
        {
          "id": "MDRW2100007787.1.4",
          "form": "네 이번에 배구만 봤어요",
          "original_form": "네 이번에 배구만 봤어요",
          "speaker_id": "1",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007787.1.5",
          "form": "진짜 대단했지요",
          "original_form": "진짜 대단했지요",
          "speaker_id": "1",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007787.1.6",
          "form": "네 여자배구!!",
          "original_form": "네 여자배구!!",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007787.1.7",
          "form": "정말 열심히 응원했어요ㅠㅠ",
          "original_form": "정말 열심히 응원했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007787.1.8",
          "form": "선수들도 최선을 다하는 게 보이니까",
          "original_form": "선수들도 최선을 다하는 게 보이니까",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007787.1.9",
          "form": "진짜 배구 멋졌어요",
          "original_form": "진짜 배구 멋졌어요",
          "speaker_id": "1",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007787.1.10",
          "form": "네네 5세트까지 가서 역전도 하고",
          "original_form": "네네 5세트까지 가서 역전도 하고",
          "speaker_id": "2",
          "time": "20210822 10:56"
        },
        {
          "id": "MDRW2100007787.1.11",
          "form": "진짜 손에 땀을 쥐고 봣어요 ㅎ",
          "original_form": "진짜 손에 땀을 쥐고 봣어요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 10:56"
        },
        {
          "id": "MDRW2100007787.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007787.1.13",
          "form": "한일전이랑 8강이랑",
          "original_form": "한일전이랑 8강이랑",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007787.1.14",
          "form": "다 5세트까지 접전이라",
          "original_form": "다 5세트까지 접전이라",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007787.1.15",
          "form": "막 제가 경기하는 것처럼 온몸에 힘을 주고 응원했네요",
          "original_form": "막 제가 경기하는 것처럼 온몸에 힘을 주고 응원했네요",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007787.1.16",
          "form": "다음을 기약해야겟죵",
          "original_form": "다음을 기약해야겟죵",
          "speaker_id": "1",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100007787.1.17",
          "form": "네 그래도 선수들이 열심히 해서 응원할 맛 났어요!!",
          "original_form": "네 그래도 선수들이 열심히 해서 응원할 맛 났어요!!",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100007787.1.18",
          "form": "평소에도 스포츠경기 자주 보세요?",
          "original_form": "평소에도 스포츠경기 자주 보세요?",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100007787.1.19",
          "form": "예전에 축구 광팬이었는데 지금은 잘 안보게되더라고요",
          "original_form": "예전에 축구 광팬이었는데 지금은 잘 안보게되더라고요",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100007787.1.20",
          "form": "야구도 직접 관람해야 재밌는데 너무 아쉬워요",
          "original_form": "야구도 직접 관람해야 재밌는데 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100007787.1.21",
          "form": "오 저도 야구관람하러 다녔는데",
          "original_form": "오 저도 야구관람하러 다녔는데",
          "speaker_id": "2",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100007787.1.22",
          "form": "야구장에서 응원하고 치맥도 하고~ 좋았는데 말이죠",
          "original_form": "야구장에서 응원하고 치맥도 하고~ 좋았는데 말이죠",
          "speaker_id": "2",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100007787.1.23",
          "form": "그때가 그리워요 ㅜ",
          "original_form": "그때가 그리워요 ㅜ",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100007787.1.24",
          "form": "야구는 가서 봐야 제맛이자니요 ㅜ ㅋ",
          "original_form": "야구는 가서 봐야 제맛이자니요 ㅜ ㅋ",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100007787.1.25",
          "form": "네네 스포츠 경기 보러가고 응원하던 때가 그리워요",
          "original_form": "네네 스포츠 경기 보러가고 응원하던 때가 그리워요",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100007787.1.26",
          "form": "맞아요 경기장에서 느껴지는 열기가 있죠!!",
          "original_form": "맞아요 경기장에서 느껴지는 열기가 있죠!!",
          "speaker_id": "2",
          "time": "20210824 23:49"
        }
      ]
    }
