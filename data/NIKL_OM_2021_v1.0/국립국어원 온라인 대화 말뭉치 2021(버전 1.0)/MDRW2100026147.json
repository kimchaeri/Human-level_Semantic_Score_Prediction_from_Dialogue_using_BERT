      "utterance": [
        {
          "id": "MDRW2100026147.1.1",
          "form": "여행가고싶다..",
          "original_form": "여행가고싶다..",
          "speaker_id": "2",
          "time": "20210901 20:07"
        },
        {
          "id": "MDRW2100026147.1.2",
          "form": "요즘 코로나때문에 여행못가니까 책으로라도 경험하기..ㅎ",
          "original_form": "요즘 코로나때문에 여행못가니까 책으로라도 경험하기..ㅎ",
          "speaker_id": "2",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026147.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄲㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄲㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100026147.1.4",
          "form": "여행책보기",
          "original_form": "여행책보기",
          "speaker_id": "1",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100026147.1.5",
          "form": "추리소설에서 좋아하는책 있어요?",
          "original_form": "추리소설에서 좋아하는책 있어요?",
          "speaker_id": "2",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100026147.1.6",
          "form": "셜록홈즈여",
          "original_form": "셜록홈즈여",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026147.1.7",
          "form": "셜록홈즈 시리즈 엄청 많지 않아요?",
          "original_form": "셜록홈즈 시리즈 엄청 많지 않아요?",
          "speaker_id": "2",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026147.1.8",
          "form": "아가사 크리스티도...",
          "original_form": "아가사 크리스티도...",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026147.1.9",
          "form": "책제목이 뭐에요",
          "original_form": "책제목이 뭐에요",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026147.1.10",
          "form": "ㅋㅋㅋㅋ저 머지",
          "original_form": "ㅋㅋㅋㅋ저 머지",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026147.1.11",
          "form": "바스커빌가의 개",
          "original_form": "바스커빌가의 개",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026147.1.12",
          "form": "조아함..",
          "original_form": "조아함..",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026147.1.13",
          "form": "숨길수없는 호러취향",
          "original_form": "숨길수없는 호러취향",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026147.1.14",
          "form": "오 전 셜록홈즈 한번도 본적없는데",
          "original_form": "오 전 셜록홈즈 한번도 본적없는데",
          "speaker_id": "2",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026147.1.15",
          "form": "나중에 한번봐야겠다ㅋㅋ",
          "original_form": "나중에 한번봐야겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026147.1.16",
          "form": "나중에 읽어바여",
          "original_form": "나중에 읽어바여",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026147.1.17",
          "form": "재밋음..",
          "original_form": "재밋음..",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026147.1.18",
          "form": "좋아욤 아무책이나 사도 내용은 다 같은거죠",
          "original_form": "좋아욤 아무책이나 사도 내용은 다 같은거죠",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026147.1.19",
          "form": "해리포터도 책으로보면 더재밋어 ㅎㅎ",
          "original_form": "해리포터도 책으로보면 더재밋어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026147.1.20",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026147.1.21",
          "form": "해리포터도 책으로 본적없어ㅋㅋㅋ",
          "original_form": "해리포터도 책으로 본적없어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026147.1.22",
          "form": "그것두 봐야겠다",
          "original_form": "그것두 봐야겠다",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026147.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ짱잼 ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ짱잼 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026147.1.24",
          "form": "저 초딩때",
          "original_form": "저 초딩때",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026147.1.25",
          "form": "책소장하고싶은데.. 이북살까 고민중",
          "original_form": "책소장하고싶은데.. 이북살까 고민중",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026147.1.26",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026147.1.27",
          "form": "엄청 좋아했던 책있는데",
          "original_form": "엄청 좋아했던 책있는데",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026147.1.28",
          "form": "머여??",
          "original_form": "머여??",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026147.1.29",
          "form": "모모 너무 감명깊게봐서",
          "original_form": "모모 너무 감명깊게봐서",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026147.1.30",
          "form": "아직도기억나",
          "original_form": "아직도기억나",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026147.1.31",
          "form": "모모?",
          "original_form": "모모?",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026147.1.32",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026147.1.33",
          "form": "모모 알아여?",
          "original_form": "모모 알아여?",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026147.1.34",
          "form": "아녀ㅠㅠ안밧음..",
          "original_form": "아녀ㅠㅠ안밧음..",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026147.1.35",
          "form": "저 추억에 젖어서 종이책도 샀어",
          "original_form": "저 추억에 젖어서 종이책도 샀어",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026147.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026147.1.37",
          "form": "저는 비밀의화원이랑 하이디",
          "original_form": "저는 비밀의화원이랑 하이디",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026147.1.38",
          "form": "초딩때 진짜 재밌게봣어",
          "original_form": "초딩때 진짜 재밌게봣어",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026147.1.39",
          "form": "짱좋아햇거",
          "original_form": "짱좋아햇거",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026147.1.40",
          "form": "오 나중에 봐야겟단",
          "original_form": "오 나중에 봐야겟단",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100026147.1.41",
          "form": "왜전혀 읽은기억이없ㅇㅠ",
          "original_form": "왜전혀 읽은기억이없ㅇㅠ",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100026147.1.42",
          "form": "네네 한번읽어봐 존잼",
          "original_form": "네네 한번읽어봐 존잼",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026147.1.43",
          "form": "비밀의화원 검색해보니까 영화나와..",
          "original_form": "비밀의화원 검색해보니까 영화나와..",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026147.1.44",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026147.1.45",
          "form": "고전이라...",
          "original_form": "고전이라...",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026147.1.46",
          "form": "책으로 검색해바",
          "original_form": "책으로 검색해바",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026147.1.47",
          "form": "아 좋아여ㅋㅋ",
