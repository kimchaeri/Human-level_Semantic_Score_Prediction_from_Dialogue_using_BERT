        },
        {
          "id": "MDRW2100043047.1.2",
          "form": "이전 답변 못 봄ㅎ",
          "original_form": "이전 답변 못 봄ㅎ",
          "speaker_id": "2",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100043047.1.3",
          "form": "딥빡 이라고 햇늠",
          "original_form": "딥빡 이라고 햇늠",
          "speaker_id": "1",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100043047.1.4",
          "form": "운동 하시는 거 있나요",
          "original_form": "운동 하시는 거 있나요",
          "speaker_id": "2",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100043047.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100043047.1.6",
          "form": "아니 ^클라이밍^ 때문에",
          "original_form": "아니 ^클라이밍^ 때문에",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043047.1.7",
          "form": "이렇게 빡칠수가 잇군요",
          "original_form": "이렇게 빡칠수가 잇군요",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043047.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043047.1.9",
          "form": "클라이밍...그게 뭐라고",
          "original_form": "클라이밍...그게 뭐라고",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043047.1.10",
          "form": "이렇게 사람을 화나게하죠",
          "original_form": "이렇게 사람을 화나게하죠",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100043047.1.11",
          "form": "그러게말입니더",
          "original_form": "그러게말입니더",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043047.1.12",
          "form": "전에 클라이밍땜에 빡쳤을 때 대화를 복기해야겟어요",
          "original_form": "전에 클라이밍땜에 빡쳤을 때 대화를 복기해야겟어요",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100043047.1.13",
          "form": "후",
          "original_form": "후",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043047.1.14",
          "form": "영원히 클라이밍을 안 할 것 같나요",
          "original_form": "영원히 클라이밍을 안 할 것 같나요",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100043047.1.15",
          "form": "네 개빡치거든요",
          "original_form": "네 개빡치거든요",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043047.1.16",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043047.1.17",
          "form": "파국인데요",
          "original_form": "파국인데요",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100043047.1.18",
          "form": "무슨일이야",
          "original_form": "무슨일이야",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100043047.1.19",
          "form": "ㅋㅋㅋㅋㅋ하....큰싸움각인데",
          "original_form": "ㅋㅋㅋㅋㅋ하....큰싸움각인데",
          "speaker_id": "1",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100043047.1.20",
          "form": "친구네 집간다는거 짜증난다고 하니까",
          "original_form": "친구네 집간다는거 짜증난다고 하니까",
          "speaker_id": "1",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043047.1.21",
          "form": "자기도 내가 짜증난다고 한게 짜증나는거래",
          "original_form": "자기도 내가 짜증난다고 한게 짜증나는거래",
          "speaker_id": "1",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043047.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:53"
        },
        {
          "id": "MDRW2100043047.1.23",
          "form": "ㅎ..",
          "original_form": "ㅎ..",
          "speaker_id": "2",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100043047.1.24",
          "form": "아니 하필 오늘 날도 안 좋고 화요일에..",
          "original_form": "아니 하필 오늘 날도 안 좋고 화요일에..",
          "speaker_id": "2",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100043047.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100043047.1.26",
          "form": "와 비진짜 마니온다",
          "original_form": "와 비진짜 마니온다",
          "speaker_id": "1",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100043047.1.27",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "1",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100043047.1.28",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "2",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043047.1.29",
          "form": "근데 왜 전화 은근 오냐",
          "original_form": "근데 왜 전화 은근 오냐",
          "speaker_id": "2",
          "time": "20210907 14:33"
        },
        {
          "id": "MDRW2100043047.1.30",
          "form": "아 쉬시라고요",
          "original_form": "아 쉬시라고요",
          "speaker_id": "2",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043047.1.31",
          "form": "여기 이모지 안보여 요",
          "original_form": "여기 이모지 안보여 요",
          "speaker_id": "1",
          "time": "20210907 14:34"
        },
        {
          "id": "MDRW2100043047.1.32",
