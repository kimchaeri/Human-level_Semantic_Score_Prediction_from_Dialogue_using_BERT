      "utterance": [
        {
          "id": "MDRW2100022254.1.1",
          "form": "오오 군대 이야기!!!",
          "original_form": "오오 군대 이야기!!!",
          "speaker_id": "1",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022254.1.2",
          "form": "앗 혹시 군대 다녀오셨나요??",
          "original_form": "앗 혹시 군대 다녀오셨나요??",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.3",
          "form": "저는 군대를 못 가봐서 ㅎㅎ",
          "original_form": "저는 군대를 못 가봐서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.4",
          "form": "아뇨아뇨 !!",
          "original_form": "아뇨아뇨 !!",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.5",
          "form": "저는 여자라서",
          "original_form": "저는 여자라서",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.6",
          "form": "군복무의 의무가 없답니다",
          "original_form": "군복무의 의무가 없답니다",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.7",
          "form": "name2은요?!",
          "original_form": "&name2&은요?!",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.8",
          "form": "저도 여자라서 의무가 없어요 ㅋㅋㅋㅋ",
          "original_form": "저도 여자라서 의무가 없어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.9",
          "form": "아주 좋네요,,",
          "original_form": "아주 좋네요,,",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.10",
          "form": "고무신도 안해봤답니다 ㅋㅋㅋㅋ",
          "original_form": "고무신도 안해봤답니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.11",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.12",
          "form": "고무신은 진짜",
          "original_form": "고무신은 진짜",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.13",
          "form": "하는게 아니어요",
          "original_form": "하는게 아니어요",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.14",
          "form": "아무리 인생 원수여도",
          "original_form": "아무리 인생 원수여도",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.15",
          "form": "그건 말릴거예요 ㅋㅋㅋㅋㅋ",
          "original_form": "그건 말릴거예요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022254.1.16",
          "form": "안좋은 케이스라도 보셨나요??",
          "original_form": "안좋은 케이스라도 보셨나요??",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022254.1.17",
          "form": "그쵸 아무래도....",
          "original_form": "그쵸 아무래도....",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022254.1.18",
          "form": "전역하면 다 차이고",
          "original_form": "전역하면 다 차이고",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022254.1.19",
          "form": "여자들만 고생하더라구요!",
          "original_form": "여자들만 고생하더라구요!",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022254.1.20",
          "form": "저는 친구들도 다 고무신은 안해봐서 그런 경우가 많은지 몰랐네요ㅠㅠ",
          "original_form": "저는 친구들도 다 고무신은 안해봐서 그런 경우가 많은지 몰랐네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022254.1.21",
          "form": "전역해서 다 차는건 양심도 없네요",
          "original_form": "전역해서 다 차는건 양심도 없네요",
          "speaker_id": "2",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022254.1.22",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022254.1.23",
          "form": "고무신 진짜",
          "original_form": "고무신 진짜",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022254.1.24",
          "form": "남자 좋은 일만 하는거같아요",
          "original_form": "남자 좋은 일만 하는거같아요",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022254.1.25",
          "form": "저 건너건너 아는 지인은",
          "original_form": "저 건너건너 아는 지인은",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022254.1.26",
          "form": "여자가 셀프로 맥심 찍어서 남친한테 보내줬는데",
          "original_form": "여자가 셀프로 맥심 찍어서 남친한테 보내줬는데",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022254.1.27",
          "form": "부대 사람들이 다 돌려봤다고...;",
          "original_form": "부대 사람들이 다 돌려봤다고...;",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022254.1.28",
          "form": "아니 남친",
          "original_form": "아니 남친",
          "speaker_id": "2",
          "time": "20210829 15:17"
        },
        {
          "id": "MDRW2100022254.1.29",
          "form": "그걸 왜 돌려봐요",
          "original_form": "그걸 왜 돌려봐요",
          "speaker_id": "2",
          "time": "20210829 15:17"
        },
        {
          "id": "MDRW2100022254.1.30",
          "form": "정신 나갔나봐요 진짜",
          "original_form": "정신 나갔나봐요 진짜",
          "speaker_id": "2",
          "time": "20210829 15:17"
        },
        {
          "id": "MDRW2100022254.1.31",
          "form": "구분을 못하나?",
          "original_form": "구분을 못하나?",
          "speaker_id": "2",
          "time": "20210829 15:17"
        },
        {
          "id": "MDRW2100022254.1.32",
          "form": "그니까옄ㅋㅋㅋㅋ",
          "original_form": "그니까옄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:21"
        },
        {
          "id": "MDRW2100022254.1.33",
          "form": "군대 들어가면 좀",
          "original_form": "군대 들어가면 좀",
          "speaker_id": "1",
          "time": "20210829 15:21"
        },
        {
          "id": "MDRW2100022254.1.34",
          "form": "선임들한테 잘보이고싶은게",
          "original_form": "선임들한테 잘보이고싶은게",
          "speaker_id": "1",
          "time": "20210829 15:22"
        },
        {
          "id": "MDRW2100022254.1.35",
          "form": "커져서....ㅎ 하",
          "original_form": "커져서....ㅎ 하",
          "speaker_id": "1",
          "time": "20210829 15:22"
        },
        {
          "id": "MDRW2100022254.1.36",
          "form": "그냥 빻은 집단같아요",
          "original_form": "그냥 빻은 집단같아요",
          "speaker_id": "1",
          "time": "20210829 15:22"
        }
      ]
    }
