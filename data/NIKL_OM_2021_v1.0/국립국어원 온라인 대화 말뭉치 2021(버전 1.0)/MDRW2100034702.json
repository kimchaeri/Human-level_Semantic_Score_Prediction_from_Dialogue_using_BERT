      "utterance": [
        {
          "id": "MDRW2100034702.1.1",
          "form": "헉 꿀팁이네요",
          "original_form": "헉 꿀팁이네요",
          "speaker_id": "1",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100034702.1.2",
          "form": "안정적인 시간대에 가야겠어요",
          "original_form": "안정적인 시간대에 가야겠어요",
          "speaker_id": "1",
          "time": "20210828 17:49"
        },
        {
          "id": "MDRW2100034702.1.3",
          "form": "네 맞아요\n그런데 여름엔 낮시간에 나가기 더워서 늦은시간에 나가니 주로 들고와야했어요ㅜㅜ",
          "original_form": "네 맞아요\n그런데 여름엔 낮시간에 나가기 더워서 늦은시간에 나가니 주로 들고와야했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100034702.1.4",
          "form": "아 ㅠㅠ그런 단점이..",
          "original_form": "아 ㅠㅠ그런 단점이..",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100034702.1.5",
          "form": "그래도 마트가 멀지는 않아서 다행이었어요",
          "original_form": "그래도 마트가 멀지는 않아서 다행이었어요",
          "speaker_id": "2",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100034702.1.6",
          "form": "그나마 다행이네요 ㅋㅋㅋ",
          "original_form": "그나마 다행이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100034702.1.7",
          "form": "이제는 날씨가 시원해져서 가기 좋을것 같아요",
          "original_form": "이제는 날씨가 시원해져서 가기 좋을것 같아요",
          "speaker_id": "2",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100034702.1.8",
          "form": "가까우면 부담이 덜하져",
          "original_form": "가까우면 부담이 덜하져",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100034702.1.9",
          "form": "맞아요! 낮에만 좀 더 선선하면 딱 좋겠어요",
          "original_form": "맞아요! 낮에만 좀 더 선선하면 딱 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100034702.1.10",
          "form": "다음달되면 더 선선해지겠죠?ㅎㅎ",
          "original_form": "다음달되면 더 선선해지겠죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100034702.1.11",
          "form": "혹시 해외직구도 해보셨나요?",
          "original_form": "혹시 해외직구도 해보셨나요?",
          "speaker_id": "2",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100034702.1.12",
          "form": "네! ㅋㅋㅋ",
          "original_form": "네! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100034702.1.13",
          "form": "쿠팡은 해외직구 무료라 좋더라구요",
          "original_form": "쿠팡은 해외직구 무료라 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100034702.1.14",
          "form": "쿠팡 이용 안 해서 해외직구 무료인지 몰랐어요!",
          "original_form": "쿠팡 이용 안 해서 해외직구 무료인지 몰랐어요!",
          "speaker_id": "2",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100034702.1.15",
          "form": "새로운 소식을 알았네요ㅎㅎ",
          "original_form": "새로운 소식을 알았네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:56"
        },
        {
          "id": "MDRW2100034702.1.16",
          "form": "전부는 아니고 그 멤버쉽만요 ㅜㅜ",
          "original_form": "전부는 아니고 그 멤버쉽만요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 17:56"
        },
        {
          "id": "MDRW2100034702.1.17",
          "form": "전 운좋게 이벤트할때 무료로 샀었어요",
          "original_form": "전 운좋게 이벤트할때 무료로 샀었어요",
          "speaker_id": "1",
          "time": "20210828 17:56"
        },
        {
          "id": "MDRW2100034702.1.18",
          "form": "좋은 기회를 잘 잡으셨네요!",
          "original_form": "좋은 기회를 잘 잡으셨네요!",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100034702.1.19",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:57"
        }
