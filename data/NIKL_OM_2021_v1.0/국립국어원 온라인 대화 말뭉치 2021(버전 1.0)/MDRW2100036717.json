      "utterance": [
        {
          "id": "MDRW2100036717.1.1",
          "form": "어떤음식 좋아하세요??",
          "original_form": "어떤음식 좋아하세요??",
          "speaker_id": "1",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100036717.1.2",
          "form": "저는 치즈 들어간 음식 좋아해요",
          "original_form": "저는 치즈 들어간 음식 좋아해요",
          "speaker_id": "2",
          "time": "20210831 11:03"
        },
        {
          "id": "MDRW2100036717.1.3",
          "form": "치즈 좋죠 ㅎㅎ 저도 치즈 좋아합니다^^",
          "original_form": "치즈 좋죠 ㅎㅎ 저도 치즈 좋아합니다^^",
          "speaker_id": "1",
          "time": "20210831 11:07"
        },
        {
          "id": "MDRW2100036717.1.4",
          "form": "커피 좋아하시나요??",
          "original_form": "커피 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100036717.1.5",
          "form": "커피 좋죠.. 지금도 마시고 있어요..ㅎㅎㅎ",
          "original_form": "커피 좋죠.. 지금도 마시고 있어요..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036717.1.6",
          "form": "ㅎㅎㅎ 아메리카노 드시나요??",
          "original_form": "ㅎㅎㅎ 아메리카노 드시나요??",
          "speaker_id": "1",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100036717.1.7",
          "form": "저도 커피 좋아하는데",
          "original_form": "저도 커피 좋아하는데",
          "speaker_id": "1",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100036717.1.8",
          "form": "아메는 못마신답니다 ㅋㅋㅋㄱ",
          "original_form": "아메는 못마신답니다 ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100036717.1.9",
          "form": "저 첨에 무조건 커피에 온갖 토핑 들어갔어야 했는데",
          "original_form": "저 첨에 무조건 커피에 온갖 토핑 들어갔어야 했는데",
          "speaker_id": "2",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036717.1.10",
          "form": "지금은 원두커피 마셔요",
          "original_form": "지금은 원두커피 마셔요",
          "speaker_id": "2",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036717.1.11",
          "form": "와 멋지네요",
          "original_form": "와 멋지네요",
          "speaker_id": "1",
          "time": "20210831 12:44"
        },
        {
          "id": "MDRW2100036717.1.12",
          "form": "원두커피라니 ㅎㅎ",
          "original_form": "원두커피라니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:44"
        },
        {
          "id": "MDRW2100036717.1.13",
          "form": "전 요즘 크림라떼에 빠져서 ㅎㅎ",
          "original_form": "전 요즘 크림라떼에 빠져서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:44"
        },
        {
          "id": "MDRW2100036717.1.14",
          "form": "맛있죠.. 저도 한때 그런거 마시다가.. 점점 아메의 세계로..ㅎㅎ 것도 점점 찐하게..ㅎㅎㅎ",
          "original_form": "맛있죠.. 저도 한때 그런거 마시다가.. 점점 아메의 세계로..ㅎㅎ 것도 점점 찐하게..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036717.1.15",
          "form": "오 아메",
          "original_form": "오 아메",
          "speaker_id": "1",
          "time": "20210831 12:46"
        },
        {
          "id": "MDRW2100036717.1.16",
          "form": "전 여전히 입맛은 초딩인가봅니다",
          "original_form": "전 여전히 입맛은 초딩인가봅니다",
          "speaker_id": "1",
          "time": "20210831 12:46"
        },
        {
          "id": "MDRW2100036717.1.17",
          "form": "날씨가 이래서 그런지 칼국수 수제비가 계속",
          "original_form": "날씨가 이래서 그런지 칼국수 수제비가 계속",
          "speaker_id": "1",
          "time": "20210831 12:47"
        },
        {
          "id": "MDRW2100036717.1.18",
          "form": "먹고 싶네요 ㅎㅎ",
          "original_form": "먹고 싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:47"
        },
        {
          "id": "MDRW2100036717.1.19",
          "form": "이런날은 밀가루가 땡기죠.. 국물있는",
          "original_form": "이런날은 밀가루가 땡기죠.. 국물있는",
          "speaker_id": "2",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100036717.1.20",
          "form": "전 이런 날 기름진게 땡겨요..ㅎㅎ",
          "original_form": "전 이런 날 기름진게 땡겨요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100036717.1.21",
          "form": "기름진거 뭐땡기나요?? ㅎㅎ",
          "original_form": "기름진거 뭐땡기나요?? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036717.1.22",
          "form": "전 최근에 칼국수 수제비를 몇그릇을 먹어댔는지\nㅋㅋ",
          "original_form": "전 최근에 칼국수 수제비를 몇그릇을 먹어댔는지\nㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036717.1.23",
          "form": "부침개요~",
          "original_form": "부침개요~",
          "speaker_id": "2",
          "time": "20210831 13:59"
        },
        {
          "id": "MDRW2100036717.1.24",
          "form": "야채전을 하도 먹어서 오늘은 안했네요",
          "original_form": "야채전을 하도 먹어서 오늘은 안했네요",
          "speaker_id": "2",
          "time": "20210831 13:59"
        },
        {
          "id": "MDRW2100036717.1.25",
          "form": "아 야채전 맛있겠어요",
          "original_form": "아 야채전 맛있겠어요",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036717.1.26",
          "form": "호박전 땡기네요 ㅎㅎ",
          "original_form": "호박전 땡기네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036717.1.27",
          "form": "늙은호반 전이요 ㅎㅎ",
          "original_form": "늙은호반 전이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036717.1.28",
          "form": "애호박도 넣었었네요..ㅎㅎ 부추도 넣고",
          "original_form": "애호박도 넣었었네요..ㅎㅎ 부추도 넣고",
          "speaker_id": "2",
          "time": "20210831 14:33"
        },
        {
          "id": "MDRW2100036717.1.29",
          "form": "ㅎㅎ 요리 잘하시네요 전 요리랑 절교한지 오래라 ㅠ",
          "original_form": "ㅎㅎ 요리 잘하시네요 전 요리랑 절교한지 오래라 ㅠ",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036717.1.30",
          "form": "저는 요리는 안어렵더라구요..",
          "original_form": "저는 요리는 안어렵더라구요..",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036717.1.31",
          "form": "그래도 집에서 해먹기 힘들거나 귀찮은 건 식당에서 사먹어요..ㅎㅎ",
          "original_form": "그래도 집에서 해먹기 힘들거나 귀찮은 건 식당에서 사먹어요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100036717.1.32",
          "form": "와 요리가 어렵지않다니",
          "original_form": "와 요리가 어렵지않다니",
          "speaker_id": "1",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100036717.1.33",
