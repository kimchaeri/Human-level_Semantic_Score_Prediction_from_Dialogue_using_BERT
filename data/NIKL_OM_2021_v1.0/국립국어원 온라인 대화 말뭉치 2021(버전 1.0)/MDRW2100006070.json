      "utterance": [
        {
          "id": "MDRW2100006070.1.1",
          "form": "식사는 맛있개 하셨나요",
          "original_form": "식사는 맛있개 하셨나요",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100006070.1.2",
          "form": "네 샐러디에서 우삼겹 사왔습니다",
          "original_form": "네 샐러디에서 우삼겹 사왔습니다",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100006070.1.3",
          "form": "배채웠습니당",
          "original_form": "배채웠습니당",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100006070.1.4",
          "form": "샐러디가 샐러드 체인이죠? 지방이라 그런지 생소하네요",
          "original_form": "샐러디가 샐러드 체인이죠? 지방이라 그런지 생소하네요",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100006070.1.5",
          "form": "아 네 샐러드파는 체인점인데",
          "original_form": "아 네 샐러드파는 체인점인데",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100006070.1.6",
          "form": "좀 퓨전 샐러드?",
          "original_form": "좀 퓨전 샐러드?",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100006070.1.7",
          "form": "고기 좀 들어가구요",
          "original_form": "고기 좀 들어가구요",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100006070.1.8",
          "form": "우삼겹이면 맛있는 샐러드겠어요 ㅎㅎ",
          "original_form": "우삼겹이면 맛있는 샐러드겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100006070.1.9",
          "form": "네 먹을만해요 ㅋㅋ",
          "original_form": "네 먹을만해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100006070.1.10",
          "form": "저녁 뭐 드셨나요",
          "original_form": "저녁 뭐 드셨나요",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100006070.1.11",
          "form": "저는 비지찌개에 오리훈제볶음이요 ㅎㅎ",
          "original_form": "저는 비지찌개에 오리훈제볶음이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100006070.1.12",
          "form": "아니 진수성찬을",
          "original_form": "아니 진수성찬을",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100006070.1.13",
          "form": "치사합니다!!!!",
          "original_form": "치사합니다!!!!",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100006070.1.14",
          "form": "배달반찬입니다 ㅎㅎ",
          "original_form": "배달반찬입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006070.1.15",
          "form": "배달이 진수성찬이네요",
          "original_form": "배달이 진수성찬이네요",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006070.1.16",
          "form": "맛집인가요",
          "original_form": "맛집인가요",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006070.1.17",
          "form": "유아반찬입니다...살기위해 먹습니다 ㅎㅎ",
          "original_form": "유아반찬입니다...살기위해 먹습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100006070.1.18",
          "form": "ㅋㅋ유아가 비지찌개를 어떻게 먹어요!!",
          "original_form": "ㅋㅋ유아가 비지찌개를 어떻게 먹어요!!",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006070.1.19",
          "form": "ㅋㅋㅋㅋ 김치를 씻어서 맑게 ?? 끓였더라구여",
