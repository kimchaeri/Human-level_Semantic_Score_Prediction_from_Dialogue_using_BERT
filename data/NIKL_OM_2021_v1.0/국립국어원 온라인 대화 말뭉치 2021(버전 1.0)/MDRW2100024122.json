      "utterance": [
        {
          "id": "MDRW2100024122.1.1",
          "form": "저는 음식 많이 가리는 편이라 주변에서 그런 소리 많이 들어요",
          "original_form": "저는 음식 많이 가리는 편이라 주변에서 그런 소리 많이 들어요",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024122.1.2",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024122.1.3",
          "form": "안 먹는 음식들 있으세요?",
          "original_form": "안 먹는 음식들 있으세요?",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024122.1.4",
          "form": "많죠ㅋㅋㅋ 저는 깻잎싫어해요",
          "original_form": "많죠ㅋㅋㅋ 저는 깻잎싫어해요",
          "speaker_id": "2",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024122.1.5",
          "form": "아 깻잎 싫어하시는군요..\r\n전 깻잎은 잘 먹어요",
          "original_form": "아 깻잎 싫어하시는군요..\r\n전 깻잎은 잘 먹어요",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024122.1.6",
          "form": "엇 그러시구나",
          "original_form": "엇 그러시구나",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100024122.1.7",
          "form": "전 해물을 싫어해요",
          "original_form": "전 해물을 싫어해요",
          "speaker_id": "1",
          "time": "20210831 00:46"
        },
        {
          "id": "MDRW2100024122.1.8",
          "form": "저도 회는 안좋아해서",
          "original_form": "저도 회는 안좋아해서",
          "speaker_id": "2",
          "time": "20210831 11:13"
        },
        {
          "id": "MDRW2100024122.1.9",
          "form": "그럼 회가 아니고 익힌 해물은 드시는군요",
          "original_form": "그럼 회가 아니고 익힌 해물은 드시는군요",
          "speaker_id": "1",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100024122.1.10",
          "form": "익힌해물도...그렇게좋아하지않아요 갑각류는 손질하기 귀찮기도하구요ㅋㅋㅋ",
          "original_form": "익힌해물도...그렇게좋아하지않아요 갑각류는 손질하기 귀찮기도하구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:51"
        },
        {
          "id": "MDRW2100024122.1.11",
          "form": "ㅋㅋㅋㅋ 그러시군요 \r\n저도 해물종류는 거의 다 안 먹어요",
          "original_form": "ㅋㅋㅋㅋ 그러시군요 \r\n저도 해물종류는 거의 다 안 먹어요",
          "speaker_id": "1",
          "time": "20210831 14:55"
        },
        {
          "id": "MDRW2100024122.1.12",
          "form": "저도 고기파라서ㅋㅋㅋ거의안먹죠",
          "original_form": "저도 고기파라서ㅋㅋㅋ거의안먹죠",
          "speaker_id": "2",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100024122.1.13",
          "form": "ㅋㅋㅋ 저도 고기파에요",
          "original_form": "ㅋㅋㅋ 저도 고기파에요",
          "speaker_id": "1",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100024122.1.14",
          "form": "근데 곱창 막창 닭발 껍데기 이런건 못 먹어요ㅋㅋㅋ",
          "original_form": "근데 곱창 막창 닭발 껍데기 이런건 못 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100024122.1.15",
          "form": "오 저둔데",
          "original_form": "오 저둔데",
          "speaker_id": "2",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100024122.1.16",
          "form": "곱창 막창은 가끔 먹긴하는데 껍데기 닭발은 못먹겠더라구요",
          "original_form": "곱창 막창은 가끔 먹긴하는데 껍데기 닭발은 못먹겠더라구요",
          "speaker_id": "2",
          "time": "20210831 16:12"
        },
        {
          "id": "MDRW2100024122.1.17",
          "form": "식성이 비슷하네요",
          "original_form": "식성이 비슷하네요",
          "speaker_id": "1",
          "time": "20210831 16:13"
        }
      ]
