      "utterance": [
        {
          "id": "MDRW2100033463.1.1",
          "form": "핫윙~~~",
          "original_form": "핫윙~~~",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100033463.1.2",
          "form": "핫윙이 안주가 ㅋㅋ",
          "original_form": "핫윙이 안주가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100033463.1.3",
          "form": "응 ㅋㅋ 근데 다시 생각해보니 배불러서 ..",
          "original_form": "응 ㅋㅋ 근데 다시 생각해보니 배불러서 ..",
          "speaker_id": "1",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100033463.1.4",
          "form": "안먹을래",
          "original_form": "안먹을래",
          "speaker_id": "1",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100033463.1.5",
          "form": "건조기 돌리고 올게",
          "original_form": "건조기 돌리고 올게",
          "speaker_id": "1",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100033463.1.6",
          "form": "역시나 집안일 주부의 일은 끝이 없구려 ㅠㅠ\r\n나도 건조기에서 빨래 좀 가져와야겠다",
          "original_form": "역시나 집안일 주부의 일은 끝이 없구려 ㅠㅠ\r\n나도 건조기에서 빨래 좀 가져와야겠다",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100033463.1.7",
          "form": "옷 무덤 추가요",
          "original_form": "옷 무덤 추가요",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033463.1.8",
          "form": "어제 신랑한테 옷무덤 해체해달라카니 옷갰던데...ㅋㅋㅋ\r\n다시 다개야할판",
          "original_form": "어제 신랑한테 옷무덤 해체해달라카니 옷갰던데...ㅋㅋㅋ\r\n다시 다개야할판",
          "speaker_id": "2",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100033463.1.9",
          "form": "아.. 야무지지못하구나 군대 스타일 까먹었나",
          "original_form": "아.. 야무지지못하구나 군대 스타일 까먹었나",
          "speaker_id": "1",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100033463.1.10",
          "form": "옷갰으면 갔다넣어야되자나 수건만 넣어놓고 바닥에 그대로 놔둬가 애들 다 흐트려놓고..",
          "original_form": "옷갰으면 갔다넣어야되자나 수건만 넣어놓고 바닥에 그대로 놔둬가 애들 다 흐트려놓고..",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100033463.1.11",
          "form": "그래서 또 슬쩍 밀어놨으 ㅋㅋㅋ",
          "original_form": "그래서 또 슬쩍 밀어놨으 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100033463.1.12",
          "form": "하하하 나도 근데 넣는건 진짜 귀찮아 ㅋㅋㅋ 요즘엔 name3꺼는 name3 시킴",
          "original_form": "하하하 나도 근데 넣는건 진짜 귀찮아 ㅋㅋㅋ 요즘엔 &name3&꺼는 &name3& 시킴",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100033463.1.13",
          "form": "옷장이 ...ㅋㅋ 보도 모한다",
          "original_form": "옷장이 ...ㅋㅋ 보도 모한다",
          "speaker_id": "1",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100033463.1.14",
          "form": "내가 넣을땐",
          "original_form": "내가 넣을땐",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100033463.1.15",
          "form": "접어서 넣었는데",
          "original_form": "접어서 넣었는데",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100033463.1.16",
          "form": "나중에 보면 다 펴져있다",
          "original_form": "나중에 보면 다 펴져있다",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100033463.1.17",
          "form": "애들이 옷 골라입지??",
          "original_form": "애들이 옷 골라입지??",
          "speaker_id": "1",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100033463.1.18",
          "form": "아무래도 특히 name4는 지가 편하고 좋아하는 옷만 골라입는다 ㅠㅠ",
          "original_form": "아무래도 특히 &name4&는 지가 편하고 좋아하는 옷만 골라입는다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100033463.1.19",
          "form": "그러니깐 하나하나 다펼쳐보니깐",
          "original_form": "그러니깐 하나하나 다펼쳐보니깐",
          "speaker_id": "2",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100033463.1.20",
          "form": "맞어 name3도 그래서 다시 넣을땐 엉망",
          "original_form": "맞어 &name3&도 그래서 다시 넣을땐 엉망",
