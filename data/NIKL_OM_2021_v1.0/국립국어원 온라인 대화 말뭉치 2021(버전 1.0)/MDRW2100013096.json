      "utterance": [
        {
          "id": "MDRW2100013096.1.1",
          "form": "유튜브 좋아하시나요",
          "original_form": "유튜브 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 12:28"
        },
        {
          "id": "MDRW2100013096.1.2",
          "form": "고양이와 먹방 유튜브 간혹 봐요",
          "original_form": "고양이와 먹방 유튜브 간혹 봐요",
          "speaker_id": "2",
          "time": "20210825 12:29"
        },
        {
          "id": "MDRW2100013096.1.3",
          "form": "즐겨보는게 있으신가요",
          "original_form": "즐겨보는게 있으신가요",
          "speaker_id": "2",
          "time": "20210825 12:29"
        },
        {
          "id": "MDRW2100013096.1.4",
          "form": "먹방, 여행유튭이요ㅎㅎ",
          "original_form": "먹방, 여행유튭이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:30"
        },
        {
          "id": "MDRW2100013096.1.5",
          "form": "먹방은 누구 것을 주로 보시나요",
          "original_form": "먹방은 누구 것을 주로 보시나요",
          "speaker_id": "2",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100013096.1.6",
          "form": "히밥이요ㅎㅎ",
          "original_form": "히밥이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:32"
        },
        {
          "id": "MDRW2100013096.1.7",
          "form": "아 전  입짧은 햇님 영상봐요",
          "original_form": "아 전  입짧은 햇님 영상봐요",
          "speaker_id": "2",
          "time": "20210825 12:33"
        },
        {
          "id": "MDRW2100013096.1.8",
          "form": "히밥님 것도 찾아봐야겠어요",
          "original_form": "히밥님 것도 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 12:33"
        },
        {
          "id": "MDRW2100013096.1.9",
          "form": "엄청 잘먹어요ㅋㅋ",
          "original_form": "엄청 잘먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:42"
        },
        {
          "id": "MDRW2100013096.1.10",
          "form": "ㅎㅎ 먹방 유튜버들 보면 신기해요",
          "original_form": "ㅎㅎ 먹방 유튜버들 보면 신기해요",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100013096.1.11",
          "form": "위 한계가 있을텐데",
          "original_form": "위 한계가 있을텐데",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100013096.1.12",
          "form": "신기하게 비만인 사람도 없더라고요",
          "original_form": "신기하게 비만인 사람도 없더라고요",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100013096.1.13",
          "form": "맞아요 다 날씬해요",
          "original_form": "맞아요 다 날씬해요",
          "speaker_id": "1",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100013096.1.14",
          "form": "여행 유튜브는 누구것을 주로 보세요?",
          "original_form": "여행 유튜브는 누구것을 주로 보세요?",
          "speaker_id": "2",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013096.1.15",
          "form": "빠니보틀이요",
          "original_form": "빠니보틀이요",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100013096.1.16",
          "form": "아 저도 전에 본 적 있어요",
          "original_form": "아 저도 전에 본 적 있어요",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100013096.1.17",
          "form": "예전엔 안봤는데 요새는 대리만족이 되네요",
          "original_form": "예전엔 안봤는데 요새는 대리만족이 되네요",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100013096.1.18",
          "form": "마자요ㅎㅎㅎ",
          "original_form": "마자요ㅎㅎㅎ",
