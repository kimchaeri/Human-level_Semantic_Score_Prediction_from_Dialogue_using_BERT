          "time": "20210907 00:40"
        },
        {
          "id": "MDRW2100033293.1.23",
          "form": "not bad",
          "original_form": "not bad",
          "speaker_id": "1",
          "time": "20210907 00:40"
        },
        {
          "id": "MDRW2100033293.1.24",
          "form": "근데 뭐 내꺼아니라서ㅋㅋㅋ",
          "original_form": "근데 뭐 내꺼아니라서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:40"
        },
        {
          "id": "MDRW2100033293.1.25",
          "form": "아 그렇긴하지",
          "original_form": "아 그렇긴하지",
          "speaker_id": "2",
          "time": "20210907 00:53"
        },
        {
          "id": "MDRW2100033293.1.26",
          "form": "난 지르고싶었는데 못 질렀거든 ㅋㄴㅋ",
          "original_form": "난 지르고싶었는데 못 질렀거든 ㅋㄴㅋ",
          "speaker_id": "2",
          "time": "20210907 00:53"
        },
        {
          "id": "MDRW2100033293.1.27",
          "form": "음 버즈 잘팔리는것 같더라",
          "original_form": "음 버즈 잘팔리는것 같더라",
          "speaker_id": "1",
          "time": "20210907 01:50"
        },
        {
          "id": "MDRW2100033293.1.28",
