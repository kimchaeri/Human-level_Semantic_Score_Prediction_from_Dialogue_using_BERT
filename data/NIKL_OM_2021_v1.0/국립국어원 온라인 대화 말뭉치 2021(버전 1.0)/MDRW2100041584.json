      "utterance": [
        {
          "id": "MDRW2100041584.1.1",
          "form": "오늘 널씨 어때요",
          "original_form": "오늘 널씨 어때요",
          "speaker_id": "1",
          "time": "20210906 11:46"
        },
        {
          "id": "MDRW2100041584.1.2",
          "form": "습하네용",
          "original_form": "습하네용",
          "speaker_id": "2",
          "time": "20210906 11:47"
        },
        {
          "id": "MDRW2100041584.1.3",
          "form": "그래도시원한편이죠",
          "original_form": "그래도시원한편이죠",
          "speaker_id": "2",
          "time": "20210906 11:47"
        },
        {
          "id": "MDRW2100041584.1.4",
          "form": "크게 안덥네요",
          "original_form": "크게 안덥네요",
          "speaker_id": "1",
          "time": "20210906 11:47"
        },
        {
          "id": "MDRW2100041584.1.5",
          "form": "긴팔 입었는데",
          "original_form": "긴팔 입었는데",
          "speaker_id": "1",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100041584.1.6",
          "form": "ㅎㅎ전아직 땀쟁이라",
          "original_form": "ㅎㅎ전아직 땀쟁이라",
          "speaker_id": "2",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100041584.1.7",
          "form": "여름옷차림이에요",
          "original_form": "여름옷차림이에요",
          "speaker_id": "2",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100041584.1.8",
          "form": "오 정말요 좀 춥지 않으세요???",
          "original_form": "오 정말요 좀 춥지 않으세요???",
          "speaker_id": "1",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041584.1.9",
          "form": "파워워킹해서 걸으면 갠찬더라구요 ㅎㅎ",
          "original_form": "파워워킹해서 걸으면 갠찬더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041584.1.10",
          "form": "오늘 일하다가 가을옷 질러서",
          "original_form": "오늘 일하다가 가을옷 질러서",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041584.1.11",
          "form": "이제 담주되면 가을옷 개시할려구요",
          "original_form": "이제 담주되면 가을옷 개시할려구요",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041584.1.12",
          "form": "오오 파워워킹 하시는구나",
          "original_form": "오오 파워워킹 하시는구나",
          "speaker_id": "1",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100041584.1.13",
          "form": "오오 저도요",
          "original_form": "오오 저도요",
          "speaker_id": "1",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100041584.1.14",
          "form": "가을이 길엇음 하는데 추석되면 진짜 추울꺼같아요 흑",
          "original_form": "가을이 길엇음 하는데 추석되면 진짜 추울꺼같아요 흑",
          "speaker_id": "2",
          "time": "20210906 12:26"
        },
        {
          "id": "MDRW2100041584.1.15",
          "form": "그러게요ㅠㅠ 부지런히 가을 옷 입어야해요",
          "original_form": "그러게요ㅠㅠ 부지런히 가을 옷 입어야해요",
          "speaker_id": "1",
          "time": "20210906 12:36"
        },
        {
          "id": "MDRW2100041584.1.16",
          "form": "맞아유ㅜㅜ",
          "original_form": "맞아유ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 13:42"
        },
        {
          "id": "MDRW2100041584.1.17",
          "form": "가을 너무 좋아용",
          "original_form": "가을 너무 좋아용",
          "speaker_id": "1",
          "time": "20210906 14:01"
        },
        {
          "id": "MDRW2100041584.1.18",
          "form": "싫어하는계절은 잇으신가요? ㅎㅎ",
          "original_form": "싫어하는계절은 잇으신가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:02"
        },
        {
          "id": "MDRW2100041584.1.19",
          "form": "저는 여름 너무 싫어요 ㅋㅋ",
          "original_form": "저는 여름 너무 싫어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041584.1.20",
          "form": "흑저두요",
          "original_form": "흑저두요",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041584.1.21",
          "form": "끔찍햇어요",
          "original_form": "끔찍햇어요",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041584.1.22",
          "form": "이번엔 너무 길어져서 짜증까지 나더라고요 ㅋㅋ",
          "original_form": "이번엔 너무 길어져서 짜증까지 나더라고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:04"
