      "utterance": [
        {
          "id": "MDRW2100027767.1.1",
          "form": "공연이랑 전시회 관람 좋아하세요?",
          "original_form": "공연이랑 전시회 관람 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100027767.1.2",
          "form": "많이 좋아해요",
          "original_form": "많이 좋아해요",
          "speaker_id": "2",
          "time": "20210902 14:55"
        },
        {
          "id": "MDRW2100027767.1.3",
          "form": "오오 어떤 분야 좋아하시나욤",
          "original_form": "오오 어떤 분야 좋아하시나욤",
          "speaker_id": "1",
          "time": "20210902 14:57"
        },
        {
          "id": "MDRW2100027767.1.4",
          "form": "두루두루 다 좋아하는 편이에요",
          "original_form": "두루두루 다 좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210902 14:57"
        },
        {
          "id": "MDRW2100027767.1.5",
          "form": "뮤지컬 연극 발레",
          "original_form": "뮤지컬 연극 발레",
          "speaker_id": "2",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027767.1.6",
          "form": "미술관은 별로 안 좋아해요",
          "original_form": "미술관은 별로 안 좋아해요",
          "speaker_id": "2",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027767.1.7",
          "form": "현대미술 이해하기 너무 어렵더라고요",
          "original_form": "현대미술 이해하기 너무 어렵더라고요",
          "speaker_id": "2",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027767.1.8",
          "form": "앜ㅋㅋ 전 그중에 미술관 제일 좋아하는데",
          "original_form": "앜ㅋㅋ 전 그중에 미술관 제일 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100027767.1.9",
          "form": "전 발레가 너무 어려워요...ㅠㅠ",
          "original_form": "전 발레가 너무 어려워요...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100027767.1.10",
          "form": "뮤지컬은 내한오면 자주 찾아봐요!!",
          "original_form": "뮤지컬은 내한오면 자주 찾아봐요!!",
          "speaker_id": "1",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027767.1.11",
          "form": "그러시군용",
          "original_form": "그러시군용",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027767.1.12",
          "form": "코로나땜에 요즘은 공연장 가기도 어려워서",
          "original_form": "코로나땜에 요즘은 공연장 가기도 어려워서",
          "speaker_id": "2",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027767.1.13",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027767.1.14",
          "form": "좋아하는 뮤지컬 공연 있으세요?",
          "original_form": "좋아하는 뮤지컬 공연 있으세요?",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027767.1.15",
          "form": "저 위키드요",
          "original_form": "저 위키드요",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027767.1.16",
          "form": "예전에 뉴욕 여행갔을 때 처음 보고 너무 좋아서 총 세번이나 봤어요 ㅠㅠㅋㅋㅋ",
          "original_form": "예전에 뉴욕 여행갔을 때 처음 보고 너무 좋아서 총 세번이나 봤어요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027767.1.17",
          "form": "푹 빠지셨군요",
          "original_form": "푹 빠지셨군요",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027767.1.18",
          "form": "저는 한국배우들 공연만 봤어요. 브로드웨이배우들",
          "original_form": "저는 한국배우들 공연만 봤어요. 브로드웨이배우들",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027767.1.19",
          "form": "은 어떤 느낌일지 궁금",
          "original_form": "은 어떤 느낌일지 궁금",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027767.1.20",
          "form": "전 오히려 한국 위키드를 못봤어요. ㅋㅋ",
          "original_form": "전 오히려 한국 위키드를 못봤어요. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027767.1.21",
          "form": "옥주현이 너무 잘한다던데...",
          "original_form": "옥주현이 너무 잘한다던데...",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027767.1.22",
          "form": "저도 옥주현 공연 봤는데 비교 대상이 없어서",
          "original_form": "저도 옥주현 공연 봤는데 비교 대상이 없어서",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027767.1.23",
          "form": "뭐라 말을 못 하겠지만 아무튼 잘 했어요 멋지고",
          "original_form": "뭐라 말을 못 하겠지만 아무튼 잘 했어요 멋지고",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027767.1.24",
          "form": "넘버들이 다 너무 좋지 않나요 ㅠㅠ????",
          "original_form": "넘버들이 다 너무 좋지 않나요 ㅠㅠ????",
          "speaker_id": "1",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100027767.1.25",
          "form": "제 최애곡은 defying gravity에요 ㅠㅠ",
          "original_form": "제 최애곡은 defying gravity에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100027767.1.26",
          "form": "위키드하면 디파잉 그래비티죠",
          "original_form": "위키드하면 디파잉 그래비티죠",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027767.1.27",
          "form": "귓가에 들리는 듯 하네요",
          "original_form": "귓가에 들리는 듯 하네요",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027767.1.28",
          "form": "그리고 알파바하고 글린다 같이 나오는 노래도 좋아해요. 글린다가 알파바 꾸며주면서 부른거 ㅋㅋ",
          "original_form": "그리고 알파바하고 글린다 같이 나오는 노래도 좋아해요. 글린다가 알파바 꾸며주면서 부른거 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:38"
        }
      ]
