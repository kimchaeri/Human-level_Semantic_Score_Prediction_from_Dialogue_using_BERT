      "utterance": [
        {
          "id": "MDRW2100030427.1.1",
          "form": "지금 스파이더맨 새로 찍고 있대요",
          "original_form": "지금 스파이더맨 새로 찍고 있대요",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030427.1.2",
          "form": "빨리 보고 싶네요",
          "original_form": "빨리 보고 싶네요",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030427.1.3",
          "form": "졸리지 않나요",
          "original_form": "졸리지 않나요",
          "speaker_id": "2",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030427.1.4",
          "form": "역대 스파이더맨 세 명이 동시에 출연 하나 봐요",
          "original_form": "역대 스파이더맨 세 명이 동시에 출연 하나 봐요",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030427.1.5",
          "form": "매우 기대가 돼요",
          "original_form": "매우 기대가 돼요",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030427.1.6",
          "form": "뭐 전혀 안 와요",
          "original_form": "뭐 전혀 안 와요",
          "speaker_id": "1",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030427.1.7",
          "form": "대단하네요",
          "original_form": "대단하네요",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030427.1.8",
          "form": "블랙 위도우도 아직 못 봤어요",
          "original_form": "블랙 위도우도 아직 못 봤어요",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030427.1.9",
          "form": "공연은 언제 볼 수 있을까요",
          "original_form": "공연은 언제 볼 수 있을까요",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030427.1.10",
          "form": "극장에서 보고 싶었는데 코로나가 심해져서",
          "original_form": "극장에서 보고 싶었는데 코로나가 심해져서",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030427.1.11",
          "form": "블랙 위도우 별로였던 거 같아요",
          "original_form": "블랙 위도우 별로였던 거 같아요",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100030427.1.12",
          "form": "연극 또 보고 싶고 뮤지컬도 보고 싶고",
          "original_form": "연극 또 보고 싶고 뮤지컬도 보고 싶고",
          "speaker_id": "1",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100030427.1.13",
          "form": "보다가 잤거든요",
          "original_form": "보다가 잤거든요",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100030427.1.14",
          "form": "그래도 보고 싶네요",
          "original_form": "그래도 보고 싶네요",
          "speaker_id": "1",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100030427.1.15",
          "form": "넷플릭스에 없나요",
          "original_form": "넷플릭스에 없나요",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100030427.1.16",
          "form": "공짜면 보세요",
          "original_form": "공짜면 보세요",
