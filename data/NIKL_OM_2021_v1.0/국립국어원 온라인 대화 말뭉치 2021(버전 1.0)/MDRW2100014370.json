      "utterance": [
        {
          "id": "MDRW2100014370.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014370.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014370.1.3",
          "form": "건강 챙기려고 하시는거 있으세요?",
          "original_form": "건강 챙기려고 하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014370.1.4",
          "form": "잠 많이 자기요!",
          "original_form": "잠 많이 자기요!",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014370.1.5",
          "form": "ㅋㅋㅋ 숙면 취하는거 중요하죠",
          "original_form": "ㅋㅋㅋ 숙면 취하는거 중요하죠",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014370.1.6",
          "form": "영양제는 어떤거 챙겨드시나요",
          "original_form": "영양제는 어떤거 챙겨드시나요",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014370.1.7",
          "form": "저는 원래 마그네슘 먹었는데\r\n별로 효과가 없어서 요즘은 안먹고있어요",
          "original_form": "저는 원래 마그네슘 먹었는데\r\n별로 효과가 없어서 요즘은 안먹고있어요",
          "speaker_id": "2",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014370.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014370.1.9",
          "form": "저는 종합 비타민이랑 오메가 열심히 챙겨먹어요",
          "original_form": "저는 종합 비타민이랑 오메가 열심히 챙겨먹어요",
          "speaker_id": "1",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014370.1.10",
          "form": "운동은 하시나요?",
          "original_form": "운동은 하시나요?",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014370.1.11",
          "form": "하하하 헬스장 1년권 끊었는데 가본 횟수가 얼마안돼요....ㅜㅜ",
          "original_form": "하하하 헬스장 1년권 끊었는데 가본 횟수가 얼마안돼요....ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014370.1.12",
          "form": "아 헬스 가면 좀 지겹지않나요ㅠ",
          "original_form": "아 헬스 가면 좀 지겹지않나요ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014370.1.13",
          "form": "저도 재수할때 했는데",
          "original_form": "저도 재수할때 했는데",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014370.1.14",
          "form": "러닝만 해서 지루했었어요",
          "original_form": "러닝만 해서 지루했었어요",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100014370.1.15",
          "form": "ㅋㅋㅋ맞아요 재미없어요 ㅜㅜ",
          "original_form": "ㅋㅋㅋ맞아요 재미없어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100014370.1.16",
          "form": "그래서 링피트를 사보았어요...",
          "original_form": "그래서 링피트를 사보았어요...",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100014370.1.17",
          "form": "오 어때요????",
          "original_form": "오 어때요????",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100014370.1.18",
          "form": "요즘 스위치 관심있어서ㅎㅎㅎ",
          "original_form": "요즘 스위치 관심있어서ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100014370.1.19",
          "form": "확실히 운동 되고 재미있어요",
          "original_form": "확실히 운동 되고 재미있어요",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100014370.1.20",
          "form": "생각보다는 덜하고 있지만요",
          "original_form": "생각보다는 덜하고 있지만요",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100014370.1.21",
          "form": "저는 위 있을때 저스트댄스 재밌었어서요",
          "original_form": "저는 위 있을때 저스트댄스 재밌었어서요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100014370.1.22",
          "form": "혹시 저스트댄스도 하세요?",
          "original_form": "혹시 저스트댄스도 하세요?",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100014370.1.23",
          "form": "네 2021버전 샀어요~",
          "original_form": "네 2021버전 샀어요~",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100014370.1.24",
          "form": "그런데 운동할때 집 공간은 충분하세요?",
          "original_form": "그런데 운동할때 집 공간은 충분하세요?",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100014370.1.25",
          "form": "저희집은 거실이 좁아서요",
          "original_form": "저희집은 거실이 좁아서요",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100014370.1.26",
          "form": "저 작은방에서 하는데 생각보다 공간은 괜찮더라고요! 제자리에서 거의 안움직여요",
          "original_form": "저 작은방에서 하는데 생각보다 공간은 괜찮더라고요! 제자리에서 거의 안움직여요",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100014370.1.27",
          "form": "링피트, 저스트 댄스 둘다요??",
          "original_form": "링피트, 저스트 댄스 둘다요??",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100014370.1.28",
          "form": "네네! 둘다 운동하기에 좋은 게임 같아요",
          "original_form": "네네! 둘다 운동하기에 좋은 게임 같아요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100014370.1.29",
          "form": "늦은시간에두 잘하시나요???",
          "original_form": "늦은시간에두 잘하시나요???",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100014370.1.30",
          "form": "링피트에 무소음 모드있는데 그건 뛰는거를 스쿼트로 바꿔서 하는거라 밤에도 할만해요!",
          "original_form": "링피트에 무소음 모드있는데 그건 뛰는거를 스쿼트로 바꿔서 하는거라 밤에도 할만해요!",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100014370.1.31",
          "form": "저댄은 밤에 하긴 힘든것 같아요",
          "original_form": "저댄은 밤에 하긴 힘든것 같아요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100014370.1.32",
          "form": "오 링피트 그런 기능 있는 줄 첨알았네요",
          "original_form": "오 링피트 그런 기능 있는 줄 첨알았네요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100014370.1.33",
          "form": "매일매일하시나요?!",
          "original_form": "매일매일하시나요?!",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100014370.1.34",
          "form": "요즘은 시들해져서 잘 안하고있네요 ㅜㅜ",
          "original_form": "요즘은 시들해져서 잘 안하고있네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 22:48"
        }
      ]
