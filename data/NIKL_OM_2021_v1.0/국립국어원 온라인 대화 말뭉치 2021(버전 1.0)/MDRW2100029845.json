      "utterance": [
        {
          "id": "MDRW2100029845.1.1",
          "form": "싸이월드 부활은 뭐냐",
          "original_form": "싸이월드 부활은 뭐냐",
          "speaker_id": "2",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100029845.1.2",
          "form": "싸이월드라는 싸이트가 다시 오픈하는 거예요",
          "original_form": "싸이월드라는 싸이트가 다시 오픈하는 거예요",
          "speaker_id": "1",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100029845.1.3",
          "form": "그래? 들어본 것도 같고.. 니가 예전에 도토리 어쩌고 했던 그거냐",
          "original_form": "그래? 들어본 것도 같고.. 니가 예전에 도토리 어쩌고 했던 그거냐",
          "speaker_id": "2",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100029845.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100029845.1.5",
          "form": "그럼 엄청 오래전 사이트일텐데",
          "original_form": "그럼 엄청 오래전 사이트일텐데",
          "speaker_id": "2",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100029845.1.6",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100029845.1.7",
          "form": "망한 거 아니었니?",
          "original_form": "망한 거 아니었니?",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100029845.1.8",
          "form": "저도 그런줄 알았어요",
          "original_form": "저도 그런줄 알았어요",
          "speaker_id": "1",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100029845.1.9",
          "form": "그런데 재오픈한다고?",
          "original_form": "그런데 재오픈한다고?",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100029845.1.10",
          "form": "네 내 자료들이 그대로 있대요",
          "original_form": "네 내 자료들이 그대로 있대요",
          "speaker_id": "1",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100029845.1.11",
          "form": "인터넷은 그게 문제다",
          "original_form": "인터넷은 그게 문제다",
          "speaker_id": "2",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100029845.1.12",
          "form": "한번 올리면 영원히 사라지지 않지",
          "original_form": "한번 올리면 영원히 사라지지 않지",
          "speaker_id": "2",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100029845.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 15:03"
        },
        {
          "id": "MDRW2100029845.1.14",
          "form": "그러니 무언걸 업로드 할때는 잘 생각하고 올려야 한다",
          "original_form": "그러니 무언걸 업로드 할때는 잘 생각하고 올려야 한다",
          "speaker_id": "2",
          "time": "20210903 15:03"
        }
