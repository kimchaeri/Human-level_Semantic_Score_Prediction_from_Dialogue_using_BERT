      "utterance": [
        {
          "id": "MDRW2100031486.1.1",
          "form": "요새 지른거잇음?",
          "original_form": "요새 지른거잇음?",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100031486.1.2",
          "form": "매일매일 뭐지를지 고민하고있지..",
          "original_form": "매일매일 뭐지를지 고민하고있지..",
          "speaker_id": "2",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100031486.1.3",
          "form": "매일매안",
          "original_form": "매일매안",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100031486.1.4",
          "form": "? 매일",
          "original_form": "? 매일",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100031486.1.5",
          "form": "엉...쇼핑중독인가바",
          "original_form": "엉...쇼핑중독인가바",
          "speaker_id": "2",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100031486.1.6",
          "form": "나는 블플이 최고의 소비였다",
          "original_form": "나는 블플이 최고의 소비였다",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100031486.1.7",
          "form": "니는 추석때 집에가면 조카선물 사주나",
          "original_form": "니는 추석때 집에가면 조카선물 사주나",
          "speaker_id": "1",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100031486.1.8",
          "form": "그건그렇지..",
          "original_form": "그건그렇지..",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100031486.1.9",
          "form": "궁금쓰",
          "original_form": "궁금쓰",
          "speaker_id": "1",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100031486.1.10",
          "form": "그게 추석이 조카 생일이더라...",
          "original_form": "그게 추석이 조카 생일이더라...",
          "speaker_id": "2",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100031486.1.11",
          "form": "홀 진짜?",
          "original_form": "홀 진짜?",
          "speaker_id": "1",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100031486.1.12",
          "form": "와 1타2피아님(?",
          "original_form": "와 1타2피아님(?",
          "speaker_id": "1",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031486.1.13",
          "form": "엉.. 태블릿해줄까 싶엇는데",
          "original_form": "엉.. 태블릿해줄까 싶엇는데",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031486.1.14",
          "form": "이미 형부가 해줘서 마인크래프트 레고 해줄까싶음",
          "original_form": "이미 형부가 해줘서 마인크래프트 레고 해줄까싶음",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031486.1.15",
          "form": "애가 커서 이제 그런거 쓸 나이네",
          "original_form": "애가 커서 이제 그런거 쓸 나이네",
          "speaker_id": "1",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031486.1.16",
          "form": "요즘애들은 어릴때부터 쓰자나",
          "original_form": "요즘애들은 어릴때부터 쓰자나",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031486.1.17",
          "form": "대박적.. 난 아직도 고마",
          "original_form": "대박적.. 난 아직도 고마",
          "speaker_id": "1",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031486.1.18",
          "form": "고민",
          "original_form": "고민",
          "speaker_id": "1",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031486.1.19",
          "form": "터치세대라고하지...",
          "original_form": "터치세대라고하지...",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031486.1.20",
          "form": "어릴땐 뭐해줌? 애가 어려서 영 감이안옴",
          "original_form": "어릴땐 뭐해줌? 애가 어려서 영 감이안옴",
