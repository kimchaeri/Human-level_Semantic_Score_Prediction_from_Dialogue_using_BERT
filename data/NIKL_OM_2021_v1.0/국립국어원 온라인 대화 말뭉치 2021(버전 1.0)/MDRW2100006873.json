      "utterance": [
        {
          "id": "MDRW2100006873.1.1",
          "form": "남천동 빵천동 아시나요",
          "original_form": "남천동 빵천동 아시나요",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100006873.1.2",
          "form": "가밨어요 티비보고요 빵 거리더라구요",
          "original_form": "가밨어요 티비보고요 빵 거리더라구요",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100006873.1.3",
          "form": "저는 별로였어요",
          "original_form": "저는 별로였어요",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100006873.1.4",
          "form": "네 전통빵집보다는 개인빵집인데 인스타를 겨냥한 느낌",
          "original_form": "네 전통빵집보다는 개인빵집인데 인스타를 겨냥한 느낌",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100006873.1.5",
          "form": "저는 중앙동 백구당요",
          "original_form": "저는 중앙동 백구당요",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100006873.1.6",
          "form": "거긴 오래 되었지요?",
          "original_form": "거긴 오래 되었지요?",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100006873.1.7",
          "form": "네 옥수수빵 일품이에요",
          "original_form": "네 옥수수빵 일품이에요",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100006873.1.8",
          "form": "맛있겠네요 고소하고 달콤하겠어요",
          "original_form": "맛있겠네요 고소하고 달콤하겠어요",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100006873.1.9",
          "form": "비엔씨라고 오래된 토박이도 있어요",
          "original_form": "비엔씨라고 오래된 토박이도 있어요",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100006873.1.10",
          "form": "거기 좋지요 근데 점점 가격이 올라요",
          "original_form": "거기 좋지요 근데 점점 가격이 올라요",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100006873.1.11",
          "form": "팥빵이랑 야채빵  좋아요",
          "original_form": "팥빵이랑 야채빵  좋아요",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100006873.1.12",
          "form": "요즘은 뚜레주르 같은 체인이 아니라 개인빵집 대세네요",
          "original_form": "요즘은 뚜레주르 같은 체인이 아니라 개인빵집 대세네요",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100006873.1.13",
          "form": "마카롱같은곳두요",
          "original_form": "마카롱같은곳두요",
          "speaker_id": "2",
          "time": "20210908 11:09"
