      "utterance": [
        {
          "id": "MDRW2100034251.1.1",
          "form": "치즈케익 만들어줄까?",
          "original_form": "치즈케익 만들어줄까?",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034251.1.2",
          "form": "그래 만들어봐라",
          "original_form": "그래 만들어봐라",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034251.1.3",
          "form": "웅 밀가루 하나도 안들어가는거~",
          "original_form": "웅 밀가루 하나도 안들어가는거~",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034251.1.4",
          "form": "그래 전에 만든거",
          "original_form": "그래 전에 만든거",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034251.1.5",
          "form": "웅웅 ㅋㅋ 맛나징?",
          "original_form": "웅웅 ㅋㅋ 맛나징?",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034251.1.6",
          "form": "그래 맛있었다",
          "original_form": "그래 맛있었다",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034251.1.7",
          "form": "웅 재료도 간단하다~",
          "original_form": "웅 재료도 간단하다~",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034251.1.8",
          "form": "만들어라 먹자",
          "original_form": "만들어라 먹자",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034251.1.9",
          "form": "웅 ㅋ 그럼 엄마가 좀 도와줘~",
          "original_form": "웅 ㅋ 그럼 엄마가 좀 도와줘~",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034251.1.10",
          "form": "그래 알았다",
          "original_form": "그래 알았다",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034251.1.11",
          "form": "계란은 집에 있져?",
          "original_form": "계란은 집에 있져?",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034251.1.12",
          "form": "그래 많다",
          "original_form": "그래 많다",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034251.1.13",
          "form": "웅 그럼 재료 다있는듯",
          "original_form": "웅 그럼 재료 다있는듯",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
