      "utterance": [
        {
          "id": "MDRW2100030943.1.1",
          "form": "아 맞아요 정준 이었어요",
          "original_form": "아 맞아요 정준 이었어요",
          "speaker_id": "2",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030943.1.2",
          "form": "목욕탕 가고 싶네요",
          "original_form": "목욕탕 가고 싶네요",
          "speaker_id": "1",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030943.1.3",
          "form": "팡이 몸 담그는 거 좋아해요",
          "original_form": "팡이 몸 담그는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030943.1.4",
          "form": "바디로션은 잘 안 쓰신다고 했나요",
          "original_form": "바디로션은 잘 안 쓰신다고 했나요",
          "speaker_id": "2",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030943.1.5",
          "form": "탕에",
          "original_form": "탕에",
          "speaker_id": "1",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030943.1.6",
          "form": "베트남에서는 쓸 일이 없어요",
          "original_form": "베트남에서는 쓸 일이 없어요",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030943.1.7",
          "form": "예전에 알로에 바디로션 썼어요",
          "original_form": "예전에 알로에 바디로션 썼어요",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030943.1.8",
          "form": "전인 올리브영에서 한 번씩 바디 로션 세일할 때 사는데요",
          "original_form": "전인 올리브영에서 한 번씩 바디 로션 세일할 때 사는데요",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030943.1.9",
          "form": "자몽 향을 좋아해요",
          "original_form": "자몽 향을 좋아해요",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030943.1.10",
          "form": "아무거나 쓰시나봐요",
          "original_form": "아무거나 쓰시나봐요",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030943.1.11",
          "form": "바디판타지 아라는 제품 이었나",
          "original_form": "바디판타지 아라는 제품 이었나",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030943.1.12",
          "form": "보습이 잘 되는 거 찾아봐야겠어요",
          "original_form": "보습이 잘 되는 거 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030943.1.13",
          "form": "이름이 기억이 안 나는데 세일을 자주 하거든요",
          "original_form": "이름이 기억이 안 나는데 세일을 자주 하거든요",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030943.1.14",
          "form": "아보카도 성분도 좋겠는데요",
          "original_form": "아보카도 성분도 좋겠는데요",
          "speaker_id": "1",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030943.1.15",
          "form": "저는 바디로션은 향을 더 중요시하게 여겨요",
          "original_form": "저는 바디로션은 향을 더 중요시하게 여겨요",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030943.1.16",
          "form": "바세린 좋구요",
          "original_form": "바세린 좋구요",
          "speaker_id": "1",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030943.1.17",
          "form": "그 미국 다단계 거기 이름 뭐죠",
          "original_form": "그 미국 다단계 거기 이름 뭐죠",
          "speaker_id": "2",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030943.1.18",
          "form": "향이 오래 가나요",
          "original_form": "향이 오래 가나요",
          "speaker_id": "1",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030943.1.19",
          "form": "치약 유명한 곳",
          "original_form": "치약 유명한 곳",
          "speaker_id": "2",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030943.1.20",
          "form": "암웨이",
          "original_form": "암웨이",
          "speaker_id": "1",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030943.1.21",
          "form": "거기 제품도 좋죠",
          "original_form": "거기 제품도 좋죠",
          "speaker_id": "1",
          "time": "20210904 14:32"
        },
        {
          "id": "MDRW2100030943.1.22",
          "form": "암웨이에서 나오는 바디로션 짱 좋아요 향도 좋고요",
          "original_form": "암웨이에서 나오는 바디로션 짱 좋아요 향도 좋고요",
          "speaker_id": "2",
          "time": "20210904 14:32"
