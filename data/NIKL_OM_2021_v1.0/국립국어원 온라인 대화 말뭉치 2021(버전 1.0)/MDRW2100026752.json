        {
          "id": "MDRW2100026752.1.1",
          "form": "쇼핑좋아하세요?",
          "original_form": "쇼핑좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026752.1.2",
          "form": "쇼핑좋져",
          "original_form": "쇼핑좋져",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026752.1.3",
          "form": "신나초",
          "original_form": "신나초",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026752.1.4",
          "form": "오프 (?) 온라인?",
          "original_form": "오프 (?) 온라인?",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026752.1.5",
          "form": "11번가 아마존 행사 있답니다",
          "original_form": "11번가 아마존 행사 있답니다",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026752.1.6",
          "form": "그게 무슨 할인소식 인가요?",
          "original_form": "그게 무슨 할인소식 인가요?",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026752.1.7",
          "form": "매달 얼마 결제하는 회원인데",
          "original_form": "매달 얼마 결제하는 회원인데",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026752.1.8",
          "form": "이번에 한달100 원",
          "original_form": "이번에 한달100 원",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026752.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026752.1.10",
          "form": "알아요",
          "original_form": "알아요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026752.1.11",
          "form": "관심있으세요?",
          "original_form": "관심있으세요?",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026752.1.12",
          "form": "저는 11번가 안해서",
          "original_form": "저는 11번가 안해서",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026752.1.13",
          "form": "무배송",
          "original_form": "무배송",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026752.1.14",
          "form": "5000 ㅇ할인",
          "original_form": "5000 ㅇ할인",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026752.1.15",
          "form": "가입하심요?",
          "original_form": "가입하심요?",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026752.1.16",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026752.1.17",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026752.1.18",
          "form": "자동결제가무서버서",
          "original_form": "자동결제가무서버서",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026752.1.19",
          "form": "주로 무슨 쇼핑하세요??",
          "original_form": "주로 무슨 쇼핑하세요??",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026752.1.20",
          "form": "뭐를 사시나요?",
          "original_form": "뭐를 사시나요?",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026752.1.21",
          "form": "애들장난감요",
          "original_form": "애들장난감요",
          "speaker_id": "1",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026752.1.22",
          "form": "저는 물티슈를 그렇게 사요",
          "original_form": "저는 물티슈를 그렇게 사요",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026752.1.23",
          "form": "떨어지면 죽는줄 앍ᆢ",
          "original_form": "떨어지면 죽는줄 앍ᆢ",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026752.1.24",
          "form": "필요하니깐요",
          "original_form": "필요하니깐요",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026752.1.25",
          "form": "내일뵈요 취침시간이네요",
          "original_form": "내일뵈요 취침시간이네요",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026752.1.26",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 00:06"
        }
