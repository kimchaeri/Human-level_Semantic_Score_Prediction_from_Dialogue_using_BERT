      "utterance": [
        {
          "id": "MDRW2100013382.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013382.1.2",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013382.1.3",
          "form": "이제 곧 하려고요!!",
          "original_form": "이제 곧 하려고요!!",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100013382.1.4",
          "form": "저녁 뭐 드실 예정이신가요",
          "original_form": "저녁 뭐 드실 예정이신가요",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100013382.1.5",
          "form": "어묵국을 먹게될것같아요. name1은요?",
          "original_form": "어묵국을 먹게될것같아요. &name1&은요?",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100013382.1.6",
          "form": "어묵국 너무 좋네요",
          "original_form": "어묵국 너무 좋네요",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100013382.1.7",
          "form": "전 닭갈비 먹을 예정이에요",
          "original_form": "전 닭갈비 먹을 예정이에요",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100013382.1.8",
          "form": "어묵국 만들어 드시나요?",
          "original_form": "어묵국 만들어 드시나요?",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100013382.1.9",
          "form": "아니요 ㅜㅜ 엄마가 해주시는거라서요 ㅎㅎㅎ\r\n저같은경우엔 살짝 조미료넣고 하고싶긴하네요",
          "original_form": "아니요 ㅜㅜ 엄마가 해주시는거라서요 ㅎㅎㅎ\r\n저같은경우엔 살짝 조미료넣고 하고싶긴하네요",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100013382.1.10",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100013382.1.11",
          "form": "조미료 넣는게 확실히 맛있는데 안넣으시나보구나",
          "original_form": "조미료 넣는게 확실히 맛있는데 안넣으시나보구나",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100013382.1.12",
          "form": "ㅋㅋㅋㅋ 그러면 엄마한테 혼나긴핮ㅅ",
          "original_form": "ㅋㅋㅋㅋ 그러면 엄마한테 혼나긴핮ㅅ",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100013382.1.13",
          "form": "그럴 수 있죠ㅎㅎ",
          "original_form": "그럴 수 있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100013382.1.14",
          "form": "엄마는 요리할때 조미료 안쓰시는데 그래도 맛잇어요",
          "original_form": "엄마는 요리할때 조미료 안쓰시는데 그래도 맛잇어요",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100013382.1.15",
          "form": "그래도 떡볶이나 몇가지 요리는 조미료가 꼭 필요하긴하죠.ㅎㅎ",
          "original_form": "그래도 떡볶이나 몇가지 요리는 조미료가 꼭 필요하긴하죠.ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100013382.1.16",
          "form": "맞아요. 엄마 요리는 뭘 해도 다 맛있는 거 같아요",
          "original_form": "맞아요. 엄마 요리는 뭘 해도 다 맛있는 거 같아요",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100013382.1.17",
          "form": "공감합니다ㅎㅎ",
          "original_form": "공감합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100013382.1.18",
          "form": "맞아요. 저는 특히 계란말이 좋아해요 ㅋㅋ",
          "original_form": "맞아요. 저는 특히 계란말이 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100013382.1.19",
          "form": "떡볶이 좋아하시나요",
          "original_form": "떡볶이 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100013382.1.20",
          "form": "넹ㅋㅋㅋ",
          "original_form": "넹ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100013382.1.21",
          "form": "앗 계란말이도 좋아하시는구나",
          "original_form": "앗 계란말이도 좋아하시는구나",
