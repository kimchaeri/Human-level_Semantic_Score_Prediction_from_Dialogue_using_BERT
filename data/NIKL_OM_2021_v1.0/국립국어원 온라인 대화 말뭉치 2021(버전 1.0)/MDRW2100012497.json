      "utterance": [
        {
          "id": "MDRW2100012497.1.1",
          "form": "여행 어디 다녀보셨어요?",
          "original_form": "여행 어디 다녀보셨어요?",
          "speaker_id": "1",
          "time": "20210824 22:33"
        },
        {
          "id": "MDRW2100012497.1.2",
          "form": "여행은 정말 많이다녔네요ㅎㅍ",
          "original_form": "여행은 정말 많이다녔네요ㅎㅍ",
          "speaker_id": "2",
          "time": "20210824 22:39"
        },
        {
          "id": "MDRW2100012497.1.3",
          "form": "해외는 일때문에 가보고 국내는 여행동호회할정도로 많이 다녔어요ㅎ",
          "original_form": "해외는 일때문에 가보고 국내는 여행동호회할정도로 많이 다녔어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:40"
        },
        {
          "id": "MDRW2100012497.1.4",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 22:40"
        },
        {
          "id": "MDRW2100012497.1.5",
          "form": "가고싶은 여행지는 많은데 시간과 비용이 안되서 늘 후회해요",
          "original_form": "가고싶은 여행지는 많은데 시간과 비용이 안되서 늘 후회해요",
          "speaker_id": "1",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100012497.1.6",
          "form": "많은 여행 경험을 하셨네요",
          "original_form": "많은 여행 경험을 하셨네요",
          "speaker_id": "1",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100012497.1.7",
          "form": "기억나는 여행이 어디인가요?",
          "original_form": "기억나는 여행이 어디인가요?",
          "speaker_id": "1",
          "time": "20210824 22:42"
        },
        {
          "id": "MDRW2100012497.1.8",
          "form": "강원도 아닐까싶네요ㅎ",
          "original_form": "강원도 아닐까싶네요ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100012497.1.9",
          "form": "그때 5일정도 강원도 곳곳을 돌았어요ㅎㅎ",
          "original_form": "그때 5일정도 강원도 곳곳을 돌았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012497.1.10",
          "form": "정선, 동해, 삼척, 뭐 이름만들어도 아는곳들ㅎㅎ",
          "original_form": "정선, 동해, 삼척, 뭐 이름만들어도 아는곳들ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:45"
        },
        {
          "id": "MDRW2100012497.1.11",
          "form": "저도 여행을 많이 다니고 추억도 쌓고 해야하는네 삶에 대해 반성하게 되네요",
          "original_form": "저도 여행을 많이 다니고 추억도 쌓고 해야하는네 삶에 대해 반성하게 되네요",
          "speaker_id": "1",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012497.1.12",
          "form": "저는 제주도 다시 가고싶은데 힐링좀 하고 오고싶어요",
          "original_form": "저는 제주도 다시 가고싶은데 힐링좀 하고 오고싶어요",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012497.1.13",
          "form": "전 자연경관이너무 좋았어요ㅎㅎ 깔딱고개도 좋고 산위에서 드라이",
          "original_form": "전 자연경관이너무 좋았어요ㅎㅎ 깔딱고개도 좋고 산위에서 드라이",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012497.1.14",
          "form": "왜요? 무슨일있으세요?",
          "original_form": "왜요? 무슨일있으세요?",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012497.1.15",
          "form": "그냥 자연과 함께하는 아름다운 섬이라 과거의 추억도 있고",
          "original_form": "그냥 자연과 함께하는 아름다운 섬이라 과거의 추억도 있고",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012497.1.16",
          "form": "군대는 강원도에서 복무해서.. 갈 마음이 없고요 ㅋㅋ",
          "original_form": "군대는 강원도에서 복무해서.. 갈 마음이 없고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012497.1.17",
          "form": "나중에 여행 가보고싶어요?",
          "original_form": "나중에 여행 가보고싶어요?",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012497.1.18",
          "form": "ㅎㅎ 그러시겟네요ㅎㅎ",
          "original_form": "ㅎㅎ 그러시겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100012497.1.19",
          "form": "과거추억이라함은 아름다운 추억이겟죠?ㄹㅎ",
          "original_form": "과거추억이라함은 아름다운 추억이겟죠?ㄹㅎ",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100012497.1.20",
          "form": "지금까지 마음속에 품은 곳이라 제주도 냄새를 맡고싶네요",
          "original_form": "지금까지 마음속에 품은 곳이라 제주도 냄새를 맡고싶네요",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012497.1.21",
          "form": "해와여행도 혼자 다녀오고 싶고요",
          "original_form": "해와여행도 혼자 다녀오고 싶고요",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012497.1.22",
          "form": "한번 용기내 가보세요ㅎ 코로나 끝나고요ㅎㅎ",
          "original_form": "한번 용기내 가보세요ㅎ 코로나 끝나고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012497.1.23",
          "form": "혼자가는 여행이 정말 찐이에요ㅎ",
          "original_form": "혼자가는 여행이 정말 찐이에요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012497.1.24",
          "form": "마음은 있어도 실행에 옮기는건 쉬운 선택이 아니라서..",
          "original_form": "마음은 있어도 실행에 옮기는건 쉬운 선택이 아니라서..",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012497.1.25",
          "form": "같이 갈사람도 없고",
          "original_form": "같이 갈사람도 없고",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012497.1.26",
          "form": "여행도 혼자 다닐 수있다고 생각하나요?",
          "original_form": "여행도 혼자 다닐 수있다고 생각하나요?",
          "speaker_id": "1",
          "time": "20210824 23:28"
        },
        {
          "id": "MDRW2100012497.1.27",
          "form": "저 맨날 혼자다녀요ㅎ",
          "original_form": "저 맨날 혼자다녀요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:30"
        },
        {
          "id": "MDRW2100012497.1.28",
          "form": "한번도 안가보셨어요?",
          "original_form": "한번도 안가보셨어요?",
          "speaker_id": "2",
          "time": "20210824 23:31"
        },
        {
          "id": "MDRW2100012497.1.29",
          "form": "마음이 소심해서.. 혼자는 못다닙니다  위험에 처할거같아요 ㅋㅋ",
          "original_form": "마음이 소심해서.. 혼자는 못다닙니다  위험에 처할거같아요 ㅋㅋ",
          "speaker_id": "1",
