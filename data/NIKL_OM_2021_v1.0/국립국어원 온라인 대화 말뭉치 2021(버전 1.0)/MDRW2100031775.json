      "utterance": [
        {
          "id": "MDRW2100031775.1.1",
          "form": "조카 잘해봐야",
          "original_form": "조카 잘해봐야",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100031775.1.2",
          "form": "아무 소용이 ㅎㅎ",
          "original_form": "아무 소용이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100031775.1.3",
          "form": "ㅋㅋㅋ 고모 요리는맛있는데 여긴너무 자연주의에요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 고모 요리는맛있는데 여긴너무 자연주의에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:27"
        },
        {
          "id": "MDRW2100031775.1.4",
          "form": "고모가 땅을 치시겠어요",
          "original_form": "고모가 땅을 치시겠어요",
          "speaker_id": "2",
          "time": "20210905 00:37"
        },
        {
          "id": "MDRW2100031775.1.5",
          "form": "과자가..먹고싶어요....",
          "original_form": "과자가..먹고싶어요....",
          "speaker_id": "1",
          "time": "20210905 00:37"
        },
        {
          "id": "MDRW2100031775.1.6",
          "form": "과자",
          "original_form": "과자",
          "speaker_id": "2",
          "time": "20210905 01:18"
        },
        {
          "id": "MDRW2100031775.1.7",
          "form": "뭐가 최애인가요",
          "original_form": "뭐가 최애인가요",
          "speaker_id": "2",
          "time": "20210905 01:18"
        },
        {
          "id": "MDRW2100031775.1.8",
          "form": "과자 많은 과자를 좋아하지만 고래밥이 먹고싶네요",
          "original_form": "과자 많은 과자를 좋아하지만 고래밥이 먹고싶네요",
          "speaker_id": "1",
          "time": "20210905 01:19"
        },
        {
          "id": "MDRW2100031775.1.9",
          "form": "에이 전 포스틱인데",
          "original_form": "에이 전 포스틱인데",
          "speaker_id": "2",
          "time": "20210905 01:41"
        },
        {
          "id": "MDRW2100031775.1.10",
          "form": "포스틱도 맛있죠 ㅋㅋㅋ",
          "original_form": "포스틱도 맛있죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:42"
        },
        {
          "id": "MDRW2100031775.1.11",
          "form": "포스틱 쉐이크 보셨어요? ㅋㅋ",
          "original_form": "포스틱 쉐이크 보셨어요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:42"
        },
        {
          "id": "MDRW2100031775.1.12",
          "form": "그게 뭐쥬....",
          "original_form": "그게 뭐쥬....",
          "speaker_id": "2",
          "time": "20210905 01:43"
        },
        {
          "id": "MDRW2100031775.1.13",
          "form": "이디야에서 괴랄한 음료를 만들었어요..",
          "original_form": "이디야에서 괴랄한 음료를 만들었어요..",
          "speaker_id": "1",
          "time": "20210905 01:43"
        },
        {
          "id": "MDRW2100031775.1.14",
          "form": "아니!왜...",
          "original_form": "아니!왜...",
          "speaker_id": "2",
          "time": "20210905 01:46"
        },
        {
          "id": "MDRW2100031775.1.15",
          "form": "도전정신이 생기는 음료더라구요 ㅋㅋ",
          "original_form": "도전정신이 생기는 음료더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100031775.1.16",
          "form": "전 패스하겠습니다 ㅎㅎ",
          "original_form": "전 패스하겠습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100031775.1.17",
          "form": "ㅋㅋㅋ감튀에 쉐이크라면 괜찮을것도 같은데요!",
          "original_form": "ㅋㅋㅋ감튀에 쉐이크라면 괜찮을것도 같은데요!",
          "speaker_id": "1",
          "time": "20210905 01:49"
        },
        {
          "id": "MDRW2100031775.1.18",
          "form": "섞였자나요....",
          "original_form": "섞였자나요....",
          "speaker_id": "2",
          "time": "20210905 01:50"
        },
        {
          "id": "MDRW2100031775.1.19",
          "form": "포스틱을 넣고 갈은건 아니에요!",
          "original_form": "포스틱을 넣고 갈은건 아니에요!",
          "speaker_id": "1",
          "time": "20210905 01:51"
        }
