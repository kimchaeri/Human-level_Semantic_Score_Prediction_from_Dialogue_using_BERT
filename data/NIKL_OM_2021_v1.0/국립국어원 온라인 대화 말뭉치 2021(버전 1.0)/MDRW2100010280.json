      "utterance": [
        {
          "id": "MDRW2100010280.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210823 16:24"
        },
        {
          "id": "MDRW2100010280.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010280.1.3",
          "form": "정모 해보셨나요?",
          "original_form": "정모 해보셨나요?",
          "speaker_id": "1",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010280.1.4",
          "form": "네~ 코로나 전엔 많이 했는데 코로나 이후로 한번도 못했네요",
          "original_form": "네~ 코로나 전엔 많이 했는데 코로나 이후로 한번도 못했네요",
          "speaker_id": "2",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010280.1.5",
          "form": "헐 어떤정모인가요",
          "original_form": "헐 어떤정모인가요",
          "speaker_id": "1",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010280.1.6",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010280.1.7",
          "form": "카페에서 소모임만들었는데 거기 있는 사람들끼리 만났어요 ㅎ",
          "original_form": "카페에서 소모임만들었는데 거기 있는 사람들끼리 만났어요 ㅎ",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010280.1.8",
          "form": "와우 어떤",
          "original_form": "와우 어떤",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010280.1.9",
          "form": "소모임이었나요?",
          "original_form": "소모임이었나요?",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010280.1.10",
          "form": "그냥 수다 모임 같은거였어요  ㅎ",
          "original_form": "그냥 수다 모임 같은거였어요  ㅎ",
          "speaker_id": "2",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100010280.1.11",
          "form": "저는 한번도",
          "original_form": "저는 한번도",
          "speaker_id": "1",
          "time": "20210823 16:50"
        },
        {
          "id": "MDRW2100010280.1.12",
          "form": "안해봤답니다",
          "original_form": "안해봤답니다",
          "speaker_id": "1",
          "time": "20210823 16:51"
        },
        {
          "id": "MDRW2100010280.1.13",
          "form": "정모 한번도 안해보셨군요",
          "original_form": "정모 한번도 안해보셨군요",
          "speaker_id": "2",
          "time": "20210823 16:52"
        },
        {
          "id": "MDRW2100010280.1.14",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:59"
        },
        {
          "id": "MDRW2100010280.1.15",
          "form": "기회가 없었어요",
          "original_form": "기회가 없었어요",
          "speaker_id": "1",
          "time": "20210823 16:59"
        },
        {
          "id": "MDRW2100010280.1.16",
          "form": "한번 해보세요 사람들 닉넴이랑 실제 이름이랑 매칭시키는데 엄청 재미있어요 ㅎ",
          "original_form": "한번 해보세요 사람들 닉넴이랑 실제 이름이랑 매칭시키는데 엄청 재미있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100010280.1.17",
          "form": "ㅋㅋㅋ 근데 어떤공통분모로",
          "original_form": "ㅋㅋㅋ 근데 어떤공통분모로",
          "speaker_id": "1",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010280.1.18",
          "form": "만나야될지를",
          "original_form": "만나야될지를",
          "speaker_id": "1",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010280.1.19",
          "form": "취미 같은거 없으세요?",
          "original_form": "취미 같은거 없으세요?",
          "speaker_id": "2",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010280.1.20",
          "form": "아직은 아니고",
          "original_form": "아직은 아니고",
          "speaker_id": "1",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010280.1.21",
          "form": "등산해보고싶어요ㅋㅋㅋ",
          "original_form": "등산해보고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010280.1.22",
          "form": "아~ 등산 하시면 등산 동호회에서 엄청 오거든요 그럴때 합류해보세요",
          "original_form": "아~ 등산 하시면 등산 동호회에서 엄청 오거든요 그럴때 합류해보세요",
          "speaker_id": "2",
          "time": "20210823 17:43"
        }
      ]
