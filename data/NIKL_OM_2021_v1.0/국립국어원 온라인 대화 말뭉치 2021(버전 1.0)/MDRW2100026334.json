      "utterance": [
        {
          "id": "MDRW2100026334.1.1",
          "form": "야구 축구 농구 배구 어떤게 젤 좋으신가요?",
          "original_form": "야구 축구 농구 배구 어떤게 젤 좋으신가요?",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026334.1.2",
          "form": "음...............",
          "original_form": "음...............",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026334.1.3",
          "form": "야구인거같아요",
          "original_form": "야구인거같아요",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026334.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100026334.1.5",
          "form": "저도 야구",
          "original_form": "저도 야구",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026334.1.6",
          "form": "어떤 팀 응원하세요?",
          "original_form": "어떤 팀 응원하세요?",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026334.1.7",
          "form": "전 부산사람이라 자동 롯데...ㅎ",
          "original_form": "전 부산사람이라 자동 롯데...ㅎ",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026334.1.8",
          "form": "아~~부산갈메기~~~~",
          "original_form": "아~~부산갈메기~~~~",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026334.1.9",
          "form": "ㅋㅋㅋㅋㅋ맞아욬ㅋㅋㅋ 노래 자동재생되네요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아욬ㅋㅋㅋ 노래 자동재생되네요",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026334.1.10",
          "form": "롯데응원문화재밌었는데",
          "original_form": "롯데응원문화재밌었는데",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026334.1.11",
          "form": "요새 관중이 없어 아쉽네요",
          "original_form": "요새 관중이 없어 아쉽네요",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026334.1.12",
          "form": "아 그쵸 ㅠ 야구에 빠진 친구들이 아주 힘들어해요",
          "original_form": "아 그쵸 ㅠ 야구에 빠진 친구들이 아주 힘들어해요",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026334.1.13",
          "form": "응원못가서 ㅠ",
          "original_form": "응원못가서 ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026334.1.14",
          "form": "저도 어릴땐 정말자주갔었거든요",
          "original_form": "저도 어릴땐 정말자주갔었거든요",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026334.1.15",
          "form": "한국시리즈도 가고",
          "original_form": "한국시리즈도 가고",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026334.1.16",
          "form": "와 한국시리즈표구하신게 더 대단한데요????",
          "original_form": "와 한국시리즈표구하신게 더 대단한데요????",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026334.1.17",
          "form": "맞아요 돈이안아까웠어요",
          "original_form": "맞아요 돈이안아까웠어요",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026334.1.18",
          "form": "잘할때 돈 많이들어가는ㅋㄱ",
          "original_form": "잘할때 돈 많이들어가는ㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026334.1.19",
          "form": "다른때는 안가두 진짜 한국시리즈때가면 존잼일거같아요 ㅋㅋ",
          "original_form": "다른때는 안가두 진짜 한국시리즈때가면 존잼일거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026334.1.20",
          "form": "야구 보는 재미가 쏠쏠-",
          "original_form": "야구 보는 재미가 쏠쏠-",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026334.1.21",
          "form": "잘할때 시작해서 폭빠졋었어요",
          "original_form": "잘할때 시작해서 폭빠졋었어요",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026334.1.22",
          "form": "퇴근후 야구장출근ㅋㅋ",
          "original_form": "퇴근후 야구장출근ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026334.1.23",
          "form": "크- 회사가는 이유는 야구장을 가기위함이네요 ㅎㅎ",
          "original_form": "크- 회사가는 이유는 야구장을 가기위함이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:54"
        }
      ]
