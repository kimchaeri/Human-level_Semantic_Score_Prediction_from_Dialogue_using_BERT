      "utterance": [
        {
          "id": "MDRW2100008467.1.1",
          "form": "요즘 저는 클튜 쓰는데 name2 님은 어떤 툴 쓰시나요?",
          "original_form": "요즘 저는 클튜 쓰는데 &name2& 님은 어떤 툴 쓰시나요?",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008467.1.2",
          "form": "엇 저도 클튜써요",
          "original_form": "엇 저도 클튜써요",
          "speaker_id": "2",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008467.1.3",
          "form": "소재가 너무 좋아요",
          "original_form": "소재가 너무 좋아요",
          "speaker_id": "2",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008467.1.4",
          "form": "가격대비 너무 만족스럽고",
          "original_form": "가격대비 너무 만족스럽고",
          "speaker_id": "2",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008467.1.5",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008467.1.6",
          "form": "그림 그리는 커뮤니티 속해 있는 거 계세요?",
          "original_form": "그림 그리는 커뮤니티 속해 있는 거 계세요?",
          "speaker_id": "1",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008467.1.7",
          "form": "저는 크로키 소모임 했었는데 흐지부지 끝났어요ㅜ",
          "original_form": "저는 크로키 소모임 했었는데 흐지부지 끝났어요ㅜ",
          "speaker_id": "1",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008467.1.8",
          "form": "앗 저도 예전에 해봤는데",
          "original_form": "앗 저도 예전에 해봤는데",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008467.1.9",
          "form": "오래 이어지질 않더라고요ㅠㅠ",
          "original_form": "오래 이어지질 않더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008467.1.10",
          "form": "친한사람들끼리 모여했더니",
          "original_form": "친한사람들끼리 모여했더니",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008467.1.11",
          "form": "다 그러려니 하게돼서..",
          "original_form": "다 그러려니 하게돼서..",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008467.1.12",
          "form": "챌린저스라고 아세요?",
          "original_form": "챌린저스라고 아세요?",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008467.1.13",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008467.1.14",
          "form": "소개해주실 수 있나요?",
          "original_form": "소개해주실 수 있나요?",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008467.1.15",
          "form": "어플인데 돈을 미리 결제해두고",
          "original_form": "어플인데 돈을 미리 결제해두고",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100008467.1.16",
          "form": "100퍼 달성하면 상금이 나오고",
          "original_form": "100퍼 달성하면 상금이 나오고",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100008467.1.17",
          "form": "달성 못하면 달성한 만큼만 돌려받는 그런 어플이에요",
          "original_form": "달성 못하면 달성한 만큼만 돌려받는 그런 어플이에요",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100008467.1.18",
          "form": "여러사람들하고 모여서하는데",
          "original_form": "여러사람들하고 모여서하는데",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100008467.1.19",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008467.1.20",
          "form": "크로키 모임도 생각있으시면 그쪽해서 해보셔도 좋을것같아요",
          "original_form": "크로키 모임도 생각있으시면 그쪽해서 해보셔도 좋을것같아요",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008467.1.21",
          "form": "저는 독서습관 거기서 잡았거든요",
          "original_form": "저는 독서습관 거기서 잡았거든요",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008467.1.22",
          "form": "아니면 오픈카톡? 모임도 자주들어가고",
          "original_form": "아니면 오픈카톡? 모임도 자주들어가고",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008467.1.23",
          "form": "오 완전 꿀팁이네요",
          "original_form": "오 완전 꿀팁이네요",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008467.1.24",
          "form": "혹시 그림 그리시면 자캐 커뮤 뛰어본 적 있으세요?",
          "original_form": "혹시 그림 그리시면 자캐 커뮤 뛰어본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008467.1.25",
          "form": "앗 있습니다 ㅋㅋㅋ",
          "original_form": "앗 있습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008467.1.26",
          "form": "커뮤뛰세요?",
          "original_form": "커뮤뛰세요?",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008467.1.27",
          "form": "한참 공부하느라 안 뛰다가 올해 초에 잠깐 뛰엇는데",
          "original_form": "한참 공부하느라 안 뛰다가 올해 초에 잠깐 뛰엇는데",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008467.1.28",
          "form": "역시 재밌더라고요",
          "original_form": "역시 재밌더라고요",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008467.1.29",
          "form": "첫 커 몇 년 전이었는지 물어봐도 될까요?",
          "original_form": "첫 커 몇 년 전이었는지 물어봐도 될까요?",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008467.1.30",
          "form": "저는 성인된 이후에 시작해서",
          "original_form": "저는 성인된 이후에 시작해서",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008467.1.31",
          "form": "한 2년 됐나?",
          "original_form": "한 2년 됐나?",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008467.1.32",
          "form": "3년됐나 그래요",
          "original_form": "3년됐나 그래요",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008467.1.33",
          "form": "지금은 저도 낡아서 ㅎ...",
          "original_form": "지금은 저도 낡아서 ㅎ...",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008467.1.34",
          "form": "저는 5~6년 전에 뛰다가 올해 다시 뛴 거였거든요",
          "original_form": "저는 5~6년 전에 뛰다가 올해 다시 뛴 거였거든요",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008467.1.35",
          "form": "요즘에는 밴드나 카페에서 많이 하더라고요",
          "original_form": "요즘에는 밴드나 카페에서 많이 하더라고요",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008467.1.36",
          "form": "밴드 커뮤 선호하시나요?",
          "original_form": "밴드 커뮤 선호하시나요?",
          "speaker_id": "1",
          "time": "20210822 16:17"
        }
      ]
    }
