      "utterance": [
        {
          "id": "MDRW2100038192.1.1",
          "form": "헐 이도현 !!! 저두여 !!!!",
          "original_form": "헐 이도현 !!! 저두여 !!!!",
          "speaker_id": "2",
          "time": "20210902 00:26"
        },
        {
          "id": "MDRW2100038192.1.2",
          "form": "스위트홈 보고 완전 반해버렸잔ㄹ아요 저...",
          "original_form": "스위트홈 보고 완전 반해버렸잔ㄹ아요 저...",
          "speaker_id": "1",
          "time": "20210902 00:27"
        },
        {
          "id": "MDRW2100038192.1.3",
          "form": "하 저두 거기서 첨 알고.. 그 오월의 청춘 아시나여..",
          "original_form": "하 저두 거기서 첨 알고.. 그 오월의 청춘 아시나여..",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038192.1.4",
          "form": "저.. 그거 엄청 슬프대서 안 봤는데",
          "original_form": "저.. 그거 엄청 슬프대서 안 봤는데",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038192.1.5",
          "form": "재밌나요...?",
          "original_form": "재밌나요...?",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038192.1.6",
          "form": "재미써요 ㅜㅜㅜ 근더 슬퍼요 ㅜㅜㅜ",
          "original_form": "재미써요 ㅜㅜㅜ 근더 슬퍼요 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100038192.1.7",
          "form": "슬픈데 이도현 좋아하면",
          "original_form": "슬픈데 이도현 좋아하면",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100038192.1.8",
          "form": "보셔야합니닷",
          "original_form": "보셔야합니닷",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100038192.1.9",
          "form": "ㅠ.ㅠ name2님이 그러니까 궁금해지잖아요...",
          "original_form": "ㅠ.ㅠ &name2&님이 그러니까 궁금해지잖아요...",
          "speaker_id": "1",
          "time": "20210902 08:15"
        },
        {
          "id": "MDRW2100038192.1.10",
          "form": "오늘 한 번... 봐보겠습니다...",
          "original_form": "오늘 한 번... 봐보겠습니다...",
          "speaker_id": "1",
          "time": "20210902 08:15"
        },
        {
          "id": "MDRW2100038192.1.11",
          "form": "꼭꼭 보셔요...!",
          "original_form": "꼭꼭 보셔요...!",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100038192.1.12",
          "form": "18 어게인은 보셨나요????",
          "original_form": "18 어게인은 보셨나요????",
