      "utterance": [
        {
          "id": "MDRW2100022365.1.1",
          "form": "근데 참치뽈찜이 990원에 파는데 ㄷㄷ",
          "original_form": "근데 참치뽈찜이 990원에 파는데 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022365.1.2",
          "form": "대박..",
          "original_form": "대박..",
          "speaker_id": "1",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022365.1.3",
          "form": "쿠폰 적용하면 590원.....",
          "original_form": "쿠폰 적용하면 590원.....",
          "speaker_id": "2",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022365.1.4",
          "form": "안살수가 없었겠네요",
          "original_form": "안살수가 없었겠네요",
          "speaker_id": "1",
          "time": "20210829 13:08"
        },
        {
          "id": "MDRW2100022365.1.5",
          "form": "아니 490원이었어요 ㅋㅋㅋ",
          "original_form": "아니 490원이었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022365.1.6",
          "form": "그래서 배송비 2500원 내고 29990원에 샀어요",
          "original_form": "그래서 배송비 2500원 내고 29990원에 샀어요",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022365.1.7",
          "form": "엄청난 가격이네요",
          "original_form": "엄청난 가격이네요",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022365.1.8",
          "form": "29990이면ㅋㅋㅋㅋ",
          "original_form": "29990이면ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022365.1.9",
          "form": "그쵸? ㅋㅋ 누가 공유해줘서 알았어요",
          "original_form": "그쵸? ㅋㅋ 누가 공유해줘서 알았어요",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022365.1.10",
          "form": "오타인거알아요",
          "original_form": "오타인거알아요",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022365.1.11",
          "form": "앗 2990원이에요~",
          "original_form": "앗 2990원이에요~",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022365.1.12",
          "form": "대박이다 맛이 궁금하네요",
          "original_form": "대박이다 맛이 궁금하네요",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022365.1.13",
          "form": "기대가 크겠어요",
          "original_form": "기대가 크겠어요",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022365.1.14",
          "form": "근데 이게 또 참치의 특수부위래서 3천원주고 살 가치 있다 생각했어요",
          "original_form": "근데 이게 또 참치의 특수부위래서 3천원주고 살 가치 있다 생각했어요",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022365.1.15",
          "form": "원래는 만얼마더라구요 가격이",
          "original_form": "원래는 만얼마더라구요 가격이",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022365.1.16",
          "form": "와 완전 거저네요",
          "original_form": "와 완전 거저네요",
          "speaker_id": "1",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022365.1.17",
          "form": "그래서 품절되기전에 호다닥 구매했죠 히힛",
          "original_form": "그래서 품절되기전에 호다닥 구매했죠 히힛",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022365.1.18",
          "form": "양념맛 진짜ㅈ궁금하다",
          "original_form": "양념맛 진짜ㅈ궁금하다",
          "speaker_id": "1",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022365.1.19",
          "form": "갈비맛이라니",
          "original_form": "갈비맛이라니",
          "speaker_id": "1",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022365.1.20",
          "form": "제가 이따 먹어보고 알려드릴게요",
          "original_form": "제가 이따 먹어보고 알려드릴게요",
