      "utterance": [
        {
          "id": "MDRW2100014767.1.1",
          "form": "맨.인더다크 라고 영화알아?",
          "original_form": "맨.인더다크 라고 영화알아?",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014767.1.2",
          "form": "그거 1편 봤었는데 이번에 2편 나오더라",
          "original_form": "그거 1편 봤었는데 이번에 2편 나오더라",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014767.1.3",
          "form": "영화관에서 보고싶어",
          "original_form": "영화관에서 보고싶어",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014767.1.4",
          "form": "아니 모르겠는데?",
          "original_form": "아니 모르겠는데?",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100014767.1.5",
          "form": "장르가 뭐야??",
          "original_form": "장르가 뭐야??",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100014767.1.6",
          "form": "공포스릴러?",
          "original_form": "공포스릴러?",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100014767.1.7",
          "form": "저거 기숙사에서 룸메언니랑 같이 봤었는데",
          "original_form": "저거 기숙사에서 룸메언니랑 같이 봤었는데",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100014767.1.8",
          "form": "심장 두근두근",
          "original_form": "심장 두근두근",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100014767.1.9",
          "form": "윽",
          "original_form": "윽",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100014767.1.10",
          "form": "귀신얘긴아니고",
          "original_form": "귀신얘긴아니고",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100014767.1.11",
          "form": "무서워",
          "original_form": "무서워",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100014767.1.12",
          "form": "벌써 싫어....",
          "original_form": "벌써 싫어....",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100014767.1.13",
          "form": "ㅋㅋㅋㅋ나는 귀신 아닌거면 그렇게 안무서움",
          "original_form": "ㅋㅋㅋㅋ나는 귀신 아닌거면 그렇게 안무서움",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100014767.1.14",
          "form": "약간 악마이런거ㅋㅋㅋ",
          "original_form": "약간 악마이런거ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100014767.1.15",
          "form": "왜 그런거 돈주고 보는거야",
          "original_form": "왜 그런거 돈주고 보는거야",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100014767.1.16",
          "form": "요즘 놀면뭐하니 유튜브로 매일봄",
          "original_form": "요즘 놀면뭐하니 유튜브로 매일봄",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100014767.1.17",
          "form": "내 밥친구",
          "original_form": "내 밥친구",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100014767.1.18",
          "form": "그 이번에 무한도전 다시 모였다고 하는 거 같던데",
          "original_form": "그 이번에 무한도전 다시 모였다고 하는 거 같던데",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100014767.1.19",
          "form": "맞아?",
          "original_form": "맞아?",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100014767.1.20",
          "form": "그거 지금 라면먹으면서 보려고",
          "original_form": "그거 지금 라면먹으면서 보려고",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100014767.1.21",
          "form": "멤버들 모인것같음",
          "original_form": "멤버들 모인것같음",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100014767.1.22",
          "form": "보고 후기 좀",
          "original_form": "보고 후기 좀",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100014767.1.23",
          "form": "나는 좀 노잼이야",
          "original_form": "나는 좀 노잼이야",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100014767.1.24",
          "form": "계속 탁구만쳐ㅋㅋㅋㅋㅋ",
          "original_form": "계속 탁구만쳐ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100014767.1.25",
          "form": "지루",
          "original_form": "지루",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100014767.1.26",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100014767.1.27",
          "form": "말로 티키타카가 안해?",
          "original_form": "말로 티키타카가 안해?",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100014767.1.28",
          "form": "그 재미잖아",
          "original_form": "그 재미잖아",
          "speaker_id": "2",
          "time": "20210825 23:58"
        }
      ]
