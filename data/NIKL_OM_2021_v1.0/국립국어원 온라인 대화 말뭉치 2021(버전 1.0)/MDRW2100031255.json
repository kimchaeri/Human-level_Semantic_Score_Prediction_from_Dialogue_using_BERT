      "utterance": [
        {
          "id": "MDRW2100031255.1.1",
          "form": "나 방금 포키 버터카라멜 맛이라는거 먹었는데 핵노맛",
          "original_form": "나 방금 포키 버터카라멜 맛이라는거 먹었는데 핵노맛",
          "speaker_id": "2",
          "time": "20210904 19:20"
        },
        {
          "id": "MDRW2100031255.1.2",
          "form": "헐 그거 기대했는데!",
          "original_form": "헐 그거 기대했는데!",
          "speaker_id": "1",
          "time": "20210904 19:26"
        },
        {
          "id": "MDRW2100031255.1.3",
          "form": "나의 돈을 구해주었군  땡큐",
          "original_form": "나의 돈을 구해주었군  땡큐",
          "speaker_id": "1",
          "time": "20210904 19:26"
        },
        {
          "id": "MDRW2100031255.1.4",
          "form": "너무너무너무 달고 느끼해",
          "original_form": "너무너무너무 달고 느끼해",
          "speaker_id": "2",
          "time": "20210904 19:27"
        },
        {
          "id": "MDRW2100031255.1.5",
          "form": "사서 다 버림ㅋ",
          "original_form": "사서 다 버림ㅋ",
          "speaker_id": "2",
          "time": "20210904 19:27"
        },
        {
          "id": "MDRW2100031255.1.6",
          "form": "세븐일레븐에서 나온 허니버터 팝콘 맛있음",
          "original_form": "세븐일레븐에서 나온 허니버터 팝콘 맛있음",
          "speaker_id": "1",
          "time": "20210904 19:27"
        },
        {
          "id": "MDRW2100031255.1.7",
          "form": "세븐일레븐...근처 어딨지",
          "original_form": "세븐일레븐...근처 어딨지",
          "speaker_id": "2",
          "time": "20210904 19:27"
        },
        {
          "id": "MDRW2100031255.1.8",
          "form": "나는 왓챠 팝콘 사봄ㅋㅋ",
          "original_form": "나는 왓챠 팝콘 사봄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:27"
        },
        {
          "id": "MDRW2100031255.1.9",
          "form": "그건 무슨 맛이니?ㅋㅋ",
          "original_form": "그건 무슨 맛이니?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:29"
        },
        {
          "id": "MDRW2100031255.1.10",
          "form": "걍 평범한 카라멜&치즈 팝콘인데 왓챠2주 이용권이 들어있음",
          "original_form": "걍 평범한 카라멜&치즈 팝콘인데 왓챠2주 이용권이 들어있음",
          "speaker_id": "2",
          "time": "20210904 19:52"
        },
        {
          "id": "MDRW2100031255.1.11",
          "form": "오 괜찮은데 그건 어디서 팜?",
          "original_form": "오 괜찮은데 그건 어디서 팜?",
          "speaker_id": "1",
          "time": "20210904 20:08"
        },
        {
          "id": "MDRW2100031255.1.12",
          "form": "이마트편의점에서ㅋㅋ",
          "original_form": "이마트편의점에서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031255.1.13",
          "form": "사러가야겠다",
          "original_form": "사러가야겠다",
          "speaker_id": "1",
          "time": "20210905 11:19"
        },
        {
          "id": "MDRW2100031255.1.14",
          "form": "내가 기대한  감자칩이 아니었음ㅜ 두꺼워서 맛없어",
          "original_form": "내가 기대한  감자칩이 아니었음ㅜ 두꺼워서 맛없어",
          "speaker_id": "1",
          "time": "20210905 11:20"
        },
        {
          "id": "MDRW2100031255.1.15",
          "form": "어떤 감자칩 말하는거??",
          "original_form": "어떤 감자칩 말하는거??",
          "speaker_id": "2",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100031255.1.16",
          "form": "봉지과잔데 튀기지 않고 공기로 튀겼다는 과잔데 맛없어",
          "original_form": "봉지과잔데 튀기지 않고 공기로 튀겼다는 과잔데 맛없어",
          "speaker_id": "1",
          "time": "20210905 11:44"
        },
        {
          "id": "MDRW2100031255.1.17",
          "form": "칩이 두껍더라고 바삭한 맛이 없어",
          "original_form": "칩이 두껍더라고 바삭한 맛이 없어",
          "speaker_id": "1",
          "time": "20210905 11:44"
        },
        {
          "id": "MDRW2100031255.1.18",
          "form": "아 그 에어베이크인가 하는 그거?",
          "original_form": "아 그 에어베이크인가 하는 그거?",
          "speaker_id": "2",
          "time": "20210905 12:02"
        },
        {
          "id": "MDRW2100031255.1.19",
          "form": "ㅇㅇ그거야",
          "original_form": "ㅇㅇ그거야",
          "speaker_id": "1",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100031255.1.20",
          "form": "나도 궁금했는데 먹지 말아야겠다ㅋㅋ",
          "original_form": "나도 궁금했는데 먹지 말아야겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:07"
        },
        {
          "id": "MDRW2100031255.1.21",
          "form": "아 그리고 가나에서 밀크티 초콜릿 나왔는데 은근 밀크티 향이 많아서 맛있었어",
          "original_form": "아 그리고 가나에서 밀크티 초콜릿 나왔는데 은근 밀크티 향이 많아서 맛있었어",
          "speaker_id": "1",
          "time": "20210905 16:14"
        },
        {
          "id": "MDRW2100031255.1.22",
          "form": "그거 에브리데이에서도 팔던데",
          "original_form": "그거 에브리데이에서도 팔던데",
          "speaker_id": "1",
          "time": "20210905 16:14"
        },
        {
          "id": "MDRW2100031255.1.23",
          "form": "세븐일레븐에서 원플원하니 거기서 사",
          "original_form": "세븐일레븐에서 원플원하니 거기서 사",
          "speaker_id": "1",
          "time": "20210905 16:14"
        },
        {
          "id": "MDRW2100031255.1.24",
          "form": "오 꿀팁감사ㅋㅋ",
          "original_form": "오 꿀팁감사ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100031255.1.25",
          "form": "밀크티 초코 그것도 항상 지나다니면서 궁금했는데",
          "original_form": "밀크티 초코 그것도 항상 지나다니면서 궁금했는데",
          "speaker_id": "2",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100031255.1.26",
          "form": "근데 세븐일레븐이 주변에 없는게 문제여",
          "original_form": "근데 세븐일레븐이 주변에 없는게 문제여",
          "speaker_id": "1",
          "time": "20210905 16:25"
        }
      ]
    }
