      "utterance": [
        {
          "id": "MDRW2100033605.1.1",
          "form": "애기는 미술학원다녀요?",
          "original_form": "애기는 미술학원다녀요?",
          "speaker_id": "1",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100033605.1.2",
          "form": "네 첫째만 보내고있어요",
          "original_form": "네 첫째만 보내고있어요",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100033605.1.3",
          "form": "그림그리는거 좋아하나보네요",
          "original_form": "그림그리는거 좋아하나보네요",
          "speaker_id": "1",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100033605.1.4",
          "form": "요즘 그리고 색칠하고 하는걸 좋아해서요",
          "original_form": "요즘 그리고 색칠하고 하는걸 좋아해서요",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100033605.1.5",
          "form": "미술에 소질이있나봐요ㅎ",
          "original_form": "미술에 소질이있나봐요ㅎ",
          "speaker_id": "1",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100033605.1.6",
          "form": "근데 자꾸 자기 친구 누구는 그림잘그려 난 못그려",
          "original_form": "근데 자꾸 자기 친구 누구는 그림잘그려 난 못그려",
          "speaker_id": "2",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100033605.1.7",
          "form": "이런말을해서...안타까워요 ㅎ",
          "original_form": "이런말을해서...안타까워요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100033605.1.8",
          "form": "그래서 울딸도 잘한다고 했더니 미술학원에서도 선생님도칭찬하시고 ㅎㅎ",
          "original_form": "그래서 울딸도 잘한다고 했더니 미술학원에서도 선생님도칭찬하시고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100033605.1.9",
          "form": "ㅎㅎ 귀엽네요",
          "original_form": "ㅎㅎ 귀엽네요",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100033605.1.10",
          "form": "그게 더 예술적 감각이 있는걸지돚",
          "original_form": "그게 더 예술적 감각이 있는걸지돚",
          "speaker_id": "1",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100033605.1.11",
          "form": "그덕인지 이제 그림그리는게 자신감이 붙은듯이 거침없이 그리더라구요",
          "original_form": "그덕인지 이제 그림그리는게 자신감이 붙은듯이 거침없이 그리더라구요",
          "speaker_id": "2",
          "time": "20210824 16:58"
        },
        {
          "id": "MDRW2100033605.1.12",
          "form": "전 미술 시간 넘싫어했는데ㅎㅎ",
          "original_form": "전 미술 시간 넘싫어했는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033605.1.13",
          "form": "집에 포스트잇 스케치북 종이란 종이에 다 그리고 있어요",
          "original_form": "집에 포스트잇 스케치북 종이란 종이에 다 그리고 있어요",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033605.1.14",
          "form": "창의력도 키우고 좋은듯요",
          "original_form": "창의력도 키우고 좋은듯요",
          "speaker_id": "1",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033605.1.15",
          "form": "아직 멀 잘그리고 그러는수준은 아니고 ㅎㅎ",
          "original_form": "아직 멀 잘그리고 그러는수준은 아니고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033605.1.16",
          "form": "원래 다그러면서 느는거죠",
          "original_form": "원래 다그러면서 느는거죠",
          "speaker_id": "1",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033605.1.17",
          "form": "창의력에 도움되라고 일부러 그림만그리는학원",
          "original_form": "창의력에 도움되라고 일부러 그림만그리는학원",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033605.1.18",
          "form": "아닌곳으로 보냈어요",
          "original_form": "아닌곳으로 보냈어요",
          "speaker_id": "2",
          "time": "20210824 16:59"
        }
      ]
