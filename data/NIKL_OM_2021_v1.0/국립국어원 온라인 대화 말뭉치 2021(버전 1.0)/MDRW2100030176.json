      "utterance": [
        {
          "id": "MDRW2100030176.1.1",
          "form": "엄마 내가 알려준 스트레칭 꾸준히 해",
          "original_form": "엄마 내가 알려준 스트레칭 꾸준히 해",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030176.1.2",
          "form": "해야 되는데 잘 안된다",
          "original_form": "해야 되는데 잘 안된다",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100030176.1.3",
          "form": "자꾸 어깨 아프고 목아프다고 하잖아",
          "original_form": "자꾸 어깨 아프고 목아프다고 하잖아",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030176.1.4",
          "form": "디스크라그래",
          "original_form": "디스크라그래",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030176.1.5",
          "form": "자세도 안 좋은데",
          "original_form": "자세도 안 좋은데",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030176.1.6",
          "form": "스트레칭이라도 꾸준히 해서 풀어줘야지",
          "original_form": "스트레칭이라도 꾸준히 해서 풀어줘야지",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030176.1.7",
          "form": "니가 스트레칭 시켜주고 안마 해주니까 훨씬 낫긴하더라",
          "original_form": "니가 스트레칭 시켜주고 안마 해주니까 훨씬 낫긴하더라",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030176.1.8",
          "form": "시원하대",
          "original_form": "시원하대",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030176.1.9",
          "form": "대신 내손이 아작남",
          "original_form": "대신 내손이 아작남",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030176.1.10",
          "form": "내 손목 ㅠㅜ",
          "original_form": "내 손목 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030176.1.11",
          "form": "그러니까 그만 하랬는데 계속했잖아",
          "original_form": "그러니까 그만 하랬는데 계속했잖아",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030176.1.12",
          "form": "아 자꾸 아프다고 하니까 그렇지",
          "original_form": "아 자꾸 아프다고 하니까 그렇지",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030176.1.13",
          "form": "니네때매 아픈거다",
          "original_form": "니네때매 아픈거다",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030176.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030176.1.15",
          "form": "니가 말안들어서",
          "original_form": "니가 말안들어서",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030176.1.16",
          "form": "무슨 난 엄마처럼 말짓 안해따",
          "original_form": "무슨 난 엄마처럼 말짓 안해따",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030176.1.17",
          "form": "니네는 얌전했짘ㅋ",
          "original_form": "니네는 얌전했짘ㅋ",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030176.1.18",
          "form": "그래 우린 말 진짜 잘듣고 순했다고",
          "original_form": "그래 우린 말 진짜 잘듣고 순했다고",
          "speaker_id": "2",
