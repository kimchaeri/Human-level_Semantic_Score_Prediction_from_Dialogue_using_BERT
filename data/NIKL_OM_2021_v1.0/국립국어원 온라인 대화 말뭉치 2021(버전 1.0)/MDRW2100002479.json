      "utterance": [
        {
          "id": "MDRW2100002479.1.1",
          "form": "선크림발라요?",
          "original_form": "선크림발라요?",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002479.1.2",
          "form": "오늘 안바르고 나왔는데",
          "original_form": "오늘 안바르고 나왔는데",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002479.1.3",
          "form": "요며칠 계속 비와서 해가 안떴었는데",
          "original_form": "요며칠 계속 비와서 해가 안떴었는데",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002479.1.4",
          "form": "오늘은 날씨가 화창하네요",
          "original_form": "오늘은 날씨가 화창하네요",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002479.1.5",
          "form": "요즘 육아때문에 힘들겠다",
          "original_form": "요즘 육아때문에 힘들겠다",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002479.1.6",
          "form": "밤에 잠은 잘 자요?",
          "original_form": "밤에 잠은 잘 자요?",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002479.1.7",
          "form": "세은이요? 4개월부터 통잠했어요 효녀!",
          "original_form": "세은이요? 4개월부터 통잠했어요 효녀!",
          "speaker_id": "2",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002479.1.8",
          "form": "오빠도 신생아때 1시간마다 깨면 회사가서 기절할거예요ㅋㅋ",
          "original_form": "오빠도 신생아때 1시간마다 깨면 회사가서 기절할거예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002479.1.9",
          "form": "육아때문에 힘든건 4개월까지예요~~",
          "original_form": "육아때문에 힘든건 4개월까지예요~~",
          "speaker_id": "2",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002479.1.10",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002479.1.11",
          "form": "조리원간데요?",
          "original_form": "조리원간데요?",
          "speaker_id": "2",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002479.1.12",
          "form": "아 미용건강 얘기합시다",
          "original_form": "아 미용건강 얘기합시다",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002479.1.13",
          "form": "우리 애기 유산됐어요",
          "original_form": "우리 애기 유산됐어요",
          "speaker_id": "1",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002479.1.14",
          "form": "모른척 있어줘요",
          "original_form": "모른척 있어줘요",
          "speaker_id": "1",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002479.1.15",
          "form": "어머니랑 아버지께는 말씀드렸어요",
          "original_form": "어머니랑 아버지께는 말씀드렸어요",
          "speaker_id": "1",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002479.1.16",
          "form": "네 몸조리 잘해야되요 언니는 괜찮아요?",
          "original_form": "네 몸조리 잘해야되요 언니는 괜찮아요?",
          "speaker_id": "2",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002479.1.17",
          "form": "걱정이네요",
          "original_form": "걱정이네요",
          "speaker_id": "2",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002479.1.18",
          "form": "아직 입덧증상도 그대로고 기운도 없고해서 걱정이에요",
          "original_form": "아직 입덧증상도 그대로고 기운도 없고해서 걱정이에요",
          "speaker_id": "1",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002479.1.19",
          "form": "한약 지어줘요",
          "original_form": "한약 지어줘요",
          "speaker_id": "2",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002479.1.20",
          "form": "소파해야된데요?",
          "original_form": "소파해야된데요?",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002479.1.21",
          "form": "링거 영양제 맞아요",
          "original_form": "링거 영양제 맞아요",
          "speaker_id": "2",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002479.1.22",
          "form": "소파가 태아 적출 말하는거에요?",
          "original_form": "소파가 태아 적출 말하는거에요?",
          "speaker_id": "1",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002479.1.23",
          "form": "유산하면 소파수술해요",
          "original_form": "유산하면 소파수술해요",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002479.1.24",
          "form": "주수마다 다르긴한데 자궁소파술 해요",
          "original_form": "주수마다 다르긴한데 자궁소파술 해요",
          "speaker_id": "2",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002479.1.25",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002479.1.26",
          "form": "생각보다 자연 유산률이 높아서 놀랐는데 우리부부가이럴줄은",
          "original_form": "생각보다 자연 유산률이 높아서 놀랐는데 우리부부가이럴줄은",
          "speaker_id": "1",
          "time": "20210820 14:46"
        },
        {
          "id": "MDRW2100002479.1.27",
          "form": "초기라 더 증상이없어서 \n일단 산부인과에서 확인한거죠?",
          "original_form": "초기라 더 증상이없어서 \n일단 산부인과에서 확인한거죠?",
          "speaker_id": "2",
          "time": "20210820 14:49"
        },
        {
          "id": "MDRW2100002479.1.28",
          "form": "혈액응고 안됬데요?",
          "original_form": "혈액응고 안됬데요?",
          "speaker_id": "2",
          "time": "20210820 14:49"
        },
        {
          "id": "MDRW2100002479.1.29",
          "form": "임신바우처카드 만들었죠? 유산 진단 기준 1년까지 사용할 수 있으니까 병원이용할 때 써요",
          "original_form": "임신바우처카드 만들었죠? 유산 진단 기준 1년까지 사용할 수 있으니까 병원이용할 때 써요",
          "speaker_id": "2",
          "time": "20210820 14:50"
        },
        {
          "id": "MDRW2100002479.1.30",
          "form": "산부인과에서 확인했고",
          "original_form": "산부인과에서 확인했고",
          "speaker_id": "1",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002479.1.31",
          "form": "바우처카드 오늘 만들러갔어요",
          "original_form": "바우처카드 오늘 만들러갔어요",
