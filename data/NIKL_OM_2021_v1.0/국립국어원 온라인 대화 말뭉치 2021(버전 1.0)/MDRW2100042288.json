      "utterance": [
        {
          "id": "MDRW2100042288.1.1",
          "form": "날씨가 비가 계속오네요",
          "original_form": "날씨가 비가 계속오네요",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042288.1.2",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100042288.1.3",
          "form": "저희쪽은 오늘 비가 안왔어요",
          "original_form": "저희쪽은 오늘 비가 안왔어요",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042288.1.4",
          "form": "저희는 오후부터 쭉오는데 많은양이 내리고 있어요",
          "original_form": "저희는 오후부터 쭉오는데 많은양이 내리고 있어요",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100042288.1.5",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042288.1.6",
          "form": "지금도 비가 많이 내리나요?",
          "original_form": "지금도 비가 많이 내리나요?",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100042288.1.7",
          "form": "네 지금도 많이 내리고 있죠",
          "original_form": "네 지금도 많이 내리고 있죠",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042288.1.8",
          "form": "낼도 계속 온다고 하네요",
          "original_form": "낼도 계속 온다고 하네요",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042288.1.9",
          "form": "그렇군요 그럼 옷차림 가볍게 해야겠네요",
          "original_form": "그렇군요 그럼 옷차림 가볍게 해야겠네요",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042288.1.10",
          "form": "비가와서 좀 춥네요",
          "original_form": "비가와서 좀 춥네요",
          "speaker_id": "1",
          "time": "20210907 01:17"
        },
        {
          "id": "MDRW2100042288.1.11",
          "form": "앏은긴팔을 입어야 겠어요",
          "original_form": "앏은긴팔을 입어야 겠어요",
          "speaker_id": "1",
          "time": "20210907 01:18"
        },
        {
          "id": "MDRW2100042288.1.12",
          "form": "따듯하게 입으셔야겠네요",
          "original_form": "따듯하게 입으셔야겠네요",
          "speaker_id": "2",
          "time": "20210907 01:23"
        },
        {
          "id": "MDRW2100042288.1.13",
          "form": "가을에 비가 넘 자주 오네요",
          "original_form": "가을에 비가 넘 자주 오네요",
          "speaker_id": "1",
          "time": "20210907 01:25"
        },
        {
          "id": "MDRW2100042288.1.14",
          "form": "그러게나말이에요",
          "original_form": "그러게나말이에요",
          "speaker_id": "2",
          "time": "20210907 01:31"
        },
        {
          "id": "MDRW2100042288.1.15",
          "form": "비오는것 좋아하시나요",
          "original_form": "비오는것 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 01:50"
        },
        {
          "id": "MDRW2100042288.1.16",
          "form": "비오는걸 보는건 좋아해요",
          "original_form": "비오는걸 보는건 좋아해요",
          "speaker_id": "2",
          "time": "20210907 11:11"
        },
        {
          "id": "MDRW2100042288.1.17",
          "form": "비록 습기는 싫지만여",
          "original_form": "비록 습기는 싫지만여",
          "speaker_id": "2",
          "time": "20210907 11:11"
        },
        {
          "id": "MDRW2100042288.1.18",
          "form": "name1님은 어떠신가요?",
          "original_form": "&name1&님은 어떠신가요?",
          "speaker_id": "2",
          "time": "20210907 11:11"
        },
        {
          "id": "MDRW2100042288.1.19",
          "form": "비는 보는건 좋아요",
          "original_form": "비는 보는건 좋아요",
          "speaker_id": "1",
          "time": "20210907 11:12"
        },
        {
          "id": "MDRW2100042288.1.20",
          "form": "커피 한잔하면서 .....",
          "original_form": "커피 한잔하면서 .....",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100042288.1.21",
          "form": "어떤신가요",
          "original_form": "어떤신가요",
