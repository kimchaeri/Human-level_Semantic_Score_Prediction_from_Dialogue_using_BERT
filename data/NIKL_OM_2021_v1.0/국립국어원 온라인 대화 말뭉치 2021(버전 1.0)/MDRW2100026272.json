      "utterance": [
        {
          "id": "MDRW2100026272.1.1",
          "form": "그래서 마인드도 책이름인가요?",
          "original_form": "그래서 마인드도 책이름인가요?",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026272.1.2",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026272.1.3",
          "form": "심리도 왜 중요한가요? ㅎㅎ",
          "original_form": "심리도 왜 중요한가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026272.1.4",
          "form": "아니요. 책이름아니구",
          "original_form": "아니요. 책이름아니구",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026272.1.5",
          "form": "주식하는 마인드?",
          "original_form": "주식하는 마인드?",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026272.1.6",
          "form": "아아 ㅋㅋㅋ 그렇구나 ㅋㅋ",
          "original_form": "아아 ㅋㅋㅋ 그렇구나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026272.1.7",
          "form": "참 '주식하는 마음'  이라는 책도 있어요",
          "original_form": "참 '주식하는 마음'  이라는 책도 있어요",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026272.1.8",
          "form": "우와아",
          "original_form": "우와아",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026272.1.9",
          "form": "도움되실거에요",
          "original_form": "도움되실거에요",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026272.1.10",
          "form": "정말 많이 읽으신거같아요",
          "original_form": "정말 많이 읽으신거같아요",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026272.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026272.1.12",
          "form": "왜 마음이 중요한가^^",
          "original_form": "왜 마음이 중요한가^^",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026272.1.13",
          "form": "한 100권은 읽으신거같은데",
          "original_form": "한 100권은 읽으신거같은데",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026272.1.14",
          "form": "너무 적나.. 음천권..?",
          "original_form": "너무 적나.. 음천권..?",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026272.1.15",
          "form": "책 좋아해요ㅎㅎ",
          "original_form": "책 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026272.1.16",
          "form": "정말 좋아하시는거같아요 ㅎㅎ",
          "original_form": "정말 좋아하시는거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026272.1.17",
          "form": "도서관도 집옆이시고",
          "original_form": "도서관도 집옆이시고",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026272.1.18",
          "form": "좋아하는 쉬운책부터보세요",
          "original_form": "좋아하는 쉬운책부터보세요",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026272.1.19",
          "form": "페미책도 괜찮고요",
          "original_form": "페미책도 괜찮고요",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026272.1.20",
          "form": "책으로 많이 배워요",
          "original_form": "책으로 많이 배워요",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026272.1.21",
          "form": "ㅋㅋㅋㅋ 맞아요 책으로 많이 배워요 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 맞아요 책으로 많이 배워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026272.1.22",
          "form": "문학상 받은 책도 보시나요?",
          "original_form": "문학상 받은 책도 보시나요?",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026272.1.23",
          "form": "페미책도 보셨어요??",
          "original_form": "페미책도 보셨어요??",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026272.1.24",
          "form": "사실 문학상 목록을 모르겠어요ㅜㅜㅋ",
          "original_form": "사실 문학상 목록을 모르겠어요ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026272.1.25",
          "form": "운동하는여자ㅡ 최근에 봤어요",
          "original_form": "운동하는여자ㅡ 최근에 봤어요",
