      "utterance": [
        {
          "id": "MDRW2100038291.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 08:46"
        },
        {
          "id": "MDRW2100038291.1.2",
          "form": "안녕하세요~!",
          "original_form": "안녕하세요~!",
          "speaker_id": "2",
          "time": "20210902 08:46"
        },
        {
          "id": "MDRW2100038291.1.3",
          "form": "넷플릭스 보시나요?",
          "original_form": "넷플릭스 보시나요?",
          "speaker_id": "2",
          "time": "20210902 08:46"
        },
        {
          "id": "MDRW2100038291.1.4",
          "form": "가끔 봐요",
          "original_form": "가끔 봐요",
          "speaker_id": "1",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100038291.1.5",
          "form": "가장 최근에 본게 아신전이네요",
          "original_form": "가장 최근에 본게 아신전이네요",
          "speaker_id": "1",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100038291.1.6",
          "form": "오ㅎㅎ",
          "original_form": "오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100038291.1.7",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100038291.1.8",
          "form": "전 최근에 구독취소했어요ㅠ",
          "original_form": "전 최근에 구독취소했어요ㅠ",
          "speaker_id": "2",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100038291.1.9",
          "form": "재미는 있는데 본편보다 내용이 무거워요",
          "original_form": "재미는 있는데 본편보다 내용이 무거워요",
          "speaker_id": "1",
          "time": "20210902 08:48"
        },
        {
          "id": "MDRW2100038291.1.10",
          "form": "저도 그거 보고 취소했어요",
          "original_form": "저도 그거 보고 취소했어요",
          "speaker_id": "1",
          "time": "20210902 08:48"
        },
        {
          "id": "MDRW2100038291.1.11",
          "form": "헉....",
          "original_form": "헉....",
          "speaker_id": "2",
          "time": "20210902 08:48"
        },
        {
          "id": "MDRW2100038291.1.12",
          "form": "아무래도 구독료가",
          "original_form": "아무래도 구독료가",
          "speaker_id": "2",
          "time": "20210902 08:48"
        },
        {
          "id": "MDRW2100038291.1.13",
          "form": "장난이 아니다보니까",
          "original_form": "장난이 아니다보니까",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038291.1.14",
          "form": "보고싶은거보면 미련없이 해지하게 되지 않아요?",
          "original_form": "보고싶은거보면 미련없이 해지하게 되지 않아요?",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038291.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038291.1.16",
          "form": "오래 구독하고 있기에는 부담되긴 하죠",
          "original_form": "오래 구독하고 있기에는 부담되긴 하죠",
          "speaker_id": "1",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038291.1.17",
          "form": "저도 왓챠",
          "original_form": "저도 왓챠",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038291.1.18",
          "form": "끊었다가 해지했다가",
          "original_form": "끊었다가 해지했다가",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038291.1.19",
          "form": "그래요ㅎㅎ",
          "original_form": "그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038291.1.20",
          "form": "쿠팡플레이는 싸던데",
          "original_form": "쿠팡플레이는 싸던데",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038291.1.21",
          "form": "그래도 넷플 오리지널 컨텐츠가 강해서",
          "original_form": "그래도 넷플 오리지널 컨텐츠가 강해서",
          "speaker_id": "1",
          "time": "20210902 08:50"
        },
        {
          "id": "MDRW2100038291.1.22",
          "form": "다시 넷플로 돌아가게 되더라구요",
          "original_form": "다시 넷플로 돌아가게 되더라구요",
          "speaker_id": "1",
          "time": "20210902 08:50"
        },
        {
          "id": "MDRW2100038291.1.23",
          "form": "아아 맞아요",
          "original_form": "아아 맞아요",
          "speaker_id": "2",
          "time": "20210902 08:50"
        },
        {
          "id": "MDRW2100038291.1.24",
          "form": "약간 옛날거지만",
          "original_form": "약간 옛날거지만",
          "speaker_id": "2",
          "time": "20210902 08:50"
        },
        {
          "id": "MDRW2100038291.1.25",
          "form": "블랙미러 같은!?",
          "original_form": "블랙미러 같은!?",
          "speaker_id": "2",
          "time": "20210902 08:50"
        },
        {
          "id": "MDRW2100038291.1.26",
          "form": "킹덤도 그렇게 재밌다고 하더라구요",
          "original_form": "킹덤도 그렇게 재밌다고 하더라구요",
          "speaker_id": "2",
          "time": "20210902 08:50"
        },
        {
          "id": "MDRW2100038291.1.27",
          "form": "킹덤 엄청 재밌어요",
          "original_form": "킹덤 엄청 재밌어요",
          "speaker_id": "1",
          "time": "20210902 08:52"
        },
        {
          "id": "MDRW2100038291.1.28",
          "form": "그리고 스위트홈도 재밌게 봤어요",
          "original_form": "그리고 스위트홈도 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210902 08:52"
        },
        {
          "id": "MDRW2100038291.1.29",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 08:52"
        },
        {
          "id": "MDRW2100038291.1.30",
          "form": "팬이신가봐요",
          "original_form": "팬이신가봐요",
          "speaker_id": "2",
          "time": "20210902 08:52"
        },
        {
          "id": "MDRW2100038291.1.31",
          "form": "아 그거 웹툰원작이죠?",
          "original_form": "아 그거 웹툰원작이죠?",
          "speaker_id": "2",
          "time": "20210902 08:52"
        },
        {
          "id": "MDRW2100038291.1.32",
          "form": "맞아요 웹툰 원작",
          "original_form": "맞아요 웹툰 원작",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100038291.1.33",
          "form": "하지만 웹툰을 보진 않았어요",
          "original_form": "하지만 웹툰을 보진 않았어요",
          "speaker_id": "1",
          "time": "20210902 08:53"
        }
      ]
    }
  ]
