      "utterance": [
        {
          "id": "MDRW2100021558.1.1",
          "form": "옙ㅋㅋ 그런데 교대임용은 사범대보다는 경쟁률이 낮은 편이에요",
          "original_form": "옙ㅋㅋ 그런데 교대임용은 사범대보다는 경쟁률이 낮은 편이에요",
          "speaker_id": "1",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100021558.1.2",
          "form": "그렇군요!",
          "original_form": "그렇군요!",
          "speaker_id": "2",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100021558.1.3",
          "form": "그래도 정말 멋지세요!~~",
          "original_form": "그래도 정말 멋지세요!~~",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021558.1.4",
          "form": "그런데 저 입학할 때는 진짜 경쟁률이 낮았거든요",
          "original_form": "그런데 저 입학할 때는 진짜 경쟁률이 낮았거든요",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021558.1.5",
          "form": "아 헉",
          "original_form": "아 헉",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021558.1.6",
          "form": "그래서 교대 입학 점수가 높았는데",
          "original_form": "그래서 교대 입학 점수가 높았는데",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021558.1.7",
          "form": "저 졸업할 때쯤 되니까 임용이 엄청 빡세진 거예요",
          "original_form": "저 졸업할 때쯤 되니까 임용이 엄청 빡세진 거예요",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021558.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021558.1.9",
          "form": "아우",
          "original_form": "아우",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021558.1.10",
          "form": "그래서 애들끼리 막 사기 당했다고 그랬어요",
          "original_form": "그래서 애들끼리 막 사기 당했다고 그랬어요",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100021558.1.11",
          "form": "하필 꼭 그런건",
          "original_form": "하필 꼭 그런건",
          "speaker_id": "2",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021558.1.12",
          "form": "본인때에 그러는거같아요",
          "original_form": "본인때에 그러는거같아요",
          "speaker_id": "2",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021558.1.13",
          "form": "맞아요 진짜",
          "original_form": "맞아요 진짜",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021558.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100021558.1.15",
          "form": "선배들이 막 시위하고 그랬어요",
          "original_form": "선배들이 막 시위하고 그랬어요",
          "speaker_id": "1",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100021558.1.16",
          "form": "어우ㅡ..",
          "original_form": "어우ㅡ..",
          "speaker_id": "2",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100021558.1.17",
          "form": "그런 경쟁률을 뚫고 한번에 합격하신 것 너무 대단해요",
          "original_form": "그런 경쟁률을 뚫고 한번에 합격하신 것 너무 대단해요",
          "speaker_id": "2",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100021558.1.18",
          "form": "저도 시위 다른걸로 참여해본적있는데",
          "original_form": "저도 시위 다른걸로 참여해본적있는데",
          "speaker_id": "2",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100021558.1.19",
          "form": "선배분들도 멋지네요 행동하는거",
          "original_form": "선배분들도 멋지네요 행동하는거",
          "speaker_id": "2",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100021558.1.20",
          "form": "오오오 ㅋㅋㅋㅋ 어떤 시위 참여해보셨나요",
          "original_form": "오오오 ㅋㅋㅋㅋ 어떤 시위 참여해보셨나요",
          "speaker_id": "1",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021558.1.21",
          "form": "저는 그 ..ㅎㅎ",
          "original_form": "저는 그 ..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021558.1.22",
          "form": "불법촬영시위 나갔었어요",
          "original_form": "불법촬영시위 나갔었어요",
          "speaker_id": "2",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021558.1.23",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021558.1.24",
          "form": "혜화역 시위요?",
          "original_form": "혜화역 시위요?",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021558.1.25",
          "form": "넵 광화문이랑 ㅎ",
          "original_form": "넵 광화문이랑 ㅎ",
          "speaker_id": "2",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021558.1.26",
          "form": "너무 멋져요",
          "original_form": "너무 멋져요",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021558.1.27",
          "form": "제가 나간 시위는",
          "original_form": "제가 나간 시위는",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021558.1.28",
          "form": "박근혜퇴진 시위뿐...",
          "original_form": "박근혜퇴진 시위뿐...",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021558.1.29",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021558.1.30",
          "form": "갑자기 떠오르는 그 이름",
          "original_form": "갑자기 떠오르는 그 이름",
          "speaker_id": "2",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021558.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ잊고 살았다...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ잊고 살았다...",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021558.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021558.1.33",
          "form": "근데 저는 임용 되고나서는 아무것도 참가를 못하는게",
          "original_form": "근데 저는 임용 되고나서는 아무것도 참가를 못하는게",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021558.1.34",
          "form": "저희는 정치적 기본권이...없거든요",
          "original_form": "저희는 정치적 기본권이...없거든요",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021558.1.35",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021558.1.36",
          "form": "ㅎㅎ ...",
          "original_form": "ㅎㅎ ...",
          "speaker_id": "1",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021558.1.37",
          "form": "그렇네요...",
          "original_form": "그렇네요...",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021558.1.38",
          "form": "나라의 도구라...",
          "original_form": "나라의 도구라...",
          "speaker_id": "1",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021558.1.39",
          "form": "쩝.......",
          "original_form": "쩝.......",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021558.1.40",
          "form": "교사의 단점 두번째네요..",
          "original_form": "교사의 단점 두번째네요..",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021558.1.41",
          "form": "정당 가입도 못하고 후원도 못해요",
          "original_form": "정당 가입도 못하고 후원도 못해요",
          "speaker_id": "1",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021558.1.42",
          "form": "시위도... 그런 거 했다가 걸리면 징계받는대유ㅠ",
          "original_form": "시위도... 그런 거 했다가 걸리면 징계받는대유ㅠ",
          "speaker_id": "1",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021558.1.43",
          "form": "그리고 학생들은 정치적 가치관이 가족이나 주위사람들한테 많이 휘둘리는 시기라",
          "original_form": "그리고 학생들은 정치적 가치관이 가족이나 주위사람들한테 많이 휘둘리는 시기라",
          "speaker_id": "2",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100021558.1.44",
          "form": "선생님들이 정치색을 띄면 안되니까..",
          "original_form": "선생님들이 정치색을 띄면 안되니까..",
          "speaker_id": "2",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100021558.1.45",
          "form": "그건 완전 맞아요",
          "original_form": "그건 완전 맞아요",
          "speaker_id": "1",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100021558.1.46",
          "form": "직업을 가진 그냥 개인으로써는 힘든일이네요ㅠㅠ",
          "original_form": "직업을 가진 그냥 개인으로써는 힘든일이네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100021558.1.47",
          "form": "근데 학교에서 기독교 주입하려 호시탐탐 노리는 교사들은 너무 많고...",
          "original_form": "근데 학교에서 기독교 주입하려 호시탐탐 노리는 교사들은 너무 많고...",
          "speaker_id": "1",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021558.1.48",
          "form": "아 혹시 교회다니시면 죄송해요",
          "original_form": "아 혹시 교회다니시면 죄송해요",
          "speaker_id": "1",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021558.1.49",
          "form": "아 저는 종교없습니다!",
          "original_form": "아 저는 종교없습니다!",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021558.1.50",
          "form": "호호 다행입니다!",
          "original_form": "호호 다행입니다!",
          "speaker_id": "1",
          "time": "20210828 21:13"
        }
      ]
