      "utterance": [
        {
          "id": "MDRW2100035426.1.1",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100035426.1.2",
          "form": "아뇨 아직이요...",
          "original_form": "아뇨 아직이요...",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100035426.1.3",
          "form": "맞으셨어요?",
          "original_form": "맞으셨어요?",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100035426.1.4",
          "form": "네 1차 접종했어요! 곧 2차 맞아요 ㅜ",
          "original_form": "네 1차 접종했어요! 곧 2차 맞아요 ㅜ",
          "speaker_id": "2",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100035426.1.5",
          "form": "그러시군요...",
          "original_form": "그러시군요...",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100035426.1.6",
          "form": "안아프셨어요?",
          "original_form": "안아프셨어요?",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100035426.1.7",
          "form": "아 너무 별로였어요...",
          "original_form": "아 너무 별로였어요...",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100035426.1.8",
          "form": "헐 어떠셨길래요",
          "original_form": "헐 어떠셨길래요",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100035426.1.9",
          "form": "부작용 뉴스 나오는거보면 백신이 불완전해보이긴 했어요",
          "original_form": "부작용 뉴스 나오는거보면 백신이 불완전해보이긴 했어요",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100035426.1.10",
          "form": "이런 얘기 불편하실수도있는데 생리있잖아요 ㅜㅜ",
          "original_form": "이런 얘기 불편하실수도있는데 생리있잖아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100035426.1.11",
          "form": "네 안불편해요",
          "original_form": "네 안불편해요",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100035426.1.12",
          "form": "저는 화이자 맞았거든요? 직업상 맞아야 해서 맞게되었는데 맞는 당일과 그 다음날까지 정도에 팔만 아팠거든요 그래서 별거아니네 했는데 생리가 대박이었어요",
          "original_form": "저는 화이자 맞았거든요? 직업상 맞아야 해서 맞게되었는데 맞는 당일과 그 다음날까지 정도에 팔만 아팠거든요 그래서 별거아니네 했는데 생리가 대박이었어요",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100035426.1.13",
          "form": "헉 그렇군요... 그거 부작용이라고",
          "original_form": "헉 그렇군요... 그거 부작용이라고",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100035426.1.14",
          "form": "얼마전에 읽었어요",
          "original_form": "얼마전에 읽었어요",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100035426.1.15",
          "form": "전몰랐거든요 ㅜㅜ 진짜  죽다살아났어요 거의 하혈이었어요",
          "original_form": "전몰랐거든요 ㅜㅜ 진짜  죽다살아났어요 거의 하혈이었어요",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100035426.1.16",
          "form": "에구 ㅜㅜ 그런 사람들 많아져서",
          "original_form": "에구 ㅜㅜ 그런 사람들 많아져서",
          "speaker_id": "1",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100035426.1.17",
          "form": "그니까 원랜 그게 부작용으로 명시가안되어있대요",
          "original_form": "그니까 원랜 그게 부작용으로 명시가안되어있대요",
          "speaker_id": "1",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100035426.1.18",
          "form": "어떤 여교수가 사람들 사례를 다 모아서",
          "original_form": "어떤 여교수가 사람들 사례를 다 모아서",
          "speaker_id": "1",
          "time": "20210829 17:05"
        },
        {
          "id": "MDRW2100035426.1.19",
          "form": "이제 생리도 부작용이라고 넣었댔나..그런 기사였어요",
          "original_form": "이제 생리도 부작용이라고 넣었댔나..그런 기사였어요",
          "speaker_id": "1",
          "time": "20210829 17:05"
        }
      ]
