      "utterance": [
        {
          "id": "MDRW2100024717.1.1",
          "form": "그런 재테크하는 사람도",
          "original_form": "그런 재테크하는 사람도",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024717.1.2",
          "form": "꽤 되더라고",
          "original_form": "꽤 되더라고",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024717.1.3",
          "form": "일본도 가면 소품싸게사서",
          "original_form": "일본도 가면 소품싸게사서",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024717.1.4",
          "form": "그것도 괜찮겠다 ㅋㅋㅋ",
          "original_form": "그것도 괜찮겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024717.1.5",
          "form": "다시 되파는거지",
          "original_form": "다시 되파는거지",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024717.1.6",
          "form": "일본여행 가서 기념품으로 난 곤약젤리를 엄청 사왔었거든?",
          "original_form": "일본여행 가서 기념품으로 난 곤약젤리를 엄청 사왔었거든?",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024717.1.7",
          "form": "그거 되팔아볼 걸 그랬어",
          "original_form": "그거 되팔아볼 걸 그랬어",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024717.1.8",
          "form": "응    맛있지   그거",
          "original_form": "응    맛있지   그거",
          "speaker_id": "2",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024717.1.9",
          "form": "ㅋㅋㅋㅌㅋ먹느라 바빠서 그만ㅎ",
          "original_form": "ㅋㅋㅋㅌㅋ먹느라 바빠서 그만ㅎ",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024717.1.10",
          "form": "먹는 행복도 중요해",
          "original_form": "먹는 행복도 중요해",
          "speaker_id": "2",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024717.1.11",
          "form": "하긴 입의 즐거움을 얻었으니 좋은 게 좋은 거겠거니",
          "original_form": "하긴 입의 즐거움을 얻었으니 좋은 게 좋은 거겠거니",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024717.1.12",
          "form": "제주도 여행도 가고싶어",
          "original_form": "제주도 여행도 가고싶어",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024717.1.13",
          "form": "헉 나도",
          "original_form": "헉 나도",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024717.1.14",
          "form": "맞아 내가 즐거운게 우선이야",
          "original_form": "맞아 내가 즐거운게 우선이야",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024717.1.15",
          "form": "제주도 처음이저 마지막으로 여행갔던 게",
          "original_form": "제주도 처음이저 마지막으로 여행갔던 게",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024717.1.16",
          "form": "고등학교 수학여행때였는데",
          "original_form": "고등학교 수학여행때였는데",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024717.1.17",
          "form": "제주도 해수욕장 가보고싶어",
          "original_form": "제주도 해수욕장 가보고싶어",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024717.1.18",
          "form": "단체여행이라 아쉬웠어",
          "original_form": "단체여행이라 아쉬웠어",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024717.1.19",
          "form": "난 수학여행 못가서",
          "original_form": "난 수학여행 못가서",
          "speaker_id": "2",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024717.1.20",
          "form": "나도! 제주도 바다 물 맑더라",
          "original_form": "나도! 제주도 바다 물 맑더라",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100024717.1.21",
          "form": "제주도 가볼일이 없었어",
          "original_form": "제주도 가볼일이 없었어",
          "speaker_id": "2",
          "time": "20210831 17:31"
