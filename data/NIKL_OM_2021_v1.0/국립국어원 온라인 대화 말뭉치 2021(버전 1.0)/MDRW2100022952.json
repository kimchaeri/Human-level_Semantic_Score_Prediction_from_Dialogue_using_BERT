        },
        {
          "id": "MDRW2100022952.1.2",
          "form": "그런거였어?",
          "original_form": "그런거였어?",
          "speaker_id": "2",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100022952.1.3",
          "form": "어디가면 나와?",
          "original_form": "어디가면 나와?",
          "speaker_id": "2",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100022952.1.4",
          "form": "뱀뼈머리광갱?",
          "original_form": "뱀뼈머리광갱?",
          "speaker_id": "1",
          "time": "20210829 19:33"
        },
        {
          "id": "MDRW2100022952.1.5",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 19:33"
        },
        {
          "id": "MDRW2100022952.1.6",
          "form": "거기 옆에",
          "original_form": "거기 옆에",
          "speaker_id": "1",
          "time": "20210829 19:33"
        },
        {
          "id": "MDRW2100022952.1.7",
          "form": "감실이 하나 있오",
          "original_form": "감실이 하나 있오",
          "speaker_id": "1",
          "time": "20210829 19:33"
        },
        {
          "id": "MDRW2100022952.1.8",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022952.1.9",
          "form": "그 바로 옆에 해류귀같은 npc가 있거든",
          "original_form": "그 바로 옆에 해류귀같은 npc가 있거든",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022952.1.10",
          "form": "걔한테 말걸면 시작됨",
          "original_form": "걔한테 말걸면 시작됨",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022952.1.11",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022952.1.12",
          "form": "한 4일동안 매일 찾아가면 됨",
          "original_form": "한 4일동안 매일 찾아가면 됨",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022952.1.13",
          "form": "이런거 많이했네",
          "original_form": "이런거 많이했네",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022952.1.14",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022952.1.15",
          "form": "리월에 비해 이나즈마가 좁아서 그런가?",
          "original_form": "리월에 비해 이나즈마가 좁아서 그런가?",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022952.1.16",
          "form": "은근히 많이 숨겨놨음",
          "original_form": "은근히 많이 숨겨놨음",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022952.1.17",
          "form": "그것도 그렇고",
          "original_form": "그것도 그렇고",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022952.1.18",
          "form": "컨텐츠 소모 속도 줄이려고",
          "original_form": "컨텐츠 소모 속도 줄이려고",
          "speaker_id": "1",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022952.1.19",
          "form": "아니 근데 이건",
          "original_form": "아니 근데 이건",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022952.1.20",
          "form": "편법이야ㅋㄱㅋㅋㅋㅋㅋㅋ",
          "original_form": "편법이야ㅋㄱㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022952.1.21",
          "form": "컨텐츠 소모되는 속도를 줄이려고",
          "original_form": "컨텐츠 소모되는 속도를 줄이려고",
          "speaker_id": "1",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022952.1.22",
          "form": "퀘를 하루에",
          "original_form": "퀘를 하루에",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022952.1.23",
          "form": "한개만 하게하는게",
          "original_form": "한개만 하게하는게",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022952.1.24",
          "form": "어딧어요",
          "original_form": "어딧어요",
          "speaker_id": "2",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022952.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:35"
        },
        {
          "id": "MDRW2100022952.1.26",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100022952.1.27",
