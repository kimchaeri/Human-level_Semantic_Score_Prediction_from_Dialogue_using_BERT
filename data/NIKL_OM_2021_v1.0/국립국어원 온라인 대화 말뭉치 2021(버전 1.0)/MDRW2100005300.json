      "utterance": [
        {
          "id": "MDRW2100005300.1.1",
          "form": "너 봉사 해 본적 있어???",
          "original_form": "너 봉사 해 본적 있어???",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005300.1.2",
          "form": "해본 적 없어",
          "original_form": "해본 적 없어",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005300.1.3",
          "form": "언니는 ??",
          "original_form": "언니는 ??",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005300.1.4",
          "form": "나는 해본 적 있어",
          "original_form": "나는 해본 적 있어",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005300.1.5",
          "form": "오 뭐해봤어?",
          "original_form": "오 뭐해봤어?",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005300.1.6",
          "form": "나 중학교때 행사 봉사 했었어",
          "original_form": "나 중학교때 행사 봉사 했었어",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005300.1.7",
          "form": "그런데 너도 고등학교때 학술제 나간것도 봉사 아니여??",
          "original_form": "그런데 너도 고등학교때 학술제 나간것도 봉사 아니여??",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005300.1.8",
          "form": "아 그러게 어린이날 행사 갔었네 !",
          "original_form": "아 그러게 어린이날 행사 갔었네 !",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005300.1.9",
          "form": "그것도 봉사지 뭐",
          "original_form": "그것도 봉사지 뭐",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005300.1.10",
          "form": "나 저번에 마라톤 봉사도 했었잖아",
          "original_form": "나 저번에 마라톤 봉사도 했었잖아",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005300.1.11",
          "form": "아 맞아 물 따르고 했다며",
          "original_form": "아 맞아 물 따르고 했다며",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005300.1.12",
          "form": "ㅇㅇ맞아 그 사람들 마실 물 따라두는거임",
          "original_form": "ㅇㅇ맞아 그 사람들 마실 물 따라두는거임",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005300.1.13",
          "form": "요즘은 봉사 할 일이 없네",
          "original_form": "요즘은 봉사 할 일이 없네",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005300.1.14",
          "form": "맞아 코로나 때문에 봉사도 잘 안받아준다더라",
          "original_form": "맞아 코로나 때문에 봉사도 잘 안받아준다더라",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005300.1.15",
          "form": "그치 고등학교때나 학교 애둘이랑 같이 단체로 갔었잖아",
          "original_form": "그치 고등학교때나 학교 애둘이랑 같이 단체로 갔었잖아",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005300.1.16",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005300.1.17",
          "form": "대학와서는 봉사 한번도 안함",
          "original_form": "대학와서는 봉사 한번도 안함",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005300.1.18",
          "form": "그러면 아예 단체를 들어볼까??",
          "original_form": "그러면 아예 단체를 들어볼까??",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005300.1.19",
          "form": "그것도 좋을 듯 !! 요즘 할 곳 찾는 것도 일인데 단체 들어가면 더 편하지 않을까 ?",
          "original_form": "그것도 좋을 듯 !! 요즘 할 곳 찾는 것도 일인데 단체 들어가면 더 편하지 않을까 ?",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005300.1.20",
          "form": "그치 봉사 활동에만 온전히 집중할 수 있으니까!",
          "original_form": "그치 봉사 활동에만 온전히 집중할 수 있으니까!",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005300.1.21",
          "form": "맞아 그리고 새로운 사람들 만나서 친해질 수도 있겠지!",
          "original_form": "맞아 그리고 새로운 사람들 만나서 친해질 수도 있겠지!",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005300.1.22",
          "form": "그치!!!그럼 집 근처에 한 번 찾아볼까???",
          "original_form": "그치!!!그럼 집 근처에 한 번 찾아볼까???",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005300.1.23",
          "form": "좋아 !! 이제 봉사 열심히 해보자",
          "original_form": "좋아 !! 이제 봉사 열심히 해보자",
          "speaker_id": "2",
          "time": "20210821 21:49"
        }
      ]
    }
