      "utterance": [
        {
          "id": "MDRW2100030339.1.1",
          "form": "요즘 가격 많이 올랐어요",
          "original_form": "요즘 가격 많이 올랐어요",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030339.1.2",
          "form": "외국 음식 드시나요",
          "original_form": "외국 음식 드시나요",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030339.1.3",
          "form": "김밥도 비싸 졌어요",
          "original_form": "김밥도 비싸 졌어요",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030339.1.4",
          "form": "태국 음식 먹어 봤나요",
          "original_form": "태국 음식 먹어 봤나요",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030339.1.5",
          "form": "없어서 못 먹죠",
          "original_form": "없어서 못 먹죠",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030339.1.6",
          "form": "김밥도 비싸고 다 비싸요",
          "original_form": "김밥도 비싸고 다 비싸요",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030339.1.7",
          "form": "분자 맛있었고",
          "original_form": "분자 맛있었고",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030339.1.8",
          "form": "태국 음식 뭘 좋아해요",
          "original_form": "태국 음식 뭘 좋아해요",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030339.1.9",
          "form": "팟타이도 만났고",
          "original_form": "팟타이도 만났고",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030339.1.10",
          "form": "분자를 아시나요",
          "original_form": "분자를 아시나요",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030339.1.11",
          "form": "네 존맛",
          "original_form": "네 존맛",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030339.1.12",
          "form": "음성 인식 편리하네요",
          "original_form": "음성 인식 편리하네요",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030339.1.13",
          "form": "똠양꿍은요",
          "original_form": "똠양꿍은요",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030339.1.14",
          "form": "너무너무 편해요",
          "original_form": "너무너무 편해요",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030339.1.15",
          "form": "분자에 333 맥주 맛있어요",
          "original_form": "분자에 333 맥주 맛있어요",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030339.1.16",
          "form": "덕분에 음성인식 하고 있어요",
          "original_form": "덕분에 음성인식 하고 있어요",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030339.1.17",
          "form": "그건 못 먹어 봤어요",
          "original_form": "그건 못 먹어 봤어요",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030339.1.18",
          "form": "바바바 맥주네 어",
          "original_form": "바바바 맥주네 어",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030339.1.19",
          "form": "똠양꿍 무슨맛인가요",
          "original_form": "똠양꿍 무슨맛인가요",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030339.1.20",
          "form": "바바바 맥주 존맛 이더라고요",
          "original_form": "바바바 맥주 존맛 이더라고요",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030339.1.21",
          "form": "새콤한 토마토 스프 같아요",
          "original_form": "새콤한 토마토 스프 같아요",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030339.1.22",
          "form": "완전 맛있겠네요",
          "original_form": "완전 맛있겠네요",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030339.1.23",
          "form": "코코넛 밀크도 들어가요",
          "original_form": "코코넛 밀크도 들어가요",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030339.1.24",
          "form": "중학교 영어 교과서에 똠양꿍 나오더라고요",
          "original_form": "중학교 영어 교과서에 똠양꿍 나오더라고요",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030339.1.25",
          "form": "특이한데 맛있어요",
          "original_form": "특이한데 맛있어요",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030339.1.26",
          "form": "다음에 먹어 볼게요",
          "original_form": "다음에 먹어 볼게요",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030339.1.27",
          "form": "저는 그 소스로 볶음밥 해 먹어요",
          "original_form": "저는 그 소스로 볶음밥 해 먹어요",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030339.1.28",
          "form": "칼국수 먹고 싶네요",
          "original_form": "칼국수 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030339.1.29",
          "form": "김치칼국수 좋아해요",
          "original_form": "김치칼국수 좋아해요",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030339.1.30",
          "form": "김치 수제비도 맛있고요",
