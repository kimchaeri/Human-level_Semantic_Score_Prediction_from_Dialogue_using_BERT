      "utterance": [
        {
          "id": "MDRW2100023186.1.1",
          "form": "가끔 그런거 들음 부러운거는 어쩔 수 없는거같아요",
          "original_form": "가끔 그런거 들음 부러운거는 어쩔 수 없는거같아요",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023186.1.2",
          "form": "진짜요...ㅠㅠㅠ",
          "original_form": "진짜요...ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023186.1.3",
          "form": "그런 상황이면 취업 걱정도 안 할텐데 말이죠ㅠㅠ",
          "original_form": "그런 상황이면 취업 걱정도 안 할텐데 말이죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023186.1.4",
          "form": "맞아요ㅜㅠ",
          "original_form": "맞아요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023186.1.5",
          "form": "후.. 빨리 취업하고싶네요ㅠ",
          "original_form": "후.. 빨리 취업하고싶네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023186.1.6",
          "form": "으아ㅏㅏㅏ저도요ㅠㅠㅠㅠ",
          "original_form": "으아ㅏㅏㅏ저도요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023186.1.7",
          "form": "힘내요오!",
          "original_form": "힘내요오!",
          "speaker_id": "1",
          "time": "20210829 23:11"
        },
        {
          "id": "MDRW2100023186.1.8",
          "form": "name2 자격증 따시고!",
          "original_form": "&name2& 자격증 따시고!",
          "speaker_id": "1",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023186.1.9",
          "form": "처음 월급타시면 하고싶으신거 있으세요?",
          "original_form": "처음 월급타시면 하고싶으신거 있으세요?",
          "speaker_id": "1",
          "time": "20210829 23:13"
        },
        {
          "id": "MDRW2100023186.1.10",
          "form": "감사해요!!",
          "original_form": "감사해요!!",
          "speaker_id": "2",
          "time": "20210829 23:58"
        },
        {
          "id": "MDRW2100023186.1.11",
          "form": "파이팅해보겟습니다!!",
          "original_form": "파이팅해보겟습니다!!",
          "speaker_id": "2",
          "time": "20210829 23:58"
        },
        {
          "id": "MDRW2100023186.1.12",
          "form": "name1님도 꼭 공무원시험 합격하세용!!",
          "original_form": "&name1&님도 꼭 공무원시험 합격하세용!!",
          "speaker_id": "2",
          "time": "20210829 23:58"
        },
        {
          "id": "MDRW2100023186.1.13",
          "form": "월급타면...",
          "original_form": "월급타면...",
          "speaker_id": "2",
          "time": "20210829 23:58"
        },
        {
          "id": "MDRW2100023186.1.14",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100023186.1.15",
          "form": "생각안해봤는데",
          "original_form": "생각안해봤는데",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100023186.1.16",
          "form": "아마 저축하지 않을까요ㅎㅎㅎ",
          "original_form": "아마 저축하지 않을까요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100023186.1.17",
          "form": "감사합니당!!",
          "original_form": "감사합니당!!",
          "speaker_id": "1",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100023186.1.18",
          "form": "역시 저축이 최고죠ㅋㅋ 요즘같은때 돈 모아야죠",
          "original_form": "역시 저축이 최고죠ㅋㅋ 요즘같은때 돈 모아야죠",
          "speaker_id": "1",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023186.1.19",
          "form": "전 첫월급은 가족끼리 맛있는곳 가서 외식하고싶어요",
          "original_form": "전 첫월급은 가족끼리 맛있는곳 가서 외식하고싶어요",
          "speaker_id": "1",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023186.1.20",
          "form": "오웅",
          "original_form": "오웅",
          "speaker_id": "2",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023186.1.21",
          "form": "그것도 좋네요!!",
          "original_form": "그것도 좋네요!!",
          "speaker_id": "2",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023186.1.22",
          "form": "뿌듯할 것 같아요!!",
          "original_form": "뿌듯할 것 같아요!!",
          "speaker_id": "2",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023186.1.23",
          "form": "그쵸?!ㅋㅋㅋ",
          "original_form": "그쵸?!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023186.1.24",
          "form": "왠지 어깨 올라가고 말이죠ㅋㅋ",
          "original_form": "왠지 어깨 올라가고 말이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023186.1.25",
          "form": "외식하는거 말고는",
          "original_form": "외식하는거 말고는",
          "speaker_id": "2",
          "time": "20210830 08:31"
        },
        {
          "id": "MDRW2100023186.1.26",
          "form": "name1도 저축하실 건가요??",
          "original_form": "&name1&도 저축하실 건가요??",
          "speaker_id": "2",
          "time": "20210830 08:31"
        },
        {
          "id": "MDRW2100023186.1.27",
          "form": "네! 열심히 저축해서 집 사는걸 목표로 하고있어요!",
          "original_form": "네! 열심히 저축해서 집 사는걸 목표로 하고있어요!",
          "speaker_id": "1",
          "time": "20210830 08:36"
        },
        {
          "id": "MDRW2100023186.1.28",
          "form": "오홍 저도요!!",
          "original_form": "오홍 저도요!!",
          "speaker_id": "2",
          "time": "20210830 10:22"
        },
        {
          "id": "MDRW2100023186.1.29",
          "form": "요즘 집값이 너무 비싸서",
          "original_form": "요즘 집값이 너무 비싸서",
          "speaker_id": "2",
          "time": "20210830 10:22"
        },
        {
          "id": "MDRW2100023186.1.30",
          "form": "열심히 안 모으면 집도 못 살 거 같아요ㅠㅠ",
          "original_form": "열심히 안 모으면 집도 못 살 거 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 10:22"
        },
        {
          "id": "MDRW2100023186.1.31",
          "form": "맞아요ㅜㅜㅠ 진짜 희망이래봤자 주택청약일까요..",
          "original_form": "맞아요ㅜㅜㅠ 진짜 희망이래봤자 주택청약일까요..",
          "speaker_id": "1",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100023186.1.32",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 11:47"
        },
        {
          "id": "MDRW2100023186.1.33",
          "form": "취직하면 월급의 대부분이",
          "original_form": "취직하면 월급의 대부분이",
          "speaker_id": "2",
          "time": "20210830 11:47"
        },
        {
          "id": "MDRW2100023186.1.34",
          "form": "주택청약으로 들어갈 예정입니다ㅋㅋㅋㅋ",
          "original_form": "주택청약으로 들어갈 예정입니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:48"
        },
        {
          "id": "MDRW2100023186.1.35",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
