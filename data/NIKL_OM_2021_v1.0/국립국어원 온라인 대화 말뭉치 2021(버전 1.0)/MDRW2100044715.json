      "utterance": [
        {
          "id": "MDRW2100044715.1.1",
          "form": "기억에 남는 장소가 어디세요??",
          "original_form": "기억에 남는 장소가 어디세요??",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044715.1.2",
          "form": "가장 마지막에간 아사쿠사요!",
          "original_form": "가장 마지막에간 아사쿠사요!",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044715.1.3",
          "form": "거기 실크푸딩이 정말 맛있었어요 ㅎㅎㅎ",
          "original_form": "거기 실크푸딩이 정말 맛있었어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044715.1.4",
          "form": "헐 실크푸딩이라니",
          "original_form": "헐 실크푸딩이라니",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044715.1.5",
          "form": "이름만 들어도 맛있겠네요",
          "original_form": "이름만 들어도 맛있겠네요",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044715.1.6",
          "form": "가시면 꼭 사오세요",
          "original_form": "가시면 꼭 사오세요",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044715.1.7",
          "form": "저 그거 다시 먹고 싶은데 우리나라엔 그런거 안팔아서  너무 아쉬워요 ㅠ",
          "original_form": "저 그거 다시 먹고 싶은데 우리나라엔 그런거 안팔아서  너무 아쉬워요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044715.1.8",
          "form": "넵 ㅠㅠㅠ 맛있던가요...",
          "original_form": "넵 ㅠㅠㅠ 맛있던가요...",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044715.1.9",
          "form": "일반 푸딩이랑 많이 다른가요?",
          "original_form": "일반 푸딩이랑 많이 다른가요?",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044715.1.10",
          "form": "음.. 훨씬 부드러웠어요",
          "original_form": "음.. 훨씬 부드러웠어요",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044715.1.11",
          "form": "커스터드 푸딩 쉽게 생각하실 수 있는데 진짜 보들보들해서",
          "original_form": "커스터드 푸딩 쉽게 생각하실 수 있는데 진짜 보들보들해서",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044715.1.12",
          "form": "그러면서도 은근 튼튼해서 한국 들고 오는데 하나도 안깨졌어요",
          "original_form": "그러면서도 은근 튼튼해서 한국 들고 오는데 하나도 안깨졌어요",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044715.1.13",
          "form": "우리나라는 커스터드 푸딩 전문으로 파는 가게가 드물더라구요 ㅠ",
          "original_form": "우리나라는 커스터드 푸딩 전문으로 파는 가게가 드물더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044715.1.14",
          "form": "그래서 너무 아쉬워요",
          "original_form": "그래서 너무 아쉬워요",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044715.1.15",
          "form": "대구에 푸딩 빙수 들어보셨나요?",
          "original_form": "대구에 푸딩 빙수 들어보셨나요?",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044715.1.16",
          "form": "보들보들한테 튼튼하다니 ㅠㅠ",
          "original_form": "보들보들한테 튼튼하다니 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044715.1.17",
          "form": "일본가서 디저트랑 일식류는 진짜 맘껏 먹고와야 하는거같아요",
          "original_form": "일본가서 디저트랑 일식류는 진짜 맘껏 먹고와야 하는거같아요",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044715.1.18",
          "form": "진짜요! 꼭이요 저건 극구 추천하는 디저트에요!",
          "original_form": "진짜요! 꼭이요 저건 극구 추천하는 디저트에요!",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044715.1.19",
          "form": "일본가서 또 어떤 음식 드셔보셨어요?",
          "original_form": "일본가서 또 어떤 음식 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044715.1.20",
          "form": "전 가서 오마카세 먹었어요!",
          "original_form": "전 가서 오마카세 먹었어요!",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044715.1.21",
          "form": "우와아앙 맛있었겠어요!!",
          "original_form": "우와아앙 맛있었겠어요!!",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044715.1.22",
          "form": "맛도 그렇지만",
          "original_form": "맛도 그렇지만",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044715.1.23",
          "form": "호두님은 일본어를 할줄 아시나요?",
          "original_form": "호두님은 일본어를 할줄 아시나요?",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044715.1.24",
          "form": "국내 오마카세는 너무 비싸고 좀 급하게 먹는 감이 있는데",
          "original_form": "국내 오마카세는 너무 비싸고 좀 급하게 먹는 감이 있는데",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044715.1.25",
          "form": "거긴 좀 여유로워서 좋았어요",
          "original_form": "거긴 좀 여유로워서 좋았어요",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044715.1.26",
          "form": "아뇨! ㅋㅋㅋㅋ",
          "original_form": "아뇨! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044715.1.27",
          "form": "앗 저도 못해요 ㅋㅋ 친구들한테 묻어갔어요",
          "original_form": "앗 저도 못해요 ㅋㅋ 친구들한테 묻어갔어요",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044715.1.28",
          "form": "지나가다 보이는 영어가 그렇게 반가울줄을 생각도 못했다니깐요",
          "original_form": "지나가다 보이는 영어가 그렇게 반가울줄을 생각도 못했다니깐요",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044715.1.29",
          "form": "그쵸 ㅋㅋㅋㅋㅋㅋ 적당히 눈치코치로 알아듣거나",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋㅋ 적당히 눈치코치로 알아듣거나",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044715.1.30",
          "form": "아님 구글맵 켜서",
          "original_form": "아님 구글맵 켜서",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044715.1.31",
          "form": "같은 그림찾기 하면서 다녓어요 ㅋㅋㅋㅋ",
          "original_form": "같은 그림찾기 하면서 다녓어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044715.1.32",
          "form": "ㅋㅋㅋ 어떻게 여행가신거에요? 친구들과?",
          "original_form": "ㅋㅋㅋ 어떻게 여행가신거에요? 친구들과?",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044715.1.33",
          "form": "네네 친구들이랑 갔어요",
          "original_form": "네네 친구들이랑 갔어요",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044715.1.34",
          "form": "한명이 좀 일어가 돼서",
          "original_form": "한명이 좀 일어가 돼서",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044715.1.35",
          "form": "더듬더듬 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "더듬더듬 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044715.1.36",
          "form": "저는 친구들 2명이랑 같이갔는데",
          "original_form": "저는 친구들 2명이랑 같이갔는데",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044715.1.37",
          "form": "아니 글쎄 얘들이... 언어되면서 자꾸 저한테시키는거 있죠",
          "original_form": "아니 글쎄 얘들이... 언어되면서 자꾸 저한테시키는거 있죠",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044715.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋ 일어 잘하시나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 일어 잘하시나요?",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044715.1.39",
          "form": "아이고 저는 할줄아는 말이라고는",
          "original_form": "아이고 저는 할줄아는 말이라고는",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044715.1.40",
          "form": "아리가또 고자이마스 스미마셍",
          "original_form": "아리가또 고자이마스 스미마셍",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044715.1.41",
          "form": "이게 다에요 ㅋㅋㅋ",
          "original_form": "이게 다에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044715.1.42",
          "form": "가서 자꾸 말하라고 그러니까",
          "original_form": "가서 자꾸 말하라고 그러니까",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044715.1.43",
          "form": "영어로 했더니 거꾸로 거기 계신분들이 손짓 발짓으로 대화했잖아요..",
          "original_form": "영어로 했더니 거꾸로 거기 계신분들이 손짓 발짓으로 대화했잖아요..",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044715.1.44",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044715.1.45",
          "form": "저도 그랫는데 친구들이 답답했는지",
          "original_form": "저도 그랫는데 친구들이 답답했는지",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044715.1.46",
          "form": "안시키더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "안시키더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044715.1.47",
          "form": "근데도 얘들은 왜 자꾸 절 써먹을라 하는지",
          "original_form": "근데도 얘들은 왜 자꾸 절 써먹을라 하는지",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044715.1.48",
          "form": "(환장)",
          "original_form": "(환장)",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044715.1.49",
          "form": "언어 능력자들 너무 부러워요 ㅠ",
          "original_form": "언어 능력자들 너무 부러워요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:05"
        }
      ]
