      "utterance": [
        {
          "id": "MDRW2100035566.1.1",
          "form": "안녕하세용 즐겨보는 프로그램 있으세용?",
          "original_form": "안녕하세용 즐겨보는 프로그램 있으세용?",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100035566.1.2",
          "form": "저 슬의생 재밌게보고있어요",
          "original_form": "저 슬의생 재밌게보고있어요",
          "speaker_id": "2",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100035566.1.3",
          "form": "체인지데이즈도!",
          "original_form": "체인지데이즈도!",
          "speaker_id": "2",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100035566.1.4",
          "form": "체인지데이즈는 한국 드라마인가용?",
          "original_form": "체인지데이즈는 한국 드라마인가용?",
          "speaker_id": "1",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100035566.1.5",
          "form": "예능이에요!",
          "original_form": "예능이에요!",
          "speaker_id": "2",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100035566.1.6",
          "form": "헤어지기 직전인 연인들이 서로 짝바꿔서 데이트하는 ㅎ..",
          "original_form": "헤어지기 직전인 연인들이 서로 짝바꿔서 데이트하는 ㅎ..",
          "speaker_id": "2",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100035566.1.7",
          "form": "아아ㅠㅠㅠ 한국 예능인가용?",
          "original_form": "아아ㅠㅠㅠ 한국 예능인가용?",
          "speaker_id": "1",
          "time": "20210830 12:27"
        },
        {
          "id": "MDRW2100035566.1.8",
          "form": "네네! 진짜 재밌어요 ㅋㅋㅋㅋ",
          "original_form": "네네! 진짜 재밌어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:53"
        },
        {
          "id": "MDRW2100035566.1.9",
          "form": "실제 커플이에용?",
          "original_form": "실제 커플이에용?",
          "speaker_id": "1",
          "time": "20210830 18:54"
        },
        {
          "id": "MDRW2100035566.1.10",
          "form": "네!! 실제커플들이에요",
          "original_form": "네!! 실제커플들이에요",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100035566.1.11",
          "form": "한 커플은 유튜브도 있어요",
          "original_form": "한 커플은 유튜브도 있어요",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100035566.1.12",
          "form": "헐ㅋㅋㅋ 대박 완전 현실상황이네요?",
          "original_form": "헐ㅋㅋㅋ 대박 완전 현실상황이네요?",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100035566.1.13",
          "form": "오오 저 이런거 좋아하는데",
          "original_form": "오오 저 이런거 좋아하는데",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100035566.1.14",
          "form": "추천감사해요!",
          "original_form": "추천감사해요!",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100035566.1.15",
          "form": "이제 두편남았는데",
          "original_form": "이제 두편남았는데",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100035566.1.16",
          "form": "엄청 흥미로운 부분에서 끊어서...",
          "original_form": "엄청 흥미로운 부분에서 끊어서...",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100035566.1.17",
          "form": "아 아직 결말이 안났구나",
          "original_form": "아 아직 결말이 안났구나",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100035566.1.18",
          "form": "좀더 기다렷다가보새요 ㅋㅋㅋ",
          "original_form": "좀더 기다렷다가보새요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100035566.1.19",
          "form": "열심히 몰아보기해서",
          "original_form": "열심히 몰아보기해서",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100035566.1.20",
          "form": "마지막화는 본방 달려볼까요?ㅎㅎ",
          "original_form": "마지막화는 본방 달려볼까요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100035566.1.21",
          "form": "오!! 그것도 재밌겟내요 ㅋㅋㅋㅋㅋ",
          "original_form": "오!! 그것도 재밌겟내요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:59"
        }
      ]
