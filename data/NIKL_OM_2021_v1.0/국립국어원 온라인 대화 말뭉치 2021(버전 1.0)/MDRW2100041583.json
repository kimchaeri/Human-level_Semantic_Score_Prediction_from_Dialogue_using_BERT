      "utterance": [
        {
          "id": "MDRW2100041583.1.1",
          "form": "어떻게해도 좀 힘든 결말일거같아요",
          "original_form": "어떻게해도 좀 힘든 결말일거같아요",
          "speaker_id": "1",
          "time": "20210906 11:46"
        },
        {
          "id": "MDRW2100041583.1.2",
          "form": "ㅋㅋㅋㅋㅋ지금어떤 상황인가요??",
          "original_form": "ㅋㅋㅋㅋㅋ지금어떤 상황인가요??",
          "speaker_id": "2",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041583.1.3",
          "form": "지남주에 이지아 죽었어요!",
          "original_form": "지남주에 이지아 죽었어요!",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041583.1.4",
          "form": "아마 다시 살아나겠죠?",
          "original_form": "아마 다시 살아나겠죠?",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041583.1.5",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041583.1.6",
          "form": "하박사도 죽었어요!",
          "original_form": "하박사도 죽었어요!",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041583.1.7",
          "form": "헐랭 ㅠㅠㅠ",
          "original_form": "헐랭 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 12:32"
        },
        {
          "id": "MDRW2100041583.1.8",
          "form": "진짜로 죽을지는 모르겠어요..ㅎㅎ",
          "original_form": "진짜로 죽을지는 모르겠어요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 13:05"
        },
        {
          "id": "MDRW2100041583.1.9",
          "form": "구거스포는당했어요",
          "original_form": "구거스포는당했어요",
          "speaker_id": "2",
          "time": "20210906 13:18"
        },
        {
          "id": "MDRW2100041583.1.10",
          "form": "하박사 눈안보인다곸ㅋㅋ",
          "original_form": "하박사 눈안보인다곸ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:18"
        },
        {
          "id": "MDRW2100041583.1.11",
          "form": "맞아요 진짜 개웃겨여",
          "original_form": "맞아요 진짜 개웃겨여",
          "speaker_id": "1",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100041583.1.12",
          "form": "눈안보이는데 곧 죽을거래요",
          "original_form": "눈안보이는데 곧 죽을거래요",
          "speaker_id": "1",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100041583.1.13",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100041583.1.14",
          "form": "근데 떨어조서 죽음",
          "original_form": "근데 떨어조서 죽음",
          "speaker_id": "1",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100041583.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ예언자였군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ예언자였군요",
          "speaker_id": "2",
          "time": "20210906 13:30"
        },
        {
          "id": "MDRW2100041583.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:30"
        },
        {
          "id": "MDRW2100041583.1.17",
          "form": "맞아요 ㅋㅋㅋㅋ진짜 개웃김 거의 예언수준이었어요",
          "original_form": "맞아요 ㅋㅋㅋㅋ진짜 개웃김 거의 예언수준이었어요",
          "speaker_id": "1",
          "time": "20210906 13:31"
        },
        {
          "id": "MDRW2100041583.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그런툼틈히 개그공략",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그런툼틈히 개그공략",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100041583.1.19",
          "form": "좋네용",
          "original_form": "좋네용",
          "speaker_id": "2",
          "time": "20210906 13:59"
        },
        {
          "id": "MDRW2100041583.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ당시에는 아무도 이해못한 개그였지만..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ당시에는 아무도 이해못한 개그였지만..",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041583.1.21",
          "form": "아무튼 차라리 얼른 끝났으면 좋겠어요",
          "original_form": "아무튼 차라리 얼른 끝났으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210906 14:05"
        },
        {
          "id": "MDRW2100041583.1.22",
