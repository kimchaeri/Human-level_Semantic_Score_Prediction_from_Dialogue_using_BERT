      "utterance": [
        {
          "id": "MDRW2100037430.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100037430.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100037430.1.3",
          "form": "오늘 날씨는 어땠나요?",
          "original_form": "오늘 날씨는 어땠나요?",
          "speaker_id": "2",
          "time": "20210831 23:40"
        },
        {
          "id": "MDRW2100037430.1.4",
          "form": "아침부터 지금까 지비가 와요",
          "original_form": "아침부터 지금까 지비가 와요",
          "speaker_id": "1",
          "time": "20210831 23:40"
        },
        {
          "id": "MDRW2100037430.1.5",
          "form": "까지",
          "original_form": "까지",
          "speaker_id": "1",
          "time": "20210831 23:40"
        },
        {
          "id": "MDRW2100037430.1.6",
          "form": "그래서 많이 습하네요",
          "original_form": "그래서 많이 습하네요",
          "speaker_id": "1",
          "time": "20210831 23:40"
        },
        {
          "id": "MDRW2100037430.1.7",
          "form": "name2님네는 어떤가요?",
          "original_form": "&name2&님네는 어떤가요?",
          "speaker_id": "1",
          "time": "20210831 23:40"
        },
        {
          "id": "MDRW2100037430.1.8",
          "form": "오후에 소나기 한번오고 흐리기만했어요!",
          "original_form": "오후에 소나기 한번오고 흐리기만했어요!",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100037430.1.9",
          "form": "밤새 비가 온다하네요ㅠ",
          "original_form": "밤새 비가 온다하네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100037430.1.10",
          "form": "아 저는 중부지방인데 계속 비만와요 ㅠㅠ 남부쪽에 사시나요?",
          "original_form": "아 저는 중부지방인데 계속 비만와요 ㅠㅠ 남부쪽에 사시나요?",
          "speaker_id": "1",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100037430.1.11",
          "form": "저도 중부에요!",
          "original_form": "저도 중부에요!",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100037430.1.12",
          "form": "같은 중부인데도 다르네요ㅠ",
          "original_form": "같은 중부인데도 다르네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100037430.1.13",
          "form": "혹시 제가,, 방에 있어서 빗소리를 잘못들었나 하구 나가보니 제쪽은 아직 비와요",
          "original_form": "혹시 제가,, 방에 있어서 빗소리를 잘못들었나 하구 나가보니 제쪽은 아직 비와요",
          "speaker_id": "1",
          "time": "20210831 23:51"
        },
        {
          "id": "MDRW2100037430.1.14",
          "form": "세상에.. 여기 올 비가 그쪽에 아직 있나봐요..",
          "original_form": "세상에.. 여기 올 비가 그쪽에 아직 있나봐요..",
          "speaker_id": "2",
          "time": "20210831 23:53"
        },
        {
          "id": "MDRW2100037430.1.15",
          "form": "계속 비오면 문도 못열고 계시겠네요ㅠ",
          "original_form": "계속 비오면 문도 못열고 계시겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:54"
        },
        {
          "id": "MDRW2100037430.1.16",
          "form": "그래 도작년 장마보단 살만하지 않나요?",
          "original_form": "그래 도작년 장마보단 살만하지 않나요?",
          "speaker_id": "1",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100037430.1.17",
          "form": "그땐 진짜 습했어요",
          "original_form": "그땐 진짜 습했어요",
          "speaker_id": "1",
          "time": "20210831 23:55"
        },
        {
          "id": "MDRW2100037430.1.18",
          "form": "아 그건 그래요",
          "original_form": "아 그건 그래요",
          "speaker_id": "2",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100037430.1.19",
          "form": "작년에 진짜.. 에어컨 끼고 산거같아요",
          "original_form": "작년에 진짜.. 에어컨 끼고 산거같아요",
          "speaker_id": "2",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100037430.1.20",
          "form": "날이 좀 좋은 해가 참 적어요 ㅋㅋ사게절이다보니",
          "original_form": "날이 좀 좋은 해가 참 적어요 ㅋㅋ사게절이다보니",
          "speaker_id": "1",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100037430.1.21",
          "form": "한계절은 꼭 문제가 생겨요",
          "original_form": "한계절은 꼭 문제가 생겨요",
          "speaker_id": "1",
          "time": "20210831 23:58"
        },
        {
          "id": "MDRW2100037430.1.22",
          "form": "맞아요 가을이 그래서 아쉬울뿐이에요",
          "original_form": "맞아요 가을이 그래서 아쉬울뿐이에요",
          "speaker_id": "2",
          "time": "20210831 23:59"
        },
        {
          "id": "MDRW2100037430.1.23",
          "form": "ㅠ코로나라 놀러못나가긴하지만,,",
          "original_form": "ㅠ코로나라 놀러못나가긴하지만,,",
