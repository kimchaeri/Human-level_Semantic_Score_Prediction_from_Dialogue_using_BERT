      "utterance": [
        {
          "id": "MDRW2100045947.1.1",
          "form": "ㅇㅇ 없어서 못먹짐 암ㅅ",
          "original_form": "ㅇㅇ 없어서 못먹짐 암ㅅ",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045947.1.2",
          "form": "..나쁜말이래",
          "original_form": "..나쁜말이래",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045947.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045947.1.4",
          "form": "ㅇㅇ 집엣 나왓어",
          "original_form": "ㅇㅇ 집엣 나왓어",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045947.1.5",
          "form": "버스 탄거야?",
          "original_form": "버스 탄거야?",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045947.1.6",
          "form": "아니 아직!",
          "original_form": "아니 아직!",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045947.1.7",
          "form": "10분 뒤에 오는 버스 타려고 맞춰서 나왔엌",
          "original_form": "10분 뒤에 오는 버스 타려고 맞춰서 나왔엌",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045947.1.8",
          "form": "출근하는데 한시간 잡아야하네",
          "original_form": "출근하는데 한시간 잡아야하네",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045947.1.9",
          "form": "언니는 지하철?",
          "original_form": "언니는 지하철?",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045947.1.10",
          "form": "ㅇㅇ..",
          "original_form": "ㅇㅇ..",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045947.1.11",
          "form": "그동네가 좀 걸리지...",
          "original_form": "그동네가 좀 걸리지...",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045947.1.12",
          "form": "ㅇㅇ 난 늦으면 지하철 타고",
          "original_form": "ㅇㅇ 난 늦으면 지하철 타고",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045947.1.13",
          "form": "우리집이 문제인가 버스가 문제인가",
          "original_form": "우리집이 문제인가 버스가 문제인가",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045947.1.14",
          "form": "아니면 걸어와 ㅋㅋㅋ",
          "original_form": "아니면 걸어와 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045947.1.15",
          "form": "제일 부럽다...ㅋㅋㅋㅋㅋ",
          "original_form": "제일 부럽다...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045947.1.16",
          "form": "그치 ㅋㅋㅋㅋ 나도 이것때문에 사택산다",
          "original_form": "그치 ㅋㅋㅋㅋ 나도 이것때문에 사택산다",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045947.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 쩔어..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 쩔어..",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045947.1.18",
          "form": "아님 자차라도 있었으면 싶고",
          "original_form": "아님 자차라도 있었으면 싶고",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045947.1.19",
          "form": "그래서 차있는 사람들이 버스 안타나봐",
          "original_form": "그래서 차있는 사람들이 버스 안타나봐",
          "speaker_id": "2",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100045947.1.20",
          "form": "ㅋㅋㅋㅋㅋ 자차로 출퇴근하면",
          "original_form": "ㅋㅋㅋㅋㅋ 자차로 출퇴근하면",
          "speaker_id": "1",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100045947.1.21",
          "form": "느낌이 또 새롭대",
          "original_form": "느낌이 또 새롭대",
          "speaker_id": "1",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100045947.1.22",
          "form": "ㅋㅋㅋㅋ 좀더 편해지는거 아닐까",
          "original_form": "ㅋㅋㅋㅋ 좀더 편해지는거 아닐까",
          "speaker_id": "2",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100045947.1.23",
          "form": "길막히는 거야 머..",
          "original_form": "길막히는 거야 머..",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045947.1.24",
          "form": "차 안에서 앉아있잔ㄹ아(?",
          "original_form": "차 안에서 앉아있잔ㄹ아(?",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045947.1.25",
          "form": "그리고 뭔가 공간이 분리된 느낌이라",
          "original_form": "그리고 뭔가 공간이 분리된 느낌이라",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045947.1.26",
          "form": "출근길에 사람한테 치이는 느낌이 없대...",
          "original_form": "출근길에 사람한테 치이는 느낌이 없대...",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045947.1.27",
          "form": "ㅇㅎ...",
          "original_form": "ㅇㅎ...",
          "speaker_id": "2",
