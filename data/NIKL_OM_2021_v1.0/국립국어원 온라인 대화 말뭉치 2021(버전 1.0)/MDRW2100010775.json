        {
          "id": "MDRW2100010775.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010775.1.3",
          "form": "전 강아지 키워요ㅎ",
          "original_form": "전 강아지 키워요ㅎ",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010775.1.4",
          "form": "저흰 벌레키워요ㅠ",
          "original_form": "저흰 벌레키워요ㅠ",
          "speaker_id": "2",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100010775.1.5",
          "form": "벌레ㅠ",
          "original_form": "벌레ㅠ",
          "speaker_id": "1",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100010775.1.6",
          "form": "ㅋㅋㅋㅋ사슴벌레 장수풍뎅이요. 아들이 곤충 홀릭입니다",
          "original_form": "ㅋㅋㅋㅋ사슴벌레 장수풍뎅이요. 아들이 곤충 홀릭입니다",
          "speaker_id": "2",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100010775.1.7",
          "form": "아 곤충이구나~~",
          "original_form": "아 곤충이구나~~",
          "speaker_id": "1",
          "time": "20210824 08:41"
        },
        {
          "id": "MDRW2100010775.1.8",
          "form": "ㅎㅎㅎㅎ 저에겐 벌레지만요.. 강아지 오래키우셨어요?",
          "original_form": "ㅎㅎㅎㅎ 저에겐 벌레지만요.. 강아지 오래키우셨어요?",
          "speaker_id": "2",
          "time": "20210824 09:17"
        },
        {
          "id": "MDRW2100010775.1.9",
          "form": "1년됐어요ㅎ",
          "original_form": "1년됐어요ㅎ",
          "speaker_id": "1",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100010775.1.10",
          "form": "아 처음 키워보시는거에요?",
          "original_form": "아 처음 키워보시는거에요?",
          "speaker_id": "2",
          "time": "20210824 12:06"
        },
        {
          "id": "MDRW2100010775.1.11",
          "form": "예전에 엄마가 키우셨는데",
          "original_form": "예전에 엄마가 키우셨는데",
          "speaker_id": "1",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100010775.1.12",
          "form": "제가 오롯이 키우는건 첨이여ㅎ",
          "original_form": "제가 오롯이 키우는건 첨이여ㅎ",
          "speaker_id": "1",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100010775.1.13",
          "form": "오~ 정말 자식만큼 확 정이 드나요?",
          "original_form": "오~ 정말 자식만큼 확 정이 드나요?",
          "speaker_id": "2",
          "time": "20210824 16:43"
        },
        {
          "id": "MDRW2100010775.1.14",
          "form": "네 자식과는 또다른",
          "original_form": "네 자식과는 또다른",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100010775.1.15",
          "form": "절 다 받아주는",
          "original_form": "절 다 받아주는",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100010775.1.16",
          "form": "아하... 그런거군요! 아이때문에 기르기 시작한건 아니고요?",
          "original_form": "아하... 그런거군요! 아이때문에 기르기 시작한건 아니고요?",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100010775.1.17",
          "form": "네 첨엔 아이때문에",
          "original_form": "네 첨엔 아이때문에",
          "speaker_id": "1",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100010775.1.18",
          "form": "근데 가족이되었어요ㅎ",
          "original_form": "근데 가족이되었어요ㅎ",
