      "utterance": [
        {
          "id": "MDRW2100020935.1.1",
          "form": "ㅋㅋㅋㅋㅋ네 군대 힘들어욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ네 군대 힘들어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020935.1.2",
          "form": "제일가고픈 공연잔시회므ㅏ에여",
          "original_form": "제일가고픈 공연잔시회므ㅏ에여",
          "speaker_id": "1",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020935.1.3",
          "form": "저 그 중앙박물관 이건희 소장품 전시회요",
          "original_form": "저 그 중앙박물관 이건희 소장품 전시회요",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020935.1.4",
          "form": "그거랑 요시고 전시회랑",
          "original_form": "그거랑 요시고 전시회랑",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020935.1.5",
          "form": "와 이건희소장품",
          "original_form": "와 이건희소장품",
          "speaker_id": "1",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020935.1.6",
          "form": "ㅋㅋㅋㅋ8월에 서울 가고싶었는데 결국 못갔네여",
          "original_form": "ㅋㅋㅋㅋ8월에 서울 가고싶었는데 결국 못갔네여",
          "speaker_id": "2",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020935.1.7",
          "form": "잉잉 좀 더 선선해지먄 가유",
          "original_form": "잉잉 좀 더 선선해지먄 가유",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020935.1.8",
          "form": "코로나도 좀 잠잠해지거",
          "original_form": "코로나도 좀 잠잠해지거",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100020935.1.9",
          "form": "보고싶은 공연 전시 있어요?",
          "original_form": "보고싶은 공연 전시 있어요?",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100020935.1.10",
          "form": "저는 아스트로 콘서트요 ㅎㅎ",
          "original_form": "저는 아스트로 콘서트요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100020935.1.11",
          "form": "ㅋㅋㅋ한대여?",
          "original_form": "ㅋㅋㅋ한대여?",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100020935.1.12",
          "form": "하지않을까요?ㅋㅋ 무기약이에요 ㅌㅋㅌ",
          "original_form": "하지않을까요?ㅋㅋ 무기약이에요 ㅌㅋㅌ",
          "speaker_id": "1",
          "time": "20210828 18:56"
        },
        {
          "id": "MDRW2100020935.1.13",
          "form": "이제 1위도 하고 했으니 준비할거 같네요",
          "original_form": "이제 1위도 하고 했으니 준비할거 같네요",
          "speaker_id": "2",
          "time": "20210828 18:57"
        },
        {
          "id": "MDRW2100020935.1.14",
          "form": "맞아요 흥해라",
          "original_form": "맞아요 흥해라",
          "speaker_id": "1",
          "time": "20210828 18:57"
        },
        {
          "id": "MDRW2100020935.1.15",
          "form": "씨에프 엄청 찍네요",
          "original_form": "씨에프 엄청 찍네요",
