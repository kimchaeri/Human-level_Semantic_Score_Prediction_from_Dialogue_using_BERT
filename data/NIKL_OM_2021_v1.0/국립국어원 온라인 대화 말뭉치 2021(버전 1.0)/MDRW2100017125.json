      "utterance": [
        {
          "id": "MDRW2100017125.1.1",
          "form": "한글화 안된 게임 못하겠음",
          "original_form": "한글화 안된 게임 못하겠음",
          "speaker_id": "1",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017125.1.2",
          "form": "맞아 힘들어 힝",
          "original_form": "맞아 힘들어 힝",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017125.1.3",
          "form": "앙스타도 좀 관심있었는데",
          "original_form": "앙스타도 좀 관심있었는데",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017125.1.4",
          "form": "카카오가 런칭해줘서 해봄",
          "original_form": "카카오가 런칭해줘서 해봄",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017125.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017125.1.6",
          "form": "베이직 리듬게임?",
          "original_form": "베이직 리듬게임?",
          "speaker_id": "2",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017125.1.7",
          "form": "아 리듬게임",
          "original_form": "아 리듬게임",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017125.1.8",
          "form": "주제 도서였네",
          "original_form": "주제 도서였네",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017125.1.9",
          "form": "아. 앙스타 소설도 있다ㅣ0",
          "original_form": "아. 앙스타 소설도 있다ㅣ0",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017125.1.10",
          "form": "그러네ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그러네ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017125.1.11",
          "form": "좀 읽어봤는데",
          "original_form": "좀 읽어봤는데",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017125.1.12",
          "form": "별로라 보다 말았어",
          "original_form": "별로라 보다 말았어",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017125.1.13",
          "form": "멘스랑 다른가봄?",
          "original_form": "멘스랑 다른가봄?",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017125.1.14",
          "form": "메인스토리랑 똑같은데 그래서 별로야",
          "original_form": "메인스토리랑 똑같은데 그래서 별로야",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017125.1.15",
          "form": "앙스타는 메인스토리가 별로거든",
          "original_form": "앙스타는 메인스토리가 별로거든",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017125.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017125.1.17",
          "form": "외전같은건 없나봐?",
          "original_form": "외전같은건 없나봐?",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017125.1.18",
          "form": "중요한 문제지 소설 원작이 별로야",
          "original_form": "중요한 문제지 소설 원작이 별로야",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017125.1.19",
          "form": "있다고 하는데 거기까지 안 봤어",
          "original_form": "있다고 하는데 거기까지 안 봤어",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017125.1.20",
          "form": "아이돌을 가장한 혁명 스토리ㅋㅋㅋㅋ",
          "original_form": "아이돌을 가장한 혁명 스토리ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017125.1.21",
          "form": "재미없긴했음",
          "original_form": "재미없긴했음",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017125.1.22",
