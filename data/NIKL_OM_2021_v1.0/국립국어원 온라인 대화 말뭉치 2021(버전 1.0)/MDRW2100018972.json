        },
        {
          "id": "MDRW2100018972.1.3",
          "form": "5시쯤에나갔는데",
          "original_form": "5시쯤에나갔는데",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018972.1.4",
          "form": "아 그럼 모를 수도 6시 20분에서 반 사이에 비 내리기 시작했음",
          "original_form": "아 그럼 모를 수도 6시 20분에서 반 사이에 비 내리기 시작했음",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018972.1.5",
          "form": "아 엄청 짧게 왔나보네",
          "original_form": "아 엄청 짧게 왔나보네",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018972.1.6",
          "form": "8시쯤에도안왔는데",
          "original_form": "8시쯤에도안왔는데",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018972.1.7",
          "form": "ㅇㅇㄹㅇ",
          "original_form": "ㅇㅇㄹㅇ",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018972.1.8",
          "form": "짧은 사이에 확 내리고 갔음",
          "original_form": "짧은 사이에 확 내리고 갔음",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018972.1.9",
          "form": "이게 스콜이 아니면 뭔지",
          "original_form": "이게 스콜이 아니면 뭔지",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018972.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018972.1.11",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018972.1.12",
          "form": "동남아날씨랑비슷해짐",
          "original_form": "동남아날씨랑비슷해짐",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018972.1.13",
          "form": "아 지구온난화 정말",
          "original_form": "아 지구온난화 정말",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018972.1.14",
          "form": "태풍온다하지않았나?",
          "original_form": "태풍온다하지않았나?",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018972.1.15",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018972.1.16",
          "form": "뭐였지 네글자였음",
          "original_form": "뭐였지 네글자였음",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018972.1.17",
          "form": "이미왔어?",
          "original_form": "이미왔어?",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018972.1.18",
          "form": "지나가고 있는 데?",
          "original_form": "지나가고 있는 데?",
          "speaker_id": "2",
          "time": "20210827 14:13"
        },
        {
          "id": "MDRW2100018972.1.19",
          "form": "오마이스",
          "original_form": "오마이스",
          "speaker_id": "2",
          "time": "20210827 14:13"
        },
        {
          "id": "MDRW2100018972.1.20",
          "form": "아",
