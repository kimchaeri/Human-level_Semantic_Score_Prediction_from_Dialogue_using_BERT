      "utterance": [
        {
          "id": "MDRW2100025525.1.1",
          "form": "ㅋㅋㄱㅋㅋㅋ 생선 짜서 밥원래 더많이먹어야해",
          "original_form": "ㅋㅋㄱㅋㅋㅋ 생선 짜서 밥원래 더많이먹어야해",
          "speaker_id": "1",
          "time": "20210901 12:45"
        },
        {
          "id": "MDRW2100025525.1.2",
          "form": "거기별루안짜든데",
          "original_form": "거기별루안짜든데",
          "speaker_id": "2",
          "time": "20210901 12:45"
        },
        {
          "id": "MDRW2100025525.1.3",
          "form": "안짜다고생각하는것조차 착각일수있어",
          "original_form": "안짜다고생각하는것조차 착각일수있어",
          "speaker_id": "1",
          "time": "20210901 12:45"
        },
        {
          "id": "MDRW2100025525.1.4",
          "form": "ㅋㅋㄱㅋㅋㅋ 아근데거기 임연수 존맛인디",
          "original_form": "ㅋㅋㄱㅋㅋㅋ 아근데거기 임연수 존맛인디",
          "speaker_id": "1",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100025525.1.5",
          "form": "배고프군",
          "original_form": "배고프군",
          "speaker_id": "1",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100025525.1.6",
          "form": "ㄴㄴ 제가 요새 닭찌먹고 그러느라 혀가 민감맨이 되었는대",
          "original_form": "ㄴㄴ 제가 요새 닭찌먹고 그러느라 혀가 민감맨이 되었는대",
          "speaker_id": "2",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100025525.1.7",
          "form": "안짯으",
          "original_form": "안짯으",
          "speaker_id": "2",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100025525.1.8",
          "form": "오 그래?",
          "original_form": "오 그래?",
          "speaker_id": "1",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100025525.1.9",
          "form": "근데원래",
          "original_form": "근데원래",
          "speaker_id": "1",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100025525.1.10",
          "form": "임연수는 담백하고 고등어는 기름졌어",
          "original_form": "임연수는 담백하고 고등어는 기름졌어",
          "speaker_id": "2",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100025525.1.11",
          "form": "고기반찬이든 생선반찬이든",
          "original_form": "고기반찬이든 생선반찬이든",
          "speaker_id": "1",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100025525.1.12",
          "form": "밥도둑",
          "original_form": "밥도둑",
          "speaker_id": "1",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100025525.1.13",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "2",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100025525.1.14",
          "form": "아 배고프다",
          "original_form": "아 배고프다",
          "speaker_id": "1",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100025525.1.15",
          "form": "그뭐여 계란찜 포기하자니깐 시켜서",
          "original_form": "그뭐여 계란찜 포기하자니깐 시켜서",
          "speaker_id": "2",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100025525.1.16",
          "form": "나도생선에 된찌 계란찜",
          "original_form": "나도생선에 된찌 계란찜",
          "speaker_id": "1",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100025525.1.17",
          "form": "엄마 두입먹고",
          "original_form": "엄마 두입먹고",
          "speaker_id": "2",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100025525.1.18",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100025525.1.19",
          "form": "너다먹어",
          "original_form": "너다먹어",
          "speaker_id": "2",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100025525.1.20",
          "form": "하더라",
          "original_form": "하더라",
          "speaker_id": "2",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100025525.1.21",
          "form": "계란찜좋아해서그래",
          "original_form": "계란찜좋아해서그래",
          "speaker_id": "1",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100025525.1.22",
          "form": "오늘같이 추운날은 뜨끈한국물이국룰인데",
