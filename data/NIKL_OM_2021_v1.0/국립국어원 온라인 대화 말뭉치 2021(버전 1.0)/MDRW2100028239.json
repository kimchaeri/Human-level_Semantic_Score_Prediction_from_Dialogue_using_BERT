      "utterance": [
        {
          "id": "MDRW2100028239.1.1",
          "form": "저녁....뭐드십니까. ....",
          "original_form": "저녁....뭐드십니까. ....",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028239.1.2",
          "form": "저녁으로 슬라이스햄으로 감싼 밥과 전어를 먹었습니다",
          "original_form": "저녁으로 슬라이스햄으로 감싼 밥과 전어를 먹었습니다",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028239.1.3",
          "form": "말만 들으면 엄청 고급스럽군요",
          "original_form": "말만 들으면 엄청 고급스럽군요",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028239.1.4",
          "form": "저는 장례식장밥을 먹으러 갑니다. 장례식장 메뉴가 뭐지",
          "original_form": "저는 장례식장밥을 먹으러 갑니다. 장례식장 메뉴가 뭐지",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028239.1.5",
          "form": "육개장...?",
          "original_form": "육개장...?",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100028239.1.6",
          "form": "육개장이랑",
          "original_form": "육개장이랑",
          "speaker_id": "1",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100028239.1.7",
          "form": "머릿고기? 편육? 그런 거 주지 않나",
          "original_form": "머릿고기? 편육? 그런 거 주지 않나",
          "speaker_id": "1",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100028239.1.8",
          "form": "집에가서 흰쌀밥에 김말아먹고싶다",
          "original_form": "집에가서 흰쌀밥에 김말아먹고싶다",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028239.1.9",
          "form": "심플이즈베스트",
          "original_form": "심플이즈베스트",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028239.1.10",
          "form": "내가 점심 한정된 곳에서 시켜먹다보니 그런게.그리워져",
          "original_form": "내가 점심 한정된 곳에서 시켜먹다보니 그런게.그리워져",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028239.1.11",
          "form": "햄버거 김밥 중국집 피자 돈까쓰",
          "original_form": "햄버거 김밥 중국집 피자 돈까쓰",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028239.1.12",
          "form": "내 점심루틴",
          "original_form": "내 점심루틴",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028239.1.13",
          "form": "가끔 그런 거 있지ㅋㅋㅋㅋㅋㅋ 아무리 주변에 먹을 곳이 많아도 결국 물리게 되어잇음",
          "original_form": "가끔 그런 거 있지ㅋㅋㅋㅋㅋㅋ 아무리 주변에 먹을 곳이 많아도 결국 물리게 되어잇음",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028239.1.14",
          "form": "진짜 집밥먹고싶어져 ㅠㅠㅠ 백반집 없는게 한",
          "original_form": "진짜 집밥먹고싶어져 ㅠㅠㅠ 백반집 없는게 한",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028239.1.15",
          "form": "백반집이 없어? 싱기하다 ㅇ0ㅇ",
          "original_form": "백반집이 없어? 싱기하다 ㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028239.1.16",
          "form": "ㅇㅇㅇㅇ 진짜 신기하게도 없더라",
          "original_form": "ㅇㅇㅇㅇ 진짜 신기하게도 없더라",
          "speaker_id": "2",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100028239.1.17",
          "form": "백반이랑 다양한 반찬나오는 집 생기면 좋겠어ㅠㅠ",
          "original_form": "백반이랑 다양한 반찬나오는 집 생기면 좋겠어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100028239.1.18",
          "form": "나 있었던 곳은 메인반찬 두종해서",
          "original_form": "나 있었던 곳은 메인반찬 두종해서",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028239.1.19",
          "form": "매일 조금씩 변용했던거 같음",
          "original_form": "매일 조금씩 변용했던거 같음",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028239.1.20",
          "form": "끼당 5000원이었지 건물주이지 않았을까",
          "original_form": "끼당 5000원이었지 건물주이지 않았을까",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028239.1.21",
          "form": "아 좋다 나 그런 곳 원해 ㅠㅠㅠ",
          "original_form": "아 좋다 나 그런 곳 원해 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028239.1.22",
          "form": "그런 곳도 없어? 한식뷔페",
          "original_form": "그런 곳도 없어? 한식뷔페",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100028239.1.23",
          "form": "웅 ㅠㅠㅠㅠ 진짜 암것도 없다 그냥 분식집",
          "original_form": "웅 ㅠㅠㅠㅠ 진짜 암것도 없다 그냥 분식집",
          "speaker_id": "2",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100028239.1.24",
          "form": "김밥집은 있는데 잘 안먹어",
          "original_form": "김밥집은 있는데 잘 안먹어",
