      "utterance": [
        {
          "id": "MDRW2100009776.1.1",
          "form": "코로나 이후로 여행 가보신적 있나요?",
          "original_form": "코로나 이후로 여행 가보신적 있나요?",
          "speaker_id": "1",
          "time": "20210823 09:46"
        },
        {
          "id": "MDRW2100009776.1.2",
          "form": "아니요 집근처말고는 못갔네요ㅠㅠ",
          "original_form": "아니요 집근처말고는 못갔네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 10:07"
        },
        {
          "id": "MDRW2100009776.1.3",
          "form": "여행 다녀오셨나요?",
          "original_form": "여행 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210823 10:07"
        },
        {
          "id": "MDRW2100009776.1.4",
          "form": "저도 못 가봤어요 코로나라서 나가기 참 그렇더라구요",
          "original_form": "저도 못 가봤어요 코로나라서 나가기 참 그렇더라구요",
          "speaker_id": "1",
          "time": "20210823 10:09"
        },
        {
          "id": "MDRW2100009776.1.5",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 10:26"
        },
        {
          "id": "MDRW2100009776.1.6",
          "form": "빨리 편히 여행 갈 수 있는 날이 왔으면 좋겠네요..",
          "original_form": "빨리 편히 여행 갈 수 있는 날이 왔으면 좋겠네요..",
          "speaker_id": "2",
          "time": "20210823 10:26"
        },
        {
          "id": "MDRW2100009776.1.7",
          "form": "마지막 여행지가 어디신가요?",
          "original_form": "마지막 여행지가 어디신가요?",
          "speaker_id": "2",
          "time": "20210823 10:26"
        },
        {
          "id": "MDRW2100009776.1.8",
          "form": "코로나 터지기 딱 1년전에 오키나와 갔다왔었어요",
          "original_form": "코로나 터지기 딱 1년전에 오키나와 갔다왔었어요",
          "speaker_id": "1",
          "time": "20210823 10:27"
        },
        {
          "id": "MDRW2100009776.1.9",
          "form": "오 재밌게 다녀오셨나요?",
          "original_form": "오 재밌게 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210823 12:52"
        },
        {
          "id": "MDRW2100009776.1.10",
          "form": "좋은 곳만 잘 구경하고 왔어요 음식도 잘 맞았고",
          "original_form": "좋은 곳만 잘 구경하고 왔어요 음식도 잘 맞았고",
          "speaker_id": "1",
          "time": "20210823 13:12"
        },
        {
          "id": "MDRW2100009776.1.11",
          "form": "아 참고로 패키지로 다녀왔어요",
          "original_form": "아 참고로 패키지로 다녀왔어요",
          "speaker_id": "1",
          "time": "20210823 13:13"
        },
        {
          "id": "MDRW2100009776.1.12",
          "form": "패키지라 편하고 좋으셨겠어요!",
          "original_form": "패키지라 편하고 좋으셨겠어요!",
          "speaker_id": "2",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100009776.1.13",
          "form": "저는 대만 여행 다녀온게 마지막이네요ㅠㅠ",
          "original_form": "저는 대만 여행 다녀온게 마지막이네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 13:15"
        },
        {
          "id": "MDRW2100009776.1.14",
          "form": "대만은 어땠나요? 궁금해요",
          "original_form": "대만은 어땠나요? 궁금해요",
          "speaker_id": "1",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100009776.1.15",
          "form": "대만 볼 것도 많고 음식도 입에 맞아서 좋았습니다ㅎㅎ",
          "original_form": "대만 볼 것도 많고 음식도 입에 맞아서 좋았습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100009776.1.16",
          "form": "오키나와에서 사올만한게 있나요?",
          "original_form": "오키나와에서 사올만한게 있나요?",
          "speaker_id": "2",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100009776.1.17",
          "form": "어떤거 사오셨나요?",
          "original_form": "어떤거 사오셨나요?",
          "speaker_id": "2",
          "time": "20210824 13:04"
        },
        {
          "id": "MDRW2100009776.1.18",
          "form": "자색고구마타르트 사왔었어요",
          "original_form": "자색고구마타르트 사왔었어요",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100009776.1.19",
          "form": "처음에는 너무 달아서 싫어했는데 가끔 생각나더라구요",
          "original_form": "처음에는 너무 달아서 싫어했는데 가끔 생각나더라구요",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100009776.1.20",
          "form": "오 맛있을거 같네요!",
          "original_form": "오 맛있을거 같네요!",
          "speaker_id": "2",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100009776.1.21",
          "form": "저는 대만 누가크래커 사왔는데 가끔 생각나네요...",
          "original_form": "저는 대만 누가크래커 사왔는데 가끔 생각나네요...",
          "speaker_id": "2",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100009776.1.22",
          "form": "누가크래커는 한국에서도 팔던데 대만거랑 맛이 똑같나요?",
          "original_form": "누가크래커는 한국에서도 팔던데 대만거랑 맛이 똑같나요?",
          "speaker_id": "1",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100009776.1.23",
          "form": "한국에서 파는걸 안먹어봤어요ㅠㅠㅠ",
          "original_form": "한국에서 파는걸 안먹어봤어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100009776.1.24",
          "form": "근데 비슷하지않을까 싶네요!",
          "original_form": "근데 비슷하지않을까 싶네요!",
          "speaker_id": "2",
          "time": "20210824 17:28"
        }
      ]
    }
  ]
