          "speaker_id": "2",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100030394.1.3",
          "form": "왜 관심없어",
          "original_form": "왜 관심없어",
          "speaker_id": "1",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100030394.1.4",
          "form": "엄마가 좋아하는 사람 아니야",
          "original_form": "엄마가 좋아하는 사람 아니야",
          "speaker_id": "2",
          "time": "20210904 00:39"
        },
        {
          "id": "MDRW2100030394.1.5",
          "form": "엄마가 좋아하는 사람은 누군데",
          "original_form": "엄마가 좋아하는 사람은 누군데",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030394.1.6",
          "form": "호중이",
          "original_form": "호중이",
          "speaker_id": "2",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030394.1.7",
          "form": "걔는 범죄자잖아",
          "original_form": "걔는 범죄자잖아",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030394.1.8",
          "form": "걔가 왜 범죄자야",
          "original_form": "걔가 왜 범죄자야",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030394.1.9",
          "form": "불법도박했다는디",
          "original_form": "불법도박했다는디",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030394.1.10",
          "form": "말도안돼....",
          "original_form": "말도안돼....",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030394.1.11",
          "form": "기사 보내줘??!!",
          "original_form": "기사 보내줘??!!",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030394.1.12",
          "form": "아냐 안보고싶어",
          "original_form": "아냐 안보고싶어",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030394.1.13",
          "form": "호중이 계속 좋아할거야 말거야",
          "original_form": "호중이 계속 좋아할거야 말거야",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030394.1.14",
          "form": "아 안좋아할게!",
          "original_form": "아 안좋아할게!",
