      "utterance": [
        {
          "id": "MDRW2100006729.1.1",
          "form": "여행 다녀오셨나요?",
          "original_form": "여행 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100006729.1.2",
          "form": "최근엔 못다니네요",
          "original_form": "최근엔 못다니네요",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100006729.1.3",
          "form": "여름 휴가 안다녀오셨어요?",
          "original_form": "여름 휴가 안다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100006729.1.4",
          "form": "네 바쁘기도하고 코로나때문에 여유가없네요",
          "original_form": "네 바쁘기도하고 코로나때문에 여유가없네요",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100006729.1.5",
          "form": "갔다오셨나요?",
          "original_form": "갔다오셨나요?",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100006729.1.6",
          "form": "네 전 한적한 전라남도로",
          "original_form": "네 전 한적한 전라남도로",
          "speaker_id": "1",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100006729.1.7",
          "form": "전라도 어디 다녀오셨나요?",
          "original_form": "전라도 어디 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100006729.1.8",
          "form": "진도라는 섬이었어요",
          "original_form": "진도라는 섬이었어요",
          "speaker_id": "1",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100006729.1.9",
          "form": "진도 초딩때 가봤는데",
          "original_form": "진도 초딩때 가봤는데",
          "speaker_id": "2",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100006729.1.10",
          "form": "추억이네요",
          "original_form": "추억이네요",
          "speaker_id": "2",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100006729.1.11",
          "form": "와 기억나세요?ㅎㅎ",
          "original_form": "와 기억나세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100006729.1.12",
          "form": "그냥 갔다는 사실만 기억나요 ㅋㅋ",
          "original_form": "그냥 갔다는 사실만 기억나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100006729.1.13",
          "form": "빨리 코로나 끝나서 해외여행 가고싶네요",
          "original_form": "빨리 코로나 끝나서 해외여행 가고싶네요",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100006729.1.14",
          "form": "해외가 좋지요ㅠ",
          "original_form": "해외가 좋지요ㅠ",
          "speaker_id": "1",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100006729.1.15",
          "form": "어디 좋으셨어요?",
          "original_form": "어디 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210908 08:04"
        },
        {
          "id": "MDRW2100006729.1.16",
          "form": "저는 포르투갈이 정말 좋더라구요",
          "original_form": "저는 포르투갈이 정말 좋더라구요",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100006729.1.17",
          "form": "name1님은 기억에 남는 여행지 있으신가요",
          "original_form": "&name1&님은 기억에 남는 여행지 있으신가요",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100006729.1.18",
          "form": "와 안가봤어요",
          "original_form": "와 안가봤어요",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100006729.1.19",
          "form": "전 파리요ㅎㅎ",
          "original_form": "전 파리요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100006729.1.20",
          "form": "포르투갈은 뭐가 좋으셨어요?",
