      "utterance": [
        {
          "id": "MDRW2100041928.1.1",
          "form": "언제까지 ?",
          "original_form": "언제까지 ?",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041928.1.2",
          "form": "겁이없다기보다는....지 감정이 먼저인거다",
          "original_form": "겁이없다기보다는....지 감정이 먼저인거다",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041928.1.3",
          "form": "고집이쎈거가",
          "original_form": "고집이쎈거가",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041928.1.4",
          "form": "초등학교 6학년까지 잘 키우라했다",
          "original_form": "초등학교 6학년까지 잘 키우라했다",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041928.1.5",
          "form": "그때 뇌형성된다고",
          "original_form": "그때 뇌형성된다고",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041928.1.6",
          "form": "학교샘도 이해못하시겠네",
          "original_form": "학교샘도 이해못하시겠네",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041928.1.7",
          "form": "학교에선 그렇게 고집 안부리지",
          "original_form": "학교에선 그렇게 고집 안부리지",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041928.1.8",
          "form": "줌안드갔잖아",
          "original_form": "줌안드갔잖아",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041928.1.9",
          "form": "저정도인줄 모를걸",
          "original_form": "저정도인줄 모를걸",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041928.1.10",
          "form": "왜 안 들어왔는지 모르지",
          "original_form": "왜 안 들어왔는지 모르지",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041928.1.11",
          "form": "맞나 ㅜ",
          "original_form": "맞나 ㅜ",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041928.1.12",
          "form": "그냥 늦잠잔다생각하겠지",
          "original_form": "그냥 늦잠잔다생각하겠지",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041928.1.13",
          "form": "안들어오는아이가 없던데우리는",
          "original_form": "안들어오는아이가 없던데우리는",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041928.1.14",
          "form": "우리반에도 안들어오는 얘들 있다",
          "original_form": "우리반에도 안들어오는 얘들 있다",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041928.1.15",
          "form": "그래생각하는가",
          "original_form": "그래생각하는가",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041928.1.16",
          "form": "맞나 ㅜ",
          "original_form": "맞나 ㅜ",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041928.1.17",
          "form": "내가 계속 문자보내고",
          "original_form": "내가 계속 문자보내고",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041928.1.18",
          "form": "다 들어오는게 신기하다",
          "original_form": "다 들어오는게 신기하다",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041928.1.19",
          "form": "진짜 ㅠ 공부에 애착은 있나",
          "original_form": "진짜 ㅠ 공부에 애착은 있나",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041928.1.20",
          "form": "수업시작하면 반만 들어와있다",
          "original_form": "수업시작하면 반만 들어와있다",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041928.1.21",
          "form": "공부는 예전보다 나아졌다",
          "original_form": "공부는 예전보다 나아졌다",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041928.1.22",
          "form": "자신감도 생긴것같고",
          "original_form": "자신감도 생긴것같고",
          "speaker_id": "1",
          "time": "20210906 17:44"
