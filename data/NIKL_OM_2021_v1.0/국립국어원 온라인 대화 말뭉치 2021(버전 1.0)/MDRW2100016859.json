      "utterance": [
        {
          "id": "MDRW2100016859.1.1",
          "form": "육퇴했나??",
          "original_form": "육퇴했나??",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100016859.1.2",
          "form": "난 오늘 시엄마랑 도련님 와서",
          "original_form": "난 오늘 시엄마랑 도련님 와서",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100016859.1.3",
          "form": "같이 밥 먹고 이제 애들 재웠음",
          "original_form": "같이 밥 먹고 이제 애들 재웠음",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100016859.1.4",
          "form": "내일 캠핑 가야 돼서  짐 싸야 되는데 피곤하네",
          "original_form": "내일 캠핑 가야 돼서  짐 싸야 되는데 피곤하네",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100016859.1.5",
          "form": "와 내일캠핑가나ㅋ2박3일?ㅋㅋ좋다이",
          "original_form": "와 내일캠핑가나ㅋ2박3일?ㅋㅋ좋다이",
          "speaker_id": "1",
          "time": "20210827 01:13"
        },
        {
          "id": "MDRW2100016859.1.6",
          "form": "어 지금은 출근했음",
          "original_form": "어 지금은 출근했음",
          "speaker_id": "2",
          "time": "20210827 08:56"
        },
        {
          "id": "MDRW2100016859.1.7",
          "form": "이따가 애들 다 하원 하고 다섯시쯤 출발",
          "original_form": "이따가 애들 다 하원 하고 다섯시쯤 출발",
          "speaker_id": "2",
          "time": "20210827 08:56"
        },
        {
          "id": "MDRW2100016859.1.8",
          "form": "비온다 ㅠ",
          "original_form": "비온다 ㅠ",
          "speaker_id": "2",
          "time": "20210827 10:45"
        },
        {
          "id": "MDRW2100016859.1.9",
          "form": "원 우산가져갔나??",
          "original_form": "원 우산가져갔나??",
          "speaker_id": "2",
          "time": "20210827 10:45"
        },
        {
          "id": "MDRW2100016859.1.10",
          "form": "아하 ㅠㅠㅠ 비오나?? 좀이따가 또 데리러 가야되겠네 ㅠㅠ",
          "original_form": "아하 ㅠㅠㅠ 비오나?? 좀이따가 또 데리러 가야되겠네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100016859.1.11",
          "form": "오늘 너무 피곤해서 ㅋㅋㅋ 계속 누워있음. ㅋㅋ 잠이 쏟아진다.",
          "original_form": "오늘 너무 피곤해서 ㅋㅋㅋ 계속 누워있음. ㅋㅋ 잠이 쏟아진다.",
          "speaker_id": "1",
          "time": "20210827 17:16"
        },
        {
          "id": "MDRW2100016859.1.12",
          "form": "비 엄청 오는데 텐트친다고 아버님 개고생...",
          "original_form": "비 엄청 오는데 텐트친다고 아버님 개고생...",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100016859.1.13",
          "form": "이제 자려고 누웠당",
          "original_form": "이제 자려고 누웠당",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100016859.1.14",
          "form": "피곤하구먼",
          "original_form": "피곤하구먼",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100016859.1.15",
          "form": "헉 ㅜㅜ 고생했다이",
          "original_form": "헉 ㅜㅜ 고생했다이",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100016859.1.16",
          "form": "나는 콩나물밥해놓고 기다리는중.",
          "original_form": "나는 콩나물밥해놓고 기다리는중.",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100016859.1.17",
          "form": "아 맛있겠노",
          "original_form": "아 맛있겠노",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100016859.1.18",
          "form": "다들 어디간겨??",
          "original_form": "다들 어디간겨??",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100016859.1.19",
          "form": "아니ㅋㅋㅋㅋㅋ밥을 늦게먹었지뭐ㅋㅋ",
          "original_form": "아니ㅋㅋㅋㅋㅋ밥을 늦게먹었지뭐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100016859.1.20",
          "form": "언니 잘 놀고있나ㅋ",
          "original_form": "언니 잘 놀고있나ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100016859.1.21",
          "form": "엉 이제 자려고 누웠다 ㅎㅎ",
          "original_form": "엉 이제 자려고 누웠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:50"
        },
        {
          "id": "MDRW2100016859.1.22",
          "form": "첫찌 드뎌 자전거 타는구멈",
          "original_form": "첫찌 드뎌 자전거 타는구멈",
          "speaker_id": "2",
          "time": "20210829 00:50"
        },
        {
          "id": "MDRW2100016859.1.23",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "2",
          "time": "20210829 00:50"
        },
        {
          "id": "MDRW2100016859.1.24",
          "form": "얍얍ㅋㅋ언니 늦었네 어서자ㅋㅋ",
          "original_form": "얍얍ㅋㅋ언니 늦었네 어서자ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:51"
        },
        {
          "id": "MDRW2100016859.1.25",
          "form": "나도 이제잔다ㅋ",
          "original_form": "나도 이제잔다ㅋ",
          "speaker_id": "1",
          "time": "20210829 00:51"
        },
        {
          "id": "MDRW2100016859.1.26",
          "form": "그랴 오늘도 고생했다이",
          "original_form": "그랴 오늘도 고생했다이",
          "speaker_id": "2",
          "time": "20210829 00:54"
        },
        {
          "id": "MDRW2100016859.1.27",
          "form": "아 오늘도 넘피곤ㅋ",
          "original_form": "아 오늘도 넘피곤ㅋ",
          "speaker_id": "1",
          "time": "20210829 10:23"
        },
        {
          "id": "MDRW2100016859.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ점심먹었나??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ점심먹었나??",
          "speaker_id": "2",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100016859.1.29",
          "form": "텐트걷을라니 비오네;;; 휴",
          "original_form": "텐트걷을라니 비오네;;; 휴",
          "speaker_id": "2",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100016859.1.30",
          "form": "재미있게 놀았나?",
          "original_form": "재미있게 놀았나?",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100016859.1.31",
          "form": "우리는 비빔면먹고 누웠다ㅋㅋ",
          "original_form": "우리는 비빔면먹고 누웠다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100016859.1.32",
          "form": "둘째 재우려고ㅋ",
          "original_form": "둘째 재우려고ㅋ",
          "speaker_id": "1",
          "time": "20210829 14:06"
        }
      ]
