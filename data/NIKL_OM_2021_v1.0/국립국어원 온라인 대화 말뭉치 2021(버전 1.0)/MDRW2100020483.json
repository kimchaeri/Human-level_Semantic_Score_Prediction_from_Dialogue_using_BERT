        },
        {
          "id": "MDRW2100020483.1.2",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.3",
          "form": "언제?",
          "original_form": "언제?",
          "speaker_id": "2",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.4",
          "form": "전에 그 사촌언니가 가지고왓을때",
          "original_form": "전에 그 사촌언니가 가지고왓을때",
          "speaker_id": "1",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.5",
          "form": "아~그때",
          "original_form": "아~그때",
          "speaker_id": "2",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.6",
          "form": "맞다맞다",
          "original_form": "맞다맞다",
          "speaker_id": "2",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.7",
          "form": "그리고 아주어릴때",
          "original_form": "그리고 아주어릴때",
          "speaker_id": "1",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.8",
          "form": "그 산위에서 먹은 아이스크림케익?",
          "original_form": "그 산위에서 먹은 아이스크림케익?",
          "speaker_id": "2",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.9",
          "form": "맞아!",
          "original_form": "맞아!",
          "speaker_id": "1",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.10",
          "form": "맛은 기억안나지만",
          "original_form": "맛은 기억안나지만",
          "speaker_id": "2",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.11",
          "form": "재밋엇을거같아",
          "original_form": "재밋엇을거같아",
          "speaker_id": "2",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.12",
          "form": "그니께",
          "original_form": "그니께",
          "speaker_id": "1",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.13",
          "form": "넘 좋앗을거가타",
          "original_form": "넘 좋앗을거가타",
          "speaker_id": "1",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.14",
          "form": "마저",
          "original_form": "마저",
          "speaker_id": "2",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.15",
          "form": "그 높은데서",
          "original_form": "그 높은데서",
          "speaker_id": "2",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020483.1.16",
          "form": "먹는 아이스크림",
          "original_form": "먹는 아이스크림",
