        },
        {
          "id": "MDRW2100043339.1.2",
          "form": "5만원 받았는데",
          "original_form": "5만원 받았는데",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.4",
          "form": "자녀 장려금은 또 뭐고 ㅎㅎ",
          "original_form": "자녀 장려금은 또 뭐고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.5",
          "form": "애들 키우면 주는 돈이 있더라고요",
          "original_form": "애들 키우면 주는 돈이 있더라고요",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.6",
          "form": "근데 집 있으면 안됨ㅋㅋㅋㅋ",
          "original_form": "근데 집 있으면 안됨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.7",
          "form": "아 대구에서",
          "original_form": "아 대구에서",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.8",
          "form": "많이 받는 사람들은 몇 백 봤대요;;",
          "original_form": "많이 받는 사람들은 몇 백 봤대요;;",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.11",
          "form": "그만큼??",
          "original_form": "그만큼??",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.12",
          "form": "대구에선 컬러풀 그거 얘기하는 거죠ㅋㅋㅋㅋ",
          "original_form": "대구에선 컬러풀 그거 얘기하는 거죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.13",
          "form": "그거 말고 근로 장여금처럼",
          "original_form": "그거 말고 근로 장여금처럼",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.14",
          "form": "어어 그거 ㅋㅋㅋ",
          "original_form": "어어 그거 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100043339.1.15",
          "form": "나도 그건 받았거든",
          "original_form": "나도 그건 받았거든",
          "speaker_id": "2",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043339.1.16",
          "form": "자녀 키우는 뭐 세대 해주는데",
          "original_form": "자녀 키우는 뭐 세대 해주는데",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043339.1.17",
          "form": "머릿속에 무지개를 그렸는데",
          "original_form": "머릿속에 무지개를 그렸는데",
          "speaker_id": "2",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043339.1.18",
          "form": "컬러풀이 생각안남 ㅋㅋㅋㅋㅋ",
          "original_form": "컬러풀이 생각안남 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043339.1.19",
          "form": "레인보우 안 간 게 어디에요ㅋㅋㅋㅋ",
          "original_form": "레인보우 안 간 게 어디에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043339.1.20",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043339.1.21",
          "form": "여튼 나라에서 주는 거 참 많음",
          "original_form": "여튼 나라에서 주는 거 참 많음",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043339.1.22",
          "form": "그래 뭐라도 생각난 게 어디고",
          "original_form": "그래 뭐라도 생각난 게 어디고",
          "speaker_id": "2",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043339.1.23",
          "form": "그니까 진짜 많이 준다니까",
          "original_form": "그니까 진짜 많이 준다니까",
          "speaker_id": "2",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043339.1.24",
          "form": "우리가 받을 수 있는 건 없음ㅋㅋㅋㅋ",
          "original_form": "우리가 받을 수 있는 건 없음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043339.1.25",
          "form": "그러니까 그런 사람들은",
          "original_form": "그러니까 그런 사람들은",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100043339.1.26",
          "form": "더 일을 안 하죠",
          "original_form": "더 일을 안 하죠",
          "speaker_id": "1",
