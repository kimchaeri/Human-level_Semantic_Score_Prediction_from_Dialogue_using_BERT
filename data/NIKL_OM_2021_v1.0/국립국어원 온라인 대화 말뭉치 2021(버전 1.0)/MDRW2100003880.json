      "utterance": [
        {
          "id": "MDRW2100003880.1.1",
          "form": "요거트",
          "original_form": "요거트",
          "speaker_id": "1",
          "time": "20210821 13:12"
        },
        {
          "id": "MDRW2100003880.1.2",
          "form": "약간 새콤?하다해야하나",
          "original_form": "약간 새콤?하다해야하나",
          "speaker_id": "1",
          "time": "20210821 13:13"
        },
        {
          "id": "MDRW2100003880.1.3",
          "form": "요맘때같은데 좀 더 진한맛?",
          "original_form": "요맘때같은데 좀 더 진한맛?",
          "speaker_id": "1",
          "time": "20210821 13:13"
        },
        {
          "id": "MDRW2100003880.1.4",
          "form": "오! 그럼 저도 좋아하는 맛이네요!",
          "original_form": "오! 그럼 저도 좋아하는 맛이네요!",
          "speaker_id": "2",
          "time": "20210821 13:13"
        },
        {
          "id": "MDRW2100003880.1.5",
          "form": "한 번 드셔보세요 특히 여러가지 맛 담으면",
          "original_form": "한 번 드셔보세요 특히 여러가지 맛 담으면",
          "speaker_id": "1",
          "time": "20210821 13:13"
        },
        {
          "id": "MDRW2100003880.1.6",
          "form": "요맘때 맛있는데 다음에 요거트 한번  담아봐야겠아요",
          "original_form": "요맘때 맛있는데 다음에 요거트 한번  담아봐야겠아요",
          "speaker_id": "2",
          "time": "20210821 13:13"
        },
        {
          "id": "MDRW2100003880.1.7",
          "form": "요거트 같이 담아보세요",
          "original_form": "요거트 같이 담아보세요",
          "speaker_id": "1",
          "time": "20210821 13:13"
        },
        {
          "id": "MDRW2100003880.1.8",
          "form": "뭔가 입 안의 혼란을 깨끗이 지워준다랄키ㅏ",
          "original_form": "뭔가 입 안의 혼란을 깨끗이 지워준다랄키ㅏ",
          "speaker_id": "1",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003880.1.9",
          "form": "초코나무숲은..이제 못퍼니까 요거트로 퍼달라 해야겠네요",
          "original_form": "초코나무숲은..이제 못퍼니까 요거트로 퍼달라 해야겠네요",
          "speaker_id": "2",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003880.1.10",
          "form": "오! 깔끔하게 다시 지워주고",
          "original_form": "오! 깔끔하게 다시 지워주고",
          "speaker_id": "2",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003880.1.11",
          "form": "거의 단짠단짠 수준의 조합이네요 ㅋㅋㅋ",
          "original_form": "거의 단짠단짠 수준의 조합이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003880.1.12",
          "form": "음 초나숲 다음엔 뭐 좋아해요?",
          "original_form": "음 초나숲 다음엔 뭐 좋아해요?",
          "speaker_id": "1",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003880.1.13",
          "form": "저는 그 팝핑?캔디? 같이 들어가있는거 뭐죠 톡톡 튀는거",
          "original_form": "저는 그 팝핑?캔디? 같이 들어가있는거 뭐죠 톡톡 튀는거",
          "speaker_id": "2",
          "time": "20210821 13:15"
        },
        {
          "id": "MDRW2100003880.1.14",
          "form": "아 이상한나라의 솜사탕?",
          "original_form": "아 이상한나라의 솜사탕?",
          "speaker_id": "1",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003880.1.15",
          "form": "그거 한 때 빠져서 엄청 먹었었는데",
          "original_form": "그거 한 때 빠져서 엄청 먹었었는데",
          "speaker_id": "1",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003880.1.16",
          "form": "아 그건가요! 그거 좋아해요",
          "original_form": "아 그건가요! 그거 좋아해요",
          "speaker_id": "2",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003880.1.17",
          "form": "입안에서 톡톡 튀는게 맛있더라구요",
          "original_form": "입안에서 톡톡 튀는게 맛있더라구요",
          "speaker_id": "2",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003880.1.18",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "1",
          "time": "20210821 13:16"
        },
        {
          "id": "MDRW2100003880.1.19",
          "form": "슈팅스타..",
          "original_form": "슈팅스타..",
          "speaker_id": "1",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100003880.1.20",
          "form": "인가..",
          "original_form": "인가..",
          "speaker_id": "1",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100003880.1.21",
          "form": "엌ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 슈팅스타인거 같아요!!!!",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 슈팅스타인거 같아요!!!!",
          "speaker_id": "2",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100003880.1.22",
          "form": "그것도 좋아해요",
          "original_form": "그것도 좋아해요",
          "speaker_id": "1",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100003880.1.23",
          "form": "파란색 하얀색!!",
          "original_form": "파란색 하얀색!!",
          "speaker_id": "2",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100003880.1.24",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "1",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100003880.1.25",
          "form": "그것도 맛있고 저번에 친구가 샤베트를 좋아해서",
          "original_form": "그것도 맛있고 저번에 친구가 샤베트를 좋아해서",
          "speaker_id": "2",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100003880.1.26",
          "form": "설마 레샤",
          "original_form": "설마 레샤",
          "speaker_id": "1",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100003880.1.27",
          "form": "같이 먹어봤는데 그건 가끔 땡기는 맛이더라구요 상큼하고!",
          "original_form": "같이 먹어봤는데 그건 가끔 땡기는 맛이더라구요 상큼하고!",
          "speaker_id": "2",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100003880.1.28",
          "form": "네네 그 주황색노랑색 여러가지 섞인 상큼상큼한거!",
          "original_form": "네네 그 주황색노랑색 여러가지 섞인 상큼상큼한거!",
          "speaker_id": "2",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100003880.1.29",
          "form": "배라의 상큼함은 레샤라고 감히 단정지을 수 있죠",
          "original_form": "배라의 상큼함은 레샤라고 감히 단정지을 수 있죠",
