      "utterance": [
        {
          "id": "MDRW2100028168.1.1",
          "form": "제가 내일 한번 찾아볼게요 ㅎㅎ",
          "original_form": "제가 내일 한번 찾아볼게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100028168.1.2",
          "form": "오늘 저녁 술약속 잡다가 파토내고 이제 왓씁니다",
          "original_form": "오늘 저녁 술약속 잡다가 파토내고 이제 왓씁니다",
          "speaker_id": "2",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100028168.1.3",
          "form": "아ㅋㅋㅋ왜 파토났나용",
          "original_form": "아ㅋㅋㅋ왜 파토났나용",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028168.1.4",
          "form": "봉은사가 그나마 거리상 만만한것 같긴 해요",
          "original_form": "봉은사가 그나마 거리상 만만한것 같긴 해요",
          "speaker_id": "1",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100028168.1.5",
          "form": "봉은사는 삼성역 봉은사인가요?",
          "original_form": "봉은사는 삼성역 봉은사인가요?",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028168.1.6",
          "form": "네ㅎ",
          "original_form": "네ㅎ",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028168.1.7",
          "form": "너무 도심이라 또 미묘하긴 해서 고민요ㅎ",
          "original_form": "너무 도심이라 또 미묘하긴 해서 고민요ㅎ",
          "speaker_id": "1",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100028168.1.8",
          "form": "완전 좋은데요?",
          "original_form": "완전 좋은데요?",
          "speaker_id": "2",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100028168.1.9",
          "form": "봉은사 정수기맛 궁금합니다 ㅎㅎ",
          "original_form": "봉은사 정수기맛 궁금합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100028168.1.10",
          "form": "아닠ㅋ 하필 정수기맛이요?",
          "original_form": "아닠ㅋ 하필 정수기맛이요?",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100028168.1.11",
          "form": "왜죠ㅋㅋㅋ",
          "original_form": "왜죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028168.1.12",
          "form": "거긴 샘물없자나요 ㅋㅋ",
          "original_form": "거긴 샘물없자나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028168.1.13",
          "form": "지하수 오염됐을듯 ㅋㅋ",
          "original_form": "지하수 오염됐을듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028168.1.14",
          "form": "정수기가 제일 시원하고 깨끗합니다 ㅋㅋ",
          "original_form": "정수기가 제일 시원하고 깨끗합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028168.1.15",
          "form": "혹시 정수기 물 안드시나요?",
          "original_form": "혹시 정수기 물 안드시나요?",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028168.1.16",
          "form": "아니ㅋㅋ 갑자기 가기 싫어지잖아요!!",
          "original_form": "아니ㅋㅋ 갑자기 가기 싫어지잖아요!!",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028168.1.17",
          "form": "저는 보리차 끓여 먹습니다ㅎ",
          "original_form": "저는 보리차 끓여 먹습니다ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028168.1.18",
          "form": "정수기는 회사서만ㅎ",
          "original_form": "정수기는 회사서만ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028168.1.19",
          "form": "오 보리차 ㅎㄷㄷ",
          "original_form": "오 보리차 ㅎㄷㄷ",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028168.1.20",
          "form": "보리차 건강에 좋고 시원하게 하는 성질을 가진 차가 아닙니까?열정적이십니다",
          "original_form": "보리차 건강에 좋고 시원하게 하는 성질을 가진 차가 아닙니까?열정적이십니다",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028168.1.21",
          "form": "어려서부터 그래서 정수기 물맛  적응 안돼요ㅋㅋ",
          "original_form": "어려서부터 그래서 정수기 물맛  적응 안돼요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028168.1.22",
          "form": "아닠 갑자기 지식이 쏟아지네요ㅋㅋ",
          "original_form": "아닠 갑자기 지식이 쏟아지네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028168.1.23",
          "form": "결혼하셔도 보리차만 드실건가요?",
          "original_form": "결혼하셔도 보리차만 드실건가요?",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028168.1.24",
          "form": "아 지식이 얕아서요 ㅋㅋ",
          "original_form": "아 지식이 얕아서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028168.1.25",
          "form": "저는 신념이 확고합니다ㅋㅋ",
          "original_form": "저는 신념이 확고합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028168.1.26",
          "form": "자박자박합니다",
          "original_form": "자박자박합니다",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028168.1.27",
          "form": "그래보입니다",
          "original_form": "그래보입니다",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028168.1.28",
          "form": "요 며칠",
          "original_form": "요 며칠",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028168.1.29",
          "form": "이 나이 됨 안 변해욧!! ㅎㅎ",
          "original_form": "이 나이 됨 안 변해욧!! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028168.1.30",
          "form": "저보다 어리시지 않아요?",
          "original_form": "저보다 어리시지 않아요?",
