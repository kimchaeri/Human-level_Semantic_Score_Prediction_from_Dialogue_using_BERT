      "utterance": [
        {
          "id": "MDRW2100044774.1.1",
          "form": "반려동물 있나요?",
          "original_form": "반려동물 있나요?",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044774.1.2",
          "form": "친정에 강아지키워요",
          "original_form": "친정에 강아지키워요",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044774.1.3",
          "form": "혹시 동물 뭐 키워보고싶으세요?",
          "original_form": "혹시 동물 뭐 키워보고싶으세요?",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044774.1.4",
          "form": "오 ㅋㅋ 저는 아이들로도 벅차서요",
          "original_form": "오 ㅋㅋ 저는 아이들로도 벅차서요",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044774.1.5",
          "form": "더는 안키울거에요 ㅋㅋ",
          "original_form": "더는 안키울거에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044774.1.6",
          "form": "강아지 이름이?",
          "original_form": "강아지 이름이?",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044774.1.7",
          "form": "아이들이 물고기같은거 막 키우자하믄 하실꺼에요?ㅋ",
          "original_form": "아이들이 물고기같은거 막 키우자하믄 하실꺼에요?ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044774.1.8",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044774.1.9",
          "form": "그래서 늘 고민합니다",
          "original_form": "그래서 늘 고민합니다",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044774.1.10",
          "form": "그런 이야기를 할 나이가 되면",
          "original_form": "그런 이야기를 할 나이가 되면",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044774.1.11",
          "form": "전 절대못할듯요 강아지이름은 사랑이요ㅋㅋ",
          "original_form": "전 절대못할듯요 강아지이름은 사랑이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044774.1.12",
          "form": "그땐 뭐라고 이야기 해줘야할까",
          "original_form": "그땐 뭐라고 이야기 해줘야할까",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044774.1.13",
          "form": "오모 사랑이",
          "original_form": "오모 사랑이{emoji:♡}",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044774.1.14",
          "form": "그 6살만되도 사슴벌레 이런거?",
          "original_form": "그 6살만되도 사슴벌레 이런거?",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044774.1.15",
          "form": "몇살이에여?",
          "original_form": "몇살이에여?",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044774.1.16",
          "form": "키운다던디ㅋㅋ",
          "original_form": "키운다던디ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044774.1.17",
          "form": "11살 할무니에요ㅜㅜ",
          "original_form": "11살 할무니에요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044774.1.18",
          "form": "자꾸안먹어서 살이빠져요 강쥐가!",
          "original_form": "자꾸안먹어서 살이빠져요 강쥐가!",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044774.1.19",
          "form": "홀ㅜㅜ 할무니",
          "original_form": "홀ㅜㅜ 할무니",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044774.1.20",
          "form": "간식같은거라도",
          "original_form": "간식같은거라도",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044774.1.21",
          "form": "많이 사드리세영",
          "original_form": "많이 사드리세영",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044774.1.22",
          "form": "잘안먹네요 특히 아들이 같이가면 괴롭혀서 더 스트레스받아하는것같아오ㅜㅋ",
          "original_form": "잘안먹네요 특히 아들이 같이가면 괴롭혀서 더 스트레스받아하는것같아오ㅜㅋ",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044774.1.23",
          "form": "고양이 vs 개",
          "original_form": "고양이 vs 개",
