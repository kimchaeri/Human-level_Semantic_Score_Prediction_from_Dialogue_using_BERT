      "utterance": [
        {
          "id": "MDRW2100006733.1.1",
          "form": "이번엔 다시 자연?",
          "original_form": "이번엔 다시 자연?",
          "speaker_id": "2",
          "time": "20210908 08:01"
        },
        {
          "id": "MDRW2100006733.1.2",
          "form": "갔던곳을 금방 질려해 ㅋㅋ",
          "original_form": "갔던곳을 금방 질려해 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:02"
        },
        {
          "id": "MDRW2100006733.1.3",
          "form": "ㅋㅋ 에구 아직 거리두기라",
          "original_form": "ㅋㅋ 에구 아직 거리두기라",
          "speaker_id": "2",
          "time": "20210908 08:03"
        },
        {
          "id": "MDRW2100006733.1.4",
          "form": "그렇긴한데 평일내내 집에만 있으니 안타까워서",
          "original_form": "그렇긴한데 평일내내 집에만 있으니 안타까워서",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100006733.1.5",
          "form": "그렇지 자연으로 가야지",
          "original_form": "그렇지 자연으로 가야지",
          "speaker_id": "2",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100006733.1.6",
          "form": "응 하지만 너무 멀리는 부담ㅇ,야",
          "original_form": "응 하지만 너무 멀리는 부담ㅇ,야",
          "speaker_id": "1",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100006733.1.7",
          "form": "그래도 저번에도 다들 잘 하던걸",
          "original_form": "그래도 저번에도 다들 잘 하던걸",
          "speaker_id": "2",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100006733.1.8",
          "form": "맞아 다녀오고 일주일 내내 좋았어",
          "original_form": "맞아 다녀오고 일주일 내내 좋았어",
          "speaker_id": "1",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100006733.1.9",
          "form": "그럼 적당히 멀리 가자",
          "original_form": "그럼 적당히 멀리 가자",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100006733.1.10",
          "form": "동생꺼로",
          "original_form": "동생꺼로",
          "speaker_id": "1",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100006733.1.11",
          "form": "호캉스 쿠폰도 써야지",
          "original_form": "호캉스 쿠폰도 써야지",
          "speaker_id": "1",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100006733.1.12",
          "form": "아 그건 언제 쓸까요",
          "original_form": "아 그건 언제 쓸까요",
          "speaker_id": "2",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100006733.1.13",
          "form": "12월안에 써야하니 기억해야죠",
          "original_form": "12월안에 써야하니 기억해야죠",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100006733.1.14",
          "form": "돈을 보태더라도 좋은 데 갑시다",
          "original_form": "돈을 보태더라도 좋은 데 갑시다",
          "speaker_id": "2",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100006733.1.15",
          "form": "그럽시다",
          "original_form": "그럽시다",
          "speaker_id": "1",
          "time": "20210908 08:35"
        }
