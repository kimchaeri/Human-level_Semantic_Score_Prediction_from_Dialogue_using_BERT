      "utterance": [
        {
          "id": "MDRW2100016872.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016872.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100016872.1.3",
          "form": "운동 좋아하시나 봐요",
          "original_form": "운동 좋아하시나 봐요",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100016872.1.4",
          "form": "음 최근에 올림픽본이후로 양궁에빠져버렸어요ㅠㅎㅎ",
          "original_form": "음 최근에 올림픽본이후로 양궁에빠져버렸어요ㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100016872.1.5",
          "form": "아하 양궁 너무 재밌었죠??",
          "original_form": "아하 양궁 너무 재밌었죠??",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100016872.1.6",
          "form": "저는 배구에 빠져서 배구 영상을 한참 찾아봤답니다",
          "original_form": "저는 배구에 빠져서 배구 영상을 한참 찾아봤답니다",
          "speaker_id": "2",
          "time": "20210826 23:50"
        },
        {
          "id": "MDRW2100016872.1.7",
          "form": "네! 아 김연경선수 걸크러쉬 넘매력쩔어요 ㅜㅜ",
          "original_form": "네! 아 김연경선수 걸크러쉬 넘매력쩔어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 04:31"
        },
        {
          "id": "MDRW2100016872.1.8",
          "form": "그렇죠 ㅎㅎ김연경선수랑 김희진 선수 너무 귀엽더라구요",
          "original_form": "그렇죠 ㅎㅎ김연경선수랑 김희진 선수 너무 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210827 07:33"
        },
        {
          "id": "MDRW2100016872.1.9",
          "form": "유퀴즈에 양궁선수들 나왔죠??",
          "original_form": "유퀴즈에 양궁선수들 나왔죠??",
          "speaker_id": "2",
          "time": "20210827 07:34"
        },
        {
          "id": "MDRW2100016872.1.10",
          "form": "맞아요~ 그것두 짤짤이 봤어요 ㅋㅋ",
          "original_form": "맞아요~ 그것두 짤짤이 봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100016872.1.11",
          "form": "네 ㅎㅎ 전 아직 못봤는데",
          "original_form": "네 ㅎㅎ 전 아직 못봤는데",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100016872.1.12",
          "form": "조만간 봐야겠어요",
          "original_form": "조만간 봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100016872.1.13",
          "form": "배구에 빠져서",
          "original_form": "배구에 빠져서",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100016872.1.14",
          "form": "배구 영상 찾아보고 했었어요",
          "original_form": "배구 영상 찾아보고 했었어요",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100016872.1.15",
          "form": "아하! 다시생각해도 배구경기 명경기였어요",
          "original_form": "아하! 다시생각해도 배구경기 명경기였어요",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100016872.1.16",
          "form": "네\n진짜 마지막 장면은 최고였어요",
          "original_form": "네\n진짜 마지막 장면은 최고였어요",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100016872.1.17",
          "form": "김연경",
          "original_form": "김연경",
          "speaker_id": "2",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100016872.1.18",
          "form": "왜 세계 최고 선수인지 알겠더라구요",
          "original_form": "왜 세계 최고 선수인지 알겠더라구요",
          "speaker_id": "2",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100016872.1.19",
          "form": "말투도 너무 재밌고",
          "original_form": "말투도 너무 재밌고",
          "speaker_id": "2",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100016872.1.20",
          "form": "맞아요.. 갓연경ㅠㅠ",
          "original_form": "맞아요.. 갓연경ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100016872.1.21",
          "form": "너무 자랑스럽슴돠",
          "original_form": "너무 자랑스럽슴돠",
          "speaker_id": "1",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100016872.1.22",
          "form": "네\n정말 자랑스럽죠^^",
          "original_form": "네\n정말 자랑스럽죠^^",
          "speaker_id": "2",
          "time": "20210827 12:58"
        },
        {
          "id": "MDRW2100016872.1.23",
          "form": "다른 스포츠는 좋아하는거 없으세요??",
          "original_form": "다른 스포츠는 좋아하는거 없으세요??",
          "speaker_id": "2",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100016872.1.24",
          "form": "전 스포츠경기 관람하러 간적은 없어서",
          "original_form": "전 스포츠경기 관람하러 간적은 없어서",
          "speaker_id": "2",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100016872.1.25",
          "form": "한번 가보고 싶긴해요",
          "original_form": "한번 가보고 싶긴해요",
          "speaker_id": "2",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100016872.1.26",
          "form": "저는 동계올림픽종목 좋아해요! 쇼트트랙 피겨 스키 요런거요~",
          "original_form": "저는 동계올림픽종목 좋아해요! 쇼트트랙 피겨 스키 요런거요~",
          "speaker_id": "1",
          "time": "20210827 13:01"
        },
        {
          "id": "MDRW2100016872.1.27",
          "form": "이런거 직관하면 정말 재미질거같아요",
          "original_form": "이런거 직관하면 정말 재미질거같아요",
          "speaker_id": "1",
          "time": "20210827 13:01"
        },
        {
          "id": "MDRW2100016872.1.28",
          "form": "오호 동계좋죠",
          "original_form": "오호 동계좋죠",
          "speaker_id": "2",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100016872.1.29",
          "form": "동계가 볼게\n많은거 같아요",
          "original_form": "동계가 볼게\n많은거 같아요",
          "speaker_id": "2",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100016872.1.30",
          "form": "저도 가서 실컷 응원하고 싶네요 ㅎㅎ",
          "original_form": "저도 가서 실컷 응원하고 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:03"
        }
      ]
    }
