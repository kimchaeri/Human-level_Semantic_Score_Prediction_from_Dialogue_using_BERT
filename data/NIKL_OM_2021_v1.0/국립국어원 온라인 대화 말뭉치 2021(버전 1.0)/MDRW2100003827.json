      "utterance": [
        {
          "id": "MDRW2100003827.1.1",
          "form": "가봤지ㅋㅋㅋ",
          "original_form": "가봤지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100003827.1.2",
          "form": "호텔이랑 비슷한거 같기도 하구",
          "original_form": "호텔이랑 비슷한거 같기도 하구",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100003827.1.3",
          "form": "호텔인데 거기 안에 노는게 잔뜩 있어서 좋은 것 같아",
          "original_form": "호텔인데 거기 안에 노는게 잔뜩 있어서 좋은 것 같아",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100003827.1.4",
          "form": "워터파크는 못가서 아쉬워ㅠㅠ",
          "original_form": "워터파크는 못가서 아쉬워ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100003827.1.5",
          "form": "웅웅 그래도 가능 사람들 많은거 아니야?",
          "original_form": "웅웅 그래도 가능 사람들 많은거 아니야?",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100003827.1.6",
          "form": "나는 코로나 때문에 안갔어ㅠㅠ",
          "original_form": "나는 코로나 때문에 안갔어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100003827.1.7",
          "form": "ㅋㅋ나도 코로나 때매 안가고 있어ㅋㅋㅋ",
          "original_form": "ㅋㅋ나도 코로나 때매 안가고 있어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100003827.1.8",
          "form": "5월에 제주도 갔는데, 신라호텔 잠깐 들렀거든? 풀장에 사람이 꽉찼더라 마스크 70%는 안끼고ㅋㅋㅋ",
          "original_form": "5월에 제주도 갔는데, 신라호텔 잠깐 들렀거든? 풀장에 사람이 꽉찼더라 마스크 70%는 안끼고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100003827.1.9",
          "form": "진짜 그런거 보면 백신 빨리 맞아야 겠더라..",
          "original_form": "진짜 그런거 보면 백신 빨리 맞아야 겠더라..",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100003827.1.10",
          "form": "어쩌다 이렇게 된건지ㅠㅠㅠㅠ",
          "original_form": "어쩌다 이렇게 된건지ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100003827.1.11",
          "form": "해수욕장에도 사람들 똑같아!",
          "original_form": "해수욕장에도 사람들 똑같아!",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100003827.1.12",
          "form": "끙- 빨리 코로나 종식되면 어디 바닷가가서 스쿠버 배우고픔",
          "original_form": "끙- 빨리 코로나 종식되면 어디 바닷가가서 스쿠버 배우고픔",
          "speaker_id": "2",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100003827.1.13",
          "form": "헐 스쿠버 멋지당",
          "original_form": "헐 스쿠버 멋지당",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100003827.1.14",
          "form": "그거 하면 물고기 볼 수 있잖아",
          "original_form": "그거 하면 물고기 볼 수 있잖아",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100003827.1.15",
          "form": "ㅋㅋㅋㅋ해보고 싶은 것일뿐ㅋㅋㅋㅋ아직 시작고 못해봤어",
          "original_form": "ㅋㅋㅋㅋ해보고 싶은 것일뿐ㅋㅋㅋㅋ아직 시작고 못해봤어",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100003827.1.16",
          "form": "앜ㅋㅋㅋㅋ 어쩌다 관심 가지게 됐어?",
          "original_form": "앜ㅋㅋㅋㅋ 어쩌다 관심 가지게 됐어?",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100003827.1.17",
          "form": "음 워낙에 새로운거 해보는거 좋아하는데 친구들이 막 다녀온 영상 같은거 보여저면 바다색도 너무 이쁘고 좋더라구 ㅜㅜ",
          "original_form": "음 워낙에 새로운거 해보는거 좋아하는데 친구들이 막 다녀온 영상 같은거 보여저면 바다색도 너무 이쁘고 좋더라구 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100003827.1.18",
          "form": "해외에서 하면 진짜 더 좋을 것 같아",
          "original_form": "해외에서 하면 진짜 더 좋을 것 같아",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100003827.1.19",
          "form": "호주 이런 국가!",
          "original_form": "호주 이런 국가!",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100003827.1.20",
          "form": "ㅋㅋㅋ어어 얘기들어보니까 한국에서도 딸수는 있대",
          "original_form": "ㅋㅋㅋ어어 얘기들어보니까 한국에서도 딸수는 있대",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100003827.1.21",
          "form": "그렇구나 따고 호주로 가자",
          "original_form": "그렇구나 따고 호주로 가자",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100003827.1.22",
          "form": "근데 배우는 가격이 비싸서 동남아에 뱅기타고가서 자격증 따는거랑 가격이 같다더라",
          "original_form": "근데 배우는 가격이 비싸서 동남아에 뱅기타고가서 자격증 따는거랑 가격이 같다더라",
          "speaker_id": "2",
          "time": "20210821 22:36"
        }
      ]
    }
  ]
