        {
          "id": "MDRW2100012545.1.1",
          "form": "학교",
          "original_form": "학교",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012545.1.2",
          "form": "이제 개강하죠",
          "original_form": "이제 개강하죠",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012545.1.3",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012545.1.4",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012545.1.5",
          "form": "내",
          "original_form": "내",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012545.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012545.1.7",
          "form": "이제 학교로 돌아가야합니다",
          "original_form": "이제 학교로 돌아가야합니다",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012545.1.8",
          "form": "시간이 빠르네요",
          "original_form": "시간이 빠르네요",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012545.1.9",
          "form": "학교 생활 화이팅입니다",
          "original_form": "학교 생활 화이팅입니다",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012545.1.10",
          "form": "화상 수업하나요?",
          "original_form": "화상 수업하나요?",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012545.1.11",
          "form": "네 이론사업은 화상인데",
          "original_form": "네 이론사업은 화상인데",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012545.1.12",
          "form": "실습수업은 직접 가야할 수도 잇어서",
          "original_form": "실습수업은 직접 가야할 수도 잇어서",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012545.1.13",
          "form": "걱정이에요",
          "original_form": "걱정이에요",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012545.1.14",
          "form": "코로나 때문에 불안하시겠어요",
          "original_form": "코로나 때문에 불안하시겠어요",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012545.1.15",
          "form": "네 아무래도 수업 다니다보면 만나는 사람도 많고",
          "original_form": "네 아무래도 수업 다니다보면 만나는 사람도 많고",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012545.1.16",
          "form": "오고가는 대중교통도 많이 이용해서 불안합니다",
          "original_form": "오고가는 대중교통도 많이 이용해서 불안합니다",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012545.1.17",
          "form": "왠만하면 학교 근처에서 밥도 안먹으려고 노렫하는데",
          "original_form": "왠만하면 학교 근처에서 밥도 안먹으려고 노렫하는데",
          "speaker_id": "2",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012545.1.18",
          "form": "스트레스가 엄청나시겠어요",
          "original_form": "스트레스가 엄청나시겠어요",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012545.1.19",
          "form": "그게 쉽지않네요",
          "original_form": "그게 쉽지않네요",
          "speaker_id": "2",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012545.1.20",
          "form": "학업은 잘되어가시나요?",
          "original_form": "학업은 잘되어가시나요?",
          "speaker_id": "1",
          "time": "20210824 23:18"
        }
      ]
