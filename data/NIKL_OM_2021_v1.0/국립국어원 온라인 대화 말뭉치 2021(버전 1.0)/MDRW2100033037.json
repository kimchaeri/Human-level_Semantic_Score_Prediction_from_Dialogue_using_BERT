      "utterance": [
        {
          "id": "MDRW2100033037.1.1",
          "form": "제주도가 고향인가요?",
          "original_form": "제주도가 고향인가요?",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033037.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033037.1.3",
          "form": "제주살이 3주차예여 혼자요",
          "original_form": "제주살이 3주차예여 혼자요",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100033037.1.4",
          "form": "멋진 삶을 살고 계시는군요",
          "original_form": "멋진 삶을 살고 계시는군요",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033037.1.5",
          "form": "그러면 추석같은 명절엔 육지로 나오나요?",
          "original_form": "그러면 추석같은 명절엔 육지로 나오나요?",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033037.1.6",
          "form": "가족있으신가요",
          "original_form": "가족있으신가요",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100033037.1.7",
          "form": "저는 식구들과 살고 있습니다",
          "original_form": "저는 식구들과 살고 있습니다",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033037.1.8",
          "form": "한집에 아내와 딸이 있고",
          "original_form": "한집에 아내와 딸이 있고",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033037.1.9",
          "form": "분가한 딸도 있어요",
          "original_form": "분가한 딸도 있어요",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033037.1.10",
          "form": "그러시군여",
          "original_form": "그러시군여",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033037.1.11",
          "form": "추석때 오기어려워서 미리 왔어요",
          "original_form": "추석때 오기어려워서 미리 왔어요",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033037.1.12",
          "form": "고향이 창원이라서요",
          "original_form": "고향이 창원이라서요",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033037.1.13",
          "form": "그렇군요 원래도 따뜻한 지역에서 살고 계셨군요",
          "original_form": "그렇군요 원래도 따뜻한 지역에서 살고 계셨군요",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100033037.1.14",
          "form": "그러다 더 남쪽으로 내려간 거네요",
          "original_form": "그러다 더 남쪽으로 내려간 거네요",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100033037.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033037.1.16",
          "form": "서울은 그래서 적응이 안됐나봐요",
          "original_form": "서울은 그래서 적응이 안됐나봐요",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033037.1.17",
          "form": "집안일은 많이 도와주시나요",
          "original_form": "집안일은 많이 도와주시나요",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033037.1.18",
          "form": "저는 원래부터 같이 하던 스타일이었습니다",
          "original_form": "저는 원래부터 같이 하던 스타일이었습니다",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033037.1.19",
          "form": "아무래도 전업주부인 아내가 더 하겠지만",
          "original_form": "아무래도 전업주부인 아내가 더 하겠지만",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033037.1.20",
          "form": "집안일 돕는데 스스럼 없었지요",
          "original_form": "집안일 돕는데 스스럼 없었지요",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100033037.1.21",
          "form": "오 잘하시는 요리 있으신가요",
          "original_form": "오 잘하시는 요리 있으신가요",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100033037.1.22",
          "form": "요리는 아내가 전문가라 옆에서 돕습니다",
          "original_form": "요리는 아내가 전문가라 옆에서 돕습니다",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033037.1.23",
          "form": "처가가 요리사 집안이라",
          "original_form": "처가가 요리사 집안이라",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033037.1.24",
          "form": "시다노릇 잘 합니다",
          "original_form": "시다노릇 잘 합니다",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033037.1.25",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033037.1.26",
          "form": "훌륭하시네요",
          "original_form": "훌륭하시네요",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033037.1.27",
          "form": "혼자사니 밥 먹는게 귀찮아요",
          "original_form": "혼자사니 밥 먹는게 귀찮아요",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033037.1.28",
          "form": "그래도 챙겨드셔야 건강 해치지 않습니다",
          "original_form": "그래도 챙겨드셔야 건강 해치지 않습니다",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033037.1.29",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210906 14:36"
        }
      ]
