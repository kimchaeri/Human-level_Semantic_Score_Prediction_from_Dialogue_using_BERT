      "utterance": [
        {
          "id": "MDRW2100024896.1.1",
          "form": "비가...",
          "original_form": "비가...",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100024896.1.2",
          "form": "이틀연속 오고있네...",
          "original_form": "이틀연속 오고있네...",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100024896.1.3",
          "form": "그러게 비가 계속 오네~",
          "original_form": "그러게 비가 계속 오네~",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100024896.1.4",
          "form": "언제까지 오는 건지...",
          "original_form": "언제까지 오는 건지...",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100024896.1.5",
          "form": "오늘까지만 오고",
          "original_form": "오늘까지만 오고",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100024896.1.6",
          "form": "내일부터 안 오는 거 같던데",
          "original_form": "내일부터 안 오는 거 같던데",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100024896.1.7",
          "form": "그래요??",
          "original_form": "그래요??",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100024896.1.8",
          "form": "그렇담 다행~",
          "original_form": "그렇담 다행~",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100024896.1.9",
          "form": "담주에는?",
          "original_form": "담주에는?",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100024896.1.10",
          "form": "담주에는 아예 안오는 거 같아",
          "original_form": "담주에는 아예 안오는 거 같아",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100024896.1.11",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100024896.1.12",
          "form": "담주부터 다시 출근인데",
          "original_form": "담주부터 다시 출근인데",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100024896.1.13",
          "form": "그러게~ 딱 출근할때 되서 비 안 오네",
          "original_form": "그러게~ 딱 출근할때 되서 비 안 오네",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100024896.1.14",
          "form": "가을비가 꽤 많이 오네요",
          "original_form": "가을비가 꽤 많이 오네요",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100024896.1.15",
          "form": "여름비가 적어서 그런가보ㅓ",
          "original_form": "여름비가 적어서 그런가보ㅓ",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100024896.1.16",
          "form": "웅웅.. 이러다 눈도 많이 오겄어..",
          "original_form": "웅웅.. 이러다 눈도 많이 오겄어..",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100024896.1.17",
          "form": "의외로 눈이 많이 안 온다나봐",
          "original_form": "의외로 눈이 많이 안 온다나봐",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100024896.1.18",
          "form": "아 그래요??",
          "original_form": "아 그래요??",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100024896.1.19",
          "form": "그럼 덜 춥다는 건가??",
          "original_form": "그럼 덜 춥다는 건가??",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100024896.1.20",
          "form": "그런거 같아~",
          "original_form": "그런거 같아~",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100024896.1.21",
          "form": "날씨가 많이 쌀쌀해진 것 같아요",
          "original_form": "날씨가 많이 쌀쌀해진 것 같아요",
          "speaker_id": "1",
