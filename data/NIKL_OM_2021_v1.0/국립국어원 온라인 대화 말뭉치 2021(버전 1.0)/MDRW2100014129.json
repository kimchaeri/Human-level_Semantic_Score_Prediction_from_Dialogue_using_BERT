      "utterance": [
        {
          "id": "MDRW2100014129.1.1",
          "form": "안녕하세요 날이 흐리네요ㅠ",
          "original_form": "안녕하세요 날이 흐리네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014129.1.2",
          "form": "저는 비오는날이싫어요 ㅠㅠ",
          "original_form": "저는 비오는날이싫어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014129.1.3",
          "form": "저두요ㅠㅜ축축하고 습하고ㅠㅠ",
          "original_form": "저두요ㅠㅜ축축하고 습하고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014129.1.4",
          "form": "우산드는것도귀찮아요 ㅎㅎㅎ",
          "original_form": "우산드는것도귀찮아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014129.1.5",
          "form": "맞아요ㅎㅎ 완전 짐이에요ㅋㅋ",
          "original_form": "맞아요ㅎㅎ 완전 짐이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014129.1.6",
          "form": "꼭 우산 가져가면 안오고 안가져가면 오더라구요 ㅋㅋㅋ",
          "original_form": "꼭 우산 가져가면 안오고 안가져가면 오더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014129.1.7",
          "form": "맞아요ㅠㅠ예보가 너무 안맞고ㅠㅠㅠ이번주는 들고 다녀야되겠더라구요",
          "original_form": "맞아요ㅠㅠ예보가 너무 안맞고ㅠㅠㅠ이번주는 들고 다녀야되겠더라구요",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014129.1.8",
          "form": "맞아요  지금도 딱히 오진않는데 흐려서 갖고는 있어요",
          "original_form": "맞아요  지금도 딱히 오진않는데 흐려서 갖고는 있어요",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014129.1.9",
          "form": "어떤계절좋아하세요?",
          "original_form": "어떤계절좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014129.1.10",
          "form": "저는 가을 제일 좋아해요ㅋㅋ 맑은 하늘ㅎㅎ 어디를 좋아하세요?",
          "original_form": "저는 가을 제일 좋아해요ㅋㅋ 맑은 하늘ㅎㅎ 어디를 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014129.1.11",
          "form": "저는 봄이요! 가을도 좋긴한데 역시 꽃핀 봄이 정말이쁜것같아요",
          "original_form": "저는 봄이요! 가을도 좋긴한데 역시 꽃핀 봄이 정말이쁜것같아요",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014129.1.12",
          "form": "너무 좋죠 짧은게 아쉬울뿐ㅜㅠ",
          "original_form": "너무 좋죠 짧은게 아쉬울뿐ㅜㅠ",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014129.1.13",
          "form": "맞아요 ㅎㅎㅎ봄인가 싶으면 여름이 되더라구요",
          "original_form": "맞아요 ㅎㅎㅎ봄인가 싶으면 여름이 되더라구요",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014129.1.14",
          "form": "네...점점 짧아지는거같아요ㅠㅠ",
          "original_form": "네...점점 짧아지는거같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 17:32"
        }
