      "utterance": [
        {
          "id": "MDRW2100001753.1.1",
          "form": "넷플릭스 보시나요",
          "original_form": "넷플릭스 보시나요",
          "speaker_id": "1",
          "time": "20210819 18:36"
        },
        {
          "id": "MDRW2100001753.1.2",
          "form": "보긴보는데 항상 뭐봐야할지 고민하다가",
          "original_form": "보긴보는데 항상 뭐봐야할지 고민하다가",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001753.1.3",
          "form": "봤던거 재탕만 해요 ㅎㅎㅎ",
          "original_form": "봤던거 재탕만 해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001753.1.4",
          "form": "요즘 뭐보세요??",
          "original_form": "요즘 뭐보세요??",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001753.1.5",
          "form": "어떤거 보시오.",
          "original_form": "어떤거 보시오.",
          "speaker_id": "1",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001753.1.6",
          "form": "저는 외국영화봐요",
          "original_form": "저는 외국영화봐요",
          "speaker_id": "1",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001753.1.7",
          "form": "오오 저는 주로 드라마 봐요!",
          "original_form": "오오 저는 주로 드라마 봐요!",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001753.1.8",
          "form": "요즘 라켓소년단 재밌게 봤어요",
          "original_form": "요즘 라켓소년단 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001753.1.9",
          "form": "그거 재밌나요?",
          "original_form": "그거 재밌나요?",
          "speaker_id": "1",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001753.1.10",
          "form": "네네 그냥 잔잔하고 귀여운 힐링물이에요 ㅋㅋㅋ",
          "original_form": "네네 그냥 잔잔하고 귀여운 힐링물이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100001753.1.11",
          "form": "사람들 많이 보긴하던데.. 궁금해요!",
          "original_form": "사람들 많이 보긴하던데.. 궁금해요!",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100001753.1.12",
          "form": "저도 한번 봐야겠어요~~!!!",
          "original_form": "저도 한번 봐야겠어요~~!!!",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100001753.1.13",
          "form": "중딩들이 너무 귀여워서 틀어놓고 밥먹기 딱 좋은 재질이에요",
          "original_form": "중딩들이 너무 귀여워서 틀어놓고 밥먹기 딱 좋은 재질이에요",
          "speaker_id": "2",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100001753.1.14",
          "form": "드라마는 챙겨보는게 힘들어서 영화를 보게되저라구요",
          "original_form": "드라마는 챙겨보는게 힘들어서 영화를 보게되저라구요",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100001753.1.15",
          "form": "아잇....너무 귀여울것같네요",
          "original_form": "아잇....너무 귀여울것같네요",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100001753.1.16",
          "form": "아아 맞아요 저도그래서 완결나면 몰아봐요",
          "original_form": "아아 맞아요 저도그래서 완결나면 몰아봐요",
          "speaker_id": "2",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100001753.1.17",
          "form": "영화는 장르 주로 어떤거 보세요?",
          "original_form": "영화는 장르 주로 어떤거 보세요?",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001753.1.18",
          "form": "저는 액션좋아해요",
          "original_form": "저는 액션좋아해요",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001753.1.19",
          "form": "총말고 주먹으로 싸우는......ㅋㅋㅋㅋㅋㅋ",
          "original_form": "총말고 주먹으로 싸우는......ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001753.1.20",
          "form": "오오 제일 재밌게 본거 하나 추천해주실수있나용?",
          "original_form": "오오 제일 재밌게 본거 하나 추천해주실수있나용?",
