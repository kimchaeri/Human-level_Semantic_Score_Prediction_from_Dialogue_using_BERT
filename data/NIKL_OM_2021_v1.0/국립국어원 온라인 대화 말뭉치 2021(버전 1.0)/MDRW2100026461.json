      "utterance": [
        {
          "id": "MDRW2100026461.1.1",
          "form": "안녕하세요 저녁식사하셨나요?!",
          "original_form": "안녕하세요 저녁식사하셨나요?!",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026461.1.2",
          "form": "넹 쭈꾸미먹엇어요",
          "original_form": "넹 쭈꾸미먹엇어요",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026461.1.3",
          "form": "쭈꾸미볶음인가요?!",
          "original_form": "쭈꾸미볶음인가요?!",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026461.1.4",
          "form": "네네 ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026461.1.5",
          "form": "쭈꾸미사랑해요",
          "original_form": "쭈꾸미사랑해요",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026461.1.6",
          "form": "ㅎㅎ매운거잘드세요?",
          "original_form": "ㅎㅎ매운거잘드세요?",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026461.1.7",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026461.1.8",
          "form": "name1은 머드셧는지",
          "original_form": "&name1&은 머드셧는지",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026461.1.9",
          "form": "저는 잔치국수 먹었어요",
          "original_form": "저는 잔치국수 먹었어요",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026461.1.10",
          "form": "배달음식 좋아하세요?",
          "original_form": "배달음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026461.1.11",
          "form": "아 시키면 항상좀 후회해요 .. .",
          "original_form": "아 시키면 항상좀 후회해요 .. .",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026461.1.12",
          "form": "혼자 살아서ㅜㅋㅋ",
          "original_form": "혼자 살아서ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026461.1.13",
          "form": "잔치국수 맛잇겟당.ㅜ",
          "original_form": "잔치국수 맛잇겟당.ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026461.1.14",
          "form": "항상 음식은 남고 가격은 만만찮죠",
          "original_form": "항상 음식은 남고 가격은 만만찮죠",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026461.1.15",
          "form": "네 가격이ㅠㅠ요즘 배달비도 ㅠㅠ",
          "original_form": "네 가격이ㅠㅠ요즘 배달비도 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026461.1.16",
          "form": "배달비가 너무올라서 시키기 좀 그래요 이제",
          "original_form": "배달비가 너무올라서 시키기 좀 그래요 이제",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026461.1.17",
          "form": "진짜ㅜㅜㅜ무슨 다 시킬라하면 이마넌넘어가고",
          "original_form": "진짜ㅜㅜㅜ무슨 다 시킬라하면 이마넌넘어가고",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026461.1.18",
          "form": "쓰레기도 마니나오공..",
          "original_form": "쓰레기도 마니나오공..",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026461.1.19",
          "form": "맞아요 이만원이 넘어요!ㅠ",
          "original_form": "맞아요 이만원이 넘어요!ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
