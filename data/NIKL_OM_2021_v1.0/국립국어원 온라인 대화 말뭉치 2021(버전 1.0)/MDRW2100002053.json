      "utterance": [
        {
          "id": "MDRW2100002053.1.1",
          "form": "자주 보는 유튜브 있으세요?",
          "original_form": "자주 보는 유튜브 있으세요?",
          "speaker_id": "1",
          "time": "20210821 11:41"
        },
        {
          "id": "MDRW2100002053.1.2",
          "form": "카페브이로그 자주 보는 편이에요",
          "original_form": "카페브이로그 자주 보는 편이에요",
          "speaker_id": "2",
          "time": "20210821 11:41"
        },
        {
          "id": "MDRW2100002053.1.3",
          "form": "음료 만드는거 보면 뭔가 마음에 안정이 오더라구요",
          "original_form": "음료 만드는거 보면 뭔가 마음에 안정이 오더라구요",
          "speaker_id": "2",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100002053.1.4",
          "form": "오 카페브이로그면 커피 만들고 이런건가요?",
          "original_form": "오 카페브이로그면 커피 만들고 이런건가요?",
          "speaker_id": "1",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100002053.1.5",
          "form": "전 그 유리컵에 얼음 담기는 소리가 좋더라구요",
          "original_form": "전 그 유리컵에 얼음 담기는 소리가 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100002053.1.6",
          "form": "네 커피나 쥬스 에이드 이런거요",
          "original_form": "네 커피나 쥬스 에이드 이런거요",
          "speaker_id": "2",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100002053.1.7",
          "form": "얼음 가득넣고 만드는게 재밌어요",
          "original_form": "얼음 가득넣고 만드는게 재밌어요",
          "speaker_id": "2",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100002053.1.8",
          "form": "즐겨보는 컨텐츠 있으신가요?",
          "original_form": "즐겨보는 컨텐츠 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100002053.1.9",
          "form": "저는 asmr 자주 봐요",
          "original_form": "저는 asmr 자주 봐요",
          "speaker_id": "1",
          "time": "20210821 11:43"
        },
        {
          "id": "MDRW2100002053.1.10",
          "form": "그냥 브이로그도 좀 봤어요 딤디 좋아해용",
          "original_form": "그냥 브이로그도 좀 봤어요 딤디 좋아해용",
          "speaker_id": "1",
          "time": "20210821 11:44"
        },
        {
          "id": "MDRW2100002053.1.11",
          "form": "asmr은 어떤 종류 찾아보세요? 다양하던데",
          "original_form": "asmr은 어떤 종류 찾아보세요? 다양하던데",
          "speaker_id": "2",
          "time": "20210821 11:44"
        },
        {
          "id": "MDRW2100002053.1.12",
          "form": "원래는 귀파주는 영상 이런거 자주 봤는데",
          "original_form": "원래는 귀파주는 영상 이런거 자주 봤는데",
          "speaker_id": "1",
          "time": "20210821 11:45"
        },
        {
          "id": "MDRW2100002053.1.13",
          "form": "그냥 다양하게 보는 거 같아요!",
          "original_form": "그냥 다양하게 보는 거 같아요!",
          "speaker_id": "1",
          "time": "20210821 11:45"
        },
        {
          "id": "MDRW2100002053.1.14",
          "form": "뭔가 마음에 안정이 올 것같아요",
          "original_form": "뭔가 마음에 안정이 올 것같아요",
          "speaker_id": "2",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100002053.1.15",
          "form": "귀파주는 영상이라니",
          "original_form": "귀파주는 영상이라니",
          "speaker_id": "2",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100002053.1.16",
          "form": "즐겨보는 예능은 있으신가요?",
          "original_form": "즐겨보는 예능은 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100002053.1.17",
          "form": "소리가 진짜 리얼해요",
          "original_form": "소리가 진짜 리얼해요",
          "speaker_id": "1",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100002053.1.18",
          "form": "간지러운 느낌도 나고",
          "original_form": "간지러운 느낌도 나고",
          "speaker_id": "1",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100002053.1.19",
          "form": "듣기만 해도 간질거리네요",
          "original_form": "듣기만 해도 간질거리네요",
          "speaker_id": "2",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100002053.1.20",
          "form": "저 놀라운토요일 좋아해요ㅎㅎ 추리예능류도 좋아하구용",
          "original_form": "저 놀라운토요일 좋아해요ㅎㅎ 추리예능류도 좋아하구용",
          "speaker_id": "1",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100002053.1.21",
          "form": "어 저랑 취향이 겹치시네요",
          "original_form": "어 저랑 취향이 겹치시네요",
          "speaker_id": "2",
          "time": "20210821 11:47"
        },
        {
          "id": "MDRW2100002053.1.22",
          "form": "추리예능이라면 여고추리반같은거요?",
          "original_form": "추리예능이라면 여고추리반같은거요?",
          "speaker_id": "2",
          "time": "20210821 11:47"
        },
        {
          "id": "MDRW2100002053.1.23",
          "form": "네네 크라임씬 대탈출 이런것도 좋아하고 여추반이 최애입니다~",
          "original_form": "네네 크라임씬 대탈출 이런것도 좋아하고 여추반이 최애입니다~",
