      "utterance": [
        {
          "id": "MDRW2100044583.1.1",
          "form": "글킨해요",
          "original_form": "글킨해요",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044583.1.2",
          "form": "회덮밥 조아하세요?",
          "original_form": "회덮밥 조아하세요?",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044583.1.3",
          "form": "회덮밥 극 호!!!",
          "original_form": "회덮밥 극 호!!!",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044583.1.4",
          "form": "오호 광어회덮밥 먹고싶어요..",
          "original_form": "오호 광어회덮밥 먹고싶어요..",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044583.1.5",
          "form": "헐 저두요ㅠ",
          "original_form": "헐 저두요ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044583.1.6",
          "form": "맛있는 회덮밥 먹고싶어요ㅠ",
          "original_form": "맛있는 회덮밥 먹고싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044583.1.7",
          "form": "님 전어도 조아해요?",
          "original_form": "님 전어도 조아해요?",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044583.1.8",
          "form": "넹 전어는 쪼금 좋아해요 가시 ㅠ",
          "original_form": "넹 전어는 쪼금 좋아해요 가시 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044583.1.9",
          "form": "아하 그게또 맛잇는데",
          "original_form": "아하 그게또 맛잇는데",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044583.1.10",
          "form": "대하는요?",
          "original_form": "대하는요?",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044583.1.11",
          "form": "곧 대하 전어철이라",
          "original_form": "곧 대하 전어철이라",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044583.1.12",
          "form": "대하 맛있는곳있나요?",
          "original_form": "대하 맛있는곳있나요?",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044583.1.13",
          "form": "걍 어디가나 비슷 아닌가요",
          "original_form": "걍 어디가나 비슷 아닌가요",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044583.1.14",
          "form": "ㅋㅋ그런가요 전 머리까지 구워주는곳이 좋아요",
          "original_form": "ㅋㅋ그런가요 전 머리까지 구워주는곳이 좋아요",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044583.1.15",
          "form": "오 그건 필수 그리고 머리튀김",
          "original_form": "오 그건 필수 그리고 머리튀김",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044583.1.16",
          "form": "그쳐그쵸 머리튀김진짜 짱맛 ㅠㅠ",
          "original_form": "그쳐그쵸 머리튀김진짜 짱맛 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044583.1.17",
          "form": "아 ...곧입니다 대하철",
          "original_form": "아 ...곧입니다 대하철",
          "speaker_id": "2",
          "time": "20210907 23:15"
        }
      ]
