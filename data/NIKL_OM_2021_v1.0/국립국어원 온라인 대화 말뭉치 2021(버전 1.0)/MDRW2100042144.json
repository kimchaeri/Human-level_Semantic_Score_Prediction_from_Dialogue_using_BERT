      "utterance": [
        {
          "id": "MDRW2100042144.1.1",
          "form": "저 정재현한테 오빠나 자기야라고 한번 해보고싶어요",
          "original_form": "저 정재현한테 오빠나 자기야라고 한번 해보고싶어요",
          "speaker_id": "2",
          "time": "20210906 20:05"
        },
        {
          "id": "MDRW2100042144.1.2",
          "form": "엄청 놀랄거같아서 재밋을거같아요",
          "original_form": "엄청 놀랄거같아서 재밋을거같아요",
          "speaker_id": "2",
          "time": "20210906 20:05"
        },
        {
          "id": "MDRW2100042144.1.3",
          "form": "헉 재밌겠다...",
          "original_form": "헉 재밌겠다...",
          "speaker_id": "1",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042144.1.4",
          "form": "평소엔 이름 부르다가 오빠라고 호칭 살짝 바꾸면",
          "original_form": "평소엔 이름 부르다가 오빠라고 호칭 살짝 바꾸면",
          "speaker_id": "1",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042144.1.5",
          "form": "정신 못차릴듯요",
          "original_form": "정신 못차릴듯요",
          "speaker_id": "1",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042144.1.6",
          "form": "실실 웃다가 머라고? 다시 해줘ㅎㅎ",
          "original_form": "실실 웃다가 머라고? 다시 해줘ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:08"
        },
        {
          "id": "MDRW2100042144.1.7",
          "form": "와 진짜 오바....",
          "original_form": "와 진짜 오바....",
          "speaker_id": "1",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042144.1.8",
          "form": "재민아!!!!",
          "original_form": "재민아!!!!",
          "speaker_id": "1",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042144.1.9",
          "form": "나랑 결혼하자!!!",
          "original_form": "나랑 결혼하자!!!",
          "speaker_id": "1",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042144.1.10",
          "form": "정재현한테 결혼 안한다고 하다가",
          "original_form": "정재현한테 결혼 안한다고 하다가",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042144.1.11",
          "form": "같이 소파에 기대 앉아서 넷플 보다가",
          "original_form": "같이 소파에 기대 앉아서 넷플 보다가",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042144.1.12",
          "form": "자기야 결혼할까? 이래서",
          "original_form": "자기야 결혼할까? 이래서",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042144.1.13",
          "form": "어버버거리게 하고 싶어요",
          "original_form": "어버버거리게 하고 싶어요",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100042144.1.14",
          "form": "좋다...",
          "original_form": "좋다...",
          "speaker_id": "1",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042144.1.15",
          "form": "제 애인은",
          "original_form": "제 애인은",
          "speaker_id": "1",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042144.1.16",
          "form": "저런걸로 결혼하자 하면 경멸할것같아요",
          "original_form": "저런걸로 결혼하자 하면 경멸할것같아요",
          "speaker_id": "1",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042144.1.17",
          "form": "재민이.. 뭐? 하다가",
          "original_form": "재민이.. 뭐? 하다가",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042144.1.18",
          "form": "결혼? 결혼하자고? 참... 심심해?",
          "original_form": "결혼? 결혼하자고? 참... 심심해?",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042144.1.19",
          "form": "너무하다....",
          "original_form": "너무하다....",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042144.1.20",
          "form": "기껏 청혼했건만 거절당하기",
          "original_form": "기껏 청혼했건만 거절당하기",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042144.1.21",
          "form": "진심으로 싫어할거같아서..",
          "original_form": "진심으로 싫어할거같아서..",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042144.1.22",
          "form": "너무해......ㅠㅠ",
          "original_form": "너무해......ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042144.1.23",
          "form": "정재현한테 이렇게 무드없게 청혼해놓고 담날 아침에 정재현 출근전에 반지주고싶어요..",
          "original_form": "정재현한테 이렇게 무드없게 청혼해놓고 담날 아침에 정재현 출근전에 반지주고싶어요..",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042144.1.24",
          "form": "비싼건아니지만 선물이야 하고..",
          "original_form": "비싼건아니지만 선물이야 하고..",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042144.1.25",
          "form": "로맨틱한 청혼이네요",
          "original_form": "로맨틱한 청혼이네요",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042144.1.26",
          "form": "정재현 이거 한정판인거 알아보고 이잉ㅠ 자기야ㅠ 해서 빨리가!!!! 윽박지르기..",
          "original_form": "정재현 이거 한정판인거 알아보고 이잉ㅠ 자기야ㅠ 해서 빨리가!!!! 윽박지르기..",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042144.1.27",
          "form": "저희는 결혼반지 고르면서도 싸울듯",
          "original_form": "저희는 결혼반지 고르면서도 싸울듯",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042144.1.28",
          "form": "결혼반지는 정재현이 자기야 내가할게 나 믿어 이래서 결혼식장에서 보니까 예쁜반지 줄거 같아요..",
          "original_form": "결혼반지는 정재현이 자기야 내가할게 나 믿어 이래서 결혼식장에서 보니까 예쁜반지 줄거 같아요..",
          "speaker_id": "2",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042144.1.29",
          "form": "하.....",
          "original_form": "하.....",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042144.1.30",
          "form": "아무래도 재민이 말고 역시 제노랑 결혼을....",
          "original_form": "아무래도 재민이 말고 역시 제노랑 결혼을....",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042144.1.31",
          "form": "제노는., 로맨틱가이..",
          "original_form": "제노는., 로맨틱가이..",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042144.1.32",
          "form": "아무래도....정상성 가이니까요",
          "original_form": "아무래도....정상성 가이니까요",
          "speaker_id": "1",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100042144.1.33",
          "form": "정상성에서 오는.. 시몬스침대의 편안함",
          "original_form": "정상성에서 오는.. 시몬스침대의 편안함",
          "speaker_id": "2",
          "time": "20210906 20:41"
        },
        {
          "id": "MDRW2100042144.1.34",
          "form": "시몬스 침댘ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "시몬스 침댘ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042144.1.35",
          "form": "근데 그럴만함",
          "original_form": "근데 그럴만함",
          "speaker_id": "1",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042144.1.36",
          "form": "제노는 시몬스침대고.. 재현이는 푹 들어가는 침대느낌이죠",
          "original_form": "제노는 시몬스침대고.. 재현이는 푹 들어가는 침대느낌이죠",
          "speaker_id": "2",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100042144.1.37",
          "form": "정말 제노랑 연애를 거쳐서 결혼하고 싶어요",
          "original_form": "정말 제노랑 연애를 거쳐서 결혼하고 싶어요",
          "speaker_id": "1",
          "time": "20210906 20:47"
        },
        {
          "id": "MDRW2100042144.1.38",
          "form": "물론 걔는 청혼안해서 제가 해야겠지만",
          "original_form": "물론 걔는 청혼안해서 제가 해야겠지만",
