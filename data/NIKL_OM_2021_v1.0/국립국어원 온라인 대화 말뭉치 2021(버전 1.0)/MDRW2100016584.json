      "utterance": [
        {
          "id": "MDRW2100016584.1.1",
          "form": "쇼핑 좋아하시나요",
          "original_form": "쇼핑 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016584.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016584.1.3",
          "form": "최근 쇼핑한거잇나요?",
          "original_form": "최근 쇼핑한거잇나요?",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016584.1.4",
          "form": "신발이요ㅎㅎ",
          "original_form": "신발이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016584.1.5",
          "form": "저는 어제 가방 셀프선물 햇슴다",
          "original_form": "저는 어제 가방 셀프선물 햇슴다",
          "speaker_id": "2",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016584.1.6",
          "form": "일할맛나네요",
          "original_form": "일할맛나네요",
          "speaker_id": "2",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016584.1.7",
          "form": "오 어떤가방사셨나요",
          "original_form": "오 어떤가방사셨나요",
          "speaker_id": "1",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100016584.1.8",
          "form": "걍 데일리 가방샀어요 ㅎㅎ",
          "original_form": "걍 데일리 가방샀어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100016584.1.9",
          "form": "위시리스트 물건있으세요?",
          "original_form": "위시리스트 물건있으세요?",
          "speaker_id": "2",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016584.1.10",
          "form": "음 폴드3 사고싶네요ㅎㅎ",
          "original_form": "음 폴드3 사고싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016584.1.11",
          "form": "엇!! 저는 플립",
          "original_form": "엇!! 저는 플립",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016584.1.12",
          "form": "아마 올해안에 바꿀꺼같아요 ㅋㅋ",
          "original_form": "아마 올해안에 바꿀꺼같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016584.1.13",
          "form": "오오 사전예약 하셧나요",
          "original_form": "오오 사전예약 하셧나요",
          "speaker_id": "1",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016584.1.14",
          "form": "아녀ㅜㅜ 지금 뽐뿌가 크게와서",
          "original_form": "아녀ㅜㅜ 지금 뽐뿌가 크게와서",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016584.1.15",
          "form": "겨울쯤 바꿀듯 싶어요",
          "original_form": "겨울쯤 바꿀듯 싶어요",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016584.1.16",
          "form": "그때쯤이면 좀 떨어질까요가격",
          "original_form": "그때쯤이면 좀 떨어질까요가격",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016584.1.17",
          "form": "아이폰 나오면 좀 떨어지지않을까하고 존버중이에요",
          "original_form": "아이폰 나오면 좀 떨어지지않을까하고 존버중이에요",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016584.1.18",
          "form": "폴듣는 가격이 ㅎㄷㄷ",
          "original_form": "폴듣는 가격이 ㅎㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016584.1.19",
