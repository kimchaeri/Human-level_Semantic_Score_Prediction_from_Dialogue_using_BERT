      "utterance": [
        {
          "id": "MDRW2100020651.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020651.1.2",
          "form": "그럼 한학기 휴학내고 여행 가시는게 어때요",
          "original_form": "그럼 한학기 휴학내고 여행 가시는게 어때요",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020651.1.3",
          "form": "다이어트 실패한 기념으로",
          "original_form": "다이어트 실패한 기념으로",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020651.1.4",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020651.1.5",
          "form": "상처..",
          "original_form": "상처..",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020651.1.6",
          "form": "받아써여..",
          "original_form": "받아써여..",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020651.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020651.1.8",
          "form": "미안해요",
          "original_form": "미안해요",
          "speaker_id": "1",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020651.1.9",
          "form": "저는 아침에 늦게 일어난 기념으로 휴학하고 국토종주했었어요",
          "original_form": "저는 아침에 늦게 일어난 기념으로 휴학하고 국토종주했었어요",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020651.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100020651.1.11",
          "form": "어땠어용??",
          "original_form": "어땠어용??",
          "speaker_id": "2",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100020651.1.12",
          "form": "갔다가 3일만에 돌아왔어요",
          "original_form": "갔다가 3일만에 돌아왔어요",
          "speaker_id": "1",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100020651.1.13",
          "form": "오호~",
          "original_form": "오호~",
          "speaker_id": "2",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100020651.1.14",
          "form": "어디 갔다오셨어요?",
          "original_form": "어디 갔다오셨어요?",
          "speaker_id": "2",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100020651.1.15",
          "form": "집이 너무 그리워서...",
          "original_form": "집이 너무 그리워서...",
          "speaker_id": "1",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100020651.1.16",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100020651.1.17",
          "form": "대구까지 갔다가 친구 만나서 친구 차 얻어타고 집 갔어요",
          "original_form": "대구까지 갔다가 친구 만나서 친구 차 얻어타고 집 갔어요",
          "speaker_id": "1",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100020651.1.18",
          "form": "집이 대구랑 가까우신가요?",
          "original_form": "집이 대구랑 가까우신가요?",
          "speaker_id": "2",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100020651.1.19",
          "form": "출발을 부산에 기차타고 갔어요ㅋㅋ",
          "original_form": "출발을 부산에 기차타고 갔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100020651.1.20",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100020651.1.21",
          "form": "집이 부산이세요?",
          "original_form": "집이 부산이세요?",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100020651.1.22",
          "form": "아뇨 부산에서 서울로 가려고 부산까지 기차타고 가서 거기서 사작했어요",
          "original_form": "아뇨 부산에서 서울로 가려고 부산까지 기차타고 가서 거기서 사작했어요",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100020651.1.23",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100020651.1.24",
          "form": "그럼 짧은 기간동안 가장 기억에 남는 것이 뭐였어요??",
          "original_form": "그럼 짧은 기간동안 가장 기억에 남는 것이 뭐였어요??",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100020651.1.25",
          "form": "땡볕에 쪄지면서 걸어가는데",
          "original_form": "땡볕에 쪄지면서 걸어가는데",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100020651.1.26",
          "form": "맞은편 자전거가 침뱉고간게 제일 기억나네요",
          "original_form": "맞은편 자전거가 침뱉고간게 제일 기억나네요",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100020651.1.27",
          "form": "와 정말 잊을 수 없는 기억이겠군요",
          "original_form": "와 정말 잊을 수 없는 기억이겠군요",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100020651.1.28",
          "form": "여름에 가셨나봐요",
          "original_form": "여름에 가셨나봐요",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100020651.1.29",
          "form": "완전 여름까진 아니었는데 엄청 더운날이었어요",
          "original_form": "완전 여름까진 아니었는데 엄청 더운날이었어요",
          "speaker_id": "1",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100020651.1.30",
          "form": "아 몇 월에 가셨어요?",
          "original_form": "아 몇 월에 가셨어요?",
          "speaker_id": "2",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100020651.1.31",
          "form": "6월쯤에 갔던것같아요",
          "original_form": "6월쯤에 갔던것같아요",
          "speaker_id": "1",
          "time": "20210830 09:47"
        },
        {
          "id": "MDRW2100020651.1.32",
          "form": "아 약간 더워질 때네요",
          "original_form": "아 약간 더워질 때네요",
          "speaker_id": "2",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100020651.1.33",
          "form": "여행 많이 다니셨어요?",
          "original_form": "여행 많이 다니셨어요?",
          "speaker_id": "2",
          "time": "20210830 11:51"
        },
        {
          "id": "MDRW2100020651.1.34",
          "form": "간단하게 국내여행 자주 갔었어요",
          "original_form": "간단하게 국내여행 자주 갔었어요",
          "speaker_id": "1",
          "time": "20210830 15:48"
        },
        {
          "id": "MDRW2100020651.1.35",
          "form": "가까운곳",
          "original_form": "가까운곳",
          "speaker_id": "1",
          "time": "20210830 15:48"
        }
      ]
