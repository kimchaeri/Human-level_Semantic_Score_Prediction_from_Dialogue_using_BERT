      "utterance": [
        {
          "id": "MDRW2100007719.1.1",
          "form": "마이 시크릿 찾으셨어요",
          "original_form": "마이 시크릿 찾으셨어요",
          "speaker_id": "1",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007719.1.2",
          "form": "네! 티빙에 있어서 3화까지 보고 잤어요ㅋㅋㅋ",
          "original_form": "네! 티빙에 있어서 3화까지 보고 잤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007719.1.3",
          "form": "보다보니 살짝 본거같기도 하더라구요ㅎㅎ",
          "original_form": "보다보니 살짝 본거같기도 하더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007719.1.4",
          "form": "그렇죠 봤을거예요",
          "original_form": "그렇죠 봤을거예요",
          "speaker_id": "1",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007719.1.5",
          "form": "별로인가요",
          "original_form": "별로인가요",
          "speaker_id": "1",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007719.1.6",
          "form": "팜 근데 범인이 생각이 안네요",
          "original_form": "팜 근데 범인이 생각이 안네요",
          "speaker_id": "1",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007719.1.7",
          "form": "참",
          "original_form": "참",
          "speaker_id": "1",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007719.1.8",
          "form": "아니요 재미있었어요! ㅋ\r\n오늘 정주행할려구요",
          "original_form": "아니요 재미있었어요! ㅋ\r\n오늘 정주행할려구요",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007719.1.9",
          "form": "생각이 요새 안나요",
          "original_form": "생각이 요새 안나요",
          "speaker_id": "1",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007719.1.10",
          "form": "저두요ㅜㅜ",
          "original_form": "저두요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007719.1.11",
          "form": "맞아요 저 2번 봤어요",
          "original_form": "맞아요 저 2번 봤어요",
          "speaker_id": "1",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007719.1.12",
          "form": "근데도",
          "original_form": "근데도",
          "speaker_id": "1",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007719.1.13",
          "form": "자꾸 오늘은 이사람 저사람 의심해서 ㅋㅋ",
          "original_form": "자꾸 오늘은 이사람 저사람 의심해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007719.1.14",
          "form": "저는 특히.,  영화가 기억이 더안나요",
          "original_form": "저는 특히.,  영화가 기억이 더안나요",
          "speaker_id": "2",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007719.1.15",
          "form": "형사 범죄사 사기꾼 이런내용이면",
          "original_form": "형사 범죄사 사기꾼 이런내용이면",
          "speaker_id": "2",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007719.1.16",
          "form": "맞아요  아 요샌 더 기억이",
          "original_form": "맞아요  아 요샌 더 기억이",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007719.1.17",
          "form": "안나요",
          "original_form": "안나요",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007719.1.18",
          "form": "다~~~~~~똑같애보여서 제목이랑 매칭을 못해요ㅋㅋ",
          "original_form": "다~~~~~~똑같애보여서 제목이랑 매칭을 못해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007719.1.19",
          "form": "저도요 검색이라도 잘되니 찾죠",
          "original_form": "저도요 검색이라도 잘되니 찾죠",
          "speaker_id": "1",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007719.1.20",
          "form": "아 다시 보고싶네요",
          "original_form": "아 다시 보고싶네요",
          "speaker_id": "1",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007719.1.21",
          "form": "저 티빙 없어서",
          "original_form": "저 티빙 없어서",
          "speaker_id": "1",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007719.1.22",
          "form": "저도 티빙 혼자보면 너무 비싸서",
          "original_form": "저도 티빙 혼자보면 너무 비싸서",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007719.1.23",
          "form": "왜이리 비싸요 ㅜㅜ",
          "original_form": "왜이리 비싸요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007719.1.24",
          "form": "4명  모집하는 사람한테 월  나눠서 지불하고 봐요ㅋ",
          "original_form": "4명  모집하는 사람한테 월  나눠서 지불하고 봐요ㅋ",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007719.1.25",
          "form": "저도 넷플 그래요",
          "original_form": "저도 넷플 그래요",
          "speaker_id": "1",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007719.1.26",
          "form": "안그럼 돈 아까워요",
          "original_form": "안그럼 돈 아까워요",
          "speaker_id": "1",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100007719.1.27",
          "form": "자주 안보는데 그돈 다 못내요",
          "original_form": "자주 안보는데 그돈 다 못내요",
          "speaker_id": "1",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100007719.1.28",
          "form": "뭐 추천 있으세요?",
          "original_form": "뭐 추천 있으세요?",
          "speaker_id": "1",
          "time": "20210822 10:08"
        },
        {
          "id": "MDRW2100007719.1.29",
          "form": "티빙은 티비엔때문에.,  ㅠ\r\n티비를 실시간으로 항상 못보니깐요",
          "original_form": "티빙은 티비엔때문에.,  ㅠ\r\n티비를 실시간으로 항상 못보니깐요",
          "speaker_id": "2",
          "time": "20210822 10:08"
        },
        {
          "id": "MDRW2100007719.1.30",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210822 10:08"
        },
        {
          "id": "MDRW2100007719.1.31",
          "form": "여기 드라마 다 좋았는데 요새 좀 별로 없어요",
          "original_form": "여기 드라마 다 좋았는데 요새 좀 별로 없어요",
          "speaker_id": "1",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100007719.1.32",
          "form": "검법남녀는 보셨어요?",
          "original_form": "검법남녀는 보셨어요?",
          "speaker_id": "2",
          "time": "20210822 10:09"
        },
        {
          "id": "MDRW2100007719.1.33",
          "form": "네 잼나요",
          "original_form": "네 잼나요",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100007719.1.34",
          "form": "붉은달 푸른해 보셨어요",
          "original_form": "붉은달 푸른해 보셨어요",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100007719.1.35",
          "form": "저 흥미진진해요",
          "original_form": "저 흥미진진해요",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100007719.1.36",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "1",
          "time": "20210822 10:10"
        },
        {
          "id": "MDRW2100007719.1.37",
          "form": "검법남녀 2도 다봤어요 ㅎㅎ",
          "original_form": "검법남녀 2도 다봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100007719.1.38",
          "form": "또 안나오나요?",
          "original_form": "또 안나오나요?",
          "speaker_id": "1",
          "time": "20210822 10:11"
        },
        {
          "id": "MDRW2100007719.1.39",
          "form": "저는 그거 몇일전에 다봤어요ㅋㅋ",
          "original_form": "저는 그거 몇일전에 다봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:11"
        }
      ]
    }
