      "utterance": [
        {
          "id": "MDRW2100008117.1.1",
          "form": "해삼젤라또 맛있나요?",
          "original_form": "해삼젤라또 맛있나요?",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100008117.1.2",
          "form": "넹 저는 그럴 거라고 믿습니다 ㅋㅋㅋㅋ",
          "original_form": "넹 저는 그럴 거라고 믿습니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008117.1.3",
          "form": "오늘 저녁 뭐 드셨어요?",
          "original_form": "오늘 저녁 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008117.1.4",
          "form": "짜장면과 탕수육  먹었습니다 ㅎㅎ",
          "original_form": "짜장면과 탕수육  먹었습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008117.1.5",
          "form": "저는 알바중이라 아직 못 먹었어요ㅠ",
          "original_form": "저는 알바중이라 아직 못 먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008117.1.6",
          "form": "이따가 알바 끝나면 배달음식 시켜먹고싶어요ㅎㅎ",
          "original_form": "이따가 알바 끝나면 배달음식 시켜먹고싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008117.1.7",
          "form": "아이구 ㅠㅠㅠ",
          "original_form": "아이구 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008117.1.8",
          "form": "뭐 드실 건가요 ㅎㅎ",
          "original_form": "뭐 드실 건가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008117.1.9",
          "form": "저 곱창전골이나 소곱창이나 두부김치 먹고싶어요ㅠㅠ",
          "original_form": "저 곱창전골이나 소곱창이나 두부김치 먹고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008117.1.10",
          "form": "헉 세상에 곱창!!",
          "original_form": "헉 세상에 곱창!!",
          "speaker_id": "2",
          "time": "20210822 19:30"
        },
        {
          "id": "MDRW2100008117.1.11",
          "form": "곱창 좋아하세요?ㅎㅎ",
          "original_form": "곱창 좋아하세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008117.1.12",
          "form": "넹넹 먹은지는 좀 오래된 것 같지만 ㅠㅠ",
          "original_form": "넹넹 먹은지는 좀 오래된 것 같지만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008117.1.13",
          "form": "곱창 망고는 무슨 음식 좋아하세요??",
          "original_form": "곱창 망고는 무슨 음식 좋아하세요??",
          "speaker_id": "1",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008117.1.14",
          "form": "말고",
          "original_form": "말고",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008117.1.15",
          "form": "음 마라탕?",
          "original_form": "음 마라탕?",
          "speaker_id": "2",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008117.1.16",
          "form": "마라탕!!! 저도 너무너무너무 좋아해요ㅠㅠㅠㅠ",
          "original_form": "마라탕!!! 저도 너무너무너무 좋아해요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 19:32"
        },
        {
          "id": "MDRW2100008117.1.17",
          "form": "오늘 먹어도 내일 또 먹고 싶은 음식 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오늘 먹어도 내일 또 먹고 싶은 음식 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008117.1.18",
          "form": "근데 집 근처에 마라탕 가게가 없어서 귀찮을 때는편의점에서 사천왕마라탕?? 컵라면 사서 먹어용 이거 진짜 맛있어요",
          "original_form": "근데 집 근처에 마라탕 가게가 없어서 귀찮을 때는편의점에서 사천왕마라탕?? 컵라면 사서 먹어용 이거 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008117.1.19",
          "form": "ㅋㅋㅋ마자요 아 마라먹고싶더ㅜ",
          "original_form": "ㅋㅋㅋ마자요 아 마라먹고싶더ㅜ",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008117.1.20",
          "form": "오 라면으로도 나왔구낭",
          "original_form": "오 라면으로도 나왔구낭",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008117.1.21",
          "form": "전 자주가던 집이 주인이 바뀌면서 맛이 바뀌었어요...크흑 ㅠㅜㅠ",
          "original_form": "전 자주가던 집이 주인이 바뀌면서 맛이 바뀌었어요...크흑 ㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008117.1.22",
          "form": "아아ㅜㅜ",
          "original_form": "아아ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008117.1.23",
          "form": "슬픔입니당 ㅜㅜ",
          "original_form": "슬픔입니당 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008117.1.24",
          "form": "이따가 배달음식과 함께 마라 컵라면도 드세영",
          "original_form": "이따가 배달음식과 함께 마라 컵라면도 드세영",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008117.1.25",
          "form": "그 생각으로 알바따위 이겨내버리는 겁니당!",
          "original_form": "그 생각으로 알바따위 이겨내버리는 겁니당!",
          "speaker_id": "2",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008117.1.26",
          "form": "조아요!",
          "original_form": "조아요!",
          "speaker_id": "1",
