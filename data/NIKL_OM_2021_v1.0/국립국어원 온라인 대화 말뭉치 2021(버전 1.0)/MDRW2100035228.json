      "utterance": [
        {
          "id": "MDRW2100035228.1.1",
          "form": "전참견 보는데 박하선 착 벤이 아니네 일반 승용차",
          "original_form": "전참견 보는데 박하선 착 벤이 아니네 일반 승용차",
          "speaker_id": "2",
          "time": "20210829 12:21"
        },
        {
          "id": "MDRW2100035228.1.2",
          "form": "차가",
          "original_form": "차가",
          "speaker_id": "2",
          "time": "20210829 12:21"
        },
        {
          "id": "MDRW2100035228.1.3",
          "form": "어디서 해?",
          "original_form": "어디서 해?",
          "speaker_id": "1",
          "time": "20210829 12:21"
        },
        {
          "id": "MDRW2100035228.1.4",
          "form": "엠비씨 드라마넷",
          "original_form": "엠비씨 드라마넷",
          "speaker_id": "2",
          "time": "20210829 12:21"
        },
        {
          "id": "MDRW2100035228.1.5",
          "form": "잠시만 틀어보께",
          "original_form": "잠시만 틀어보께",
          "speaker_id": "1",
          "time": "20210829 12:21"
        },
        {
          "id": "MDRW2100035228.1.6",
          "form": "박하선 정도면 그냥 벤타고 다녀야 하는거 아니가",
          "original_form": "박하선 정도면 그냥 벤타고 다녀야 하는거 아니가",
          "speaker_id": "2",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100035228.1.7",
          "form": "꼭 그렇지는 않은듯",
          "original_form": "꼭 그렇지는 않은듯",
          "speaker_id": "1",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100035228.1.8",
          "form": "검은태양이라는 국정원 드라마 출연하는갑다",
          "original_form": "검은태양이라는 국정원 드라마 출연하는갑다",
          "speaker_id": "2",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100035228.1.9",
          "form": "이제 하는가보네",
          "original_form": "이제 하는가보네",
          "speaker_id": "1",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100035228.1.10",
          "form": "9월17일 첫방이다",
          "original_form": "9월17일 첫방이다",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100035228.1.11",
          "form": "엠비씨에서 하는거?",
          "original_form": "엠비씨에서 하는거?",
          "speaker_id": "1",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100035228.1.12",
          "form": "엉 12부작이다",
          "original_form": "엉 12부작이다",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100035228.1.13",
          "form": "위에 떠 있네ㅋ",
          "original_form": "위에 떠 있네ㅋ",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100035228.1.14",
          "form": "ㅋㅋ남궁민이 국정원 최고 요원인데 사고가 일어나고 그래서 내부 배신자를 찾는다는",
          "original_form": "ㅋㅋ남궁민이 국정원 최고 요원인데 사고가 일어나고 그래서 내부 배신자를 찾는다는",
          "speaker_id": "2",
          "time": "20210829 12:25"
        }
      ]
