      "utterance": [
        {
          "id": "MDRW2100033675.1.1",
          "form": "동물 키우시나요",
          "original_form": "동물 키우시나요",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100033675.1.2",
          "form": "아뇨! 동물은 키워본 적이 없어요ㅎ",
          "original_form": "아뇨! 동물은 키워본 적이 없어요ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033675.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033675.1.4",
          "form": "전 완전 어릴 때요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 완전 어릴 때요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033675.1.5",
          "form": "키우고 싶은 동물은요?",
          "original_form": "키우고 싶은 동물은요?",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033675.1.6",
          "form": "키울 수 있다면 고양이요!",
          "original_form": "키울 수 있다면 고양이요!",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033675.1.7",
          "form": "어떤 동물 키우셨어요?",
          "original_form": "어떤 동물 키우셨어요?",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033675.1.8",
          "form": "강아지요!!!!!!",
          "original_form": "강아지요!!!!!!",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033675.1.9",
          "form": "어떤 강아지였어요??",
          "original_form": "어떤 강아지였어요??",
          "speaker_id": "1",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033675.1.10",
          "form": "말티즈요",
          "original_form": "말티즈요",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033675.1.11",
          "form": "근데 제가 너무 어려서 입양 보냈어요 흑",
          "original_form": "근데 제가 너무 어려서 입양 보냈어요 흑",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033675.1.12",
          "form": "슬펐겠어요ㅜ",
          "original_form": "슬펐겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210824 23:27"
        },
        {
          "id": "MDRW2100033675.1.13",
          "form": "네 ㅠㅠㅠㅠ",
          "original_form": "네 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033675.1.14",
          "form": "근데 지금은 고양이 좋아해요",
          "original_form": "근데 지금은 고양이 좋아해요",
          "speaker_id": "2",
          "time": "20210824 23:29"
        },
        {
          "id": "MDRW2100033675.1.15",
          "form": "혹시 고양이카페 가보셨어요?",
          "original_form": "혹시 고양이카페 가보셨어요?",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100033675.1.16",
          "form": "완전 옛날에 갔던 거 같은데",
          "original_form": "완전 옛날에 갔던 거 같은데",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100033675.1.17",
          "form": "그땐 고양이릉 무서워했어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그땐 고양이릉 무서워했어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100033675.1.18",
          "form": "ㅋㅋㅋ저도 초반에는 무서워했는데 할퀼까봐ㅋㅋ",
          "original_form": "ㅋㅋㅋ저도 초반에는 무서워했는데 할퀼까봐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033675.1.19",
          "form": "지금은 솜방망이가 세상 귀엽죠",
