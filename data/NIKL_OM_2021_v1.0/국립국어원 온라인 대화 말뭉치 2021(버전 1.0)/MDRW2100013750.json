      "utterance": [
        {
          "id": "MDRW2100013750.1.1",
          "form": "안녕하세요 어떤 연예인 좋아하시나요?",
          "original_form": "안녕하세요 어떤 연예인 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013750.1.2",
          "form": "이동욱님이요!",
          "original_form": "이동욱님이요!",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013750.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013750.1.4",
          "form": "아항 저는 배우는 딱히 없는데 손예진 관심있어요 ㅎㅎ!",
          "original_form": "아항 저는 배우는 딱히 없는데 손예진 관심있어요 ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013750.1.5",
          "form": "어떤 작품 좋아하기나용?",
          "original_form": "어떤 작품 좋아하기나용?",
          "speaker_id": "1",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013750.1.6",
          "form": "드라마요?",
          "original_form": "드라마요?",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013750.1.7",
          "form": "음 넹 아무거나욥",
          "original_form": "음 넹 아무거나욥",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013750.1.8",
          "form": "저는 최근에(? 빈센조 재밌게봣어용 ㅎㅎ",
          "original_form": "저는 최근에(? 빈센조 재밌게봣어용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013750.1.9",
          "form": "저는 결혼작사 이혼작곡이요ㅋㅋ빡치면서도 사이다를 기대하고 봤죠...",
          "original_form": "저는 결혼작사 이혼작곡이요ㅋㅋ빡치면서도 사이다를 기대하고 봤죠...",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013750.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㄱㅇㄱ 저희 엄마도 그거 엄청 보셨어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㄱㅇㄱ 저희 엄마도 그거 엄청 보셨어요",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013750.1.11",
          "form": "막장이면... 저는 가끔 뇌비우고 펜하 봐요 ㅋㅎㅋㅎㅋㅋ",
          "original_form": "막장이면... 저는 가끔 뇌비우고 펜하 봐요 ㅋㅎㅋㅎㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013750.1.12",
          "form": "아 펜하...!",
          "original_form": "아 펜하...!",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013750.1.13",
          "form": "저도 펜하 봤다가 중간부터 보다말다했는데",
          "original_form": "저도 펜하 봤다가 중간부터 보다말다했는데",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013750.1.14",
          "form": "이번에 한 번 봤다가 충격먹었어요",
          "original_form": "이번에 한 번 봤다가 충격먹었어요",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013750.1.15",
          "form": "마지막장면보고ㅋㅋㅋㅋㅋ",
          "original_form": "마지막장면보고ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013750.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아 인정이요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아 인정이요...",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013750.1.17",
          "form": "김소연 연기 진짜 잘해요 존멋",
          "original_form": "김소연 연기 진짜 잘해요 존멋",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013750.1.18",
          "form": "ㅋㅋㅋㅋ진짜 인성도 너무 좋은 배우..",
          "original_form": "ㅋㅋㅋㅋ진짜 인성도 너무 좋은 배우..",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013750.1.19",
          "form": "입에서 분수뿜듯...그 .. 말 못할 장면은 잊지 못할거에요...",
          "original_form": "입에서 분수뿜듯...그 .. 말 못할 장면은 잊지 못할거에요...",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013750.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
