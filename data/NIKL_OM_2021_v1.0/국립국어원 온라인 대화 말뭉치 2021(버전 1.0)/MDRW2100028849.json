      "utterance": [
        {
          "id": "MDRW2100028849.1.1",
          "form": "최근여행지가 있나요?",
          "original_form": "최근여행지가 있나요?",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028849.1.2",
          "form": "네..호캉스요",
          "original_form": "네..호캉스요",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028849.1.3",
          "form": "와~~\n멋지시네요.\n어느장소로 가셨어요?",
          "original_form": "와~~\n멋지시네요.\n어느장소로 가셨어요?",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028849.1.4",
          "form": "대전신세계 호텔요!!",
          "original_form": "대전신세계 호텔요!!",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028849.1.5",
          "form": "뷔페먹고 푹쉬고 구경하구요",
          "original_form": "뷔페먹고 푹쉬고 구경하구요",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028849.1.6",
          "form": "즐거웠겠네요",
          "original_form": "즐거웠겠네요",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028849.1.7",
          "form": "가족들이 너무  좋와 했겠네요.",
          "original_form": "가족들이 너무  좋와 했겠네요.",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100028849.1.8",
          "form": "저도 누구보다 가족과 가는 것이 제일 좋와요.ㅎ",
          "original_form": "저도 누구보다 가족과 가는 것이 제일 좋와요.ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100028849.1.9",
          "form": "가족과 간건아니구요..",
          "original_form": "가족과 간건아니구요..",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100028849.1.10",
          "form": "갑자기 가족한테미안하네요",
          "original_form": "갑자기 가족한테미안하네요",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028849.1.11",
          "form": "가족하고 간거이 아니에요?",
          "original_form": "가족하고 간거이 아니에요?",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028849.1.12",
          "form": "네..아는언니랑요",
          "original_form": "네..아는언니랑요",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100028849.1.13",
          "form": "휴가받은거죠",
          "original_form": "휴가받은거죠",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100028849.1.14",
          "form": "저는 가족하고 같이 안가면 밥도 안 먹히 더라고요",
          "original_form": "저는 가족하고 같이 안가면 밥도 안 먹히 더라고요",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100028849.1.15",
          "form": "저두그렇긴해요",
          "original_form": "저두그렇긴해요",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028849.1.16",
          "form": "뷔페갔는데 많이못먹었어요",
          "original_form": "뷔페갔는데 많이못먹었어요",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028849.1.17",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028849.1.18",
          "form": "속이 안 좋으셨어요",
          "original_form": "속이 안 좋으셨어요",
