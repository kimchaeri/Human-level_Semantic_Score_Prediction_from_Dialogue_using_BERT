      "utterance": [
        {
          "id": "MDRW2100013801.1.1",
          "form": "예능 볼게 정말 많네요",
          "original_form": "예능 볼게 정말 많네요",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013801.1.2",
          "form": "웹소설 빠져있는 와중이라서",
          "original_form": "웹소설 빠져있는 와중이라서",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013801.1.3",
          "form": "지난 목욜쯤부터 영상은 못봤는대",
          "original_form": "지난 목욜쯤부터 영상은 못봤는대",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013801.1.4",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013801.1.5",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013801.1.6",
          "form": "아는 형님도 재밌었어요",
          "original_form": "아는 형님도 재밌었어요",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013801.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013801.1.8",
          "form": "앗 아직 지지난주까지만 봤어요",
          "original_form": "앗 아직 지지난주까지만 봤어요",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013801.1.9",
          "form": "펜싱팀 넘 웃기더라고요ㅋㅋ",
          "original_form": "펜싱팀 넘 웃기더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013801.1.10",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013801.1.11",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013801.1.12",
          "form": "노는 브로도 ㅋㅋ",
          "original_form": "노는 브로도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013801.1.13",
          "form": "오늘은 야구 하겠죠",
          "original_form": "오늘은 야구 하겠죠",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013801.1.14",
          "form": "지금 난리인데",
          "original_form": "지금 난리인데",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013801.1.15",
          "form": "부산은 비가 꽤 와서 아마 안할지도 몰라요",
          "original_form": "부산은 비가 꽤 와서 아마 안할지도 몰라요",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013801.1.16",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013801.1.17",
          "form": "오늘 유퀴즈는 볼수있을런지",
          "original_form": "오늘 유퀴즈는 볼수있을런지",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013801.1.18",
          "form": "지금 경기수를 줄여야",
          "original_form": "지금 경기수를 줄여야",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013801.1.19",
          "form": "올림픽 선수들 나오거든요",
          "original_form": "올림픽 선수들 나오거든요",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013801.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013801.1.21",
          "form": "지난주 유퀴즈는 봤는대 의외로",
          "original_form": "지난주 유퀴즈는 봤는대 의외로",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013801.1.22",
          "form": "럭비 선수들 이야기가 젤 좋았어요",
          "original_form": "럭비 선수들 이야기가 젤 좋았어요",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013801.1.23",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013801.1.24",
          "form": "럭비는 못봤는데",
          "original_form": "럭비는 못봤는데",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013801.1.25",
          "form": "봤음 감동 받았을거같아요",
          "original_form": "봤음 감동 받았을거같아요",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013801.1.26",
          "form": "은근 잘해요",
          "original_form": "은근 잘해요",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013801.1.27",
          "form": "일본이 럭비 강국인것도 몰랐어요ㅋㅋ",
          "original_form": "일본이 럭비 강국인것도 몰랐어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013801.1.28",
          "form": "아 강국?은 아니고",
          "original_form": "아 강국?은 아니고",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013801.1.29",
          "form": "아시아내에서?",
          "original_form": "아시아내에서?",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013801.1.30",
          "form": "홍콩이 아시아에선 젤 강국인데 거의 귀화 선수래요",
          "original_form": "홍콩이 아시아에선 젤 강국인데 거의 귀화 선수래요",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013801.1.31",
          "form": "일본도 귀화 선수들 많고 럭비 인구도 많고",
          "original_form": "일본도 귀화 선수들 많고 럭비 인구도 많고",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013801.1.32",
          "form": "일본은 자체리그도 꽤",
          "original_form": "일본은 자체리그도 꽤",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013801.1.33",
          "form": "영국 좋아해서",
          "original_form": "영국 좋아해서",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013801.1.34",
          "form": "우리나라는 거의 불모지",
          "original_form": "우리나라는 거의 불모지",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013801.1.35",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013801.1.36",
          "form": "아 영국좋아해서 그런거에요?ㅋㅋ",
          "original_form": "아 영국좋아해서 그런거에요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013801.1.37",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013801.1.38",
          "form": "다 따라하는",
          "original_form": "다 따라하는",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013801.1.39",
          "form": "홍콩은 영국령이었으니까 이해했는데",
          "original_form": "홍콩은 영국령이었으니까 이해했는데",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013801.1.40",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013801.1.41",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013801.1.42",
          "form": "영국 엄청 좋아해요",
          "original_form": "영국 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013801.1.43",
          "form": "우리나라 진짜 신기해요",
          "original_form": "우리나라 진짜 신기해요",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013801.1.44",
          "form": "제국주의시대 영국을 동경하나요ㅋㅋ",
          "original_form": "제국주의시대 영국을 동경하나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013801.1.45",
          "form": "불모지인 스포츠 엄청 많은데 또 하면",
          "original_form": "불모지인 스포츠 엄청 많은데 또 하면",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013801.1.46",
          "form": "천재들이ㅇㅇ",
          "original_form": "천재들이ㅇㅇ",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013801.1.47",
          "form": "일단 여왕있는거",
          "original_form": "일단 여왕있는거",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013801.1.48",
          "form": "일본 만화",
          "original_form": "일본 만화",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013801.1.49",
          "form": "대영제국이라할때 이름도 재수없는데ㅋㅋ",
          "original_form": "대영제국이라할때 이름도 재수없는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013801.1.50",
          "form": "보면 막 천재가 나오는데",
          "original_form": "보면 막 천재가 나오는데",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013801.1.51",
          "form": "그거 한국",
          "original_form": "그거 한국",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013801.1.52",
          "form": "영국 기레기가 갑자기 한국보고 난민 받으라고 ㅡㅡ",
          "original_form": "영국 기레기가 갑자기 한국보고 난민 받으라고 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013801.1.53",
          "form": "아프간 사태 만든건 니네라고ㅡㅡ",
          "original_form": "아프간 사태 만든건 니네라고ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013801.1.54",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013801.1.55",
          "form": "근데 아프간이야기 할 주제는 없네요ㅋㅋ",
          "original_form": "근데 아프간이야기 할 주제는 없네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013801.1.56",
          "form": "아까 드니 빌뇌브감독 이야기 잠깐 했잖아요",
          "original_form": "아까 드니 빌뇌브감독 이야기 잠깐 했잖아요",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013801.1.57",
          "form": "그 감독이 주목받은게 그을린 사랑이란 영화였는데",
          "original_form": "그 감독이 주목받은게 그을린 사랑이란 영화였는데",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013801.1.58",
          "form": "그 영화에 레바논 내전 나오거든요",
          "original_form": "그 영화에 레바논 내전 나오거든요",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013801.1.59",
          "form": "루카스 사과뭄 올렸네요",
          "original_form": "루카스 사과뭄 올렸네요",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
