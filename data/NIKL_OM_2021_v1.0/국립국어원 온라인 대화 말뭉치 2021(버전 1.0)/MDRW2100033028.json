      "utterance": [
        {
          "id": "MDRW2100033028.1.1",
          "form": "오오 이거는 진짜",
          "original_form": "오오 이거는 진짜",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100033028.1.2",
          "form": "흥미로운 주제예요!!!",
          "original_form": "흥미로운 주제예요!!!",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100033028.1.3",
          "form": "폴드 사기보셨어요?",
          "original_form": "폴드 사기보셨어요?",
          "speaker_id": "2",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100033028.1.4",
          "form": "대리점에서 폴드 대여하서",
          "original_form": "대리점에서 폴드 대여하서",
          "speaker_id": "2",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100033028.1.5",
          "form": "중고나라에서 사기쳐서 팔았던가",
          "original_form": "중고나라에서 사기쳐서 팔았던가",
          "speaker_id": "2",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100033028.1.6",
          "form": "그랬던거같은데",
          "original_form": "그랬던거같은데",
          "speaker_id": "2",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100033028.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100033028.1.8",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "1",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100033028.1.9",
          "form": "삼성이 법적대응하겠다했다네요",
          "original_form": "삼성이 법적대응하겠다했다네요",
          "speaker_id": "2",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100033028.1.10",
          "form": "요즘 핫한 아이템이라고 이렇게",
          "original_form": "요즘 핫한 아이템이라고 이렇게",
          "speaker_id": "1",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100033028.1.11",
          "form": "아 그러면 대리점에서",
          "original_form": "아 그러면 대리점에서",
          "speaker_id": "1",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100033028.1.12",
          "form": "그렇게 한거네요...",
          "original_form": "그렇게 한거네요...",
          "speaker_id": "1",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100033028.1.13",
          "form": "그러면 삼성전자 측에서 조치를 취해야겠네요 진짜ㅠ",
          "original_form": "그러면 삼성전자 측에서 조치를 취해야겠네요 진짜ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:10"
        },
        {
          "id": "MDRW2100033028.1.14",
          "form": "자세히는 모르겠는데",
          "original_form": "자세히는 모르겠는데",
          "speaker_id": "2",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.15",
          "form": "흥행하는 물건이라고 다들 그렇게 안좋은 방법으로 이득을 취하려고...!!!",
          "original_form": "흥행하는 물건이라고 다들 그렇게 안좋은 방법으로 이득을 취하려고...!!!",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.17",
          "form": "삼성이 빡쳤다고 들었어요",
          "original_form": "삼성이 빡쳤다고 들었어요",
          "speaker_id": "2",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.19",
          "form": "그럴거같아요",
          "original_form": "그럴거같아요",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.20",
          "form": "일단 금전적인 손실도 그렇긴 한데",
          "original_form": "일단 금전적인 손실도 그렇긴 한데",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.21",
          "form": "삼성 이미지가",
          "original_form": "삼성 이미지가",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.22",
          "form": "안좋아질 수도 있으니...",
          "original_form": "안좋아질 수도 있으니...",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.23",
          "form": "네네 아무래도 이미지가ㅋㅋ",
          "original_form": "네네 아무래도 이미지가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.24",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.25",
          "form": "겨우 인터넷 중고 시장에서",
          "original_form": "겨우 인터넷 중고 시장에서",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100033028.1.26",
          "form": "최신폰 같은거 빨리바꾸시는편이세요?",
          "original_form": "최신폰 같은거 빨리바꾸시는편이세요?",
          "speaker_id": "2",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100033028.1.27",
          "form": "거래되는 물건이라는 이미지는....!!!",
          "original_form": "거래되는 물건이라는 이미지는....!!!",
          "speaker_id": "1",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100033028.1.28",
          "form": "아니요아니요",
          "original_form": "아니요아니요",
          "speaker_id": "1",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100033028.1.29",
          "form": "저는 무슨 물건이든지 한 번 사면 되게 오래 쓰거든요",
          "original_form": "저는 무슨 물건이든지 한 번 사면 되게 오래 쓰거든요",
          "speaker_id": "1",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100033028.1.30",
          "form": "오 지금휴대폰은 어느정도 쓰신거예요",
          "original_form": "오 지금휴대폰은 어느정도 쓰신거예요",
