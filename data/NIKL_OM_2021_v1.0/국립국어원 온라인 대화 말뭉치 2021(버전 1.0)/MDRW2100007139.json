      "utterance": [
        {
          "id": "MDRW2100007139.1.1",
          "form": "게임 좋아하세요? 저는 올해 동물의숲했는데 아기자기하니 귀엽더라구요",
          "original_form": "게임 좋아하세요? 저는 올해 동물의숲했는데 아기자기하니 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007139.1.2",
          "form": "아 저 게임고자라 ㅠ",
          "original_form": "아 저 게임고자라 ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007139.1.3",
          "form": "동숲은 보긴 많이봤어요",
          "original_form": "동숲은 보긴 많이봤어요",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007139.1.4",
          "form": "게임영상은 보시나봐요!",
          "original_form": "게임영상은 보시나봐요!",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007139.1.5",
          "form": "이런..겜 영상은..선바가 한단것밖에",
          "original_form": "이런..겜 영상은..선바가 한단것밖에",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007139.1.6",
          "form": "겜 좋아하시나보당",
          "original_form": "겜 좋아하시나보당",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007139.1.7",
          "form": "요새 뭐 하세여?",
          "original_form": "요새 뭐 하세여?",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007139.1.8",
          "form": "아 선바 보셨구나...",
          "original_form": "아 선바 보셨구나...",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007139.1.9",
          "form": "저는 종합게임방송 좋아하는 편이라 게임방송 자주 봤었어요",
          "original_form": "저는 종합게임방송 좋아하는 편이라 게임방송 자주 봤었어요",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007139.1.10",
          "form": "알기만해요 ㅎ",
          "original_form": "알기만해요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007139.1.11",
          "form": "롤이며 안하는게 없으시겠당",
          "original_form": "롤이며 안하는게 없으시겠당",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007139.1.12",
          "form": "쿠키런킹덤 열심히했었는데 요새는 안해요",
          "original_form": "쿠키런킹덤 열심히했었는데 요새는 안해요",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007139.1.13",
          "form": "모바일 게임은 안하세요?",
          "original_form": "모바일 게임은 안하세요?",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007139.1.14",
          "form": "모바일 하다보면 인생순삭이라 자제중입니다 ㅠ",
          "original_form": "모바일 하다보면 인생순삭이라 자제중입니다 ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007139.1.15",
          "form": "중노동하는 모바일겜하면 괴롭다능",
          "original_form": "중노동하는 모바일겜하면 괴롭다능",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007139.1.16",
          "form": "아 ㅋㅋ 근데 또 그런 게임은 그만의 재미가 있어서 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋ 근데 또 그런 게임은 그만의 재미가 있어서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007139.1.17",
          "form": "어떤게임 하셨어요?",
          "original_form": "어떤게임 하셨어요?",
          "speaker_id": "1",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007139.1.18",
          "form": "놀러와 마이홈..이런거 ㅋ",
          "original_form": "놀러와 마이홈..이런거 ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007139.1.19",
          "form": "유치해서 부끄럽네여 ㅎ",
          "original_form": "유치해서 부끄럽네여 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007139.1.20",
          "form": "모바일게임은 다 그런재미로 하는거죠 ㅋㅋ",
          "original_form": "모바일게임은 다 그런재미로 하는거죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007139.1.21",
          "form": "장작모으기 양털모으기  ..시간도 함께 순삭",
          "original_form": "장작모으기 양털모으기  ..시간도 함께 순삭",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007139.1.22",
          "form": "저도 온라인게임을 즐기는 편이 아니라 그런류 좋아해요",
          "original_form": "저도 온라인게임을 즐기는 편이 아니라 그런류 좋아해요",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007139.1.23",
          "form": "농장게임이나 의미없는 터치 노가다류 ㅋㅋㅋ",
          "original_form": "농장게임이나 의미없는 터치 노가다류 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007139.1.24",
          "form": "창고는 왤케 부족한지",
          "original_form": "창고는 왤케 부족한지",
          "speaker_id": "2",
          "time": "20210822 00:45"
        },
        {
          "id": "MDRW2100007139.1.25",
          "form": "ㅎㅎ 시간떼우기 좋긴해여",
          "original_form": "ㅎㅎ 시간떼우기 좋긴해여",
          "speaker_id": "2",
          "time": "20210822 00:45"
        }
      ]
