      "utterance": [
        {
          "id": "MDRW2100028268.1.1",
          "form": "밥 먹었어?",
          "original_form": "밥 먹었어?",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028268.1.2",
          "form": "아니 지금 일하는 중이라 못먹고 있어ㅠㅠ",
          "original_form": "아니 지금 일하는 중이라 못먹고 있어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100028268.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100028268.1.4",
          "form": "난 돈까스 먹었어",
          "original_form": "난 돈까스 먹었어",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028268.1.5",
          "form": "일 언제끝나 얼른 먹어야지 ㅠㅠ",
          "original_form": "일 언제끝나 얼른 먹어야지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028268.1.6",
          "form": "한시간 남았어!",
          "original_form": "한시간 남았어!",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028268.1.7",
          "form": "빨리 집가서 밥 먹고싶다ㅜㅜㅜ",
          "original_form": "빨리 집가서 밥 먹고싶다ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028268.1.8",
          "form": "돈까스라니 맛있었겠다!",
          "original_form": "돈까스라니 맛있었겠다!",
          "speaker_id": "2",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028268.1.9",
          "form": "사먹었어??",
          "original_form": "사먹었어??",
          "speaker_id": "2",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028268.1.10",
          "form": "응 사와서 집에서 먹었어!!",
          "original_form": "응 사와서 집에서 먹었어!!",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028268.1.11",
          "form": "집 가면 뭐 먹을 거야?",
          "original_form": "집 가면 뭐 먹을 거야?",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028268.1.12",
          "form": "나 된장찌개 먹으려고!",
          "original_form": "나 된장찌개 먹으려고!",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028268.1.13",
          "form": "요즘 찌개류 너무 좋아!",
          "original_form": "요즘 찌개류 너무 좋아!",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028268.1.14",
          "form": "오 된장찌개 맛있지!!! 밥한그릇 뚝딱",
          "original_form": "오 된장찌개 맛있지!!! 밥한그릇 뚝딱",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100028268.1.15",
          "form": "맞아 어제 너무 맛있게 먹었어ㅎㅎㅎ",
          "original_form": "맞아 어제 너무 맛있게 먹었어ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 09:33"
        },
        {
          "id": "MDRW2100028268.1.16",
          "form": "아침 먹었어??",
          "original_form": "아침 먹었어??",
          "speaker_id": "2",
          "time": "20210903 09:33"
        },
        {
          "id": "MDRW2100028268.1.17",
