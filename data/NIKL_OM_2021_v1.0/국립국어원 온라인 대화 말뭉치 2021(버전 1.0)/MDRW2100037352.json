        {
          "id": "MDRW2100037352.1.1",
          "form": "오늘 밖에 비 와",
          "original_form": "오늘 밖에 비 와",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037352.1.2",
          "form": "여기도 비 와",
          "original_form": "여기도 비 와",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037352.1.3",
          "form": "매번 비 와",
          "original_form": "매번 비 와",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037352.1.4",
          "form": "이번에 빈 진짜 많이 온다",
          "original_form": "이번에 빈 진짜 많이 온다",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037352.1.5",
          "form": "진짜 많이 온다",
          "original_form": "진짜 많이 온다",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037352.1.6",
          "form": "비 오는 날에는 햇빛이 없어서 좋아",
          "original_form": "비 오는 날에는 햇빛이 없어서 좋아",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037352.1.7",
          "form": "그렇지만 옷이 젖는 걸",
          "original_form": "그렇지만 옷이 젖는 걸",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037352.1.8",
          "form": "우산을 드는 거 자체가 귀찮아",
          "original_form": "우산을 드는 거 자체가 귀찮아",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037352.1.9",
          "form": "맞아 그리고 물구 덩이가 싫어",
          "original_form": "맞아 그리고 물구 덩이가 싫어",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037352.1.10",
          "form": "신발 젖는거 짜증",
          "original_form": "신발 젖는거 짜증",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037352.1.11",
          "form": "사볼까봐 장화",
          "original_form": "사볼까봐 장화",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037352.1.12",
          "form": "나는 장화 사놓고 한 번도 안 씀",
          "original_form": "나는 장화 사놓고 한 번도 안 씀",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037352.1.13",
          "form": "발냄새 나지 않을까",
          "original_form": "발냄새 나지 않을까",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037352.1.14",
          "form": "키높이 신발은 어때",
          "original_form": "키높이 신발은 어때",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037352.1.15",
          "form": "넘어지면 다칠 거 같아",
          "original_form": "넘어지면 다칠 거 같아",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037352.1.16",
          "form": "비 오는 날에 높은 것을 신으면 은 덜 젖어",
          "original_form": "비 오는 날에 높은 것을 신으면 은 덜 젖어",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100037352.1.17",
          "form": "아 그렇긴 하겠다",
          "original_form": "아 그렇긴 하겠다",
          "speaker_id": "1",
          "time": "20210831 22:50"
        }
      ]
