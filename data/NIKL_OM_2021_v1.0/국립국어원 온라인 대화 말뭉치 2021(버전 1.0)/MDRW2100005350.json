        {
          "id": "MDRW2100005350.1.2",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005350.1.3",
          "form": "결혼은 연애결혼이지ㅋㅋ",
          "original_form": "결혼은 연애결혼이지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005350.1.4",
          "form": "중매노노",
          "original_form": "중매노노",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005350.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ중매도 좋지 않아여?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ중매도 좋지 않아여?",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005350.1.6",
          "form": "에너지 낭비 없고 ㅋㅋ",
          "original_form": "에너지 낭비 없고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005350.1.7",
          "form": "근데..뭐 우린 지금..",
          "original_form": "근데..뭐 우린 지금..",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005350.1.8",
          "form": "이얘기가 다 무슨 필요 ㅋㅋㅋ",
          "original_form": "이얘기가 다 무슨 필요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005350.1.9",
          "form": "자식농사 얘기해야되것는데 ㅋㅋ",
          "original_form": "자식농사 얘기해야되것는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005350.1.10",
          "form": "그렇네ㅋㅋ 무슨연애고ㅋㅋ",
          "original_form": "그렇네ㅋㅋ 무슨연애고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005350.1.11",
          "form": "연예 하도 오래되가 ㅋㅋㅋ",
          "original_form": "연예 하도 오래되가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005350.1.12",
          "form": "기억이 가물가물하네옄ㅋㅋㅋ",
          "original_form": "기억이 가물가물하네옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005350.1.13",
          "form": "영화보면 쫌 좋은데ㅋㅋ",
          "original_form": "영화보면 쫌 좋은데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005350.1.14",
          "form": "언니 이상형은 형부라요?",
          "original_form": "언니 이상형은 형부라요?",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005350.1.15",
          "form": "현실은 가족임ㅋ",
          "original_form": "현실은 가족임ㅋ",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005350.1.16",
          "form": "뭐래ㅋㅋ",
          "original_form": "뭐래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005350.1.17",
          "form": "ㅋㅋㅋㅋ너무나 가족 ㅋㅋㅋ",
