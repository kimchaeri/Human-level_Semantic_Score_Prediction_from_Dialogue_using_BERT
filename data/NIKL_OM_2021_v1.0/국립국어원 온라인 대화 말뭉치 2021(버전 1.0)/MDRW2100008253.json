      "utterance": [
        {
          "id": "MDRW2100008253.1.1",
          "form": "요즘 루핑 잘 안팔더라",
          "original_form": "요즘 루핑 잘 안팔더라",
          "speaker_id": "2",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100008253.1.2",
          "form": "루핑이 뭔데?",
          "original_form": "루핑이 뭔데?",
          "speaker_id": "1",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100008253.1.3",
          "form": "인공눈물",
          "original_form": "인공눈물",
          "speaker_id": "2",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100008253.1.4",
          "form": "아 전에 쓰던거? ㅎㅎ",
          "original_form": "아 전에 쓰던거? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100008253.1.5",
          "form": "ㅇㅇ 닫아놓을 수 있어 쓰기 편했는데 잘 없어",
          "original_form": "ㅇㅇ 닫아놓을 수 있어 쓰기 편했는데 잘 없어",
          "speaker_id": "2",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100008253.1.6",
          "form": "이번에 사온 건 뚜껑 안닫히더만",
          "original_form": "이번에 사온 건 뚜껑 안닫히더만",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008253.1.7",
          "form": "ㅇㅇ 그래서 한번 따면 다 써야해ㅠ 여간 불편한 게 아니야",
          "original_form": "ㅇㅇ 그래서 한번 따면 다 써야해ㅠ 여간 불편한 게 아니야",
          "speaker_id": "2",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008253.1.8",
          "form": "한 번에 넣을 양이 아니던데.. 땄을 때 작은 아들도 넣어줘",
          "original_form": "한 번에 넣을 양이 아니던데.. 땄을 때 작은 아들도 넣어줘",
          "speaker_id": "1",
          "time": "20210822 14:32"
        },
        {
          "id": "MDRW2100008253.1.9",
          "form": "인공눈물 애들한테 좋을 건 없으니까.. ㅋ",
          "original_form": "인공눈물 애들한테 좋을 건 없으니까.. ㅋ",
          "speaker_id": "2",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008253.1.10",
          "form": "그래도 아꿉자나 ㅎㅎ",
          "original_form": "그래도 아꿉자나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008253.1.11",
          "form": "따면 뚜껑 닫아놓을 수 있는 거 있나 알아봐야겠어",
          "original_form": "따면 뚜껑 닫아놓을 수 있는 거 있나 알아봐야겠어",
          "speaker_id": "2",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008253.1.12",
          "form": "그랴요~ 나도 뚜껑 있는게 편하더라",
          "original_form": "그랴요~ 나도 뚜껑 있는게 편하더라",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008253.1.13",
          "form": "한두번은 더 쓸 수 있는데 너무 아까워 ㅎㅎ",
          "original_form": "한두번은 더 쓸 수 있는데 너무 아까워 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008253.1.14",
          "form": "루핑 말고 다른 것도 있을거야",
          "original_form": "루핑 말고 다른 것도 있을거야",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008253.1.15",
          "form": "순금 감사패 하나 만들라고 하는데",
          "original_form": "순금 감사패 하나 만들라고 하는데",
