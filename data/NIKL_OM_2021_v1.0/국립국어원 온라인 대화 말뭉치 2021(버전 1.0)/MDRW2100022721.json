      "utterance": [
        {
          "id": "MDRW2100022721.1.1",
          "form": "걔 이야기 알아?",
          "original_form": "걔 이야기 알아?",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022721.1.2",
          "form": "누구?",
          "original_form": "누구?",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022721.1.3",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022721.1.4",
          "form": "걔도 길에서 떠돌던 강아지",
          "original_form": "걔도 길에서 떠돌던 강아지",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022721.1.5",
          "form": "그 유튜버가 임보하다가",
          "original_form": "그 유튜버가 임보하다가",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022721.1.6",
          "form": "아 영상 챙겨보는건아니라서 잘은모름",
          "original_form": "아 영상 챙겨보는건아니라서 잘은모름",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022721.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022721.1.8",
          "form": "다른집에 입양 보냈거든",
          "original_form": "다른집에 입양 보냈거든",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022721.1.9",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022721.1.10",
          "form": "근데 몇달만에 다시 돌아온거란 말아야",
          "original_form": "근데 몇달만에 다시 돌아온거란 말아야",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022721.1.11",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022721.1.12",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022721.1.13",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022721.1.14",
          "form": "그 내 추측이기는 한데",
          "original_form": "그 내 추측이기는 한데",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022721.1.15",
          "form": "자기 생각만큼 안 똑똑해서 그런것 같음",
          "original_form": "자기 생각만큼 안 똑똑해서 그런것 같음",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022721.1.16",
          "form": "아......",
          "original_form": "아......",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022721.1.17",
          "form": "새로운 훈련 가르쳐주는 영상 보면",
          "original_form": "새로운 훈련 가르쳐주는 영상 보면",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022721.1.18",
          "form": "아놔 ㅡㅡㅡㅡ",
          "original_form": "아놔 ㅡㅡㅡㅡ",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022721.1.19",
          "form": "다른 2마리는 엄청 빨리 익히는데",
          "original_form": "다른 2마리는 엄청 빨리 익히는데",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022721.1.20",
          "form": "화나네",
          "original_form": "화나네",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022721.1.21",
          "form": "그 하이라는 애는",
          "original_form": "그 하이라는 애는",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022721.1.22",
          "form": "엄청 느리거나",
