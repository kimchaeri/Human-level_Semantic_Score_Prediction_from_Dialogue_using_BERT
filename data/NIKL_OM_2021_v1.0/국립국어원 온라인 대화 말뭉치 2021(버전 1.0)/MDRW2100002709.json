      "utterance": [
        {
          "id": "MDRW2100002709.1.1",
          "form": "싸이 부활 보셨나요",
          "original_form": "싸이 부활 보셨나요",
          "speaker_id": "2",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002709.1.2",
          "form": "네 봤어요",
          "original_form": "네 봤어요",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002709.1.3",
          "form": "어떻게 생각하시나요?",
          "original_form": "어떻게 생각하시나요?",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002709.1.4",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002709.1.5",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002709.1.6",
          "form": "추억이 새록새록이네요",
          "original_form": "추억이 새록새록이네요",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002709.1.7",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002709.1.8",
          "form": "근데 과거 흑역사 때문에 싫어하는 사람들도 많더라구요",
          "original_form": "근데 과거 흑역사 때문에 싫어하는 사람들도 많더라구요",
          "speaker_id": "1",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002709.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002709.1.10",
          "form": "아이디 복구는 잘 되나요?",
          "original_form": "아이디 복구는 잘 되나요?",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002709.1.11",
          "form": "저도 잘 모르겠어요",
          "original_form": "저도 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002709.1.12",
          "form": "아직 사용 전이라",
          "original_form": "아직 사용 전이라",
          "speaker_id": "2",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002709.1.13",
          "form": "저도 아직 사용 안해봐서",
          "original_form": "저도 아직 사용 안해봐서",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002709.1.14",
          "form": "궁금하긴 하네요",
          "original_form": "궁금하긴 하네요",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002709.1.15",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002709.1.16",
          "form": "어릴 때 싸이 참 많이 했었는데",
          "original_form": "어릴 때 싸이 참 많이 했었는데",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002709.1.17",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:01"
        },
        {
