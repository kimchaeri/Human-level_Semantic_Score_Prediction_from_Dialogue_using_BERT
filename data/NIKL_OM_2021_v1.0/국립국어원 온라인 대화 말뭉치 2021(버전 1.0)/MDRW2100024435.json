        {
          "id": "MDRW2100024435.1.1",
          "form": "요즘은 동호회 안하니?",
          "original_form": "요즘은 동호회 안하니?",
          "speaker_id": "2",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100024435.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100024435.1.3",
          "form": "코로나라 그런거냐?",
          "original_form": "코로나라 그런거냐?",
          "speaker_id": "2",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100024435.1.4",
          "form": "그런것도 있구요",
          "original_form": "그런것도 있구요",
          "speaker_id": "1",
          "time": "20210831 09:26"
        },
        {
          "id": "MDRW2100024435.1.5",
          "form": "위드 코로나되면 활동도 좀 해야지.. 넌 너무 집순이야",
          "original_form": "위드 코로나되면 활동도 좀 해야지.. 넌 너무 집순이야",
          "speaker_id": "2",
          "time": "20210831 09:27"
        },
        {
          "id": "MDRW2100024435.1.6",
          "form": "전 엄마랑 다르잖아요.. 집이 좋아요",
          "original_form": "전 엄마랑 다르잖아요.. 집이 좋아요",
          "speaker_id": "1",
          "time": "20210831 09:29"
        },
        {
          "id": "MDRW2100024435.1.7",
          "form": "아빠랑 똑같구나",
          "original_form": "아빠랑 똑같구나",
          "speaker_id": "2",
          "time": "20210901 06:58"
        },
        {
          "id": "MDRW2100024435.1.8",
          "form": "그럼요 아빠 딸인데요 ㅎㅎ",
          "original_form": "그럼요 아빠 딸인데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 07:07"
        },
        {
          "id": "MDRW2100024435.1.9",
          "form": "전에 인라인 타지 않았었니?",
          "original_form": "전에 인라인 타지 않았었니?",
          "speaker_id": "2",
          "time": "20210901 07:08"
        },
        {
          "id": "MDRW2100024435.1.10",
          "form": "네 지금은 다 잊었네요",
          "original_form": "네 지금은 다 잊었네요",
          "speaker_id": "1",
          "time": "20210901 07:08"
        },
        {
          "id": "MDRW2100024435.1.11",
          "form": "어쩜.. 다시 하면 금방 생각날거다",
          "original_form": "어쩜.. 다시 하면 금방 생각날거다",
          "speaker_id": "2",
          "time": "20210901 07:09"
        },
        {
          "id": "MDRW2100024435.1.12",
          "form": "다시 타보려구요",
          "original_form": "다시 타보려구요",
          "speaker_id": "1",
          "time": "20210901 07:09"
        },
        {
          "id": "MDRW2100024435.1.13",
          "form": "그래.. 있는거 활용해야지",
          "original_form": "그래.. 있는거 활용해야지",
