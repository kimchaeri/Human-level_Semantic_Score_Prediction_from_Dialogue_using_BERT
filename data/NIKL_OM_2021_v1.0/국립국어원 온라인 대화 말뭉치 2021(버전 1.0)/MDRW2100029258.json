      "utterance": [
        {
          "id": "MDRW2100029258.1.1",
          "form": "주로 배달이나 포장해서 사와요 ㅋㅋ",
          "original_form": "주로 배달이나 포장해서 사와요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029258.1.2",
          "form": "흠 얼마전까진 날씨가 너무 더워서 집에서 해먹기도 싫더라구요",
          "original_form": "흠 얼마전까진 날씨가 너무 더워서 집에서 해먹기도 싫더라구요",
          "speaker_id": "1",
          "time": "20210903 00:46"
        },
        {
          "id": "MDRW2100029258.1.3",
          "form": "진짜 여름엔 해먹기가 힘들어요ㅠ",
          "original_form": "진짜 여름엔 해먹기가 힘들어요ㅠ",
          "speaker_id": "2",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029258.1.4",
          "form": "넵 ㅜㅡ 진짜루요 ㅜㅡ 요즘엔 그나마 날씨가 좋아져서 할만해요",
          "original_form": "넵 ㅜㅡ 진짜루요 ㅜㅡ 요즘엔 그나마 날씨가 좋아져서 할만해요",
          "speaker_id": "1",
          "time": "20210903 00:47"
        },
        {
          "id": "MDRW2100029258.1.5",
          "form": "집에서 음식도 하세요?",
          "original_form": "집에서 음식도 하세요?",
          "speaker_id": "2",
          "time": "20210903 00:49"
        },
        {
          "id": "MDRW2100029258.1.6",
          "form": "거의 안하긴하는데 하려고 노력해볼려구요",
          "original_form": "거의 안하긴하는데 하려고 노력해볼려구요",
          "speaker_id": "1",
          "time": "20210903 00:49"
        },
        {
          "id": "MDRW2100029258.1.7",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210903 00:50"
        },
        {
          "id": "MDRW2100029258.1.8",
          "form": "전 거의 못해요ㅠ",
          "original_form": "전 거의 못해요ㅠ",
          "speaker_id": "2",
          "time": "20210903 00:50"
        },
        {
          "id": "MDRW2100029258.1.9",
          "form": "넵 저두 그다지 ㅎㅎㅎ 간단한거라도 하려고 노력해야죠 ㅎㅎ",
          "original_form": "넵 저두 그다지 ㅎㅎㅎ 간단한거라도 하려고 노력해야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:51"
        },
        {
          "id": "MDRW2100029258.1.10",
          "form": "요새 계란찜이 그렇게 맛있던데",
          "original_form": "요새 계란찜이 그렇게 맛있던데",
          "speaker_id": "2",
          "time": "20210903 00:53"
        },
        {
          "id": "MDRW2100029258.1.11",
          "form": "ㅋㅋㅋ계란이야 언제나 옳죠 ㅎ",
          "original_form": "ㅋㅋㅋ계란이야 언제나 옳죠 ㅎ",
          "speaker_id": "1",
          "time": "20210903 00:54"
        },
        {
          "id": "MDRW2100029258.1.12",
          "form": "근데 계란이 너무 비싸요ㅠㅠ 흑",
          "original_form": "근데 계란이 너무 비싸요ㅠㅠ 흑",
          "speaker_id": "2",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029258.1.13",
          "form": "넵 ㅜㅡ 요즘 너무 비싸서 못산지 6개월은 넘은듯해요 ㅜㅡ",
          "original_form": "넵 ㅜㅡ 요즘 너무 비싸서 못산지 6개월은 넘은듯해요 ㅜㅡ",
          "speaker_id": "1",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100029258.1.14",
          "form": "계란이라도 사야 요리해먹을것 같아요.",
