      "utterance": [
        {
          "id": "MDRW2100029600.1.1",
          "form": "폰으로 즐기니 좋지요",
          "original_form": "폰으로 즐기니 좋지요",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100029600.1.2",
          "form": "길건너 친구들 아시나요",
          "original_form": "길건너 친구들 아시나요",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029600.1.3",
          "form": "그거잼난다고",
          "original_form": "그거잼난다고",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029600.1.4",
          "form": "하더라1ㅜ요",
          "original_form": "하더라1ㅜ요",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029600.1.5",
          "form": "맛집이 많이 있잖아요",
          "original_form": "맛집이 많이 있잖아요",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100029600.1.6",
          "form": "갈치 파는 곳도 있고",
          "original_form": "갈치 파는 곳도 있고",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100029600.1.7",
          "form": "흑돼지 파는 곳도 있고",
          "original_form": "흑돼지 파는 곳도 있고",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100029600.1.8",
          "form": "주제를 보세요",
          "original_form": "주제를 보세요",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100029600.1.9",
          "form": "바뀌었어요",
          "original_form": "바뀌었어요",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100029600.1.10",
          "form": "제주도 여행 책을 읽으니 그렇게 나오더라고요",
          "original_form": "제주도 여행 책을 읽으니 그렇게 나오더라고요",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100029600.1.11",
          "form": "주식하는 마음이라는 책",
          "original_form": "주식하는 마음이라는 책",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100029600.1.12",
          "form": "읽고 있어요",
          "original_form": "읽고 있어요",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100029600.1.13",
          "form": "그거 도움 많이 되나요",
          "original_form": "그거 도움 많이 되나요",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100029600.1.14",
          "form": "교묘하게 주제를 바꾸셨네요",
          "original_form": "교묘하게 주제를 바꾸셨네요",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100029600.1.15",
          "form": "주식 관련 책 여러권 읽으려고 시도해 봤는데",
          "original_form": "주식 관련 책 여러권 읽으려고 시도해 봤는데",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100029600.1.16",
          "form": "기본 용어를 잘 몰라서 진도가 안 나가더라고요",
          "original_form": "기본 용어를 잘 몰라서 진도가 안 나가더라고요",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.17",
          "form": "이거 본 거 있어요",
          "original_form": "이거 본 거 있어요",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.18",
          "form": "읽어",
          "original_form": "읽어",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.19",
          "form": "유튜브로 영어를 좀 배우면서 보려고 시도 중입니다",
          "original_form": "유튜브로 영어를 좀 배우면서 보려고 시도 중입니다",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.20",
          "form": "아주 기본 책을 보세요",
          "original_form": "아주 기본 책을 보세요",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.21",
          "form": "기본 용어부터 보세요",
          "original_form": "기본 용어부터 보세요",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.22",
          "form": "추천해 주실 만한 책이 있나요",
          "original_form": "추천해 주실 만한 책이 있나요",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.23",
          "form": "완전 생초보 책이요",
          "original_form": "완전 생초보 책이요",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.24",
          "form": "월급독립프로젝트",
          "original_form": "월급독립프로젝트",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.25",
          "form": "유목민 저서",
          "original_form": "유목민 저서",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.26",
          "form": "유목민인 건가요",
          "original_form": "유목민인 건가요",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.27",
          "form": "5번 읽었어요",
          "original_form": "5번 읽었어요",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100029600.1.28",
          "form": "저자가 유목민",
          "original_form": "저자가 유목민",
          "speaker_id": "1",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100029600.1.29",
          "form": "그거 조금 읽다가 못 읽었어요 어려워요",
          "original_form": "그거 조금 읽다가 못 읽었어요 어려워요",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100029600.1.30",
          "form": "본명은 몰라요",
          "original_form": "본명은 몰라요",
          "speaker_id": "1",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100029600.1.31",
          "form": "기초 지식이 부족합니다",
          "original_form": "기초 지식이 부족합니다",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100029600.1.32",
          "form": "그 이름으로 강사로 활동하고 있어요",
          "original_form": "그 이름으로 강사로 활동하고 있어요",
          "speaker_id": "1",
          "time": "20210904 00:17"
        }
