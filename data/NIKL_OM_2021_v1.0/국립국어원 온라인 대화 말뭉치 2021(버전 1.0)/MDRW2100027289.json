        },
        {
          "id": "MDRW2100027289.1.2",
          "form": "햇빛이 강해사오",
          "original_form": "햇빛이 강해사오",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027289.1.3",
          "form": "오늘은 해가 났네요",
          "original_form": "오늘은 해가 났네요",
          "speaker_id": "2",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027289.1.4",
          "form": "더워요",
          "original_form": "더워요",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027289.1.5",
          "form": "회사오면 귀찮아서 거의 안나가서요ㅋ",
          "original_form": "회사오면 귀찮아서 거의 안나가서요ㅋ",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027289.1.6",
          "form": "밖에 더워서",
          "original_form": "밖에 더워서",
          "speaker_id": "1",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027289.1.7",
          "form": "자주 왔다갔다 하시나봐요",
