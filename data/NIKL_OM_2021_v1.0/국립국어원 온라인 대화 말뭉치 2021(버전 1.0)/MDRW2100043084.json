      "utterance": [
        {
          "id": "MDRW2100043084.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100043084.1.2",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100043084.1.3",
          "form": "네! 식사했습니다. 식사하셨죠???",
          "original_form": "네! 식사했습니다. 식사하셨죠???",
          "speaker_id": "2",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043084.1.4",
          "form": "지금은 식후커피 마시고 있습니다~",
          "original_form": "지금은 식후커피 마시고 있습니다~",
          "speaker_id": "2",
          "time": "20210907 14:07"
        },
        {
          "id": "MDRW2100043084.1.5",
          "form": "네 식사했어요 뭐드셨나요?",
          "original_form": "네 식사했어요 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043084.1.6",
          "form": "회사 구내식당 이용했는데 맛이 없었어요!!!",
          "original_form": "회사 구내식당 이용했는데 맛이 없었어요!!!",
          "speaker_id": "2",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100043084.1.7",
          "form": "앗ㅠㅠ메뉴가 뭐였나요?",
          "original_form": "앗ㅠㅠ메뉴가 뭐였나요?",
          "speaker_id": "1",
          "time": "20210907 14:09"
        },
        {
          "id": "MDRW2100043084.1.8",
          "form": "볶음우동이랑 생선까스가 나왔는데 뭔가 조화롭지 못했어요!!",
          "original_form": "볶음우동이랑 생선까스가 나왔는데 뭔가 조화롭지 못했어요!!",
          "speaker_id": "2",
          "time": "20210907 14:17"
        },
        {
          "id": "MDRW2100043084.1.9",
          "form": "어떤 메뉴 드셨나요???",
          "original_form": "어떤 메뉴 드셨나요???",
          "speaker_id": "2",
          "time": "20210907 14:17"
        },
        {
          "id": "MDRW2100043084.1.10",
          "form": "아쉽네요ㅠㅠ",
          "original_form": "아쉽네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:32"
        },
        {
          "id": "MDRW2100043084.1.11",
          "form": "저는 오므라이스 먹었어요",
          "original_form": "저는 오므라이스 먹었어요",
          "speaker_id": "1",
          "time": "20210907 14:32"
        },
        {
          "id": "MDRW2100043084.1.12",
          "form": "구내식당 평소에는 맛있게 드시나요?",
          "original_form": "구내식당 평소에는 맛있게 드시나요?",
          "speaker_id": "1",
          "time": "20210907 14:32"
        },
        {
          "id": "MDRW2100043084.1.13",
          "form": "와 오므라이스 맛있겠네요ㅠㅠ 좋아하는 음식이에요!!!",
          "original_form": "와 오므라이스 맛있겠네요ㅠㅠ 좋아하는 음식이에요!!!",
          "speaker_id": "2",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043084.1.14",
          "form": "음 아니요. 저희 구내식당은 퀄리티가 별로라서 그냥 포기하고 먹어요. 가끔 외식도 하고요!!!",
          "original_form": "음 아니요. 저희 구내식당은 퀄리티가 별로라서 그냥 포기하고 먹어요. 가끔 외식도 하고요!!!",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100043084.1.15",
          "form": "오므라이스 좋아하시는구나! 저는 집에서 자주해먹어요 간단해서",
          "original_form": "오므라이스 좋아하시는구나! 저는 집에서 자주해먹어요 간단해서",
          "speaker_id": "1",
          "time": "20210907 15:51"
        },
        {
          "id": "MDRW2100043084.1.16",
          "form": "와 대단해요!!!",
          "original_form": "와 대단해요!!!",
          "speaker_id": "2",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043084.1.17",
          "form": "전 요리 거의 안 해서 잘하는지 못하는지도 모르고, 잘하시는 분들 보면 신기해요",
          "original_form": "전 요리 거의 안 해서 잘하는지 못하는지도 모르고, 잘하시는 분들 보면 신기해요",
          "speaker_id": "2",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043084.1.18",
          "form": "별거없어서 민망하네요ㅎㅎ",
          "original_form": "별거없어서 민망하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 16:16"
        },
        {
          "id": "MDRW2100043084.1.19",
          "form": "저는 자취요리정도 생존용으로 해서 하던거 자주해먹어요",
          "original_form": "저는 자취요리정도 생존용으로 해서 하던거 자주해먹어요",
          "speaker_id": "1",
          "time": "20210907 16:17"
        },
        {
          "id": "MDRW2100043084.1.20",
          "form": "그럼 좋아하는 배달음식은 무엇인가요???!!",
          "original_form": "그럼 좋아하는 배달음식은 무엇인가요???!!",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100043084.1.21",
          "form": "치킨 다들 좋아하시던데 전 치킨보단 피자요",
          "original_form": "치킨 다들 좋아하시던데 전 치킨보단 피자요",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043084.1.22",
          "form": "저는 피자보단 치킨이구요 좋아하는건",
          "original_form": "저는 피자보단 치킨이구요 좋아하는건",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043084.1.23",
          "form": "보쌈이나 아구찜이요!",
          "original_form": "보쌈이나 아구찜이요!",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100043084.1.24",
          "form": "오 보쌈 맛있죠!!!",
          "original_form": "오 보쌈 맛있죠!!!",
          "speaker_id": "2",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100043084.1.25",
          "form": "피자 어디꺼 좋아하시나요?",
          "original_form": "피자 어디꺼 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100043084.1.26",
          "form": "오 아구찜... 그럼 매운 거 잘 드시나요???",
          "original_form": "오 아구찜... 그럼 매운 거 잘 드시나요???",
          "speaker_id": "2",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100043084.1.27",
          "form": "피자스쿨이 가성비가 좋더라고요 ㅋㅋㅋ",
          "original_form": "피자스쿨이 가성비가 좋더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100043084.1.28",
          "form": "매운거 좋아해요!",
          "original_form": "매운거 좋아해요!",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100043084.1.29",
          "form": "도미노도 좋아요!!",
          "original_form": "도미노도 좋아요!!",
          "speaker_id": "2",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100043084.1.30",
          "form": "피자스쿨좋죠!",
          "original_form": "피자스쿨좋죠!",
          "speaker_id": "1",
          "time": "20210907 16:49"
        }
      ]
    }
