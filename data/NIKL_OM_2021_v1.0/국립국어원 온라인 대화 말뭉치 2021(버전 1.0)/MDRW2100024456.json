        },
        {
          "id": "MDRW2100024456.1.2",
          "form": "싸이 하셨어요?",
          "original_form": "싸이 하셨어요?",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100024456.1.3",
          "form": "싸이세대신가",
          "original_form": "싸이세대신가",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024456.1.4",
          "form": "당연하죠 고1까진했음",
          "original_form": "당연하죠 고1까진했음",
          "speaker_id": "1",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024456.1.5",
          "form": "도토리 엄청썻는데 ㅠㅠ문상사가지고",
          "original_form": "도토리 엄청썻는데 ㅠㅠ문상사가지고",
          "speaker_id": "1",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024456.1.6",
          "form": "근데 아이디랑 비밀번호 까먹어서 들어가지도못함ㅋㅋㅋㅋ",
          "original_form": "근데 아이디랑 비밀번호 까먹어서 들어가지도못함ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024456.1.7",
          "form": "찾으니까 나오지도않고 속상",
          "original_form": "찾으니까 나오지도않고 속상",
          "speaker_id": "1",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024456.1.8",
          "form": "헐ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:29"
        },
        {
          "id": "MDRW2100024456.1.9",
          "form": "다람쥐셨구나",
          "original_form": "다람쥐셨구나",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024456.1.10",
          "form": "전 노래 딱 하나 사봄",
          "original_form": "전 노래 딱 하나 사봄",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024456.1.11",
          "form": "나머진 선물받고ㅋㅋ",
          "original_form": "나머진 선물받고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024456.1.12",
          "form": "아 미니미는 살짝 꾸몄나보다",
          "original_form": "아 미니미는 살짝 꾸몄나보다",
          "speaker_id": "2",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024456.1.13",
          "form": "인싸",
          "original_form": "인싸",
          "speaker_id": "1",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024456.1.14",
          "form": "네요",
          "original_form": "네요",
          "speaker_id": "1",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024456.1.15",
          "form": "저도전에 선물좀 많이받았었는데",
          "original_form": "저도전에 선물좀 많이받았었는데",
          "speaker_id": "1",
          "time": "20210831 10:30"
        },
        {
          "id": "MDRW2100024456.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024456.1.17",
          "form": "아 들어가보고싶다 ㅠㅠ",
          "original_form": "아 들어가보고싶다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024456.1.18",
          "form": "많이요? 이거바이거바",
          "original_form": "많이요? 이거바이거바",
          "speaker_id": "2",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024456.1.19",
          "form": "인싸가 아싸 코스프레를",
          "original_form": "인싸가 아싸 코스프레를",
          "speaker_id": "2",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024456.1.20",
          "form": "전 찾았다가 탈퇴함ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 찾았다가 탈퇴함ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024456.1.21",
          "form": "왜요 ㅋㅋㅋㅋ흑역사라서?",
          "original_form": "왜요 ㅋㅋㅋㅋ흑역사라서?",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024456.1.22",
          "form": "민망해서ㅋㅋㅋㅋ",
          "original_form": "민망해서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024456.1.23",
          "form": "아 완전재밌겠는데",
          "original_form": "아 완전재밌겠는데",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024456.1.24",
          "form": "ㅋㅋㅋㅋ잘 생각해보세요",
          "original_form": "ㅋㅋㅋㅋ잘 생각해보세요",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024456.1.25",
          "form": "아이디랑 비번 근데 대부분 공용으로 하지 않아요?",
          "original_form": "아이디랑 비번 근데 대부분 공용으로 하지 않아요?",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024456.1.26",
          "form": "전가서 빨리 보고싶어요 ㅋㅋㅋㅋ 못들어가는사람 여기ㅠ",
          "original_form": "전가서 빨리 보고싶어요 ㅋㅋㅋㅋ 못들어가는사람 여기ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024456.1.27",
          "form": "따로 만드시나",
          "original_form": "따로 만드시나",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024456.1.28",
          "form": "저그때 부모님껄로 가입햇나봐요",
          "original_form": "저그때 부모님껄로 가입햇나봐요",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024456.1.29",
          "form": "찾아도 안나오는거보니",
          "original_form": "찾아도 안나오는거보니",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024456.1.30",
          "form": "부모님 해킹ㄱㄱ",
          "original_form": "부모님 해킹ㄱㄱ",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.31",
          "form": "기억도안남 ㅠ",
          "original_form": "기억도안남 ㅠ",
          "speaker_id": "1",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.32",
          "form": "아빠껄로 한거같은데",
          "original_form": "아빠껄로 한거같은데",
          "speaker_id": "1",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.33",
          "form": "아빠폰 해킹ㄱㄱ",
          "original_form": "아빠폰 해킹ㄱㄱ",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.34",
          "form": "아빠가 외국에살아서",
          "original_form": "아빠가 외국에살아서",
          "speaker_id": "1",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.35",
          "form": "불가",
          "original_form": "불가",
          "speaker_id": "1",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.36",
          "form": "죄송합니다",
          "original_form": "죄송합니다",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.37",
          "form": "햇으면 이미햇죠 ㅋㅋㅋ",
          "original_form": "햇으면 이미햇죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.38",
          "form": "왜욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "왜욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.39",
          "form": "코로나때매 입국못하고있음",
          "original_form": "코로나때매 입국못하고있음",
          "speaker_id": "1",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.40",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.41",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100024456.1.42",
          "form": "사업하세요? 저번에 들었던 그건가",
          "original_form": "사업하세요? 저번에 들었던 그건가",
          "speaker_id": "2",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024456.1.43",
          "form": "오 기억력~ 맞음",
          "original_form": "오 기억력~ 맞음",
          "speaker_id": "1",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024456.1.44",
          "form": "이민은 포기하시고 사업하시는구나 대단하시네요",
          "original_form": "이민은 포기하시고 사업하시는구나 대단하시네요",
          "speaker_id": "2",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024456.1.45",
          "form": "전 불가능할듯",
          "original_form": "전 불가능할듯",
          "speaker_id": "2",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024456.1.46",
          "form": "언어안통해서 힘들어함",
          "original_form": "언어안통해서 힘들어함",
          "speaker_id": "1",
          "time": "20210831 10:35"
        },
        {
          "id": "MDRW2100024456.1.47",
          "form": "못 살아도 가족끼리 뭉쳐서 살듯",
          "original_form": "못 살아도 가족끼리 뭉쳐서 살듯",
          "speaker_id": "2",
          "time": "20210831 10:35"
        },
        {
          "id": "MDRW2100024456.1.48",
          "form": "ㅈㅊ",
          "original_form": "ㅈㅊ",
          "speaker_id": "1",
          "time": "20210831 10:35"
        },
        {
          "id": "MDRW2100024456.1.49",
          "form": "에고...",
          "original_form": "에고...",
          "speaker_id": "2",
          "time": "20210831 10:35"
        },
        {
          "id": "MDRW2100024456.1.50",
