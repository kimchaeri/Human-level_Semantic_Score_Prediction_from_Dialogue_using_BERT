      "utterance": [
        {
          "id": "MDRW2100034965.1.1",
          "form": "좀 맛있는 트럭집 있으면 전 맨날 기다리겠는걸요 ㅋㅋ",
          "original_form": "좀 맛있는 트럭집 있으면 전 맨날 기다리겠는걸요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100034965.1.2",
          "form": "근데 오는 날짜가 좀",
          "original_form": "근데 오는 날짜가 좀",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100034965.1.3",
          "form": "사장님 맘대로예요 ㅋㅋㅋㅋ",
          "original_form": "사장님 맘대로예요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100034965.1.4",
          "form": "그냥 안정해놓고 오시나봐요 ㅋㅋㅋ !!",
          "original_form": "그냥 안정해놓고 오시나봐요 ㅋㅋㅋ !!",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100034965.1.5",
          "form": "장같은거 열리면 보통 뭐 구매하세요?",
          "original_form": "장같은거 열리면 보통 뭐 구매하세요?",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100034965.1.6",
          "form": "보통 과일이랑",
          "original_form": "보통 과일이랑",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100034965.1.7",
          "form": "만두같은 간식거리 사먹어요 ㅋㅋㅋ",
          "original_form": "만두같은 간식거리 사먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100034965.1.8",
          "form": "우와 뭔가 신기하네요 ㅎㅎ 여름에 수박이나 복숭아도 많이 파나요?",
          "original_form": "우와 뭔가 신기하네요 ㅎㅎ 여름에 수박이나 복숭아도 많이 파나요?",
          "speaker_id": "1",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100034965.1.9",
          "form": "네 계절과일들 많이 팔더라구요",
          "original_form": "네 계절과일들 많이 팔더라구요",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100034965.1.10",
          "form": "어떤 과일 가장 좋아하시나요?",
          "original_form": "어떤 과일 가장 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100034965.1.11",
          "form": "전 딸기 복숭아 좋아해요 ㅎㅎ",
          "original_form": "전 딸기 복숭아 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100034965.1.12",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100034965.1.13",
          "form": "딸기 진짜 맛있죠",
          "original_form": "딸기 진짜 맛있죠",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100034965.1.14",
          "form": "맞죠 ㅜㅜ 근데 딸기는 너무 짧게 나와서 아쉬워요",
          "original_form": "맞죠 ㅜㅜ 근데 딸기는 너무 짧게 나와서 아쉬워요",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100034965.1.15",
          "form": "비싸기도 하고 ㅎㅎ",
          "original_form": "비싸기도 하고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100034965.1.16",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100034965.1.17",
          "form": "다른 과일들은 가격이 많이 내렸는데",
          "original_form": "다른 과일들은 가격이 많이 내렸는데",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100034965.1.18",
          "form": "유독 딸기는 가격이 꾸준하더라구요",
          "original_form": "유독 딸기는 가격이 꾸준하더라구요",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100034965.1.19",
          "form": "맞아요 ㅠㅠ 비닐하우스라 그런걸까요?",
          "original_form": "맞아요 ㅠㅠ 비닐하우스라 그런걸까요?",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100034965.1.20",
          "form": "딸기는 맛없는것도 비싸요 ㅋㅋㅋ",
          "original_form": "딸기는 맛없는것도 비싸요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100034965.1.21",
          "form": "맞아요 가격도 가격인데 맛이 너무 복불복이라 ㅋㅋ",
          "original_form": "맞아요 가격도 가격인데 맛이 너무 복불복이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100034965.1.22",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100034965.1.23",
          "form": "혹시 딸기청도 해드시나요??",
          "original_form": "혹시 딸기청도 해드시나요??",
          "speaker_id": "1",
