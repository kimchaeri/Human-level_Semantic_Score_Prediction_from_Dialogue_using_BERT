      "utterance": [
        {
          "id": "MDRW2100007533.1.1",
          "form": "연애는 잘하고 계신가요",
          "original_form": "연애는 잘하고 계신가요",
          "speaker_id": "1",
          "time": "20210822 02:59"
        },
        {
          "id": "MDRW2100007533.1.2",
          "form": "아뇨...ㅎ 모솔이에요",
          "original_form": "아뇨...ㅎ 모솔이에요",
          "speaker_id": "2",
          "time": "20210822 03:01"
        },
        {
          "id": "MDRW2100007533.1.3",
          "form": "좋은 인연이 있으실거 같은데ㅎㅎ",
          "original_form": "좋은 인연이 있으실거 같은데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 03:04"
        },
        {
          "id": "MDRW2100007533.1.4",
          "form": "언젠간 인연이 찾아오겠지...싶어서 체념하고 있어요",
          "original_form": "언젠간 인연이 찾아오겠지...싶어서 체념하고 있어요",
          "speaker_id": "2",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007533.1.5",
          "form": "연애하고계세요?",
          "original_form": "연애하고계세요?",
          "speaker_id": "2",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007533.1.6",
          "form": "아니요!!!ㅎㅎㅎ",
          "original_form": "아니요!!!ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007533.1.7",
          "form": "좋은 분 같은데",
          "original_form": "좋은 분 같은데",
          "speaker_id": "1",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007533.1.8",
          "form": "때되면 좋은 분 만나실겁니다",
          "original_form": "때되면 좋은 분 만나실겁니다",
          "speaker_id": "1",
          "time": "20210822 03:07"
        },
        {
          "id": "MDRW2100007533.1.9",
          "form": "안녕하산 님도 좋은 분 만나실거예요!",
          "original_form": "안녕하산 님도 좋은 분 만나실거예요!",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007533.1.10",
          "form": "이상형 있으세요?",
          "original_form": "이상형 있으세요?",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007533.1.11",
          "form": "귀여운 여자요",
          "original_form": "귀여운 여자요",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007533.1.12",
          "form": "해물님은요",
          "original_form": "해물님은요",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007533.1.13",
          "form": "전...잘생기고 착한 사람이요...",
          "original_form": "전...잘생기고 착한 사람이요...",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007533.1.14",
          "form": "사실 저를 좋아해주는 사람이 제일 좋아요",
          "original_form": "사실 저를 좋아해주는 사람이 제일 좋아요",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007533.1.15",
          "form": "잘 생긴게 중요하군요ㅎㅎ",
          "original_form": "잘 생긴게 중요하군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007533.1.16",
          "form": "얼굴은... 중요하죠",
          "original_form": "얼굴은... 중요하죠",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007533.1.17",
          "form": "좋아해주는 사람 vs 좋아하는사람?",
          "original_form": "좋아해주는 사람 vs 좋아하는사람?",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007533.1.18",
          "form": "좋아해주는 사람 쪽이 좋아요",
          "original_form": "좋아해주는 사람 쪽이 좋아요",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007533.1.19",
          "form": "안녕하산 님은요?",
          "original_form": "안녕하산 님은요?",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007533.1.20",
          "form": "저는 좋아하는!",
          "original_form": "저는 좋아하는!",
          "speaker_id": "1",
          "time": "20210822 03:21"
        },
        {
          "id": "MDRW2100007533.1.21",
          "form": "확고하시네요",
          "original_form": "확고하시네요",
          "speaker_id": "2",
          "time": "20210822 03:24"
        },
        {
          "id": "MDRW2100007533.1.22",
          "form": "ㅋㄱ 남녀가 그런게 다르대요",
          "original_form": "ㅋㄱ 남녀가 그런게 다르대요",
          "speaker_id": "1",
          "time": "20210822 03:26"
        },
        {
          "id": "MDRW2100007533.1.23",
          "form": "ㅋㅋㅋ ㅠ 아무래도 그런 것 같네요",
          "original_form": "ㅋㅋㅋ ㅠ 아무래도 그런 것 같네요",
          "speaker_id": "2",
