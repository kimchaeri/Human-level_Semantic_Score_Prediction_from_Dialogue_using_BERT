      "utterance": [
        {
          "id": "MDRW2100037720.1.1",
          "form": "배달 어플",
          "original_form": "배달 어플",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100037720.1.2",
          "form": "어디 많이 이용하세요?",
          "original_form": "어디 많이 이용하세요?",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100037720.1.3",
          "form": "배민이요",
          "original_form": "배민이요",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037720.1.4",
          "form": "지자체 앱도 깔려있긴 해요",
          "original_form": "지자체 앱도 깔려있긴 해요",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037720.1.5",
          "form": "전 배민 쓰다가",
          "original_form": "전 배민 쓰다가",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037720.1.6",
          "form": "요기요로 넘어갔어요 !",
          "original_form": "요기요로 넘어갔어요 !",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037720.1.7",
          "form": "어때요? 다른점 있나요?",
          "original_form": "어때요? 다른점 있나요?",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037720.1.8",
          "form": "만족이에요",
          "original_form": "만족이에요",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037720.1.9",
          "form": "배민은 쿠폰이 별로 없어서",
          "original_form": "배민은 쿠폰이 별로 없어서",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037720.1.10",
          "form": "아쉽거등요",
          "original_form": "아쉽거등요",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037720.1.11",
          "form": "맞아요. 광고는 많은데ㅋㅋㅋ",
          "original_form": "맞아요. 광고는 많은데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037720.1.12",
          "form": "정말요..",
          "original_form": "정말요..",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037720.1.13",
          "form": "요기요는 슈퍼할인 그거 가입하면",
          "original_form": "요기요는 슈퍼할인 그거 가입하면",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037720.1.14",
          "form": "좋더라두요!!!",
          "original_form": "좋더라두요!!!",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037720.1.15",
          "form": "오... 한번 사용해봐야겠어요ㅋㅋㅋ",
          "original_form": "오... 한번 사용해봐야겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100037720.1.16",
          "form": "지자체 배달앱은 가입이 많이 안 되있더라구요",
          "original_form": "지자체 배달앱은 가입이 많이 안 되있더라구요",
          "speaker_id": "2",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100037720.1.17",
          "form": "네 저희도 없어요",
          "original_form": "네 저희도 없어요",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100037720.1.18",
          "form": "만들고 있다고 해요ㅋㅋ",
          "original_form": "만들고 있다고 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100037720.1.19",
          "form": "서울은 쓰기 좋다던데... 지방이라ㅠㅠ",
          "original_form": "서울은 쓰기 좋다던데... 지방이라ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100037720.1.20",
          "form": "맛집도 서울이 많더라구요",
          "original_form": "맛집도 서울이 많더라구요",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100037720.1.21",
          "form": "그렇죠 ㅠ",
          "original_form": "그렇죠 ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037720.1.22",
          "form": "서울엥 많을거에요",
          "original_form": "서울엥 많을거에요",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100037720.1.23",
          "form": "맛집 돌아다니고 싶네요",
          "original_form": "맛집 돌아다니고 싶네요",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037720.1.24",
          "form": "그런거 좋아하시나요",
          "original_form": "그런거 좋아하시나요",
          "speaker_id": "2",
          "time": "20210901 17:35"
        },
        {
          "id": "MDRW2100037720.1.25",
          "form": "ㅠㅠ 네 좋죠",
          "original_form": "ㅠㅠ 네 좋죠",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100037720.1.26",
          "form": "먹는건 뭐든...",
          "original_form": "먹는건 뭐든...",
          "speaker_id": "1",
          "time": "20210901 17:38"
        }
      ]
