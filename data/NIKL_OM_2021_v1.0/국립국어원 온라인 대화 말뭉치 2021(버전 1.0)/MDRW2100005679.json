      "utterance": [
        {
          "id": "MDRW2100005679.1.1",
          "form": "영화 어떤 장르좋아해요?",
          "original_form": "영화 어떤 장르좋아해요?",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005679.1.2",
          "form": "전 범죄스릴러요",
          "original_form": "전 범죄스릴러요",
          "speaker_id": "1",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005679.1.3",
          "form": "골고루 다보긴하는거같아요",
          "original_form": "골고루 다보긴하는거같아요",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005679.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005679.1.5",
          "form": "전 멜로나 액션이요ㅎㅎ",
          "original_form": "전 멜로나 액션이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005679.1.6",
          "form": "어떤영화 기억나요? 스릴러영화중에?",
          "original_form": "어떤영화 기억나요? 스릴러영화중에?",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005679.1.7",
          "form": "추격자 쏘우 이런거 좋아하거든요ㅎ",
          "original_form": "추격자 쏘우 이런거 좋아하거든요ㅎ",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005679.1.8",
          "form": "잔인한것도 잘보고",
          "original_form": "잔인한것도 잘보고",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005679.1.9",
          "form": "ㅎㅎ추격자 좋죠ㅎ 전 잘못보겟어요ㅎ",
          "original_form": "ㅎㅎ추격자 좋죠ㅎ 전 잘못보겟어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005679.1.10",
          "form": "왜냐하면 보통 피해자는 여자라..",
          "original_form": "왜냐하면 보통 피해자는 여자라..",
          "speaker_id": "2",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100005679.1.11",
          "form": "그게 좀 불편해요ㅠ 예전에도 그랬고 지금도 그랬지만 왜맨날 여자만 피해를 보는건지",
          "original_form": "그게 좀 불편해요ㅠ 예전에도 그랬고 지금도 그랬지만 왜맨날 여자만 피해를 보는건지",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005679.1.12",
          "form": "그게 공포감이 잘 표현이되서 그런거겟죠",
          "original_form": "그게 공포감이 잘 표현이되서 그런거겟죠",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100005679.1.13",
          "form": "아무래도 힘이 없으니 현실도 그렇고ㅠㅠ",
          "original_form": "아무래도 힘이 없으니 현실도 그렇고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005679.1.14",
          "form": "영화라 그냥 보는거죠 보고나면 기분이 썩 좋진않지만",
          "original_form": "영화라 그냥 보는거죠 보고나면 기분이 썩 좋진않지만",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100005679.1.15",
          "form": "여자가 세상에 중심인데 자꾸 그렇게 표현하니ㅍ",
          "original_form": "여자가 세상에 중심인데 자꾸 그렇게 표현하니ㅍ",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005679.1.16",
          "form": "그래서 밝은영화만봐요ㅎㅎ",
          "original_form": "그래서 밝은영화만봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005679.1.17",
          "form": "영화중에 내머리속에 지우개보셨어요?",
          "original_form": "영화중에 내머리속에 지우개보셨어요?",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005679.1.18",
          "form": "그냥 티비로 해줄때 보긴했는데",
          "original_form": "그냥 티비로 해줄때 보긴했는데",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005679.1.19",
          "form": "정확히 내용이 기억이 안나네요",
          "original_form": "정확히 내용이 기억이 안나네요",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005679.1.20",
          "form": "유쾌한영화도 재밌고 좋죠",
          "original_form": "유쾌한영화도 재밌고 좋죠",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005679.1.21",
          "form": "손여진이 결혼하고 치매가 걸린내용이에요ㅎㅎ",
          "original_form": "손여진이 결혼하고 치매가 걸린내용이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005679.1.22",
          "form": "손예진ㅎ",
          "original_form": "손예진ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005679.1.23",
          "form": "코믹영화도 좋아해요ㅎㅎ",
          "original_form": "코믹영화도 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005679.1.24",
          "form": "요즘 영화가 몇개안해서",
          "original_form": "요즘 영화가 몇개안해서",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005679.1.25",
          "form": "싱크홀? 그건 킬링타임용으로 괜찮대요",
          "original_form": "싱크홀? 그건 킬링타임용으로 괜찮대요",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005679.1.26",
          "form": "아 그래요ㅎㅎ 봐야겟네요ㅎㅎ",
          "original_form": "아 그래요ㅎㅎ 봐야겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005679.1.27",
          "form": "그거 영화관에서 봐야하죠?",
          "original_form": "그거 영화관에서 봐야하죠?",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005679.1.28",
          "form": "코로나라 영화관 가기가 좀그래요ㅠ",
          "original_form": "코로나라 영화관 가기가 좀그래요ㅠ",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100005679.1.29",
          "form": "제값주고 보긴 아까울수도 ㅎ 저도 아직 못봐서",
          "original_form": "제값주고 보긴 아까울수도 ㅎ 저도 아직 못봐서",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005679.1.30",
          "form": "요즘은 iptv로 빨리 나올껄요ㅎ",
          "original_form": "요즘은 iptv로 빨리 나올껄요ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005679.1.31",
          "form": "그걸로 보시는게 나을수도 ㅎ",
          "original_form": "그걸로 보시는게 나을수도 ㅎ",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100005679.1.32",
          "form": "코로나라 영화관이 잘안될것같은데ㅎ 최근에 가보셨어요?",
          "original_form": "코로나라 영화관이 잘안될것같은데ㅎ 최근에 가보셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100005679.1.33",
          "form": "모가디슈는 극장서 보는걸 추천",
          "original_form": "모가디슈는 극장서 보는걸 추천",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100005679.1.34",
