      "utterance": [
        {
          "id": "MDRW2100040766.1.1",
          "form": "헐 저근데 사실",
          "original_form": "헐 저근데 사실",
          "speaker_id": "2",
          "time": "20210905 04:05"
        },
        {
          "id": "MDRW2100040766.1.2",
          "form": "저희언니랑 같이살았을때가있었는데",
          "original_form": "저희언니랑 같이살았을때가있었는데",
          "speaker_id": "2",
          "time": "20210905 04:05"
        },
        {
          "id": "MDRW2100040766.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 04:05"
        },
        {
          "id": "MDRW2100040766.1.4",
          "form": "뉴욕에서 잠시같이살았거든요 친언니랑",
          "original_form": "뉴욕에서 잠시같이살았거든요 친언니랑",
          "speaker_id": "2",
          "time": "20210905 04:05"
        },
        {
          "id": "MDRW2100040766.1.5",
          "form": "근데 언니가 엄청 아팠던기억이...",
          "original_form": "근데 언니가 엄청 아팠던기억이...",
          "speaker_id": "2",
          "time": "20210905 04:05"
        },
        {
          "id": "MDRW2100040766.1.6",
          "form": "딱 한국오기한달전부터?",
          "original_form": "딱 한국오기한달전부터?",
          "speaker_id": "2",
          "time": "20210905 04:05"
        },
        {
          "id": "MDRW2100040766.1.7",
          "form": "헉....?!",
          "original_form": "헉....?!",
          "speaker_id": "1",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.8",
          "form": "감기심하게걸려서제가 막",
          "original_form": "감기심하게걸려서제가 막",
          "speaker_id": "2",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.9",
          "form": "언니분 지금은 괜찮으세요?",
          "original_form": "언니분 지금은 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.10",
          "form": "죽도주고",
          "original_form": "죽도주고",
          "speaker_id": "2",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.11",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.12",
          "form": "건강맨이에요",
          "original_form": "건강맨이에요",
          "speaker_id": "2",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.13",
          "form": "언니랑 같이 미국에 가신거구나 ㅇㅇㅇ",
          "original_form": "언니랑 같이 미국에 가신거구나 ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.14",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.15",
          "form": "헉?!",
          "original_form": "헉?!",
          "speaker_id": "1",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.16",
          "form": "막챙겨주고했었는데",
          "original_form": "막챙겨주고했었는데",
          "speaker_id": "2",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.17",
          "form": "자연...치유?!",
          "original_form": "자연...치유?!",
          "speaker_id": "1",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.18",
          "form": "언니가 감기증세사라지고나서도",
          "original_form": "언니가 감기증세사라지고나서도",
          "speaker_id": "2",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.19",
          "form": "근데 미국은 진짜 마스크 안쓰고 다니고 그랬나요?",
          "original_form": "근데 미국은 진짜 마스크 안쓰고 다니고 그랬나요?",
          "speaker_id": "1",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.20",
          "form": "언니가 개코에 미식가거든요",
          "original_form": "언니가 개코에 미식가거든요",
          "speaker_id": "2",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.22",
          "form": "한국분들은 쓰셨을거같은데",
          "original_form": "한국분들은 쓰셨을거같은데",
          "speaker_id": "1",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.23",
          "form": "근데 후각이랑미각을잃어버린거에요",
          "original_form": "근데 후각이랑미각을잃어버린거에요",
          "speaker_id": "2",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.24",
          "form": "할....?!",
          "original_form": "할....?!",
          "speaker_id": "1",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.25",
          "form": "아니 세상에!?!?!?!",
          "original_form": "아니 세상에!?!?!?!",
          "speaker_id": "1",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.26",
          "form": "네네 작년4월에는없었어요마스크쓰는사람이",
          "original_form": "네네 작년4월에는없었어요마스크쓰는사람이",
          "speaker_id": "2",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.27",
          "form": "아뮤튼그러다가 코로나심각해져서같이한국와서",
          "original_form": "아뮤튼그러다가 코로나심각해져서같이한국와서",
          "speaker_id": "2",
          "time": "20210905 04:06"
        },
        {
          "id": "MDRW2100040766.1.28",
          "form": "언니가 뭔가 쎄해서 바로 검사를받았어요 저랑같이",
          "original_form": "언니가 뭔가 쎄해서 바로 검사를받았어요 저랑같이",
          "speaker_id": "2",
          "time": "20210905 04:07"
        },
        {
          "id": "MDRW2100040766.1.29",
          "form": "근데 다음날아침에 언니한테 9시에바로전화오고",
          "original_form": "근데 다음날아침에 언니한테 9시에바로전화오고",
          "speaker_id": "2",
          "time": "20210905 04:07"
        },
        {
          "id": "MDRW2100040766.1.30",
          "form": "언니 확진이라면서",
          "original_form": "언니 확진이라면서",
          "speaker_id": "2",
          "time": "20210905 04:07"
        },
        {
          "id": "MDRW2100040766.1.31",
          "form": "네네네 8ㅅ8",
          "original_form": "네네네 8ㅅ8",
          "speaker_id": "1",
          "time": "20210905 04:07"
        },
        {
          "id": "MDRW2100040766.1.32",
          "form": "네?!?!?",
          "original_form": "네?!?!?",
          "speaker_id": "1",
          "time": "20210905 04:07"
        },
        {
          "id": "MDRW2100040766.1.33",
          "form": "냅다 응급차타고갔답니다",
          "original_form": "냅다 응급차타고갔답니다",
          "speaker_id": "2",
          "time": "20210905 04:07"
        },
        {
          "id": "MDRW2100040766.1.34",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210905 04:07"
        },
        {
          "id": "MDRW2100040766.1.35",
          "form": "근데저는음성이었어요",
          "original_form": "근데저는음성이었어요",
          "speaker_id": "2",
          "time": "20210905 04:07"
        }
      ]
