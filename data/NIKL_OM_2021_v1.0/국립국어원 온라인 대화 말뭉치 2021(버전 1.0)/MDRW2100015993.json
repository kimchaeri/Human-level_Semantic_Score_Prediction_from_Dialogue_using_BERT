        },
        {
          "id": "MDRW2100015993.1.2",
          "form": "좋아하는데 잘 ㅁ못봐요",
          "original_form": "좋아하는데 잘 ㅁ못봐요",
          "speaker_id": "2",
          "time": "20210826 09:58"
        },
        {
          "id": "MDRW2100015993.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100015993.1.4",
          "form": "그쵸ㅠ 바빠서 ㅜㅜ",
          "original_form": "그쵸ㅠ 바빠서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100015993.1.5",
          "form": "네 시간이",
          "original_form": "네 시간이",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100015993.1.6",
          "form": "공연은 더 못보고",
          "original_form": "공연은 더 못보고",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100015993.1.7",
          "form": "거리두기가문제죠",
          "original_form": "거리두기가문제죠",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100015993.1.8",
          "form": "맞아요 공연계도 문제고",
          "original_form": "맞아요 공연계도 문제고",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100015993.1.9",
          "form": "모이기가 쉽지않죠",
          "original_form": "모이기가 쉽지않죠",
          "speaker_id": "1",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100015993.1.10",
          "form": "그쵸 ㅠ 안타까워요",
          "original_form": "그쵸 ㅠ 안타까워요",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100015993.1.11",
          "form": "현실이 그렇죠",
          "original_form": "현실이 그렇죠",
          "speaker_id": "1",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100015993.1.12",
          "form": "뮤지컬 좋아하세요?",
          "original_form": "뮤지컬 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 13:06"
