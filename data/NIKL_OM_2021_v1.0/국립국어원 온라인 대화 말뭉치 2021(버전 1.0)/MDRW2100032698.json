      "utterance": [
        {
          "id": "MDRW2100032698.1.1",
          "form": "혹시 붕어빵 좋아하시나요?",
          "original_form": "혹시 붕어빵 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032698.1.2",
          "form": "너무좋아해요!!",
          "original_form": "너무좋아해요!!",
          "speaker_id": "2",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032698.1.3",
          "form": "슈크림이랑 팥 중에 어떤게 좋아요??",
          "original_form": "슈크림이랑 팥 중에 어떤게 좋아요??",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100032698.1.4",
          "form": "저 팥좋아해서",
          "original_form": "저 팥좋아해서",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100032698.1.5",
          "form": "슈붕을 먹어본적도없어요",
          "original_form": "슈붕을 먹어본적도없어요",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032698.1.6",
          "form": "아 정말요?? 강경한 팥파네요",
          "original_form": "아 정말요?? 강경한 팥파네요",
          "speaker_id": "1",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032698.1.7",
          "form": "ㅋㅋㅋ슈붕 드셔보셨나요?",
          "original_form": "ㅋㅋㅋ슈붕 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100032698.1.8",
          "form": "네 전 섞어 먹는거 좋아해요!",
          "original_form": "네 전 섞어 먹는거 좋아해요!",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032698.1.9",
          "form": "집근처에 붕어빵집 많이 있나요?  요즘",
          "original_form": "집근처에 붕어빵집 많이 있나요?  요즘",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032698.1.10",
          "form": "뭐가더맛있어요?",
          "original_form": "뭐가더맛있어요?",
          "speaker_id": "2",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032698.1.11",
          "form": "네 저 붕세권이라",
          "original_form": "네 저 붕세권이라",
          "speaker_id": "2",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032698.1.12",
          "form": "1년내내먹을수있어요",
          "original_form": "1년내내먹을수있어요",
          "speaker_id": "2",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100032698.1.13",
          "form": "저도 팥이요!",
          "original_form": "저도 팥이요!",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032698.1.14",
          "form": "와 좋으시겠어요",
          "original_form": "와 좋으시겠어요",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032698.1.15",
          "form": "붕세권 아니신가요?!",
          "original_form": "붕세권 아니신가요?!",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032698.1.16",
          "form": "아쉬운..",
          "original_form": "아쉬운..",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032698.1.17",
          "form": "역시 팥이죠!",
          "original_form": "역시 팥이죠!",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032698.1.18",
          "form": "조끔 걸어가면 있긴한데",
          "original_form": "조끔 걸어가면 있긴한데",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100032698.1.19",
          "form": "그럼전걷습니다",
          "original_form": "그럼전걷습니다",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032698.1.20",
          "form": "요즘은 많이 사라지고 있다고해서요",
          "original_form": "요즘은 많이 사라지고 있다고해서요",
          "speaker_id": "1",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100032698.1.21",
          "form": "돼지에요",
          "original_form": "돼지에요",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
