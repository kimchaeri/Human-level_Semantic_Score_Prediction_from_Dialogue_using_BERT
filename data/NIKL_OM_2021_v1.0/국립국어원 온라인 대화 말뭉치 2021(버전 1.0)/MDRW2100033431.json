      "utterance": [
        {
          "id": "MDRW2100033431.1.1",
          "form": "건강을 위해 챙기는.것들 있으신가요",
          "original_form": "건강을 위해 챙기는.것들 있으신가요",
          "speaker_id": "1",
          "time": "20210823 21:22"
        },
        {
          "id": "MDRW2100033431.1.2",
          "form": "건강을 위해 챙기는 습관 있나요?",
          "original_form": "건강을 위해 챙기는 습관 있나요?",
          "speaker_id": "1",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100033431.1.3",
          "form": "음 영양제",
          "original_form": "음 영양제",
          "speaker_id": "2",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100033431.1.4",
          "form": "먹어요",
          "original_form": "먹어요",
          "speaker_id": "2",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100033431.1.5",
          "form": "오메가 비타민 유산균",
          "original_form": "오메가 비타민 유산균",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100033431.1.6",
          "form": "여러가지요",
          "original_form": "여러가지요",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100033431.1.7",
          "form": "어떤걸 하세요?",
          "original_form": "어떤걸 하세요?",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100033431.1.8",
          "form": "오 저도 영양제요! 비슷하게 드시네요",
          "original_form": "오 저도 영양제요! 비슷하게 드시네요",
          "speaker_id": "1",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100033431.1.9",
          "form": "다들 그런가봐요",
          "original_form": "다들 그런가봐요",
          "speaker_id": "2",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100033431.1.10",
          "form": "영양제도 유행이 있나봐요",
          "original_form": "영양제도 유행이 있나봐요",
          "speaker_id": "2",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100033431.1.11",
          "form": "전 걷기좀 해야 할것 같아요",
          "original_form": "전 걷기좀 해야 할것 같아요",
          "speaker_id": "2",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100033431.1.12",
          "form": "젤 좋은 운동이래요",
          "original_form": "젤 좋은 운동이래요",
          "speaker_id": "2",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100033431.1.13",
          "form": "맞아요 걷기어플 깔아서 걸으면 걸음수보며 걷는 재미도 있어요",
          "original_form": "맞아요 걷기어플 깔아서 걸으면 걸음수보며 걷는 재미도 있어요",
          "speaker_id": "1",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100033431.1.14",
          "form": "저도 그래야 겠어요",
          "original_form": "저도 그래야 겠어요",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100033431.1.15",
          "form": "동시부여 하게요",
          "original_form": "동시부여 하게요",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100033431.1.16",
          "form": "동기",
          "original_form": "동기",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100033431.1.17",
          "form": "아 치아 관리도 중요해요",
          "original_form": "아 치아 관리도 중요해요",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100033431.1.18",
          "form": "잇몸이 점점 안좋아져서 ㅜㅜ",
          "original_form": "잇몸이 점점 안좋아져서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100033431.1.19",
          "form": "정기검진 꼭해요",
          "original_form": "정기검진 꼭해요",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100033431.1.20",
          "form": "아 검진은 얼마마다 가시나요?  저도 가야하는데 ㅠ",
          "original_form": "아 검진은 얼마마다 가시나요?  저도 가야하는데 ㅠ",
          "speaker_id": "1",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100033431.1.21",
          "form": "6개월 한번인데",
          "original_form": "6개월 한번인데",
          "speaker_id": "2",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100033431.1.22",
          "form": "3개월한번 오래요 ㅜㅜ샘이",
          "original_form": "3개월한번 오래요 ㅜㅜ샘이",
          "speaker_id": "2",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100033431.1.23",
          "form": "전 너무 심각하대요 ㅜㅜ",
          "original_form": "전 너무 심각하대요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100033431.1.24",
          "form": "꼭 자주 가세요",
          "original_form": "꼭 자주 가세요",
          "speaker_id": "2",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100033431.1.25",
          "form": "저 요번에도 돈 수십들었어요",
          "original_form": "저 요번에도 돈 수십들었어요",
          "speaker_id": "2",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100033431.1.26",
          "form": "잇몸 다 붓고 터지면 이 못살려서 임플란트 해야해요",
          "original_form": "잇몸 다 붓고 터지면 이 못살려서 임플란트 해야해요",
          "speaker_id": "2",
          "time": "20210823 21:32"
        },
        {
          "id": "MDRW2100033431.1.27",
          "form": "아..저는 이미 금니도 많은데..치과 정말 무섭죠 ㅠ 치료도..치료비도..",
          "original_form": "아..저는 이미 금니도 많은데..치과 정말 무섭죠 ㅠ 치료도..치료비도..",
          "speaker_id": "1",
          "time": "20210823 21:32"
        },
        {
          "id": "MDRW2100033431.1.28",
          "form": "네 요번에도 ㅜㅜ",
          "original_form": "네 요번에도 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 21:33"
        },
        {
          "id": "MDRW2100033431.1.29",
          "form": "눈물찔끔",
          "original_form": "눈물찔끔",
          "speaker_id": "2",
          "time": "20210823 21:33"
        },
        {
          "id": "MDRW2100033431.1.30",
          "form": "그냥 마취 해달라 말하래요",
          "original_form": "그냥 마취 해달라 말하래요",
          "speaker_id": "2",
          "time": "20210823 21:34"
        },
        {
          "id": "MDRW2100033431.1.31",
          "form": "저도 치과는 늘 마취해달라고.. 넘 무서워요",
          "original_form": "저도 치과는 늘 마취해달라고.. 넘 무서워요",
          "speaker_id": "1",
          "time": "20210823 21:35"
        },
        {
          "id": "MDRW2100033431.1.32",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:36"
        },
        {
          "id": "MDRW2100033431.1.33",
          "form": "저막 소리질러요",
          "original_form": "저막 소리질러요",
          "speaker_id": "2",
          "time": "20210823 21:36"
        },
        {
          "id": "MDRW2100033431.1.34",
          "form": "근데 넘 친절하게 잘해주셔서 잘하고있어요",
          "original_form": "근데 넘 친절하게 잘해주셔서 잘하고있어요",
          "speaker_id": "2",
          "time": "20210823 21:36"
        },
        {
          "id": "MDRW2100033431.1.35",
          "form": "치아 진짜 중요해요",
          "original_form": "치아 진짜 중요해요",
          "speaker_id": "2",
          "time": "20210823 21:36"
        },
        {
          "id": "MDRW2100033431.1.36",
          "form": "아이들 양치도 잘 시켜야하는데 왜이리 하기 싫어하는지 ㅠㅠ",
          "original_form": "아이들 양치도 잘 시켜야하는데 왜이리 하기 싫어하는지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 21:37"
        }
      ]
    }
