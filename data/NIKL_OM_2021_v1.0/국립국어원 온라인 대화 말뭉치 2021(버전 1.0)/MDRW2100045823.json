      "utterance": [
        {
          "id": "MDRW2100045823.1.1",
          "form": "앗 들킴 왜 그렇게 생각하셨죠",
          "original_form": "앗 들킴 왜 그렇게 생각하셨죠",
          "speaker_id": "2",
          "time": "20210908 07:37"
        },
        {
          "id": "MDRW2100045823.1.2",
          "form": "각각 이유가 궁금해요",
          "original_form": "각각 이유가 궁금해요",
          "speaker_id": "2",
          "time": "20210908 07:37"
        },
        {
          "id": "MDRW2100045823.1.3",
          "form": "직감?",
          "original_form": "직감?",
          "speaker_id": "1",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100045823.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100045823.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ문과 이과 중 어디일 것 같나요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ문과 이과 중 어디일 것 같나요!",
          "speaker_id": "2",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100045823.1.6",
          "form": "음... 코카콜라....",
          "original_form": "음... 코카콜라....",
          "speaker_id": "1",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100045823.1.7",
          "form": "문과요!",
          "original_form": "문과요!",
          "speaker_id": "1",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100045823.1.8",
          "form": "헉 정답~~ 문과입니다",
          "original_form": "헉 정답~~ 문과입니다",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100045823.1.9",
          "form": "!!!",
          "original_form": "!!!",
          "speaker_id": "1",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100045823.1.10",
          "form": "마..맞췄다...?ㅋㅋㅋ",
          "original_form": "마..맞췄다...?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045823.1.11",
          "form": "ㅋㅋㅋ네 문과입니다 제 학과도 좀 생소하실 수 있어요",
          "original_form": "ㅋㅋㅋ네 문과입니다 제 학과도 좀 생소하실 수 있어요",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045823.1.12",
          "form": "음... ㅡ여쭤보기전에 직감으로...앗...",
          "original_form": "음... ㅡ여쭤보기전에 직감으로...앗...",
          "speaker_id": "1",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045823.1.13",
          "form": "구럼 아니겠네요",
          "original_form": "구럼 아니겠네요",
          "speaker_id": "1",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045823.1.14",
          "form": "국어 관련된 학과이지 않을까 했는데 생소하다면 음....",
          "original_form": "국어 관련된 학과이지 않을까 했는데 생소하다면 음....",
          "speaker_id": "1",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100045823.1.15",
          "form": "저도 의식주중 하나예요!",
          "original_form": "저도 의식주중 하나예요!",
          "speaker_id": "2",
          "time": "20210908 07:41"
        },
        {
          "id": "MDRW2100045823.1.16",
          "form": "문관데 의식주요?",
          "original_form": "문관데 의식주요?",
          "speaker_id": "1",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100045823.1.17",
          "form": "전햐 매치가 안되는데요??",
          "original_form": "전햐 매치가 안되는데요??",
          "speaker_id": "1",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100045823.1.18",
          "form": "건축학과?",
          "original_form": "건축학과?",
          "speaker_id": "1",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045823.1.19",
          "form": "응? 이건 이과죠?ㅋㅋㅋㅋ",
          "original_form": "응? 이건 이과죠?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045823.1.20",
          "form": "네 ㅋㅋㅋㅋ독특하죠?",
          "original_form": "네 ㅋㅋㅋㅋ독특하죠?",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045823.1.21",
          "form": "아 이제는 이과랑 합쳐져서 이과로 편입됐대요",
          "original_form": "아 이제는 이과랑 합쳐져서 이과로 편입됐대요",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045823.1.22",
          "form": "의하면 패션이고 식하면 외식 관련이고 주는 건축공학이 생각나는데",
          "original_form": "의하면 패션이고 식하면 외식 관련이고 주는 건축공학이 생각나는데",
          "speaker_id": "1",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100045823.1.23",
          "form": "헐 그래요? 그럴 수도 있다니 신기하네요",
          "original_form": "헐 그래요? 그럴 수도 있다니 신기하네요",
