      "utterance": [
        {
          "id": "MDRW2100009345.1.1",
          "form": "쇼핑 좋아하시나요",
          "original_form": "쇼핑 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009345.1.2",
          "form": "좋아했었죠...  요즘은 물질욕도 사라졌어요..",
          "original_form": "좋아했었죠...  요즘은 물질욕도 사라졌어요..",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009345.1.3",
          "form": "ㅅ쇼핑좋아하시죠?",
          "original_form": "ㅅ쇼핑좋아하시죠?",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009345.1.4",
          "form": "좋아하는데 요즘 코로나때문에",
          "original_form": "좋아하는데 요즘 코로나때문에",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009345.1.5",
          "form": "잘 꾸미질못해서",
          "original_form": "잘 꾸미질못해서",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009345.1.6",
          "form": "요새는 많이줄었어요",
          "original_form": "요새는 많이줄었어요",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009345.1.7",
          "form": "ㅋㅋ 대 코로나시대에 많은게 변했네요..",
          "original_form": "ㅋㅋ 대 코로나시대에 많은게 변했네요..",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009345.1.8",
          "form": "오프라인 쇼핑은 대형몰가세요? 스트리트샵?",
          "original_form": "오프라인 쇼핑은 대형몰가세요? 스트리트샵?",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009345.1.9",
          "form": "스트리트샵이 이쁜게 많은거같아요 ㅎㅎ",
          "original_form": "스트리트샵이 이쁜게 많은거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009345.1.10",
          "form": "중고거래 해보셨나요?",
          "original_form": "중고거래 해보셨나요?",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009345.1.11",
          "form": "당근 50도에요..ㅋㅋㅋ",
          "original_form": "당근 50도에요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009345.1.12",
          "form": "당근많이 하세요?",
          "original_form": "당근많이 하세요?",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009345.1.13",
          "form": "저 당근아예 안해봤어요",
          "original_form": "저 당근아예 안해봤어요",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009345.1.14",
          "form": "당근 50도는 뭐예요?",
          "original_form": "당근 50도는 뭐예요?",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009345.1.15",
          "form": "오..아직 안해보셧구낭.. \r\n매너점수에요.. 36.5도에서 시작해서",
          "original_form": "오..아직 안해보셧구낭.. \r\n매너점수에요.. 36.5도에서 시작해서",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009345.1.16",
          "form": "중고거래 아예 안 해보신거에요?",
          "original_form": "중고거래 아예 안 해보신거에요?",
          "speaker_id": "2",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009345.1.17",
          "form": "오 몇도까지 있어요?",
          "original_form": "오 몇도까지 있어요?",
          "speaker_id": "1",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009345.1.18",
          "form": "새로운거 많이알게되네용 ㅇㅅㅇ",
          "original_form": "새로운거 많이알게되네용 ㅇㅅㅇ",
          "speaker_id": "1",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009345.1.19",
          "form": "넵 안해봤어여",
          "original_form": "넵 안해봤어여",
          "speaker_id": "1",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009345.1.20",
          "form": "온도는 몰라요.. 찔끔찔끔 올라서.. \r\n별에별게 다팔려요..",
          "original_form": "온도는 몰라요.. 찔끔찔끔 올라서.. \r\n별에별게 다팔려요..",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009345.1.21",
          "form": "회사에서 받은 샴푸 같은거도 팔림.. ㅋㅋ",
          "original_form": "회사에서 받은 샴푸 같은거도 팔림.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009345.1.22",
          "form": "아 진짜요 ..?",
          "original_form": "아 진짜요 ..?",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009345.1.23",
          "form": "대박 새로운사실 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "대박 새로운사실 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009345.1.24",
          "form": "저 금방 질려서 옷도 몇전 안입고 그러는데..\r\n왠만하면 다 팔려요..",
          "original_form": "저 금방 질려서 옷도 몇전 안입고 그러는데..\r\n왠만하면 다 팔려요..",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009345.1.25",
          "form": "그래서 더 막지르게 되요.. \r\n맘에 안듬 당근ㅇ 팔면되니까..",
          "original_form": "그래서 더 막지르게 되요.. \r\n맘에 안듬 당근ㅇ 팔면되니까..",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009345.1.26",
          "form": "신기하네요 사고싶은건 업ㄱ어요?",
          "original_form": "신기하네요 사고싶은건 업ㄱ어요?",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009345.1.27",
          "form": "대왕꼬부기 인형? ㅋㅋㅋ",
          "original_form": "대왕꼬부기 인형? ㅋㅋㅋ",
