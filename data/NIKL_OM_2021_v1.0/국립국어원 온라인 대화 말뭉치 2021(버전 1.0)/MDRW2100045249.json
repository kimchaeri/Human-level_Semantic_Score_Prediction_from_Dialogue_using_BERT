      "utterance": [
        {
          "id": "MDRW2100045249.1.1",
          "form": "저녁 드셧어요?",
          "original_form": "저녁 드셧어요?",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045249.1.2",
          "form": "안녕하세요~ 네 먹었어요ㅎㅎ",
          "original_form": "안녕하세요~ 네 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045249.1.3",
          "form": "오 어떤거 드셧나요",
          "original_form": "오 어떤거 드셧나요",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045249.1.4",
          "form": "부대찌개요ㅎㅎ",
          "original_form": "부대찌개요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045249.1.5",
          "form": "name1님은 저녁 어떤거 드셨어요?",
          "original_form": "&name1&님은 저녁 어떤거 드셨어요?",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045249.1.6",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045249.1.7",
          "form": "저는 볶음밥이요",
          "original_form": "저는 볶음밥이요",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045249.1.8",
          "form": "부대찌개 요리 하셧어요?",
          "original_form": "부대찌개 요리 하셧어요?",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045249.1.9",
          "form": "아 아뇨 포장해온거 끓이기만 했어요!ㅋㅋㅋㅋ",
          "original_form": "아 아뇨 포장해온거 끓이기만 했어요!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045249.1.10",
          "form": "볶음밥도 맛있겠네요ㅠㅠ",
          "original_form": "볶음밥도 맛있겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045249.1.11",
          "form": "오오 요즘 부대찌개 잘 나오드라구요 포장도",
          "original_form": "오오 요즘 부대찌개 잘 나오드라구요 포장도",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045249.1.12",
          "form": "네 육수도 따로 줘서",
          "original_form": "네 육수도 따로 줘서",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045249.1.13",
          "form": "진짜 끓이기만 하면 되게 나오더라구요!",
          "original_form": "진짜 끓이기만 하면 되게 나오더라구요!",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045249.1.14",
          "form": "오 다음에 사봐야겟어요 ㅋㅋㅋ",
          "original_form": "오 다음에 사봐야겟어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045249.1.15",
          "form": "육수도요?? 우와",
          "original_form": "육수도요?? 우와",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045249.1.16",
          "form": "맛집이라 그런지 아예 자기들 방식으로 세팅 다 해서 주는 것 같았어요ㅎㅎ",
          "original_form": "맛집이라 그런지 아예 자기들 방식으로 세팅 다 해서 주는 것 같았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045249.1.17",
          "form": "우아 토핑 많이 추가 하셧나요??",
          "original_form": "우아 토핑 많이 추가 하셧나요??",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045249.1.18",
          "form": "토핑 추가 안해도 이미 많더라구욬ㅋㅋㅋㅋ 햄 많이 들어있는걸로 유명한 집이었어서..",
          "original_form": "토핑 추가 안해도 이미 많더라구욬ㅋㅋㅋㅋ 햄 많이 들어있는걸로 유명한 집이었어서..",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045249.1.19",
          "form": "오오 너무 조항요ㅜㅜ",
          "original_form": "오오 너무 조항요ㅜㅜ",
