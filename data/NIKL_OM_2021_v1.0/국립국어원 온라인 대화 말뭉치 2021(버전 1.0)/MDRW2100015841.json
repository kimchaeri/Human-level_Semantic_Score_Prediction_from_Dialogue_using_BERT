      "utterance": [
        {
          "id": "MDRW2100015841.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100015841.1.2",
          "form": "안녕하세요 ~",
          "original_form": "안녕하세요 ~",
          "speaker_id": "2",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100015841.1.3",
          "form": "커뮤니티 많이 이용하세요?",
          "original_form": "커뮤니티 많이 이용하세요?",
          "speaker_id": "1",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015841.1.4",
          "form": "네 요즘 꿀팁같은거 많이 얻을수 있어서 좋더라구요~",
          "original_form": "네 요즘 꿀팁같은거 많이 얻을수 있어서 좋더라구요~",
          "speaker_id": "2",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015841.1.5",
          "form": "아 맞아요 꿀팁이나 할인정보? 유용한거같아요!",
          "original_form": "아 맞아요 꿀팁이나 할인정보? 유용한거같아요!",
          "speaker_id": "1",
          "time": "20210826 02:20"
        },
        {
          "id": "MDRW2100015841.1.6",
          "form": "요즘 커뮤니티에서 보고 파리바게트 요기요에서 열심히 시켜먹고있어여",
          "original_form": "요즘 커뮤니티에서 보고 파리바게트 요기요에서 열심히 시켜먹고있어여",
          "speaker_id": "2",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015841.1.7",
          "form": "아아 마자요 저두 ㅋㅋㅋㅋ 요기요 파바 주변에 많이 사먹네요",
          "original_form": "아아 마자요 저두 ㅋㅋㅋㅋ 요기요 파바 주변에 많이 사먹네요",
          "speaker_id": "1",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015841.1.8",
          "form": "ㅋㅋ저도요! 커뮤니티에서 정보 많이 얻게되네요",
          "original_form": "ㅋㅋ저도요! 커뮤니티에서 정보 많이 얻게되네요",
          "speaker_id": "2",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100015841.1.9",
          "form": "맞아요 ㅋㅋㅋ이벤트 참여해서 당첨되면 기분좋더라구요",
          "original_form": "맞아요 ㅋㅋㅋ이벤트 참여해서 당첨되면 기분좋더라구요",
          "speaker_id": "1",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100015841.1.10",
          "form": "오 저는 이벤트 당첨된적이 없어서 부러워요",
          "original_form": "오 저는 이벤트 당첨된적이 없어서 부러워요",
          "speaker_id": "2",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100015841.1.11",
          "form": "그렇군요 저는 대신 좀 보이는대로 참여하는편이에요 ㅋㅋㅋㅋ",
          "original_form": "그렇군요 저는 대신 좀 보이는대로 참여하는편이에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100015841.1.12",
          "form": "많이 참여하면 당첨되지않을까요!",
          "original_form": "많이 참여하면 당첨되지않을까요!",
          "speaker_id": "1",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100015841.1.13",
          "form": "저도 더 노력해봐야겠어요 ㅋㄱ",
          "original_form": "저도 더 노력해봐야겠어요 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100015841.1.14",
          "form": "그리구 할인정보같응거 넘 좋아요",
          "original_form": "그리구 할인정보같응거 넘 좋아요",
          "speaker_id": "1",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100015841.1.15",
          "form": "맞아요 커뮤니티 하기 전이랑 비교하면 할인된 가격으로 자주 쇼핑할수 있어서 좋아요",
          "original_form": "맞아요 커뮤니티 하기 전이랑 비교하면 할인된 가격으로 자주 쇼핑할수 있어서 좋아요",
          "speaker_id": "2",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100015841.1.16",
          "form": "맞아요ㅋㅋㅋㅋ그래서 인터넷주문하는게 더 늘엇어요",
          "original_form": "맞아요ㅋㅋㅋㅋ그래서 인터넷주문하는게 더 늘엇어요",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100015841.1.17",
          "form": "커뮤니티 더 자후 활용해야겠어요 ㅋㅋ",
          "original_form": "커뮤니티 더 자후 활용해야겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:12"
        }
      ]
