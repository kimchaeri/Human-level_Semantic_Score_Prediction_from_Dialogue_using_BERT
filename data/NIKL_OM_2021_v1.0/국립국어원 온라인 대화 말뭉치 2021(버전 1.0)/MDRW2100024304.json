      "utterance": [
        {
          "id": "MDRW2100024304.1.1",
          "form": "name1이 학교생활 요즘 어때?",
          "original_form": "&name1&이 학교생활 요즘 어때?",
          "speaker_id": "2",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100024304.1.2",
          "form": "곧 개강인데 소감 말해줘~@",
          "original_form": "곧 개강인데 소감 말해줘~@",
          "speaker_id": "2",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100024304.1.3",
          "form": "학교생활~~~ 넘모 힘드러",
          "original_form": "학교생활~~~ 넘모 힘드러",
          "speaker_id": "1",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100024304.1.4",
          "form": "누가 괴롭혀?!",
          "original_form": "누가 괴롭혀?!",
          "speaker_id": "2",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100024304.1.5",
          "form": "내가 다 혼내줄게!",
          "original_form": "내가 다 혼내줄게!",
          "speaker_id": "2",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100024304.1.6",
          "form": "언니가 다 괴롭혀줘!!!!!!!",
          "original_form": "언니가 다 괴롭혀줘!!!!!!!",
          "speaker_id": "1",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100024304.1.7",
          "form": "아니뭐래",
          "original_form": "아니뭐래",
          "speaker_id": "1",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100024304.1.8",
          "form": "나지금ㅋㅋㅋㅋㅋㅋㅋ딴생각하면서 쳤나봐 언니가 다 혼내줘!!!!!!!",
          "original_form": "나지금ㅋㅋㅋㅋㅋㅋㅋ딴생각하면서 쳤나봐 언니가 다 혼내줘!!!!!!!",
          "speaker_id": "1",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100024304.1.9",
          "form": "",
          "original_form": "{censored}",
          "speaker_id": "1",
          "time": "20210831 00:40"
        },
        {
          "id": "MDRW2100024304.1.10",
          "form": "너~~~~~ 울언니한테 혼쭐나기싫으면 조심해라!!!!",
          "original_form": "너~~~~~ 울언니한테 혼쭐나기싫으면 조심해라!!!!",
          "speaker_id": "1",
          "time": "20210831 00:40"
        },
        {
          "id": "MDRW2100024304.1.11",
          "form": "나 진짜 혼내줄 수 있어!!!",
          "original_form": "나 진짜 혼내줄 수 있어!!!",
          "speaker_id": "2",
          "time": "20210831 01:18"
        },
        {
          "id": "MDRW2100024304.1.12",
          "form": "학교생활 진짜",
          "original_form": "학교생활 진짜",
          "speaker_id": "2",
          "time": "20210831 01:18"
        },
        {
          "id": "MDRW2100024304.1.13",
          "form": "뭐랄까...",
          "original_form": "뭐랄까...",
          "speaker_id": "2",
          "time": "20210831 01:18"
        },
        {
          "id": "MDRW2100024304.1.14",
          "form": "맘에 안드는 애들이랑도",
          "original_form": "맘에 안드는 애들이랑도",
          "speaker_id": "2",
          "time": "20210831 01:18"
        },
        {
          "id": "MDRW2100024304.1.15",
          "form": "같은 반이라는 이유만으로 매일 얼굴 봐야하고",
          "original_form": "같은 반이라는 이유만으로 매일 얼굴 봐야하고",
          "speaker_id": "2",
          "time": "20210831 01:19"
        },
        {
          "id": "MDRW2100024304.1.16",
          "form": "시험도 보고 점수도 내야하고....",
          "original_form": "시험도 보고 점수도 내야하고....",
          "speaker_id": "2",
          "time": "20210831 01:19"
        },
        {
          "id": "MDRW2100024304.1.17",
          "form": "ㅠㅠㅠㅠㅜㅠㅠㅠ그니까.. 증말 웃겨........",
          "original_form": "ㅠㅠㅠㅠㅜㅠㅠㅠ그니까.. 증말 웃겨........",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100024304.1.18",
          "form": "생각해보니 언니가 나보다 더",
          "original_form": "생각해보니 언니가 나보다 더",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100024304.1.19",
          "form": "학교 생활 시작의",
          "original_form": "학교 생활 시작의",
          "speaker_id": "2",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100024304.1.20",
          "form": "D-1이다!",
          "original_form": "D-1이다!",
          "speaker_id": "2",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100024304.1.21",
          "form": "뭔가...ㅜㅜㅜ ㅜㅜㅜ 대면?!?!? 라이프라..",
          "original_form": "뭔가...ㅜㅜㅜ ㅜㅜㅜ 대면?!?!? 라이프라..",
          "speaker_id": "1",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024304.1.22",
          "form": "응원해주는게 넘 몸둘바모르겠달까..",
          "original_form": "응원해주는게 넘 몸둘바모르겠달까..",
          "speaker_id": "1",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024304.1.23",
          "form": "그러게!!!! 개 강 직 전",
          "original_form": "그러게!!!! 개 강 직 전",
          "speaker_id": "1",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024304.1.24",
          "form": "으아아앙아아아 교수님 좋은말로할때 저한테 잘보이세요(?)",
          "original_form": "으아아앙아아아 교수님 좋은말로할때 저한테 잘보이세요(?)",
          "speaker_id": "1",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024304.1.25",
          "form": "나도 내일부터",
          "original_form": "나도 내일부터",
          "speaker_id": "2",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024304.1.26",
          "form": "이제 수업 들어야해서 은근히 긴장된다!!!",
          "original_form": "이제 수업 들어야해서 은근히 긴장된다!!!",
          "speaker_id": "2",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024304.1.27",
          "form": "맞아 교수님~~~",
          "original_form": "맞아 교수님~~~",
          "speaker_id": "2",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024304.1.28",
          "form": "우리한테 잘 보이란 말이야~~~",
          "original_form": "우리한테 잘 보이란 말이야~~~",
          "speaker_id": "2",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024304.1.29",
          "form": "으아아아아 그니까요!!!!!",
          "original_form": "으아아아아 그니까요!!!!!",
          "speaker_id": "1",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024304.1.30",
          "form": "고생이많아",
          "original_form": "고생이많아",
          "speaker_id": "1",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024304.1.31",
          "form": "우리둘다!!!!",
          "original_form": "우리둘다!!!!",
          "speaker_id": "1",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024304.1.32",
          "form": "맞아!",
          "original_form": "맞아!",
          "speaker_id": "2",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024304.1.33",
          "form": "학교 생활 쉽지 않아~~~",
          "original_form": "학교 생활 쉽지 않아~~~",
          "speaker_id": "2",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024304.1.34",
          "form": "이번에 유기특강 가르치시는 교수님은",
          "original_form": "이번에 유기특강 가르치시는 교수님은",
          "speaker_id": "2",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100024304.1.35",
          "form": "완전 성실하셔서",
          "original_form": "완전 성실하셔서",
          "speaker_id": "2",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100024304.1.36",
          "form": "이번에 영상 새로 다 찍으셨네...?",
          "original_form": "이번에 영상 새로 다 찍으셨네...?",
          "speaker_id": "2",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100024304.1.37",
          "form": "오오오 언제 다찍으신겨 방학동안 하신건가 대단하신데?!?!!!!!!",
          "original_form": "오오오 언제 다찍으신겨 방학동안 하신건가 대단하신데?!?!!!!!!",
          "speaker_id": "1",
          "time": "20210831 13:37"
        },
        {
          "id": "MDRW2100024304.1.38",
          "form": "교수님... 제법이시네요(?)",
          "original_form": "교수님... 제법이시네요(?)",
          "speaker_id": "1",
          "time": "20210831 13:38"
        },
        {
          "id": "MDRW2100024304.1.39",
          "form": "미리 다 찍어놓고",
          "original_form": "미리 다 찍어놓고",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100024304.1.40",
          "form": "한 주 지날 때마다 사이버 캠퍼스에 업로드 하실 예정인가봐...",
          "original_form": "한 주 지날 때마다 사이버 캠퍼스에 업로드 하실 예정인가봐...",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100024304.1.41",
          "form": "교수님 진짜 부지런하시다;;;;",
          "original_form": "교수님 진짜 부지런하시다;;;;",
