        {
          "id": "MDRW2100039759.1.1",
          "form": "name2님은 좋아하는 음식 있으세요?",
          "original_form": "&name2&님은 좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039759.1.2",
          "form": "저 매운음식 좋아해요 ㅎ",
          "original_form": "저 매운음식 좋아해요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039759.1.3",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "1",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039759.1.4",
          "form": "떡볶이 좋아하세요?",
          "original_form": "떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039759.1.5",
          "form": "신전만 좋아하는 편ㅇ에요 ㅎㅎ",
          "original_form": "신전만 좋아하는 편ㅇ에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039759.1.6",
          "form": "집에서 만드는 떡볶이랑~? ㅋㅋ",
          "original_form": "집에서 만드는 떡볶이랑~? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039759.1.7",
          "form": "집에서 만들 때도 신전처럼 자극적으로 만드는 편이세요?",
          "original_form": "집에서 만들 때도 신전처럼 자극적으로 만드는 편이세요?",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039759.1.8",
          "form": "아니용 ㅋㅋ",
          "original_form": "아니용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039759.1.9",
          "form": "신전이랑은 다르게하죠",
          "original_form": "신전이랑은 다르게하죠",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039759.1.10",
          "form": "그냥 집에서 고추장넣고하는...그런거??",
          "original_form": "그냥 집에서 고추장넣고하는...그런거??",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039759.1.11",
          "form": "아 그런 것도 맛있죠",
          "original_form": "아 그런 것도 맛있죠",
          "speaker_id": "1",
          "time": "20210904 07:37"
        },
        {
          "id": "MDRW2100039759.1.12",
          "form": "떡볶이 좋아하시나바요 ㅎ",
          "original_form": "떡볶이 좋아하시나바요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 07:59"
        },
        {
          "id": "MDRW2100039759.1.13",
          "form": "맞아요ㅋㅋ 거의 모든 떡볶이는 다 좋아하죠",
          "original_form": "맞아요ㅋㅋ 거의 모든 떡볶이는 다 좋아하죠",
          "speaker_id": "1",
          "time": "20210904 10:40"
        },
        {
          "id": "MDRW2100039759.1.14",
          "form": "제일좋아하는떡볶이는 어디에여??ㅎㅎ",
          "original_form": "제일좋아하는떡볶이는 어디에여??ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100039759.1.15",
          "form": "흠 전 체인점 떡볶이도 다 좋아해서 못 고르겠어요ㅋㅋ",
          "original_form": "흠 전 체인점 떡볶이도 다 좋아해서 못 고르겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100039759.1.16",
          "form": "정말좋아하시나보당",
          "original_form": "정말좋아하시나보당",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100039759.1.17",
          "form": "떡볶이말고는 좋아하시는 음식있으세여?",
          "original_form": "떡볶이말고는 좋아하시는 음식있으세여?",
          "speaker_id": "2",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100039759.1.18",
          "form": "음 케이크요ㅋㅋ",
          "original_form": "음 케이크요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100039759.1.19",
          "form": "저도 딸기생크림 좋아하는데",
          "original_form": "저도 딸기생크림 좋아하는데",
          "speaker_id": "2",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100039759.1.20",
          "form": "엄청 맛있는곳은 못먹어본고겉어요 ㅋㅋ..",
          "original_form": "엄청 맛있는곳은 못먹어본고겉어요 ㅋㅋ..",
