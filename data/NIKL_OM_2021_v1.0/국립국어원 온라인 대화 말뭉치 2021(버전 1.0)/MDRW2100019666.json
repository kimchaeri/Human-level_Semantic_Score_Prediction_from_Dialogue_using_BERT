      "utterance": [
        {
          "id": "MDRW2100019666.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019666.1.2",
          "form": "저는 졸업했어요!",
          "original_form": "저는 졸업했어요!",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019666.1.3",
          "form": "학교다니시나요?",
          "original_form": "학교다니시나요?",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019666.1.4",
          "form": "저는 고등학교까지만 나와서요 대학이 궁금해요",
          "original_form": "저는 고등학교까지만 나와서요 대학이 궁금해요",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100019666.1.5",
          "form": "저는 고등학교랑 큰차이 없엇던거같아요 ㅋㅋㅋ",
          "original_form": "저는 고등학교랑 큰차이 없엇던거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019666.1.6",
          "form": "수강신청을 직접 하는거잖아요 대학은",
          "original_form": "수강신청을 직접 하는거잖아요 대학은",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019666.1.7",
          "form": "네근데 저희학교는 시간표가 짜져서",
          "original_form": "네근데 저희학교는 시간표가 짜져서",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019666.1.8",
          "form": "치열할거도 없었어요",
          "original_form": "치열할거도 없었어요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019666.1.9",
          "form": "아 시간표를 짜주는 학교도 있군요",
          "original_form": "아 시간표를 짜주는 학교도 있군요",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019666.1.10",
          "form": "무슨 과 나오셨나요?",
          "original_form": "무슨 과 나오셨나요?",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019666.1.11",
          "form": "저는 경영과에요 ㅋㅋ",
          "original_form": "저는 경영과에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019666.1.12",
          "form": "아 저 어렸을때 경영과 진짜 멋지다고 생각했었는데ㅎㅎ",
          "original_form": "아 저 어렸을때 경영과 진짜 멋지다고 생각했었는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019666.1.13",
          "form": "저도 그랬는데 막상 와보니까",
          "original_form": "저도 그랬는데 막상 와보니까",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100019666.1.14",
          "form": "저랑 너무 안맞더라구요 ㅜㅜ",
          "original_form": "저랑 너무 안맞더라구요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100019666.1.15",
          "form": "님은 그럼 인문쪽이 더 취향이셨나요?",
          "original_form": "님은 그럼 인문쪽이 더 취향이셨나요?",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019666.1.16",
          "form": "원래는 세무관련으로 가고싶었는데",
          "original_form": "원래는 세무관련으로 가고싶었는데",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019666.1.17",
          "form": "떨어져서 경영갔죠뭐 ㅋㅋㅋ",
          "original_form": "떨어져서 경영갔죠뭐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019666.1.18",
          "form": "그렇군요. 학업 스트레스가 좀 있으셨겠어요",
          "original_form": "그렇군요. 학업 스트레스가 좀 있으셨겠어요",
