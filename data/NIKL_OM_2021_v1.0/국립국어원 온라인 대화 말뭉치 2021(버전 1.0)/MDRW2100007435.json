      "utterance": [
        {
          "id": "MDRW2100007435.1.1",
          "form": "전 07군번인데 혹시 군번이 어떻게 되세요?",
          "original_form": "전 07군번인데 혹시 군번이 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100007435.1.2",
          "form": "엄청 오래 되셨네요",
          "original_form": "엄청 오래 되셨네요",
          "speaker_id": "1",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100007435.1.3",
          "form": "전 이제 예비군 새내기입니다 ㅋㅋㅋㅋ전역 1년차에요 이제",
          "original_form": "전 이제 예비군 새내기입니다 ㅋㅋㅋㅋ전역 1년차에요 이제",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100007435.1.4",
          "form": "전역 축하드려요",
          "original_form": "전역 축하드려요",
          "speaker_id": "2",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100007435.1.5",
          "form": "저는 육군인데 육해공 중에 어디신가요?",
          "original_form": "저는 육군인데 육해공 중에 어디신가요?",
          "speaker_id": "2",
          "time": "20210822 18:58"
        },
        {
          "id": "MDRW2100007435.1.6",
          "form": "저도 육군 출신이에요",
          "original_form": "저도 육군 출신이에요",
          "speaker_id": "1",
          "time": "20210822 20:19"
        },
        {
          "id": "MDRW2100007435.1.7",
          "form": "오 육군이시군요 어디서 복무 하셨어요?",
          "original_form": "오 육군이시군요 어디서 복무 하셨어요?",
          "speaker_id": "2",
          "time": "20210822 20:20"
        },
        {
          "id": "MDRW2100007435.1.8",
          "form": "저 서울쪽에서 근무했어요. 북한산 쪽이요",
          "original_form": "저 서울쪽에서 근무했어요. 북한산 쪽이요",
          "speaker_id": "1",
          "time": "20210822 20:21"
        },
        {
          "id": "MDRW2100007435.1.9",
          "form": "운이 좋았죠 ㅋㅋㅋㅋ",
          "original_form": "운이 좋았죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:21"
        },
        {
          "id": "MDRW2100007435.1.10",
          "form": "저 예비군 북한산 근처에서 받았었는데",
          "original_form": "저 예비군 북한산 근처에서 받았었는데",
          "speaker_id": "2",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100007435.1.11",
          "form": "지축훈련장이요",
          "original_form": "지축훈련장이요",
          "speaker_id": "2",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100007435.1.12",
          "form": "저한테 받으셨겠군요 ㅋㅋㅋㅋ반갑습니다 선배님",
          "original_form": "저한테 받으셨겠군요 ㅋㅋㅋㅋ반갑습니다 선배님",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100007435.1.13",
          "form": "혹시나 했는데 ㅎㅎ",
          "original_form": "혹시나 했는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 20:26"
        },
        {
          "id": "MDRW2100007435.1.14",
          "form": "반갑습니다 ㅋㅋ",
          "original_form": "반갑습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100007435.1.15",
          "form": "이런인연이 있네요",
          "original_form": "이런인연이 있네요",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100007435.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러게요 사격장 올라가시느라 고생하셨겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러게요 사격장 올라가시느라 고생하셨겠어요",
          "speaker_id": "1",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100007435.1.17",
          "form": "저희 사격장이 완전 등산이라",
          "original_form": "저희 사격장이 완전 등산이라",
          "speaker_id": "1",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100007435.1.18",
          "form": "맞아요 비포장도로 비탈길 한참가야죠",
          "original_form": "맞아요 비포장도로 비탈길 한참가야죠",
          "speaker_id": "2",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100007435.1.19",
          "form": "비많이와서 강당애서 자다가 퇴소했던 기억도 나네요",
          "original_form": "비많이와서 강당애서 자다가 퇴소했던 기억도 나네요",
          "speaker_id": "2",
          "time": "20210822 20:30"
        },
        {
          "id": "MDRW2100007435.1.20",
          "form": "충성관 저도 오랜만이네요 ㅋㅋㅋㅋㅋ",
          "original_form": "충성관 저도 오랜만이네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100007435.1.21",
          "form": "예비군 식당에서 식사하셨나요? 나름 다른 곳보다 맛있다고 저희 훈련장의 자랑이었는데ㅋㅋㅋㅋㅋ",
          "original_form": "예비군 식당에서 식사하셨나요? 나름 다른 곳보다 맛있다고 저희 훈련장의 자랑이었는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100007435.1.22",
          "form": "이제 생각해보면 갇혀있는 저희한테나 그랬지 싶긴하네요",
          "original_form": "이제 생각해보면 갇혀있는 저희한테나 그랬지 싶긴하네요",
          "speaker_id": "1",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100007435.1.23",
          "form": "저는 배고파서 늘 먹었어요 거는대 2시간걸려서 늘 아침을 못먹고 갔거든요",
          "original_form": "저는 배고파서 늘 먹었어요 거는대 2시간걸려서 늘 아침을 못먹고 갔거든요",
          "speaker_id": "2",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100007435.1.24",
          "form": "나름 맛있었어요",
          "original_form": "나름 맛있었어요",
