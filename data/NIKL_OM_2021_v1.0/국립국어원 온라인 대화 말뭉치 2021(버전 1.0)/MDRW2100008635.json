      "utterance": [
        {
          "id": "MDRW2100008635.1.1",
          "form": "즐겨보는 드라마가있나요",
          "original_form": "즐겨보는 드라마가있나요",
          "speaker_id": "1",
          "time": "20210822 17:05"
        },
        {
          "id": "MDRW2100008635.1.2",
          "form": "최근 방영하는 드라마는 없고",
          "original_form": "최근 방영하는 드라마는 없고",
          "speaker_id": "2",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008635.1.3",
          "form": "예전엔 방영한 '블랙독'이라는 드라마 최근에 봤어요",
          "original_form": "예전엔 방영한 '블랙독'이라는 드라마 최근에 봤어요",
          "speaker_id": "2",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008635.1.4",
          "form": "오 저그거 재밌게봤어요",
          "original_form": "오 저그거 재밌게봤어요",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008635.1.5",
          "form": "서현진씨 나오는거",
          "original_form": "서현진씨 나오는거",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008635.1.6",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008635.1.7",
          "form": "보셨네요??",
          "original_form": "보셨네요??",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008635.1.8",
          "form": "드라마 어떤점이 좋았나요",
          "original_form": "드라마 어떤점이 좋았나요",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008635.1.9",
          "form": "제가 과거에 한 때 교사를 꿈꿨는데",
          "original_form": "제가 과거에 한 때 교사를 꿈꿨는데",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008635.1.10",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008635.1.11",
          "form": "교사도 쉬운 직업이 아니구나...를 느꼈어요 ㅋㅋㅋ",
          "original_form": "교사도 쉬운 직업이 아니구나...를 느꼈어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008635.1.12",
          "form": "그거 보면서 교사분들이 많이 공감한다 그러시더라구요",
          "original_form": "그거 보면서 교사분들이 많이 공감한다 그러시더라구요",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008635.1.13",
          "form": "맞아요 현실반영 많이 했다고 하더라구요",
          "original_form": "맞아요 현실반영 많이 했다고 하더라구요",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008635.1.14",
          "form": "최근에 드라마 다 보신 거 있나요?",
          "original_form": "최근에 드라마 다 보신 거 있나요?",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008635.1.15",
          "form": "저 라켓소년단 봤어요",
          "original_form": "저 라켓소년단 봤어요",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008635.1.16",
          "form": "오 그거 어때요???",
          "original_form": "오 그거 어때요???",
          "speaker_id": "2",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008635.1.17",
          "form": "성장드라마라 좋더라구요",
          "original_form": "성장드라마라 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008635.1.18",
          "form": "요즘 너무 자극적인게 판쳐서",
          "original_form": "요즘 너무 자극적인게 판쳐서",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008635.1.19",
          "form": "일상 속 작은 즐거움이 되었다랄까",
          "original_form": "일상 속 작은 즐거움이 되었다랄까",
