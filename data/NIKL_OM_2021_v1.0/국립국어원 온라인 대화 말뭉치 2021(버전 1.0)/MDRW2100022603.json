      "utterance": [
        {
          "id": "MDRW2100022603.1.1",
          "form": "그런데서 일하시는분들 기이한일이 많겠다ㅜㅜ",
          "original_form": "그런데서 일하시는분들 기이한일이 많겠다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022603.1.2",
          "form": "너도 심야괴담회봐 ㅋㅋ존잼임",
          "original_form": "너도 심야괴담회봐 ㅋㅋ존잼임",
          "speaker_id": "1",
          "time": "20210829 16:19"
        },
        {
          "id": "MDRW2100022603.1.3",
          "form": "으 듣기만해도 싫어 ㅜㅜ",
          "original_form": "으 듣기만해도 싫어 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022603.1.4",
          "form": "재연더싫어ㅜㅜ",
          "original_form": "재연더싫어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 16:20"
        },
        {
          "id": "MDRW2100022603.1.5",
          "form": "ㅋㅋㅋㅋㅋ 암튼 그 장례식장에서 무당을 불러서 보니까 여기 애 둘 어른 둘 있다하더래 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 암튼 그 장례식장에서 무당을 불러서 보니까 여기 애 둘 어른 둘 있다하더래 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100022603.1.6",
          "form": "헐 그쌍둥이?",
          "original_form": "헐 그쌍둥이?",
          "speaker_id": "2",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022603.1.7",
          "form": "ㅇㅇ 그리고 젊은나이에 죽어서 그 여자가 젊은여자만 보면 괴롭히는거라고 ...",
          "original_form": "ㅇㅇ 그리고 젊은나이에 죽어서 그 여자가 젊은여자만 보면 괴롭히는거라고 ...",
          "speaker_id": "1",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022603.1.8",
          "form": "ㅜㅜㅜㅜㅜ신비아파트를 많이봐서ㅋㅋㅋ",
          "original_form": "ㅜㅜㅜㅜㅜ신비아파트를 많이봐서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022603.1.9",
          "form": "가슴아픈 사연있는귀신을 기대했네ㅋㅋ",
          "original_form": "가슴아픈 사연있는귀신을 기대했네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022603.1.10",
          "form": "보면 별로 안타까운귀신보다는 그냥 나쁜귀신도많은듯 ㅋㅋ",
          "original_form": "보면 별로 안타까운귀신보다는 그냥 나쁜귀신도많은듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100022603.1.11",
          "form": "ㅋㅋ그런거보면 귀신이 진짜있나",
          "original_form": "ㅋㅋ그런거보면 귀신이 진짜있나",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022603.1.12",
          "form": "엠씨가 누구야?",
          "original_form": "엠씨가 누구야?",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022603.1.13",
          "form": "김구라 김숙 ㅋㅋㅋㅋ",
          "original_form": "김구라 김숙 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022603.1.14",
          "form": "김구라는 얘기안하고 김숙은 함 ㅋㅋ",
          "original_form": "김구라는 얘기안하고 김숙은 함 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022603.1.15",
          "form": "의외 전혀 귀신얘기 안믿을것같은 사람들이네z",
          "original_form": "의외 전혀 귀신얘기 안믿을것같은 사람들이네z",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022603.1.16",
          "form": "김구라는안믿는데 김숙은 믿음 ㅋㅋㅋ",
          "original_form": "김구라는안믿는데 김숙은 믿음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022603.1.17",
          "form": "그래? 일부러 기쎈사람들 뽑아서 하는건가ㅋㅋ",
          "original_form": "그래? 일부러 기쎈사람들 뽑아서 하는건가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022603.1.18",
          "form": "김숙안믿을것같은데 의외다ㅋㅋ",
          "original_form": "김숙안믿을것같은데 의외다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022603.1.19",
          "form": "원래 신동엽이 파일렀이었는데 무섭다고 그만뒀뎊ㅋㅋ",
          "original_form": "원래 신동엽이 파일렀이었는데 무섭다고 그만뒀뎊ㅋㅋ",
