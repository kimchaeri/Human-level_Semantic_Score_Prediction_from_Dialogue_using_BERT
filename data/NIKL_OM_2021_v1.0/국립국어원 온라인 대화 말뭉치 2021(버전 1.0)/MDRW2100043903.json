        },
        {
          "id": "MDRW2100043903.1.2",
          "form": "저는 아이돌음악이요",
          "original_form": "저는 아이돌음악이요",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043903.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043903.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043903.1.5",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043903.1.6",
          "form": "ㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043903.1.7",
          "form": "완전요",
          "original_form": "완전요",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043903.1.8",
          "form": "좋아하는 아이돌 있으신가요??",
          "original_form": "좋아하는 아이돌 있으신가요??",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043903.1.9",
          "form": "저~ 걸그룹 대부분 좋아해요",
          "original_form": "저~ 걸그룹 대부분 좋아해요",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043903.1.10",
          "form": "남돌은...아스트로..아실까나",
          "original_form": "남돌은...아스트로..아실까나",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043903.1.11",
          "form": "알죠ㅋㅋㅋㅋ",
          "original_form": "알죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043903.1.12",
          "form": "최애가 누구신가요?",
          "original_form": "최애가 누구신가요?",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043903.1.13",
          "form": "ㅋㅋㅋ아시군요",
          "original_form": "ㅋㅋㅋ아시군요",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043903.1.14",
          "form": "저는 문빈이요",
          "original_form": "저는 문빈이요",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043903.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043903.1.16",
          "form": "이유가 있을까요?",
          "original_form": "이유가 있을까요?",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043903.1.17",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043903.1.18",
          "form": "개구지게 웃는 모습에 치이고",
          "original_form": "개구지게 웃는 모습에 치이고",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043903.1.19",
          "form": "몸좋아서 치이고..?",
          "original_form": "몸좋아서 치이고..?",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043903.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043903.1.21",
          "form": "웃는 모습이 귀여우면 설레죠ㅋㅋㅋㅋ",
          "original_form": "웃는 모습이 귀여우면 설레죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043903.1.22",
          "form": "네네 ㅋㅋ 근데 춤도 잘춰서 어느순간 덕질하고있더라고요",
          "original_form": "네네 ㅋㅋ 근데 춤도 잘춰서 어느순간 덕질하고있더라고요",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043903.1.23",
          "form": "좋아하는 아이돌 있으세요?",
          "original_form": "좋아하는 아이돌 있으세요?",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043903.1.24",
          "form": "저는 아이유랑 엔시티요",
          "original_form": "저는 아이유랑 엔시티요",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043903.1.25",
          "form": "입덕 계기가 있을까요?",
          "original_form": "입덕 계기가 있을까요?",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043903.1.26",
          "form": "오 엔시티",
          "original_form": "오 엔시티",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100043903.1.27",
          "form": "저도 좋아했었어요 ㅎ문빈전에",
          "original_form": "저도 좋아했었어요 ㅎ문빈전에",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100043903.1.28",
          "form": "음... 그 전 덕질에 현타가와서",
          "original_form": "음... 그 전 덕질에 현타가와서",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100043903.1.29",
          "form": "갈아타볼까해서 다른아이돌보다가",
          "original_form": "갈아타볼까해서 다른아이돌보다가",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100043903.1.30",
          "form": "뽈인럽했죠",
          "original_form": "뽈인럽했죠",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100043903.1.31",
