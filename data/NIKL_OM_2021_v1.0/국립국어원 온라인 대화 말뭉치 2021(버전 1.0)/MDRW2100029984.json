      "utterance": [
        {
          "id": "MDRW2100029984.1.1",
          "form": "무슨 반려동물 키우고 싶어?",
          "original_form": "무슨 반려동물 키우고 싶어?",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029984.1.2",
          "form": "고양이~~",
          "original_form": "고양이~~",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029984.1.3",
          "form": "키우고 싶은 고양이 종류는 뭐야?",
          "original_form": "키우고 싶은 고양이 종류는 뭐야?",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100029984.1.4",
          "form": "당연히 모든 고앵아니겠니",
          "original_form": "당연히 모든 고앵아니겠니",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100029984.1.5",
          "form": "알록달록이는 무슨 동물 키우고싶어",
          "original_form": "알록달록이는 무슨 동물 키우고싶어",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029984.1.6",
          "form": "나는 강아지가 키우고싶어!",
          "original_form": "나는 강아지가 키우고싶어!",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029984.1.7",
          "form": "고양이겠지만",
          "original_form": "고양이겠지만",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029984.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029984.1.9",
          "form": "틀려버림",
          "original_form": "틀려버림",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029984.1.10",
          "form": "속단하기ㅋㅋㅋ",
          "original_form": "속단하기ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029984.1.11",
          "form": "왜 강아지야?",
          "original_form": "왜 강아지야?",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029984.1.12",
          "form": "강아지는 코가 촉촉해서 귀여워",
          "original_form": "강아지는 코가 촉촉해서 귀여워",
          "speaker_id": "1",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100029984.1.13",
          "form": "너는 왜 고양이야?",
          "original_form": "너는 왜 고양이야?",
          "speaker_id": "1",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100029984.1.14",
          "form": "고양이는 흐물흐물거리는게 귀여워",
          "original_form": "고양이는 흐물흐물거리는게 귀여워",
          "speaker_id": "2",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100029984.1.15",
          "form": "강아지는 단단하대",
          "original_form": "강아지는 단단하대",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100029984.1.16",
          "form": "고양이를 키우게된다면 언제 키울것같아?",
          "original_form": "고양이를 키우게된다면 언제 키울것같아?",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100029984.1.17",
          "form": "강아지는 묵직하더라",
          "original_form": "강아지는 묵직하더라",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100029984.1.18",
          "form": "아마도... 안키우지않을까",
          "original_form": "아마도... 안키우지않을까",
          "speaker_id": "2",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100029984.1.19",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100029984.1.20",
          "form": "내가 집에 없는동안 혼자 있어야하는걸?",
          "original_form": "내가 집에 없는동안 혼자 있어야하는걸?",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100029984.1.21",
          "form": "맞아 그건 생각 못했다 정말 생각이 깊다",
          "original_form": "맞아 그건 생각 못했다 정말 생각이 깊다",
          "speaker_id": "1",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029984.1.22",
          "form": "나는 강아지를 아파트에서 못 길러서 못 키울 것 같아",
          "original_form": "나는 강아지를 아파트에서 못 길러서 못 키울 것 같아",
          "speaker_id": "1",
          "time": "20210903 17:51"
        }
      ]
