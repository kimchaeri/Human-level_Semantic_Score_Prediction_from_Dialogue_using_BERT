      "utterance": [
        {
          "id": "MDRW2100001797.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100001797.1.2",
          "form": "넌 무슨 공연 가봤어?",
          "original_form": "넌 무슨 공연 가봤어?",
          "speaker_id": "1",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100001797.1.3",
          "form": "공연의 범위가 너무 큰데...ㅋㅋ 제일 최근에 본건 뮤지컬!",
          "original_form": "공연의 범위가 너무 큰데...ㅋㅋ 제일 최근에 본건 뮤지컬!",
          "speaker_id": "2",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001797.1.4",
          "form": "뮤지컬 좋아해?",
          "original_form": "뮤지컬 좋아해?",
          "speaker_id": "2",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001797.1.5",
          "form": "기회가 있다면 보는 건 좋아해",
          "original_form": "기회가 있다면 보는 건 좋아해",
          "speaker_id": "1",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001797.1.6",
          "form": "난 주로 콘서트 갔었어",
          "original_form": "난 주로 콘서트 갔었어",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001797.1.7",
          "form": "요즘은 코로나로 밖에 못 나가서 제한이 많아",
          "original_form": "요즘은 코로나로 밖에 못 나가서 제한이 많아",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001797.1.8",
          "form": "맞아, 요즘은 코로나때문에 아이돌들은 콘서트 안 하더라ㅠ",
          "original_form": "맞아, 요즘은 코로나때문에 아이돌들은 콘서트 안 하더라ㅠ",
          "speaker_id": "2",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001797.1.9",
          "form": "미스터트롯이랑 나훈아는 열심히 콘서트 돌던데",
          "original_form": "미스터트롯이랑 나훈아는 열심히 콘서트 돌던데",
          "speaker_id": "2",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001797.1.10",
          "form": "대부분 온라인 콘서트더라",
          "original_form": "대부분 온라인 콘서트더라",
          "speaker_id": "1",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001797.1.11",
          "form": "정말 밖에서 예전처럼 가고싶어",
          "original_form": "정말 밖에서 예전처럼 가고싶어",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001797.1.12",
          "form": "넌 무슨 뮤지컬 봤어?",
          "original_form": "넌 무슨 뮤지컬 봤어?",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001797.1.13",
          "form": "제일 최근 본건 엑스칼리버!",
          "original_form": "제일 최근 본건 엑스칼리버!",
          "speaker_id": "2",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001797.1.14",
          "form": "무슨 내용이야?",
          "original_form": "무슨 내용이야?",
          "speaker_id": "1",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001797.1.15",
          "form": "아서왕과 원탁의 기사들 알아?",
          "original_form": "아서왕과 원탁의 기사들 알아?",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001797.1.16",
          "form": "옛날 고전 소설인건 알아",
          "original_form": "옛날 고전 소설인건 알아",
          "speaker_id": "1",
          "time": "20210819 21:45"
        },
        {
          "id": "MDRW2100001797.1.17",
          "form": "자세한건 까먹었지만...",
          "original_form": "자세한건 까먹었지만...",
          "speaker_id": "1",
          "time": "20210819 21:45"
        },
        {
          "id": "MDRW2100001797.1.18",
          "form": "영국 전설인데, 뮤지컬내용은 대충 아서가 엑스칼리버 뽑고 왕되서 이교도 색슨족이랑 싸우는 거야.",
          "original_form": "영국 전설인데, 뮤지컬내용은 대충 아서가 엑스칼리버 뽑고 왕되서 이교도 색슨족이랑 싸우는 거야.",
          "speaker_id": "2",
          "time": "20210819 21:49"
        },
        {
          "id": "MDRW2100001797.1.19",
          "form": "엑스칼리버 검 이름이구나",
          "original_form": "엑스칼리버 검 이름이구나",
          "speaker_id": "1",
          "time": "20210819 21:50"
        },
        {
          "id": "MDRW2100001797.1.20",
          "form": "응!",
          "original_form": "응!",
          "speaker_id": "2",
          "time": "20210819 21:50"
        },
        {
          "id": "MDRW2100001797.1.21",
          "form": "뮤지컬이든 콘서트든 직접 현장에서 보는 맛이 있어",
          "original_form": "뮤지컬이든 콘서트든 직접 현장에서 보는 맛이 있어",
          "speaker_id": "1",
          "time": "20210819 21:50"
        },
        {
          "id": "MDRW2100001797.1.22",
          "form": "근데 다 필요없고 모르가나가 부르는 '아비의 죄'라는 넘버 꼭 들어봐",
          "original_form": "근데 다 필요없고 모르가나가 부르는 '아비의 죄'라는 넘버 꼭 들어봐",
          "speaker_id": "2",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001797.1.23",
          "form": "집에서 티비로 보는 거랑 달라",
          "original_form": "집에서 티비로 보는 거랑 달라",
          "speaker_id": "1",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001797.1.24",
          "form": "한 장면이야?",
          "original_form": "한 장면이야?",
          "speaker_id": "1",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001797.1.25",
          "form": "현장에서 듣는게 백배천배 더 멋있긴한데 진짜 너무 멋져.",
          "original_form": "현장에서 듣는게 백배천배 더 멋있긴한데 진짜 너무 멋져.",
          "speaker_id": "2",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001797.1.26",
          "form": "장면은 장면이지..!아비의 죄라는 노래를 부르는 장면...?",
          "original_form": "장면은 장면이지..!아비의 죄라는 노래를 부르는 장면...?",
          "speaker_id": "2",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001797.1.27",
          "form": "아하 좋아하는 부분 현장감이 최고야",
          "original_form": "아하 좋아하는 부분 현장감이 최고야",
          "speaker_id": "1",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001797.1.28",
          "form": "나중에 유튜브로 찾아봐야겠다",
          "original_form": "나중에 유튜브로 찾아봐야겠다",
          "speaker_id": "1",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001797.1.29",
          "form": "진짜 유튜브 영상도 봤는데 그 감동의 반도 못담았지만 진짜 카리스마가 너무 멋지니까 추천해!",
          "original_form": "진짜 유튜브 영상도 봤는데 그 감동의 반도 못담았지만 진짜 카리스마가 너무 멋지니까 추천해!",
          "speaker_id": "2",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001797.1.30",
          "form": "공연장 안에 울려퍼지는 음성이 장난 아니겠다",
          "original_form": "공연장 안에 울려퍼지는 음성이 장난 아니겠다",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001797.1.31",
          "form": "역시 공연은 직접 가봐야해",
          "original_form": "역시 공연은 직접 가봐야해",
          "speaker_id": "1",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001797.1.32",
          "form": "멋진 취미야",
          "original_form": "멋진 취미야",
          "speaker_id": "1",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001797.1.33",
          "form": "응 멋진 취미야",
          "original_form": "응 멋진 취미야",
          "speaker_id": "2",
          "time": "20210819 21:57"
        }
      ]
    }
  ]
