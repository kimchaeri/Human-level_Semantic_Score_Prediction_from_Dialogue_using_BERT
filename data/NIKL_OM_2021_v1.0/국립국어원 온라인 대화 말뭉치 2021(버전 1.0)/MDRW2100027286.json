      "utterance": [
        {
          "id": "MDRW2100027286.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 11:08"
        },
        {
          "id": "MDRW2100027286.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027286.1.3",
          "form": "최근 보신 영화 있으신가요",
          "original_form": "최근 보신 영화 있으신가요",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027286.1.4",
          "form": "제일 최근에 본 영화는 블랙위도우에요",
          "original_form": "제일 최근에 본 영화는 블랙위도우에요",
          "speaker_id": "1",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027286.1.5",
          "form": "샤오루님은 최근 영화 보러 가신 적 있나요?",
          "original_form": "샤오루님은 최근 영화 보러 가신 적 있나요?",
          "speaker_id": "1",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100027286.1.6",
          "form": "저는 싱크홀 봤어요",
          "original_form": "저는 싱크홀 봤어요",
          "speaker_id": "2",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027286.1.7",
          "form": "그거는 못봤어요 ㅜㅜ",
          "original_form": "그거는 못봤어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027286.1.8",
          "form": "싱크홀은 어떤 영화인가요?",
          "original_form": "싱크홀은 어떤 영화인가요?",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027286.1.9",
          "form": "슬플줄 알았는데",
          "original_form": "슬플줄 알았는데",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027286.1.10",
          "form": "이광수 때문에 쫌 웃겨요 ㅋㅋ",
          "original_form": "이광수 때문에 쫌 웃겨요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027286.1.11",
          "form": "차승원이랑",
          "original_form": "차승원이랑",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100027286.1.12",
          "form": "그렇군요! 웃기다고 하니 조금 볼 용기가 나네요",
          "original_form": "그렇군요! 웃기다고 하니 조금 볼 용기가 나네요",
          "speaker_id": "1",
          "time": "20210902 11:26"
        },
        {
          "id": "MDRW2100027286.1.13",
          "form": "잔인하거나 슬프고 절망적인 장면을 잘 못 봐서 한국 영화 많이 못봤어요",
          "original_form": "잔인하거나 슬프고 절망적인 장면을 잘 못 봐서 한국 영화 많이 못봤어요",
          "speaker_id": "1",
          "time": "20210902 11:27"
        },
        {
          "id": "MDRW2100027286.1.14",
          "form": "조금 슬퍼서 눈물 한방울 흘렸다는 ㅋㅋㅋㅋ",
          "original_form": "조금 슬퍼서 눈물 한방울 흘렸다는 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027286.1.15",
          "form": "저는 신파 장면에서 수도꼭지 틀듯 울거든요",
          "original_form": "저는 신파 장면에서 수도꼭지 틀듯 울거든요",
          "speaker_id": "1",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027286.1.16",
          "form": "7번방의 선물 보고 눈물 날때는 좀 분했어요ㅋㅋ",
          "original_form": "7번방의 선물 보고 눈물 날때는 좀 분했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027286.1.17",
          "form": "겨우 이렇게 뻔한 것에 눈물이 나다니...",
          "original_form": "겨우 이렇게 뻔한 것에 눈물이 나다니...",
          "speaker_id": "1",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027286.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ 저도",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저도",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027286.1.19",
          "form": "울보 그자체...",
          "original_form": "울보 그자체...",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027286.1.20",
          "form": "눈물이 많으시군요",
          "original_form": "눈물이 많으시군요",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100027286.1.21",
          "form": "저처럼",
          "original_form": "저처럼",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100027286.1.22",
          "form": "한국영화 특유의 꿉꿉한 스토리 있잖아요 불쌍한 약자가 당하는 장면... 그런걸 보면 며칠간 기분이 너무 울적하더라고요",
          "original_form": "한국영화 특유의 꿉꿉한 스토리 있잖아요 불쌍한 약자가 당하는 장면... 그런걸 보면 며칠간 기분이 너무 울적하더라고요",
          "speaker_id": "1",
          "time": "20210902 11:34"
        },
        {
          "id": "MDRW2100027286.1.23",
          "form": "좀 가볍긴 해도 마블영화같은게 기분 전환하기 좋은 것 같아요",
          "original_form": "좀 가볍긴 해도 마블영화같은게 기분 전환하기 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027286.1.24",
          "form": "그 추격자",
          "original_form": "그 추격자",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027286.1.25",
          "form": "너무 찜찝했다는 ㅠㅠ",
          "original_form": "너무 찜찝했다는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027286.1.26",
          "form": "최근 샹치 개봉하던데!!",
          "original_form": "최근 샹치 개봉하던데!!",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027286.1.27",
          "form": "네 어제였던것으로 알고있어요",
          "original_form": "네 어제였던것으로 알고있어요",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027286.1.28",
          "form": "재밌을것같나요?",
          "original_form": "재밌을것같나요?",
          "speaker_id": "2",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027286.1.29",
          "form": "내일 연차를 내놔서 조조로 보러갈까 고민중입니다",
          "original_form": "내일 연차를 내놔서 조조로 보러갈까 고민중입니다",
          "speaker_id": "1",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100027286.1.30",
          "form": "오 굿 3일연속 쉬시는 건가요 ㅎㅎ",
          "original_form": "오 굿 3일연속 쉬시는 건가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 11:40"
        }
      ]
