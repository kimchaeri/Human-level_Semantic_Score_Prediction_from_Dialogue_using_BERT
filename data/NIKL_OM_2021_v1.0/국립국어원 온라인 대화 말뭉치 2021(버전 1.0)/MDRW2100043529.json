      "utterance": [
        {
          "id": "MDRW2100043529.1.1",
          "form": "저는 포카리요",
          "original_form": "저는 포카리요",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043529.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043529.1.3",
          "form": "저는 데미소다 레몬맛 좋아해요!",
          "original_form": "저는 데미소다 레몬맛 좋아해요!",
          "speaker_id": "1",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043529.1.4",
          "form": "잘 안파늨데",
          "original_form": "잘 안파늨데",
          "speaker_id": "1",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043529.1.5",
          "form": "팔면 환장하구사용 ㅎㅎ",
          "original_form": "팔면 환장하구사용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043529.1.6",
          "form": "탄산수인가요?",
          "original_form": "탄산수인가요?",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043529.1.7",
          "form": "탄산음료입니다 ㅎㅎ",
          "original_form": "탄산음료입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043529.1.8",
          "form": "그럼 단 음료 인가요?",
          "original_form": "그럼 단 음료 인가요?",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043529.1.9",
          "form": "네 달아요",
          "original_form": "네 달아요",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043529.1.10",
          "form": "name1님은 좋아하는 음식 있으신가요?",
          "original_form": "&name1&님은 좋아하는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043529.1.11",
          "form": "음~ 최근에먹은거중에 생각나는건 육개장이요!",
          "original_form": "음~ 최근에먹은거중에 생각나는건 육개장이요!",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043529.1.12",
          "form": "라면이요 아님 진짜 육개장이요?",
          "original_form": "라면이요 아님 진짜 육개장이요?",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043529.1.13",
          "form": "진짜 육개장이욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 육개장이욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043529.1.14",
          "form": "라면은 생각도못햤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "라면은 생각도못햤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043529.1.15",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043529.1.16",
          "form": "라면 육개장이 인생이신분이 많아서ㅋㅋㅋ",
          "original_form": "라면 육개장이 인생이신분이 많아서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043529.1.17",
          "form": "ㅋㅋㅋ 그럴 수도 있죠 ㅎㅎㅎㅎㅎ",
          "original_form": "ㅋㅋㅋ 그럴 수도 있죠 ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043529.1.18",
          "form": "님은요?? 뭐좋아하세요?",
