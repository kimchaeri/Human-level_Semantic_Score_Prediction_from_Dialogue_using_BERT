          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017315.1.3",
          "form": "반려동물 키워?",
          "original_form": "반려동물 키워?",
          "speaker_id": "1",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017315.1.4",
          "form": "안키워 넌?",
          "original_form": "안키워 넌?",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017315.1.5",
          "form": "난 강아지 두마리",
          "original_form": "난 강아지 두마리",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017315.1.6",
          "form": "우와ㅠㅠ",
          "original_form": "우와ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017315.1.7",
          "form": "치와와 두마리야",
          "original_form": "치와와 두마리야",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017315.1.8",
          "form": "나이들어서 시간여유되면 꼭 키울꺼야",
          "original_form": "나이들어서 시간여유되면 꼭 키울꺼야",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017315.1.9",
          "form": "많이 바쁜가 보구나",
          "original_form": "많이 바쁜가 보구나",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017315.1.10",
          "form": "바쁘진않은데 자취중이라 출퇴근할때",
          "original_form": "바쁘진않은데 자취중이라 출퇴근할때",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017315.1.11",
          "form": "강아지혼자 심심해서 안대",
          "original_form": "강아지혼자 심심해서 안대",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017315.1.12",
          "form": "요즘은 애견유치원도 있잖아",
          "original_form": "요즘은 애견유치원도 있잖아",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017315.1.13",
          "form": "난 출근할때 강아지들이랑 같이 출근해",
          "original_form": "난 출근할때 강아지들이랑 같이 출근해",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017315.1.14",
          "form": "헉 난 그렇게 할 여건이 안됨",
          "original_form": "헉 난 그렇게 할 여건이 안됨",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017315.1.15",
          "form": "보통 그렇지 강아지 키우려면 책임감이 필요한건 맞아",
          "original_form": "보통 그렇지 강아지 키우려면 책임감이 필요한건 맞아",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017315.1.16",
          "form": "시간여유가 생길때는 꼭 키우고싶어",
          "original_form": "시간여유가 생길때는 꼭 키우고싶어",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017315.1.17",
          "form": "귀여워",
          "original_form": "귀여워",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017315.1.18",
          "form": "완전 사랑스럽지 치와와 추천",
          "original_form": "완전 사랑스럽지 치와와 추천",
          "speaker_id": "1",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017315.1.19",
          "form": "댕댕이들 몇살이야?",
          "original_form": "댕댕이들 몇살이야?",
          "speaker_id": "2",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100017315.1.20",
          "form": "5살 9살 노견이야",
          "original_form": "5살 9살 노견이야",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017315.1.21",
          "form": "치와와들이 첫 반려견?",
          "original_form": "치와와들이 첫 반려견?",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017315.1.22",
          "form": "아니 어릴때부터 키웠어",
          "original_form": "아니 어릴때부터 키웠어",
          "speaker_id": "1",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017315.1.23",
