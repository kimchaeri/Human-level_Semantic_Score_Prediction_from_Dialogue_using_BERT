      "utterance": [
        {
          "id": "MDRW2100024015.1.1",
          "form": "카라멜쉐이크?가 맛있더라구요",
          "original_form": "카라멜쉐이크?가 맛있더라구요",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024015.1.2",
          "form": "오 저는 밀크만 먹어봤는데",
          "original_form": "오 저는 밀크만 먹어봤는데",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024015.1.3",
          "form": "캐러멜은 무슨 맛인가요?",
          "original_form": "캐러멜은 무슨 맛인가요?",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024015.1.4",
          "form": "가격이 5900원인가 그랬던것같아용",
          "original_form": "가격이 5900원인가 그랬던것같아용",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024015.1.5",
          "form": "그 약간 짠맛보다는",
          "original_form": "그 약간 짠맛보다는",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024015.1.6",
          "form": "달달한맛이 더 풍부한",
          "original_form": "달달한맛이 더 풍부한",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024015.1.7",
          "form": "카라멜맛이었어요",
          "original_form": "카라멜맛이었어요",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024015.1.8",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024015.1.9",
          "form": "다음에 가봐야겠네요",
          "original_form": "다음에 가봐야겠네요",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024015.1.10",
          "form": "버거리버거 아세요?",
          "original_form": "버거리버거 아세요?",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024015.1.11",
          "form": "거기도 맛있어요 ㅎㄹ",
          "original_form": "거기도 맛있어요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024015.1.12",
          "form": "오 처음들어봐요!",
          "original_form": "오 처음들어봐요!",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024015.1.13",
          "form": "여기도 수제버거집인가요?",
          "original_form": "여기도 수제버거집인가요?",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024015.1.14",
          "form": "프랜차이즈인데 수제버거집이에요",
          "original_form": "프랜차이즈인데 수제버거집이에요",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024015.1.15",
          "form": "꼭 기억할게요 프랜차이즈라니",
          "original_form": "꼭 기억할게요 프랜차이즈라니",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024015.1.16",
          "form": "가까운곳이 있었으면...",
          "original_form": "가까운곳이 있었으면...",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024015.1.17",
          "form": "저는 저녁을 일찍 먹어서 그런지 살짝 출출해요ㅋㅋㅋ",
          "original_form": "저는 저녁을 일찍 먹어서 그런지 살짝 출출해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024015.1.18",
          "form": "얼마나 일찍 드셨길래..?!!",
          "original_form": "얼마나 일찍 드셨길래..?!!",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100024015.1.19",
          "form": "저는 늦게 먹었어요",
          "original_form": "저는 늦게 먹었어요",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100024015.1.20",
          "form": "5시조금 안됐던것같아요 허허",
          "original_form": "5시조금 안됐던것같아요 허허",
          "speaker_id": "1",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100024015.1.21",
          "form": "치맥을 먹을까말까 고민중이에요",
          "original_form": "치맥을 먹을까말까 고민중이에요",
          "speaker_id": "1",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100024015.1.22",
          "form": "치맥 좋죠",
          "original_form": "치맥 좋죠",
          "speaker_id": "2",
          "time": "20210830 21:20"
        },
        {
          "id": "MDRW2100024015.1.23",
          "form": "어느 브랜드 치킨 좋아하세요?",
          "original_form": "어느 브랜드 치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024015.1.24",
          "form": "저는 치킨매니아 새우치킨 좋아하는데",
          "original_form": "저는 치킨매니아 새우치킨 좋아하는데",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024015.1.25",
          "form": "전 교촌이었는데, 요즘은 푸라닭,,,",
          "original_form": "전 교촌이었는데, 요즘은 푸라닭,,,",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024015.1.26",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100024015.1.27",
          "form": "오 푸라닭 블랙 어쩌고하는 메뉴였는데 맛있더라구요",
          "original_form": "오 푸라닭 블랙 어쩌고하는 메뉴였는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210830 21:21"
        }
      ]
    }
