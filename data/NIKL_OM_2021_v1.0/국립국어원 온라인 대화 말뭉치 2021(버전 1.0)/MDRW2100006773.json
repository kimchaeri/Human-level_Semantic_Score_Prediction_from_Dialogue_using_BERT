      "utterance": [
        {
          "id": "MDRW2100006773.1.1",
          "form": "오 그런 방법이있네요 ㅋㅋㅋㅋ",
          "original_form": "오 그런 방법이있네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006773.1.2",
          "form": "그럼 혹시",
          "original_form": "그럼 혹시",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006773.1.3",
          "form": "국물라면 말고 비빔면도 좋아하시나요??",
          "original_form": "국물라면 말고 비빔면도 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006773.1.4",
          "form": "저는 비빔면을 진짜 좋아해서...",
          "original_form": "저는 비빔면을 진짜 좋아해서...",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006773.1.5",
          "form": "와 너무 좋아해욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 너무 좋아해욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006773.1.6",
          "form": "팔도!",
          "original_form": "팔도!",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006773.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006773.1.8",
          "form": "팔도비빔면 맛있죠..",
          "original_form": "팔도비빔면 맛있죠..",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006773.1.9",
          "form": "양이 좀 더 많았으면 좋겠어요",
          "original_form": "양이 좀 더 많았으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100006773.1.10",
          "form": "네네 양이 좀 적어서 ㅜㅜ 다른거랑 같이먹어요 ㅋㅋ",
          "original_form": "네네 양이 좀 적어서 ㅜㅜ 다른거랑 같이먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100006773.1.11",
          "form": "보통 주로 뭐랑 드세요??",
          "original_form": "보통 주로 뭐랑 드세요??",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100006773.1.12",
          "form": "저는 주로 만두 구워서 같이 먹어요!",
          "original_form": "저는 주로 만두 구워서 같이 먹어요!",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100006773.1.13",
          "form": "오오 저는 삼김이나 치킨..? 그런걸로 배채워요 ㅋㅋㅋㅋ",
          "original_form": "오오 저는 삼김이나 치킨..? 그런걸로 배채워요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100006773.1.14",
          "form": "비빔면이랑 삼각김밥을 먹어본 적은 없는 거 같네요",
          "original_form": "비빔면이랑 삼각김밥을 먹어본 적은 없는 거 같네요",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100006773.1.15",
          "form": "삼김 무슨맛 좋아하세요",
          "original_form": "삼김 무슨맛 좋아하세요",
          "speaker_id": "1",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100006773.1.16",
          "form": "저 닭갈비맛이요!",
          "original_form": "저 닭갈비맛이요!",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100006773.1.17",
          "form": "닭갈비를 좋아해서 ㅋㅋㅋ",
          "original_form": "닭갈비를 좋아해서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100006773.1.18",
          "form": "우아 저는 참치마요요!!!!",
          "original_form": "우아 저는 참치마요요!!!!",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100006773.1.19",
          "form": "삼김이랑 라면이랑 잘어울리는거가탕여",
          "original_form": "삼김이랑 라면이랑 잘어울리는거가탕여",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100006773.1.20",
          "form": "참치마요도 맛있죠",
          "original_form": "참치마요도 맛있죠",
          "speaker_id": "2",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100006773.1.21",
          "form": "저는 맨날 국물라면이랑만 먹어봤네요",
          "original_form": "저는 맨날 국물라면이랑만 먹어봤네요",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100006773.1.22",
          "form": "진짬뽕 좋아하세요??",
          "original_form": "진짬뽕 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100006773.1.23",
          "form": "오오 예전에 자주 먹었어요!!",
          "original_form": "오오 예전에 자주 먹었어요!!",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100006773.1.24",
          "form": "진짬뽕 너무 맛있지 않던가요",
          "original_form": "진짬뽕 너무 맛있지 않던가요",
          "speaker_id": "2",
