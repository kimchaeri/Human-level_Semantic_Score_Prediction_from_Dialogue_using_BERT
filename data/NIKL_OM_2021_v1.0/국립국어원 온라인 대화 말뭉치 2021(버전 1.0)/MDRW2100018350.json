      "utterance": [
        {
          "id": "MDRW2100018350.1.1",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 00:08"
        },
        {
          "id": "MDRW2100018350.1.2",
          "form": "책은 좋아는 하는데 요즘엔 통 못봤습니다",
          "original_form": "책은 좋아는 하는데 요즘엔 통 못봤습니다",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018350.1.3",
          "form": "책을 좋아하시나요?",
          "original_form": "책을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018350.1.4",
          "form": "네 저도 책 좋아해요ㅎㅎㅎ 잘 안보지만...",
          "original_form": "네 저도 책 좋아해요ㅎㅎㅎ 잘 안보지만...",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018350.1.5",
          "form": "어떤 장르 좋아하세요?",
          "original_form": "어떤 장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018350.1.6",
          "form": "저는 소설좋아했었어요ㅋ",
          "original_form": "저는 소설좋아했었어요ㅋ",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018350.1.7",
          "form": "진짜 안읽은지 오래 됐네요",
          "original_form": "진짜 안읽은지 오래 됐네요",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018350.1.8",
          "form": "아 저도 소설이 젤 잘읽히더라구요ㅋㅋㅋ",
          "original_form": "아 저도 소설이 젤 잘읽히더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018350.1.9",
          "form": "제일 좋아하는 책이 뭐에요?",
          "original_form": "제일 좋아하는 책이 뭐에요?",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018350.1.10",
          "form": "저는 개미요",
          "original_form": "저는 개미요",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018350.1.11",
          "form": "베르나르베르베르",
          "original_form": "베르나르베르베르",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018350.1.12",
          "form": "한참 좋아했었어요",
          "original_form": "한참 좋아했었어요",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018350.1.13",
          "form": "아 저두요ㅋㅋㅋ 저는 신 시리즈 제일 좋아했어요",
          "original_form": "아 저두요ㅋㅋㅋ 저는 신 시리즈 제일 좋아했어요",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018350.1.14",
          "form": "오 저는 신부터 안봤는데",
          "original_form": "오 저는 신부터 안봤는데",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018350.1.15",
          "form": "진심 봐야겠네요",
          "original_form": "진심 봐야겠네요",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018350.1.16",
          "form": "네네 꼭 보세요 발상이 너무 창의적이에요ㅋㅋㅋ",
          "original_form": "네네 꼭 보세요 발상이 너무 창의적이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018350.1.17",
          "form": "그전건 다봤어여",
          "original_form": "그전건 다봤어여",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018350.1.18",
          "form": "파피용이 그 전건가요?",
          "original_form": "파피용이 그 전건가요?",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018350.1.19",
          "form": "신 이후에 나왔나?",
          "original_form": "신 이후에 나왔나?",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018350.1.20",
          "form": "파피용도 안본거 같아여ㅋㅋ",
          "original_form": "파피용도 안본거 같아여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018350.1.21",
          "form": "파피용도 재미있게 읽었었어요",
          "original_form": "파피용도 재미있게 읽었었어요",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018350.1.22",
          "form": "성인되고 책을 많이 안보게 되더라구여",
          "original_form": "성인되고 책을 많이 안보게 되더라구여",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018350.1.23",
          "form": "책을 좋아하시는군요",
          "original_form": "책을 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018350.1.24",
          "form": "아 저도 성인 된 다음부터 책이랑 멀어지더라구요..",
          "original_form": "아 저도 성인 된 다음부터 책이랑 멀어지더라구요..",
          "speaker_id": "1",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018350.1.25",
          "form": "스마트폰이 너무 재미있어서..ㅠㅠ",
          "original_form": "스마트폰이 너무 재미있어서..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018350.1.26",
          "form": "밀리의서재 이런거",
          "original_form": "밀리의서재 이런거",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018350.1.27",
          "form": "안보셨었나요?",
          "original_form": "안보셨었나요?",
          "speaker_id": "2",
          "time": "20210827 13:44"
        }
      ]
    }
