      "utterance": [
        {
          "id": "MDRW2100042789.1.1",
          "form": "오래쓰셨나요??",
          "original_form": "오래쓰셨나요??",
          "speaker_id": "1",
          "time": "20210907 09:09"
        },
        {
          "id": "MDRW2100042789.1.2",
          "form": "지금",
          "original_form": "지금",
          "speaker_id": "2",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042789.1.3",
          "form": "딱 이년됏어요",
          "original_form": "딱 이년됏어요",
          "speaker_id": "2",
          "time": "20210907 09:10"
        },
        {
          "id": "MDRW2100042789.1.4",
          "form": "아 저는 조금더 쓴것같은데ㅠㅠㅠ 너무 비싸니 ㅠㅠ",
          "original_form": "아 저는 조금더 쓴것같은데ㅠㅠㅠ 너무 비싸니 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042789.1.5",
          "form": "맞아욤ㅜㅜ근데",
          "original_form": "맞아욤ㅜㅜ근데",
          "speaker_id": "2",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042789.1.6",
          "form": "쓸떼없이 넘 이뻐서",
          "original_form": "쓸떼없이 넘 이뻐서",
          "speaker_id": "2",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042789.1.7",
          "form": "자꾸 눈에들어와요",
          "original_form": "자꾸 눈에들어와요",
          "speaker_id": "2",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042789.1.8",
          "form": "맞아여 ㅠㅠ 사이즈도 글코.. 케이스도 이쁘고",
          "original_form": "맞아여 ㅠㅠ 사이즈도 글코.. 케이스도 이쁘고",
          "speaker_id": "1",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042789.1.9",
          "form": "근데 그 접히는 흰지 부분이 불량이 꽤 있다더라구요",
          "original_form": "근데 그 접히는 흰지 부분이 불량이 꽤 있다더라구요",
          "speaker_id": "1",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042789.1.10",
          "form": "네네ㅋㅋ그래서",
          "original_form": "네네ㅋㅋ그래서",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042789.1.11",
          "form": "일년만더 존버해야지 하믄서",
          "original_form": "일년만더 존버해야지 하믄서",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042789.1.12",
          "form": "참고잇어요",
          "original_form": "참고잇어요",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042789.1.13",
          "form": "플립4!!!!!",
          "original_form": "플립4!!!!!",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042789.1.14",
          "form": "아 자꾸 나오다보면 더 좋은게 나오겠죠?? ㅎㅎ",
          "original_form": "아 자꾸 나오다보면 더 좋은게 나오겠죠?? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042789.1.15",
          "form": "네네!!!!!!!",
          "original_form": "네네!!!!!!!",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042789.1.16",
          "form": "배터리랑 액정 초큼 더 개선되겟쥬",
          "original_form": "배터리랑 액정 초큼 더 개선되겟쥬",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042789.1.17",
          "form": "맞아용 ㅎㅎ 배터리도 문제있데요??",
          "original_form": "맞아용 ㅎㅎ 배터리도 문제있데요??",
          "speaker_id": "1",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042789.1.18",
          "form": "배터리용량이 좀 작데요",
          "original_form": "배터리용량이 좀 작데요",
          "speaker_id": "2",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042789.1.19",
          "form": "쓰다보면 더 작게 느껴진댓어유",
          "original_form": "쓰다보면 더 작게 느껴진댓어유",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042789.1.20",
          "form": "아 ㅠㅠ 많이쓰는데ㅠㅠ 전 일체형보다 차라리 교체형이 더 좋은것 같아여 여분으로 갈아쓰고 좋은데 왜 ㅠㅠ",
          "original_form": "아 ㅠㅠ 많이쓰는데ㅠㅠ 전 일체형보다 차라리 교체형이 더 좋은것 같아여 여분으로 갈아쓰고 좋은데 왜 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042789.1.21",
          "form": "아 진짜ㅜㅜㅜㅜㅜ",
          "original_form": "아 진짜ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042789.1.22",
          "form": "제말이여ㅠㅠ",
          "original_form": "제말이여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042789.1.23",
          "form": "저는 손목이안좋아서",
          "original_form": "저는 손목이안좋아서",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042789.1.24",
          "form": "더 가벼워졋음좋겟어요 흑",
          "original_form": "더 가벼워졋음좋겟어요 흑",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042789.1.25",
          "form": "가끔씩 핸드폰 먹통될때 밧데리빼면 만능인데",
          "original_form": "가끔씩 핸드폰 먹통될때 밧데리빼면 만능인데",
          "speaker_id": "1",
          "time": "20210907 09:26"
        },
        {
