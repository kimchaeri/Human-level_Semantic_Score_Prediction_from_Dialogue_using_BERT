      "utterance": [
        {
          "id": "MDRW2100003900.1.1",
          "form": "아직 바람은 잘 나오잖아",
          "original_form": "아직 바람은 잘 나오잖아",
          "speaker_id": "2",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003900.1.2",
          "form": "회전 안하고 바람 계속 쐬면 추울텐데",
          "original_form": "회전 안하고 바람 계속 쐬면 추울텐데",
          "speaker_id": "1",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003900.1.3",
          "form": "난 상관없어 ㅎㅎ",
          "original_form": "난 상관없어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003900.1.4",
          "form": "ㅎㅎ 버리기 싫어서 그런건 아니고?",
          "original_form": "ㅎㅎ 버리기 싫어서 그런건 아니고?",
          "speaker_id": "1",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003900.1.5",
          "form": "아냐 ㅋ",
          "original_form": "아냐 ㅋ",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003900.1.6",
          "form": "난 원래 회전 안써",
          "original_form": "난 원래 회전 안써",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003900.1.7",
          "form": "난 회전 없으면 안돼ㅠ 선풍기 바람 계속 쐬면 담날 바로 감기",
          "original_form": "난 회전 없으면 안돼ㅠ 선풍기 바람 계속 쐬면 담날 바로 감기",
          "speaker_id": "1",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003900.1.8",
          "form": "ㅇㅇ 새로 산 거는 색시가 써",
          "original_form": "ㅇㅇ 새로 산 거는 색시가 써",
          "speaker_id": "2",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003900.1.9",
          "form": "우리 집에 도대체 선풍기가 몇대야 ㅋㅋㅋ",
          "original_form": "우리 집에 도대체 선풍기가 몇대야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003900.1.10",
          "form": "고장난 것까지 6대?",
          "original_form": "고장난 것까지 6대?",
          "speaker_id": "2",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003900.1.11",
          "form": "훌 선풍기 장사해야겠네 ㅎㅎ",
          "original_form": "훌 선풍기 장사해야겠네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003900.1.12",
          "form": "내가 다 쓸테니 걱정마",
          "original_form": "내가 다 쓸테니 걱정마",
          "speaker_id": "2",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003900.1.13",
          "form": "공간 너무 많이 차지하는데 ㅋ",
          "original_form": "공간 너무 많이 차지하는데 ㅋ",
          "speaker_id": "1",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003900.1.14",
          "form": "나 몰래 버리지마",
          "original_form": "나 몰래 버리지마",
