      "utterance": [
        {
          "id": "MDRW2100010215.1.1",
          "form": "살 빼야되는데",
          "original_form": "살 빼야되는데",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100010215.1.2",
          "form": "그럼 살빼",
          "original_form": "그럼 살빼",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100010215.1.3",
          "form": "그게 쉽지가 않아",
          "original_form": "그게 쉽지가 않아",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100010215.1.4",
          "form": "난 잘 몰라 살 쪄본 적이 없어서",
          "original_form": "난 잘 몰라 살 쪄본 적이 없어서",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100010215.1.5",
          "form": "부럽다 나도 그런 체질이었으면 좋겠어",
          "original_form": "부럽다 나도 그런 체질이었으면 좋겠어",
          "speaker_id": "1",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100010215.1.6",
          "form": "난 살 쪘으면 좋겠다",
          "original_form": "난 살 쪘으면 좋겠다",
          "speaker_id": "2",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100010215.1.7",
          "form": "나한텐 그거만큼 쉬운 거 없는데",
          "original_form": "나한텐 그거만큼 쉬운 거 없는데",
          "speaker_id": "1",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100010215.1.8",
          "form": "ㅋㅋㅋㅋ 체질 바꿨으면 좋겠다",
          "original_form": "ㅋㅋㅋㅋ 체질 바꿨으면 좋겠다",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100010215.1.9",
          "form": "그니까 내 소원이야",
          "original_form": "그니까 내 소원이야",
          "speaker_id": "1",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100010215.1.10",
          "form": "아 60 찍어야 되는데",
          "original_form": "아 60 찍어야 되는데",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100010215.1.11",
          "form": "내가 대신 찍어서 주면 안될까?",
          "original_form": "내가 대신 찍어서 주면 안될까?",
          "speaker_id": "1",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100010215.1.12",
          "form": "ㅋㅋㅋㅋ 그러게",
          "original_form": "ㅋㅋㅋㅋ 그러게",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100010215.1.13",
          "form": "나 먹는 거 70프로만 따라먹어도 60은 될텐데",
          "original_form": "나 먹는 거 70프로만 따라먹어도 60은 될텐데",
          "speaker_id": "1",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100010215.1.14",
          "form": "뭐 먹어야 돼?",
          "original_form": "뭐 먹어야 돼?",
          "speaker_id": "2",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100010215.1.15",
          "form": "이거저거 많이",
          "original_form": "이거저거 많이",
          "speaker_id": "1",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100010215.1.16",
          "form": "군것질?",
          "original_form": "군것질?",
          "speaker_id": "2",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100010215.1.17",
          "form": "식사+군것질",
          "original_form": "식사+군것질",
          "speaker_id": "1",
          "time": "20210824 16:19"
        },
        {
          "id": "MDRW2100010215.1.18",
          "form": "요즘 건강에 더 중요해지는 거 같아",
          "original_form": "요즘 건강에 더 중요해지는 거 같아",
