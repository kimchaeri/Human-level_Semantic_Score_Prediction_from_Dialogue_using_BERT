      "utterance": [
        {
          "id": "MDRW2100010313.1.1",
          "form": "이번여름 너무 더웠죠ㅠ",
          "original_form": "이번여름 너무 더웠죠ㅠ",
          "speaker_id": "1",
          "time": "20210823 16:42"
        },
        {
          "id": "MDRW2100010313.1.2",
          "form": "맞아요ㅠ 오늘은",
          "original_form": "맞아요ㅠ 오늘은",
          "speaker_id": "2",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100010313.1.3",
          "form": "오늘은 비가와서 밖은 시원했어요",
          "original_form": "오늘은 비가와서 밖은 시원했어요",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010313.1.4",
          "form": "맞아요ㅋㅋ 지금은",
          "original_form": "맞아요ㅋㅋ 지금은",
          "speaker_id": "2",
          "time": "20210823 16:54"
        },
        {
          "id": "MDRW2100010313.1.5",
          "form": "여기는 그쳤네요",
          "original_form": "여기는 그쳤네요",
          "speaker_id": "2",
          "time": "20210823 16:54"
        },
        {
          "id": "MDRW2100010313.1.6",
          "form": "여기는 아직 조금 오는데 습도가 높아서 실내는 좀 더워요ㅠ",
          "original_form": "여기는 아직 조금 오는데 습도가 높아서 실내는 좀 더워요ㅠ",
          "speaker_id": "1",
          "time": "20210823 16:55"
        },
        {
          "id": "MDRW2100010313.1.7",
          "form": "맞아요 엄청 습하네요ㅠ",
          "original_form": "맞아요 엄청 습하네요ㅠ",
          "speaker_id": "2",
          "time": "20210823 17:00"
        },
        {
          "id": "MDRW2100010313.1.8",
          "form": "금방 가을이 올것같아요",
          "original_form": "금방 가을이 올것같아요",
          "speaker_id": "2",
          "time": "20210823 17:01"
        },
        {
          "id": "MDRW2100010313.1.9",
          "form": "이번주 비소식이 계속 이어지는데 가을장마인가봐요",
          "original_form": "이번주 비소식이 계속 이어지는데 가을장마인가봐요",
          "speaker_id": "1",
          "time": "20210823 17:05"
        },
        {
          "id": "MDRW2100010313.1.10",
          "form": "그러게요 여름이 가기전",
          "original_form": "그러게요 여름이 가기전",
          "speaker_id": "2",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010313.1.11",
          "form": "마지막인가봐요ㅎㅎ",
          "original_form": "마지막인가봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010313.1.12",
          "form": "저는 가을이 제일 좋아서 얼른 왔으면 좋겠어요",
          "original_form": "저는 가을이 제일 좋아서 얼른 왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010313.1.13",
          "form": "무슨계절 제일좋아하세요?",
          "original_form": "무슨계절 제일좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010313.1.14",
          "form": "저도 가을이요ㅋㅋㅋ",
          "original_form": "저도 가을이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010313.1.15",
          "form": "날씨가 가을이 짱이죠",
          "original_form": "날씨가 가을이 짱이죠",
          "speaker_id": "2",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010313.1.16",
          "form": "그죠 사실 사계절 있는 곳에선 대부분 가을을 좋아하지 않을까 싶어요",
          "original_form": "그죠 사실 사계절 있는 곳에선 대부분 가을을 좋아하지 않을까 싶어요",
          "speaker_id": "1",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100010313.1.17",
          "form": "걷기좋은 날씨 너무 좋아요",
          "original_form": "걷기좋은 날씨 너무 좋아요",
          "speaker_id": "1",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010313.1.18",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:53"
        },
        {
          "id": "MDRW2100010313.1.19",
          "form": "가을이 참 나들이가기도좋죠",
          "original_form": "가을이 참 나들이가기도좋죠",
          "speaker_id": "2",
          "time": "20210824 10:53"
        },
        {
          "id": "MDRW2100010313.1.20",
          "form": "산책이나 궁구경 가기 최고죠",
          "original_form": "산책이나 궁구경 가기 최고죠",
          "speaker_id": "1",
          "time": "20210824 10:55"
        },
        {
          "id": "MDRW2100010313.1.21",
          "form": "코로나만 아니였어도 여기저기 다닐텐데 아쉽네요",
          "original_form": "코로나만 아니였어도 여기저기 다닐텐데 아쉽네요",
          "speaker_id": "1",
          "time": "20210824 10:56"
        }
      ]
