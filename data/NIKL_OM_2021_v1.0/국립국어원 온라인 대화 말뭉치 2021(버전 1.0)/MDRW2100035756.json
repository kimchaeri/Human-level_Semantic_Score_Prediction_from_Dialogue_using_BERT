      "utterance": [
        {
          "id": "MDRW2100035756.1.1",
          "form": "응 내일 또 이야기하장 ㅋ",
          "original_form": "응 내일 또 이야기하장 ㅋ",
          "speaker_id": "2",
          "time": "20210830 00:52"
        },
        {
          "id": "MDRW2100035756.1.2",
          "form": "얍얍 잘자",
          "original_form": "얍얍 잘자",
          "speaker_id": "1",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100035756.1.3",
          "form": "굿모닝 ㅋ",
          "original_form": "굿모닝 ㅋ",
          "speaker_id": "1",
          "time": "20210830 07:52"
        },
        {
          "id": "MDRW2100035756.1.4",
          "form": "응ㅋㅋ아침이왔네 알바갔겠다",
          "original_form": "응ㅋㅋ아침이왔네 알바갔겠다",
          "speaker_id": "2",
          "time": "20210830 10:44"
        },
        {
          "id": "MDRW2100035756.1.5",
          "form": "점심 먹음??",
          "original_form": "점심 먹음??",
          "speaker_id": "2",
          "time": "20210830 14:39"
        },
        {
          "id": "MDRW2100035756.1.6",
          "form": "응 점심먹었어ㅋㅋ동생와서 같이먹음ㅋ",
          "original_form": "응 점심먹었어ㅋㅋ동생와서 같이먹음ㅋ",
          "speaker_id": "1",
          "time": "20210830 15:52"
        },
        {
          "id": "MDRW2100035756.1.7",
          "form": "좋다아아 그래서바빴구나 ㅎㅎ",
          "original_form": "좋다아아 그래서바빴구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 15:53"
        },
        {
          "id": "MDRW2100035756.1.8",
          "form": "아 애들 와서 정신이 하나도 없다이 ㅋ",
          "original_form": "아 애들 와서 정신이 하나도 없다이 ㅋ",
          "speaker_id": "1",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100035756.1.9",
          "form": "첫째 친구들와서 또 지들끼리 싸우고 언성높이길래",
          "original_form": "첫째 친구들와서 또 지들끼리 싸우고 언성높이길래",
          "speaker_id": "1",
          "time": "20210830 19:12"
        },
        {
          "id": "MDRW2100035756.1.10",
          "form": "결국 셋 다 혼냄",
          "original_form": "결국 셋 다 혼냄",
          "speaker_id": "1",
          "time": "20210830 19:12"
        },
        {
          "id": "MDRW2100035756.1.11",
          "form": "지금 또 놀러나갔다",
          "original_form": "지금 또 놀러나갔다",
          "speaker_id": "1",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100035756.1.12",
          "form": "오늘 난리네 마 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오늘 난리네 마 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:41"
        },
        {
          "id": "MDRW2100035756.1.13",
          "form": "애들은 ㅋㅋㅋ 진짜 그렇더라 뭐땜에 싸웠데??",
          "original_form": "애들은 ㅋㅋㅋ 진짜 그렇더라 뭐땜에 싸웠데??",
          "speaker_id": "2",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100035756.1.14",
          "form": "ㅋㅋㅋ 피아노 때문에",
          "original_form": "ㅋㅋㅋ 피아노 때문에",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100035756.1.15",
          "form": "서로 더 하고싶어서ㅋㅋ",
          "original_form": "서로 더 하고싶어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100035756.1.16",
          "form": "아ㅠㅠ지안이피아노?ㅋㅋ",
          "original_form": "아ㅠㅠ지안이피아노?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100035756.1.17",
          "form": "꼭 맨날하던건데도 그렇게 싸우더라더할라고ㅋㅋ",
          "original_form": "꼭 맨날하던건데도 그렇게 싸우더라더할라고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100035756.1.18",
          "form": "애들 다비슷",
          "original_form": "애들 다비슷",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100035756.1.19",
          "form": "응응ㅋㄱㅋㅋ맞아ㅋ",
          "original_form": "응응ㅋㄱㅋㅋ맞아ㅋ",
          "speaker_id": "1",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100035756.1.20",
          "form": "여기 어른없냐고 누가 그렇게 언성높이며 싸우냐고 그렇게 싸울거면 놀지말라고했더니 그뒤론 조용하이놀더라ㅋㅋ",
          "original_form": "여기 어른없냐고 누가 그렇게 언성높이며 싸우냐고 그렇게 싸울거면 놀지말라고했더니 그뒤론 조용하이놀더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:40"
        },
        {
          "id": "MDRW2100035756.1.21",
          "form": "애들 다 갔나??",
          "original_form": "애들 다 갔나??",
          "speaker_id": "2",
          "time": "20210831 08:40"
        },
        {
          "id": "MDRW2100035756.1.22",
          "form": "첫째가고ㅋㅋ",
          "original_form": "첫째가고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100035756.1.23",
          "form": "그러게 ㅎㅎㅎ 진짜그래도 애들은 언제그랬냐는듯 또 잘놀고",
          "original_form": "그러게 ㅎㅎㅎ 진짜그래도 애들은 언제그랬냐는듯 또 잘놀고",
          "speaker_id": "2",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100035756.1.24",
          "form": "둘째남았지.",
          "original_form": "둘째남았지.",
          "speaker_id": "1",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100035756.1.25",
          "form": "거기도 비오지?? 등원하기 힘들겠군",
          "original_form": "거기도 비오지?? 등원하기 힘들겠군",
          "speaker_id": "2",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100035756.1.26",
          "form": "지금 첵스초코먹는중ㅋㅋ아침",
          "original_form": "지금 첵스초코먹는중ㅋㅋ아침",
          "speaker_id": "1",
          "time": "20210831 08:41"
        }
      ]
