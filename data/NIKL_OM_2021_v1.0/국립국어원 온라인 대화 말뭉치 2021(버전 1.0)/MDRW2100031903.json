      "utterance": [
        {
          "id": "MDRW2100031903.1.1",
          "form": "저 지난달에 선물용으로",
          "original_form": "저 지난달에 선물용으로",
          "speaker_id": "2",
          "time": "20210905 08:04"
        },
        {
          "id": "MDRW2100031903.1.2",
          "form": "아이패드 선물한적 있는데",
          "original_form": "아이패드 선물한적 있는데",
          "speaker_id": "2",
          "time": "20210905 08:04"
        },
        {
          "id": "MDRW2100031903.1.3",
          "form": "누구한테요?",
          "original_form": "누구한테요?",
          "speaker_id": "1",
          "time": "20210905 08:04"
        },
        {
          "id": "MDRW2100031903.1.4",
          "form": "와이프요",
          "original_form": "와이프요",
          "speaker_id": "2",
          "time": "20210905 08:04"
        },
        {
          "id": "MDRW2100031903.1.5",
          "form": "name1님은 최근에 선물로 뭘 구입하신적 있나요?",
          "original_form": "&name1&님은 최근에 선물로 뭘 구입하신적 있나요?",
          "speaker_id": "2",
          "time": "20210905 08:04"
        },
        {
          "id": "MDRW2100031903.1.6",
          "form": "아~~저는 아이들 옷사서 선물했네요",
          "original_form": "아~~저는 아이들 옷사서 선물했네요",
          "speaker_id": "1",
          "time": "20210905 08:05"
        },
        {
          "id": "MDRW2100031903.1.7",
          "form": "조카들이요?",
          "original_form": "조카들이요?",
          "speaker_id": "2",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031903.1.8",
          "form": "아니요..저희애들요",
          "original_form": "아니요..저희애들요",
          "speaker_id": "1",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031903.1.9",
          "form": "자녀가 있으시구나",
          "original_form": "자녀가 있으시구나",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031903.1.10",
          "form": "그러면 아무래도 본이보다 아이들을 위한 소비를 더 하겠죠?",
          "original_form": "그러면 아무래도 본이보다 아이들을 위한 소비를 더 하겠죠?",
          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031903.1.11",
          "form": "네~~거의 아이들물건많이사는것같아요",
          "original_form": "네~~거의 아이들물건많이사는것같아요",
          "speaker_id": "1",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031903.1.12",
          "form": "아이들꺼도사고 제것도사고",
          "original_form": "아이들꺼도사고 제것도사고",
          "speaker_id": "1",
          "time": "20210905 09:24"
        },
        {
          "id": "MDRW2100031903.1.13",
          "form": "주로 어떤거쇼핑하세요?",
          "original_form": "주로 어떤거쇼핑하세요?",
          "speaker_id": "1",
          "time": "20210905 09:44"
        },
        {
          "id": "MDRW2100031903.1.14",
          "form": "저는 집에 물건 많은거 안좋아해서",
          "original_form": "저는 집에 물건 많은거 안좋아해서",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100031903.1.15",
          "form": "생필품류 쇼핑을 많이하는데",
          "original_form": "생필품류 쇼핑을 많이하는데",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100031903.1.16",
          "form": "와이프는 의류랑 소품류 사는걸 좋아해서",
          "original_form": "와이프는 의류랑 소품류 사는걸 좋아해서",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100031903.1.17",
          "form": "집에 짐이 많이 들어왔어요",
          "original_form": "집에 짐이 많이 들어왔어요",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100031903.1.18",
          "form": "여자들은 좋아하죠",
          "original_form": "여자들은 좋아하죠",
          "speaker_id": "1",
          "time": "20210905 11:18"
        },
        {
          "id": "MDRW2100031903.1.19",
          "form": "그런거같아요 마음에 들면 같은데 색다른걸 또사더라구요 신기했어요",
          "original_form": "그런거같아요 마음에 들면 같은데 색다른걸 또사더라구요 신기했어요",
          "speaker_id": "2",
          "time": "20210905 11:25"
        },
        {
          "id": "MDRW2100031903.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210905 11:39"
        },
        {
          "id": "MDRW2100031903.1.21",
          "form": "조금씩 달라요",
