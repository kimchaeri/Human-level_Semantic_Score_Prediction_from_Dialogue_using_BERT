      },
      "utterance": [
        {
          "id": "MDRW2100028897.1.1",
          "form": "해외는 어디어디 가봤나요",
          "original_form": "해외는 어디어디 가봤나요",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028897.1.2",
          "form": "전 사실 국내파예요",
          "original_form": "전 사실 국내파예요",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028897.1.3",
          "form": "비행기는 제주도 갈 때만 타봤답니다",
          "original_form": "비행기는 제주도 갈 때만 타봤답니다",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028897.1.4",
          "form": "오 이거 의외다 베를린 안 간줄은 알았는데..",
          "original_form": "오 이거 의외다 베를린 안 간줄은 알았는데..",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028897.1.5",
          "form": "차멀미가 베리베리 심해서",
          "original_form": "차멀미가 베리베리 심해서",
          "speaker_id": "2",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028897.1.6",
          "form": "여행을 잘 못가요",
          "original_form": "여행을 잘 못가요",
          "speaker_id": "2",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028897.1.7",
          "form": "수학여행이나 수련회는 항상 목숨을 걸고(과장) 다녀와야했답니다",
          "original_form": "수학여행이나 수련회는 항상 목숨을 걸고(과장) 다녀와야했답니다",
          "speaker_id": "2",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028897.1.8",
          "form": "자연스럽게 여행이 싫어졌어요...",
          "original_form": "자연스럽게 여행이 싫어졌어요...",
          "speaker_id": "2",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028897.1.9",
          "form": "되게 신기하네요. 저는 차에서 책을 읽어도 멀미를 안해서...",
          "original_form": "되게 신기하네요. 저는 차에서 책을 읽어도 멀미를 안해서...",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028897.1.10",
          "form": "부러워 차에서 활자 읽으면 목적지 도착해서도 반쯤 실신해 있기 때문에",
          "original_form": "부러워 차에서 활자 읽으면 목적지 도착해서도 반쯤 실신해 있기 때문에",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028897.1.11",
          "form": "제게 허락된 건 음악뿐",
          "original_form": "제게 허락된 건 음악뿐",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100028897.1.12",
          "form": "근데 저는 제주도를 간 적 없어요",
          "original_form": "근데 저는 제주도를 간 적 없어요",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028897.1.13",
          "form": "사실 작년 상반기에 대만가려고 했는데 코로나 터지고",
          "original_form": "사실 작년 상반기에 대만가려고 했는데 코로나 터지고",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028897.1.14",
          "form": "하반기에 제주도 가려고 했는데 확진자 늘어소",
          "original_form": "하반기에 제주도 가려고 했는데 확진자 늘어소",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028897.1.15",
          "form": "제주도는 아직인..",
          "original_form": "제주도는 아직인..",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028897.1.16",
          "form": "제주도는 맛있는 게 많아요",
          "original_form": "제주도는 맛있는 게 많아요",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028897.1.17",
          "form": "근데 차가 꼭 필요해요",
          "original_form": "근데 차가 꼭 필요해요",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028897.1.18",
          "form": "네에 그래서 친구 운전 연습 하고 있어요",
          "original_form": "네에 그래서 친구 운전 연습 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028897.1.19",
          "form": "친구ㅋㅋㅋㅋㅋㅋ친구 키워서 여행가기냐구요",
          "original_form": "친구ㅋㅋㅋㅋㅋㅋ친구 키워서 여행가기냐구요",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100028897.1.20",
          "form": "친구가 여행 계획 짜보니까",
          "original_form": "친구가 여행 계획 짜보니까",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100028897.1.21",
          "form": "반드시 필요하다는 걸 깨달은 거지.",
          "original_form": "반드시 필요하다는 걸 깨달은 거지.",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028897.1.22",
          "form": "맞아 여행에 차는 필수예요",
          "original_form": "맞아 여행에 차는 필수예요",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028897.1.23",
          "form": "그래서 친구가 얼마전부터 연습하고 읶어요",
          "original_form": "그래서 친구가 얼마전부터 연습하고 읶어요",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100028897.1.24",
          "form": "한라산 데려간다는데 큰일이",
          "original_form": "한라산 데려간다는데 큰일이",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100028897.1.25",
          "form": "저는 걷는 건 잘하는데 오르는 걸 못해서...",
          "original_form": "저는 걷는 건 잘하는데 오르는 걸 못해서...",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100028897.1.26",
          "form": "한라산 코스가 어떻다더라",
          "original_form": "한라산 코스가 어떻다더라",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028897.1.27",
          "form": "초심자용 코스",
          "original_form": "초심자용 코스",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028897.1.28",
          "form": "로 살.살",
          "original_form": "로 살.살",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028897.1.29",
          "form": "도전해봅시다",
          "original_form": "도전해봅시다",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100028897.1.30",
          "form": "정상보는게 목표인 거 같던데 기운빠짐잼",
          "original_form": "정상보는게 목표인 거 같던데 기운빠짐잼",
          "speaker_id": "1",
          "time": "20210902 23:18"
        }
      ]
