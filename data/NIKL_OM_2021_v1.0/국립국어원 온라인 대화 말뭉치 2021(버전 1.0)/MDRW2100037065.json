      "utterance": [
        {
          "id": "MDRW2100037065.1.1",
          "form": "저 오늘 메밀소바시켜먹었어요",
          "original_form": "저 오늘 메밀소바시켜먹었어요",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037065.1.2",
          "form": "name1님은여?",
          "original_form": "&name1&님은여?",
          "speaker_id": "2",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037065.1.3",
          "form": "우와 맛있는거 드셨네요!!!",
          "original_form": "우와 맛있는거 드셨네요!!!",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100037065.1.4",
          "form": "저는 두부를 초고추장에 찍어먹고",
          "original_form": "저는 두부를 초고추장에 찍어먹고",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100037065.1.5",
          "form": "병아리콩으로 만든 콩자반을 먹었어요!",
          "original_form": "병아리콩으로 만든 콩자반을 먹었어요!",
          "speaker_id": "1",
          "time": "20210831 21:22"
        },
        {
          "id": "MDRW2100037065.1.6",
          "form": "채식주의자가 되려는 건 아닌데 어쩌다보니",
          "original_form": "채식주의자가 되려는 건 아닌데 어쩌다보니",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100037065.1.7",
          "form": "채식을 하고 있네요 ㅋㅋㅋ",
          "original_form": "채식을 하고 있네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100037065.1.8",
          "form": "ㅋㅋㅋ진짜여 채식식단이에여",
          "original_form": "ㅋㅋㅋ진짜여 채식식단이에여",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037065.1.9",
          "form": "병아리콩어때요? 맛있어요?",
          "original_form": "병아리콩어때요? 맛있어요?",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037065.1.10",
          "form": "병아리콩이 되게 달달한거 아세요?!",
          "original_form": "병아리콩이 되게 달달한거 아세요?!",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037065.1.11",
          "form": "그래서 요즘은 병아리콩으로",
          "original_form": "그래서 요즘은 병아리콩으로",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037065.1.12",
          "form": "스프레드 만들어서 먹기도 하더라구요",
          "original_form": "스프레드 만들어서 먹기도 하더라구요",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037065.1.13",
          "form": "그리고 저녁에는 또 두유에다가 시리얼을,,,",
          "original_form": "그리고 저녁에는 또 두유에다가 시리얼을,,,",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037065.1.14",
          "form": "오오 그 샐러드에 조금들어있는건먹어봤는데",
          "original_form": "오오 그 샐러드에 조금들어있는건먹어봤는데",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037065.1.15",
          "form": "콩자반은신기하네오",
          "original_form": "콩자반은신기하네오",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037065.1.16",
          "form": "다이어트식으로 드시는건가요",
          "original_form": "다이어트식으로 드시는건가요",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037065.1.17",
          "form": "저는 정말 다이어트할 생각이 없는데",
          "original_form": "저는 정말 다이어트할 생각이 없는데",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037065.1.18",
          "form": "집에서 먹는 음식을 싸오다 보니",
          "original_form": "집에서 먹는 음식을 싸오다 보니",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037065.1.19",
          "form": "그렇게 되었어요,,,",
          "original_form": "그렇게 되었어요,,,",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037065.1.20",
          "form": "그래서 밖에서 먹는 음식은 정말 거한걸 먹게돼요 ㅋㅋㅋㅋ",
          "original_form": "그래서 밖에서 먹는 음식은 정말 거한걸 먹게돼요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100037065.1.21",
          "form": "아 ㅋㅋㅋ다 원치않은 강제식단이었군여",
          "original_form": "아 ㅋㅋㅋ다 원치않은 강제식단이었군여",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100037065.1.22",
          "form": "그래도 건강해질거같아요",
          "original_form": "그래도 건강해질거같아요",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100037065.1.23",
          "form": "허허 맞습니다,,",
          "original_form": "허허 맞습니다,,",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037065.1.24",
          "form": "name2님은 건강을 위해서 어떤 걸 챙겨드시나요?",
          "original_form": "&name2&님은 건강을 위해서 어떤 걸 챙겨드시나요?",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037065.1.25",
          "form": "건강을위해..",
          "original_form": "건강을위해..",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037065.1.26",
          "form": "물을먹습니다..!",
          "original_form": "물을먹습니다..!",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037065.1.27",
          "form": "쥐어짜내도 건강식이 그나마 물이네요",
          "original_form": "쥐어짜내도 건강식이 그나마 물이네요",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100037065.1.28",
          "form": "앗 그러셨나요!!!",
          "original_form": "앗 그러셨나요!!!",
          "speaker_id": "1",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100037065.1.29",
          "form": "하지만 물도 정말 중요한 건강식(?)이죠~~~",
          "original_form": "하지만 물도 정말 중요한 건강식(?)이죠~~~",
          "speaker_id": "1",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100037065.1.30",
          "form": "제 친구는 정말 액체를 안먹더라구요,,",
          "original_form": "제 친구는 정말 액체를 안먹더라구요,,",
          "speaker_id": "1",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100037065.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ그쳐 저도 물안좋아해서 어거지로 살려고 먹어요ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그쳐 저도 물안좋아해서 어거지로 살려고 먹어요ㅜ",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100037065.1.32",
          "form": "오늘식사하셨어요",
          "original_form": "오늘식사하셨어요",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100037065.1.33",
          "form": "네네! 저녁도 점심이랑 똑같이 먹었네요ㅠ",
          "original_form": "네네! 저녁도 점심이랑 똑같이 먹었네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100037065.1.34",
          "form": "뭔가 자극적인 게 땡겨서",
          "original_form": "뭔가 자극적인 게 땡겨서",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100037065.1.35",
          "form": "김 빠진 콜라도 먹었어요 ㅋㅋㅋㅋ",
          "original_form": "김 빠진 콜라도 먹었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037065.1.36",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 19:03"
        },
        {
          "id": "MDRW2100037065.1.37",
          "form": "김빠진콜라 ㅋㅋㅋㅋㅋ크으 달달했겠네요",
