          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022897.1.7",
          "form": "가격이더당황스러움",
          "original_form": "가격이더당황스러움",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022897.1.8",
          "form": "그니까 거의 3만원 댜임",
          "original_form": "그니까 거의 3만원 댜임",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022897.1.9",
          "form": "무ㅏ하는",
          "original_form": "무ㅏ하는",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022897.1.10",
          "form": "그냥레깅스위에",
          "original_form": "그냥레깅스위에",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022897.1.11",
          "form": "반바지하나더",
          "original_form": "반바지하나더",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022897.1.12",
          "form": "입으면되는데",
          "original_form": "입으면되는데",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022897.1.13",
          "form": "으휴",
          "original_form": "으휴",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022897.1.14",
          "form": "그니까 반바지 싼거 5000원에서 8000원대에 팔던데",
          "original_form": "그니까 반바지 싼거 5000원에서 8000원대에 팔던데",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022897.1.15",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022897.1.16",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022897.1.17",
          "form": "이옷도지금은반응안좋지만",
          "original_form": "이옷도지금은반응안좋지만",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022897.1.18",
          "form": "유튜버나연예인이하고나오면",
          "original_form": "유튜버나연예인이하고나오면",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022897.1.19",
          "form": "유행할듯",
          "original_form": "유행할듯",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022897.1.20",
          "form": "항상그랬듯이",
          "original_form": "항상그랬듯이",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022897.1.21",
          "form": "아 ㅇㅈ",
          "original_form": "아 ㅇㅈ",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022897.1.22",
          "form": "크롭티도",
          "original_form": "크롭티도",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022897.1.23",
          "form": "원래 저거 어떻게 입고 다니냐고 햇잖아",
          "original_form": "원래 저거 어떻게 입고 다니냐고 햇잖아",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022897.1.24",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022897.1.25",
          "form": "그 밑에 부분 잘린 후드 티도",
          "original_form": "그 밑에 부분 잘린 후드 티도",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022897.1.26",
          "form": "으",
          "original_form": "으",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022897.1.27",
          "form": "그렇게유행하면안좋은반응도다",
          "original_form": "그렇게유행하면안좋은반응도다",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022897.1.28",
          "form": "없어짐",
          "original_form": "없어짐",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022897.1.29",
          "form": "그리고좀그렇다고하면",
          "original_form": "그리고좀그렇다고하면",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022897.1.30",
          "form": "못입어서",
          "original_form": "못입어서",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022897.1.31",
          "form": "질투하냐고",
          "original_form": "질투하냐고",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022897.1.32",
          "form": "그니까 혹은 유행 못따라가는 문찐 취급 받잖아",
          "original_form": "그니까 혹은 유행 못따라가는 문찐 취급 받잖아",
          "speaker_id": "2",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100022897.1.33",
          "form": "맞아. 자기가 꾸미고 싶어서",
          "original_form": "맞아. 자기가 꾸미고 싶어서",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022897.1.34",
          "form": "입는다는거",
          "original_form": "입는다는거",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022897.1.35",
          "form": "주체적꾸밈이러는거",
          "original_form": "주체적꾸밈이러는거",
          "speaker_id": "1",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022897.1.36",
          "form": "진짜짜증나",
          "original_form": "진짜짜증나",
          "speaker_id": "1",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022897.1.37",
          "form": "이렇게 여자 돈빼먹기에 혈안이면서",
          "original_form": "이렇게 여자 돈빼먹기에 혈안이면서",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022897.1.38",
          "form": "여자 욕할 때는 아빠나 오빠돈 쓴다고 하는게",
          "original_form": "여자 욕할 때는 아빠나 오빠돈 쓴다고 하는게",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022897.1.39",
          "form": "어이없음",
          "original_form": "어이없음",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022897.1.40",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022897.1.41",
          "form": "그것도어이없어",
          "original_form": "그것도어이없어",
          "speaker_id": "1",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022897.1.42",
          "form": "냐ㅐ가아빠돈쓰든말든",
          "original_form": "냐ㅐ가아빠돈쓰든말든",
          "speaker_id": "1",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022897.1.43",
          "form": "뭔상관이야",
          "original_form": "뭔상관이야",
          "speaker_id": "1",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022897.1.44",
          "form": "지돈쓰느걸수도있고",
          "original_form": "지돈쓰느걸수도있고",
          "speaker_id": "1",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022897.1.45",
          "form": "그니까 뭘 안다고 그런 소리 하는 건지 모르겠음",
          "original_form": "그니까 뭘 안다고 그런 소리 하는 건지 모르겠음",
          "speaker_id": "2",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022897.1.46",
          "form": "여자소비낭비이런식으로",
          "original_form": "여자소비낭비이런식으로",
