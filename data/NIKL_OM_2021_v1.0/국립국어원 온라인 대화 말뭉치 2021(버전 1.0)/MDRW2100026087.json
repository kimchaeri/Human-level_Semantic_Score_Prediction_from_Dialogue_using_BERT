        },
        {
          "id": "MDRW2100026087.1.2",
          "form": "올림픽 재밌었어",
          "original_form": "올림픽 재밌었어",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026087.1.3",
          "form": "안산선수는  감동이었어",
          "original_form": "안산선수는  감동이었어",
          "speaker_id": "1",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026087.1.4",
          "form": "어린나이에 대단해",
          "original_form": "어린나이에 대단해",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026087.1.5",
          "form": "우상혁선수 긍정파워 좋았어",
          "original_form": "우상혁선수 긍정파워 좋았어",
          "speaker_id": "1",
          "time": "20210901 19:50"
        },
        {
          "id": "MDRW2100026087.1.6",
          "form": "근대5종이 제일 신기하고",
          "original_form": "근대5종이 제일 신기하고",
          "speaker_id": "2",
          "time": "20210901 19:50"
        },
        {
          "id": "MDRW2100026087.1.7",
          "form": "으뜸이는 김연경",
          "original_form": "으뜸이는 김연경",
          "speaker_id": "2",
          "time": "20210901 19:50"
        },
        {
          "id": "MDRW2100026087.1.8",
          "form": "배구는 드라마였어",
          "original_form": "배구는 드라마였어",
          "speaker_id": "1",
          "time": "20210901 19:50"
        },
        {
          "id": "MDRW2100026087.1.9",
          "form": "올림픽 보네안보네해도 하나되는마음이었어",
          "original_form": "올림픽 보네안보네해도 하나되는마음이었어",
          "speaker_id": "2",
          "time": "20210901 19:50"
        },
        {
          "id": "MDRW2100026087.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026087.1.11",
          "form": "1년뒤 동계올림픽도곧",
          "original_form": "1년뒤 동계올림픽도곧",
          "speaker_id": "1",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026087.1.12",
          "form": "세월빠르다",
          "original_form": "세월빠르다",
          "speaker_id": "1",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026087.1.13",
          "form": "직관하면 좋겠어",
          "original_form": "직관하면 좋겠어",
          "speaker_id": "2",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026087.1.14",
          "form": "이 모든게 코로나때문",
          "original_form": "이 모든게 코로나때문",
          "speaker_id": "1",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026087.1.15",
          "form": "평창올림픽이 최고엿지",
          "original_form": "평창올림픽이 최고엿지",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026087.1.16",
          "form": "올림픽도 벌써 한달전이야",
          "original_form": "올림픽도 벌써 한달전이야",
          "speaker_id": "2",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026087.1.17",
          "form": "세월빠르다",
          "original_form": "세월빠르다",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026087.1.18",
          "form": "야구는 너무 못해",
          "original_form": "야구는 너무 못해",
          "speaker_id": "2",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026087.1.19",
          "form": "실망이다",
          "original_form": "실망이다",
          "speaker_id": "2",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026087.1.20",
          "form": "배가 다 나와가지고",
          "original_form": "배가 다 나와가지고",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026087.1.21",
          "form": "선수는 자기관리해야지",
          "original_form": "선수는 자기관리해야지",
          "speaker_id": "1",
          "time": "20210901 19:52"
        }
      ]
