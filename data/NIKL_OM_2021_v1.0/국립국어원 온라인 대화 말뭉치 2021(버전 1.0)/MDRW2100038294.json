      "utterance": [
        {
          "id": "MDRW2100038294.1.1",
          "form": "학교 다니세요?",
          "original_form": "학교 다니세요?",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038294.1.2",
          "form": "지금은 아니요ㅎㅎ 학교다닐때 재밌으셨어요?",
          "original_form": "지금은 아니요ㅎㅎ 학교다닐때 재밌으셨어요?",
          "speaker_id": "1",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100038294.1.3",
          "form": "네 나름",
          "original_form": "네 나름",
          "speaker_id": "2",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100038294.1.4",
          "form": "재밌게 다녔어요",
          "original_form": "재밌게 다녔어요",
          "speaker_id": "2",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100038294.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100038294.1.6",
          "form": "알바도 하면서 다녀서 엄청 바빴던 기억이..ㅜ",
          "original_form": "알바도 하면서 다녀서 엄청 바빴던 기억이..ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100038294.1.7",
          "form": "나쁘지않았어요..",
          "original_form": "나쁘지않았어요..",
          "speaker_id": "1",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100038294.1.8",
          "form": "알바하면서 하느라",
          "original_form": "알바하면서 하느라",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100038294.1.9",
          "form": "바쁘셨겠네요!!!",
          "original_form": "바쁘셨겠네요!!!",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100038294.1.10",
          "form": "ㅎㅎ학교 학식 맛있으셨어여?",
          "original_form": "ㅎㅎ학교 학식 맛있으셨어여?",
          "speaker_id": "1",
          "time": "20210902 14:54"
        },
        {
          "id": "MDRW2100038294.1.11",
          "form": "학식.. 그저 그랬어요..",
          "original_form": "학식.. 그저 그랬어요..",
          "speaker_id": "2",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100038294.1.12",
          "form": "주변은 좋았죠ㅋㅋ",
          "original_form": "주변은 좋았죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100038294.1.13",
          "form": "ㅎㅎ학교 주변에 저렴한 맛집이 많죠",
          "original_form": "ㅎㅎ학교 주변에 저렴한 맛집이 많죠",
          "speaker_id": "1",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038294.1.14",
          "form": "ㅋㅋ 혹시 알촌 아세요?",
          "original_form": "ㅋㅋ 혹시 알촌 아세요?",
          "speaker_id": "2",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038294.1.15",
          "form": "밖에서 자주 먹었어요 ㅠㅋㅋ",
          "original_form": "밖에서 자주 먹었어요 ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038294.1.16",
          "form": "아뇨 저 첨보는데 검색해봤어요!!",
          "original_form": "아뇨 저 첨보는데 검색해봤어요!!",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038294.1.17",
          "form": "가격 정말 혜자스럽네여",
          "original_form": "가격 정말 혜자스럽네여",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038294.1.18",
          "form": "알밥 좋아하는뎅",
          "original_form": "알밥 좋아하는뎅",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038294.1.19",
          "form": "여기 진짜 맛있거든요..",
          "original_form": "여기 진짜 맛있거든요..",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100038294.1.20",
          "form": "가격대비 짱이였어요",
          "original_form": "가격대비 짱이였어요",
