        {
          "id": "MDRW2100010172.1.1",
          "form": "운동 뭐하세요!",
          "original_form": "운동 뭐하세요!",
          "speaker_id": "1",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010172.1.2",
          "form": "저는 요가요",
          "original_form": "저는 요가요",
          "speaker_id": "2",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010172.1.3",
          "form": "요즘도 열어요?",
          "original_form": "요즘도 열어요?",
          "speaker_id": "1",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010172.1.4",
          "form": "네 제가 다니는덴  마스크 쓰고 해요",
          "original_form": "네 제가 다니는덴  마스크 쓰고 해요",
          "speaker_id": "2",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010172.1.5",
          "form": "마스크 쓰고 운동이라니...  엄첨 빡세겠어요",
          "original_form": "마스크 쓰고 운동이라니...  엄첨 빡세겠어요",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010172.1.6",
          "form": "첨엔 힘들었는데 하다보니 괜찮더라구요",
          "original_form": "첨엔 힘들었는데 하다보니 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010172.1.7",
          "form": "저는 조깅하고있어요",
          "original_form": "저는 조깅하고있어요",
          "speaker_id": "1",
          "time": "20210823 15:07"
        },
        {
          "id": "MDRW2100010172.1.8",
          "form": "날이 선선해서 조깅하기 좋죠",
          "original_form": "날이 선선해서 조깅하기 좋죠",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010172.1.9",
          "form": "내일은 태풍이니까 운동 안 갈거에요ㅋㅋ",
          "original_form": "내일은 태풍이니까 운동 안 갈거에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010172.1.10",
          "form": "그럼요 날 안좋을 땐 무조건 집에서 휴식이 답이죠",
          "original_form": "그럼요 날 안좋을 땐 무조건 집에서 휴식이 답이죠",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100010172.1.11",
          "form": "코로나 괜찮아지면 테니스 배우고싶어요",
          "original_form": "코로나 괜찮아지면 테니스 배우고싶어요",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010172.1.12",
          "form": "테니스좋죠",
          "original_form": "테니스좋죠",
          "speaker_id": "2",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100010172.1.13",
          "form": "실내테니슨가요?",
          "original_form": "실내테니슨가요?",
          "speaker_id": "2",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100010172.1.14",
          "form": "네 아파트단지에서 가르치는 것두 있던데 그건 입주민전용같아보여요",
          "original_form": "네 아파트단지에서 가르치는 것두 있던데 그건 입주민전용같아보여요",
          "speaker_id": "1",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100010172.1.15",
          "form": "아마도 그러겠네요",
          "original_form": "아마도 그러겠네요",
          "speaker_id": "2",
