      "utterance": [
        {
          "id": "MDRW2100045592.1.1",
          "form": "저는 아까 마라탕 컵라면을 먹은지 얼마 안돼서...",
          "original_form": "저는 아까 마라탕 컵라면을 먹은지 얼마 안돼서...",
          "speaker_id": "2",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100045592.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100045592.1.3",
          "form": "언제 드신거에요..ㅇㅁㅇ ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "언제 드신거에요..ㅇㅁㅇ ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045592.1.4",
          "form": "한... 1시?",
          "original_form": "한... 1시?",
          "speaker_id": "2",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045592.1.5",
          "form": "이젠 규칙적인 식습관도 고장난 사람이란걸 들키고 만...",
          "original_form": "이젠 규칙적인 식습관도 고장난 사람이란걸 들키고 만...",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045592.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045592.1.7",
          "form": "병원가면 합죽이 되시겠네요",
          "original_form": "병원가면 합죽이 되시겠네요",
          "speaker_id": "1",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045592.1.8",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045592.1.9",
          "form": "죄인이죠",
          "original_form": "죄인이죠",
          "speaker_id": "2",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045592.1.10",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045592.1.11",
          "form": "요리 잘 하세요?",
          "original_form": "요리 잘 하세요?",
          "speaker_id": "1",
          "time": "20210908 04:08"
        },
        {
          "id": "MDRW2100045592.1.12",
          "form": "딱 이정도면 살아갈수있나?",
          "original_form": "딱 이정도면 살아갈수있나?",
          "speaker_id": "2",
          "time": "20210908 04:09"
        },
        {
          "id": "MDRW2100045592.1.13",
          "form": "싶은정도로만..",
          "original_form": "싶은정도로만..",
          "speaker_id": "2",
          "time": "20210908 04:09"
        },
        {
          "id": "MDRW2100045592.1.14",
          "form": "해요...",
          "original_form": "해요...",
          "speaker_id": "2",
          "time": "20210908 04:09"
        },
        {
          "id": "MDRW2100045592.1.15",
          "form": "물론 어머니께서 주방에 오지말아라 하셨지만..",
          "original_form": "물론 어머니께서 주방에 오지말아라 하셨지만..",
          "speaker_id": "2",
          "time": "20210908 04:10"
        },
        {
          "id": "MDRW2100045592.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ앗!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ앗!",
          "speaker_id": "1",
          "time": "20210908 04:10"
        },
        {
          "id": "MDRW2100045592.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:10"
        },
        {
          "id": "MDRW2100045592.1.18",
          "form": "요리 자신있는거 뭐 있어요?",
          "original_form": "요리 자신있는거 뭐 있어요?",
          "speaker_id": "1",
          "time": "20210908 04:10"
        },
        {
          "id": "MDRW2100045592.1.19",
          "form": "토마토 계란 볶음...",
          "original_form": "토마토 계란 볶음...",
          "speaker_id": "2",
          "time": "20210908 04:11"
        },
        {
          "id": "MDRW2100045592.1.20",
          "form": "부추계란...",
          "original_form": "부추계란...",
          "speaker_id": "2",
          "time": "20210908 04:11"
        },
        {
          "id": "MDRW2100045592.1.21",
          "form": "헐 맛있는거잖아요",
          "original_form": "헐 맛있는거잖아요",
          "speaker_id": "1",
          "time": "20210908 04:11"
        },
        {
          "id": "MDRW2100045592.1.22",
          "form": "저 요새 중식에 꽂혀서...",
          "original_form": "저 요새 중식에 꽂혀서...",
          "speaker_id": "2",
          "time": "20210908 04:11"
        },
        {
          "id": "MDRW2100045592.1.23",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210908 04:11"
        },
        {
          "id": "MDRW2100045592.1.24",
          "form": "근데시도하다가 되는게 있고",
          "original_form": "근데시도하다가 되는게 있고",
          "speaker_id": "2",
          "time": "20210908 04:11"
        },
        {
          "id": "MDRW2100045592.1.25",
          "form": "다른것도 더 있어요?",
          "original_form": "다른것도 더 있어요?",
          "speaker_id": "1",
          "time": "20210908 04:11"
        },
        {
          "id": "MDRW2100045592.1.26",
          "form": "하다가 엄마한테 혼나는것도 있고..",
          "original_form": "하다가 엄마한테 혼나는것도 있고..",
          "speaker_id": "2",
          "time": "20210908 04:11"
        },
        {
          "id": "MDRW2100045592.1.27",
          "form": "지금 성공한것만",
          "original_form": "지금 성공한것만",
          "speaker_id": "2",
          "time": "20210908 04:11"
        },
        {
          "id": "MDRW2100045592.1.28",
          "form": "말했어요",
          "original_form": "말했어요",
          "speaker_id": "2",
          "time": "20210908 04:11"
        },
        {
          "id": "MDRW2100045592.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:11"
        },
        {
          "id": "MDRW2100045592.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ부추계란은 어떻게 하는거에요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ부추계란은 어떻게 하는거에요?",
          "speaker_id": "1",
          "time": "20210908 04:12"
        },
        {
          "id": "MDRW2100045592.1.31",
          "form": "정말 그냥 부추를 기름에 볶고",
          "original_form": "정말 그냥 부추를 기름에 볶고",
          "speaker_id": "2",
          "time": "20210908 04:13"
        },
        {
          "id": "MDRW2100045592.1.32",
          "form": "계란도 볶아서 같이 한번 후루룩 볶으면 끝인데",
          "original_form": "계란도 볶아서 같이 한번 후루룩 볶으면 끝인데",
          "speaker_id": "2",
          "time": "20210908 04:13"
        },
        {
          "id": "MDRW2100045592.1.33",
          "form": "맛있어요 정말",
          "original_form": "맛있어요 정말",
          "speaker_id": "2",
          "time": "20210908 04:13"
        }
      ]
    }
