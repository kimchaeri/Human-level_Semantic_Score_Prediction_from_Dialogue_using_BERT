      "utterance": [
        {
          "id": "MDRW2100003624.1.1",
          "form": "그냥 방독면에 이상 있나없나",
          "original_form": "그냥 방독면에 이상 있나없나",
          "speaker_id": "2",
          "time": "20210821 09:03"
        },
        {
          "id": "MDRW2100003624.1.2",
          "form": "아 직접적으로 마시지는 않고?",
          "original_form": "아 직접적으로 마시지는 않고?",
          "speaker_id": "1",
          "time": "20210821 09:03"
        },
        {
          "id": "MDRW2100003624.1.3",
          "form": "쓰고 들어가서 체크하는수준",
          "original_form": "쓰고 들어가서 체크하는수준",
          "speaker_id": "2",
          "time": "20210821 09:03"
        },
        {
          "id": "MDRW2100003624.1.4",
          "form": "재수없게",
          "original_form": "재수없게",
          "speaker_id": "2",
          "time": "20210821 09:03"
        },
        {
          "id": "MDRW2100003624.1.5",
          "form": "방독면 불량걸리면",
          "original_form": "방독면 불량걸리면",
          "speaker_id": "2",
          "time": "20210821 09:04"
        },
        {
          "id": "MDRW2100003624.1.6",
          "form": "혼자서 아악",
          "original_form": "혼자서 아악",
          "speaker_id": "2",
          "time": "20210821 09:04"
        },
        {
          "id": "MDRW2100003624.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210821 09:04"
        },
        {
          "id": "MDRW2100003624.1.8",
          "form": "끔찍해",
          "original_form": "끔찍해",
          "speaker_id": "1",
          "time": "20210821 09:04"
        },
        {
          "id": "MDRW2100003624.1.9",
          "form": "하는....",
          "original_form": "하는....",
          "speaker_id": "2",
          "time": "20210821 09:04"
        },
        {
          "id": "MDRW2100003624.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:04"
        },
        {
          "id": "MDRW2100003624.1.11",
          "form": "군대있을 때 군대리아 진짜 맛있어요?",
          "original_form": "군대있을 때 군대리아 진짜 맛있어요?",
          "speaker_id": "1",
          "time": "20210821 09:04"
        },
        {
          "id": "MDRW2100003624.1.12",
          "form": "그거 진짜 줘요?",
          "original_form": "그거 진짜 줘요?",
          "speaker_id": "1",
          "time": "20210821 09:04"
        },
        {
          "id": "MDRW2100003624.1.13",
          "form": "맛있게 보여요?????",
          "original_form": "맛있게 보여요?????",
          "speaker_id": "2",
          "time": "20210821 09:04"
        },
        {
          "id": "MDRW2100003624.1.14",
          "form": "음 아무래도?",
          "original_form": "음 아무래도?",
          "speaker_id": "1",
          "time": "20210821 09:05"
        },
        {
          "id": "MDRW2100003624.1.15",
          "form": "미디어의 영향이있죠",
          "original_form": "미디어의 영향이있죠",
          "speaker_id": "1",
          "time": "20210821 09:05"
        },
        {
          "id": "MDRW2100003624.1.16",
          "form": "ㅋㅋㅋㅋ 솔직히 맛있다는거",
          "original_form": "ㅋㅋㅋㅋ 솔직히 맛있다는거",
          "speaker_id": "2",
          "time": "20210821 09:05"
        },
        {
          "id": "MDRW2100003624.1.17",
          "form": "거짓말?",
          "original_form": "거짓말?",
          "speaker_id": "1",
          "time": "20210821 09:05"
        },
        {
          "id": "MDRW2100003624.1.18",
          "form": "다 군대여서 맛있는거지",
          "original_form": "다 군대여서 맛있는거지",
          "speaker_id": "2",
          "time": "20210821 09:05"
        },
        {
          "id": "MDRW2100003624.1.19",
          "form": "나와서 먹으면",
          "original_form": "나와서 먹으면",
          "speaker_id": "2",
          "time": "20210821 09:06"
        },
        {
          "id": "MDRW2100003624.1.20",
          "form": "ㅋㅋㅋㅋ 그런거에요?",
          "original_form": "ㅋㅋㅋㅋ 그런거에요?",
          "speaker_id": "1",
          "time": "20210821 09:06"
        },
        {
          "id": "MDRW2100003624.1.21",
          "form": "다 맛없어요 ㅋㅋㅋㅋㅋ",
          "original_form": "다 맛없어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:06"
        },
        {
          "id": "MDRW2100003624.1.22",
          "form": "아니 버거 맛있는게 널렸자나요 ㅋㅋ",
          "original_form": "아니 버거 맛있는게 널렸자나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:06"
        },
        {
          "id": "MDRW2100003624.1.23",
          "form": "거기선 그게 없으니...ㅜ",
          "original_form": "거기선 그게 없으니...ㅜ",
          "speaker_id": "2",
          "time": "20210821 09:06"
        }
      ]
