      "utterance": [
        {
          "id": "MDRW2100044793.1.1",
          "form": "자주는 아니지만 가려고 노력합니다",
          "original_form": "자주는 아니지만 가려고 노력합니다",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044793.1.2",
          "form": "어떤가요",
          "original_form": "어떤가요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044793.1.3",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044793.1.4",
          "form": "노래 잘 부르는 가수들한테 고쳐서",
          "original_form": "노래 잘 부르는 가수들한테 고쳐서",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044793.1.5",
          "form": "그쵸 가수는 노래를 잘 불러 야죠",
          "original_form": "그쵸 가수는 노래를 잘 불러 야죠",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044793.1.6",
          "form": "크로스오버 쪽에 관심이 많아서",
          "original_form": "크로스오버 쪽에 관심이 많아서",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044793.1.7",
          "form": "그게 뭐죠",
          "original_form": "그게 뭐죠",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044793.1.8",
          "form": "성악 오페라 가요 트로트 등등등",
          "original_form": "성악 오페라 가요 트로트 등등등",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044793.1.9",
          "form": "다양한 장르를 복합해서 하는 얘기예요",
          "original_form": "다양한 장르를 복합해서 하는 얘기예요",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044793.1.10",
          "form": "어렵네요",
          "original_form": "어렵네요",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044793.1.11",
          "form": "아니요 그냥 이것저것 다 안다 얘기해요",
          "original_form": "아니요 그냥 이것저것 다 안다 얘기해요",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044793.1.12",
          "form": "그걸 다 하려면 엄청 실력이 좋아야 겠네요",
          "original_form": "그걸 다 하려면 엄청 실력이 좋아야 겠네요",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044793.1.13",
          "form": "맞아요 그래서 정말 잘 부르는 네 명이 모여서",
          "original_form": "맞아요 그래서 정말 잘 부르는 네 명이 모여서",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044793.1.14",
          "form": "다음에 사서 노래를 불러요",
          "original_form": "다음에 사서 노래를 불러요",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044793.1.15",
          "form": "직접 가서 들으면 좋겠네요",
          "original_form": "직접 가서 들으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044793.1.16",
          "form": "노래를 너무 잘 불러서 무서워요",
          "original_form": "노래를 너무 잘 불러서 무서워요",
