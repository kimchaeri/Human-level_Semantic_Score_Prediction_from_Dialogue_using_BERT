      "utterance": [
        {
          "id": "MDRW2100025800.1.1",
          "form": "요거트",
          "original_form": "요거트",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025800.1.2",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025800.1.3",
          "form": "고럼요 ㅎㅎ 요거트 환장해요!",
          "original_form": "고럼요 ㅎㅎ 요거트 환장해요!",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025800.1.4",
          "form": "크으 그럼",
          "original_form": "크으 그럼",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025800.1.5",
          "form": "비요뜨도 좋아하시죠?",
          "original_form": "비요뜨도 좋아하시죠?",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025800.1.6",
          "form": "딩동댕 ㅋㅋㅋ 요거트 너무 맛있어요... 누가 발명한 건지 상주고싶어요",
          "original_form": "딩동댕 ㅋㅋㅋ 요거트 너무 맛있어요... 누가 발명한 건지 상주고싶어요",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025800.1.7",
          "form": "아 인정요 ㅠ",
          "original_form": "아 인정요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025800.1.8",
          "form": "비요뜨 유사제품도 많던데",
          "original_form": "비요뜨 유사제품도 많던데",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025800.1.9",
          "form": "드셔봤나요",
          "original_form": "드셔봤나요",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025800.1.10",
          "form": "아니요... 오로지 비요뜨 + 시중 요플레 + 수제그릭요거트!ㅋㅋㅋ",
          "original_form": "아니요... 오로지 비요뜨 + 시중 요플레 + 수제그릭요거트!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100025800.1.11",
          "form": "수제그릭요거트도 맛은 있는데 정말 양이 적게나와서 항상 실망이커요 ㅠㅋㅋㅋ",
          "original_form": "수제그릭요거트도 맛은 있는데 정말 양이 적게나와서 항상 실망이커요 ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:13"
        },
        {
          "id": "MDRW2100025800.1.12",
          "form": "아ㅋㅋㅋ 실제로 만들어봤러요???",
          "original_form": "아ㅋㅋㅋ 실제로 만들어봤러요???",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100025800.1.13",
          "form": "네 ㅋㅋㅋ 되게 쉬워요~ 면보만 있으면 돼요 ㅎㅎ",
          "original_form": "네 ㅋㅋㅋ 되게 쉬워요~ 면보만 있으면 돼요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100025800.1.14",
          "form": "헉 맛이 궁금하네요ㅋㅋㅋ",
          "original_form": "헉 맛이 궁금하네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100025800.1.15",
          "form": "음... 꾸덕꾸덕한 요플레인데 아무맛안나는 치즈같기도 하고 ㅎㅎ 엄청 꾸덕하게 만드니까 맛있어요",
          "original_form": "음... 꾸덕꾸덕한 요플레인데 아무맛안나는 치즈같기도 하고 ㅎㅎ 엄청 꾸덕하게 만드니까 맛있어요",
          "speaker_id": "1",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100025800.1.16",
          "form": "ㅋㅋ 혹시 그 염소치즈 이런 느낌인가요?",
          "original_form": "ㅋㅋ 혹시 그 염소치즈 이런 느낌인가요?",
          "speaker_id": "2",
          "time": "20210902 16:25"
        },
        {
          "id": "MDRW2100025800.1.17",
          "form": "염소치즈가 뭔지 모르겠어요 머쓱하지만..\r\n만들어보세요! ㅎㅎㅎㅎ 꿀뿌려먹으면 너무너무 맛있어요",
          "original_form": "염소치즈가 뭔지 모르겠어요 머쓱하지만..\r\n만들어보세요! ㅎㅎㅎㅎ 꿀뿌려먹으면 너무너무 맛있어요",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100025800.1.18",
          "form": "뭔가 믿음이 가네요",
          "original_form": "뭔가 믿음이 가네요",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100025800.1.19",
          "form": "나중에 한번 해봐야겠어요",
          "original_form": "나중에 한번 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210902 16:30"
        }
      ]
