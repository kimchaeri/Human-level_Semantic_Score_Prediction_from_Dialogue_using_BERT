      "utterance": [
        {
          "id": "MDRW2100017657.1.1",
          "form": "하리보 룰렛이라고 동그란 젤리가 원기동 형태로 포장된게 있거든요",
          "original_form": "하리보 룰렛이라고 동그란 젤리가 원기동 형태로 포장된게 있거든요",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017657.1.2",
          "form": "운전할때 까먹기 좋아요",
          "original_form": "운전할때 까먹기 좋아요",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017657.1.3",
          "form": "오 저는 아직 못봣는데",
          "original_form": "오 저는 아직 못봣는데",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017657.1.4",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017657.1.5",
          "form": "편의점서도 가금 팔아요",
          "original_form": "편의점서도 가금 팔아요",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017657.1.6",
          "form": "눈여겨봐야겟어요",
          "original_form": "눈여겨봐야겟어요",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017657.1.7",
          "form": "식사하셧나요?",
          "original_form": "식사하셧나요?",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017657.1.8",
          "form": "네네 저는 그냥 집밥이랑 김ㅁ거었어요",
          "original_form": "네네 저는 그냥 집밥이랑 김ㅁ거었어요",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017657.1.9",
          "form": "저는 잔치국수!!",
          "original_form": "저는 잔치국수!!",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017657.1.10",
          "form": "탄수화물중독이에욬ㅋ면!",
          "original_form": "탄수화물중독이에욬ㅋ면!",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017657.1.11",
          "form": "면 매니아시구나 전 국수는 고명이 집에 없어서 잘 안먹게 돼요 고명만드는 수고가 다른 요리하는거랑 비슷해서요",
          "original_form": "면 매니아시구나 전 국수는 고명이 집에 없어서 잘 안먹게 돼요 고명만드는 수고가 다른 요리하는거랑 비슷해서요",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017657.1.12",
          "form": "배민시켯어요 ㅎㅎㅎㅎ",
          "original_form": "배민시켯어요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017657.1.13",
          "form": "넘 맛잇어요ㅜ",
          "original_form": "넘 맛잇어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017657.1.14",
          "form": "와 요즘은 국수도 배달하나봐요 안 불어요?",
          "original_form": "와 요즘은 국수도 배달하나봐요 안 불어요?",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017657.1.15",
          "form": "아 면이 따로오겠구나",
          "original_form": "아 면이 따로오겠구나",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017657.1.16",
          "form": "네 ㅎㅎ 국물이 따로 담아져와요",
          "original_form": "네 ㅎㅎ 국물이 따로 담아져와요",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017657.1.17",
          "form": "어떤음식좋아하세요?",
          "original_form": "어떤음식좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017657.1.18",
          "form": "전 좋아하는 음식이 너무 많아서 하나만 말할 수 없어요",
          "original_form": "전 좋아하는 음식이 너무 많아서 하나만 말할 수 없어요",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017657.1.19",
          "form": "크 저도 먹는낙으로 살아요",
          "original_form": "크 저도 먹는낙으로 살아요",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017657.1.20",
          "form": "사실 없어서 못먹져",
          "original_form": "사실 없어서 못먹져",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017657.1.21",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017657.1.22",
          "form": "가을겨울엔 따뜻한 음식 여름엔 차가운 음식",
          "original_form": "가을겨울엔 따뜻한 음식 여름엔 차가운 음식",
          "speaker_id": "1",
          "time": "20210826 20:49"
        }
      ]
