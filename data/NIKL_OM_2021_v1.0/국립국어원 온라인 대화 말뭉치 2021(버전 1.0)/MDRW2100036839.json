      "utterance": [
        {
          "id": "MDRW2100036839.1.1",
          "form": "쇼핑하신 이야기 들으면 재밌어요",
          "original_form": "쇼핑하신 이야기 들으면 재밌어요",
          "speaker_id": "1",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100036839.1.2",
          "form": "악 ㅋㅋㅋㅋ 별건 없는데 ㅋㅋㅋㅋ",
          "original_form": "악 ㅋㅋㅋㅋ 별건 없는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100036839.1.3",
          "form": "어제는 엄마부탁으로 식탁조명을 사드렸는데..",
          "original_form": "어제는 엄마부탁으로 식탁조명을 사드렸는데..",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100036839.1.4",
          "form": "너무 제스탈이 아닌거에요 ㅋㅋㅋㅋ",
          "original_form": "너무 제스탈이 아닌거에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100036839.1.5",
          "form": "근데 어무니가 어무니집에 다신다는데 뭐 제가 군말달거 있나요",
          "original_form": "근데 어무니가 어무니집에 다신다는데 뭐 제가 군말달거 있나요",
          "speaker_id": "2",
          "time": "20210831 13:45"
        },
        {
          "id": "MDRW2100036839.1.6",
          "form": "그냥 결제해드렸습니당 ㅋㅋㅋㅋ",
          "original_form": "그냥 결제해드렸습니당 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:45"
        },
        {
          "id": "MDRW2100036839.1.7",
          "form": "아아 그러게요",
          "original_form": "아아 그러게요",
          "speaker_id": "1",
          "time": "20210831 13:45"
        },
        {
          "id": "MDRW2100036839.1.8",
          "form": "내 집이 아니면 머....",
          "original_form": "내 집이 아니면 머....",
          "speaker_id": "1",
          "time": "20210831 13:45"
        },
        {
          "id": "MDRW2100036839.1.9",
          "form": "요즘 제일 사고싶은 건 안마의자에요",
          "original_form": "요즘 제일 사고싶은 건 안마의자에요",
          "speaker_id": "2",
          "time": "20210831 13:46"
        },
        {
          "id": "MDRW2100036839.1.10",
          "form": "온몸이 뻐근하거든요ㅠㅠㅠㅠ",
          "original_form": "온몸이 뻐근하거든요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 13:47"
        },
        {
          "id": "MDRW2100036839.1.11",
          "form": "아아 안마의자 있으면 좋을거같아요",
          "original_form": "아아 안마의자 있으면 좋을거같아요",
          "speaker_id": "1",
          "time": "20210831 13:48"
        },
        {
          "id": "MDRW2100036839.1.12",
          "form": "요즘 위시리스트 있으신가여??",
          "original_form": "요즘 위시리스트 있으신가여??",
          "speaker_id": "2",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100036839.1.13",
          "form": "아이패드 사고싶어요",
          "original_form": "아이패드 사고싶어요",
          "speaker_id": "1",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100036839.1.14",
          "form": "오 아이패드 ㅋㅋㅋㅋ갤탭도 꽤 괜찮다고 하던데 ㅋㅋㅋ저도 아직 태블릿은 없어서 나중에 하나 장만하고 싶네요",
          "original_form": "오 아이패드 ㅋㅋㅋㅋ갤탭도 꽤 괜찮다고 하던데 ㅋㅋㅋ저도 아직 태블릿은 없어서 나중에 하나 장만하고 싶네요",
          "speaker_id": "2",
          "time": "20210831 13:58"
        },
        {
          "id": "MDRW2100036839.1.15",
          "form": "예전에 썼는데 넘 오래되어서요",
          "original_form": "예전에 썼는데 넘 오래되어서요",
          "speaker_id": "1",
          "time": "20210831 13:59"
        },
        {
          "id": "MDRW2100036839.1.16",
          "form": "근데 사려니 비싸네요",
          "original_form": "근데 사려니 비싸네요",
          "speaker_id": "1",
          "time": "20210831 13:59"
        },
        {
          "id": "MDRW2100036839.1.17",
          "form": "요즘 기기들이 정말 ㅋㅋㅌ 근데 또 좋긴 엄청 좋더라구요 ㅋㅋㅋ z플립 이번에 새로나온거 넘 이뻐보이더라구요",
          "original_form": "요즘 기기들이 정말 ㅋㅋㅌ 근데 또 좋긴 엄청 좋더라구요 ㅋㅋㅋ z플립 이번에 새로나온거 넘 이뻐보이더라구요",
          "speaker_id": "2",
          "time": "20210831 14:02"
        },
        {
          "id": "MDRW2100036839.1.18",
          "form": "아 접히는  폰이요?",
          "original_form": "아 접히는  폰이요?",
          "speaker_id": "1",
          "time": "20210831 14:03"
        },
        {
          "id": "MDRW2100036839.1.19",
          "form": "네네!! 그거 이번에 평도 좋더라구요 ㅋㅋㅋ궁금해용",
          "original_form": "네네!! 그거 이번에 평도 좋더라구요 ㅋㅋㅋ궁금해용",
          "speaker_id": "2",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100036839.1.20",
          "form": "다들 그런폰 쓰게될까요 궁금하네요",
          "original_form": "다들 그런폰 쓰게될까요 궁금하네요",
          "speaker_id": "1",
