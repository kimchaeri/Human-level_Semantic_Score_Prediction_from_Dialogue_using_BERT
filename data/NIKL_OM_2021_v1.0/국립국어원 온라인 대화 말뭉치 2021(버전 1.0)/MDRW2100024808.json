      "utterance": [
        {
          "id": "MDRW2100024808.1.1",
          "form": "저녁드셨나용",
          "original_form": "저녁드셨나용",
          "speaker_id": "1",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100024808.1.2",
          "form": "네 방금 막 먹었어요 ㅎㅎ",
          "original_form": "네 방금 막 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100024808.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100024808.1.4",
          "form": "넹 샌드위치먹었어요",
          "original_form": "넹 샌드위치먹었어요",
          "speaker_id": "1",
          "time": "20210831 19:00"
        },
        {
          "id": "MDRW2100024808.1.5",
          "form": "뭐드셨어용",
          "original_form": "뭐드셨어용",
          "speaker_id": "1",
          "time": "20210831 19:00"
        },
        {
          "id": "MDRW2100024808.1.6",
          "form": "저는 집에 있는 반찬에 밥먹었어요",
          "original_form": "저는 집에 있는 반찬에 밥먹었어요",
          "speaker_id": "2",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024808.1.7",
          "form": "혼자 사시나요?",
          "original_form": "혼자 사시나요?",
          "speaker_id": "1",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024808.1.8",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024808.1.9",
          "form": "아 그럼 반찬 다 직접 하신건가요",
          "original_form": "아 그럼 반찬 다 직접 하신건가요",
          "speaker_id": "1",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024808.1.10",
          "form": "아뇨ㅋㅋ 저는 그리 요리를 잘하는 편이 아니라서 집에서 엄마가 보내준 거랑 마트에서 사온 반찬이에요",
          "original_form": "아뇨ㅋㅋ 저는 그리 요리를 잘하는 편이 아니라서 집에서 엄마가 보내준 거랑 마트에서 사온 반찬이에요",
          "speaker_id": "2",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024808.1.11",
          "form": "아 ㅋㅋ할줄아는 요리는 뭐잇으신가요",
          "original_form": "아 ㅋㅋ할줄아는 요리는 뭐잇으신가요",
          "speaker_id": "1",
          "time": "20210831 19:29"
        },
        {
          "id": "MDRW2100024808.1.12",
          "form": "라면이랑 계란후라이 정도?",
          "original_form": "라면이랑 계란후라이 정도?",
          "speaker_id": "2",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024808.1.13",
          "form": "정말 딱 굶어죽지 않을 정도의 요리만 할 수 있어요",
          "original_form": "정말 딱 굶어죽지 않을 정도의 요리만 할 수 있어요",
          "speaker_id": "2",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024808.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 저랑 비슷하시네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저랑 비슷하시네요",
          "speaker_id": "1",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100024808.1.15",
          "form": "전 그냥 거기에 시판소스 파스탘ㅋㅋㅋㅋㅋ",
          "original_form": "전 그냥 거기에 시판소스 파스탘ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100024808.1.16",
          "form": "면만 삶고 볶기 ㅋㅋㅋㅋ",
          "original_form": "면만 삶고 볶기 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100024808.1.17",
          "form": "ㅋㅋㄱㅋㄱ 아 맞아요 ㅋㄱㅋㄱ 저도 파스타 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㄱ 아 맞아요 ㅋㄱㅋㄱ 저도 파스타 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024808.1.18",
          "form": "그리고 좀 수준높은 요리는 가끔 밀키트를 활용해요",
          "original_form": "그리고 좀 수준높은 요리는 가끔 밀키트를 활용해요",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024808.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ 저돜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저돜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024808.1.20",
          "form": "똑같다",
          "original_form": "똑같다",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024808.1.21",
          "form": "혼자 사니까 재료 일일히.다 사면",
          "original_form": "혼자 사니까 재료 일일히.다 사면",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024808.1.22",
          "form": "ㄹㄱㅋㅋㅋㄱㅋ",
          "original_form": "ㄹㄱㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024808.1.23",
          "form": "오히려 버리는게많구",
          "original_form": "오히려 버리는게많구",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024808.1.24",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024808.1.25",
          "form": "차라리 딱 쓸 양만 손질되어있는 밀키트가 합리적이죠",
          "original_form": "차라리 딱 쓸 양만 손질되어있는 밀키트가 합리적이죠",
          "speaker_id": "2",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024808.1.26",
          "form": "ㅋㅋ 밀키트 머머 드셔봤나요",
          "original_form": "ㅋㅋ 밀키트 머머 드셔봤나요",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024808.1.27",
          "form": "스테이크랑 감바스 자주 시켜먹어요",
          "original_form": "스테이크랑 감바스 자주 시켜먹어요",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024808.1.28",
          "form": "최근엔 불고기솥밥 밀키트를 사봤는데 그건 좀 별로더라구요",
          "original_form": "최근엔 불고기솥밥 밀키트를 사봤는데 그건 좀 별로더라구요",
          "speaker_id": "2",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100024808.1.29",
          "form": "헐 ...저도 불고기솥밥 샀었는데 ㅋㅋㅋㅋ",
          "original_form": "헐 ...저도 불고기솥밥 샀었는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:39"
        },
        {
          "id": "MDRW2100024808.1.30",
          "form": "아 해먹어보셨어요?",
          "original_form": "아 해먹어보셨어요?",
          "speaker_id": "2",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100024808.1.31",
          "form": "제가 조리를 잘못해서 별로였을 수도 있어요 ㅋㄱㄱ",
          "original_form": "제가 조리를 잘못해서 별로였을 수도 있어요 ㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100024808.1.32",
          "form": "네 그냥 그랫어요 ㅋㅋ",
          "original_form": "네 그냥 그랫어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100024808.1.33",
          "form": "저도 프리시지 스테이크 감바스 또 삿어용",
          "original_form": "저도 프리시지 스테이크 감바스 또 삿어용",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100024808.1.34",
          "form": "맛있죠 뭔가 내가 해먹는건데도 맛있고 스테이크 채소도 잘 들어있어서 뭔가 영양도 챙기는 것 같죠 ㅋㄱㄱ",
          "original_form": "맛있죠 뭔가 내가 해먹는건데도 맛있고 스테이크 채소도 잘 들어있어서 뭔가 영양도 챙기는 것 같죠 ㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210831 19:51"
        },
        {
          "id": "MDRW2100024808.1.35",
          "form": "마지요 ㅋㅋㅋ혼자 고기사면 야채챙기기도귀찮규",
          "original_form": "마지요 ㅋㅋㅋ혼자 고기사면 야채챙기기도귀찮규",
          "speaker_id": "1",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100024808.1.36",
          "form": "밀키트는 정말 자취생에게 너무 고마운 존재에요 ㅋㅋㄱㅋㅋ",
          "original_form": "밀키트는 정말 자취생에게 너무 고마운 존재에요 ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:58"
        },
        {
          "id": "MDRW2100024808.1.37",
          "form": "맞아요 ㅋㅋㅋ 이번에 마라탕도 한번 사봣네요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ 이번에 마라탕도 한번 사봣네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:00"
        },
        {
          "id": "MDRW2100024808.1.38",
          "form": "ㅋㅋㅋ 정말 이젠 집에서 못만들 요리가 없어요",
          "original_form": "ㅋㅋㅋ 정말 이젠 집에서 못만들 요리가 없어요",
