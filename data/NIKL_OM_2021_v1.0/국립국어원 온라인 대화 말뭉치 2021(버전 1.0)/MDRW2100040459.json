        },
        {
          "id": "MDRW2100040459.1.2",
          "form": "배민커넥트라고 ㅋㅋ 배달해야겠음",
          "original_form": "배민커넥트라고 ㅋㅋ 배달해야겠음",
          "speaker_id": "1",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100040459.1.3",
          "form": "배달 알바를 한다고",
          "original_form": "배달 알바를 한다고",
          "speaker_id": "2",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100040459.1.4",
          "form": "옹옹 ㅋ",
          "original_form": "옹옹 ㅋ",
          "speaker_id": "1",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100040459.1.5",
          "form": "힘들겠다",
          "original_form": "힘들겠다",
          "speaker_id": "2",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100040459.1.6",
          "form": "아니다 할만하대",
          "original_form": "아니다 할만하대",
          "speaker_id": "1",
          "time": "20210904 20:45"
        },
        {
          "id": "MDRW2100040459.1.7",
          "form": "돈이 되나",
          "original_form": "돈이 되나",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040459.1.8",
          "form": "되지요 ㅋㅋ 하루에 2마넌정도는 벌수있을듯",
          "original_form": "되지요 ㅋㅋ 하루에 2마넌정도는 벌수있을듯",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040459.1.9",
          "form": "쉬어야지",
          "original_form": "쉬어야지",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040459.1.10",
          "form": "됐당 ㅋ 일이 그렇게 힘들지도않은데 더 할 수 있다",
          "original_form": "됐당 ㅋ 일이 그렇게 힘들지도않은데 더 할 수 있다",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040459.1.11",
          "form": "그러면 무리하지 말고 해 봐라",
          "original_form": "그러면 무리하지 말고 해 봐라",
          "speaker_id": "2",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040459.1.12",
          "form": "웅웅 ㅎ무리안하징",
          "original_form": "웅웅 ㅎ무리안하징",
          "speaker_id": "1",
          "time": "20210904 20:46"
        },
        {
          "id": "MDRW2100040459.1.13",
          "form": "그래 파이팅",
          "original_form": "그래 파이팅",
