      "utterance": [
        {
          "id": "MDRW2100026097.1.1",
          "form": "조만간 또 영화보러가자",
          "original_form": "조만간 또 영화보러가자",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026097.1.2",
          "form": "저번에 영화 오랫만에보니 좋았지?",
          "original_form": "저번에 영화 오랫만에보니 좋았지?",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026097.1.3",
          "form": "어 근데 너무 뒷줄이었어",
          "original_form": "어 근데 너무 뒷줄이었어",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026097.1.4",
          "form": "내가 괜찮아보이는 영화 알아놓음",
          "original_form": "내가 괜찮아보이는 영화 알아놓음",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026097.1.5",
          "form": "맞아 이번엔 조금 앞열에서 보자",
          "original_form": "맞아 이번엔 조금 앞열에서 보자",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026097.1.6",
          "form": "뭔데?",
          "original_form": "뭔데?",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026097.1.7",
          "form": "무슨 쉐이크 이런거였는데",
          "original_form": "무슨 쉐이크 이런거였는데",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026097.1.8",
          "form": "액션영화야",
          "original_form": "액션영화야",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026097.1.9",
          "form": "니가 좋아하는 영화겠당",
          "original_form": "니가 좋아하는 영화겠당",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026097.1.10",
          "form": "여자들 많이 나온대",
          "original_form": "여자들 많이 나온대",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026097.1.11",
          "form": "구월 중순 개봉",
          "original_form": "구월 중순 개봉",
          "speaker_id": "1",
          "time": "20210901 19:47"
        },
        {
          "id": "MDRW2100026097.1.12",
          "form": "기대되지",
          "original_form": "기대되지",
          "speaker_id": "1",
          "time": "20210901 19:47"
        },
        {
          "id": "MDRW2100026097.1.13",
          "form": "멋지겠당 냐 남자액션보다 여자액션이 좋아",
          "original_form": "멋지겠당 냐 남자액션보다 여자액션이 좋아",
          "speaker_id": "2",
          "time": "20210901 19:47"
        },
        {
          "id": "MDRW2100026097.1.14",
          "form": "근데 저번에 영화관갔을때 뭔 공포영화 개봉한다고 돼있었거든 이름 기억이 안나 근데",
          "original_form": "근데 저번에 영화관갔을때 뭔 공포영화 개봉한다고 돼있었거든 이름 기억이 안나 근데",
          "speaker_id": "1",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026097.1.15",
          "form": "나도 몰러 ㅎ\r\n뮤지컬도 보고싶다",
          "original_form": "나도 몰러 ㅎ\r\n뮤지컬도 보고싶다",
          "speaker_id": "2",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100026097.1.16",
          "form": "우리 마지막으로 본게 뭐야",
          "original_form": "우리 마지막으로 본게 뭐야",
          "speaker_id": "1",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026097.1.17",
          "form": "옥주현의 그거 뭐더라",
          "original_form": "옥주현의 그거 뭐더라",
          "speaker_id": "2",
          "time": "20210901 19:50"
        },
        {
          "id": "MDRW2100026097.1.18",
          "form": "위키드",
          "original_form": "위키드",
          "speaker_id": "1",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026097.1.19",
          "form": "아 마따",
          "original_form": "아 마따",
          "speaker_id": "2",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026097.1.20",
          "form": "지킬 앤 하이드도 좋았어",
          "original_form": "지킬 앤 하이드도 좋았어",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026097.1.21",
          "form": "나도 \r\n주제가 무지 불렀었는데\r\n지금 이순간 ㅎ",
          "original_form": "나도 \r\n주제가 무지 불렀었는데\r\n지금 이순간 ㅎ",
          "speaker_id": "2",
          "time": "20210901 19:53"
        }
