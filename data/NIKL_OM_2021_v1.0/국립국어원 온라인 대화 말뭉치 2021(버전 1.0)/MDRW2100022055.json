      "utterance": [
        {
          "id": "MDRW2100022055.1.1",
          "form": "아 저 어릴때 첫직업이",
          "original_form": "아 저 어릴때 첫직업이",
          "speaker_id": "1",
          "time": "20210829 01:38"
        },
        {
          "id": "MDRW2100022055.1.2",
          "form": "그럼 업무,적으로 괴롭히는 사람있어요?",
          "original_form": "그럼 업무,적으로 괴롭히는 사람있어요?",
          "speaker_id": "2",
          "time": "20210829 01:38"
        },
        {
          "id": "MDRW2100022055.1.3",
          "form": "아아ㅎㅎ name1님먼저ㅎ",
          "original_form": "아아ㅎㅎ &name1&님먼저ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:38"
        },
        {
          "id": "MDRW2100022055.1.4",
          "form": "직업은 아니구 첨엔 알바 ㅋ",
          "original_form": "직업은 아니구 첨엔 알바 ㅋ",
          "speaker_id": "1",
          "time": "20210829 01:38"
        },
        {
          "id": "MDRW2100022055.1.5",
          "form": "알바는 어떤거하셨어요?",
          "original_form": "알바는 어떤거하셨어요?",
          "speaker_id": "2",
          "time": "20210829 01:38"
        },
        {
          "id": "MDRW2100022055.1.6",
          "form": "백화점에서 판매업무 였어요",
          "original_form": "백화점에서 판매업무 였어요",
          "speaker_id": "1",
          "time": "20210829 01:38"
        },
        {
          "id": "MDRW2100022055.1.7",
          "form": "과장님이 알바  할거 그냥 사원되라고",
          "original_form": "과장님이 알바  할거 그냥 사원되라고",
          "speaker_id": "1",
          "time": "20210829 01:39"
        },
        {
          "id": "MDRW2100022055.1.8",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:39"
        },
        {
          "id": "MDRW2100022055.1.9",
          "form": "구렁이 담넘듯이 사원 됐다가",
          "original_form": "구렁이 담넘듯이 사원 됐다가",
          "speaker_id": "1",
          "time": "20210829 01:39"
        },
        {
          "id": "MDRW2100022055.1.10",
          "form": "왠지 잘파셨을듯ㅎ",
          "original_form": "왠지 잘파셨을듯ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:39"
        },
        {
          "id": "MDRW2100022055.1.11",
          "form": "경력 쌓고 매장 매출 1~2위 만들고",
          "original_form": "경력 쌓고 매장 매출 1~2위 만들고",
          "speaker_id": "1",
          "time": "20210829 01:40"
        },
        {
          "id": "MDRW2100022055.1.12",
          "form": "회사고 들어간 케이스여요",
          "original_form": "회사고 들어간 케이스여요",
          "speaker_id": "1",
          "time": "20210829 01:40"
        },
        {
          "id": "MDRW2100022055.1.13",
          "form": "우와ㅎㅎㅎㅎㅎ",
          "original_form": "우와ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:40"
        },
        {
          "id": "MDRW2100022055.1.14",
          "form": "대단하시네요ㅎ",
          "original_form": "대단하시네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:40"
        },
        {
          "id": "MDRW2100022055.1.15",
          "form": "대단은 아니고",
          "original_form": "대단은 아니고",
          "speaker_id": "1",
          "time": "20210829 01:40"
        },
        {
          "id": "MDRW2100022055.1.16",
          "form": "역시 똑순이 이신줄ㅎㅎ",
          "original_form": "역시 똑순이 이신줄ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:40"
        },
        {
          "id": "MDRW2100022055.1.17",
          "form": "그냥 뭐랄까나",
          "original_form": "그냥 뭐랄까나",
          "speaker_id": "1",
          "time": "20210829 01:40"
        },
        {
          "id": "MDRW2100022055.1.18",
          "form": "이미 그러실것 같더라구요ㅎ",
          "original_form": "이미 그러실것 같더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:40"
        },
        {
          "id": "MDRW2100022055.1.19",
          "form": "현장이 더 힘들어서 ㅋㅋ",
          "original_form": "현장이 더 힘들어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:40"
        },
        {
          "id": "MDRW2100022055.1.20",
          "form": "회사다 힘든지 모르나봐요ㅠㅋ",
          "original_form": "회사다 힘든지 모르나봐요ㅠㅋ",
          "speaker_id": "1",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022055.1.21",
          "form": "그래도 매장매출 1~2위면 엄청난거죠ㅎ",
          "original_form": "그래도 매장매출 1~2위면 엄청난거죠ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022055.1.22",
          "form": "그리고 백화점 브랜드면 어마어마한거구요ㄹ",
          "original_form": "그리고 백화점 브랜드면 어마어마한거구요ㄹ",
          "speaker_id": "2",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022055.1.23",
          "form": "어릴때 말도 못해서 퇴근하고 맨날 울엇어요ㅠㅋㅋ",
          "original_form": "어릴때 말도 못해서 퇴근하고 맨날 울엇어요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022055.1.24",
          "form": "잘하셨네요ㅎㅎ",
          "original_form": "잘하셨네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022055.1.25",
          "form": "에이 왜요ㅎㅎ",
          "original_form": "에이 왜요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022055.1.26",
          "form": "근데 저도 그땐 그랬어요ㅎ",
          "original_form": "근데 저도 그땐 그랬어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:42"
        },
        {
          "id": "MDRW2100022055.1.27",
          "form": "뭐가 힘들던지ㅎ 지금은 아무것도아닌데ㅎ",
          "original_form": "뭐가 힘들던지ㅎ 지금은 아무것도아닌데ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:42"
        }
      ]
    }
  ]
