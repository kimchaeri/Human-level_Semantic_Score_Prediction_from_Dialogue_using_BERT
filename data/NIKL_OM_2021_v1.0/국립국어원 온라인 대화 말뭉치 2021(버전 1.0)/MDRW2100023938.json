      "utterance": [
        {
          "id": "MDRW2100023938.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023938.1.2",
          "form": "동남아 여행 가본 적 있으신가요?",
          "original_form": "동남아 여행 가본 적 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023938.1.3",
          "form": "넵 안녕하세요!!",
          "original_form": "넵 안녕하세요!!",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023938.1.4",
          "form": "앗 전 동남아는 가본적없어요ㅜ!!",
          "original_form": "앗 전 동남아는 가본적없어요ㅜ!!",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023938.1.5",
          "form": "name1님은 가보셨나요??",
          "original_form": "&name1&님은 가보셨나요??",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023938.1.6",
          "form": "네! 저 베트남 여행 가본 적 있어요 ㅎㅎ",
          "original_form": "네! 저 베트남 여행 가본 적 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023938.1.7",
          "form": "오 어떠셨나요? 맛난거 많을 것 같은데",
          "original_form": "오 어떠셨나요? 맛난거 많을 것 같은데",
          "speaker_id": "2",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023938.1.8",
          "form": "하하 베트남... 맛있는 음식! 많았어요!",
          "original_form": "하하 베트남... 맛있는 음식! 많았어요!",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023938.1.9",
          "form": "한국에도 유명한 분짜가 아직도 맴돌아요",
          "original_form": "한국에도 유명한 분짜가 아직도 맴돌아요",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023938.1.10",
          "form": "분짜.... 현지음식은 다르겠죠?!!",
          "original_form": "분짜.... 현지음식은 다르겠죠?!!",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023938.1.11",
          "form": "한국에선 별로에ㅛ요? 먹어본 적 없어서..",
          "original_form": "한국에선 별로에ㅛ요? 먹어본 적 없어서..",
          "speaker_id": "1",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023938.1.12",
          "form": "현지에선 진짜 너무 맛있어가지고 저 두번 세번 시켜먹었어요 그 자리에서 ㅋㅋㅋㅋㅋ",
          "original_form": "현지에선 진짜 너무 맛있어가지고 저 두번 세번 시켜먹었어요 그 자리에서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023938.1.13",
          "form": "앗 전 좋아하는데 현지는 더 맛있을 것 같아요!",
          "original_form": "앗 전 좋아하는데 현지는 더 맛있을 것 같아요!",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023938.1.14",
          "form": "코로나 끝나면 동남아 여행도 계획해야겠어요ㅎㅎ",
          "original_form": "코로나 끝나면 동남아 여행도 계획해야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023938.1.15",
          "form": "거기서 반미도 먹어봤어야 했는데 못먹어본게 한이에요",
          "original_form": "거기서 반미도 먹어봤어야 했는데 못먹어본게 한이에요",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023938.1.16",
          "form": "동남아는 무조건 가야겠어요ㅎㅎ",
          "original_form": "동남아는 무조건 가야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023938.1.17",
          "form": "동남아 말고 또 다른곳은 어디가보셨어요?전 일본밖에 안가봐서....",
          "original_form": "동남아 말고 또 다른곳은 어디가보셨어요?전 일본밖에 안가봐서....",
          "speaker_id": "2",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023938.1.18",
          "form": "저 중국이랑 유럽여행이요~",
          "original_form": "저 중국이랑 유럽여행이요~",
          "speaker_id": "1",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023938.1.19",
          "form": "생각해보니 캄보디아도 다녀왔었네요 ㅎㅎ 일본은 어땠어요?",
          "original_form": "생각해보니 캄보디아도 다녀왔었네요 ㅎㅎ 일본은 어땠어요?",
          "speaker_id": "1",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023938.1.20",
          "form": "일본은 거리가 엄청 깨끗하고 사람들이 너무 친절해서 약간 무서울정도였어요ㅎㅋㅋ",
          "original_form": "일본은 거리가 엄청 깨끗하고 사람들이 너무 친절해서 약간 무서울정도였어요ㅎㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023938.1.21",
          "form": "와.. 진짜 일본인 되게 착하고 친절하다던데",
          "original_form": "와.. 진짜 일본인 되게 착하고 친절하다던데",
          "speaker_id": "1",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100023938.1.22",
          "form": "그런 면에서 선진국이긴 하네요",
          "original_form": "그런 면에서 선진국이긴 하네요",
          "speaker_id": "1",
          "time": "20210830 20:51"
        }
      ]
    }
