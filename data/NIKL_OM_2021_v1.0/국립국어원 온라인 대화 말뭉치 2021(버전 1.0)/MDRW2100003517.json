      "utterance": [
        {
          "id": "MDRW2100003517.1.1",
          "form": "회사 이사한다고 ?",
          "original_form": "회사 이사한다고 ?",
          "speaker_id": "1",
          "time": "20210821 14:16"
        },
        {
          "id": "MDRW2100003517.1.2",
          "form": "어 몇일전에 부동산에서 보고 갔어",
          "original_form": "어 몇일전에 부동산에서 보고 갔어",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003517.1.3",
          "form": "보고만간거야?",
          "original_form": "보고만간거야?",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003517.1.4",
          "form": "나도 물어봣는데 팔렸데",
          "original_form": "나도 물어봣는데 팔렸데",
          "speaker_id": "2",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003517.1.5",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003517.1.6",
          "form": "내년 1월쯤 이사간다는데",
          "original_form": "내년 1월쯤 이사간다는데",
          "speaker_id": "2",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003517.1.7",
          "form": "어디로",
          "original_form": "어디로",
          "speaker_id": "1",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003517.1.8",
          "form": "아직 알아보고 있다고 하네",
          "original_form": "아직 알아보고 있다고 하네",
          "speaker_id": "2",
          "time": "20210821 14:19"
        },
        {
          "id": "MDRW2100003517.1.9",
          "form": "그근처로 보는 거겟지",
          "original_form": "그근처로 보는 거겟지",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003517.1.10",
          "form": "판교랑 광교 보고있다는데",
          "original_form": "판교랑 광교 보고있다는데",
          "speaker_id": "2",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003517.1.11",
          "form": "판교면...",
          "original_form": "판교면...",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003517.1.12",
          "form": "지금 미금보다는 조금더 낳긴한데",
          "original_form": "지금 미금보다는 조금더 낳긴한데",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003517.1.13",
          "form": "광교로 가게되면 더가는 거라서",
          "original_form": "광교로 가게되면 더가는 거라서",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003517.1.14",
          "form": "다른곳은 안본다니",
          "original_form": "다른곳은 안본다니",
          "speaker_id": "1",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003517.1.15",
          "form": "다들 집이 그근처라서 안알아볼꺼 같은데",
          "original_form": "다들 집이 그근처라서 안알아볼꺼 같은데",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003517.1.16",
          "form": "좀더 봐약 겠구나",
          "original_form": "좀더 봐약 겠구나",
          "speaker_id": "1",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003517.1.17",
          "form": "어떻게 할지 고민좀 해봐야 할꺼같아",
          "original_form": "어떻게 할지 고민좀 해봐야 할꺼같아",
          "speaker_id": "2",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003517.1.18",
          "form": "무슨고민",
          "original_form": "무슨고민",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003517.1.19",
          "form": "따라갈지 말지 대해서",
          "original_form": "따라갈지 말지 대해서",
          "speaker_id": "2",
          "time": "20210821 14:22"
        }
      ]
