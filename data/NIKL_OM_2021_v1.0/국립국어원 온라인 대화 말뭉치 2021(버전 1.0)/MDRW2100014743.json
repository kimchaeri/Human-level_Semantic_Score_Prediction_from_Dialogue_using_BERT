      "utterance": [
        {
          "id": "MDRW2100014743.1.1",
          "form": "여행많이다니세요?",
          "original_form": "여행많이다니세요?",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014743.1.2",
          "form": "예전에는 잘 다녔는데 안 다닌지 꽤 됐네요",
          "original_form": "예전에는 잘 다녔는데 안 다닌지 꽤 됐네요",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014743.1.3",
          "form": "name2님은 최근에 어디 다녀오셨어요?",
          "original_form": "&name2&님은 최근에 어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014743.1.4",
          "form": "저는 고흥갔다왔어요ㅎㅎ",
          "original_form": "저는 고흥갔다왔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014743.1.5",
          "form": "나로호 우수센터요ㅎㅎ",
          "original_form": "나로호 우수센터요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014743.1.6",
          "form": "딸이있어서 둘이갔다왔는데 좋더라구요 ㅎ",
          "original_form": "딸이있어서 둘이갔다왔는데 좋더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014743.1.7",
          "form": "바다도있고ㅎㅎ",
          "original_form": "바다도있고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014743.1.8",
          "form": "오 우주센터에도 들어갈 수 있나요?",
          "original_form": "오 우주센터에도 들어갈 수 있나요?",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014743.1.9",
          "form": "그냥 전시관이애요ㅎㅎ",
          "original_form": "그냥 전시관이애요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014743.1.10",
          "form": "아이데리고 갈만하더라구요ㅎ",
          "original_form": "아이데리고 갈만하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014743.1.11",
          "form": "ㅋㅋ 우주 관련 내용일테니 아이들은 좋아하겠어요",
          "original_form": "ㅋㅋ 우주 관련 내용일테니 아이들은 좋아하겠어요",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014743.1.12",
          "form": "아무래도 그래요ㅎㅎㅎ",
          "original_form": "아무래도 그래요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014743.1.13",
          "form": "나중에 데리고가보셔요ㄹㄹㄹ",
          "original_form": "나중에 데리고가보셔요ㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014743.1.14",
          "form": "또 괜찮았던 곳 있으신가요?",
          "original_form": "또 괜찮았던 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014743.1.15",
          "form": "국내는 다 좋아요ㅎㅎ",
          "original_form": "국내는 다 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014743.1.16",
          "form": "어딜가나 사랑하는 사람과 행복하면 좋답니댜ㅎㅎ",
          "original_form": "어딜가나 사랑하는 사람과 행복하면 좋답니댜ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014743.1.17",
          "form": "ㅋㅋ 진리네요",
          "original_form": "ㅋㅋ 진리네요",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014743.1.18",
          "form": "혹시 혼자 여행도 가보신 적 있나요?",
          "original_form": "혹시 혼자 여행도 가보신 적 있나요?",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014743.1.19",
          "form": "혼자여행 많이가죠ㅎㅎ",
          "original_form": "혼자여행 많이가죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014743.1.20",
          "form": "드라이브하면서 음악듣는거좋아해요ㅎㅎ",
          "original_form": "드라이브하면서 음악듣는거좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014743.1.21",
          "form": "혼자자주가보셨어요?",
          "original_form": "혼자자주가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014743.1.22",
          "form": "감성여행이네요ㅋㅋ",
          "original_form": "감성여행이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014743.1.23",
          "form": "전 예전에 일본 혼자 갔었어요. 오사카 교토 나라 이쪽으로",
          "original_form": "전 예전에 일본 혼자 갔었어요. 오사카 교토 나라 이쪽으로",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014743.1.24",
          "form": "우와ㅎㅎ부럽습니다",
          "original_form": "우와ㅎㅎ부럽습니다",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014743.1.25",
          "form": "전 국내는 혼자많이갔는데",
          "original_form": "전 국내는 혼자많이갔는데",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014743.1.26",
          "form": "해외는 안가봤어요ㅎ",
          "original_form": "해외는 안가봤어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014743.1.27",
          "form": "어땠어요?",
          "original_form": "어땠어요?",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014743.1.28",
          "form": "대도시라 혼자 다니는 데 어려움은 없었어요 ㅋㅋ",
          "original_form": "대도시라 혼자 다니는 데 어려움은 없었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014743.1.29",
          "form": "말은 잘 안 통했지만요",
          "original_form": "말은 잘 안 통했지만요",
          "speaker_id": "1",
          "time": "20210825 21:00"
        }
      ]
    }
