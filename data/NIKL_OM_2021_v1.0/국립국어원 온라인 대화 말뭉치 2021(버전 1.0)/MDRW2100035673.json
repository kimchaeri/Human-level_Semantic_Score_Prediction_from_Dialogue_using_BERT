      "utterance": [
        {
          "id": "MDRW2100035673.1.1",
          "form": "연말무대 무슨 문제 있었나요",
          "original_form": "연말무대 무슨 문제 있었나요",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035673.1.2",
          "form": "근데 거기서 그 멤버가 춤을 완전 심하게",
          "original_form": "근데 거기서 그 멤버가 춤을 완전 심하게",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035673.1.3",
          "form": "날리고",
          "original_form": "날리고",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035673.1.4",
          "form": "아예 박자 놓치고 그래서",
          "original_form": "아예 박자 놓치고 그래서",
          "speaker_id": "1",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035673.1.5",
          "form": "아 뚝딱거리면서 추는건",
          "original_form": "아 뚝딱거리면서 추는건",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035673.1.6",
          "form": "뭐라고 못하는데",
          "original_form": "뭐라고 못하는데",
          "speaker_id": "2",
          "time": "20210829 22:15"
        },
        {
          "id": "MDRW2100035673.1.7",
          "form": "좀 난리났었어요",
          "original_form": "좀 난리났었어요",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035673.1.8",
          "form": "날리고 놓치고 하는건...",
          "original_form": "날리고 놓치고 하는건...",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035673.1.9",
          "form": "저거는 연습부족이라구",
          "original_form": "저거는 연습부족이라구",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035673.1.10",
          "form": "그니깐요...",
          "original_form": "그니깐요...",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035673.1.11",
          "form": "기회가 되신다면 함 봐보세요 ,,",
          "original_form": "기회가 되신다면 함 봐보세요 ,,",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035673.1.12",
          "form": "충격이랍니다 하하",
          "original_form": "충격이랍니다 하하",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035673.1.13",
          "form": "ㅋㅋㅋ예... 그친구 클린한 멤버도 아니지 않나요",
          "original_form": "ㅋㅋㅋ예... 그친구 클린한 멤버도 아니지 않나요",
          "speaker_id": "2",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035673.1.14",
          "form": "아 맞아요 ㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035673.1.15",
          "form": "그친구 참 혼자서 ..",
          "original_form": "그친구 참 혼자서 ..",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100035673.1.16",
          "form": "구멍이군요 .. ^^",
          "original_form": "구멍이군요 .. ^^",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035673.1.17",
          "form": "그니깐유...",
          "original_form": "그니깐유...",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035673.1.18",
          "form": "다른 멤버들은 진짜",
          "original_form": "다른 멤버들은 진짜",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035673.1.19",
          "form": "다들 넘 잘춰서",
          "original_form": "다들 넘 잘춰서",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035673.1.20",
          "form": "더 비교되는 거 같기도 하고요",
          "original_form": "더 비교되는 거 같기도 하고요",
          "speaker_id": "2",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100035673.1.21",
          "form": "맞아요 진짜",
          "original_form": "맞아요 진짜",
          "speaker_id": "1",
          "time": "20210829 22:17"
        }
      ]
