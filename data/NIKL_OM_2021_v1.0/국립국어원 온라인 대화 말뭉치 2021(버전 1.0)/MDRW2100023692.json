      "utterance": [
        {
          "id": "MDRW2100023692.1.1",
          "form": "여자에게 재밌는 군대썰이라...",
          "original_form": "여자에게 재밌는 군대썰이라...",
          "speaker_id": "2",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100023692.1.2",
          "form": "없어요ㅋㅋㅋㅋㅋ",
          "original_form": "없어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023692.1.3",
          "form": "물음표안감",
          "original_form": "물음표안감",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023692.1.4",
          "form": "귀신본썰도좋아요",
          "original_form": "귀신본썰도좋아요",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023692.1.5",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023692.1.6",
          "form": "귀신은 아니고",
          "original_form": "귀신은 아니고",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023692.1.7",
          "form": "제가 의무대 근무해서",
          "original_form": "제가 의무대 근무해서",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100023692.1.8",
          "form": "사건사고나면 거진 우리를 거쳐가거든요",
          "original_form": "사건사고나면 거진 우리를 거쳐가거든요",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023692.1.9",
          "form": "헐,,,자,,살?",
          "original_form": "헐,,,자,,살?",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100023692.1.10",
          "form": "아뭐야",
          "original_form": "아뭐야",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100023692.1.11",
          "form": "살자",
          "original_form": "살자",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100023692.1.12",
          "form": "살자로 합시자",
          "original_form": "살자로 합시자",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100023692.1.13",
          "form": "ㄱㄱ",
          "original_form": "ㄱㄱ",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100023692.1.14",
          "form": "살자는 부대에 두명 있었는데",
          "original_form": "살자는 부대에 두명 있었는데",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100023692.1.15",
          "form": "한명은 섬이라 그대로 헬기로 날아갔고",
          "original_form": "한명은 섬이라 그대로 헬기로 날아갔고",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100023692.1.16",
          "form": "왜그랫대요?",
          "original_form": "왜그랫대요?",
          "speaker_id": "1",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023692.1.17",
          "form": "아씨",
          "original_form": "아씨",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023692.1.18",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023692.1.19",
          "form": "아씨",
          "original_form": "아씨",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023692.1.20",
          "form": "한명은 동인천역 모",
          "original_form": "한명은 동인천역 모",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023692.1.21",
          "form": "텔에서 살자해가지고",
          "original_form": "텔에서 살자해가지고",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023692.1.22",
          "form": "시체 수거하러 갔어요..",
          "original_form": "시체 수거하러 갔어요..",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023692.1.23",
          "form": "헐,,,,",
          "original_form": "헐,,,,",
          "speaker_id": "1",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023692.1.24",
          "form": "왜그랬는지는 모르죠뭐",
          "original_form": "왜그랬는지는 모르죠뭐",
          "speaker_id": "2",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023692.1.25",
          "form": "수거도 직접해요,,?",
          "original_form": "수거도 직접해요,,?",
          "speaker_id": "1",
          "time": "20210830 14:53"
        },
        {
          "id": "MDRW2100023692.1.26",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023692.1.27",
          "form": "벌레 엄청 꼬였어요...",
          "original_form": "벌레 엄청 꼬였어요...",
          "speaker_id": "2",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023692.1.28",
          "form": "냄새",
          "original_form": "냄새",
          "speaker_id": "2",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023692.1.29",
          "form": "아ㅠㅠㅠㅠㅠㅠㅠ진짜 충격젹이였겠다",
          "original_form": "아ㅠㅠㅠㅠㅠㅠㅠ진짜 충격젹이였겠다",
          "speaker_id": "1",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023692.1.30",
          "form": "나고",
          "original_form": "나고",
          "speaker_id": "2",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023692.1.31",
          "form": "별게 다 나쁜말이네",
          "original_form": "별게 다 나쁜말이네",
          "speaker_id": "2",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023692.1.32",
          "form": "저 군대때 연평도 포격도발있어가지구..",
          "original_form": "저 군대때 연평도 포격도발있어가지구..",
          "speaker_id": "2",
          "time": "20210830 14:54"
        },
        {
          "id": "MDRW2100023692.1.33",
          "form": "심지어 연평도가 저희 예하부대였거든요",
          "original_form": "심지어 연평도가 저희 예하부대였거든요",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023692.1.34",
          "form": "스펙타클했네요 군생활이",
          "original_form": "스펙타클했네요 군생활이",
          "speaker_id": "1",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023692.1.35",
          "form": "그래서 적십자에서 혈액 받아서",
          "original_form": "그래서 적십자에서 혈액 받아서",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023692.1.36",
          "form": "전투준비함",
          "original_form": "전투준비함",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023692.1.37",
          "form": "아스피린 한통씩 받고",
          "original_form": "아스피린 한통씩 받고",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023692.1.38",
          "form": "부대원 절반은 연평도 파견ㅋㅋ",
          "original_form": "부대원 절반은 연평도 파견ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023692.1.39",
          "form": "와 ㅋㅋㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023692.1.40",
          "form": "ㄹㅇ 전투 바로 가능",
          "original_form": "ㄹㅇ 전투 바로 가능",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023692.1.41",
          "form": "근데 의무병은 의대사람들 갈줄알앗는데 그건또아니네요",
          "original_form": "근데 의무병은 의대사람들 갈줄알앗는데 그건또아니네요",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023692.1.42",
          "form": "조금만 더 했으면 전쟁났을걸여",
          "original_form": "조금만 더 했으면 전쟁났을걸여",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023692.1.43",
          "form": "ㅇㅇ 반은 맞고 반은 아닌 사람들",
          "original_form": "ㅇㅇ 반은 맞고 반은 아닌 사람들",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023692.1.44",
          "form": "그래서 군대 친구 간호사랑 방사선사 있어요",
          "original_form": "그래서 군대 친구 간호사랑 방사선사 있어요",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023692.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023692.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023692.1.47",
          "form": "부천 성모병원이랑 저기 서울 아산병원에 하나씩",
          "original_form": "부천 성모병원이랑 저기 서울 아산병원에 하나씩",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023692.1.48",
          "form": "소개팅좀해주세요",
          "original_form": "소개팅좀해주세요",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023692.1.49",
          "form": "애기낳았는데 언제 보려나",
          "original_form": "애기낳았는데 언제 보려나",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023692.1.50",
          "form": "망할코로나",
          "original_form": "망할코로나",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023692.1.51",
          "form": "이미 결혼 했습니다만",
          "original_form": "이미 결혼 했습니다만",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023692.1.52",
          "form": "애기낳앗다구요?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "애기낳앗다구요?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023692.1.53",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023692.1.54",
          "form": "저번달에 한명 낳았구",
          "original_form": "저번달에 한명 낳았구",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100023692.1.55",
          "form": "한 친구는 결혼할 것 같은데 소식이 아직 없네요?",
          "original_form": "한 친구는 결혼할 것 같은데 소식이 아직 없네요?",
          "speaker_id": "2",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023692.1.56",
          "form": "코로나 터지고 다들못봄",
          "original_form": "코로나 터지고 다들못봄",
          "speaker_id": "2",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023692.1.57",
          "form": "그쪽은 병원이라서 서로 조심하느라",
          "original_form": "그쪽은 병원이라서 서로 조심하느라",
          "speaker_id": "2",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023692.1.58",
          "form": "진짜 살자하는사람들 많네요 말로만들어봤는데",
          "original_form": "진짜 살자하는사람들 많네요 말로만들어봤는데",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023692.1.59",
          "form": "아 더 그렇겟네요",
          "original_form": "아 더 그렇겟네요",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100023692.1.60",
          "form": "근데 부대생활하는데 2명이면 양호하죠",
          "original_form": "근데 부대생활하는데 2명이면 양호하죠",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023692.1.61",
          "form": "2년에 2명이었으니",
          "original_form": "2년에 2명이었으니",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023692.1.62",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023692.1.63",
          "form": "부모님들 진짜 마음찢어질듯 ㅠ",
          "original_form": "부모님들 진짜 마음찢어질듯 ㅠ",
          "speaker_id": "1",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023692.1.64",
          "form": "중위랑 상병이랑",
          "original_form": "중위랑 상병이랑",
