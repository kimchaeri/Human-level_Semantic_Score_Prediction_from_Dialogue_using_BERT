      "utterance": [
        {
          "id": "MDRW2100030904.1.1",
          "form": "저는 그래서 컵을 샀어요",
          "original_form": "저는 그래서 컵을 샀어요",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030904.1.2",
          "form": "carver",
          "original_form": "carver",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030904.1.3",
          "form": "허리 운동도 꾸준이 해야 한대요",
          "original_form": "허리 운동도 꾸준이 해야 한대요",
          "speaker_id": "1",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030904.1.4",
          "form": "컵을",
          "original_form": "컵을",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030904.1.5",
          "form": "커블",
          "original_form": "커블",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030904.1.6",
          "form": "편해요 그거",
          "original_form": "편해요 그거",
          "speaker_id": "1",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030904.1.7",
          "form": "커블의자요",
          "original_form": "커블의자요",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030904.1.8",
          "form": "잘 알아들었어요",
          "original_form": "잘 알아들었어요",
          "speaker_id": "1",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030904.1.9",
          "form": "저녁을 늦게 먹다 보니까 밥을 먹고 나서 잠이 오면 계속 바로 누워 자서",
          "original_form": "저녁을 늦게 먹다 보니까 밥을 먹고 나서 잠이 오면 계속 바로 누워 자서",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030904.1.10",
          "form": "캬캬",
          "original_form": "캬캬",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030904.1.11",
          "form": "역류성 식도염이 걸려서요",
          "original_form": "역류성 식도염이 걸려서요",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030904.1.12",
          "form": "그 의자 괜찮나요",
          "original_form": "그 의자 괜찮나요",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030904.1.13",
          "form": "컵을 사고 나서 거기 앉아 있으면 바로 자게 되지는 않아요",
          "original_form": "컵을 사고 나서 거기 앉아 있으면 바로 자게 되지는 않아요",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030904.1.14",
          "form": "흔들흔들 거리던데",
          "original_form": "흔들흔들 거리던데",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030904.1.15",
          "form": "허리가 편한가요",
          "original_form": "허리가 편한가요",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030904.1.16",
          "form": "괜찮은 거 같아요 게임을 할 때도 거기에 앉아서 하면 허리가 덜 아파요",
          "original_form": "괜찮은 거 같아요 게임을 할 때도 거기에 앉아서 하면 허리가 덜 아파요",
          "speaker_id": "2",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030904.1.17",
          "form": "많이들 샀더라 구요",
          "original_form": "많이들 샀더라 구요",
