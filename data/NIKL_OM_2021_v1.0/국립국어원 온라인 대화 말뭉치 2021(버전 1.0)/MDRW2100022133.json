      "utterance": [
        {
          "id": "MDRW2100022133.1.1",
          "form": "안녕하세용 아침 드셨야용?",
          "original_form": "안녕하세용 아침 드셨야용?",
          "speaker_id": "1",
          "time": "20210829 08:53"
        },
        {
          "id": "MDRW2100022133.1.2",
          "form": "안녕하세요. 간단하게 먹으려구요",
          "original_form": "안녕하세요. 간단하게 먹으려구요",
          "speaker_id": "2",
          "time": "20210829 08:58"
        },
        {
          "id": "MDRW2100022133.1.3",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 08:58"
        },
        {
          "id": "MDRW2100022133.1.4",
          "form": "전 방금 카레 먹고 왔어용!",
          "original_form": "전 방금 카레 먹고 왔어용!",
          "speaker_id": "1",
          "time": "20210829 08:58"
        },
        {
          "id": "MDRW2100022133.1.5",
          "form": "뭐 드시나용?",
          "original_form": "뭐 드시나용?",
          "speaker_id": "1",
          "time": "20210829 08:58"
        },
        {
          "id": "MDRW2100022133.1.6",
          "form": "냉동피자 데워 먹으려고요",
          "original_form": "냉동피자 데워 먹으려고요",
          "speaker_id": "2",
          "time": "20210829 08:59"
        },
        {
          "id": "MDRW2100022133.1.7",
          "form": "와 냉동피자 요즘 잘 나오더라구요ㅋㅋㅋ",
          "original_form": "와 냉동피자 요즘 잘 나오더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 08:59"
        },
        {
          "id": "MDRW2100022133.1.8",
          "form": "겉이 빵 테두리 없는걸로",
          "original_form": "겉이 빵 테두리 없는걸로",
          "speaker_id": "1",
          "time": "20210829 08:59"
        },
        {
          "id": "MDRW2100022133.1.9",
          "form": "어? 지금 데우고 있는 거 그거에요",
          "original_form": "어? 지금 데우고 있는 거 그거에요",
          "speaker_id": "2",
          "time": "20210829 09:10"
        },
        {
          "id": "MDRW2100022133.1.10",
          "form": "엇ㅋㅋㅋㅋ 우연이네용ㅋㅋㅋ",
          "original_form": "엇ㅋㅋㅋㅋ 우연이네용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 09:10"
        },
        {
          "id": "MDRW2100022133.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ노엣지 피자 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ노엣지 피자 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:10"
        },
        {
          "id": "MDRW2100022133.1.12",
          "form": "전 냉동피자 이게 제일 맛있어서",
          "original_form": "전 냉동피자 이게 제일 맛있어서",
          "speaker_id": "1",
          "time": "20210829 09:10"
        },
        {
          "id": "MDRW2100022133.1.13",
          "form": "이것만 먹어요",
          "original_form": "이것만 먹어요",
          "speaker_id": "1",
          "time": "20210829 09:10"
        },
        {
          "id": "MDRW2100022133.1.14",
          "form": "ㅋㅋㅋㅋ 그냥 세일해서 샀는데 잘 골랐네요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 그냥 세일해서 샀는데 잘 골랐네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100022133.1.15",
          "form": "아직 안드셔보신건가용?ㅋㅋㅋㅋ",
          "original_form": "아직 안드셔보신건가용?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100022133.1.16",
          "form": "넘 맛있어요",
          "original_form": "넘 맛있어요",
          "speaker_id": "1",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100022133.1.17",
          "form": "밀가루가 안딱딱하고ㅋㅋ",
          "original_form": "밀가루가 안딱딱하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100022133.1.18",
          "form": "아직 데우고 있는 중이라 맛은 못 봤어요",
          "original_form": "아직 데우고 있는 중이라 맛은 못 봤어요",
          "speaker_id": "2",
          "time": "20210829 09:13"
        },
        {
          "id": "MDRW2100022133.1.19",
          "form": "아하 드셔보시고 알려주세용!ㅎㅎ",
          "original_form": "아하 드셔보시고 알려주세용!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 09:13"
        },
        {
          "id": "MDRW2100022133.1.20",
          "form": "냉동식품중에",
          "original_form": "냉동식품중에",
          "speaker_id": "1",
          "time": "20210829 09:13"
        },
        {
          "id": "MDRW2100022133.1.21",
          "form": "치즈볼도 좋아하는데",
