      "utterance": [
        {
          "id": "MDRW2100043752.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043752.1.2",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043752.1.3",
          "form": "안녕하세요~ 저녁은 족발먹었습니다 ㅋㅋ 아침드셨나요",
          "original_form": "안녕하세요~ 저녁은 족발먹었습니다 ㅋㅋ 아침드셨나요",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100043752.1.4",
          "form": "아뇨 아직 안먹었어요",
          "original_form": "아뇨 아직 안먹었어요",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100043752.1.5",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100043752.1.6",
          "form": "저는 원래 아침을 안먹어요 ㅋㅋ name1님은 챙겨드셔요?",
          "original_form": "저는 원래 아침을 안먹어요 ㅋㅋ &name1&님은 챙겨드셔요?",
          "speaker_id": "2",
          "time": "20210908 08:21"
        },
        {
          "id": "MDRW2100043752.1.7",
          "form": "저는 종종 먹는 편이에요 ㅋㅋㅋ",
          "original_form": "저는 종종 먹는 편이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100043752.1.8",
          "form": "오 본가에서 사시나요?",
          "original_form": "오 본가에서 사시나요?",
          "speaker_id": "2",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100043752.1.9",
          "form": "아뇨 나와 살아요!",
          "original_form": "아뇨 나와 살아요!",
          "speaker_id": "1",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100043752.1.10",
          "form": "근데 아침 안먹으면 속쓰려셔",
          "original_form": "근데 아침 안먹으면 속쓰려셔",
          "speaker_id": "1",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100043752.1.11",
          "form": "먹게되더라구요",
          "original_form": "먹게되더라구요",
          "speaker_id": "1",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100043752.1.12",
          "form": "오 혼자 사시면 아침 먹기 힘든데..",
          "original_form": "오 혼자 사시면 아침 먹기 힘든데..",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100043752.1.13",
          "form": "그냥 간단하게 우유랑 시리얼 먹어요",
          "original_form": "그냥 간단하게 우유랑 시리얼 먹어요",
          "speaker_id": "1",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100043752.1.14",
          "form": "아 그런거면 괜찮겠어요 ㅋㅋ",
          "original_form": "아 그런거면 괜찮겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100043752.1.15",
          "form": "네 편하더라구요 ㅋㅋㅋ",
          "original_form": "네 편하더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100043752.1.16",
          "form": "저도 그렇게라도먹어볼까봐요 ㅋㅋㅋ 귀찮아서",
          "original_form": "저도 그렇게라도먹어볼까봐요 ㅋㅋㅋ 귀찮아서",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100043752.1.17",
          "form": "한번 시도해보세요 ㅋㅋㅋ",
          "original_form": "한번 시도해보세요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100043752.1.18",
          "form": "요거트에 말아먹어도 괜찮아요",
