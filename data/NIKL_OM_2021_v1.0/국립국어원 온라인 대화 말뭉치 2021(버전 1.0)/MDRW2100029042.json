      "utterance": [
        {
          "id": "MDRW2100029042.1.1",
          "form": "직업 스트레스가 있으신가요?",
          "original_form": "직업 스트레스가 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029042.1.2",
          "form": "스트레스 있어요??",
          "original_form": "스트레스 있어요??",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029042.1.3",
          "form": "네네 많습니다. 많아요 아주 많다구요",
          "original_form": "네네 많습니다. 많아요 아주 많다구요",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029042.1.4",
          "form": "당신은 있습니까??",
          "original_form": "당신은 있습니까??",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029042.1.5",
          "form": "네 있습니다",
          "original_form": "네 있습니다",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029042.1.6",
          "form": "가끔 악플이 달려요",
          "original_form": "가끔 악플이 달려요",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029042.1.7",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029042.1.8",
          "form": "연예인이신가봐요",
          "original_form": "연예인이신가봐요",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029042.1.9",
          "form": "인플러서 일을 겸업하고 있어요",
          "original_form": "인플러서 일을 겸업하고 있어요",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029042.1.10",
          "form": "인플런서",
          "original_form": "인플런서",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029042.1.11",
          "form": "대학원생이면 아르바이트도 하시나요?",
          "original_form": "대학원생이면 아르바이트도 하시나요?",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029042.1.12",
          "form": "안해요 시간이 없어요",
          "original_form": "안해요 시간이 없어요",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029042.1.13",
          "form": "저는 하루 2시간 정도는 월루한답니다",
          "original_form": "저는 하루 2시간 정도는 월루한답니다",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029042.1.14",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029042.1.15",
          "form": "맨날 일만 하세요?",
          "original_form": "맨날 일만 하세요?",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029042.1.16",
          "form": "저기요?",
          "original_form": "저기요?",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029042.1.17",
          "form": "똑똑",
          "original_form": "똑똑",
          "speaker_id": "1",
          "time": "20210903 08:40"
        },
        {
          "id": "MDRW2100029042.1.18",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100029042.1.19",
          "form": "연구소이신가요? 월급은 며칠에 받으세요?",
          "original_form": "연구소이신가요? 월급은 며칠에 받으세요?",
          "speaker_id": "1",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100029042.1.20",
          "form": "24일에 받습니다. 당신은 언제 받으시나요?",
          "original_form": "24일에 받습니다. 당신은 언제 받으시나요?",
          "speaker_id": "2",
          "time": "20210903 12:19"
        },
        {
          "id": "MDRW2100029042.1.21",
          "form": "저는 25일입니다 비슷하네요",
          "original_form": "저는 25일입니다 비슷하네요",
          "speaker_id": "1",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100029042.1.22",
          "form": "저보다 하루 늦게 받으시는군요",
          "original_form": "저보다 하루 늦게 받으시는군요",
          "speaker_id": "2",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100029042.1.23",
          "form": "점심은 드셨나요? 점심시간이네요",
          "original_form": "점심은 드셨나요? 점심시간이네요",
          "speaker_id": "2",
          "time": "20210903 12:48"
        },
        {
          "id": "MDRW2100029042.1.24",
          "form": "코로나 때문에 햄버서 테이크 아웃해서 들고 갈 예정이에요",
          "original_form": "코로나 때문에 햄버서 테이크 아웃해서 들고 갈 예정이에요",
          "speaker_id": "1",
          "time": "20210903 12:49"
        }
      ]
