      "utterance": [
        {
          "id": "MDRW2100003457.1.1",
          "form": "카페 좋아하시나요?",
          "original_form": "카페 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210820 23:20"
        },
        {
          "id": "MDRW2100003457.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003457.1.3",
          "form": "요즘은 자주 못가지만 시간 때울때 자주 가요",
          "original_form": "요즘은 자주 못가지만 시간 때울때 자주 가요",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003457.1.4",
          "form": "저도 코로나 이후로 카페를 잘 안가게되더라구요ㅠㅠ",
          "original_form": "저도 코로나 이후로 카페를 잘 안가게되더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003457.1.5",
          "form": "디저트 좋아하는데 배달만 시켜먹구..",
          "original_form": "디저트 좋아하는데 배달만 시켜먹구..",
          "speaker_id": "2",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003457.1.6",
          "form": "저는 디저트 배달 잘 되는지 몰랐는데",
          "original_form": "저는 디저트 배달 잘 되는지 몰랐는데",
          "speaker_id": "1",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003457.1.7",
          "form": "친구집에서 배달하는거 보고 신기했어요",
          "original_form": "친구집에서 배달하는거 보고 신기했어요",
          "speaker_id": "1",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003457.1.8",
          "form": "잘 오더라고요",
          "original_form": "잘 오더라고요",
          "speaker_id": "1",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003457.1.9",
          "form": "요즘은 배달 아니면 장사가 어려우니까",
          "original_form": "요즘은 배달 아니면 장사가 어려우니까",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003457.1.10",
          "form": "절대 배달안하던 동네 카페맛집들도 다 배달하더라구요",
          "original_form": "절대 배달안하던 동네 카페맛집들도 다 배달하더라구요",
          "speaker_id": "2",
          "time": "20210820 23:25"
        },
        {
          "id": "MDRW2100003457.1.11",
          "form": "근데 아무리 그래도 가서 먹는 느낌이 안나죠ㅠㅎㅎㅎ",
          "original_form": "근데 아무리 그래도 가서 먹는 느낌이 안나죠ㅠㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003457.1.12",
          "form": "맞아요ㅜㅜ 카페에서 먹는게 좋은데",
          "original_form": "맞아요ㅜㅜ 카페에서 먹는게 좋은데",
          "speaker_id": "1",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003457.1.13",
          "form": "최애 카페가 어디신가요?",
          "original_form": "최애 카페가 어디신가요?",
          "speaker_id": "2",
          "time": "20210820 23:49"
        },
        {
          "id": "MDRW2100003457.1.14",
          "form": "저는 스타벅스 기프티콘이 종종 들어와서",
          "original_form": "저는 스타벅스 기프티콘이 종종 들어와서",
          "speaker_id": "1",
          "time": "20210820 23:50"
        },
        {
          "id": "MDRW2100003457.1.15",
          "form": "스타벅스 자주 가요",
          "original_form": "스타벅스 자주 가요",
          "speaker_id": "1",
          "time": "20210820 23:50"
        },
        {
          "id": "MDRW2100003457.1.16",
          "form": "만만하게 가기 좋네요",
          "original_form": "만만하게 가기 좋네요",
          "speaker_id": "1",
          "time": "20210820 23:50"
        },
        {
          "id": "MDRW2100003457.1.17",
          "form": "스타벅스는 사이렌오더가 좋더라구요",
          "original_form": "스타벅스는 사이렌오더가 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 09:20"
        },
        {
          "id": "MDRW2100003457.1.18",
          "form": "사이렌오더 많이 들어봤는데 안써봤어요",
          "original_form": "사이렌오더 많이 들어봤는데 안써봤어요",
          "speaker_id": "1",
          "time": "20210821 13:19"
        },
        {
          "id": "MDRW2100003457.1.19",
          "form": "편한가요?",
          "original_form": "편한가요?",
          "speaker_id": "1",
          "time": "20210821 13:19"
        },
        {
          "id": "MDRW2100003457.1.20",
          "form": "편해요!!",
          "original_form": "편해요!!",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003457.1.21",
          "form": "스벅 다와갈때쯤 미리 주문해두면",
          "original_form": "스벅 다와갈때쯤 미리 주문해두면",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003457.1.22",
          "form": "대기 안해도 되구 ㅋㅋ 메뉴 커스텀도 편해요",
          "original_form": "대기 안해도 되구 ㅋㅋ 메뉴 커스텀도 편해요",
          "speaker_id": "2",
          "time": "20210821 13:57"
        },
        {
          "id": "MDRW2100003457.1.23",
          "form": "저도 다음엔 사이렌오더를 써야겠어요",
          "original_form": "저도 다음엔 사이렌오더를 써야겠어요",
          "speaker_id": "1",
          "time": "20210821 14:37"
        }
      ]
    }
