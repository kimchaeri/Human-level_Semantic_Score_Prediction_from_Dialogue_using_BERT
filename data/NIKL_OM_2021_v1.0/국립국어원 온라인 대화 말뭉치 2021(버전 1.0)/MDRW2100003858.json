      "utterance": [
        {
          "id": "MDRW2100003858.1.1",
          "form": "훌 앞이 안보일 정도로 비가 오네",
          "original_form": "훌 앞이 안보일 정도로 비가 오네",
          "speaker_id": "1",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100003858.1.2",
          "form": "하늘에 구멍 뚫린 듯",
          "original_form": "하늘에 구멍 뚫린 듯",
          "speaker_id": "2",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100003858.1.3",
          "form": "좀 있다가 음식 포장하러 가야하는데 ㅠㅠ",
          "original_form": "좀 있다가 음식 포장하러 가야하는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100003858.1.4",
          "form": "메뉴는 다 골랐심?",
          "original_form": "메뉴는 다 골랐심?",
          "speaker_id": "2",
          "time": "20210821 12:59"
        },
        {
          "id": "MDRW2100003858.1.5",
          "form": "ㅇㅇ 근데 이렇게 비 많이 오면 우산써도 다 젖을 듯",
          "original_form": "ㅇㅇ 근데 이렇게 비 많이 오면 우산써도 다 젖을 듯",
          "speaker_id": "1",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003858.1.6",
          "form": "그래도 맛있는 거 먹으려면 그 정도는 감수해야지 뭐 ㅎㅎ",
          "original_form": "그래도 맛있는 거 먹으려면 그 정도는 감수해야지 뭐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003858.1.7",
          "form": "비 사이로 막 가고 싶다 ㅋㅋ",
          "original_form": "비 사이로 막 가고 싶다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003858.1.8",
          "form": "헉 아재 개그야? ㅋㅋ",
          "original_form": "헉 아재 개그야? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 13:00"
        },
        {
          "id": "MDRW2100003858.1.9",
          "form": "아니 아짐 개그 ㅎㅎ",
          "original_form": "아니 아짐 개그 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 13:01"
        },
        {
          "id": "MDRW2100003858.1.10",
          "form": "차 타고 갈거니까 우산 쓸 일 별로 없을거야",
          "original_form": "차 타고 갈거니까 우산 쓸 일 별로 없을거야",
          "speaker_id": "2",
          "time": "20210821 13:01"
        },
        {
          "id": "MDRW2100003858.1.11",
          "form": "여긴 지하주차장이 엘베랑 연결 안되어 있어서 어찌됐건 우산은 써야 하니까 ㅎㅎ",
          "original_form": "여긴 지하주차장이 엘베랑 연결 안되어 있어서 어찌됐건 우산은 써야 하니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 13:01"
        },
        {
          "id": "MDRW2100003858.1.12",
          "form": "조금만 고생하면 입이 즐거울 것이야 ㅎㅎ",
          "original_form": "조금만 고생하면 입이 즐거울 것이야 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 13:01"
        },
        {
          "id": "MDRW2100003858.1.13",
          "form": "ㅇㅇ 즐거운 저녁을 위해ㅠ",
          "original_form": "ㅇㅇ 즐거운 저녁을 위해ㅠ",
          "speaker_id": "1",
          "time": "20210821 13:02"
        },
        {
          "id": "MDRW2100003858.1.14",
          "form": "비가 조금이라도 그치길 바래야지",
          "original_form": "비가 조금이라도 그치길 바래야지",
          "speaker_id": "2",
          "time": "20210821 13:02"
        },
        {
          "id": "MDRW2100003858.1.15",
          "form": "엇! 비 그치고 해뜨려고 한다",
          "original_form": "엇! 비 그치고 해뜨려고 한다",
          "speaker_id": "1",
          "time": "20210821 13:13"
        }
      ]
