      "utterance": [
        {
          "id": "MDRW2100042770.1.1",
          "form": "솔직히 의욕이 없다ㅋㅋㅋㅋㅋ",
          "original_form": "솔직히 의욕이 없다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:49"
        },
        {
          "id": "MDRW2100042770.1.2",
          "form": "그래서 자꾸 약이나 알아본다ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 자꾸 약이나 알아본다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:50"
        },
        {
          "id": "MDRW2100042770.1.3",
          "form": "정작 약 먹으면 안 될 거 같긴 하다ㅋㅋ",
          "original_form": "정작 약 먹으면 안 될 거 같긴 하다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:50"
        },
        {
          "id": "MDRW2100042770.1.4",
          "form": "그러니깐ㅋㅋㅋ",
          "original_form": "그러니깐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:50"
        },
        {
          "id": "MDRW2100042770.1.5",
          "form": "다시 돌아온다ㅋㅋ",
          "original_form": "다시 돌아온다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:50"
        },
        {
          "id": "MDRW2100042770.1.6",
          "form": "일단 뭐든지 해보자ㅋㅋ",
          "original_form": "일단 뭐든지 해보자ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:50"
        },
        {
          "id": "MDRW2100042770.1.7",
          "form": "그렇지~ 난 출퇴근 열심히 걸어볼게ㅋㅋㅋㅋ",
          "original_form": "그렇지~ 난 출퇴근 열심히 걸어볼게ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:50"
        },
        {
          "id": "MDRW2100042770.1.8",
          "form": "근데 이것도",
          "original_form": "근데 이것도",
          "speaker_id": "2",
          "time": "20210907 08:50"
        },
        {
          "id": "MDRW2100042770.1.9",
          "form": "거의 일견 하니까 그냥 몸이 적응된 듯",
          "original_form": "거의 일견 하니까 그냥 몸이 적응된 듯",
          "speaker_id": "2",
          "time": "20210907 08:50"
        },
        {
          "id": "MDRW2100042770.1.10",
          "form": "1년",
          "original_form": "1년",
          "speaker_id": "2",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042770.1.11",
          "form": "응응ㅋㅋ",
          "original_form": "응응ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042770.1.12",
          "form": "그러니깐ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042770.1.13",
          "form": "유지되는게 그것때문인듯ㅋ",
          "original_form": "유지되는게 그것때문인듯ㅋ",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042770.1.14",
          "form": "좋다ㅋㅋ",
          "original_form": "좋다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042770.1.15",
          "form": "그치 그나마 이거 때문에",
          "original_form": "그치 그나마 이거 때문에",
          "speaker_id": "2",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042770.1.16",
          "form": "더 이상 찌지는 않는 거 같은데",
          "original_form": "더 이상 찌지는 않는 거 같은데",
          "speaker_id": "2",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042770.1.17",
          "form": "너무나 유지하고 있다는 거지ㅋㅋㅋㅋㅋ",
          "original_form": "너무나 유지하고 있다는 거지ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042770.1.18",
          "form": "조금 빠져 줘도 될 텐데ㅋㅋㅋㅋ",
          "original_form": "조금 빠져 줘도 될 텐데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042770.1.19",
          "form": "ㅋㅋㅋ 그렇지ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 그렇지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042770.1.20",
          "form": "나두그래ㅋㅋㅋ",
          "original_form": "나두그래ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042770.1.21",
          "form": "오늘은 정말!ㅋㅋ옷정리도하고ㅋㅋ",
          "original_form": "오늘은 정말!ㅋㅋ옷정리도하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.22",
          "form": "벌써 할라고",
          "original_form": "벌써 할라고",
          "speaker_id": "2",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.23",
          "form": "운동도하고",
          "original_form": "운동도하고",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.24",
          "form": "해야겠음ㅋ",
          "original_form": "해야겠음ㅋ",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.25",
          "form": "아니 좀따가",
          "original_form": "아니 좀따가",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.26",
          "form": "나는 조금 밀어야겠다ㅋㅋㅋ",
          "original_form": "나는 조금 밀어야겠다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.27",
          "form": "아침먹고ㅋㅋ",
          "original_form": "아침먹고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.28",
          "form": "뭘 미는데?",
          "original_form": "뭘 미는데?",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.29",
          "form": "난 아직까지 덥다ㅋㅋㅋ",
          "original_form": "난 아직까지 덥다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.30",
          "form": "미룬다고?ㅋ",
          "original_form": "미룬다고?ㅋ",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.31",
          "form": "미뤄 ㅋㅋㅋ",
          "original_form": "미뤄 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.32",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042770.1.33",
          "form": "개떡같이 얘기해도 찰떡같이 알아들어 도ㅋㅋㅋ",
