      "utterance": [
        {
          "id": "MDRW2100017639.1.1",
          "form": "유튭 보시나요",
          "original_form": "유튭 보시나요",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017639.1.2",
          "form": "가끔이요ㅎㅎ \r\nname1님은요?",
          "original_form": "가끔이요ㅎㅎ \r\n&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017639.1.3",
          "form": "자주봅니다ㅎㅎ",
          "original_form": "자주봅니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017639.1.4",
          "form": "주로 어떤 영상을 보시나요?",
          "original_form": "주로 어떤 영상을 보시나요?",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017639.1.5",
          "form": "여행유튭이요",
          "original_form": "여행유튭이요",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017639.1.6",
          "form": "여행 좋아하시나봐요!\r\n저는 최근에 요리나 브이로그 그리고 티비프로그램 클립영상들 주로 봤던거같아요",
          "original_form": "여행 좋아하시나봐요!\r\n저는 최근에 요리나 브이로그 그리고 티비프로그램 클립영상들 주로 봤던거같아요",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017639.1.7",
          "form": "추천좀 해주세요!",
          "original_form": "추천좀 해주세요!",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017639.1.8",
          "form": "음..최근에는 효뎡/나달 두 채널을 자주 보았던거 같아요 편하게 보기 좋더라구요\r\n추천해주고싶은 채널 있으신가요?",
          "original_form": "음..최근에는 효뎡/나달 두 채널을 자주 보았던거 같아요 편하게 보기 좋더라구요\r\n추천해주고싶은 채널 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017639.1.9",
          "form": "빠니보틀이요!",
          "original_form": "빠니보틀이요!",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100017639.1.10",
          "form": "기억해두었다가 시간나면 한 번 봐야겠네요ㅎㅎ",
          "original_form": "기억해두었다가 시간나면 한 번 봐야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100017639.1.11",
          "form": "지금 미국이에요ㅎㅎ",
          "original_form": "지금 미국이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100017639.1.12",
          "form": "미국이요? 여행가신거에요?",
          "original_form": "미국이요? 여행가신거에요?",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100017639.1.13",
          "form": "백신맞고 갔더라구요ㅎㅎ",
          "original_form": "백신맞고 갔더라구요ㅎㅎ",
