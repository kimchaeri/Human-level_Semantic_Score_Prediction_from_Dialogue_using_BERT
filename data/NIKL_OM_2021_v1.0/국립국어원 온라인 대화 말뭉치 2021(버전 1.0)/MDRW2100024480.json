      "utterance": [
        {
          "id": "MDRW2100024480.1.1",
          "form": "근데 21학점이면",
          "original_form": "근데 21학점이면",
          "speaker_id": "2",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100024480.1.2",
          "form": "수업을 몇개듣는거예요?",
          "original_form": "수업을 몇개듣는거예요?",
          "speaker_id": "2",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100024480.1.3",
          "form": "9과목이요",
          "original_form": "9과목이요",
          "speaker_id": "1",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100024480.1.4",
          "form": "와........대박.....",
          "original_form": "와........대박.....",
          "speaker_id": "2",
          "time": "20210831 11:38"
        },
        {
          "id": "MDRW2100024480.1.5",
          "form": "공부 잘하시나봐요ㅋㅋㅋ",
          "original_form": "공부 잘하시나봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:38"
        },
        {
          "id": "MDRW2100024480.1.6",
          "form": "전과하려고",
          "original_form": "전과하려고",
          "speaker_id": "1",
          "time": "20210831 11:41"
        },
        {
          "id": "MDRW2100024480.1.7",
          "form": "수업 더들어야해요",
          "original_form": "수업 더들어야해요",
          "speaker_id": "1",
          "time": "20210831 11:41"
        },
        {
          "id": "MDRW2100024480.1.8",
          "form": "와...진짜 대학생활 열심히하신다",
          "original_form": "와...진짜 대학생활 열심히하신다",
          "speaker_id": "2",
          "time": "20210831 11:41"
        },
        {
          "id": "MDRW2100024480.1.9",
          "form": "저때는 전과도 공부잘해야 하는ㄷ",
          "original_form": "저때는 전과도 공부잘해야 하는ㄷ",
          "speaker_id": "2",
          "time": "20210831 11:41"
        },
        {
          "id": "MDRW2100024480.1.10",
          "form": "지금도 그렇겠죠?",
          "original_form": "지금도 그렇겠죠?",
          "speaker_id": "2",
          "time": "20210831 11:41"
        },
        {
          "id": "MDRW2100024480.1.11",
          "form": "공부.. 잘해야죠",
          "original_form": "공부.. 잘해야죠",
          "speaker_id": "1",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100024480.1.12",
          "form": "근데 예전이랑은 다를거같아요",
          "original_form": "근데 예전이랑은 다를거같아요",
          "speaker_id": "1",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100024480.1.13",
          "form": "수업 어떤거 들으세요?",
          "original_form": "수업 어떤거 들으세요?",
          "speaker_id": "2",
          "time": "20210831 12:13"
        },
        {
          "id": "MDRW2100024480.1.14",
          "form": "오늘!",
          "original_form": "오늘!",
          "speaker_id": "2",
          "time": "20210831 12:13"
        },
        {
          "id": "MDRW2100024480.1.15",
          "form": "오늘 교양듣고 전공들을 예정이에요",
          "original_form": "오늘 교양듣고 전공들을 예정이에요",
          "speaker_id": "1",
          "time": "20210831 12:14"
        },
        {
          "id": "MDRW2100024480.1.16",
          "form": "둘다 오늘 첫시간이에요",
          "original_form": "둘다 오늘 첫시간이에요",
          "speaker_id": "1",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100024480.1.17",
          "form": "교양수업 은근 재밌지않아요??",
          "original_form": "교양수업 은근 재밌지않아요??",
          "speaker_id": "2",
          "time": "20210831 12:34"
        },
        {
          "id": "MDRW2100024480.1.18",
          "form": "어떤 수업이었어요?!",
          "original_form": "어떤 수업이었어요?!",
          "speaker_id": "2",
          "time": "20210831 12:34"
        },
        {
          "id": "MDRW2100024480.1.19",
          "form": "만화와 관련된 수업인데",
          "original_form": "만화와 관련된 수업인데",
          "speaker_id": "1",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024480.1.20",
          "form": "초반이란 이론만 배우고 나중에 실습배울거같아요!",
          "original_form": "초반이란 이론만 배우고 나중에 실습배울거같아요!",
          "speaker_id": "1",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024480.1.21",
          "form": "애니메이션 만들거나 그럴거같아요!",
          "original_form": "애니메이션 만들거나 그럴거같아요!",
          "speaker_id": "1",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100024480.1.22",
          "form": "와.... 재밌겠다",
          "original_form": "와.... 재밌겠다",
          "speaker_id": "2",
          "time": "20210831 13:24"
        },
        {
          "id": "MDRW2100024480.1.23",
          "form": "그게 교양수업이에요??",
          "original_form": "그게 교양수업이에요??",
          "speaker_id": "2",
          "time": "20210831 13:27"
        },
        {
          "id": "MDRW2100024480.1.24",
          "form": "왜....우리학교는 그런 수업이 없었는지ㅜ",
          "original_form": "왜....우리학교는 그런 수업이 없었는지ㅜ",
          "speaker_id": "2",
          "time": "20210831 13:27"
        },
        {
          "id": "MDRW2100024480.1.25",
          "form": "수업너무힘드네요ㅜ",
          "original_form": "수업너무힘드네요ㅜ",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100024480.1.26",
          "form": "오늘은무슨수업",
          "original_form": "오늘은무슨수업",
          "speaker_id": "2",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100024480.1.27",
          "form": "하세ㅇ",
          "original_form": "하세ㅇ",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100024480.1.28",
          "form": "하세요?",
          "original_form": "하세요?",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100024480.1.29",
          "form": "창의수업",
          "original_form": "창의수업",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100024480.1.30",
          "form": "하고 잇어요",
          "original_form": "하고 잇어요",
          "speaker_id": "1",
          "time": "20210908 09:33"
        }
      ]
    }
  ]
