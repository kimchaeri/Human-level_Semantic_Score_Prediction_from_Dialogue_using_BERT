      "utterance": [
        {
          "id": "MDRW2100017213.1.1",
          "form": "무슨 음료 좋아하세요",
          "original_form": "무슨 음료 좋아하세요",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017213.1.2",
          "form": "아메리카노에 휩 올려먹는 걸 좋아해요",
          "original_form": "아메리카노에 휩 올려먹는 걸 좋아해요",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017213.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017213.1.4",
          "form": "저는 카페음료는 잘 모르고 그냥 마트에서 파는 음료들 좋아해요",
          "original_form": "저는 카페음료는 잘 모르고 그냥 마트에서 파는 음료들 좋아해요",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017213.1.5",
          "form": "오렌지주스나 아침햇살같은..",
          "original_form": "오렌지주스나 아침햇살같은..",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017213.1.6",
          "form": "아 주로 어떤 음료를 사시나요",
          "original_form": "아 주로 어떤 음료를 사시나요",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017213.1.7",
          "form": "아ㅎ 아침햇살 스테디죠",
          "original_form": "아ㅎ 아침햇살 스테디죠",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017213.1.8",
          "form": "그런데 이게 은근 호불호를 타더라고요",
          "original_form": "그런데 이게 은근 호불호를 타더라고요",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017213.1.9",
          "form": "데자와도 그렇고...",
          "original_form": "데자와도 그렇고...",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017213.1.10",
          "form": "데자와 내 사랑",
          "original_form": "데자와 내 사랑",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017213.1.11",
          "form": "데자와도 대체불가 맛이죠",
          "original_form": "데자와도 대체불가 맛이죠",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017213.1.12",
          "form": "지금까지 단종 안된 덴 다 이유가ㅎㅎ",
          "original_form": "지금까지 단종 안된 덴 다 이유가ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017213.1.13",
          "form": "민초는 좋아하시나요",
          "original_form": "민초는 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017213.1.14",
          "form": "저 민초 좋아해요!",
          "original_form": "저 민초 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017213.1.15",
          "form": "민초아이스크림이 맛있어요",
          "original_form": "민초아이스크림이 맛있어요",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017213.1.16",
          "form": "저와 통하셨네요",
          "original_form": "저와 통하셨네요",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017213.1.17",
          "form": "민초가 배척받아도 배라 인기멤이죠ㅎ",
          "original_form": "민초가 배척받아도 배라 인기멤이죠ㅎ",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017213.1.18",
          "form": "아 맞아요 민초 되게 오래가는 맛이죠ㅋㅋ",
          "original_form": "아 맞아요 민초 되게 오래가는 맛이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017213.1.19",
          "form": "베라에서 또 뭐 좋아하는맛 있으세요? 전 바람과 함께 사라지다가 맛있더라고요",
          "original_form": "베라에서 또 뭐 좋아하는맛 있으세요? 전 바람과 함께 사라지다가 맛있더라고요",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017213.1.20",
          "form": "전 아몬드봉봉 꼭 넣어요 민초와 투톱",
          "original_form": "전 아몬드봉봉 꼭 넣어요 민초와 투톱",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017213.1.21",
          "form": "바람과함께사라지다도 맛있죠ㅎ",
          "original_form": "바람과함께사라지다도 맛있죠ㅎ",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017213.1.22",
          "form": "저 아몬드봉봉 한번도 안먹어봤어요",
          "original_form": "저 아몬드봉봉 한번도 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017213.1.23",
          "form": "아몬드 씹히는 맛이 일품이에요",
          "original_form": "아몬드 씹히는 맛이 일품이에요",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017213.1.24",
          "form": "이것도 인기멤이더라고요",
          "original_form": "이것도 인기멤이더라고요",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017213.1.25",
          "form": "다음에 함 먹어봐야겠네요... 혹시 아몬드브리즈 음료 먹어보셨어요?",
          "original_form": "다음에 함 먹어봐야겠네요... 혹시 아몬드브리즈 음료 먹어보셨어요?",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017213.1.26",
          "form": "네 건강한 맛이더라고요ㅎ",
          "original_form": "네 건강한 맛이더라고요ㅎ",
          "speaker_id": "1",
          "time": "20210826 19:05"
        }
      ]
    }
