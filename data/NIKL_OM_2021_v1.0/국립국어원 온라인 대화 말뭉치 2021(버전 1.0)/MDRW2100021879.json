      "utterance": [
        {
          "id": "MDRW2100021879.1.1",
          "form": "야 나지금 심야괴담회보는데 짱무섭 ㅋㅋㅋ",
          "original_form": "야 나지금 심야괴담회보는데 짱무섭 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100021879.1.2",
          "form": "응응ㅋㅋ그게뭔데",
          "original_form": "응응ㅋㅋ그게뭔데",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100021879.1.3",
          "form": "심야괴담회모름??",
          "original_form": "심야괴담회모름??",
          "speaker_id": "1",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100021879.1.4",
          "form": "ㅇㅇ한번도 본적없음ㅋㅌ",
          "original_form": "ㅇㅇ한번도 본적없음ㅋㅌ",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100021879.1.5",
          "form": "ㄹㅇ말그대로 괴담 말해주는 프로그램 ㅋㅋㅋ",
          "original_form": "ㄹㅇ말그대로 괴담 말해주는 프로그램 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100021879.1.6",
          "form": "저번에 너가 같이보자고 했던건가",
          "original_form": "저번에 너가 같이보자고 했던건가",
          "speaker_id": "2",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100021879.1.7",
          "form": "ㅋㅋㅋ 근데 너 못볼거같어",
          "original_form": "ㅋㅋㅋ 근데 너 못볼거같어",
          "speaker_id": "1",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100021879.1.8",
          "form": "귀신얘기야?",
          "original_form": "귀신얘기야?",
          "speaker_id": "2",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100021879.1.9",
          "form": "어 ㄹㅇ 귀신얘기임ㅋㅋㅋㅋ",
          "original_form": "어 ㄹㅇ 귀신얘기임ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100021879.1.10",
          "form": "헐진짴ㅋㅋㅋ",
          "original_form": "헐진짴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100021879.1.11",
          "form": "설마하고 물어본건데ㅋ",
          "original_form": "설마하고 물어본건데ㅋ",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100021879.1.12",
          "form": "그런거볼수있음? ㅋㅋ",
          "original_form": "그런거볼수있음? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:44"
        },
        {
          "id": "MDRW2100021879.1.13",
          "form": "아닠ㅋㅋㅋ막 상황재연도 나와?",
          "original_form": "아닠ㅋㅋㅋ막 상황재연도 나와?",
          "speaker_id": "2",
          "time": "20210829 14:45"
        },
        {
          "id": "MDRW2100021879.1.14",
          "form": "어엋ㅋㅋㅋㅋㅋ 무서움 ㅋㅋㅋ",
          "original_form": "어엋ㅋㅋㅋㅋㅋ 무서움 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100021879.1.15",
          "form": "이번주에 울산나옴ㅋㅋ",
          "original_form": "이번주에 울산나옴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100021879.1.16",
          "form": "헐 울산에도 그런일이 있어?",
          "original_form": "헐 울산에도 그런일이 있어?",
          "speaker_id": "2",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100021879.1.17",
          "form": "어 짱 무서웠어 ㅋㅋㅋ",
          "original_form": "어 짱 무서웠어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100021879.1.18",
          "form": "사연있는귀신이야....?",
          "original_form": "사연있는귀신이야....?",
