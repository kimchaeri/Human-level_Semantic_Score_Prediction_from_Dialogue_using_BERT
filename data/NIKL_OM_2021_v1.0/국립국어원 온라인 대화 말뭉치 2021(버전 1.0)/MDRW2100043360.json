      "utterance": [
        {
          "id": "MDRW2100043360.1.1",
          "form": "저녁 모먹어?",
          "original_form": "저녁 모먹어?",
          "speaker_id": "1",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043360.1.2",
          "form": "뭐먹을까 추천해줘요",
          "original_form": "뭐먹을까 추천해줘요",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100043360.1.3",
          "form": "난 쌀국수 시켰오",
          "original_form": "난 쌀국수 시켰오",
          "speaker_id": "1",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100043360.1.4",
          "form": "면 먹어!!! 추천해",
          "original_form": "면 먹어!!! 추천해",
          "speaker_id": "1",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100043360.1.5",
          "form": "오 전 베트남 쌀국스 좋아하는데!!!",
          "original_form": "오 전 베트남 쌀국스 좋아하는데!!!",
          "speaker_id": "2",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043360.1.6",
          "form": "근데 다이어트중이에요",
          "original_form": "근데 다이어트중이에요",
          "speaker_id": "2",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043360.1.7",
          "form": "잉",
          "original_form": "잉",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043360.1.8",
          "form": "쌀국수는 살안쪄ㅠㅠ",
          "original_form": "쌀국수는 살안쪄ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:23"
        },
        {
          "id": "MDRW2100043360.1.9",
          "form": "진짜요?!",
          "original_form": "진짜요?!",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043360.1.10",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043360.1.11",
          "form": "근데 저 단백질만먹어야함 뱃살이 출렁거려요..",
          "original_form": "근데 저 단백질만먹어야함 뱃살이 출렁거려요..",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100043360.1.12",
          "form": "아아ㅠㅠ저도 그래요ㅠㅠㅠ",
          "original_form": "아아ㅠㅠ저도 그래요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043360.1.13",
          "form": "그럼 계란 츄천해요ㅠㅠ",
          "original_form": "그럼 계란 츄천해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043360.1.14",
          "form": "으아앙 계란 삶아묵어야겠다 ㅜㅜㅜ",
          "original_form": "으아앙 계란 삶아묵어야겠다 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043360.1.15",
          "form": "근데.이러고 야식먹을 각이에요",
          "original_form": "근데.이러고 야식먹을 각이에요",
          "speaker_id": "2",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043360.1.16",
          "form": "응 ㅠㅠ 크롭니트를 위해 참자",
          "original_form": "응 ㅠㅠ 크롭니트를 위해 참자",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043360.1.17",
          "form": "그러게 ㅋㅋ항상 난 야식 먹더라 ㅋㅋㅋ",
          "original_form": "그러게 ㅋㅋ항상 난 야식 먹더라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043360.1.18",
          "form": "ㅋㅋㅋㅋㅋ야식으로 쌀국수 시킬듯...",
          "original_form": "ㅋㅋㅋㅋㅋ야식으로 쌀국수 시킬듯...",
          "speaker_id": "2",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043360.1.19",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043360.1.20",
          "form": "지금 시켜먹어ㅠㅠ",
          "original_form": "지금 시켜먹어ㅠㅠ",
          "speaker_id": "1",
