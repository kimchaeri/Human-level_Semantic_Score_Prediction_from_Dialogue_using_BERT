      "utterance": [
        {
          "id": "MDRW2100000788.1.1",
          "form": "어떤일 하시나요?",
          "original_form": "어떤일 하시나요?",
          "speaker_id": "2",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100000788.1.2",
          "form": "사무직이요",
          "original_form": "사무직이요",
          "speaker_id": "1",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100000788.1.3",
          "form": "전 직장다니다 그만두고 전업해요",
          "original_form": "전 직장다니다 그만두고 전업해요",
          "speaker_id": "2",
          "time": "20210819 19:36"
        },
        {
          "id": "MDRW2100000788.1.4",
          "form": "어떤게 나으세요?",
          "original_form": "어떤게 나으세요?",
          "speaker_id": "1",
          "time": "20210819 19:37"
        },
        {
          "id": "MDRW2100000788.1.5",
          "form": "회사다닐때가 덜 스트레스 받는것 같아요",
          "original_form": "회사다닐때가 덜 스트레스 받는것 같아요",
          "speaker_id": "2",
          "time": "20210819 19:38"
        },
        {
          "id": "MDRW2100000788.1.6",
          "form": "육아가 젤 힘들다구 하더라구여",
          "original_form": "육아가 젤 힘들다구 하더라구여",
          "speaker_id": "1",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000788.1.7",
          "form": "코로나때문에 하루종일 같이있으니 회사가고. 싶어요",
          "original_form": "코로나때문에 하루종일 같이있으니 회사가고. 싶어요",
          "speaker_id": "2",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000788.1.8",
          "form": "아.",
          "original_form": "아.",
          "speaker_id": "1",
          "time": "20210819 19:40"
        },
        {
          "id": "MDRW2100000788.1.9",
          "form": "힘들어 보여요",
          "original_form": "힘들어 보여요",
          "speaker_id": "1",
          "time": "20210819 19:40"
        },
        {
          "id": "MDRW2100000788.1.10",
          "form": "회사다닐때도 업무스트레스가 없는건 아니었는데   사무실에 있을때는 나한테만 집중할수있으니 좋은것 같아요",
          "original_form": "회사다닐때도 업무스트레스가 없는건 아니었는데   사무실에 있을때는 나한테만 집중할수있으니 좋은것 같아요",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100000788.1.11",
          "form": "맞아요 내 시간이 필뇨한데",
          "original_form": "맞아요 내 시간이 필뇨한데",
          "speaker_id": "1",
          "time": "20210819 19:43"
        },
        {
          "id": "MDRW2100000788.1.12",
          "form": "직장다닐때 회식하는것도 재미있었는데 요즘은 사람들을 만날수 없네요",
          "original_form": "직장다닐때 회식하는것도 재미있었는데 요즘은 사람들을 만날수 없네요",
          "speaker_id": "2",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000788.1.13",
          "form": "네 모이기도 힘들구",
          "original_form": "네 모이기도 힘들구",
          "speaker_id": "1",
          "time": "20210819 19:45"
        },
        {
          "id": "MDRW2100000788.1.14",
          "form": "아이더 크면 구직활동을 해야 하나 고민이 되네요",
          "original_form": "아이더 크면 구직활동을 해야 하나 고민이 되네요",
          "speaker_id": "2",
          "time": "20210819 19:46"
