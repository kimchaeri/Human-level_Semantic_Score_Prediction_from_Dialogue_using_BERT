      "utterance": [
        {
          "id": "MDRW2100039316.1.1",
          "form": "불을 내다니... 저라도 접었어요....",
          "original_form": "불을 내다니... 저라도 접었어요....",
          "speaker_id": "1",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100039316.1.2",
          "form": "name2님 여행 좋아하셔요?!",
          "original_form": "&name2&님 여행 좋아하셔요?!",
          "speaker_id": "1",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100039316.1.3",
          "form": "음 좋지도 않고 싫지도 않고 그냥 그래요 ㅋㅋㅋㅋ",
          "original_form": "음 좋지도 않고 싫지도 않고 그냥 그래요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100039316.1.4",
          "form": "그래도 막상 어디 놀러가면 재밌긴 하더라구요",
          "original_form": "그래도 막상 어디 놀러가면 재밌긴 하더라구요",
          "speaker_id": "2",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100039316.1.5",
          "form": "name1님은 여행 좋아하시나요?",
          "original_form": "&name1&님은 여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100039316.1.6",
          "form": "저는 좋아해요! 특히 요새는 호캉스가 재밌더라고요!!!!!!",
          "original_form": "저는 좋아해요! 특히 요새는 호캉스가 재밌더라고요!!!!!!",
          "speaker_id": "1",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100039316.1.7",
          "form": "워낙 집순이라 그른가 ㅎ.ㅎ",
          "original_form": "워낙 집순이라 그른가 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100039316.1.8",
          "form": "오 저는 호캉스 한번도 안 가봤어요",
          "original_form": "오 저는 호캉스 한번도 안 가봤어요",
          "speaker_id": "2",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100039316.1.9",
          "form": "나중에 한 번 가보고 싶네요 ㅋㅋㅋ",
          "original_form": "나중에 한 번 가보고 싶네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100039316.1.10",
          "form": "완전 좋아요!! 넓은 침대에 뒹걸뒹굴 거리다거",
          "original_form": "완전 좋아요!! 넓은 침대에 뒹걸뒹굴 거리다거",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039316.1.11",
          "form": "좋아하는 배달음식 시키면 완전 천국!",
          "original_form": "좋아하는 배달음식 시키면 완전 천국!",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039316.1.12",
          "form": "물론 주변 관광도 조오금 하고 오면 좋아요",
          "original_form": "물론 주변 관광도 조오금 하고 오면 좋아요",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039316.1.13",
          "form": "혼캉스도 가세요?",
          "original_form": "혼캉스도 가세요?",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039316.1.14",
          "form": "네네! 저 호캉스는 거의 혼자 가요",
          "original_form": "네네! 저 호캉스는 거의 혼자 가요",
          "speaker_id": "1",
          "time": "20210903 18:08"
        },
        {
          "id": "MDRW2100039316.1.15",
          "form": "둘이서 가면 대화를 해야한다는 생각에...",
          "original_form": "둘이서 가면 대화를 해야한다는 생각에...",
          "speaker_id": "1",
          "time": "20210903 18:08"
        },
        {
          "id": "MDRW2100039316.1.16",
          "form": "약간 의무감이 생기걸랑요",
          "original_form": "약간 의무감이 생기걸랑요",
          "speaker_id": "1",
          "time": "20210903 18:08"
        },
        {
          "id": "MDRW2100039316.1.17",
          "form": "아하 ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:14"
        },
        {
          "id": "MDRW2100039316.1.18",
          "form": "뭔지 알아요",
          "original_form": "뭔지 알아요",
          "speaker_id": "2",
          "time": "20210903 18:14"
        },
        {
          "id": "MDRW2100039316.1.19",
          "form": "name1님 템플스테이는 가보셨나요?",
          "original_form": "&name1&님 템플스테이는 가보셨나요?",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039316.1.20",
          "form": "헉 아니요! 그 절로 가는 거 말씀하시는 거죠!?",
          "original_form": "헉 아니요! 그 절로 가는 거 말씀하시는 거죠!?",
          "speaker_id": "1",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039316.1.21",
          "form": "name2님은요?!",
          "original_form": "&name2&님은요?!",
          "speaker_id": "1",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039316.1.22",
          "form": "네네 전 가봤어요",
          "original_form": "네네 전 가봤어요",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039316.1.23",
          "form": "친구들이 가자고 해서 다녀왔는데 좋더라구요",
          "original_form": "친구들이 가자고 해서 다녀왔는데 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039316.1.24",
          "form": "헐 그럼 거기 가서는 보통 어떤 활동을 하나요!?",
          "original_form": "헐 그럼 거기 가서는 보통 어떤 활동을 하나요!?",
          "speaker_id": "1",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100039316.1.25",
          "form": "음 저는 휴식형으로 가서 체험은 안 했는데",
          "original_form": "음 저는 휴식형으로 가서 체험은 안 했는데",
          "speaker_id": "2",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039316.1.26",
          "form": "백팔배하고 묵주만들기 체험도 있더라구요",
          "original_form": "백팔배하고 묵주만들기 체험도 있더라구요",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039316.1.27",
          "form": "코로나때문에 못했지만..",
          "original_form": "코로나때문에 못했지만..",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039316.1.28",
          "form": "헐헐 목주만들기 완전 재밌을 것 같아요",
          "original_form": "헐헐 목주만들기 완전 재밌을 것 같아요",
          "speaker_id": "1",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039316.1.29",
          "form": "백팔배는.... 싫..겟지만",
          "original_form": "백팔배는.... 싫..겟지만",
          "speaker_id": "1",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039316.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039316.1.31",
          "form": "아 그건 했었어요",
          "original_form": "아 그건 했었어요",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039316.1.32",
          "form": "스님이랑 차마시면서 담소나누기",
          "original_form": "스님이랑 차마시면서 담소나누기",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039316.1.33",
          "form": "ㅎㅎ 같이 간 친구가",
          "original_form": "ㅎㅎ 같이 간 친구가",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039316.1.34",
          "form": "스님한테 엄청 고민상담을 하더라구요 ㅋㅋㅋ",
          "original_form": "스님한테 엄청 고민상담을 하더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039316.1.35",
          "form": "스님이 녹차 우려주셨는데 좋았어요",
          "original_form": "스님이 녹차 우려주셨는데 좋았어요",
          "speaker_id": "2",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100039316.1.36",
          "form": "헐 그건 완전 이색체험이겠는 걸요?",
          "original_form": "헐 그건 완전 이색체험이겠는 걸요?",
