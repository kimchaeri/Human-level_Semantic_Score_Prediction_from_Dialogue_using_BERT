      "utterance": [
        {
          "id": "MDRW2100016008.1.1",
          "form": "집안일끝없네",
          "original_form": "집안일끝없네",
          "speaker_id": "2",
          "time": "20210826 07:47"
        },
        {
          "id": "MDRW2100016008.1.2",
          "form": "이제 등교시켜야지",
          "original_form": "이제 등교시켜야지",
          "speaker_id": "2",
          "time": "20210826 07:47"
        },
        {
          "id": "MDRW2100016008.1.3",
          "form": "ㅋㅋㅋ 언니 집안일은 끝이없죠 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 언니 집안일은 끝이없죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 07:55"
        },
        {
          "id": "MDRW2100016008.1.4",
          "form": "저도 오늘은 일이 크게 없어서 서랍장, 수납장 정리 좀 하려구요.",
          "original_form": "저도 오늘은 일이 크게 없어서 서랍장, 수납장 정리 좀 하려구요.",
          "speaker_id": "1",
          "time": "20210826 07:56"
        },
        {
          "id": "MDRW2100016008.1.5",
          "form": "좀 창틀도 닦고 해야될듯요",
          "original_form": "좀 창틀도 닦고 해야될듯요",
          "speaker_id": "1",
          "time": "20210826 07:56"
        },
        {
          "id": "MDRW2100016008.1.6",
          "form": "너무 부지런한거아니가?ㅋㅋ",
          "original_form": "너무 부지런한거아니가?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:30"
        },
        {
          "id": "MDRW2100016008.1.7",
          "form": "어휴 좀쉬그라ㅋㅋ",
          "original_form": "어휴 좀쉬그라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:30"
        },
        {
          "id": "MDRW2100016008.1.8",
          "form": "ㅋㅋ일단 첫째 병원 좀 다녀오려구요. ㅋ",
          "original_form": "ㅋㅋ일단 첫째 병원 좀 다녀오려구요. ㅋ",
          "speaker_id": "1",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016008.1.9",
          "form": "둘째 일단 보내놓고요 ㅋㅋ",
          "original_form": "둘째 일단 보내놓고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100016008.1.10",
          "form": "아하ㅋ",
          "original_form": "아하ㅋ",
          "speaker_id": "2",
          "time": "20210826 08:52"
        },
        {
          "id": "MDRW2100016008.1.11",
          "form": "ㅋㅋ이제나가유",
          "original_form": "ㅋㅋ이제나가유",
          "speaker_id": "1",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100016008.1.12",
          "form": "학교는?우짜고?",
          "original_form": "학교는?우짜고?",
          "speaker_id": "2",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100016008.1.13",
          "form": "하교하고 가는줄알았다",
          "original_form": "하교하고 가는줄알았다",
          "speaker_id": "2",
          "time": "20210826 09:06"
        },
        {
          "id": "MDRW2100016008.1.14",
          "form": "학교는 천천히와도 된대ㅋ",
          "original_form": "학교는 천천히와도 된대ㅋ",
          "speaker_id": "1",
          "time": "20210826 09:49"
        },
        {
          "id": "MDRW2100016008.1.15",
          "form": "선생님께 여쭤봤어ㅋ",
          "original_form": "선생님께 여쭤봤어ㅋ",
          "speaker_id": "1",
          "time": "20210826 09:50"
        },
        {
          "id": "MDRW2100016008.1.16",
          "form": "잘했네 학교 보내고 집안일 하여라 크크",
          "original_form": "잘했네 학교 보내고 집안일 하여라 크크",
          "speaker_id": "2",
          "time": "20210826 09:56"
        },
        {
          "id": "MDRW2100016008.1.17",
          "form": "네 언니는요??ㅋㅋ 일갔어요???",
          "original_form": "네 언니는요??ㅋㅋ 일갔어요???",
          "speaker_id": "1",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016008.1.18",
          "form": "일갔다ㅋ",
          "original_form": "일갔다ㅋ",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016008.1.19",
          "form": "언제 오는데요??",
          "original_form": "언제 오는데요??",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016008.1.20",
          "form": "좀이따ㅋㅋ 밥먹어야재ㅋ",
          "original_form": "좀이따ㅋㅋ 밥먹어야재ㅋ",
          "speaker_id": "2",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100016008.1.21",
          "form": "맞아요. 저도 오늘 된장찌게 먹으려구요.ㅋ",
          "original_form": "맞아요. 저도 오늘 된장찌게 먹으려구요.ㅋ",
          "speaker_id": "1",
          "time": "20210826 10:55"
        }
