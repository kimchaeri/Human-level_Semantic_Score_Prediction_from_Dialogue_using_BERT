      "utterance": [
        {
          "id": "MDRW2100012204.1.1",
          "form": "안녕하세요! 혹시 책 좋아하세요?",
          "original_form": "안녕하세요! 혹시 책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012204.1.2",
          "form": "저 책 좋아해요 ㅎㅎ",
          "original_form": "저 책 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:57"
        },
        {
          "id": "MDRW2100012204.1.3",
          "form": "그러고보니 잡지본 지 오래됐네요",
          "original_form": "그러고보니 잡지본 지 오래됐네요",
          "speaker_id": "2",
          "time": "20210828 13:57"
        },
        {
          "id": "MDRW2100012204.1.4",
          "form": "오 어떤 장르 좋아하세요?",
          "original_form": "오 어떤 장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100012204.1.5",
          "form": "저는 걍 소설 좋아해요",
          "original_form": "저는 걍 소설 좋아해요",
          "speaker_id": "2",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100012204.1.6",
          "form": "근데 추리쪽은 좀 흥미는 없어요",
          "original_form": "근데 추리쪽은 좀 흥미는 없어요",
          "speaker_id": "2",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100012204.1.7",
          "form": "아하- 그럼 음.. 역사나 정치 그런쪽? 아니면 로맨스...",
          "original_form": "아하- 그럼 음.. 역사나 정치 그런쪽? 아니면 로맨스...",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100012204.1.8",
          "form": "요즘은 생각없이 살고싶어서 로맨스 좋아해요 ㅎㅎ",
          "original_form": "요즘은 생각없이 살고싶어서 로맨스 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100012204.1.9",
          "form": "아 제일 좋죠, 책 볼땐 현실이랑 확 떨어지잖아요",
          "original_form": "아 제일 좋죠, 책 볼땐 현실이랑 확 떨어지잖아요",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100012204.1.10",
          "form": "특별하게 좋아하는 작가나 포맷 있으세요?",
          "original_form": "특별하게 좋아하는 작가나 포맷 있으세요?",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100012204.1.11",
          "form": "ㅎㅎ 네 맞아요",
          "original_form": "ㅎㅎ 네 맞아요",
          "speaker_id": "2",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100012204.1.12",
          "form": "특별히 좋아하는분은 없고 마구잡이 독서를 좋아해요",
          "original_form": "특별히 좋아하는분은 없고 마구잡이 독서를 좋아해요",
          "speaker_id": "2",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100012204.1.13",
          "form": "으음- 그럼 가장 최근에 본 책은요?",
          "original_form": "으음- 그럼 가장 최근에 본 책은요?",
          "speaker_id": "1",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100012204.1.14",
          "form": "요즘은 오디오북으로 재혼황후 듣고있어요",
          "original_form": "요즘은 오디오북으로 재혼황후 듣고있어요",
          "speaker_id": "2",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100012204.1.15",
          "form": "오오..!! 저 그거 찜만 해두고 아직 못들었는데 광고처럼 진짜 그렇게 흥미진진한가요?",
          "original_form": "오오..!! 저 그거 찜만 해두고 아직 못들었는데 광고처럼 진짜 그렇게 흥미진진한가요?",
          "speaker_id": "1",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100012204.1.16",
          "form": "ㅎㅎㅎ 네 재밌어요",
          "original_form": "ㅎㅎㅎ 네 재밌어요",
          "speaker_id": "2",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100012204.1.17",
          "form": "전 웹소설, 웹툰, 오디오북까지 다 듣고 있어요",
          "original_form": "전 웹소설, 웹툰, 오디오북까지 다 듣고 있어요",
          "speaker_id": "2",
          "time": "20210829 13:57"
        },
        {
          "id": "MDRW2100012204.1.18",
          "form": "헙 그렇게나 흥미롭군요ㅋㅋㅋ 오늘 당장 시작해볼게요",
          "original_form": "헙 그렇게나 흥미롭군요ㅋㅋㅋ 오늘 당장 시작해볼게요",
          "speaker_id": "1",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100012204.1.19",
          "form": "약간 벼르고만 있었는데 급 궁금해지네요",
          "original_form": "약간 벼르고만 있었는데 급 궁금해지네요",
          "speaker_id": "1",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100012204.1.20",
          "form": "ㅎㅎ 네 일할때 오디오북 들으면 시간도 잘가더라고요",
          "original_form": "ㅎㅎ 네 일할때 오디오북 들으면 시간도 잘가더라고요",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100012204.1.21",
          "form": "혹시 또 추천해주실 작품은 없으세요?",
          "original_form": "혹시 또 추천해주실 작품은 없으세요?",
          "speaker_id": "1",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100012204.1.22",
          "form": "내남편과 결혼해줘도 생각없이 듣기 좋아요",
          "original_form": "내남편과 결혼해줘도 생각없이 듣기 좋아요",
          "speaker_id": "2",
          "time": "20210829 14:00"
        },
        {
          "id": "MDRW2100012204.1.23",
          "form": "지금 바로 찜해두고 올게요!",
          "original_form": "지금 바로 찜해두고 올게요!",
          "speaker_id": "1",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100012204.1.24",
          "form": "와우 바로 찜했어요",
          "original_form": "와우 바로 찜했어요",
          "speaker_id": "1",
          "time": "20210829 14:06"
        },
        {
          "id": "MDRW2100012204.1.25",
          "form": "덕분에 일하면서 들을 좋은 소재가 생겼습니다 감사해요",
          "original_form": "덕분에 일하면서 들을 좋은 소재가 생겼습니다 감사해요",
          "speaker_id": "1",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100012204.1.26",
          "form": "ㅎㅎㅎ 저도 감사합니다",
          "original_form": "ㅎㅎㅎ 저도 감사합니다",
          "speaker_id": "2",
          "time": "20210829 14:42"
        }
      ]
