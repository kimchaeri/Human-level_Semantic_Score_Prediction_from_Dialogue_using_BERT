      "utterance": [
        {
          "id": "MDRW2100008321.1.1",
          "form": "회사 점심시간에",
          "original_form": "회사 점심시간에",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100008321.1.2",
          "form": "도시락 대신에",
          "original_form": "도시락 대신에",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100008321.1.3",
          "form": "컵라면 사가고 싶어요 ㅇㅅㅇ",
          "original_form": "컵라면 사가고 싶어요 ㅇㅅㅇ",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100008321.1.4",
          "form": "ㅇ0ㅇ 안댐?",
          "original_form": "ㅇ0ㅇ 안댐?",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100008321.1.5",
          "form": "탕비실에는 없어?",
          "original_form": "탕비실에는 없어?",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100008321.1.6",
          "form": "잉 ㅇㅅㅇ 싸가도 댐여",
          "original_form": "잉 ㅇㅅㅇ 싸가도 댐여",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100008321.1.7",
          "form": "근데 컵라면이 투쁠원하는것중에 먹을만한게 읎어서",
          "original_form": "근데 컵라면이 투쁠원하는것중에 먹을만한게 읎어서",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100008321.1.8",
          "form": "간보는 중여",
          "original_form": "간보는 중여",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100008321.1.9",
          "form": "아하 글쿤",
          "original_form": "아하 글쿤",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100008321.1.10",
          "form": "그래도 적당히 먹자 ㅋㅋㅋㅋ",
          "original_form": "그래도 적당히 먹자 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100008321.1.11",
          "form": "나이드니까 컵라면 주식으로 하면 속이 더부룩하니까...",
          "original_form": "나이드니까 컵라면 주식으로 하면 속이 더부룩하니까...",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100008321.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ또르르...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ또르르...",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100008321.1.13",
          "form": "그치만...오징어젓갈만 맨날천날 먹으니까",
          "original_form": "그치만...오징어젓갈만 맨날천날 먹으니까",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100008321.1.14",
          "form": "질린다구요 ㅠㅠ",
          "original_form": "질린다구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100008321.1.15",
          "form": "아이고 ㅋㅋㅋㅋㅋㅋㅋ 오징아 젓갈러쉬 ㅋㅋㅋ",
          "original_form": "아이고 ㅋㅋㅋㅋㅋㅋㅋ 오징아 젓갈러쉬 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100008321.1.16",
          "form": "탕비실에 냉동고나 전자레인지 있어?",
          "original_form": "탕비실에 냉동고나 전자레인지 있어?",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100008321.1.17",
          "form": "냉장고는 있고 전자레인지 있어요 ㅋㅋㅋㅋ",
          "original_form": "냉장고는 있고 전자레인지 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100008321.1.18",
          "form": "근데 전자레인지 쓰는 사람들 많아서 그냥저냥...",
          "original_form": "근데 전자레인지 쓰는 사람들 많아서 그냥저냥...",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100008321.1.19",
          "form": "둘 다 있으면 밀프렙 만들어서",
          "original_form": "둘 다 있으면 밀프렙 만들어서",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100008321.1.20",
          "form": "해동해먹어도 되겠다",
          "original_form": "해동해먹어도 되겠다",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100008321.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그치만...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그치만...",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100008321.1.22",
          "form": "빨리 대충 먹고",
          "original_form": "빨리 대충 먹고",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100008321.1.23",
          "form": "쉬는 시간을 즐기고 싶은 사람",
          "original_form": "쉬는 시간을 즐기고 싶은 사람",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100008321.1.24",
          "form": "ㅋㅋㅋㅋ 이런거보면 알약 하나로 한끼 해결~ 이런게",
          "original_form": "ㅋㅋㅋㅋ 이런거보면 알약 하나로 한끼 해결~ 이런게",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100008321.1.25",
          "form": "참 필요해보여",
          "original_form": "참 필요해보여",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100008321.1.26",
          "form": "그치만 맛은 느끼고 싶어...",
          "original_form": "그치만 맛은 느끼고 싶어...",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100008321.1.27",
          "form": "먹는건 즐겁지만 식사시간이 휴식시간이니가 ㅋ큐ㅠ",
          "original_form": "먹는건 즐겁지만 식사시간이 휴식시간이니가 ㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100008321.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맛은 느끼고 싶다구여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맛은 느끼고 싶다구여",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100008321.1.29",
          "form": "그건 맞아요 ㅠㅠㅋㅋㅋㅋ 일할땐 그래요...",
          "original_form": "그건 맞아요 ㅠㅠㅋㅋㅋㅋ 일할땐 그래요...",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100008321.1.30",
          "form": "맛과 건강과 빠른 섭취 이거 밸런스 지키기가",
          "original_form": "맛과 건강과 빠른 섭취 이거 밸런스 지키기가",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100008321.1.31",
          "form": "어려운 듯",
          "original_form": "어려운 듯",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100008321.1.32",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210825 22:58"
        }
      ]
