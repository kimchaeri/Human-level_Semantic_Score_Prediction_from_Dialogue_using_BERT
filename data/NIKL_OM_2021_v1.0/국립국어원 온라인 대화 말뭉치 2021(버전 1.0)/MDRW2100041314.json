      "utterance": [
        {
          "id": "MDRW2100041314.1.1",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100041314.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100041314.1.3",
          "form": "운동 자주하세요?",
          "original_form": "운동 자주하세요?",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100041314.1.4",
          "form": "홈트하는데",
          "original_form": "홈트하는데",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041314.1.5",
          "form": "혼자해서 그런지\r\n지속이 안되네용",
          "original_form": "혼자해서 그런지\r\n지속이 안되네용",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041314.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041314.1.7",
          "form": "동영상보고 같아 따라하시는거에요?",
          "original_form": "동영상보고 같아 따라하시는거에요?",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041314.1.8",
          "form": "저도222...",
          "original_form": "저도222...",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041314.1.9",
          "form": "네ㅋㅋㅋㅋ 땅끄부부꺼\r\n보면서 따라해여 ㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ 땅끄부부꺼\r\n보면서 따라해여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100041314.1.10",
          "form": "오 유명하더라구요ㅋㅋㅋㅋ저도 봐야지 하고 저장만 해놨어요ㅋㅋㅋㅋㅋ",
          "original_form": "오 유명하더라구요ㅋㅋㅋㅋ저도 봐야지 하고 저장만 해놨어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100041314.1.11",
          "form": "왜 운동을 안하게 되는걸까...",
          "original_form": "왜 운동을 안하게 되는걸까...",
          "speaker_id": "1",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100041314.1.12",
          "form": "음.... 나태해서ㅜㅜㅋ",
          "original_form": "음.... 나태해서ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041314.1.13",
          "form": "ㅋㅋㅋㅋㅋ먹는건 안게으를 수 있는데..",
          "original_form": "ㅋㅋㅋㅋㅋ먹는건 안게으를 수 있는데..",
          "speaker_id": "1",
          "time": "20210905 22:00"
        },
        {
          "id": "MDRW2100041314.1.14",
          "form": "저는 동영상 그 요가소년 따라했었어욬ㅋㅋㅋ허허",
          "original_form": "저는 동영상 그 요가소년 따라했었어욬ㅋㅋㅋ허허",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100041314.1.15",
          "form": "할때는 좋던데",
          "original_form": "할때는 좋던데",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100041314.1.16",
          "form": "요가소년은 또 누구죠ㅋㅋㅋㅋ",
          "original_form": "요가소년은 또 누구죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100041314.1.17",
          "form": "요가도 좋아했었는데\r\n찾아봐야겠어영 ㅋ",
          "original_form": "요가도 좋아했었는데\r\n찾아봐야겠어영 ㅋ",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100041314.1.18",
          "form": "목소리 성우? 이신 남자분이요ㅋㅋㅋ소년은 아니고 아저씨인데",
          "original_form": "목소리 성우? 이신 남자분이요ㅋㅋㅋ소년은 아니고 아저씨인데",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100041314.1.19",
          "form": "좋아요 빡센거도 있고 명상도 좋고",
          "original_form": "좋아요 빡센거도 있고 명상도 좋고",
          "speaker_id": "1",
          "time": "20210905 22:04"
        },
        {
          "id": "MDRW2100041314.1.20",
          "form": "감사해요 ㅋㅋ\r\n간만에 요가매트를\r\n펼쳐보겠어영 ㅋㅋ",
          "original_form": "감사해요 ㅋㅋ\r\n간만에 요가매트를\r\n펼쳐보겠어영 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041314.1.21",
          "form": "오늘 가보나요ㅋㅋㅋ저도 안들어간지 반년은 된듯한 느낌..",
          "original_form": "오늘 가보나요ㅋㅋㅋ저도 안들어간지 반년은 된듯한 느낌..",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041314.1.22",
          "form": "폼롤러도 사고 매트도 샀는데ㅜㅜㅎㅎㅎ",
          "original_form": "폼롤러도 사고 매트도 샀는데ㅜㅜㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100041314.1.23",
          "form": "장비만...  준비되고",
          "original_form": "장비만...  준비되고",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041314.1.24",
          "form": "저희는 아직 준비가 덜\r\n되었나봅니다ㅜㅜㅋ",
          "original_form": "저희는 아직 준비가 덜\r\n되었나봅니다ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041314.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ최근에는 마사지볼이랑ㅋㅋㅋ마사자 건도 샀는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ최근에는 마사지볼이랑ㅋㅋㅋ마사자 건도 샀는데",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041314.1.26",
          "form": "몸만ㅋㅋㅋㅋ덜 준비되가지구ㅜㅜㅋㅋㅋ",
          "original_form": "몸만ㅋㅋㅋㅋ덜 준비되가지구ㅜㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041314.1.27",
          "form": "다이어트 운동하면서 해요? ㅎㅎ",
          "original_form": "다이어트 운동하면서 해요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:13"
        }
      ]
