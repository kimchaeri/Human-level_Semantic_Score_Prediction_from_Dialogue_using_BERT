        },
        {
          "id": "MDRW2100035897.1.3",
          "form": "ㅋㅋㅋ뽕뽑으셔야쥬",
          "original_form": "ㅋㅋㅋ뽕뽑으셔야쥬",
          "speaker_id": "1",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100035897.1.4",
          "form": "난 식음료로 갓는디",
          "original_form": "난 식음료로 갓는디",
          "speaker_id": "2",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100035897.1.5",
          "form": "무튼 책언제다보나",
          "original_form": "무튼 책언제다보나",
          "speaker_id": "2",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100035897.1.6",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100035897.1.7",
          "form": "ㅋㅋㅋ뭐지! 오류였나봐 ㅋㅋ",
          "original_form": "ㅋㅋㅋ뭐지! 오류였나봐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100035897.1.8",
          "form": "암튼 공부하기전에 책상정리를 좀 해야겠어",
          "original_form": "암튼 공부하기전에 책상정리를 좀 해야겠어",
          "speaker_id": "1",
          "time": "20210830 11:00"
        },
        {
          "id": "MDRW2100035897.1.9",
          "form": "오!!",
          "original_form": "오!!",
          "speaker_id": "2",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100035897.1.10",
          "form": "좋은생각 나도 어제 책정리함",
          "original_form": "좋은생각 나도 어제 책정리함",
          "speaker_id": "2",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100035897.1.11",
          "form": "누운애들을 세워줬지",
          "original_form": "누운애들을 세워줬지",
          "speaker_id": "2",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100035897.1.12",
          "form": "ㅋㅋㅋ먼지가 뽀얗게 앉았음",
          "original_form": "ㅋㅋㅋ먼지가 뽀얗게 앉았음",
          "speaker_id": "1",
          "time": "20210830 11:01"
        },
        {
          "id": "MDRW2100035897.1.13",
          "form": "ㅋㅋㅋㅋㅋ나도그랭 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ나도그랭 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 11:03"
        },
        {
          "id": "MDRW2100035897.1.14",
          "form": "ㅋㅋㅋ괜히 책만 사가지고 에휴",
          "original_form": "ㅋㅋㅋ괜히 책만 사가지고 에휴",
          "speaker_id": "1",
          "time": "20210830 11:03"
        },
        {
          "id": "MDRW2100035897.1.15",
          "form": "책이잇어야지ㅠㅠ",
          "original_form": "책이잇어야지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035897.1.16",
          "form": "ㅋㅋ아니 근데 뭐했다고 11시람",
          "original_form": "ㅋㅋ아니 근데 뭐했다고 11시람",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035897.1.17",
          "form": "긍까유 난 정신을 못차리고잇다",
          "original_form": "긍까유 난 정신을 못차리고잇다",
          "speaker_id": "2",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035897.1.18",
          "form": "약이소용이읍넹",
          "original_form": "약이소용이읍넹",
          "speaker_id": "2",
          "time": "20210830 11:05"
        },
        {
          "id": "MDRW2100035897.1.19",
          "form": "헉 ㅠㅠ 한숨자고 일어나!!",
          "original_form": "헉 ㅠㅠ 한숨자고 일어나!!",
          "speaker_id": "1",
          "time": "20210830 11:06"
        },
        {
          "id": "MDRW2100035897.1.20",
          "form": "약을 다먹엇느지 찾아도읍땅 ㅠㅠ ㅋㅋㅋ",
          "original_form": "약을 다먹엇느지 찾아도읍땅 ㅠㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:06"
        },
        {
          "id": "MDRW2100035897.1.21",
          "form": "병원가야할거같은디 ㅠㅠ",
          "original_form": "병원가야할거같은디 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100035897.1.22",
          "form": "좀다보고 ㅠㅠ 빨리 공부해!!",
          "original_form": "좀다보고 ㅠㅠ 빨리 공부해!!",
          "speaker_id": "2",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100035897.1.23",
          "form": "계획표짜는갓도어려웡",
          "original_form": "계획표짜는갓도어려웡",
          "speaker_id": "2",
          "time": "20210830 11:07"
        },
        {
          "id": "MDRW2100035897.1.24",
          "form": "언능하고 넘어가자!!",
          "original_form": "언능하고 넘어가자!!",
          "speaker_id": "2",
          "time": "20210830 11:08"
        },
        {
