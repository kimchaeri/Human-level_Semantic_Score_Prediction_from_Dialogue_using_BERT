      "utterance": [
        {
          "id": "MDRW2100011791.1.1",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011791.1.2",
          "form": "직장인이세요??",
          "original_form": "직장인이세요??",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011791.1.3",
          "form": "아 직장인이었는데 잠깐 쉬다가 9월부터 다시 출근해요 ㅎㅎ",
          "original_form": "아 직장인이었는데 잠깐 쉬다가 9월부터 다시 출근해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011791.1.4",
          "form": "어떤 일 하시나요??",
          "original_form": "어떤 일 하시나요??",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011791.1.5",
          "form": "난 디자인 직종이야~! 전 회사에서 퇴직하기전까진 재택 했었어",
          "original_form": "난 디자인 직종이야~! 전 회사에서 퇴직하기전까진 재택 했었어",
          "speaker_id": "1",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011791.1.6",
          "form": "와 디자인 멋쥐다..",
          "original_form": "와 디자인 멋쥐다..",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011791.1.7",
          "form": "난 아직 학생이야",
          "original_form": "난 아직 학생이야",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011791.1.8",
          "form": "오 무슨 과인지 물어봐도 되??",
          "original_form": "오 무슨 과인지 물어봐도 되??",
          "speaker_id": "1",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011791.1.9",
          "form": "예술경영!",
          "original_form": "예술경영!",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011791.1.10",
          "form": "근데 전공 안살리려고,,",
          "original_form": "근데 전공 안살리려고,,",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011791.1.11",
          "form": "오??? 예술 경영과 멋지다 전공은 왜 안살리려는 거야??",
          "original_form": "오??? 예술 경영과 멋지다 전공은 왜 안살리려는 거야??",
          "speaker_id": "1",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100011791.1.12",
          "form": "너무 레드오션이기도 하고 직업보단 취미로 남기고 싶어서!",
          "original_form": "너무 레드오션이기도 하고 직업보단 취미로 남기고 싶어서!",
          "speaker_id": "2",
          "time": "20210824 16:13"
        },
        {
          "id": "MDRW2100011791.1.13",
          "form": "원래 디자인쪽 하고싶었던거야(",
          "original_form": "원래 디자인쪽 하고싶었던거야(",
          "speaker_id": "2",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011791.1.14",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011791.1.15",
          "form": "앗 나도 대학 전공은 순수미술쪽이었는데 졸업하고 나서는 취업때문에 전공을 틀었어",
          "original_form": "앗 나도 대학 전공은 순수미술쪽이었는데 졸업하고 나서는 취업때문에 전공을 틀었어",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011791.1.16",
          "form": "정말 좋아하는거랑 현실은 다르지",
          "original_form": "정말 좋아하는거랑 현실은 다르지",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011791.1.17",
          "form": "그래도 나름 만족하는거같아!",
          "original_form": "그래도 나름 만족하는거같아!",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011791.1.18",
          "form": "하고싶은거 만족하면서 하는거 부럽더라구,,",
          "original_form": "하고싶은거 만족하면서 하는거 부럽더라구,,",
          "speaker_id": "2",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011791.1.19",
          "form": "재택은 할만했어??",
          "original_form": "재택은 할만했어??",
          "speaker_id": "2",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011791.1.20",
          "form": "응 출근보단 훨씬 좋았어!",
          "original_form": "응 출근보단 훨씬 좋았어!",
          "speaker_id": "1",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011791.1.21",
          "form": "ㅋㅋㅋ 오다가는 시간이 확 줄어서 대만족이었어",
          "original_form": "ㅋㅋㅋ 오다가는 시간이 확 줄어서 대만족이었어",
          "speaker_id": "1",
          "time": "20210824 16:16"
        }
      ]
