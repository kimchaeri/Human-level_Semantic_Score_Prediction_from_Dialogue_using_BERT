      "utterance": [
        {
          "id": "MDRW2100006886.1.1",
          "form": "거긴 자연경관이요",
          "original_form": "거긴 자연경관이요",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100006886.1.2",
          "form": "고산지대라 날씨가 좋잖아요",
          "original_form": "고산지대라 날씨가 좋잖아요",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100006886.1.3",
          "form": "낮은 여름 밤은 겨울이에요",
          "original_form": "낮은 여름 밤은 겨울이에요",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100006886.1.4",
          "form": "그럼 밤에 파카라도 입나요?",
          "original_form": "그럼 밤에 파카라도 입나요?",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100006886.1.5",
          "form": "목도리에 장갑도 해요",
          "original_form": "목도리에 장갑도 해요",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100006886.1.6",
          "form": "왜 휴양지로 적합했는지 알거같아요",
          "original_form": "왜 휴양지로 적합했는지 알거같아요",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100006886.1.7",
          "form": "다낭바나힐처럼",
          "original_form": "다낭바나힐처럼",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100006886.1.8",
          "form": "다낭 바나힐도 안개끼고 진짜 춥더라구요",
          "original_form": "다낭 바나힐도 안개끼고 진짜 춥더라구요",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100006886.1.9",
          "form": "거기도 다른세상이지요",
          "original_form": "거기도 다른세상이지요",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100006886.1.10",
          "form": "달랏 맛집은 어디에요?",
          "original_form": "달랏 맛집은 어디에요?",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100006886.1.11",
          "form": "가네쉬 인도레스토라요",
          "original_form": "가네쉬 인도레스토라요",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100006886.1.12",
          "form": "가네쉬는 베트남 전역에 있나봐요",
          "original_form": "가네쉬는 베트남 전역에 있나봐요",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100006886.1.13",
          "form": "맞아요 인기많아요 인도인도 많거든요",
          "original_form": "맞아요 인기많아요 인도인도 많거든요",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100006886.1.14",
          "form": "인도 커리는 우리네 입맛에도 잘 맞아요",
          "original_form": "인도 커리는 우리네 입맛에도 잘 맞아요",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100006886.1.15",
          "form": "달랏 중심에 큰 호수가 있잖아요",
          "original_form": "달랏 중심에 큰 호수가 있잖아요",
