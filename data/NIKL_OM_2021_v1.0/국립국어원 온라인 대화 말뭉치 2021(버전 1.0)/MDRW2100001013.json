      "utterance": [
        {
          "id": "MDRW2100001013.1.1",
          "form": "고등학교 다닐 때가 좋았는데",
          "original_form": "고등학교 다닐 때가 좋았는데",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100001013.1.2",
          "form": "고등학교때 먹던 밥이 생각나네",
          "original_form": "고등학교때 먹던 밥이 생각나네",
          "speaker_id": "2",
          "time": "20210820 11:53"
        },
        {
          "id": "MDRW2100001013.1.3",
          "form": "밥이 생각난다고? 진심?",
          "original_form": "밥이 생각난다고? 진심?",
          "speaker_id": "1",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100001013.1.4",
          "form": "밥은 맛 없었는데 ㅜㅜ",
          "original_form": "밥은 맛 없었는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100001013.1.5",
          "form": "졸업하고 나서 맨날 집에서 대충먹다보니 생각이 나더라구",
          "original_form": "졸업하고 나서 맨날 집에서 대충먹다보니 생각이 나더라구",
          "speaker_id": "2",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100001013.1.6",
          "form": "그래도 균형있긴했잖아",
          "original_form": "그래도 균형있긴했잖아",
          "speaker_id": "2",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001013.1.7",
          "form": "영양사님이 계획해서 만든거니까",
          "original_form": "영양사님이 계획해서 만든거니까",
          "speaker_id": "1",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001013.1.8",
          "form": "영양학적으로 균형있긴 했지",
          "original_form": "영양학적으로 균형있긴 했지",
          "speaker_id": "1",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001013.1.9",
          "form": "시험치고나서나 특별한 날이면 꽤 맛잇는것도 나오지 않았나?",
          "original_form": "시험치고나서나 특별한 날이면 꽤 맛잇는것도 나오지 않았나?",
          "speaker_id": "2",
          "time": "20210820 11:56"
        },
        {
          "id": "MDRW2100001013.1.10",
          "form": "음 난 특별한 음식을 별로 안좋아했던거 같아",
          "original_form": "음 난 특별한 음식을 별로 안좋아했던거 같아",
          "speaker_id": "1",
          "time": "20210820 11:56"
        },
        {
          "id": "MDRW2100001013.1.11",
          "form": "평범한 음식을 조리 잘 하길 바랐어",
          "original_form": "평범한 음식을 조리 잘 하길 바랐어",
          "speaker_id": "1",
          "time": "20210820 11:56"
        },
        {
          "id": "MDRW2100001013.1.12",
          "form": "너는 그래도 입에 맞았다니 다행이네",
          "original_form": "너는 그래도 입에 맞았다니 다행이네",
          "speaker_id": "1",
          "time": "20210820 11:56"
        },
        {
          "id": "MDRW2100001013.1.13",
          "form": "난 음식을 가리지 않아서 그런가봐",
          "original_form": "난 음식을 가리지 않아서 그런가봐",
          "speaker_id": "2",
          "time": "20210820 11:57"
        },
        {
          "id": "MDRW2100001013.1.14",
          "form": "정말 좋은 습관이야",
          "original_form": "정말 좋은 습관이야",
          "speaker_id": "1",
          "time": "20210820 11:57"
        },
        {
          "id": "MDRW2100001013.1.15",
          "form": "난 학교 급식이 영 안맞았어서 ㅋㅋ",
          "original_form": "난 학교 급식이 영 안맞았어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:57"
        },
        {
          "id": "MDRW2100001013.1.16",
          "form": "그건 좀 힘들었겠네",
          "original_form": "그건 좀 힘들었겠네",
          "speaker_id": "2",
          "time": "20210820 11:57"
        },
        {
          "id": "MDRW2100001013.1.17",
          "form": "학교 다닐때 생각해보면",
          "original_form": "학교 다닐때 생각해보면",
          "speaker_id": "1",
          "time": "20210820 11:58"
        },
        {
          "id": "MDRW2100001013.1.18",
          "form": "도서관이 있는게 참 좋았어",
          "original_form": "도서관이 있는게 참 좋았어",
          "speaker_id": "1",
          "time": "20210820 11:58"
        },
        {
          "id": "MDRW2100001013.1.19",
          "form": "나도 고1때는 도서실에 꽤 많이 갔던것 같아",
          "original_form": "나도 고1때는 도서실에 꽤 많이 갔던것 같아",
          "speaker_id": "2",
          "time": "20210820 11:59"
        },
        {
          "id": "MDRW2100001013.1.20",
          "form": "조용하고 책읽기도 좋고",
          "original_form": "조용하고 책읽기도 좋고",
          "speaker_id": "2",
          "time": "20210820 11:59"
        },
        {
          "id": "MDRW2100001013.1.21",
          "form": "지금은 책 사서 보자니 돈 없고",
          "original_form": "지금은 책 사서 보자니 돈 없고",
          "speaker_id": "1",
          "time": "20210820 11:59"
        },
        {
          "id": "MDRW2100001013.1.22",
          "form": "도서관 가자니 귀찮아",
          "original_form": "도서관 가자니 귀찮아",
          "speaker_id": "1",
          "time": "20210820 11:59"
        },
        {
          "id": "MDRW2100001013.1.23",
          "form": "학교도서관이 짱인듯",
          "original_form": "학교도서관이 짱인듯",
          "speaker_id": "1",
          "time": "20210820 11:59"
        }
      ]
    }
