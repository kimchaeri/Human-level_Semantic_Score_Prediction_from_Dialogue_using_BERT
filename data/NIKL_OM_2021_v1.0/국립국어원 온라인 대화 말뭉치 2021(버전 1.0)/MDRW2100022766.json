      "utterance": [
        {
          "id": "MDRW2100022766.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022766.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022766.1.3",
          "form": "부모님이 다 해줘요",
          "original_form": "부모님이 다 해줘요",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022766.1.4",
          "form": "전 제가 할 수 있어요",
          "original_form": "전 제가 할 수 있어요",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022766.1.5",
          "form": "어떤걸 제일 잘해요",
          "original_form": "어떤걸 제일 잘해요",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022766.1.6",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022766.1.7",
          "form": "청소도 여러가지잖아요",
          "original_form": "청소도 여러가지잖아요",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022766.1.8",
          "form": "화장실전담이요",
          "original_form": "화장실전담이요",
          "speaker_id": "1",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022766.1.9",
          "form": "힘든거 맡았네요",
          "original_form": "힘든거 맡았네요",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022766.1.10",
          "form": "혼자사세요?",
          "original_form": "혼자사세요?",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022766.1.11",
          "form": "가족이랑요",
          "original_form": "가족이랑요",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022766.1.12",
          "form": "분담해야죠",
          "original_form": "분담해야죠",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022766.1.13",
          "form": "독립 할 생각 없나요",
          "original_form": "독립 할 생각 없나요",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022766.1.14",
          "form": "독립 다들 하고싶어하던데",
          "original_form": "독립 다들 하고싶어하던데",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022766.1.15",
          "form": "외로움 많이 타서요",
          "original_form": "외로움 많이 타서요",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022766.1.16",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022766.1.17",
          "form": "동물 키워볼 생각은요",
          "original_form": "동물 키워볼 생각은요",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022766.1.18",
          "form": "일할때 혼자 두기가 미안해서",
          "original_form": "일할때 혼자 두기가 미안해서",
