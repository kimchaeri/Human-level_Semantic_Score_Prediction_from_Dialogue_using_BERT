      "utterance": [
        {
          "id": "MDRW2100003390.1.1",
          "form": "온라인 피트니스 봤어?",
          "original_form": "온라인 피트니스 봤어?",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003390.1.2",
          "form": "응 코로나 터지고 봤어",
          "original_form": "응 코로나 터지고 봤어",
          "speaker_id": "2",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003390.1.3",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003390.1.4",
          "form": "코로나때문에 헬스 못가니까",
          "original_form": "코로나때문에 헬스 못가니까",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003390.1.5",
          "form": "그러게 화상으로나 유튜브로나 하더라",
          "original_form": "그러게 화상으로나 유튜브로나 하더라",
          "speaker_id": "2",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003390.1.6",
          "form": "신기해",
          "original_form": "신기해",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003390.1.7",
          "form": "어디까지 발전할까",
          "original_form": "어디까지 발전할까",
          "speaker_id": "1",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003390.1.8",
          "form": "근데 집에서 운동이 잘 될까",
          "original_form": "근데 집에서 운동이 잘 될까",
          "speaker_id": "2",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003390.1.9",
          "form": "카메라로 자세 봐주면 괜찮지 않을까",
          "original_form": "카메라로 자세 봐주면 괜찮지 않을까",
          "speaker_id": "1",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003390.1.10",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "2",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003390.1.11",
          "form": "나도 유튜브 보고 가끔 홈트 해",
          "original_form": "나도 유튜브 보고 가끔 홈트 해",
          "speaker_id": "1",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003390.1.12",
          "form": "오오 재미있어?",
          "original_form": "오오 재미있어?",
          "speaker_id": "2",
          "time": "20210820 22:34"
        },
        {
          "id": "MDRW2100003390.1.13",
          "form": "춤추는 거 따라하는데 은근 재밌어",
          "original_form": "춤추는 거 따라하는데 은근 재밌어",
          "speaker_id": "1",
          "time": "20210820 22:34"
        },
        {
          "id": "MDRW2100003390.1.14",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210820 22:34"
        },
        {
          "id": "MDRW2100003390.1.15",
          "form": "추천해줘",
          "original_form": "추천해줘",
          "speaker_id": "2",
          "time": "20210820 22:34"
        },
        {
          "id": "MDRW2100003390.1.16",
          "form": "무아지경 이라고 있는데",
          "original_form": "무아지경 이라고 있는데",
          "speaker_id": "1",
          "time": "20210820 22:35"
        },
        {
          "id": "MDRW2100003390.1.17",
          "form": "무산소 운동 제대로야",
          "original_form": "무산소 운동 제대로야",
          "speaker_id": "1",
          "time": "20210820 22:35"
        },
        {
          "id": "MDRW2100003390.1.18",
          "form": "오 ㅋㅋㅋ 나중에 해봐야겠다",
          "original_form": "오 ㅋㅋㅋ 나중에 해봐야겠다",
          "speaker_id": "2",
          "time": "20210820 22:35"
        }
      ]
