      "utterance": [
        {
          "id": "MDRW2100041417.1.1",
          "form": "낼 출근하시나용",
          "original_form": "낼 출근하시나용",
          "speaker_id": "1",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100041417.1.2",
          "form": "백신맞으러갑니더",
          "original_form": "백신맞으러갑니더",
          "speaker_id": "2",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100041417.1.3",
          "form": "다",
          "original_form": "다",
          "speaker_id": "2",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100041417.1.4",
          "form": "엥 백신맞으면 출근안해요ㅠㅠ? 개꿀..",
          "original_form": "엥 백신맞으면 출근안해요ㅠㅠ? 개꿀..",
          "speaker_id": "1",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100041417.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ쉴려구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ쉴려구요",
          "speaker_id": "2",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100041417.1.6",
          "form": "백신휴가 있는 직장도 있던데 ..",
          "original_form": "백신휴가 있는 직장도 있던데 ..",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100041417.1.7",
          "form": "넘 부럽 ㅠㅠ",
          "original_form": "넘 부럽 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100041417.1.8",
          "form": "화욜은 정상출근이구요?",
          "original_form": "화욜은 정상출근이구요?",
          "speaker_id": "1",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100041417.1.9",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100041417.1.10",
          "form": "보통잘없지않아요?",
          "original_form": "보통잘없지않아요?",
          "speaker_id": "2",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100041417.1.11",
          "form": "몇시 출근이세요?",
          "original_form": "몇시 출근이세요?",
          "speaker_id": "1",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100041417.1.12",
          "form": "주변에도 있더라구용 ㅠ",
          "original_form": "주변에도 있더라구용 ㅠ",
          "speaker_id": "1",
          "time": "20210905 23:19"
        },
        {
          "id": "MDRW2100041417.1.13",
          "form": "9시 출근입니다",
          "original_form": "9시 출근입니다",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100041417.1.14",
          "form": "저두요 ㅎㅎ 스트레스 받는 일 없나요",
          "original_form": "저두요 ㅎㅎ 스트레스 받는 일 없나요",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100041417.1.15",
          "form": "스트레스는 원래 다 인간관계잖아욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "스트레스는 원래 다 인간관계잖아욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100041417.1.16",
          "form": "지금은 괜찮아요",
          "original_form": "지금은 괜찮아요",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100041417.1.17",
          "form": "맞아용 몇년차세요?",
          "original_form": "맞아용 몇년차세요?",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100041417.1.18",
          "form": "4년차입니다",
          "original_form": "4년차입니다",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100041417.1.19",
          "form": "하시는 일은 적응하셨나요 ㅎ",
          "original_form": "하시는 일은 적응하셨나요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100041417.1.20",
          "form": "ㅋㅋ항상 때리치고 집에누워있고싶네요..",
          "original_form": "ㅋㅋ항상 때리치고 집에누워있고싶네요..",
          "speaker_id": "2",
          "time": "20210905 23:22"
        }
      ]
