      "utterance": [
        {
          "id": "MDRW2100041929.1.1",
          "form": "운동하시는거 잇으신가용?",
          "original_form": "운동하시는거 잇으신가용?",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100041929.1.2",
          "form": "저는 요즘 숨쉬기만해서",
          "original_form": "저는 요즘 숨쉬기만해서",
          "speaker_id": "2",
          "time": "20210906 23:35"
        },
        {
          "id": "MDRW2100041929.1.3",
          "form": "걱정이에요..",
          "original_form": "걱정이에요..",
          "speaker_id": "2",
          "time": "20210906 23:35"
        },
        {
          "id": "MDRW2100041929.1.4",
          "form": "헬스 다시 열심히하려구요",
          "original_form": "헬스 다시 열심히하려구요",
          "speaker_id": "2",
          "time": "20210906 23:35"
        },
        {
          "id": "MDRW2100041929.1.5",
          "form": "ㅋㅋ우와",
          "original_form": "ㅋㅋ우와",
          "speaker_id": "1",
          "time": "20210907 08:12"
        },
        {
          "id": "MDRW2100041929.1.6",
          "form": "헬스하시는군요!!",
          "original_form": "헬스하시는군요!!",
          "speaker_id": "1",
          "time": "20210907 08:12"
        },
        {
          "id": "MDRW2100041929.1.7",
          "form": "네근데 마스크끼니 넘 숨차긴해요",
          "original_form": "네근데 마스크끼니 넘 숨차긴해요",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100041929.1.8",
          "form": "부지런하세요ㅠㅠ",
          "original_form": "부지런하세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100041929.1.9",
          "form": "저는 피티받고싶다 생각만하고잇어요",
          "original_form": "저는 피티받고싶다 생각만하고잇어요",
          "speaker_id": "1",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100041929.1.10",
          "form": "허리랑 무릎이 안좋아서ㅜ",
          "original_form": "허리랑 무릎이 안좋아서ㅜ",
          "speaker_id": "1",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100041929.1.11",
          "form": "몸 아프시면 운동 조심히하세요 ㅜ",
          "original_form": "몸 아프시면 운동 조심히하세요 ㅜ",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100041929.1.12",
          "form": "네ㅋㅋ그래서 수영다녓엇고",
          "original_form": "네ㅋㅋ그래서 수영다녓엇고",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100041929.1.13",
          "form": "피티는 받아보고싶어서생각중이에여",
          "original_form": "피티는 받아보고싶어서생각중이에여",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100041929.1.14",
          "form": "수영 재밌겠어요:)",
          "original_form": "수영 재밌겠어요:)",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100041929.1.15",
          "form": "네ㅋㅋ이시국이라 못간지 2년 넘엇어여 흑흑",
          "original_form": "네ㅋㅋ이시국이라 못간지 2년 넘엇어여 흑흑",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100041929.1.16",
          "form": "ㅠ 정말 일상 모든것에 제약이에요 ㅠ",
          "original_form": "ㅠ 정말 일상 모든것에 제약이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100041929.1.17",
          "form": "그쳐 이제 위드코로나라니까",
          "original_form": "그쳐 이제 위드코로나라니까",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100041929.1.18",
          "form": "환장하겟어요",
          "original_form": "환장하겟어요",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100041929.1.19",
          "form": "희",
          "original_form": "희",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100041929.1.20",
          "form": "ㅋㅋㅋ잡아둬도",
          "original_form": "ㅋㅋㅋ잡아둬도",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100041929.1.21",
          "form": "풀어도",
          "original_form": "풀어도",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100041929.1.22",
          "form": "건강 걱정이되는것 같아요",
          "original_form": "건강 걱정이되는것 같아요",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100041929.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ맞슴다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞슴다",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100041929.1.24",
