      "utterance": [
        {
          "id": "MDRW2100038523.1.1",
          "form": "녹차는 비싼거보다 저는 저런 저렴한게 좋더라구요",
          "original_form": "녹차는 비싼거보다 저는 저런 저렴한게 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100038523.1.2",
          "form": "아하 너무 진하면 좀 입맛에 안맞으시는구나!!!",
          "original_form": "아하 너무 진하면 좀 입맛에 안맞으시는구나!!!",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100038523.1.3",
          "form": "그럼 스타벅스 그린티 프라푸치노 좋아하세요?",
          "original_form": "그럼 스타벅스 그린티 프라푸치노 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100038523.1.4",
          "form": "요즘은 말차로 바뀌면서 맛이 좀 달라지긴 했는데!",
          "original_form": "요즘은 말차로 바뀌면서 맛이 좀 달라지긴 했는데!",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100038523.1.5",
          "form": "앗 저 거긴 맛있긴한데 너무 달아요",
          "original_form": "앗 저 거긴 맛있긴한데 너무 달아요",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100038523.1.6",
          "form": "말차로 바뀌었군요?",
          "original_form": "말차로 바뀌었군요?",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100038523.1.7",
          "form": "아이스크림말고 그냥 녹차도 좋아하세요?",
          "original_form": "아이스크림말고 그냥 녹차도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100038523.1.8",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "1",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100038523.1.9",
          "form": "바뀌니까 하나도 안달더라구요",
          "original_form": "바뀌니까 하나도 안달더라구요",
          "speaker_id": "1",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100038523.1.10",
          "form": "네 그냥 차도 좋아해요!!!",
          "original_form": "네 그냥 차도 좋아해요!!!",
          "speaker_id": "1",
          "time": "20210902 15:24"
        },
        {
          "id": "MDRW2100038523.1.11",
          "form": "오설록 진짜 좋아합니다ㅜ",
          "original_form": "오설록 진짜 좋아합니다ㅜ",
          "speaker_id": "1",
          "time": "20210902 15:24"
        },
        {
          "id": "MDRW2100038523.1.12",
          "form": "오설록 좋죠ㅠㅠ 없어서 못마실뿐.. .",
          "original_form": "오설록 좋죠ㅠㅠ 없어서 못마실뿐.. .",
          "speaker_id": "2",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100038523.1.13",
          "form": "맞아요 정말 없어서 못먹어요ㅠㅠ",
          "original_form": "맞아요 정말 없어서 못먹어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100038523.1.14",
          "form": "오설록은 케이크도 진짜 맛있는데...",
          "original_form": "오설록은 케이크도 진짜 맛있는데...",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038523.1.15",
          "form": "아이스크림도 맛있구요ㅠㅠ",
          "original_form": "아이스크림도 맛있구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100038523.1.16",
          "form": "매번사기는 너무 부담스럽고...",
          "original_form": "매번사기는 너무 부담스럽고...",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038523.1.17",
          "form": "누가선물해주면좋겠어요",
          "original_form": "누가선물해주면좋겠어요",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038523.1.18",
          "form": "맞아요 ㅋㅋㅋㅋ 요즘은 디저트도 너무 비싸서...",
          "original_form": "맞아요 ㅋㅋㅋㅋ 요즘은 디저트도 너무 비싸서...",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038523.1.19",
          "form": "오설록 같은 건 이벤트가 있어야지만 먹을 수 있어오ㅠ",
          "original_form": "오설록 같은 건 이벤트가 있어야지만 먹을 수 있어오ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038523.1.20",
          "form": "그죠ㅋㅋㅋㅋ 먹는것 선물이 은근 반가워요",
          "original_form": "그죠ㅋㅋㅋㅋ 먹는것 선물이 은근 반가워요",
          "speaker_id": "2",
          "time": "20210903 08:29"
        },
        {
          "id": "MDRW2100038523.1.21",
          "form": "저는 저번에 하겐다즈 종류별로",
          "original_form": "저는 저번에 하겐다즈 종류별로",
          "speaker_id": "1",
          "time": "20210903 08:35"
        },
        {
          "id": "MDRW2100038523.1.22",
          "form": "선물받았는데 진짜 너무 좋더라구요...",
          "original_form": "선물받았는데 진짜 너무 좋더라구요...",
          "speaker_id": "1",
          "time": "20210903 08:35"
        },
        {
          "id": "MDRW2100038523.1.23",
          "form": "헉 하겐다즈 종류별로라니...",
          "original_form": "헉 하겐다즈 종류별로라니...",
          "speaker_id": "2",
          "time": "20210903 08:36"
        },
        {
          "id": "MDRW2100038523.1.24",
          "form": "프로포즈아닌가요?",
          "original_form": "프로포즈아닌가요?",
          "speaker_id": "2",
          "time": "20210903 08:36"
        },
        {
          "id": "MDRW2100038523.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038523.1.26",
          "form": "진짜 그런걸까요?!",
          "original_form": "진짜 그런걸까요?!",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038523.1.27",
          "form": "카카오톡 선물하기에 있었는데",
          "original_form": "카카오톡 선물하기에 있었는데",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038523.1.28",
          "form": "하겐다즈는 왜이렇게 맛있게 잘 만드는건지,,,",
          "original_form": "하겐다즈는 왜이렇게 맛있게 잘 만드는건지,,,",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038523.1.29",
          "form": "맛있죠... 네고왕할때 사먹었는데 너무 좋았어요",
          "original_form": "맛있죠... 네고왕할때 사먹었는데 너무 좋았어요",
          "speaker_id": "2",
          "time": "20210903 08:50"
        }
      ]
