      "utterance": [
        {
          "id": "MDRW2100037657.1.1",
          "form": "아 마자여",
          "original_form": "아 마자여",
          "speaker_id": "2",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100037657.1.2",
          "form": "서예지 하니까.. 제 인생 드라마가 사이코지만 괜찮아 ㅠ.^ 였거든여",
          "original_form": "서예지 하니까.. 제 인생 드라마가 사이코지만 괜찮아 ㅠ.^ 였거든여",
          "speaker_id": "1",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100037657.1.3",
          "form": "하.. 저두 열심히 봤답니다",
          "original_form": "하.. 저두 열심히 봤답니다",
          "speaker_id": "2",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100037657.1.4",
          "form": "둘이 짱 잘어울렸는데...",
          "original_form": "둘이 짱 잘어울렸는데...",
          "speaker_id": "2",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100037657.1.5",
          "form": "각자 역할도 좋았구여 ㅜㅜ",
          "original_form": "각자 역할도 좋았구여 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100037657.1.6",
          "form": "그니까요.. 그래서 사실 흐린눈 하고 정주행해요..",
          "original_form": "그니까요.. 그래서 사실 흐린눈 하고 정주행해요..",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100037657.1.7",
          "form": "정말 ㅠㅠ 만ㅎ이 울고 감동받은 드라마라 ㅠㅠ",
          "original_form": "정말 ㅠㅠ 만ㅎ이 울고 감동받은 드라마라 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100037657.1.8",
          "form": "ㅜㅜ 저는 얼굴보면 생각나서.. 몰입이 안되ㅏ여ㅜㅜ",
          "original_form": "ㅜㅜ 저는 얼굴보면 생각나서.. 몰입이 안되ㅏ여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100037657.1.9",
          "form": "ㅜㅜ ㅋㅋㅋㅋㅋ 그럴만하죠",
          "original_form": "ㅜㅜ ㅋㅋㅋㅋㅋ 그럴만하죠",
          "speaker_id": "1",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100037657.1.10",
          "form": "name2님 펑펑 울면서 본 드라마 있으세뇨?",
          "original_form": "&name2&님 펑펑 울면서 본 드라마 있으세뇨?",
          "speaker_id": "1",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100037657.1.11",
          "form": "저는... 하이바이마마 아세요?",
          "original_form": "저는... 하이바이마마 아세요?",
          "speaker_id": "2",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100037657.1.12",
          "form": "네! 김태희가 죽었다 깨어나는 드라마 맞죠!",
          "original_form": "네! 김태희가 죽었다 깨어나는 드라마 맞죠!",
          "speaker_id": "1",
          "time": "20210901 16:29"
        },
        {
          "id": "MDRW2100037657.1.13",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100037657.1.14",
          "form": "드라마는 이상하지만..",
          "original_form": "드라마는 이상하지만..",
          "speaker_id": "2",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100037657.1.15",
          "form": "슬프답니다 ㅜㅜ",
          "original_form": "슬프답니다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100037657.1.16",
          "form": "그것도 결말이 아쉽더는 말 들었던 것 같은데",
          "original_form": "그것도 결말이 아쉽더는 말 들었던 것 같은데",
          "speaker_id": "1",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100037657.1.17",
          "form": "맞나요오?",
          "original_form": "맞나요오?",
          "speaker_id": "1",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100037657.1.18",
          "form": "네 ㅜㅜ 그냥 드라마가",
          "original_form": "네 ㅜㅜ 그냥 드라마가",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037657.1.19",
          "form": "산으로 흘러가요 ㅋㅋㅋ ㅠㅠ",
          "original_form": "산으로 흘러가요 ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037657.1.20",
          "form": "작가가 서브빙의하거든요...",
          "original_form": "작가가 서브빙의하거든요...",
          "speaker_id": "2",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100037657.1.21",
          "form": "네...에?",
          "original_form": "네...에?",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100037657.1.22",
          "form": "드라마 작가요???????",
          "original_form": "드라마 작가요???????",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100037657.1.23",
          "form": "극 쓴??",
          "original_form": "극 쓴??",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100037657.1.24",
          "form": "넵... 본인이 서브빙의해서",
          "original_form": "넵... 본인이 서브빙의해서",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100037657.1.25",
          "form": "서브에게 모든 서사를 몰빵하고",
          "original_form": "서브에게 모든 서사를 몰빵하고",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100037657.1.26",
          "form": "여주보다 서브가 더 자주나와여..",
          "original_form": "여주보다 서브가 더 자주나와여..",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100037657.1.27",
          "form": "...느에...? 그게 무슨... 재미일까요?",
          "original_form": "...느에...? 그게 무슨... 재미일까요?",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100037657.1.28",
          "form": "그 김태희 아들의 새엄마가 서브..인가요?",
          "original_form": "그 김태희 아들의 새엄마가 서브..인가요?",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100037657.1.29",
