      "utterance": [
        {
          "id": "MDRW2100005690.1.1",
          "form": "그럼 나랑도",
          "original_form": "그럼 나랑도",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005690.1.2",
          "form": "결혼애 대해 얘기해",
          "original_form": "결혼애 대해 얘기해",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005690.1.3",
          "form": "결혼이요?",
          "original_form": "결혼이요?",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005690.1.4",
          "form": "언제하실건가요",
          "original_form": "언제하실건가요",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005690.1.5",
          "form": "저는 올해가 가기전에 결혼할려구요ㅋ",
          "original_form": "저는 올해가 가기전에 결혼할려구요ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005690.1.6",
          "form": "20대 후반은...",
          "original_form": "20대 후반은...",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005690.1.7",
          "form": "상폐랄까?ㅋ",
          "original_form": "상폐랄까?ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.8",
          "form": "아ㅋ",
          "original_form": "아ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.9",
          "form": "상대는잇으시구요?",
          "original_form": "상대는잇으시구요?",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.10",
          "form": "베트남에서",
          "original_form": "베트남에서",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.11",
          "form": "사오려구요 ㅋ",
          "original_form": "사오려구요 ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.12",
          "form": "상폐직전! 경고!",
          "original_form": "상폐직전! 경고!",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.13",
          "form": "제가 새 시장을 개척해보려 합니다 ㅋ",
          "original_form": "제가 새 시장을 개척해보려 합니다 ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.14",
          "form": "나쁜말은쓰몃안되요~",
          "original_form": "나쁜말은쓰몃안되요~",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.15",
          "form": "교육청에신고할게요...",
          "original_form": "교육청에신고할게요...",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.16",
          "form": "나쁜말 안썻어요",
          "original_form": "나쁜말 안썻어요",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.17",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.18",
          "form": "매매혼은 나라에서 장려하는",
          "original_form": "매매혼은 나라에서 장려하는",
          "speaker_id": "1",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005690.1.19",
          "form": "신고할게요...겸업금지",
          "original_form": "신고할게요...겸업금지",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.20",
          "form": "새로운 혼인방식입니다",
          "original_form": "새로운 혼인방식입니다",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.21",
          "form": "공무원부업금지...",
          "original_form": "공무원부업금지...",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.22",
          "form": "아 주제에 대해서 얘기해요 우리",
          "original_form": "아 주제에 대해서 얘기해요 우리",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.23",
          "form": "연애관이 어떻게되세요?",
          "original_form": "연애관이 어떻게되세요?",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.24",
          "form": "실어요...베트남 매매혼",
          "original_form": "실어요...베트남 매매혼",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.25",
          "form": "분명 흥할 사업이라구요",
          "original_form": "분명 흥할 사업이라구요",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.26",
          "form": "저는연애관은",
          "original_form": "저는연애관은",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.27",
          "form": "김희진 암??",
          "original_form": "김희진 암??",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.28",
          "form": "서로아끼고,사랑해주고",
          "original_form": "서로아끼고,사랑해주고",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.29",
          "form": "나 진짜 김희진보고",
          "original_form": "나 진짜 김희진보고",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.30",
          "form": "격려하는거라고생각해요",
          "original_form": "격려하는거라고생각해요",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.31",
          "form": "넘 잘생겨서 반함ㅠ",
          "original_form": "넘 잘생겨서 반함ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.32",
          "form": "그렇게 아ㆍ사ㆍ격하면",
          "original_form": "그렇게 아ㆍ사ㆍ격하면",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.33",
          "form": "여잔데 이래 잘생겼을수가....",
          "original_form": "여잔데 이래 잘생겼을수가....",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005690.1.34",
          "form": "행복한신혼이되겠지요...",
          "original_form": "행복한신혼이되겠지요...",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.35",
          "form": "님 천지인 쓰나요?ㅋ",
          "original_form": "님 천지인 쓰나요?ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.36",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.37",
          "form": "나이가ㅋ 만으시군요ㅋ",
          "original_form": "나이가ㅋ 만으시군요ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.38",
          "form": "손가락이 굵어서",
          "original_form": "손가락이 굵어서",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.39",
          "form": "쿼티로바꿧다가 실패햇어요",
          "original_form": "쿼티로바꿧다가 실패햇어요",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.40",
          "form": "오타가너무나서요",
          "original_form": "오타가너무나서요",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.41",
          "form": "그래서제가모쏠인걸까요?",
          "original_form": "그래서제가모쏠인걸까요?",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.42",
          "form": "그짓말 치지마세요",
          "original_form": "그짓말 치지마세요",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.43",
          "form": "님 지금 저 놀리나요???!!",
          "original_form": "님 지금 저 놀리나요???!!",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.44",
          "form": "님 연애경험 10번 넘자나요ㅡㅡ",
          "original_form": "님 연애경험 10번 넘자나요ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.45",
          "form": "쿼티쓰는사람만",
          "original_form": "쿼티쓰는사람만",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.46",
          "form": "야 나 쿠킹덤 하고옴",
          "original_form": "야 나 쿠킹덤 하고옴",
          "speaker_id": "1",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.47",
          "form": "연애가가능하나요!?!?",
          "original_form": "연애가가능하나요!?!?",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005690.1.48",
          "form": "ㄱㄷ",
          "original_form": "ㄱㄷ",
          "speaker_id": "1",
          "time": "20210821 23:50"
        }
      ]
    }
  ]
