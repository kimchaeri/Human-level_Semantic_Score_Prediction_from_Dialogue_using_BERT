      "utterance": [
        {
          "id": "MDRW2100037220.1.1",
          "form": "아ㅜ빵먹고싶어요ㅜ",
          "original_form": "아ㅜ빵먹고싶어요ㅜ",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037220.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ조금만 힘내세용ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ조금만 힘내세용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037220.1.3",
          "form": "그런데 아까 디톡스 아쉬운점이 뭐길래 그러세여 ㅜㅜ",
          "original_form": "그런데 아까 디톡스 아쉬운점이 뭐길래 그러세여 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037220.1.4",
          "form": "아 그거 가격이 사악하고요 개당 3720원인데 60ml",
          "original_form": "아 그거 가격이 사악하고요 개당 3720원인데 60ml",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037220.1.5",
          "form": "성분 진짜별거없어요",
          "original_form": "성분 진짜별거없어요",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037220.1.6",
          "form": "하다못해 가르시니아 아세요?",
          "original_form": "하다못해 가르시니아 아세요?",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037220.1.7",
          "form": "넵..",
          "original_form": "넵..",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037220.1.8",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037220.1.9",
          "form": "그것조차 안들어있고",
          "original_form": "그것조차 안들어있고",
          "speaker_id": "2",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100037220.1.10",
          "form": "건강기능식품도 아니네요",
          "original_form": "건강기능식품도 아니네요",
          "speaker_id": "2",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100037220.1.11",
          "form": "그냥 식품유형 과채주스",
          "original_form": "그냥 식품유형 과채주스",
          "speaker_id": "2",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100037220.1.12",
          "form": "아.. 그런데 다른곳도 비슷한가봐요..\r\n그래도 물만 먹는것보단 자몽맛이니까 부디 잘맞으셧으면 좋겟네요 ㅋㅋ",
          "original_form": "아.. 그런데 다른곳도 비슷한가봐요..\r\n그래도 물만 먹는것보단 자몽맛이니까 부디 잘맞으셧으면 좋겟네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037220.1.13",
          "form": "맛은 좋아요 맛은 ㅋㅋ",
          "original_form": "맛은 좋아요 맛은 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037220.1.14",
          "form": "진짜 그냥 음료수같아요",
          "original_form": "진짜 그냥 음료수같아요",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037220.1.15",
          "form": "지금제 위는",
          "original_form": "지금제 위는",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037220.1.16",
          "form": "ㅋㅋㅋㅋ내내 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ내내 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037220.1.17",
          "form": "방금 뭐가 지나갔나 이러고있을듯 ㅋㅋ",
          "original_form": "방금 뭐가 지나갔나 이러고있을듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037220.1.18",
          "form": "불쌍한 내 위",
          "original_form": "불쌍한 내 위",
          "speaker_id": "2",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100037220.1.19",
          "form": "ㅋㅋㅋㅋ 그래도 몇주 하다보면 좀 줄어들지않을까요.ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 그래도 몇주 하다보면 좀 줄어들지않을까요.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100037220.1.20",
          "form": "빵은 어떤거 좋아하세여?ㅋㅋ",
          "original_form": "빵은 어떤거 좋아하세여?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100037220.1.21",
          "form": "이게 제가 10개받았는데",
          "original_form": "이게 제가 10개받았는데",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037220.1.22",
          "form": "하루암것도 안먹고",
          "original_form": "하루암것도 안먹고",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037220.1.23",
          "form": "네..",
