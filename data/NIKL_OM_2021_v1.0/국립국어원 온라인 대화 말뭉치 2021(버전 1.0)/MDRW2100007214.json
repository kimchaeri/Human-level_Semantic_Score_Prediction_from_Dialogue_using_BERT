      "utterance": [
        {
          "id": "MDRW2100007214.1.1",
          "form": "반려동물이 있으신가요?",
          "original_form": "반려동물이 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007214.1.2",
          "form": "전 구피 키워요! name1 님은요?",
          "original_form": "전 구피 키워요! &name1& 님은요?",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007214.1.3",
          "form": "구피!",
          "original_form": "구피!",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007214.1.4",
          "form": "물고기 집사시군요",
          "original_form": "물고기 집사시군요",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007214.1.5",
          "form": "저는 새 키워요 앵무새",
          "original_form": "저는 새 키워요 앵무새",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007214.1.6",
          "form": "헉 앵무새 !!",
          "original_form": "헉 앵무새 !!",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007214.1.7",
          "form": "새키우면서 어려운건 없나요?",
          "original_form": "새키우면서 어려운건 없나요?",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007214.1.8",
          "form": "소음문제가 가장 예민하죠 다들 집에 계시니깐!!",
          "original_form": "소음문제가 가장 예민하죠 다들 집에 계시니깐!!",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007214.1.9",
          "form": "구피면 여러마리 키우시나요?",
          "original_form": "구피면 여러마리 키우시나요?",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007214.1.10",
          "form": "네 ~ 자꾸 점점 더늘어나요 ㅋㅋㅋㅋ",
          "original_form": "네 ~ 자꾸 점점 더늘어나요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007214.1.11",
          "form": "ㅋㅋㅋㅋ 번식하나요?",
          "original_form": "ㅋㅋㅋㅋ 번식하나요?",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007214.1.12",
          "form": "네 근데 어린 구피를 잡아먹어서 보이면 바로 분리해줘야해요",
          "original_form": "네 근데 어린 구피를 잡아먹어서 보이면 바로 분리해줘야해요",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007214.1.13",
          "form": "으으 구피도 작은데 치어가 보이나요?",
          "original_form": "으으 구피도 작은데 치어가 보이나요?",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007214.1.14",
          "form": "세상 작겠다",
          "original_form": "세상 작겠다",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007214.1.15",
          "form": "열심히 보면 보이더라고요 ㅋㅋㅋ",
          "original_form": "열심히 보면 보이더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007214.1.16",
          "form": "앵무새는 먹이 어떻게주나요?",
          "original_form": "앵무새는 먹이 어떻게주나요?",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007214.1.17",
          "form": "애기들마다 다른데 곡식이나 과일 야채를 먹어요",
          "original_form": "애기들마다 다른데 곡식이나 과일 야채를 먹어요",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007214.1.18",
          "form": "잡식이라 사람것도 냠냠",
          "original_form": "잡식이라 사람것도 냠냠",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007214.1.19",
          "form": "헉 신기해요 !! 귀엽겠어요",
          "original_form": "헉 신기해요 !! 귀엽겠어요",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007214.1.20",
          "form": "아주 큐띠뽀짝합니다",
          "original_form": "아주 큐띠뽀짝합니다",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007214.1.21",
          "form": "성격은 제멋에 사는 애들이구",
          "original_form": "성격은 제멋에 사는 애들이구",
          "speaker_id": "1",
