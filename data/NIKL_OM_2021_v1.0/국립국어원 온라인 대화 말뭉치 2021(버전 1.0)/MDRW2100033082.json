      "utterance": [
        {
          "id": "MDRW2100033082.1.1",
          "form": "앗 다시 세금 이야기네요",
          "original_form": "앗 다시 세금 이야기네요",
          "speaker_id": "1",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100033082.1.2",
          "form": "아아ㅋㅋ",
          "original_form": "아아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100033082.1.3",
          "form": "예산을 늘려도",
          "original_form": "예산을 늘려도",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100033082.1.4",
          "form": "인터넷뱅킹  사고같은거나",
          "original_form": "인터넷뱅킹  사고같은거나",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100033082.1.5",
          "form": "네네네네",
          "original_form": "네네네네",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100033082.1.6",
          "form": "비대면대출사고 확실히 막아준다면",
          "original_form": "비대면대출사고 확실히 막아준다면",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100033082.1.7",
          "form": "맞아요 그리고 제발",
          "original_form": "맞아요 그리고 제발",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100033082.1.8",
          "form": "괜찮을거같은데",
          "original_form": "괜찮을거같은데",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.9",
          "form": "공인인증서 같은거",
          "original_form": "공인인증서 같은거",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.10",
          "form": "하 맞아요ㅠ",
          "original_form": "하 맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.11",
          "form": "과연 어떻게 쓸지 궁금하네요",
          "original_form": "과연 어떻게 쓸지 궁금하네요",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.12",
          "form": "인증서 어쩌구 이런 것도",
          "original_form": "인증서 어쩌구 이런 것도",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.13",
          "form": "좀 간편하게 만들면 좋겠어요",
          "original_form": "좀 간편하게 만들면 좋겠어요",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.14",
          "form": "공인인증서가 공동인증서된거는",
          "original_form": "공인인증서가 공동인증서된거는",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.15",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.16",
          "form": "사실 영문을 모르겠어요",
          "original_form": "사실 영문을 모르겠어요",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.17",
          "form": "이런 것도 개인정보위가 하는걸까요?",
          "original_form": "이런 것도 개인정보위가 하는걸까요?",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.18",
          "form": "그러니까요!!!",
          "original_form": "그러니까요!!!",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.19",
          "form": "다른나라는카드결제할때 그런거없는데",
          "original_form": "다른나라는카드결제할때 그런거없는데",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.20",
          "form": "왜 우리나라만 공동인증서가 있고",
          "original_form": "왜 우리나라만 공동인증서가 있고",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033082.1.21",
          "form": "그럼에도 불구하고 대출사고같은게",
          "original_form": "그럼에도 불구하고 대출사고같은게",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033082.1.22",
          "form": "왜자꾸일어나는지",
          "original_form": "왜자꾸일어나는지",
          "speaker_id": "2",
