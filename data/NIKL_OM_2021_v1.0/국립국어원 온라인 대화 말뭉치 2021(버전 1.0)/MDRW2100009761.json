      "utterance": [
        {
          "id": "MDRW2100009761.1.1",
          "form": "반차쓸번한거죠ㅋㅋ",
          "original_form": "반차쓸번한거죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:36"
        },
        {
          "id": "MDRW2100009761.1.2",
          "form": "좋은회사는 전혀아님",
          "original_form": "좋은회사는 전혀아님",
          "speaker_id": "2",
          "time": "20210823 09:36"
        },
        {
          "id": "MDRW2100009761.1.3",
          "form": "ㅋㅋ다 자기 회사는 좋지 않음",
          "original_form": "ㅋㅋ다 자기 회사는 좋지 않음",
          "speaker_id": "1",
          "time": "20210823 09:36"
        },
        {
          "id": "MDRW2100009761.1.4",
          "form": "인거 같아요",
          "original_form": "인거 같아요",
          "speaker_id": "1",
          "time": "20210823 09:36"
        },
        {
          "id": "MDRW2100009761.1.5",
          "form": "공휴일 연차에서 까는 회사 못보셨죠?ㅎㅎ",
          "original_form": "공휴일 연차에서 까는 회사 못보셨죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 09:36"
        },
        {
          "id": "MDRW2100009761.1.6",
          "form": "저희가 그런회사ㅠㅠ",
          "original_form": "저희가 그런회사ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 09:37"
        },
        {
          "id": "MDRW2100009761.1.7",
          "form": "임시공휴일 말씀 하시는건가요ㅎㅎ",
          "original_form": "임시공휴일 말씀 하시는건가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:37"
        },
        {
          "id": "MDRW2100009761.1.8",
          "form": "법정공휴일도요ㅎ",
          "original_form": "법정공휴일도요ㅎ",
          "speaker_id": "2",
          "time": "20210823 09:37"
        },
        {
          "id": "MDRW2100009761.1.9",
          "form": "노동법에 그게 불법이 아니래요ㅡㅡ",
          "original_form": "노동법에 그게 불법이 아니래요ㅡㅡ",
          "speaker_id": "2",
          "time": "20210823 09:37"
        },
        {
          "id": "MDRW2100009761.1.10",
          "form": "어 법정은 좀 선넘는데요ㅠㅠ",
          "original_form": "어 법정은 좀 선넘는데요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 09:37"
        },
        {
          "id": "MDRW2100009761.1.11",
          "form": "그 맞아요 줄 수 있다?",
          "original_form": "그 맞아요 줄 수 있다?",
          "speaker_id": "1",
          "time": "20210823 09:38"
        },
        {
          "id": "MDRW2100009761.1.12",
          "form": "막 말이 애매해서 ㅠㅠ",
          "original_form": "막 말이 애매해서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 09:38"
        },
        {
          "id": "MDRW2100009761.1.13",
          "form": "원래 공휴일은 공무원이 쉬는날이라네요",
          "original_form": "원래 공휴일은 공무원이 쉬는날이라네요",
          "speaker_id": "2",
          "time": "20210823 09:38"
        },
        {
          "id": "MDRW2100009761.1.14",
          "form": "민간기없는 상관없대요 그냥 예의상 다들 쉬는거지",
          "original_form": "민간기없는 상관없대요 그냥 예의상 다들 쉬는거지",
          "speaker_id": "2",
          "time": "20210823 09:39"
        },
        {
          "id": "MDRW2100009761.1.15",
          "form": "저도 전직장에서 겪어봐서ㅎㅎ",
          "original_form": "저도 전직장에서 겪어봐서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:41"
        },
        {
          "id": "MDRW2100009761.1.16",
          "form": "2년 다녔는데 제가 필요한 반차 딱 한번 썼네요",
          "original_form": "2년 다녔는데 제가 필요한 반차 딱 한번 썼네요",
          "speaker_id": "1",
          "time": "20210823 09:42"
        },
        {
          "id": "MDRW2100009761.1.17",
          "form": "나머지는 다 그냥 회사에서 차감",
          "original_form": "나머지는 다 그냥 회사에서 차감",
          "speaker_id": "1",
          "time": "20210823 09:42"
        },
        {
          "id": "MDRW2100009761.1.18",
          "form": "아 겪어봤군요 ㅎㅎ",
          "original_form": "아 겪어봤군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 09:43"
        },
        {
          "id": "MDRW2100009761.1.19",
          "form": "그래도 내년부턴 법적으로 연차에서 못까게 바뀐대요",
          "original_form": "그래도 내년부턴 법적으로 연차에서 못까게 바뀐대요",
          "speaker_id": "2",
          "time": "20210823 09:44"
        },
        {
          "id": "MDRW2100009761.1.20",
          "form": "그거 늦었지만 다행이네요ㅎㅎ",
          "original_form": "그거 늦었지만 다행이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:45"
        },
        {
          "id": "MDRW2100009761.1.21",
          "form": "당연한것을 감사히 여겨야한다니...",
          "original_form": "당연한것을 감사히 여겨야한다니...",
          "speaker_id": "1",
          "time": "20210823 09:45"
        },
        {
          "id": "MDRW2100009761.1.22",
          "form": "제말이 그말이에요",
          "original_form": "제말이 그말이에요",
          "speaker_id": "2",
          "time": "20210823 09:46"
        },
        {
          "id": "MDRW2100009761.1.23",
          "form": "월욜아침부터 피곤하네요",
          "original_form": "월욜아침부터 피곤하네요",
          "speaker_id": "2",
          "time": "20210823 09:46"
        },
        {
          "id": "MDRW2100009761.1.24",
          "form": "ㅋㅋ기분 좋은 이야기 하시죠~~~!!",
          "original_form": "ㅋㅋ기분 좋은 이야기 하시죠~~~!!",
          "speaker_id": "1",
          "time": "20210823 09:48"
        },
