      "utterance": [
        {
          "id": "MDRW2100036982.1.1",
          "form": "오늘 저녁 뭐 드실거에용",
          "original_form": "오늘 저녁 뭐 드실거에용",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100036982.1.2",
          "form": "제가 오늘은 좀 늦었네요 밥은 오늘 바빠서 그냥 밑반찬에 김이요",
          "original_form": "제가 오늘은 좀 늦었네요 밥은 오늘 바빠서 그냥 밑반찬에 김이요",
          "speaker_id": "1",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100036982.1.3",
          "form": "한달의 반은 그냥 ㅋㅋ 밑반찬과 김이네요",
          "original_form": "한달의 반은 그냥 ㅋㅋ 밑반찬과 김이네요",
          "speaker_id": "1",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100036982.1.4",
          "form": "name2님은 뭘드셨는지?",
          "original_form": "&name2&님은 뭘드셨는지?",
          "speaker_id": "1",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100036982.1.5",
          "form": "ㅋㅋㅋㅋㅋ 그래도 맛있게",
          "original_form": "ㅋㅋㅋㅋㅋ 그래도 맛있게",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100036982.1.6",
          "form": "드셨나요?!",
          "original_form": "드셨나요?!",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100036982.1.7",
          "form": "ㅠㅠ 이런 질문을 받으니 범사에 감사해야할것 같지만,,",
          "original_form": "ㅠㅠ 이런 질문을 받으니 범사에 감사해야할것 같지만,,",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100036982.1.8",
          "form": "별생각없이 먹었습니다 ㅋㅋ",
          "original_form": "별생각없이 먹었습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100036982.1.9",
          "form": "ㅋㅋㅋㅋ아ㅎ 그럴 수 있죠",
          "original_form": "ㅋㅋㅋㅋ아ㅎ 그럴 수 있죠",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100036982.1.10",
          "form": "저는 순대국밥 먹었습니다",
          "original_form": "저는 순대국밥 먹었습니다",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100036982.1.11",
          "form": "역시ㅋㅋ 저도 한 메뉴 꽂히면 그걸 자주 먹어요",
          "original_form": "역시ㅋㅋ 저도 한 메뉴 꽂히면 그걸 자주 먹어요",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100036982.1.12",
          "form": "한달 내내 순두부만 머근적도 있어요",
          "original_form": "한달 내내 순두부만 머근적도 있어요",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100036982.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 진짜요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 진짜요?",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100036982.1.14",
          "form": "한달이요???!",
          "original_form": "한달이요???!",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100036982.1.15",
          "form": "점심때 한번식만 먹어도",
          "original_form": "점심때 한번식만 먹어도",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100036982.1.16",
          "form": "글케 많인 목먹죠 한달에 20번이니 ㅋㅋ",
          "original_form": "글케 많인 목먹죠 한달에 20번이니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100036982.1.17",
          "form": "그래도 20일이면.. 어마어마..",
          "original_form": "그래도 20일이면.. 어마어마..",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100036982.1.18",
          "form": "점저를 ㅡ다 먹진 않았잖아요 ㅋㅋㅋ",
          "original_form": "점저를 ㅡ다 먹진 않았잖아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100036982.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그래도 제.딴에서는",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그래도 제.딴에서는",
          "speaker_id": "2",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100036982.1.20",
          "form": "대단해요",
          "original_form": "대단해요",
          "speaker_id": "2",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100036982.1.21",
          "form": "저에게 순대국 얘길 하신 분이 name2님이 아니셨나요",
          "original_form": "저에게 순대국 얘길 하신 분이 &name2&님이 아니셨나요",
          "speaker_id": "1",
          "time": "20210901 11:29"
        },
        {
          "id": "MDRW2100036982.1.22",
          "form": "제가 기억을 잘 못해서 ㅋㅋ",
          "original_form": "제가 기억을 잘 못해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:30"
        },
        {
          "id": "MDRW2100036982.1.23",
          "form": "한 분 순대국을 계속 드시는 줄 알았어욬",
          "original_form": "한 분 순대국을 계속 드시는 줄 알았어욬",
          "speaker_id": "1",
          "time": "20210901 11:30"
        },
        {
          "id": "MDRW2100036982.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아니에요ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아니에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100036982.1.25",
          "form": "어쩐지...!",
          "original_form": "어쩐지...!",
          "speaker_id": "2",
          "time": "20210901 12:05"
        },
        {
          "id": "MDRW2100036982.1.26",
          "form": "ㅠㅠㅠ 누군가 저에게 순대국을 점심때도 먹고 저녁때도 먹었다고 했는데",
          "original_form": "ㅠㅠㅠ 누군가 저에게 순대국을 점심때도 먹고 저녁때도 먹었다고 했는데",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100036982.1.27",
          "form": "ㅋㅋㅋ그래가지고 와 오늘도 순대국드시는구나 대단하다며 저도 순두부 한달간 먹은적 있다고 말한거였어욬ㅋ",
          "original_form": "ㅋㅋㅋ그래가지고 와 오늘도 순대국드시는구나 대단하다며 저도 순두부 한달간 먹은적 있다고 말한거였어욬ㅋ",
