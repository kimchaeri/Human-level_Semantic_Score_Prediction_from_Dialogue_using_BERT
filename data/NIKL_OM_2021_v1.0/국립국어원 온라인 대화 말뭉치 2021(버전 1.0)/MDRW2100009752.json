      "utterance": [
        {
          "id": "MDRW2100009752.1.1",
          "form": "여기도 가보고 싶고 저기도 가보고 싶고",
          "original_form": "여기도 가보고 싶고 저기도 가보고 싶고",
          "speaker_id": "2",
          "time": "20210823 09:27"
        },
        {
          "id": "MDRW2100009752.1.2",
          "form": "언젠가는 이룰 수 있겠죠?",
          "original_form": "언젠가는 이룰 수 있겠죠?",
          "speaker_id": "2",
          "time": "20210823 09:28"
        },
        {
          "id": "MDRW2100009752.1.3",
          "form": "네 꼭 그럴거에요",
          "original_form": "네 꼭 그럴거에요",
          "speaker_id": "1",
          "time": "20210823 09:28"
        },
        {
          "id": "MDRW2100009752.1.4",
          "form": "제주도라도 가야하나요",
          "original_form": "제주도라도 가야하나요",
          "speaker_id": "1",
          "time": "20210823 09:28"
        },
        {
          "id": "MDRW2100009752.1.5",
          "form": "제주도에는 여행객이 엄청 몰렸대요",
          "original_form": "제주도에는 여행객이 엄청 몰렸대요",
          "speaker_id": "2",
          "time": "20210823 09:29"
        },
        {
          "id": "MDRW2100009752.1.6",
          "form": "다들 비슷한 생각인가봐요",
          "original_form": "다들 비슷한 생각인가봐요",
          "speaker_id": "2",
          "time": "20210823 09:30"
        },
        {
          "id": "MDRW2100009752.1.7",
          "form": "그러게요 ㅎㅎ 그러면 여행 가기 무섭죠",
          "original_form": "그러게요 ㅎㅎ 그러면 여행 가기 무섭죠",
          "speaker_id": "1",
          "time": "20210823 09:36"
        },
        {
          "id": "MDRW2100009752.1.8",
          "form": "운전 면허라도 있으면 드라이브 나가고 싶네요",
          "original_form": "운전 면허라도 있으면 드라이브 나가고 싶네요",
          "speaker_id": "2",
          "time": "20210823 09:37"
        },
        {
          "id": "MDRW2100009752.1.9",
          "form": "ㅋㅋ그거 좋죠 교외로 슝",
          "original_form": "ㅋㅋ그거 좋죠 교외로 슝",
          "speaker_id": "1",
          "time": "20210823 09:41"
        },
        {
          "id": "MDRW2100009752.1.10",
          "form": "날씨가 개면 별보러 가고 싶어요",
          "original_form": "날씨가 개면 별보러 가고 싶어요",
          "speaker_id": "2",
          "time": "20210823 09:43"
        },
        {
          "id": "MDRW2100009752.1.11",
          "form": "별 어릴때는 몰랐는데",
          "original_form": "별 어릴때는 몰랐는데",
          "speaker_id": "1",
          "time": "20210823 09:44"
        },
        {
          "id": "MDRW2100009752.1.12",
          "form": "크니까 이쁘더라구요",
          "original_form": "크니까 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210823 09:44"
        },
        {
          "id": "MDRW2100009752.1.13",
          "form": "딱 한번 별을 많이 본적이 있는데 별이 쏟아진다는게 뭔지 알았어요 그때",
          "original_form": "딱 한번 별을 많이 본적이 있는데 별이 쏟아진다는게 뭔지 알았어요 그때",
          "speaker_id": "2",
          "time": "20210823 09:45"
        },
        {
          "id": "MDRW2100009752.1.14",
          "form": "보러가고 싶네요ㅎㅎ",
          "original_form": "보러가고 싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:47"
        },
        {
          "id": "MDRW2100009752.1.15",
          "form": "빨리 날이 갰으면 좋겠네요",
          "original_form": "빨리 날이 갰으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210823 09:49"
        },
        {
          "id": "MDRW2100009752.1.16",
          "form": "당분간 어렵지 않으려나요",
          "original_form": "당분간 어렵지 않으려나요",
          "speaker_id": "1",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009752.1.17",
          "form": "태풍 지나고 나면 괜찮을 거 같아요",
          "original_form": "태풍 지나고 나면 괜찮을 거 같아요",
          "speaker_id": "2",
