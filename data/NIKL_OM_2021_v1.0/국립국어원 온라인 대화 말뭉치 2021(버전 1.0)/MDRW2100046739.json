      "utterance": [
        {
          "id": "MDRW2100046739.1.1",
          "form": "반갑습니다",
          "original_form": "반갑습니다",
          "speaker_id": "1",
          "time": "20211104 11:56"
        },
        {
          "id": "MDRW2100046739.1.2",
          "form": "안녕하세요! 점심 먹고 올게요! 맛점하세요!",
          "original_form": "안녕하세요! 점심 먹고 올게요! 맛점하세요!",
          "speaker_id": "2",
          "time": "20211104 11:57"
        },
        {
          "id": "MDRW2100046739.1.3",
          "form": "네.맛있게드세요",
          "original_form": "네.맛있게드세요",
          "speaker_id": "1",
          "time": "20211104 11:58"
        },
        {
          "id": "MDRW2100046739.1.4",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20211104 13:18"
        },
        {
          "id": "MDRW2100046739.1.5",
          "form": "ㅎㅎ네.저는 이른 점심  먹었습니다",
          "original_form": "ㅎㅎ네.저는 이른 점심  먹었습니다",
          "speaker_id": "1",
          "time": "20211104 13:18"
        },
        {
          "id": "MDRW2100046739.1.6",
          "form": "맛있는거 드셨나요? ㅎㅎ",
          "original_form": "맛있는거 드셨나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:22"
        },
        {
          "id": "MDRW2100046739.1.7",
          "form": "국밥이요ㅋ",
          "original_form": "국밥이요ㅋ",
          "speaker_id": "1",
          "time": "20211104 13:23"
        },
        {
          "id": "MDRW2100046739.1.8",
          "form": "역시 직장인 점심은 국밥이죠 ㅎㅎㅎㅎ",
          "original_form": "역시 직장인 점심은 국밥이죠 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:26"
        },
        {
          "id": "MDRW2100046739.1.9",
          "form": "저는 덮밥 먹고 왔어요!",
          "original_form": "저는 덮밥 먹고 왔어요!",
          "speaker_id": "2",
          "time": "20211104 13:26"
        },
        {
          "id": "MDRW2100046739.1.10",
          "form": "덮밥도 맛있죠.쌀쌀해지니 입이 달달합니다.다 맛있어요ㅎㅎ",
          "original_form": "덮밥도 맛있죠.쌀쌀해지니 입이 달달합니다.다 맛있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 13:27"
        },
        {
          "id": "MDRW2100046739.1.11",
          "form": "맞아요.. 가을은 역시 살찌는 계절인가봐요 ㅋㅋㅋ",
          "original_form": "맞아요.. 가을은 역시 살찌는 계절인가봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 13:36"
        },
        {
          "id": "MDRW2100046739.1.12",
          "form": "가을 과일 좋아하세요?",
          "original_form": "가을 과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20211104 13:36"
        },
        {
          "id": "MDRW2100046739.1.13",
          "form": "과일은 그닥 안좋아하지만 배 좋아합니다ㅋ",
          "original_form": "과일은 그닥 안좋아하지만 배 좋아합니다ㅋ",
          "speaker_id": "1",
          "time": "20211104 13:38"
        },
        {
          "id": "MDRW2100046739.1.14",
          "form": "배 좋아하시는구나 ㅎㅎ",
          "original_form": "배 좋아하시는구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:49"
        },
        {
          "id": "MDRW2100046739.1.15",
          "form": "저도 과일을 그렇게 좋아하는 편은 아닌데 가을엔 단감이 맛있더라구요 ㅎㅎ",
          "original_form": "저도 과일을 그렇게 좋아하는 편은 아닌데 가을엔 단감이 맛있더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:49"
        },
        {
          "id": "MDRW2100046739.1.16",
          "form": "아.감도 좋지요.몸에도좋고",
          "original_form": "아.감도 좋지요.몸에도좋고",
          "speaker_id": "1",
          "time": "20211104 13:56"
        },
        {
          "id": "MDRW2100046739.1.17",
          "form": "가을하면  배.단감철",
          "original_form": "가을하면  배.단감철",
          "speaker_id": "1",
          "time": "20211104 13:56"
        },
        {
          "id": "MDRW2100046739.1.18",
          "form": "시골에 감나무가 있어서 엄청 따왔어요 ㅋㅋㅋ",
          "original_form": "시골에 감나무가 있어서 엄청 따왔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 13:57"
        },
        {
          "id": "MDRW2100046739.1.19",
          "form": "저도 외할머니댁 감나무생각나네요",
          "original_form": "저도 외할머니댁 감나무생각나네요",
          "speaker_id": "1",
          "time": "20211104 13:57"
        }
      ]
