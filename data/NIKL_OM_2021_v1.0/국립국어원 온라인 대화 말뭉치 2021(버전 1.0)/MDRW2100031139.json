      "utterance": [
        {
          "id": "MDRW2100031139.1.1",
          "form": "프로그램 챙겨보는거 있으세요?",
          "original_form": "프로그램 챙겨보는거 있으세요?",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100031139.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100031139.1.3",
          "form": "놀면뭐하니랑 그알정도?",
          "original_form": "놀면뭐하니랑 그알정도?",
          "speaker_id": "1",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100031139.1.4",
          "form": "오늘 놀면뭐하니 보셨나요?",
          "original_form": "오늘 놀면뭐하니 보셨나요?",
          "speaker_id": "2",
          "time": "20210905 00:05"
        },
        {
          "id": "MDRW2100031139.1.5",
          "form": "네 너무 웃겼어요",
          "original_form": "네 너무 웃겼어요",
          "speaker_id": "1",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100031139.1.6",
          "form": "아 정준하ㅜㅠㅋㅋ 진짜 흑채보고ㅜㅠ",
          "original_form": "아 정준하ㅜㅠㅋㅋ 진짜 흑채보고ㅜㅠ",
          "speaker_id": "2",
          "time": "20210905 00:13"
        },
        {
          "id": "MDRW2100031139.1.7",
          "form": "뿜었습니다",
          "original_form": "뿜었습니다",
          "speaker_id": "1",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100031139.1.8",
          "form": "예전 무한도전 생각났어요",
          "original_form": "예전 무한도전 생각났어요",
          "speaker_id": "1",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100031139.1.9",
          "form": "진짜 정준하 억울하고 뽀글머리할때가 제일 웃겨요",
          "original_form": "진짜 정준하 억울하고 뽀글머리할때가 제일 웃겨요",
          "speaker_id": "2",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100031139.1.10",
          "form": "ㅋㅋㅋㅋㅋ 얼굴 흑채범벅 ㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 얼굴 흑채범벅 ㅋ",
          "speaker_id": "1",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031139.1.11",
          "form": "ㅜㅜ 그리고 눈이 너무 짠한게 너무 웃겼어욬ㅋ",
          "original_form": "ㅜㅜ 그리고 눈이 너무 짠한게 너무 웃겼어욬ㅋ",
          "speaker_id": "2",
          "time": "20210905 08:29"
        },
        {
          "id": "MDRW2100031139.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100031139.1.13",
          "form": "맞춤법 상자 빠지는것도 웃겼어요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞춤법 상자 빠지는것도 웃겼어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100031139.1.14",
          "form": "무한도전이 그리워지는 ㅋㅋ",
          "original_form": "무한도전이 그리워지는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100031139.1.15",
          "form": "아 하하도 웃겼는데 정준하가 진짜ㅜ",
          "original_form": "아 하하도 웃겼는데 정준하가 진짜ㅜ",
          "speaker_id": "2",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100031139.1.16",
          "form": "저 무한도전 진짜 좋아했거든요",
          "original_form": "저 무한도전 진짜 좋아했거든요",
          "speaker_id": "1",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100031139.1.17",
          "form": "진짜 이번에 다 뭉쳤으면 좋았을껄",
          "original_form": "진짜 이번에 다 뭉쳤으면 좋았을껄",
          "speaker_id": "1",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100031139.1.18",
          "form": "저도 소랑 대결할때부터 ㅋㅋ",
          "original_form": "저도 소랑 대결할때부터 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100031139.1.19",
          "form": "어제 무도 향수를 느끼며 잼나게 봤어요 ㅎㅎ",
          "original_form": "어제 무도 향수를 느끼며 잼나게 봤어요 ㅎㅎ",
