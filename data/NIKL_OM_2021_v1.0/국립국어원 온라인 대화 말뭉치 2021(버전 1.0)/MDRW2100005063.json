      "utterance": [
        {
          "id": "MDRW2100005063.1.1",
          "form": "내일 가는집 맛집이겟지 ?",
          "original_form": "내일 가는집 맛집이겟지 ?",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005063.1.2",
          "form": "ㅋㅋㅋ 그냥 김치찌개지 뭐",
          "original_form": "ㅋㅋㅋ 그냥 김치찌개지 뭐",
          "speaker_id": "2",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005063.1.3",
          "form": "그지역 명물이라든가 ㅋㅋ",
          "original_form": "그지역 명물이라든가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005063.1.4",
          "form": "김치가 묵은지고 솥밥이라서 좋더라구. 가격도 8000원 저렴하구",
          "original_form": "김치가 묵은지고 솥밥이라서 좋더라구. 가격도 8000원 저렴하구",
          "speaker_id": "2",
          "time": "20210821 20:18"
        },
        {
          "id": "MDRW2100005063.1.5",
          "form": "메뉴 뭐 더있는지 봐야겟다",
          "original_form": "메뉴 뭐 더있는지 봐야겟다",
          "speaker_id": "1",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005063.1.6",
          "form": "이름이 뭐였지?",
          "original_form": "이름이 뭐였지?",
          "speaker_id": "1",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005063.1.7",
          "form": "갓지은 솥밥이었나",
          "original_form": "갓지은 솥밥이었나",
          "speaker_id": "2",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005063.1.8",
          "form": "검색해봐야겠어",
          "original_form": "검색해봐야겠어",
          "speaker_id": "1",
          "time": "20210821 20:19"
        },
        {
          "id": "MDRW2100005063.1.9",
          "form": "응 저녁은 먹었어?",
          "original_form": "응 저녁은 먹었어?",
          "speaker_id": "2",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005063.1.10",
          "form": "응 어징어볶음이랑 먹었어",
          "original_form": "응 어징어볶음이랑 먹었어",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005063.1.11",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100005063.1.12",
          "form": "아 맞다 오징어 볶음 먹었다 했지. 맛있었겠답.",
          "original_form": "아 맞다 오징어 볶음 먹었다 했지. 맛있었겠답.",
          "speaker_id": "2",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005063.1.13",
          "form": "난 교촌기다리는데 아직30분은 더 기다려야될듯",
          "original_form": "난 교촌기다리는데 아직30분은 더 기다려야될듯",
          "speaker_id": "2",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005063.1.14",
          "form": "오는데 한참걸리네",
          "original_form": "오는데 한참걸리네",
          "speaker_id": "1",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005063.1.15",
          "form": "배고파ㅠ",
          "original_form": "배고파ㅠ",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005063.1.16",
          "form": "어제 너의 아부지가 해주신 부침개 먹구싶다",
          "original_form": "어제 너의 아부지가 해주신 부침개 먹구싶다",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005063.1.17",
          "form": "ㅋㅋㅋㅋ 그거 남았다",
          "original_form": "ㅋㅋㅋㅋ 그거 남았다",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005063.1.18",
          "form": "오늘 언니에 왓엇는데 다 안팔림",
          "original_form": "오늘 언니에 왓엇는데 다 안팔림",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005063.1.19",
          "form": "짭짤하게 맛있게 잘하셨던데 왜 안팔렸댜",
          "original_form": "짭짤하게 맛있게 잘하셨던데 왜 안팔렸댜",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005063.1.20",
          "form": "갓지은 솥밥으로 출발해",
          "original_form": "갓지은 솥밥으로 출발해",
          "speaker_id": "1",
          "time": "20210822 11:22"
        }
      ]
