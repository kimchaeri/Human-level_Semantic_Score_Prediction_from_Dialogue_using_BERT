      "utterance": [
        {
          "id": "MDRW2100000754.1.1",
          "form": "제주도 놀러갈까",
          "original_form": "제주도 놀러갈까",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000754.1.2",
          "form": "가자 ㅋㅋ 좀 선선해지면",
          "original_form": "가자 ㅋㅋ 좀 선선해지면",
          "speaker_id": "1",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000754.1.3",
          "form": "우리나라 제주도 멋진곳 언제갈까",
          "original_form": "우리나라 제주도 멋진곳 언제갈까",
          "speaker_id": "2",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000754.1.4",
          "form": "휴가 낼수있나",
          "original_form": "휴가 낼수있나",
          "speaker_id": "2",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000754.1.5",
          "form": "휴가 낼수있지~ 2주휴가 ㅎㅎ",
          "original_form": "휴가 낼수있지~ 2주휴가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100000754.1.6",
          "form": "그럼 계획짜 가야지",
          "original_form": "그럼 계획짜 가야지",
          "speaker_id": "2",
          "time": "20210819 19:04"
        },
        {
          "id": "MDRW2100000754.1.7",
          "form": "구램 계획을 좀 짜봐야겠군~",
          "original_form": "구램 계획을 좀 짜봐야겠군~",
          "speaker_id": "1",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100000754.1.8",
          "form": "제주도는 식도락이쥬 ㅎ",
          "original_form": "제주도는 식도락이쥬 ㅎ",
          "speaker_id": "1",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100000754.1.9",
          "form": "여행은 맛집이지 ㅎ",
          "original_form": "여행은 맛집이지 ㅎ",
          "speaker_id": "2",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000754.1.10",
          "form": "옹 ㅋㅋ 제주도 흑돼지 먹고싶당 ㅋ",
          "original_form": "옹 ㅋㅋ 제주도 흑돼지 먹고싶당 ㅋ",
          "speaker_id": "1",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000754.1.11",
          "form": "난 톳국수 ㅎ",
          "original_form": "난 톳국수 ㅎ",
          "speaker_id": "2",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000754.1.12",
          "form": "맛나겠네 제주도 가면 맛난거 많이 먹자 ㅋㅋㅋ",
          "original_form": "맛나겠네 제주도 가면 맛난거 많이 먹자 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:24"
        },
        {
          "id": "MDRW2100000754.1.13",
          "form": "그래 맛집 탐방 ㅋ",
          "original_form": "그래 맛집 탐방 ㅋ",
          "speaker_id": "2",
          "time": "20210819 19:24"
        },
        {
          "id": "MDRW2100000754.1.14",
          "form": "좋았으 ㅋ 빨리 가고싶다",
          "original_form": "좋았으 ㅋ 빨리 가고싶다",
          "speaker_id": "1",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100000754.1.15",
          "form": "여행하길 좋은 계절이 다가오네 \r\n가즈아 제주도로~ ㅎ",
          "original_form": "여행하길 좋은 계절이 다가오네 \r\n가즈아 제주도로~ ㅎ",
