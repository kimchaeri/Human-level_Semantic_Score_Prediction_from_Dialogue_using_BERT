        {
          "id": "MDRW2100038328.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100038328.1.2",
          "form": "안녕하셑",
          "original_form": "안녕하셑",
          "speaker_id": "2",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100038328.1.3",
          "form": "세요",
          "original_form": "세요",
          "speaker_id": "2",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100038328.1.4",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100038328.1.5",
          "form": "아니요 ㅋㅋㅋ 아침 드셨어요?",
          "original_form": "아니요 ㅋㅋㅋ 아침 드셨어요?",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100038328.1.6",
          "form": "저도요 ㅋㅋㅋ 점심 때 폭식할 것 같아요 ㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋ 점심 때 폭식할 것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100038328.1.7",
          "form": "점심 뭐 드실건가요??",
          "original_form": "점심 뭐 드실건가요??",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100038328.1.8",
          "form": "그냥 구내식당에서 주는대로 먹어야죠ㅋㅋ",
          "original_form": "그냥 구내식당에서 주는대로 먹어야죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100038328.1.9",
          "form": "전 오늘 추어탕이래서 나가서 먹으려구요 ㅋㅋ",
          "original_form": "전 오늘 추어탕이래서 나가서 먹으려구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100038328.1.10",
          "form": "앗 그런 호불호 갈리는 메뉴도 구내식당에 나오나봐요",
          "original_form": "앗 그런 호불호 갈리는 메뉴도 구내식당에 나오나봐요",
          "speaker_id": "2",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100038328.1.11",
          "form": "개개인 호불호까지 챙겨주진 못하니까요",
          "original_form": "개개인 호불호까지 챙겨주진 못하니까요",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100038328.1.12",
          "form": "추어탕도 꽤 매니악한 메뉴라고 생각했는데... 어쩔 수 없죠. 나가서 뭐 드실 건가요?",
          "original_form": "추어탕도 꽤 매니악한 메뉴라고 생각했는데... 어쩔 수 없죠. 나가서 뭐 드실 건가요?",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100038328.1.13",
          "form": "마제소바 먹을까싶어요",
          "original_form": "마제소바 먹을까싶어요",
          "speaker_id": "1",
          "time": "20210902 11:08"
        },
        {
          "id": "MDRW2100038328.1.14",
          "form": "소바 좋아하세요?",
          "original_form": "소바 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 11:08"
        },
        {
          "id": "MDRW2100038328.1.15",
          "form": "소바 좋아해요! 근데 마제소바는 먹어본 적 없어요ㅋㅋ",
          "original_form": "소바 좋아해요! 근데 마제소바는 먹어본 적 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100038328.1.16",
          "form": "소바 좋아하시면 잘 드실 것 같아요\r\n노른자에 비벼 먹는 비빔국수 느낌이에요!",
          "original_form": "소바 좋아하시면 잘 드실 것 같아요\r\n노른자에 비벼 먹는 비빔국수 느낌이에요!",
          "speaker_id": "1",
          "time": "20210902 11:14"
        }
      ]
