      "utterance": [
        {
          "id": "MDRW2100014805.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014805.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014805.1.3",
          "form": "솔리테어 즐겨하시나봐묘~",
          "original_form": "솔리테어 즐겨하시나봐묘~",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014805.1.4",
          "form": "넹넹 ㅋㅋㅋㅋ 솔리테어에 요즘 꽂혔어요",
          "original_form": "넹넹 ㅋㅋㅋㅋ 솔리테어에 요즘 꽂혔어요",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014805.1.5",
          "form": "저도 솔리테어 해봤는데 너무 재밌더라구요",
          "original_form": "저도 솔리테어 해봤는데 너무 재밌더라구요",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014805.1.6",
          "form": "숫자가 맞아  떨어지는게 꽤 매력있어서 좋았어요",
          "original_form": "숫자가 맞아  떨어지는게 꽤 매력있어서 좋았어요",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014805.1.7",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014805.1.8",
          "form": "한 판에 한 1~2분밖에 안걸려서",
          "original_form": "한 판에 한 1~2분밖에 안걸려서",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014805.1.9",
          "form": "시간 떼우기에 좋은 것 같아요 ㅋㅋㅋ",
          "original_form": "시간 떼우기에 좋은 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014805.1.10",
          "form": "맞아요 금방 금방 끝나고 단순하고",
          "original_form": "맞아요 금방 금방 끝나고 단순하고",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014805.1.11",
          "form": "생각도 많이 하지않아도 되고 장점이 많은 게임 같아오",
          "original_form": "생각도 많이 하지않아도 되고 장점이 많은 게임 같아오",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014805.1.12",
          "form": "다른 게임은 어떤거 좋아하세요?",
          "original_form": "다른 게임은 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014805.1.13",
          "form": "저 모두의 마블 좋아해요",
          "original_form": "저 모두의 마블 좋아해요",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014805.1.14",
          "form": "모두의 마블 아시나요",
          "original_form": "모두의 마블 아시나요",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014805.1.15",
          "form": "넹넹 알죠 ㅋㅋㅋㅋ",
          "original_form": "넹넹 알죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014805.1.16",
          "form": "저도 한 때 열심히 했었어요!!",
          "original_form": "저도 한 때 열심히 했었어요!!",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014805.1.17",
          "form": "약간 주사위 굴러가는 소리가좋아서",
          "original_form": "약간 주사위 굴러가는 소리가좋아서",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014805.1.18",
          "form": "열심히했던거같아요",
          "original_form": "열심히했던거같아요",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014805.1.19",
          "form": "맞아욬ㅋㅋㅋㅋㅋ 주사위도 종류많고 효과 예쁜 것도 많잖아요",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋ 주사위도 종류많고 효과 예쁜 것도 많잖아요",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014805.1.20",
          "form": "그리고 캐릭터도 다양하고",
          "original_form": "그리고 캐릭터도 다양하고",
          "speaker_id": "1",
