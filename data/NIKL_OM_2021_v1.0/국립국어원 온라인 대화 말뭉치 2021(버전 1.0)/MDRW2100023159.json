      "utterance": [
        {
          "id": "MDRW2100023159.1.1",
          "form": "넹 전연애는 장거리엿으니까 전화가 필수엿는데",
          "original_form": "넹 전연애는 장거리엿으니까 전화가 필수엿는데",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023159.1.2",
          "form": "사실 이번 연애는 집이 좀 가까우니까",
          "original_form": "사실 이번 연애는 집이 좀 가까우니까",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023159.1.3",
          "form": "꼭 필수까진 아니어도 되겟다는",
          "original_form": "꼭 필수까진 아니어도 되겟다는",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023159.1.4",
          "form": "생각이 드네요",
          "original_form": "생각이 드네요",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023159.1.5",
          "form": "집가까우니까 자주봐서 좋아요?",
          "original_form": "집가까우니까 자주봐서 좋아요?",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023159.1.6",
          "form": "전 사실 집가까운 사람이랑 만난적이 없어요",
          "original_form": "전 사실 집가까운 사람이랑 만난적이 없어요",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023159.1.7",
          "form": "아직 자주보는 단계는 아닌데",
          "original_form": "아직 자주보는 단계는 아닌데",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023159.1.8",
          "form": "맨날 직장동료나 친구랑 저녁먹길래",
          "original_form": "맨날 직장동료나 친구랑 저녁먹길래",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023159.1.9",
          "form": "나한테도 저녁 시간 좀 내주라~ 햇어여 ㅋㅋㅋㅋ",
          "original_form": "나한테도 저녁 시간 좀 내주라~ 햇어여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023159.1.10",
          "form": "ㅋㅋㅋㅋ 오",
          "original_form": "ㅋㅋㅋㅋ 오",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023159.1.11",
          "form": "전 직장동료는 퇴근후엔 연락도 안하는데",
          "original_form": "전 직장동료는 퇴근후엔 연락도 안하는데",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023159.1.12",
          "form": "근데 저번주에 세번 봤다면서요 ㅋㅋㅋ",
          "original_form": "근데 저번주에 세번 봤다면서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023159.1.13",
          "form": "더 보고 싶어요?ㅋㅋㅋ",
          "original_form": "더 보고 싶어요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023159.1.14",
          "form": "앜ㅋㅋ 앞으로 약속을 잡으려면ㅋㅋㅋ 그렇게ㅋㅋ",
          "original_form": "앜ㅋㅋ 앞으로 약속을 잡으려면ㅋㅋㅋ 그렇게ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023159.1.15",
          "form": "제가 퇴근시간이 빨라서 사실 집에 일찍 가는데",
          "original_form": "제가 퇴근시간이 빨라서 사실 집에 일찍 가는데",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023159.1.16",
          "form": "심심하거든요",
          "original_form": "심심하거든요",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023159.1.17",
          "form": "선생님이세요?ㅋㅋㅋ",
          "original_form": "선생님이세요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023159.1.18",
          "form": "여친하고 똑같은 말을 ㅋㅋ",
          "original_form": "여친하고 똑같은 말을 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023159.1.19",
          "form": "앜ㅋㅋ넼ㅋㅋㅋ",
          "original_form": "앜ㅋㅋ넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023159.1.20",
          "form": "여친도 선생님이거든요",
          "original_form": "여친도 선생님이거든요",
          "speaker_id": "1",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023159.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023159.1.22",
          "form": "앜ㅋㅋㅋㅋㅋ그래서 좀 비슷한 면이 잇엇나봐요",
          "original_form": "앜ㅋㅋㅋㅋㅋ그래서 좀 비슷한 면이 잇엇나봐요",
