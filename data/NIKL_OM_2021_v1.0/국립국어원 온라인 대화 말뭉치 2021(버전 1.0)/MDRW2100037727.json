      "utterance": [
        {
          "id": "MDRW2100037727.1.1",
          "form": "휴 빨리 퇴근하고 신규 맵 탐험하고싶다",
          "original_form": "휴 빨리 퇴근하고 신규 맵 탐험하고싶다",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037727.1.2",
          "form": "겁나 빡세......",
          "original_form": "겁나 빡세......",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100037727.1.3",
          "form": "저 열매 좀 모으려면",
          "original_form": "저 열매 좀 모으려면",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037727.1.4",
          "form": "계속 번개치고",
          "original_form": "계속 번개치고",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037727.1.5",
          "form": "피깎이고",
          "original_form": "피깎이고",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037727.1.6",
          "form": "아 신규맵 또 번개쳐??",
          "original_form": "아 신규맵 또 번개쳐??",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037727.1.7",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037727.1.8",
          "form": "비 안오고",
          "original_form": "비 안오고",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037727.1.9",
          "form": "번개만 치는데",
          "original_form": "번개만 치는데",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037727.1.10",
          "form": "여긴 번개 내리치는 텀이",
          "original_form": "여긴 번개 내리치는 텀이",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037727.1.11",
          "form": "엄청 짧아",
          "original_form": "엄청 짧아",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037727.1.12",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100037727.1.13",
          "form": "번개 없애는 퀘는 아직 안떴지?",
          "original_form": "번개 없애는 퀘는 아직 안떴지?",
          "speaker_id": "1",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100037727.1.14",
          "form": "나 퀘 안보고",
          "original_form": "나 퀘 안보고",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100037727.1.15",
          "form": "걍 뛰어다니면서",
          "original_form": "걍 뛰어다니면서",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100037727.1.16",
          "form": "열매만 열심히 주웟어",
          "original_form": "열매만 열심히 주웟어",
          "speaker_id": "2",
          "time": "20210901 17:18"
        },
        {
          "id": "MDRW2100037727.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100037727.1.18",
          "form": "낚시퀘 뜬것까지만 봤는데...",
          "original_form": "낚시퀘 뜬것까지만 봤는데...",
          "speaker_id": "1",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100037727.1.19",
          "form": "이제 또",
          "original_form": "이제 또",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100037727.1.20",
          "form": "전기큐브 잡으러가야겠어",
          "original_form": "전기큐브 잡으러가야겠어",
          "speaker_id": "2",
          "time": "20210901 17:19"
        },
        {
          "id": "MDRW2100037727.1.21",
          "form": "아 그리고 이 섬",
          "original_form": "아 그리고 이 섬",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100037727.1.22",
          "form": "5렙월드 몹이 70렙이야",
          "original_form": "5렙월드 몹이 70렙이야",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100037727.1.23",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100037727.1.24",
          "form": "안그래도 해란귀랑 거울우인단은",
          "original_form": "안그래도 해란귀랑 거울우인단은",
          "speaker_id": "1",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100037727.1.25",
          "form": "같은 레벨 타 몹에 비해",
          "original_form": "같은 레벨 타 몹에 비해",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037727.1.26",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ몹 인플레",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ몹 인플레",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037727.1.27",
          "form": "원소저항도 세고 체력도 크던데",
          "original_form": "원소저항도 세고 체력도 크던데",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037727.1.28",
          "form": "심해지기 시작했어",
          "original_form": "심해지기 시작했어",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037727.1.29",
          "form": "이러다가 스네즈나야 가면 잡몹이 숨만 쉬어도 얼어붙는거 아니냐구....",
          "original_form": "이러다가 스네즈나야 가면 잡몹이 숨만 쉬어도 얼어붙는거 아니냐구....",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037727.1.30",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037727.1.31",
          "form": "밸런스 진짜 무슨일",
          "original_form": "밸런스 진짜 무슨일",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100037727.1.32",
          "form": "가봤어?",
          "original_form": "가봤어?",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100037727.1.33",
          "form": "나 거기는 당분간 파밍만 하러 다녀야겠어.....",
          "original_form": "나 거기는 당분간 파밍만 하러 다녀야겠어.....",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100037727.1.34",
          "form": "90렙 찍고 가는게 좋을것같아",
          "original_form": "90렙 찍고 가는게 좋을것같아",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100037727.1.35",
          "form": "아니 아직 퇴근 안해서...",
          "original_form": "아니 아직 퇴근 안해서...",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100037727.1.36",
          "form": "나 인터넷에서 공략 봤는데",
          "original_form": "나 인터넷에서 공략 봤는데",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100037727.1.37",
          "form": "이나즈마 모험가길드 퀘스트 하면",
          "original_form": "이나즈마 모험가길드 퀘스트 하면",
          "speaker_id": "1",
          "time": "20210901 18:43"
        }
      ]
