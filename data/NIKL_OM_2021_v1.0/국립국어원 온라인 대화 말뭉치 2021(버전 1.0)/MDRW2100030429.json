      "utterance": [
        {
          "id": "MDRW2100030429.1.1",
          "form": "오늘도 피쉬돔 했어요",
          "original_form": "오늘도 피쉬돔 했어요",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100030429.1.2",
          "form": "잠은 안 오나요",
          "original_form": "잠은 안 오나요",
          "speaker_id": "1",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030429.1.3",
          "form": "몇 단계 통과 했나요",
          "original_form": "몇 단계 통과 했나요",
          "speaker_id": "1",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030429.1.4",
          "form": "꽤 많이 했지요",
          "original_form": "꽤 많이 했지요",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030429.1.5",
          "form": "전주 기포 했어요",
          "original_form": "전주 기포 했어요",
          "speaker_id": "1",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030429.1.6",
          "form": "한번 볼게요",
          "original_form": "한번 볼게요",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030429.1.7",
          "form": "주키퍼",
          "original_form": "주키퍼",
          "speaker_id": "1",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030429.1.8",
          "form": "아까 영상 찍은 거 한번 봤나요",
          "original_form": "아까 영상 찍은 거 한번 봤나요",
          "speaker_id": "1",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030429.1.9",
          "form": "보긴 했어요",
          "original_form": "보긴 했어요",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.10",
          "form": "하고 싶은 게임이 많네요",
          "original_form": "하고 싶은 게임이 많네요",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.11",
          "form": "예전에 해 본 적 있나요",
          "original_form": "예전에 해 본 적 있나요",
          "speaker_id": "1",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.12",
          "form": "닌텐도에서 해 본 거 같기도 하고",
          "original_form": "닌텐도에서 해 본 거 같기도 하고",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.13",
          "form": "비슷한 게임이 있었어요",
          "original_form": "비슷한 게임이 있었어요",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.14",
          "form": "컴퓨터로 할 때 마우스 움직이기 힘들었는데",
          "original_form": "컴퓨터로 할 때 마우스 움직이기 힘들었는데",
          "speaker_id": "1",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.15",
          "form": "터치로 하니 편하고 좋아요 더 빨라요 높은 점수 받을 수 있어요",
          "original_form": "터치로 하니 편하고 좋아요 더 빨라요 높은 점수 받을 수 있어요",
          "speaker_id": "1",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.16",
          "form": "휴대폰이 편하죠",
          "original_form": "휴대폰이 편하죠",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.17",
          "form": "점점 컴퓨터가 필요 없어요",
          "original_form": "점점 컴퓨터가 필요 없어요",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.18",
          "form": "탭으로 해도 재밌어요",
          "original_form": "탭으로 해도 재밌어요",
          "speaker_id": "1",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.19",
          "form": "탭이 크니까 좋네요",
          "original_form": "탭이 크니까 좋네요",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.20",
          "form": "제 노트북은 화면이 두 개예요",
          "original_form": "제 노트북은 화면이 두 개예요",
          "speaker_id": "1",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100030429.1.21",
          "form": "터치 기능도 있고요",
          "original_form": "터치 기능도 있고요",
          "speaker_id": "1",
          "time": "20210904 00:59"
        }
