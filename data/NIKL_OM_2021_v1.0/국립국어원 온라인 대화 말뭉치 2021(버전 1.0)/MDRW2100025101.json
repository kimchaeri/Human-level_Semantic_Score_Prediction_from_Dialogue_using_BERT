        {
          "id": "MDRW2100025101.1.1",
          "form": "저기요",
          "original_form": "저기요",
          "speaker_id": "2",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025101.1.2",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025101.1.3",
          "form": "우주여행 가보샸나요",
          "original_form": "우주여행 가보샸나요",
          "speaker_id": "2",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025101.1.4",
          "form": "아니요 돈 없어요",
          "original_form": "아니요 돈 없어요",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025101.1.5",
          "form": "근데 제 버킷리스트에요",
          "original_form": "근데 제 버킷리스트에요",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025101.1.6",
          "form": "전 가봤어요",
          "original_form": "전 가봤어요",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025101.1.7",
          "form": "오 어디가보셨어요",
          "original_form": "오 어디가보셨어요",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025101.1.8",
          "form": "용케 안드로메다에 빠지지 않으셨군요",
          "original_form": "용케 안드로메다에 빠지지 않으셨군요",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025101.1.9",
          "form": "..",
          "original_form": "..",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025101.1.10",
          "form": "내려서 구경한게 아니라",
          "original_form": "내려서 구경한게 아니라",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025101.1.11",
          "form": "비행선 타고 한바퀴 도는 거예요",
          "original_form": "비행선 타고 한바퀴 도는 거예요",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025101.1.12",
          "form": "그니까 어디어디 다녀오셨어요?",
          "original_form": "그니까 어디어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025101.1.13",
          "form": "달 보고 왔어요",
          "original_form": "달 보고 왔어요",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025101.1.14",
          "form": "달은 못볼텐데요",
          "original_form": "달은 못볼텐데요",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025101.1.15",
          "form": "얼어죽을텐데요",
          "original_form": "얼어죽을텐데요",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025101.1.16",
          "form": "비행선 안에서 봤다고요",
          "original_form": "비행선 안에서 봤다고요",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025101.1.17",
          "form": "비행선도 얼을텤제요",
          "original_form": "비행선도 얼을텤제요",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025101.1.18",
          "form": "님 유딩이죠",
          "original_form": "님 유딩이죠",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
