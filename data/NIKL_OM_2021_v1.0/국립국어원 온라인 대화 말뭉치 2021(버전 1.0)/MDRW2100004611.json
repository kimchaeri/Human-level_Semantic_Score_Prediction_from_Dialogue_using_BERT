      "utterance": [
        {
          "id": "MDRW2100004611.1.1",
          "form": "책 괜찮으세요?",
          "original_form": "책 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004611.1.2",
          "form": "네네 저 책 엄청 좋아해요",
          "original_form": "네네 저 책 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004611.1.3",
          "form": "어떤 장르 주로 보세요?",
          "original_form": "어떤 장르 주로 보세요?",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004611.1.4",
          "form": "저는 종이책으로는 추리, 과학 쪽 많이 보고 인터넷으로는 로판 많이 보는 거 같아요",
          "original_form": "저는 종이책으로는 추리, 과학 쪽 많이 보고 인터넷으로는 로판 많이 보는 거 같아요",
          "speaker_id": "2",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004611.1.5",
          "form": "재밌게 본 추리 소설 있으세요?",
          "original_form": "재밌게 본 추리 소설 있으세요?",
          "speaker_id": "1",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004611.1.6",
          "form": "저는 한참 추리 소설 볼 때 히가시노 게이고 책 엄청 봤어요",
          "original_form": "저는 한참 추리 소설 볼 때 히가시노 게이고 책 엄청 봤어요",
          "speaker_id": "1",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004611.1.7",
          "form": "헉 저도 히가시노 게이고요! 히가시노 책중에는 라플라스의 마녀 좋아해요 추리 우타노쇼고의 밀실살인게임도 재밌어요",
          "original_form": "헉 저도 히가시노 게이고요! 히가시노 책중에는 라플라스의 마녀 좋아해요 추리 우타노쇼고의 밀실살인게임도 재밌어요",
          "speaker_id": "2",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004611.1.8",
          "form": "오 시간나면 밀실살인게임 한번 찾아볼게요",
          "original_form": "오 시간나면 밀실살인게임 한번 찾아볼게요",
          "speaker_id": "1",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004611.1.9",
          "form": "재밌을 거 같아요",
          "original_form": "재밌을 거 같아요",
          "speaker_id": "1",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004611.1.10",
          "form": "과학 쪽은 어떤 책 재밌게 읽으셨나요? 저는 어렵지 않은 과학 책 위주로 봐요",
          "original_form": "과학 쪽은 어떤 책 재밌게 읽으셨나요? 저는 어렵지 않은 과학 책 위주로 봐요",
          "speaker_id": "1",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004611.1.11",
          "form": "네네 진짜 재밌어요 꼭 보세요",
          "original_form": "네네 진짜 재밌어요 꼭 보세요",
          "speaker_id": "2",
          "time": "20210821 17:42"
        },
        {
          "id": "MDRW2100004611.1.12",
          "form": "과학은",
          "original_form": "과학은",
          "speaker_id": "2",
          "time": "20210821 17:42"
        },
        {
          "id": "MDRW2100004611.1.13",
          "form": "옛날에는 베르나르 베르베르 옛날책 좋아했어요",
          "original_form": "옛날에는 베르나르 베르베르 옛날책 좋아했어요",
          "speaker_id": "2",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004611.1.14",
          "form": "그 작가 말고는",
          "original_form": "그 작가 말고는",
          "speaker_id": "2",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004611.1.15",
          "form": "메리 로치의 인체재활용이나",
          "original_form": "메리 로치의 인체재활용이나",
          "speaker_id": "2",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004611.1.16",
          "form": "앨러건트 유니버스 이런거 좋아해요",
          "original_form": "앨러건트 유니버스 이런거 좋아해요",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004611.1.17",
          "form": "저는 전공이 과학이라 나름 재밌게 읽어요ㅋㅋㅋ",
          "original_form": "저는 전공이 과학이라 나름 재밌게 읽어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004611.1.18",
          "form": "근데 전공공부는 재미없어요",
          "original_form": "근데 전공공부는 재미없어요",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004611.1.19",
          "form": "베르나르 베르베르 책 저도 좋아해요",
          "original_form": "베르나르 베르베르 책 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004611.1.20",
          "form": "진짜 과학을 어떻게 그렇게 재밌게 소설로 풀어내는지!",
          "original_form": "진짜 과학을 어떻게 그렇게 재밌게 소설로 풀어내는지!",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004611.1.21",
          "form": "맞아요ㅋㅋㅋ 근데 안읽은지 좀 됐는데 제3인류까지는 엄청 재밌게 읽었어요 죽음 1, 2도 사긴했는데 아직 못읽어 봤어요",
          "original_form": "맞아요ㅋㅋㅋ 근데 안읽은지 좀 됐는데 제3인류까지는 엄청 재밌게 읽었어요 죽음 1, 2도 사긴했는데 아직 못읽어 봤어요",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004611.1.22",
          "form": "저는 최근에 마이크 브라운의 <나는 어쩌다 명왕성을 죽였나> 읽었는데",
          "original_form": "저는 최근에 마이크 브라운의 <나는 어쩌다 명왕성을 죽였나> 읽었는데",
          "speaker_id": "1",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004611.1.23",
          "form": "이것도 어렵지 않고 재밌더라고요",
          "original_form": "이것도 어렵지 않고 재밌더라고요",
          "speaker_id": "1",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004611.1.24",
          "form": "다큐 보는 거 같았어요",
          "original_form": "다큐 보는 거 같았어요",
          "speaker_id": "1",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004611.1.25",
          "form": "아ㅋㅋㅋ 마이크 브라운 자체가 유쾌한 사람이라 더 재밌게 읽었던거 같아요",
          "original_form": "아ㅋㅋㅋ 마이크 브라운 자체가 유쾌한 사람이라 더 재밌게 읽었던거 같아요",
          "speaker_id": "2",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004611.1.26",
          "form": "정말 재밌게 썼더라고요",
          "original_form": "정말 재밌게 썼더라고요",
          "speaker_id": "1",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004611.1.27",
          "form": "그리고 대학원생 체험하는 느낌이었어요",
          "original_form": "그리고 대학원생 체험하는 느낌이었어요",
          "speaker_id": "1",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004611.1.28",
          "form": "약간 그쪽 지식 느낌이 있죠ㅋㅋㅋ 우주는 언제나 무한하고 재밌는거 같아요",
          "original_form": "약간 그쪽 지식 느낌이 있죠ㅋㅋㅋ 우주는 언제나 무한하고 재밌는거 같아요",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004611.1.29",
          "form": "무한한 게 가끔 무섭기도 해요 제가 너무 작은 존재 같아서",
          "original_form": "무한한 게 가끔 무섭기도 해요 제가 너무 작은 존재 같아서",
          "speaker_id": "1",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004611.1.30",
          "form": "그래도 신비롭고 재밌는 건 맞아요",
          "original_form": "그래도 신비롭고 재밌는 건 맞아요",
          "speaker_id": "1",
          "time": "20210821 17:59"
        }
      ]
    }
