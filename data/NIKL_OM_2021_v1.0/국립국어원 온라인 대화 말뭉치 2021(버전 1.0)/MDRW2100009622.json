      "utterance": [
        {
          "id": "MDRW2100009622.1.1",
          "form": "교대가 다 같은 초등교육인데 그 안에서도 부전공으로 나뉘거든요",
          "original_form": "교대가 다 같은 초등교육인데 그 안에서도 부전공으로 나뉘거든요",
          "speaker_id": "2",
          "time": "20210823 01:50"
        },
        {
          "id": "MDRW2100009622.1.2",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210823 01:50"
        },
        {
          "id": "MDRW2100009622.1.3",
          "form": "몰ㅇ랐어요",
          "original_form": "몰ㅇ랐어요",
          "speaker_id": "1",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.4",
          "form": "국어\r\n수학\r\n영어\r\n사회\r\n과학\r\n도덕\r\n음악\r\n실과\r\n미술\r\n체육",
          "original_form": "국어\r\n수학\r\n영어\r\n사회\r\n과학\r\n도덕\r\n음악\r\n실과\r\n미술\r\n체육",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.5",
          "form": "부전공이  뭐였나요",
          "original_form": "부전공이  뭐였나요",
          "speaker_id": "1",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.6",
          "form": "등등 12개로요",
          "original_form": "등등 12개로요",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.7",
          "form": "전 음",
          "original_form": "전 음",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.8",
          "form": "영어인데",
          "original_form": "영어인데",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.9",
          "form": "제가 고등학교  때 영어를 제일 못했어서",
          "original_form": "제가 고등학교  때 영어를 제일 못했어서",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.10",
          "form": "와우 English",
          "original_form": "와우 English",
          "speaker_id": "1",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.11",
          "form": "힘들어하고 있어요",
          "original_form": "힘들어하고 있어요",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.12",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "1",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.13",
          "form": "ㅋㅋㅋㅋㅋㄲㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄲㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.14",
          "form": "영어를 선택한 이유가  있나요",
          "original_form": "영어를 선택한 이유가  있나요",
          "speaker_id": "1",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.15",
          "form": "실과 가고싶었는데",
          "original_form": "실과 가고싶었는데",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.16",
          "form": "못질에 자신있어서",
          "original_form": "못질에 자신있어서",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009622.1.17",
          "form": "입학 당시 유일하게 알고 있던 친구가",
          "original_form": "입학 당시 유일하게 알고 있던 친구가",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.18",
          "form": "같이 영어 가자고 꼬셨어요",
          "original_form": "같이 영어 가자고 꼬셨어요",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.19",
          "form": "그래서 영어 지원했는데",
          "original_form": "그래서 영어 지원했는데",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.20",
          "form": "저런...",
          "original_form": "저런...",
          "speaker_id": "1",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.21",
          "form": "오니까 걘 없고",
          "original_form": "오니까 걘 없고",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.22",
          "form": "저뿐이었어요",
          "original_form": "저뿐이었어요",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.23",
          "form": "그친구는 영어를 잘하던가요",
          "original_form": "그친구는 영어를 잘하던가요",
          "speaker_id": "1",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.24",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "1",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.25",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.26",
          "form": "저런..",
          "original_form": "저런..",
          "speaker_id": "1",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.27",
          "form": "나쁜친구..",
          "original_form": "나쁜친구..",
          "speaker_id": "1",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.28",
          "form": "저보다 훨씬이요",
          "original_form": "저보다 훨씬이요",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.29",
          "form": "4지망에 체육을 썼는데",
          "original_form": "4지망에 체육을 썼는데",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.30",
          "form": "체육 원하는 사람이 워낙 적어서",
          "original_form": "체육 원하는 사람이 워낙 적어서",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.31",
          "form": "4지망 안에 체육 쓴 사라은",
          "original_form": "4지망 안에 체육 쓴 사라은",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.32",
          "form": "람은",
          "original_form": "람은",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009622.1.33",
          "form": "무조건 체육으로 배정된대요",
          "original_form": "무조건 체육으로 배정된대요",
          "speaker_id": "2",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009622.1.34",
          "form": "그래서 체육에 있더라고요",
          "original_form": "그래서 체육에 있더라고요",
          "speaker_id": "2",
