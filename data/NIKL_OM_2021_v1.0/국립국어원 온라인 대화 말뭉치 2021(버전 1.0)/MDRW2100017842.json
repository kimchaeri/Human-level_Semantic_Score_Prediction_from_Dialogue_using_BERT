      "utterance": [
        {
          "id": "MDRW2100017842.1.1",
          "form": "체인 어디꺼 못쓰고 계세요?",
          "original_form": "체인 어디꺼 못쓰고 계세요?",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017842.1.2",
          "form": "브레댄코인가",
          "original_form": "브레댄코인가",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017842.1.3",
          "form": "이름도 첨들어봤어요 ㅋㅋ",
          "original_form": "이름도 첨들어봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017842.1.4",
          "form": "며칠후 사울가는데 거기서 써보려고요",
          "original_form": "며칠후 사울가는데 거기서 써보려고요",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017842.1.5",
          "form": "샌드위치가 맛나데요",
          "original_form": "샌드위치가 맛나데요",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017842.1.6",
          "form": "이것도 네이버 본거라 정확하지가 않아요",
          "original_form": "이것도 네이버 본거라 정확하지가 않아요",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017842.1.7",
          "form": "첨들어봐요 ㅋㅋ",
          "original_form": "첨들어봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017842.1.8",
          "form": "전 파리나 뚜레쥬르 뭐 스벅 요런거만 주위에 있어서요",
          "original_form": "전 파리나 뚜레쥬르 뭐 스벅 요런거만 주위에 있어서요",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017842.1.9",
          "form": "이왕 멀리서 사는데",
          "original_form": "이왕 멀리서 사는데",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017842.1.10",
          "form": "맛나면 좋겠어요",
          "original_form": "맛나면 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017842.1.11",
          "form": "오늘 점심 뭐 하셨나요?",
          "original_form": "오늘 점심 뭐 하셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017842.1.12",
          "form": "점심은 밖에서 먹기 회사 눈치보여서 편의점 음식으로 때웠습니다",
          "original_form": "점심은 밖에서 먹기 회사 눈치보여서 편의점 음식으로 때웠습니다",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017842.1.13",
          "form": "근래는 도시락 아니면",
          "original_form": "근래는 도시락 아니면",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017842.1.14",
          "form": "편의점으로 해결하고 있어요",
          "original_form": "편의점으로 해결하고 있어요",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017842.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017842.1.16",
          "form": "그렇죠 많이 도시락 시켜서 먹고",
          "original_form": "그렇죠 많이 도시락 시켜서 먹고",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017842.1.17",
          "form": "근데 편의점 매번 메뉴 바꿔도",
          "original_form": "근데 편의점 매번 메뉴 바꿔도",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017842.1.18",
          "form": "이상하게 똑같은거 먹는거 같아요",
          "original_form": "이상하게 똑같은거 먹는거 같아요",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017842.1.19",
          "form": "어떤 도시락 좋아하세요?",
          "original_form": "어떤 도시락 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017842.1.20",
          "form": "아 도시락은 싸서 다니고 대게 김밥이나 카레 많이 쌉니다",
          "original_form": "아 도시락은 싸서 다니고 대게 김밥이나 카레 많이 쌉니다",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017842.1.21",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017842.1.22",
          "form": "아침에 어띠싸세요",
          "original_form": "아침에 어띠싸세요",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017842.1.23",
          "form": "어띠",
          "original_form": "어띠",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017842.1.24",
          "form": "아휴어찌",
          "original_form": "아휴어찌",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017842.1.25",
          "form": "전 반찬 맨날 고민 될것 같아요",
          "original_form": "전 반찬 맨날 고민 될것 같아요",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017842.1.26",
          "form": "뭐 싸세요?",
          "original_form": "뭐 싸세요?",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017842.1.27",
          "form": "저 남편 몇번싸주다 예전에 때려치웠네요 ㅋㅋ",
          "original_form": "저 남편 몇번싸주다 예전에 때려치웠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017842.1.28",
          "form": "다행히 회사에 전자레인지가 있어서요 ㅠ",
          "original_form": "다행히 회사에 전자레인지가 있어서요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017842.1.29",
          "form": "네 저도 예전에 싸서 데워머ㅓㄱ었어요",
          "original_form": "네 저도 예전에 싸서 데워머ㅓㄱ었어요",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017842.1.30",
          "form": "먹었어요",
          "original_form": "먹었어요",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017842.1.31",
          "form": "전 도시락싸다 나중에 컵밥 사갔어요 ㅎㅎ",
          "original_form": "전 도시락싸다 나중에 컵밥 사갔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017842.1.32",
          "form": "컵밥은 생각보다 저는 양이 적어서 당황했어요.. 먹오 약간 허한느낌?",
          "original_form": "컵밥은 생각보다 저는 양이 적어서 당황했어요.. 먹오 약간 허한느낌?",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017842.1.33",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017842.1.34",
          "form": "두개 먹어야해요",
          "original_form": "두개 먹어야해요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017842.1.35",
          "form": "너무 밥도 적죠 ㅜㅜ",
          "original_form": "너무 밥도 적죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017842.1.36",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017842.1.37",
          "form": "사발면이랑 먹어야해요",
          "original_form": "사발면이랑 먹어야해요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017842.1.38",
          "form": "좀 많이 넣어주지 간사해요",
          "original_form": "좀 많이 넣어주지 간사해요",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017842.1.39",
          "form": "맞아요 그럼 딱 맞는거 같아요 국물에",
          "original_form": "맞아요 그럼 딱 맞는거 같아요 국물에",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017842.1.40",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017842.1.41",
          "form": "국물 빠질수 없어요",
          "original_form": "국물 빠질수 없어요",
