      "utterance": [
        {
          "id": "MDRW2100002491.1.1",
          "form": "지인 중에 군대 간 사람 있으세요?",
          "original_form": "지인 중에 군대 간 사람 있으세요?",
          "speaker_id": "2",
          "time": "20210821 09:59"
        },
        {
          "id": "MDRW2100002491.1.2",
          "form": "친구? 있긴한데 제대했어요",
          "original_form": "친구? 있긴한데 제대했어요",
          "speaker_id": "1",
          "time": "20210821 10:00"
        },
        {
          "id": "MDRW2100002491.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210821 10:00"
        },
        {
          "id": "MDRW2100002491.1.4",
          "form": "전 가족이 들어가있어요",
          "original_form": "전 가족이 들어가있어요",
          "speaker_id": "2",
          "time": "20210821 10:00"
        },
        {
          "id": "MDRW2100002491.1.5",
          "form": "남동생이요?",
          "original_form": "남동생이요?",
          "speaker_id": "1",
          "time": "20210821 10:00"
        },
        {
          "id": "MDRW2100002491.1.6",
          "form": "오빠인가..?",
          "original_form": "오빠인가..?",
          "speaker_id": "1",
          "time": "20210821 10:00"
        },
        {
          "id": "MDRW2100002491.1.7",
          "form": "동생이요 ㅎ",
          "original_form": "동생이요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 10:00"
        },
        {
          "id": "MDRW2100002491.1.8",
          "form": "이야기 들어보면 싫다는데",
          "original_form": "이야기 들어보면 싫다는데",
          "speaker_id": "2",
          "time": "20210821 10:00"
        },
        {
          "id": "MDRW2100002491.1.9",
          "form": "제 입장에선 널널해보여요ㅋㅋ",
          "original_form": "제 입장에선 널널해보여요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 10:01"
        },
        {
          "id": "MDRW2100002491.1.10",
          "form": "아무래도 인식이 예전보다는 나아졌다는 게 있어서ㅋㅋ",
          "original_form": "아무래도 인식이 예전보다는 나아졌다는 게 있어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 10:01"
        },
        {
          "id": "MDRW2100002491.1.11",
          "form": "군대가면 PX있잖아요 뭐 부탁해보신적 있으세요?",
          "original_form": "군대가면 PX있잖아요 뭐 부탁해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210821 10:01"
        },
        {
          "id": "MDRW2100002491.1.12",
          "form": "생각 못하고 그냥 포카리가루만 받아봤어요",
          "original_form": "생각 못하고 그냥 포카리가루만 받아봤어요",
          "speaker_id": "2",
          "time": "20210821 10:02"
        },
        {
          "id": "MDRW2100002491.1.13",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 10:02"
        },
        {
          "id": "MDRW2100002491.1.14",
          "form": "뭐 좋응 거 있나요?",
          "original_form": "뭐 좋응 거 있나요?",
          "speaker_id": "2",
          "time": "20210821 10:02"
        },
        {
          "id": "MDRW2100002491.1.15",
          "form": "사실 군대갔을 때 부탁해본게\r\n없어서",
          "original_form": "사실 군대갔을 때 부탁해본게\r\n없어서",
          "speaker_id": "1",
          "time": "20210821 10:02"
        },
        {
          "id": "MDRW2100002491.1.16",
          "form": "전화는 엄청오던데 안받았어여ㅋㅋㅋㅋ",
          "original_form": "전화는 엄청오던데 안받았어여ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 10:02"
        },
        {
          "id": "MDRW2100002491.1.17",
          "form": "약간 들어보면 맨날 풀 뽑는다고 하더라구요",
          "original_form": "약간 들어보면 맨날 풀 뽑는다고 하더라구요",
          "speaker_id": "1",
          "time": "20210821 10:03"
        },
        {
          "id": "MDRW2100002491.1.18",
          "form": "ㅋㅋㅋㅋ 풀이여? 그건 좀 노동이네요",
          "original_form": "ㅋㅋㅋㅋ 풀이여? 그건 좀 노동이네요",
          "speaker_id": "2",
          "time": "20210821 10:08"
        },
        {
          "id": "MDRW2100002491.1.19",
          "form": "그 이야기도 하던데 군대에서는 치약이 만병통치약이래요",
          "original_form": "그 이야기도 하던데 군대에서는 치약이 만병통치약이래요",
          "speaker_id": "1",
          "time": "20210821 10:09"
        },
        {
          "id": "MDRW2100002491.1.20",
          "form": "오 진짜요?",
          "original_form": "오 진짜요?",
          "speaker_id": "2",
          "time": "20210821 10:15"
        },
        {
          "id": "MDRW2100002491.1.21",
