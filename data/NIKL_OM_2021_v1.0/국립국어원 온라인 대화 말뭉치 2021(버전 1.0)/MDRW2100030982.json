      "utterance": [
        {
          "id": "MDRW2100030982.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030982.1.2",
          "form": "엩 엄마랑방금 간식먹엇는데료",
          "original_form": "엩 엄마랑방금 간식먹엇는데료",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100030982.1.3",
          "form": "레몬케이쿠 맛잇게 먹엇어요",
          "original_form": "레몬케이쿠 맛잇게 먹엇어요",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100030982.1.4",
          "form": "휘낭시에도 맛잇고 커피는 별로였오요",
          "original_form": "휘낭시에도 맛잇고 커피는 별로였오요",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100030982.1.5",
          "form": "님은 식사하셨나요",
          "original_form": "님은 식사하셨나요",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100030982.1.6",
          "form": "라면이요",
          "original_form": "라면이요",
          "speaker_id": "1",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100030982.1.7",
          "form": "무슨라면드셨나요",
          "original_form": "무슨라면드셨나요",
          "speaker_id": "2",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100030982.1.8",
          "form": "진라면요",
          "original_form": "진라면요",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100030982.1.9",
          "form": "진맵이었나요?",
          "original_form": "진맵이었나요?",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100030982.1.10",
          "form": "라면은 진매지",
          "original_form": "라면은 진매지",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100030982.1.11",
          "form": "저는 계란 넣어먹는거 좋아해요 그리고 밥 말기",
          "original_form": "저는 계란 넣어먹는거 좋아해요 그리고 밥 말기",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100030982.1.12",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100030982.1.13",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100030982.1.14",
          "form": "가끔 치즈 올려먹기",
          "original_form": "가끔 치즈 올려먹기",
          "speaker_id": "2",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100030982.1.15",
          "form": "라면... 무슨라면 제일 좋아하세요",
          "original_form": "라면... 무슨라면 제일 좋아하세요",
          "speaker_id": "2",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100030982.1.16",
          "form": "저는 불닭볶음면이요",
          "original_form": "저는 불닭볶음면이요",
          "speaker_id": "2",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100030982.1.17",
          "form": "진라면요",
          "original_form": "진라면요",
          "speaker_id": "1",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100030982.1.18",
          "form": "전 매운걸 잘 못먹어요",
          "original_form": "전 매운걸 잘 못먹어요",
          "speaker_id": "1",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100030982.1.19",
          "form": "저도 매운거 잘 못먹어서 옛날엔 신라면도 매웠는데",
          "original_form": "저도 매운거 잘 못먹어서 옛날엔 신라면도 매웠는데",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100030982.1.20",
          "form": "맛잇어서... 불닭 먹다보니까 먹을만은 해요",
          "original_form": "맛잇어서... 불닭 먹다보니까 먹을만은 해요",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100030982.1.21",
          "form": "먹고나서 죽어있긴 하지만요",
          "original_form": "먹고나서 죽어있긴 하지만요",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100030982.1.22",
          "form": "ㅋㅋ 즐기나요?",
          "original_form": "ㅋㅋ 즐기나요?",
          "speaker_id": "1",
          "time": "20210904 18:39"
        },
        {
          "id": "MDRW2100030982.1.23",
          "form": "매운거 좋아해요",
          "original_form": "매운거 좋아해요",
          "speaker_id": "2",
          "time": "20210904 19:24"
        },
        {
          "id": "MDRW2100030982.1.24",
          "form": "못먹어서 그렇지",
          "original_form": "못먹어서 그렇지",
          "speaker_id": "2",
          "time": "20210904 19:24"
        },
        {
          "id": "MDRW2100030982.1.25",
          "form": "마라탕 불닭 엽떡...",
          "original_form": "마라탕 불닭 엽떡...",
          "speaker_id": "2",
          "time": "20210904 19:24"
        },
        {
          "id": "MDRW2100030982.1.26",
          "form": "오 좋네여",
          "original_form": "오 좋네여",
          "speaker_id": "1",
          "time": "20210904 20:23"
        }
      ]
