      "utterance": [
        {
          "id": "MDRW2100007276.1.1",
          "form": "좋아하는 야구팀 있어요?",
          "original_form": "좋아하는 야구팀 있어요?",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007276.1.2",
          "form": "롯데 좋아했는데....",
          "original_form": "롯데 좋아했는데....",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007276.1.3",
          "form": "좋아했었는데...ㅜㅜㅜㅜ",
          "original_form": "좋아했었는데...ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007276.1.4",
          "form": "어느팀 응원하세요?",
          "original_form": "어느팀 응원하세요?",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007276.1.5",
          "form": "저는 기아 응원했었어요ㅎㅎ...",
          "original_form": "저는 기아 응원했었어요ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007276.1.6",
          "form": "지금은 사실 야구에 대해서 그리 열정적이진 못하네요",
          "original_form": "지금은 사실 야구에 대해서 그리 열정적이진 못하네요",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007276.1.7",
          "form": "요즘 기아는 잘 하지 않나요!",
          "original_form": "요즘 기아는 잘 하지 않나요!",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007276.1.8",
          "form": "저도... 다시 암흑기 온듯보여서",
          "original_form": "저도... 다시 암흑기 온듯보여서",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007276.1.9",
          "form": "최근 올림픽에서도 말 많고....ㅜ",
          "original_form": "최근 올림픽에서도 말 많고....ㅜ",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007276.1.10",
          "form": "네 팀 성적은 괜찮은 것 같은데 약간 흥미와 관심이 사라졌달까",
          "original_form": "네 팀 성적은 괜찮은 것 같은데 약간 흥미와 관심이 사라졌달까",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007276.1.11",
          "form": "네 맞아요 올림픽",
          "original_form": "네 맞아요 올림픽",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007276.1.12",
          "form": "그래도 진짜 메달은",
          "original_form": "그래도 진짜 메달은",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007276.1.13",
          "form": "딸 줄 알았는데 패자부활이 몇번이고 있었는데 그걸...",
          "original_form": "딸 줄 알았는데 패자부활이 몇번이고 있었는데 그걸...",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007276.1.14",
          "form": "땃으면 했는데!ㅜㅜㅜ",
          "original_form": "땃으면 했는데!ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007276.1.15",
          "form": "너무 아쉬워요ㅜㅜㅜㅜ",
          "original_form": "너무 아쉬워요ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007276.1.16",
          "form": "투수어깨가 너무 안타까워서ㅜㅜㅜ",
          "original_form": "투수어깨가 너무 안타까워서ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007276.1.17",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007276.1.18",
          "form": "직관 가신적도 있나요?",
          "original_form": "직관 가신적도 있나요?",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007276.1.19",
          "form": "넼ㅋㅋ 예전엔 자주 갔었는데 안간지 5년은 된거 같아요",
          "original_form": "넼ㅋㅋ 예전엔 자주 갔었는데 안간지 5년은 된거 같아요",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007276.1.20",
          "form": "전 강민호선수 이적 하면서 아예 안보기 시작한거 같아요",
          "original_form": "전 강민호선수 이적 하면서 아예 안보기 시작한거 같아요",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007276.1.21",
          "form": "ㅋㅋㅋㅋ아직도 어색해서ㅜㅜ 우리팀이 아닌게ㅜㅜㅜ",
          "original_form": "ㅋㅋㅋㅋ아직도 어색해서ㅜㅜ 우리팀이 아닌게ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007276.1.22",
          "form": "아 선수한테 정 붙이면 진짜 팀 관심도에도 직결이죠 ...ㅠㅠ",
          "original_form": "아 선수한테 정 붙이면 진짜 팀 관심도에도 직결이죠 ...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007276.1.23",
          "form": "또 뭐 최애선수는 아닌데 그렇드라구욯ㅎㅎㅎ",
          "original_form": "또 뭐 최애선수는 아닌데 그렇드라구욯ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007276.1.24",
          "form": "삼성으로 가면서 진짜 약간 섭섭하셨겠어요",
          "original_form": "삼성으로 가면서 진짜 약간 섭섭하셨겠어요",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007276.1.25",
          "form": "직관 자주 가시는 편이셨나요?",
          "original_form": "직관 자주 가시는 편이셨나요?",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007276.1.26",
          "form": "뭐 선수 본인의 선택이겠지만",
          "original_form": "뭐 선수 본인의 선택이겠지만",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007276.1.27",
          "form": "팬의 입장으로서는...",
          "original_form": "팬의 입장으로서는...",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007276.1.28",
          "form": "저는 직관은 많이 안가봤어요",
          "original_form": "저는 직관은 많이 안가봤어요",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007276.1.29",
          "form": "그냥 집에서 가족들이랑 치킨먹으면서 보는 편?",
          "original_form": "그냥 집에서 가족들이랑 치킨먹으면서 보는 편?",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007276.1.30",
          "form": "그게 제일 편하죠!!! 집관이 제일 잘 보이고!ㅋㅋㅋ",
          "original_form": "그게 제일 편하죠!!! 집관이 제일 잘 보이고!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007276.1.31",
          "form": "야구장은 소리지르러...갔죠...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "야구장은 소리지르러...갔죠...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:17"
        }
      ]
