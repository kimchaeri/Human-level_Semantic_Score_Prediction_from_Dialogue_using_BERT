      "utterance": [
        {
          "id": "MDRW2100007459.1.1",
          "form": "맞아요 당류 제한해서 먹는게",
          "original_form": "맞아요 당류 제한해서 먹는게",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007459.1.2",
          "form": "유튜브 컨텐츠가 유행하면서 기괴한게 너무 많이 나와서",
          "original_form": "유튜브 컨텐츠가 유행하면서 기괴한게 너무 많이 나와서",
          "speaker_id": "1",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007459.1.3",
          "form": "다이어터들도 그렇게 먹는데",
          "original_form": "다이어터들도 그렇게 먹는데",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007459.1.4",
          "form": "당뇨걸린 사람들도 그렇게 먹더라고요",
          "original_form": "당뇨걸린 사람들도 그렇게 먹더라고요",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007459.1.5",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007459.1.6",
          "form": "꿀젤리 들어보셨나요",
          "original_form": "꿀젤리 들어보셨나요",
          "speaker_id": "1",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007459.1.7",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007459.1.8",
          "form": "이거 처음 보고 너무 경악스러웠던..",
          "original_form": "이거 처음 보고 너무 경악스러웠던..",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007459.1.9",
          "form": "그거완전",
          "original_form": "그거완전",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007459.1.10",
          "form": "당뇨 유발",
          "original_form": "당뇨 유발",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007459.1.11",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007459.1.12",
          "form": "어우..전 보기만 해도 당쩔되는 느낌이라",
          "original_form": "어우..전 보기만 해도 당쩔되는 느낌이라",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007459.1.13",
          "form": "어우 아뇨",
          "original_form": "어우 아뇨",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007459.1.14",
          "form": "약간 토속적인 입맛(?) 이시라",
          "original_form": "약간 토속적인 입맛(?) 이시라",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007459.1.15",
          "form": "먼가 그런거 안좋아하실것같긴합니다",
          "original_form": "먼가 그런거 안좋아하실것같긴합니다",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007459.1.16",
          "form": "그쵸 아무래도 그거 먹음 단명할거 같더라구요",
          "original_form": "그쵸 아무래도 그거 먹음 단명할거 같더라구요",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007459.1.17",
          "form": "당류는 진짜 제한하긴 해야해요ㄱ",
          "original_form": "당류는 진짜 제한하긴 해야해요ㄱ",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007459.1.18",
          "form": "그래서 요즘 설탕대신 스테비아 넣고 그러더라고요",
          "original_form": "그래서 요즘 설탕대신 스테비아 넣고 그러더라고요",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007459.1.19",
          "form": "스테비아 대체품 좋다더라구요",
          "original_form": "스테비아 대체품 좋다더라구요",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007459.1.20",
          "form": "카페도 프로틴카페 이런게 생기는 추세래요",
          "original_form": "카페도 프로틴카페 이런게 생기는 추세래요",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007459.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007459.1.22",
          "form": "알룰로스도 좋아요",
          "original_form": "알룰로스도 좋아요",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007459.1.23",
          "form": "오 신기하다 프로틴이면 단백질?",
          "original_form": "오 신기하다 프로틴이면 단백질?",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007459.1.24",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007459.1.25",
          "form": "바닐라라떼대신",
          "original_form": "바닐라라떼대신",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007459.1.26",
          "form": "프로틴 라떼",
          "original_form": "프로틴 라떼",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007459.1.27",
          "form": "오 신기해",
          "original_form": "오 신기해",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007459.1.28",
          "form": "이런거 팔더라고요",
