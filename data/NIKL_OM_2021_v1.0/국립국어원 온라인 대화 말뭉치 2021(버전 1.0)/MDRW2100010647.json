      "utterance": [
        {
          "id": "MDRW2100010647.1.1",
          "form": "여름 끝나기 전에 빙수 한 번 먹고 싶다",
          "original_form": "여름 끝나기 전에 빙수 한 번 먹고 싶다",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010647.1.2",
          "form": "먹으면 되지 ㅎㅎ",
          "original_form": "먹으면 되지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010647.1.3",
          "form": "작은 아들 좋아하는 설빙으로다가",
          "original_form": "작은 아들 좋아하는 설빙으로다가",
          "speaker_id": "2",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010647.1.4",
          "form": "ㅎㅎ 좋지 저번처럼 오레오 든 설빙?",
          "original_form": "ㅎㅎ 좋지 저번처럼 오레오 든 설빙?",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010647.1.5",
          "form": "ㅇㅇ 쿠키앤크림처럼 바삭하고 맛있던데",
          "original_form": "ㅇㅇ 쿠키앤크림처럼 바삭하고 맛있던데",
          "speaker_id": "2",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010647.1.6",
          "form": "난 호떡이랑 붕어빵이 맛있더라",
          "original_form": "난 호떡이랑 붕어빵이 맛있더라",
          "speaker_id": "1",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010647.1.7",
          "form": "ㅎㅎ 맞아 설빙은 사이드도 맛있는 것 같아",
          "original_form": "ㅎㅎ 맞아 설빙은 사이드도 맛있는 것 같아",
          "speaker_id": "2",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010647.1.8",
          "form": "사이드 맛나제 ㅋㅋ",
          "original_form": "사이드 맛나제 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:18"
        },
        {
          "id": "MDRW2100010647.1.9",
          "form": "ㅇㅇ 시킬 때 물어봐~ 매번 입맛이 달라져서 ㅎ",
          "original_form": "ㅇㅇ 시킬 때 물어봐~ 매번 입맛이 달라져서 ㅎ",
          "speaker_id": "1",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010647.1.10",
          "form": "인절미 설빙도 맛있는데 어떤 걸로 할지 아들들한테 물어봐야겠다",
          "original_form": "인절미 설빙도 맛있는데 어떤 걸로 할지 아들들한테 물어봐야겠다",
          "speaker_id": "2",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010647.1.11",
          "form": "그랴요 난 언제 시켜도 오케이야 ㅎㅎ",
          "original_form": "그랴요 난 언제 시켜도 오케이야 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:20"
        },
        {
          "id": "MDRW2100010647.1.12",
          "form": "웅 ㅎㅎ",
          "original_form": "웅 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100010647.1.13",
          "form": "팥은 꼭 추가해주고",
          "original_form": "팥은 꼭 추가해주고",
          "speaker_id": "1",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100010647.1.14",
          "form": "알쏘용",
          "original_form": "알쏘용",
          "speaker_id": "2",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100010647.1.15",
          "form": "이번 주는 냉장고 좀 털어 먹어야겠어",
          "original_form": "이번 주는 냉장고 좀 털어 먹어야겠어",
