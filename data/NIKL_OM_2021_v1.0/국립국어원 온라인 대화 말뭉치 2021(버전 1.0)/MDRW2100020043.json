      "utterance": [
        {
          "id": "MDRW2100020043.1.1",
          "form": "피부관리는 하시나요?ㅎㅎ",
          "original_form": "피부관리는 하시나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020043.1.2",
          "form": "피부는 좀 제가 무신경한 편이라서요",
          "original_form": "피부는 좀 제가 무신경한 편이라서요",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020043.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020043.1.4",
          "form": "전 관심많죠.ㅋㅋ",
          "original_form": "전 관심많죠.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020043.1.5",
          "form": "선크림은 바르시나요?",
          "original_form": "선크림은 바르시나요?",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020043.1.6",
          "form": "아니요 ㅋ썬크림은 여름에 자닷가 놀러갈때면 조근씩 발라요",
          "original_form": "아니요 ㅋ썬크림은 여름에 자닷가 놀러갈때면 조근씩 발라요",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020043.1.7",
          "form": "아니요 ㅎ 잘안바라는데요",
          "original_form": "아니요 ㅎ 잘안바라는데요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020043.1.8",
          "form": "선크림.꼭 바르셔야해요.ㅠ",
          "original_form": "선크림.꼭 바르셔야해요.ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020043.1.9",
          "form": "피부가 늙어요ㅠ",
          "original_form": "피부가 늙어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020043.1.10",
          "form": "그건 아는데 몇시간마다 바르는게 어려워서요 ㅎ",
          "original_form": "그건 아는데 몇시간마다 바르는게 어려워서요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020043.1.11",
          "form": "전 아침에만 바르는데 그거라도 하려구요.ㅋㅋ",
          "original_form": "전 아침에만 바르는데 그거라도 하려구요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020043.1.12",
          "form": "넵 ㅎㅎㅎ 보통 남자들은 그런거 귀찬아 하죠 ㅎㅎㅎ",
          "original_form": "넵 ㅎㅎㅎ 보통 남자들은 그런거 귀찬아 하죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020043.1.13",
          "form": "저는 땀도 많아서요 ㅜㅡ",
          "original_form": "저는 땀도 많아서요 ㅜㅡ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020043.1.14",
          "form": "얼굴이 까맣게 되지 않나요?",
          "original_form": "얼굴이 까맣게 되지 않나요?",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020043.1.15",
          "form": "ㅎㅎㅎ 얼굴은 원래 까매서요 ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ 얼굴은 원래 까매서요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020043.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020043.1.17",
          "form": "까매서 안타시겠네요.ㅋㅋㅋㅋ",
          "original_form": "까매서 안타시겠네요.ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020043.1.18",
          "form": "전 피부가 흰 편인데 햇볕을 받으면 너무 빨개져요",
          "original_form": "전 피부가 흰 편인데 햇볕을 받으면 너무 빨개져요",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020043.1.19",
          "form": "ㅋㅋㅋㅋ 넵",
          "original_form": "ㅋㅋㅋㅋ 넵",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020043.1.20",
          "form": "그런데 전 하얀 피부가 부러워요 ㅜㅡ",
          "original_form": "그런데 전 하얀 피부가 부러워요 ㅜㅡ",
