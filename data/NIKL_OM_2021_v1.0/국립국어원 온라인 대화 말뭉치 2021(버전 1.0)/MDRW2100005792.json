      "utterance": [
        {
          "id": "MDRW2100005792.1.1",
          "form": "좋아했죠!! ㅋㅋㅋㅋ책자체를 잘안읽다보니 요새는 ㅠㅠ",
          "original_form": "좋아했죠!! ㅋㅋㅋㅋ책자체를 잘안읽다보니 요새는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100005792.1.2",
          "form": "읽으셨다는 얇은책들이 소설책이신가용?",
          "original_form": "읽으셨다는 얇은책들이 소설책이신가용?",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100005792.1.3",
          "form": "간단한 인문학도서도 읽긴한데",
          "original_form": "간단한 인문학도서도 읽긴한데",
          "speaker_id": "1",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100005792.1.4",
          "form": "대부분 소설책이죠ㅎㅎ",
          "original_form": "대부분 소설책이죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100005792.1.5",
          "form": "저는 추리소설 개좋아해서 허버버읽어요",
          "original_form": "저는 추리소설 개좋아해서 허버버읽어요",
          "speaker_id": "1",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100005792.1.6",
          "form": "헉 추리 넘 좋죠",
          "original_form": "헉 추리 넘 좋죠",
          "speaker_id": "2",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100005792.1.7",
          "form": "저도 히가시노게이고는 종종읽어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 히가시노게이고는 종종읽어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100005792.1.8",
          "form": "ㅋㅋㄱㅋㄱ히가시노 게이고소설 너무 많은것같아요",
          "original_form": "ㅋㅋㄱㅋㄱ히가시노 게이고소설 너무 많은것같아요",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100005792.1.9",
          "form": "진짜 팀단위 작가그룹인지",
          "original_form": "진짜 팀단위 작가그룹인지",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100005792.1.10",
          "form": "의심될지경",
          "original_form": "의심될지경",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100005792.1.11",
          "form": "저정도면 Ai 아니냐고요 ....",
          "original_form": "저정도면 Ai 아니냐고요 ....",
          "speaker_id": "2",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100005792.1.12",
          "form": "그쵸ㅋㄱㄱㅋㅋㅋ",
          "original_form": "그쵸ㅋㄱㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100005792.1.13",
          "form": "아니 왜분명",
          "original_form": "아니 왜분명",
          "speaker_id": "1",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100005792.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 사건이랑 키워드 몇개 넣으면 소설 한편 만들어주는 그런",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 사건이랑 키워드 몇개 넣으면 소설 한편 만들어주는 그런",
          "speaker_id": "2",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100005792.1.15",
          "form": "저번달신작에 잇엇는데",
          "original_form": "저번달신작에 잇엇는데",
          "speaker_id": "1",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100005792.1.16",
          "form": "이번달에도 잇는지.,",
          "original_form": "이번달에도 잇는지.,",
          "speaker_id": "1",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100005792.1.17",
          "form": "ㄱㄱㄱㅋㄱㅋ",
          "original_form": "ㄱㄱㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100005792.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 독자입장에선 그냥 감사합니다...지만요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 독자입장에선 그냥 감사합니다...지만요",
          "speaker_id": "2",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100005792.1.19",
          "form": "회사일도 저렇게 열심히안하는데 창작을 어떻게 저렇게 열심히하시지",
          "original_form": "회사일도 저렇게 열심히안하는데 창작을 어떻게 저렇게 열심히하시지",
          "speaker_id": "2",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100005792.1.20",
          "form": "혹시 추천하시는 책이 있나요?! 오늘 도서관가서 한번 볼게요 ㅋㅋㅋ",
          "original_form": "혹시 추천하시는 책이 있나요?! 오늘 도서관가서 한번 볼게요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100005792.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㄱㅋ아ㅠ공감합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㄱㅋ아ㅠ공감합니다",
          "speaker_id": "1",
          "time": "20210907 12:30"
        },
        {
          "id": "MDRW2100005792.1.22",
          "form": "저 최근에 읽은책이 과학이라는 헛소리라는",
          "original_form": "저 최근에 읽은책이 과학이라는 헛소리라는",
          "speaker_id": "1",
          "time": "20210907 12:30"
        },
        {
          "id": "MDRW2100005792.1.23",
          "form": "유사과학까는 인문학책인데",
          "original_form": "유사과학까는 인문학책인데",
          "speaker_id": "1",
          "time": "20210907 12:30"
        },
        {
          "id": "MDRW2100005792.1.24",
          "form": "과학주제인데 쉽고 재밌게 설명해줘서 추천합니다",
          "original_form": "과학주제인데 쉽고 재밌게 설명해줘서 추천합니다",
          "speaker_id": "1",
          "time": "20210907 12:31"
        },
        {
          "id": "MDRW2100005792.1.25",
          "form": "ㄹㅇ게르마늄팔찌 까는부분 재밌으니 추천!!",
          "original_form": "ㄹㅇ게르마늄팔찌 까는부분 재밌으니 추천!!",
          "speaker_id": "1",
          "time": "20210907 12:31"
        },
        {
          "id": "MDRW2100005792.1.26",
          "form": "게다가 책도 얇아요",
          "original_form": "게다가 책도 얇아요",
          "speaker_id": "1",
          "time": "20210907 12:31"
        },
        {
          "id": "MDRW2100005792.1.27",
          "form": "헐 그런거 좋아해요 저 감사합니다",
          "original_form": "헐 그런거 좋아해요 저 감사합니다",
          "speaker_id": "2",
          "time": "20210907 12:33"
        },
        {
