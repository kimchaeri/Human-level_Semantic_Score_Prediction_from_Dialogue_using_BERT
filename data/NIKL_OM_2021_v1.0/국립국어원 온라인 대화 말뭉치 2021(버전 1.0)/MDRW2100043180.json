          "speaker_id": "1",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100043180.1.4",
          "form": "name1님은요?!?!",
          "original_form": "&name1&님은요?!?!",
          "speaker_id": "2",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043180.1.5",
          "form": "저는설거지정도요?ㅎㅎ",
          "original_form": "저는설거지정도요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043180.1.6",
          "form": "ㅋㅋㅋㅋㅋ부모님이랑 같이사세요?",
          "original_form": "ㅋㅋㅋㅋㅋ부모님이랑 같이사세요?",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043180.1.7",
          "form": "헿 맞아요",
          "original_form": "헿 맞아요",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043180.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ자취해보신적 있으세요?!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ자취해보신적 있으세요?!",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043180.1.9",
          "form": "저는 집에서사는거보다 자취가 잘 맞는거같아요",
          "original_form": "저는 집에서사는거보다 자취가 잘 맞는거같아요",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043180.1.10",
          "form": "저없어요!",
          "original_form": "저없어요!",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043180.1.11",
          "form": "대학도통학했어요",
          "original_form": "대학도통학했어요",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043180.1.12",
          "form": "자취의장점이뭔가요",
          "original_form": "자취의장점이뭔가요",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043180.1.13",
          "form": "자유 ....",
          "original_form": "자유 ....",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043180.1.14",
          "form": "이거하나면 충분.....",
          "original_form": "이거하나면 충분.....",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043180.1.15",
          "form": "전 대학도자취해가지곸ㅋㅋㅋㅋㅋ 졸업하고 본가들어왓다가 다시 나온케이스에요",
          "original_form": "전 대학도자취해가지곸ㅋㅋㅋㅋㅋ 졸업하고 본가들어왓다가 다시 나온케이스에요",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100043180.1.16",
          "form": "앜ㅋㄲㅋㅋㅋ",
          "original_form": "앜ㅋㄲㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:59"
        },
        {
          "id": "MDRW2100043180.1.17",
          "form": "뭔가그려지네요",
          "original_form": "뭔가그려지네요",
          "speaker_id": "1",
          "time": "20210907 15:59"
        },
        {
          "id": "MDRW2100043180.1.18",
          "form": "부모님도 맞벌이하시다보니 ㅋㅋㅋㅋ 집안일도 다 각자하고 빨래도 다 자기꺼 자기가하게 되더라구요",
          "original_form": "부모님도 맞벌이하시다보니 ㅋㅋㅋㅋ 집안일도 다 각자하고 빨래도 다 자기꺼 자기가하게 되더라구요",
          "speaker_id": "2",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100043180.1.19",
