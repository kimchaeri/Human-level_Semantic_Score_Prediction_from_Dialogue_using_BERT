      "utterance": [
        {
          "id": "MDRW2100040522.1.1",
          "form": "저는 그냥 집밥먹었어요!",
          "original_form": "저는 그냥 집밥먹었어요!",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100040522.1.2",
          "form": "뭐 드셨나요??",
          "original_form": "뭐 드셨나요??",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100040522.1.3",
          "form": "삼겹살구워먹었습니다!",
          "original_form": "삼겹살구워먹었습니다!",
          "speaker_id": "1",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100040522.1.4",
          "form": "와 맛있었겠아요",
          "original_form": "와 맛있었겠아요",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100040522.1.5",
          "form": "삼겹살은 뭐에드셔시나요?",
          "original_form": "삼겹살은 뭐에드셔시나요?",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100040522.1.6",
          "form": "전 깻잎이나 쌈무를 좋아해요 ㅎㅎ",
          "original_form": "전 깻잎이나 쌈무를 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100040522.1.7",
          "form": "상추에 싸먹었습니다!",
          "original_form": "상추에 싸먹었습니다!",
          "speaker_id": "1",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100040522.1.8",
          "form": "저도 좋아해요 ㅜㅜ",
          "original_form": "저도 좋아해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100040522.1.9",
          "form": "단지 없었을뿐...",
          "original_form": "단지 없었을뿐...",
          "speaker_id": "1",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100040522.1.10",
          "form": "앗 그러시군요",
          "original_form": "앗 그러시군요",
          "speaker_id": "2",
          "time": "20210905 10:18"
        },
        {
          "id": "MDRW2100040522.1.11",
          "form": "저도 상추도 좋아해요 ㅎㅎ",
          "original_form": "저도 상추도 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:18"
        },
        {
          "id": "MDRW2100040522.1.12",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "2",
          "time": "20210905 10:18"
        },
        {
          "id": "MDRW2100040522.1.13",
          "form": "아니요 ㅋㅋㅋ 아직누워있습니다 ㅋㅋㅋ 점심을 좀 이르게먹으려구요 ㅋㅋ",
          "original_form": "아니요 ㅋㅋㅋ 아직누워있습니다 ㅋㅋㅋ 점심을 좀 이르게먹으려구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:19"
        },
        {
          "id": "MDRW2100040522.1.14",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100040522.1.15",
          "form": "뭐 드실계획이신가요?",
          "original_form": "뭐 드실계획이신가요?",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100040522.1.16",
          "form": "전 아침으로 유부초밥을 먹었답니다 ㅎㅎ",
          "original_form": "전 아침으로 유부초밥을 먹었답니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100040522.1.17",
          "form": "저에게 선택권이 없어서 주는대로먹습니다 ㅋㅋㅋㅋ유부초밥맛있겠네요 ㅜㅜ",
          "original_form": "저에게 선택권이 없어서 주는대로먹습니다 ㅋㅋㅋㅋ유부초밥맛있겠네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100040522.1.18",
          "form": "그렇군요 ㅋㅋㅋ",
          "original_form": "그렇군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100040522.1.19",
          "form": "네 맛있었어요 오랜만어. 먹으니 좋더라구요",
          "original_form": "네 맛있었어요 오랜만어. 먹으니 좋더라구요",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100040522.1.20",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100040522.1.21",
          "form": "토스트두너무 맛있죠 전 이삭토스트처럼 안에 풍족한걸 좋아해요",
          "original_form": "토스트두너무 맛있죠 전 이삭토스트처럼 안에 풍족한걸 좋아해요",
          "speaker_id": "2",
          "time": "20210905 10:27"
        },
        {
          "id": "MDRW2100040522.1.22",
          "form": "헐 이삭 ㅜㅜ 저희동네네 없어져서 아쉬운데 ㅜㅜ 그맛내기어려워요 ㅜㅜ",
          "original_form": "헐 이삭 ㅜㅜ 저희동네네 없어져서 아쉬운데 ㅜㅜ 그맛내기어려워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100040522.1.23",
          "form": "헉 너무 아쉽네요 ㅠ",
          "original_form": "헉 너무 아쉽네요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 10:33"
        },
        {
          "id": "MDRW2100040522.1.24",
          "form": "이삭만의 맛이 있잖아요",
          "original_form": "이삭만의 맛이 있잖아요",
          "speaker_id": "2",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100040522.1.25",
          "form": "진짜 맛있고 저렴해서 좋아요!!",
          "original_form": "진짜 맛있고 저렴해서 좋아요!!",
          "speaker_id": "2",
          "time": "20210905 10:34"
        },
        {
          "id": "MDRW2100040522.1.26",
          "form": "진짜요 ㅜㅜ 너무아쉽습니다 섭웨라도 있으면좋을텐데 섭웨도없네요...",
          "original_form": "진짜요 ㅜㅜ 너무아쉽습니다 섭웨라도 있으면좋을텐데 섭웨도없네요...",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100040522.1.27",
          "form": "앗 섭웨정말좋아해요 ㅠㅠㅠ",
          "original_form": "앗 섭웨정말좋아해요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100040522.1.28",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100040522.1.29",
          "form": "저 사는곳에도 없어요...",
          "original_form": "저 사는곳에도 없어요...",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100040522.1.30",
          "form": "갑자기먹고싶네요",
          "original_form": "갑자기먹고싶네요",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100040522.1.31",
          "form": "전 항상 올리브랑 야채추가 많이해요",
          "original_form": "전 항상 올리브랑 야채추가 많이해요",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100040522.1.32",
          "form": "어떻게드시나요?",
          "original_form": "어떻게드시나요?",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100040522.1.33",
          "form": "저도올리브 많이요!!! 빵은 늘 위트로 먹습니다 ㅋㅋㅋㅋ",
          "original_form": "저도올리브 많이요!!! 빵은 늘 위트로 먹습니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100040522.1.34",
          "form": "ㅎㅎㅎ 저는 플랫브레드요!!",
          "original_form": "ㅎㅎㅎ 저는 플랫브레드요!!",
          "speaker_id": "2",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100040522.1.35",
          "form": "쫄깃한게 너무 맛있어요 ㅎㅎ",
          "original_form": "쫄깃한게 너무 맛있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100040522.1.36",
          "form": "쿠키도 좋아하시나요?",
          "original_form": "쿠키도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 12:10"
        }
      ]
    }
