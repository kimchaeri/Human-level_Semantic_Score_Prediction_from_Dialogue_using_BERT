      "utterance": [
        {
          "id": "MDRW2100041207.1.1",
          "form": "저녁은 맛있게 드셨나요?",
          "original_form": "저녁은 맛있게 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 20:13"
        },
        {
          "id": "MDRW2100041207.1.2",
          "form": "떡볶이에 만두먹었어요",
          "original_form": "떡볶이에 만두먹었어요",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041207.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041207.1.4",
          "form": "오 야식 느낌",
          "original_form": "오 야식 느낌",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041207.1.5",
          "form": "전 갈치구이 해서 식구들과 맛있게 먹었답니다~",
          "original_form": "전 갈치구이 해서 식구들과 맛있게 먹었답니다~",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041207.1.6",
          "form": "오~갈치구이 맛있겠네요",
          "original_form": "오~갈치구이 맛있겠네요",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100041207.1.7",
          "form": "몇안되는 먹을 수 있는 생선이네요",
          "original_form": "몇안되는 먹을 수 있는 생선이네요",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100041207.1.8",
          "form": "생선은 잘 안먹나요? 아니면 8체질 식이요법?",
          "original_form": "생선은 잘 안먹나요? 아니면 8체질 식이요법?",
          "speaker_id": "1",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100041207.1.9",
          "form": "8체질? 그런건 아니구요",
          "original_form": "8체질? 그런건 아니구요",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041207.1.10",
          "form": "별로 즐겨먹지는 않아요",
          "original_form": "별로 즐겨먹지는 않아요",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041207.1.11",
          "form": "아 식성이군요",
          "original_form": "아 식성이군요",
          "speaker_id": "1",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100041207.1.12",
          "form": "한방 8체질 보면 뭐 먹어라 뭐 먹지마라 그런 게 있어서요",
          "original_form": "한방 8체질 보면 뭐 먹어라 뭐 먹지마라 그런 게 있어서요",
          "speaker_id": "1",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100041207.1.13",
          "form": "가시바르는 것도 귀찮고 비리기도 하구요",
          "original_form": "가시바르는 것도 귀찮고 비리기도 하구요",
          "speaker_id": "2",
          "time": "20210905 23:28"
        },
        {
          "id": "MDRW2100041207.1.14",
          "form": "비린내가 나면 먹기 힘들죠",
          "original_form": "비린내가 나면 먹기 힘들죠",
          "speaker_id": "1",
          "time": "20210906 06:55"
        },
        {
          "id": "MDRW2100041207.1.15",
          "form": "사람마다 감각이 달라서요",
          "original_form": "사람마다 감각이 달라서요",
          "speaker_id": "1",
          "time": "20210906 06:55"
        },
        {
          "id": "MDRW2100041207.1.16",
          "form": "육고기도 다른 사람들은 괜찮은데 어떤 사람은 고기비린내를 잘 맡아서 못먹기도 하더라구요",
          "original_form": "육고기도 다른 사람들은 괜찮은데 어떤 사람은 고기비린내를 잘 맡아서 못먹기도 하더라구요",
          "speaker_id": "1",
          "time": "20210906 06:56"
        },
        {
          "id": "MDRW2100041207.1.17",
          "form": "은근 있더라구요",
          "original_form": "은근 있더라구요",
          "speaker_id": "2",
          "time": "20210906 08:09"
        },
        {
          "id": "MDRW2100041207.1.18",
          "form": "물에 빠진 고긴 안먹는 사람도",
          "original_form": "물에 빠진 고긴 안먹는 사람도",
          "speaker_id": "2",
          "time": "20210906 08:09"
        },
        {
          "id": "MDRW2100041207.1.19",
          "form": "맞아요.. 처음엔 저게 무슨 말인가 했어요",
          "original_form": "맞아요.. 처음엔 저게 무슨 말인가 했어요",
          "speaker_id": "1",
          "time": "20210906 08:35"
        },
        {
          "id": "MDRW2100041207.1.20",
          "form": "누가 물에 빠진 고기 안먹는다고 해서 물고기 말인가? 이랬죠",
          "original_form": "누가 물에 빠진 고기 안먹는다고 해서 물고기 말인가? 이랬죠",
          "speaker_id": "1",
          "time": "20210906 08:35"
        },
        {
          "id": "MDRW2100041207.1.21",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210906 11:11"
        },
        {
          "id": "MDRW2100041207.1.22",
          "form": "표현이 너무 우껴요",
          "original_form": "표현이 너무 우껴요",
          "speaker_id": "2",
          "time": "20210906 11:11"
        },
        {
          "id": "MDRW2100041207.1.23",
          "form": "그런 사람이 꽤 되니까 그런 표현도 나왔겠죠",
          "original_form": "그런 사람이 꽤 되니까 그런 표현도 나왔겠죠",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100041207.1.24",
          "form": "커피 좋아하세요?^^",
          "original_form": "커피 좋아하세요?^^",
          "speaker_id": "1",
          "time": "20210906 14:45"
        }
      ]
    }
