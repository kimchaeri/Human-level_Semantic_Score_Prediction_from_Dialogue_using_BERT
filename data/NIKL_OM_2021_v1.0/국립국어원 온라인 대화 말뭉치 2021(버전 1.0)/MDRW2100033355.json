        },
        {
          "id": "MDRW2100033355.1.2",
          "form": "제가 좋아하는 영상입니다",
          "original_form": "제가 좋아하는 영상입니다",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100033355.1.3",
          "form": "우와 링크도 주고받을 수 있군요",
          "original_form": "우와 링크도 주고받을 수 있군요",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033355.1.4",
          "form": "정말 세상이 많이 좋아졌습니다 ^^",
          "original_form": "정말 세상이 많이 좋아졌습니다 ^^",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033355.1.5",
          "form": "남학생말고 우리학생 리액션이 좋은것이지만,,,",
          "original_form": "남학생말고 우리학생 리액션이 좋은것이지만,,,",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033355.1.6",
          "form": "세상이 급격히 변하네요,,.",
          "original_form": "세상이 급격히 변하네요,,.",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033355.1.7",
          "form": "아주 ... 놀랍습니다...",
          "original_form": "아주 ... 놀랍습니다...",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033355.1.8",
          "form": "저도 본받겠습니다 아버님 배움의 열정",
          "original_form": "저도 본받겠습니다 아버님 배움의 열정",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033355.1.9",
          "form": "저는 임영웅 이런거 안듣습니다",
          "original_form": "저는 임영웅 이런거 안듣습니다",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033355.1.10",
          "form": "ㅋㄱㄱㅋㄱㄱㅋㄱㅋㄱㅋㅋㄱㅋㅋㅋㄱㄱㅋㄱㅋㅋㄱㅋ",
          "original_form": "ㅋㄱㄱㅋㄱㄱㅋㄱㅋㄱㅋㅋㄱㅋㅋㅋㄱㄱㅋㄱㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100033355.1.11",
          "form": "아임,,히어로,,.",
          "original_form": "아임,,히어로,,.",
          "speaker_id": "2",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033355.1.12",
          "form": "노래는 송 가인이가 최고지요",
          "original_form": "노래는 송 가인이가 최고지요",
          "speaker_id": "2",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033355.1.13",
          "form": "송가인이는",
          "original_form": "송가인이는",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033355.1.14",
          "form": "복스러워서 아주 좋습니다",
          "original_form": "복스러워서 아주 좋습니다",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033355.1.15",
          "form": "하지만 물론",
          "original_form": "하지만 물론",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033355.1.16",
          "form": "황소윤이가 최고입니다",
          "original_form": "황소윤이가 최고입니다",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033355.1.17",
          "form": "그리고 뭣이다냐...",
          "original_form": "그리고 뭣이다냐...",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033355.1.18",
          "form": "사랑합니다 황소윤이,,,",
          "original_form": "사랑합니다 황소윤이,,,",
          "speaker_id": "2",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033355.1.19",
          "form": "저도 사랑합니다....",
          "original_form": "저도 사랑합니다....",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033355.1.20",
          "form": "황소윤언니언니",
          "original_form": "황소윤언니언니",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033355.1.21",
          "form": "또누가 심금을 울렸을까요",
          "original_form": "또누가 심금을 울렸을까요",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033355.1.22",
          "form": "어디보자",
          "original_form": "어디보자",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033355.1.23",
          "form": "우리 딸아이 플레이리스트를",
          "original_form": "우리 딸아이 플레이리스트를",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033355.1.24",
          "form": "훔쳐보겠읍니다",
          "original_form": "훔쳐보겠읍니다",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033355.1.25",
          "form": "제가 또 팝-송을 듣는데요",
          "original_form": "제가 또 팝-송을 듣는데요",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033355.1.26",
          "form": "혹시...",
          "original_form": "혹시...",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033355.1.27",
          "form": "Baby powder를 아시나요?",
          "original_form": "Baby powder를 아시나요?",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100033355.1.28",
          "form": "ㅋㄱㄱㄱㄱㅋㅋㄱㅋㅋㅋㄲㅋ노력파시네요ㅠ 역시",
          "original_form": "ㅋㄱㄱㄱㄱㅋㅋㄱㅋㅋㅋㄲㅋ노력파시네요ㅠ 역시",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100033355.1.29",
          "form": "이정도는",
          "original_form": "이정도는",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100033355.1.30",
          "form": "배우고 익혀야합니다",
