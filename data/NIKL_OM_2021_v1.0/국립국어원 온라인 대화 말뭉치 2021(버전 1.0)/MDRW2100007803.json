      "utterance": [
        {
          "id": "MDRW2100007803.1.1",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007803.1.2",
          "form": "저는 안키우는데 부모님댁에 레브라도 리트리버 한마리 키우고 있어요",
          "original_form": "저는 안키우는데 부모님댁에 레브라도 리트리버 한마리 키우고 있어요",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007803.1.3",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007803.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007803.1.5",
          "form": "키우고는 싶다가 또 아니다가 그래요",
          "original_form": "키우고는 싶다가 또 아니다가 그래요",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007803.1.6",
          "form": "강아지 이름은 뭔가요",
          "original_form": "강아지 이름은 뭔가요",
          "speaker_id": "1",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007803.1.7",
          "form": "name3에요",
          "original_form": "&name3&에요",
          "speaker_id": "2",
          "time": "20210822 11:04"
        },
        {
          "id": "MDRW2100007803.1.8",
          "form": "이제 3살이에요",
          "original_form": "이제 3살이에요",
          "speaker_id": "2",
          "time": "20210822 11:04"
        },
        {
          "id": "MDRW2100007803.1.9",
          "form": "동물은 좋아하세요",
          "original_form": "동물은 좋아하세요",
          "speaker_id": "2",
          "time": "20210822 11:04"
        },
        {
          "id": "MDRW2100007803.1.10",
          "form": "아직 한참 귀여울때네요",
          "original_form": "아직 한참 귀여울때네요",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007803.1.11",
          "form": "동물 좋아해요",
          "original_form": "동물 좋아해요",
          "speaker_id": "1",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007803.1.12",
          "form": "어떤 동물이요? 전 개",
          "original_form": "어떤 동물이요? 전 개",
          "speaker_id": "2",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007803.1.13",
          "form": "전 개도 고양이도 좋아요",
          "original_form": "전 개도 고양이도 좋아요",
          "speaker_id": "1",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007803.1.14",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007803.1.15",
          "form": "길러보신적은 있어요?",
          "original_form": "길러보신적은 있어요?",
          "speaker_id": "2",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007803.1.16",
          "form": "털있는 동물은 없어요ㅜ",
          "original_form": "털있는 동물은 없어요ㅜ",
          "speaker_id": "1",
          "time": "20210822 11:07"
        },
        {
          "id": "MDRW2100007803.1.17",
          "form": "?? 파충류 기르셨어요?",
          "original_form": "?? 파충류 기르셨어요?",
          "speaker_id": "2",
          "time": "20210822 11:08"
        },
        {
          "id": "MDRW2100007803.1.18",
          "form": "거북이랑 금붕어 올챙이? ㅋㅋㅋ",
          "original_form": "거북이랑 금붕어 올챙이? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007803.1.19",
          "form": "아 그렇구나 올챙이는 특이하네",
          "original_form": "아 그렇구나 올챙이는 특이하네",
          "speaker_id": "2",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007803.1.20",
          "form": "개구리 됐나요?",
          "original_form": "개구리 됐나요?",
          "speaker_id": "2",
          "time": "20210822 11:09"
        },
        {
          "id": "MDRW2100007803.1.21",
          "form": "진짜 대박인게",
          "original_form": "진짜 대박인게",
          "speaker_id": "1",
          "time": "20210822 11:11"
        },
        {
          "id": "MDRW2100007803.1.22",
          "form": "물갈아줄때 수돗물을줘서ㅜ",
          "original_form": "물갈아줄때 수돗물을줘서ㅜ",
          "speaker_id": "1",
          "time": "20210822 11:11"
        },
        {
          "id": "MDRW2100007803.1.23",
          "form": "흑ㅜㅜㅜ",
          "original_form": "흑ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 11:11"
        },
        {
          "id": "MDRW2100007803.1.24",
          "form": "아 수돗물을 주면 못사나요?",
          "original_form": "아 수돗물을 주면 못사나요?",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007803.1.25",
          "form": "흠 .. 안타깝네요",
          "original_form": "흠 .. 안타깝네요",
          "speaker_id": "2",
          "time": "20210822 12:28"
        }
      ]
