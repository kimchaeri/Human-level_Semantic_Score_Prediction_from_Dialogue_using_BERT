      "utterance": [
        {
          "id": "MDRW2100012241.1.1",
          "form": "쇼핑 좋아하시나요",
          "original_form": "쇼핑 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 10:11"
        },
        {
          "id": "MDRW2100012241.1.2",
          "form": "아이쇼핑은 자주 해요",
          "original_form": "아이쇼핑은 자주 해요",
          "speaker_id": "2",
          "time": "20210825 10:14"
        },
        {
          "id": "MDRW2100012241.1.3",
          "form": "어떤 물건하시나요?",
          "original_form": "어떤 물건하시나요?",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012241.1.4",
          "form": "책이나 전자기기 많이 보는거 같아요",
          "original_form": "책이나 전자기기 많이 보는거 같아요",
          "speaker_id": "2",
          "time": "20210825 10:19"
        },
        {
          "id": "MDRW2100012241.1.5",
          "form": "최근에 구매한 물건 있으세요?",
          "original_form": "최근에 구매한 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012241.1.6",
          "form": "신발 샀어요ㅎ",
          "original_form": "신발 샀어요ㅎ",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100012241.1.7",
          "form": "어떤 신발이요?",
          "original_form": "어떤 신발이요?",
          "speaker_id": "2",
          "time": "20210825 10:22"
        },
        {
          "id": "MDRW2100012241.1.8",
          "form": "나이키요!",
          "original_form": "나이키요!",
          "speaker_id": "1",
          "time": "20210825 10:22"
        },
        {
          "id": "MDRW2100012241.1.9",
          "form": "나이키 좋죠ㅋㅋ 옷도 많이 사시나요?",
          "original_form": "나이키 좋죠ㅋㅋ 옷도 많이 사시나요?",
          "speaker_id": "2",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100012241.1.10",
          "form": "네 옷도 좋아해요",
          "original_form": "네 옷도 좋아해요",
          "speaker_id": "1",
          "time": "20210825 11:26"
        },
        {
          "id": "MDRW2100012241.1.11",
          "form": "이제 슬슬 가을옷도 더 사야될 거 같아요",
          "original_form": "이제 슬슬 가을옷도 더 사야될 거 같아요",
          "speaker_id": "2",
          "time": "20210825 11:39"
        },
        {
          "id": "MDRW2100012241.1.12",
          "form": "아근데 금방또 겨울이라ㅠ",
          "original_form": "아근데 금방또 겨울이라ㅠ",
          "speaker_id": "1",
          "time": "20210825 11:44"
        },
        {
          "id": "MDRW2100012241.1.13",
          "form": "그렇긴 하죠. 할인하니까 사고싶어져요ㅋㅋ",
          "original_form": "그렇긴 하죠. 할인하니까 사고싶어져요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:15"
        },
        {
          "id": "MDRW2100012241.1.14",
