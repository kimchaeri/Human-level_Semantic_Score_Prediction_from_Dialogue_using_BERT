      "utterance": [
        {
          "id": "MDRW2100009389.1.1",
          "form": "최근에 베스트셀러가 뭔지도 모르겠네요 ㅋㅋ",
          "original_form": "최근에 베스트셀러가 뭔지도 모르겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009389.1.2",
          "form": "학생땐 독서가 취미였는데",
          "original_form": "학생땐 독서가 취미였는데",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009389.1.3",
          "form": "ㅋㅋ저두요",
          "original_form": "ㅋㅋ저두요",
          "speaker_id": "1",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009389.1.4",
          "form": "요샌 책 보기도 힘드네요",
          "original_form": "요샌 책 보기도 힘드네요",
          "speaker_id": "1",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009389.1.5",
          "form": "이북으로 만화는 좀봤어요",
          "original_form": "이북으로 만화는 좀봤어요",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009389.1.6",
          "form": "왜 코난도 원피스도 베스트셀러라며 ㅋㅋ",
          "original_form": "왜 코난도 원피스도 베스트셀러라며 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009389.1.7",
          "form": "오 코난은 최고죠",
          "original_form": "오 코난은 최고죠",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009389.1.8",
          "form": "전 웹툰은 즐겨봐요",
          "original_form": "전 웹툰은 즐겨봐요",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009389.1.9",
          "form": "저도 네이버 웹툰봐요",
          "original_form": "저도 네이버 웹툰봐요",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009389.1.10",
          "form": "뭐보세요?",
          "original_form": "뭐보세요?",
          "speaker_id": "2",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009389.1.11",
          "form": "요일별로요ㅎ",
          "original_form": "요일별로요ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009389.1.12",
          "form": "저는 개를 낳았다 그거 꼬박꼬박 챙겨봐요",
          "original_form": "저는 개를 낳았다 그거 꼬박꼬박 챙겨봐요",
          "speaker_id": "2",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009389.1.13",
          "form": "저도 그거는 매주!",
          "original_form": "저도 그거는 매주!",
          "speaker_id": "1",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009389.1.14",
          "form": "전 두번이나 정주행했어요 ㄱㅋ",
          "original_form": "전 두번이나 정주행했어요 ㄱㅋ",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009389.1.15",
          "form": "전 중간중간 울었어요ㅠ",
          "original_form": "전 중간중간 울었어요ㅠ",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009389.1.16",
          "form": "저도 볼때마다 눈물이고여요",
          "original_form": "저도 볼때마다 눈물이고여요",
          "speaker_id": "2",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009389.1.17",
          "form": "그거랑 노곤하개 ㅋㅋ",
          "original_form": "그거랑 노곤하개 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009389.1.18",
          "form": "그쳐 유기하는 사람들 넘 나빠여",
          "original_form": "그쳐 유기하는 사람들 넘 나빠여",
          "speaker_id": "1",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009389.1.19",
          "form": "아 수면 책 얼마전에 빌려보긴했네요 도서관에서",
          "original_form": "아 수면 책 얼마전에 빌려보긴했네요 도서관에서",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009389.1.20",
          "form": "전 까불고 경제관련빌렸다가 졸려서ㅋㅋ",
          "original_form": "전 까불고 경제관련빌렸다가 졸려서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009389.1.21",
          "form": "저는 책제목이 ㅋㅋ",
          "original_form": "저는 책제목이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009389.1.22",
          "form": "안볼수없게하더라고요",
          "original_form": "안볼수없게하더라고요",
          "speaker_id": "2",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009389.1.23",
          "form": "제목이뭐였어요?",
          "original_form": "제목이뭐였어요?",
          "speaker_id": "1",
          "time": "20210823 00:00"
        },
        {
          "id": "MDRW2100009389.1.24",
          "form": "하루 다섯시간만자도 충분합니다 였나",
          "original_form": "하루 다섯시간만자도 충분합니다 였나",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009389.1.25",
          "form": "뭐 이런식의 문장이요 ㅋㅋ",
          "original_form": "뭐 이런식의 문장이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009389.1.26",
          "form": "ㅋㅋ부족해요",
          "original_form": "ㅋㅋ부족해요",
          "speaker_id": "1",
          "time": "20210823 09:05"
        },
        {
          "id": "MDRW2100009389.1.27",
          "form": "제목이 혹했어요",
          "original_form": "제목이 혹했어요",
          "speaker_id": "2",
          "time": "20210823 09:46"
        },
        {
          "id": "MDRW2100009389.1.28",
          "form": "일본인이 쓴 책이라 그런가",
          "original_form": "일본인이 쓴 책이라 그런가",
          "speaker_id": "2",
          "time": "20210823 09:47"
        },
        {
          "id": "MDRW2100009389.1.29",
          "form": "제목이 길었다는 ㅋㅋ",
          "original_form": "제목이 길었다는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:47"
        },
        {
          "id": "MDRW2100009389.1.30",
          "form": "일본식 제목이 좀 그런것들이 있잖아요",
          "original_form": "일본식 제목이 좀 그런것들이 있잖아요",
          "speaker_id": "2",
          "time": "20210823 09:47"
        },
        {
          "id": "MDRW2100009389.1.31",
          "form": "너의 췌장을 먹고 싶어라던가..",
          "original_form": "너의 췌장을 먹고 싶어라던가..",
          "speaker_id": "2",
          "time": "20210823 09:47"
        },
        {
          "id": "MDRW2100009389.1.32",
          "form": "아ㅋㅋ도움이 되셧나요?",
          "original_form": "아ㅋㅋ도움이 되셧나요?",
          "speaker_id": "1",
          "time": "20210823 10:34"
        },
        {
          "id": "MDRW2100009389.1.33",
          "form": "저도 자기계발서같은거 몇번봣어요ㅎ",
          "original_form": "저도 자기계발서같은거 몇번봣어요ㅎ",
          "speaker_id": "1",
          "time": "20210823 10:34"
        },
        {
          "id": "MDRW2100009389.1.34",
          "form": "저는 다이어트 책 ㅋㅋ",
          "original_form": "저는 다이어트 책 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:54"
        }
      ]
