      "utterance": [
        {
          "id": "MDRW2100029821.1.1",
          "form": "간식으로명랑핫도그먹었머요",
          "original_form": "간식으로명랑핫도그먹었머요",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029821.1.2",
          "form": "명랑 맛있죠 ㅎㅎ",
          "original_form": "명랑 맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100029821.1.3",
          "form": "전 맵구마? 맛나더라구요",
          "original_form": "전 맵구마? 맛나더라구요",
          "speaker_id": "2",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100029821.1.4",
          "form": "오늘 저녁에 머먹을껀가요?",
          "original_form": "오늘 저녁에 머먹을껀가요?",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100029821.1.5",
          "form": "저는 치킨먹을것같아요",
          "original_form": "저는 치킨먹을것같아요",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100029821.1.6",
          "form": "오늘 저녁은 패스..!!",
          "original_form": "오늘 저녁은 패스..!!",
          "speaker_id": "2",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100029821.1.7",
          "form": "닭갈비가 너무너무너무 너무 너무 먹고싶어요ㅠㅠ",
          "original_form": "닭갈비가 너무너무너무 너무 너무 먹고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100029821.1.8",
          "form": "앜ㅋ어떡해",
          "original_form": "앜ㅋ어떡해",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100029821.1.9",
          "form": "저도 닭갈비먹고싶어요",
          "original_form": "저도 닭갈비먹고싶어요",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100029821.1.10",
          "form": "치킨배 닭갈비배",
          "original_form": "치킨배 닭갈비배",
          "speaker_id": "1",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100029821.1.11",
          "form": "있으면좋겠어요",
          "original_form": "있으면좋겠어요",
          "speaker_id": "1",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100029821.1.12",
          "form": "철판닭갈비... 진짜 먹고싶어서",
          "original_form": "철판닭갈비... 진짜 먹고싶어서",
          "speaker_id": "2",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100029821.1.13",
          "form": "배달시키고싶지먼...ㅠㅠ참고 운동왓어요",
          "original_form": "배달시키고싶지먼...ㅠㅠ참고 운동왓어요",
          "speaker_id": "2",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100029821.1.14",
          "form": "원조죠!!!",
          "original_form": "원조죠!!!",
          "speaker_id": "1",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100029821.1.15",
          "form": "그맛은",
          "original_form": "그맛은",
          "speaker_id": "1",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100029821.1.16",
          "form": "어뜩해ㅜㅜ",
          "original_form": "어뜩해ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100029821.1.17",
          "form": "근처에 진짜 맛있는곳 있거든요ㅠㅠ",
          "original_form": "근처에 진짜 맛있는곳 있거든요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100029821.1.18",
          "form": "먹고싶을때먹어야죠",
          "original_form": "먹고싶을때먹어야죠",
          "speaker_id": "1",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100029821.1.19",
          "form": "지하철 10분 거리. ...",
          "original_form": "지하철 10분 거리. ...",
          "speaker_id": "2",
          "time": "20210906 18:55"
        },
        {
          "id": "MDRW2100029821.1.20",
          "form": "어뜩해ㅜㆍㅜ",
          "original_form": "어뜩해ㅜㆍㅜ",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100029821.1.21",
          "form": "오늘 참아보고 내일도 먹고싶으면 먹으려구뎌ㅠㅠ",
          "original_form": "오늘 참아보고 내일도 먹고싶으면 먹으려구뎌ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100029821.1.22",
          "form": "참지말고 먹어요!!!",
          "original_form": "참지말고 먹어요!!!",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100029821.1.23",
          "form": "생리중에는 식욕 조절이 안되서",
          "original_form": "생리중에는 식욕 조절이 안되서",
          "speaker_id": "2",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100029821.1.24",
          "form": "참으면 후유증생기던데",
          "original_form": "참으면 후유증생기던데",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100029821.1.25",
          "form": "먹기시작하면 폭발할까봐요ㅠㅋㅋㅋㅋㅋㅋ",
          "original_form": "먹기시작하면 폭발할까봐요ㅠㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100029821.1.26",
          "form": "그니깐욬ㅋㅋㄱㅋ",
          "original_form": "그니깐욬ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100029821.1.27",
          "form": "그래서 다이어트망했어요",
          "original_form": "그래서 다이어트망했어요",
          "speaker_id": "1",
          "time": "20210906 18:56"
        },
        {
          "id": "MDRW2100029821.1.28",
          "form": "오래눈 못참아요.. 최대 1일..",
          "original_form": "오래눈 못참아요.. 최대 1일..",
          "speaker_id": "2",
