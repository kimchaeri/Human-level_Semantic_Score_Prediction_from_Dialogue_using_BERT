      "utterance": [
        {
          "id": "MDRW2100014930.1.1",
          "form": "음식이야기ㅎㄹㄹㄹ",
          "original_form": "음식이야기ㅎㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014930.1.2",
          "form": "배고파지겟네요ㅎㅎ",
          "original_form": "배고파지겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014930.1.3",
          "form": "저녁은? 드셨어요?",
          "original_form": "저녁은? 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014930.1.4",
          "form": "name2님 하고 부르니",
          "original_form": "&name2&님 하고 부르니",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014930.1.5",
          "form": "단밤먹고 싶어짐 ㅋㅋㅋ요 ㅋㅋ",
          "original_form": "단밤먹고 싶어짐 ㅋㅋㅋ요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014930.1.6",
          "form": "ㅎㅎㅎㅎㄹㄹㅎㄹ",
          "original_form": "ㅎㅎㅎㅎㄹㄹㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014930.1.7",
          "form": "어색하긴해요ㅎ name1님하다ㄹㅎ",
          "original_form": "어색하긴해요ㅎ &name1&님하다ㄹㅎ",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014930.1.8",
          "form": "근데 왠지 미션같으니ㅎ",
          "original_form": "근데 왠지 미션같으니ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014930.1.9",
          "form": "간식 많이드시는편이세요?",
          "original_form": "간식 많이드시는편이세요?",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014930.1.10",
          "form": "ㅋㅋ  안쓰는 아디 써보고 싶어짐요",
          "original_form": "ㅋㅋ  안쓰는 아디 써보고 싶어짐요",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014930.1.11",
          "form": "음 간식 먹을땐 먹고",
          "original_form": "음 간식 먹을땐 먹고",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014930.1.12",
          "form": "안먹을땐또 안먹고",
          "original_form": "안먹을땐또 안먹고",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014930.1.13",
          "form": "근데 왜 name2이에요?ㅎㅎ",
          "original_form": "근데 왜 &name2&이에요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014930.1.14",
          "form": "단밤이 먹고싶어서요?ㅎㅎ",
          "original_form": "단밤이 먹고싶어서요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014930.1.15",
          "form": "아뇨 ㅋ",
          "original_form": "아뇨 ㅋ",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014930.1.16",
          "form": "간식은너무자주먹어서 걱정이네요ㅎㅎ",
          "original_form": "간식은너무자주먹어서 걱정이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014930.1.17",
          "form": "이태원클라쓰  가게 이름이에요 ㅎㅎ",
          "original_form": "이태원클라쓰  가게 이름이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014930.1.18",
          "form": "오늘도 캬랴멜..4개먹음ㅎ 믹스는 기본이고 ㅎㅎ",
          "original_form": "오늘도 캬랴멜..4개먹음ㅎ 믹스는 기본이고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014930.1.19",
          "form": "드라마 ~",
          "original_form": "드라마 ~",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014930.1.20",
          "form": "아ㄹㄹㅎ",
          "original_form": "아ㄹㄹㅎ",
          "speaker_id": "2",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014930.1.21",
          "form": "전안봐서ㅎㅎㅎ",
          "original_form": "전안봐서ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014930.1.22",
          "form": "잠깢 본 장면에서",
          "original_form": "잠깢 본 장면에서",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014930.1.23",
          "form": "저도요 ㅋ",
          "original_form": "저도요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014930.1.24",
          "form": "아하ㅎㅎㅎㅎ",
          "original_form": "아하ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014930.1.25",
          "form": "달달한거 좋아하시는 편이에요? 아니면 단백한거?",
          "original_form": "달달한거 좋아하시는 편이에요? 아니면 단백한거?",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014930.1.26",
          "form": "가게이름이 단밤이냐고 물으니 주인공이\r\n힘들고 쓸쓸한 사람들이 우리가게오면\r\n달달한 밤(night)이 되면 좋겟다고",
          "original_form": "가게이름이 단밤이냐고 물으니 주인공이\r\n힘들고 쓸쓸한 사람들이 우리가게오면\r\n달달한 밤(night)이 되면 좋겟다고",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014930.1.27",
          "form": "단밤이래요 ㅎㅎ",
          "original_form": "단밤이래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014930.1.28",
          "form": "뜻이 좋아서 훗",
          "original_form": "뜻이 좋아서 훗",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014930.1.29",
          "form": "우와ㅎㅎ 좋은뜻이네요ㅎ",
          "original_form": "우와ㅎㅎ 좋은뜻이네요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014930.1.30",
          "form": "어쩐지 이름이달달하더라니ㅎ",
          "original_form": "어쩐지 이름이달달하더라니ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014930.1.31",
          "form": "ㅎㅎㅎㅎ 제가 다이어트 소식 알려드릴게요",
          "original_form": "ㅎㅎㅎㅎ 제가 다이어트 소식 알려드릴게요",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014930.1.32",
          "form": "요기요 나이트 쿠폰 15000이상 구매로 바꼇데요 ㄷㄴㄷ",
          "original_form": "요기요 나이트 쿠폰 15000이상 구매로 바꼇데요 ㄷㄴㄷ",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014930.1.33",
          "form": "저런 글보면 왠지모르게 오그라들어야하는데ㅎ 그렇진 않고 좋아요ㅎㅎ 왠지 잠자고있는 갬성을 건들이니깐요ㅎ",
          "original_form": "저런 글보면 왠지모르게 오그라들어야하는데ㅎ 그렇진 않고 좋아요ㅎㅎ 왠지 잠자고있는 갬성을 건들이니깐요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014930.1.34",
          "form": "요기요를 이제 끊어야죠ㅎㅎㅎㄹ",
          "original_form": "요기요를 이제 끊어야죠ㅎㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 21:24"
        }
      ]
