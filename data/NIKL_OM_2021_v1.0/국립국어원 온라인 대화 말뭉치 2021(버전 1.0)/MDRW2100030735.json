      "utterance": [
        {
          "id": "MDRW2100030735.1.1",
          "form": "오늘 날씨가 참 좋네요!",
          "original_form": "오늘 날씨가 참 좋네요!",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030735.1.2",
          "form": "그러게요 선선한 날씨네요",
          "original_form": "그러게요 선선한 날씨네요",
          "speaker_id": "2",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030735.1.3",
          "form": "비가 자주 오더니, 날이 맑아졌어요.",
          "original_form": "비가 자주 오더니, 날이 맑아졌어요.",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030735.1.4",
          "form": "이제 정말 가을인가 봐요",
          "original_form": "이제 정말 가을인가 봐요",
          "speaker_id": "2",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030735.1.5",
          "form": "그러게요, name2님은 어떤 날씨를 좋아하시나요?",
          "original_form": "그러게요, &name2&님은 어떤 날씨를 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100030735.1.6",
          "form": "저는 봄을 제일 좋아해요",
          "original_form": "저는 봄을 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100030735.1.7",
          "form": "봄 좋네요! 꽃구경도 좋아하세요?",
          "original_form": "봄 좋네요! 꽃구경도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 12:07"
        },
        {
          "id": "MDRW2100030735.1.8",
          "form": "네 꽃구경하는 걸 좋아해요",
          "original_form": "네 꽃구경하는 걸 좋아해요",
          "speaker_id": "2",
          "time": "20210904 12:08"
        },
        {
          "id": "MDRW2100030735.1.9",
          "form": "저는 가을도 좋아하는데, 가을은 싫어하시나요?",
          "original_form": "저는 가을도 좋아하는데, 가을은 싫어하시나요?",
          "speaker_id": "1",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030735.1.10",
          "form": "가을도 좋아해요",
          "original_form": "가을도 좋아해요",
          "speaker_id": "2",
          "time": "20210904 12:10"
        },
        {
          "id": "MDRW2100030735.1.11",
          "form": "여름을 제일 좋아하지 않아요",
          "original_form": "여름을 제일 좋아하지 않아요",
          "speaker_id": "2",
          "time": "20210904 12:10"
        },
        {
          "id": "MDRW2100030735.1.12",
          "form": "간편한 옷차림은 편하지만 땀이 나는 날씨를 선호하지 않아요",
          "original_form": "간편한 옷차림은 편하지만 땀이 나는 날씨를 선호하지 않아요",
          "speaker_id": "2",
          "time": "20210904 12:10"
        },
        {
          "id": "MDRW2100030735.1.13",
          "form": "여름은 그래서 힘들죠! 저는 겨울도 좋아해요",
          "original_form": "여름은 그래서 힘들죠! 저는 겨울도 좋아해요",
          "speaker_id": "1",
          "time": "20210904 12:10"
        },
        {
          "id": "MDRW2100030735.1.14",
          "form": "겨울은 아침에 일어나기가 힘들어요",
          "original_form": "겨울은 아침에 일어나기가 힘들어요",
          "speaker_id": "2",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030735.1.15",
          "form": "맞아요. 이불에 조금 더 누워있고 싶은 계절이에요",
          "original_form": "맞아요. 이불에 조금 더 누워있고 싶은 계절이에요",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030735.1.16",
          "form": "겨울의 날씨는 좋지만 옷차림이 너무 두꺼워서",
          "original_form": "겨울의 날씨는 좋지만 옷차림이 너무 두꺼워서",
          "speaker_id": "2",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030735.1.17",
          "form": "활동하기가 불편해요",
          "original_form": "활동하기가 불편해요",
          "speaker_id": "2",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030735.1.18",
          "form": "눈이 내리는 것도 좋지만 쌓이면 이동할 때 불편하죠",
          "original_form": "눈이 내리는 것도 좋지만 쌓이면 이동할 때 불편하죠",
          "speaker_id": "1",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030735.1.19",
          "form": "역시 가을이 가장 좋은 것 같아요",
          "original_form": "역시 가을이 가장 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210904 12:13"
        }
      ]
