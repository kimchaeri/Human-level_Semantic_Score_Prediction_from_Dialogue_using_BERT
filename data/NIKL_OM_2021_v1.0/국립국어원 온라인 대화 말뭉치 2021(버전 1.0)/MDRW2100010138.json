      "utterance": [
        {
          "id": "MDRW2100010138.1.1",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "1",
          "time": "20210823 14:46"
        },
        {
          "id": "MDRW2100010138.1.2",
          "form": "예전에 잠깐했는데 요즘은 코로나땜에 못해요",
          "original_form": "예전에 잠깐했는데 요즘은 코로나땜에 못해요",
          "speaker_id": "2",
          "time": "20210823 14:46"
        },
        {
          "id": "MDRW2100010138.1.3",
          "form": "님은 하시는거있나요?",
          "original_form": "님은 하시는거있나요?",
          "speaker_id": "2",
          "time": "20210823 14:46"
        },
        {
          "id": "MDRW2100010138.1.4",
          "form": "요즘은 집이나 집 근처에서 유산소 조금 하고 있어요",
          "original_form": "요즘은 집이나 집 근처에서 유산소 조금 하고 있어요",
          "speaker_id": "1",
          "time": "20210823 14:47"
        },
        {
          "id": "MDRW2100010138.1.5",
          "form": "예전에는 어떤 운동하셨나요?",
          "original_form": "예전에는 어떤 운동하셨나요?",
          "speaker_id": "1",
          "time": "20210823 14:47"
        },
        {
          "id": "MDRW2100010138.1.6",
          "form": "전 운동취미없긴한데 스피닝이랑 줌바요",
          "original_form": "전 운동취미없긴한데 스피닝이랑 줌바요",
          "speaker_id": "2",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010138.1.7",
          "form": "필라테스 쬐끔",
          "original_form": "필라테스 쬐끔",
          "speaker_id": "2",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010138.1.8",
          "form": "순환pt쬐끔ㅎ",
          "original_form": "순환pt쬐끔ㅎ",
          "speaker_id": "2",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010138.1.9",
          "form": "필라테스 어떤가요? 저도 코로나 진정되면 한번 해보고 싶어서요",
          "original_form": "필라테스 어떤가요? 저도 코로나 진정되면 한번 해보고 싶어서요",
          "speaker_id": "1",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010138.1.10",
          "form": "자세교정도 되고 괜찮은거같아요",
          "original_form": "자세교정도 되고 괜찮은거같아요",
          "speaker_id": "2",
          "time": "20210823 14:49"
        },
        {
          "id": "MDRW2100010138.1.11",
          "form": "정적인 운동이라 신나고그렇진않은데",
          "original_form": "정적인 운동이라 신나고그렇진않은데",
          "speaker_id": "2",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010138.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010138.1.13",
          "form": "근력위주라 유산도 병행하셔야 할거예요",
          "original_form": "근력위주라 유산도 병행하셔야 할거예요",
          "speaker_id": "2",
          "time": "20210823 14:50"
        },
        {
          "id": "MDRW2100010138.1.14",
          "form": "근력위주인 건 의외네요 참고할게요",
          "original_form": "근력위주인 건 의외네요 참고할게요",
          "speaker_id": "1",
          "time": "20210823 14:51"
        },
        {
          "id": "MDRW2100010138.1.15",
          "form": "코어근육 발달시키는거라",
          "original_form": "코어근육 발달시키는거라",
          "speaker_id": "2",
          "time": "20210823 14:51"
        },
        {
          "id": "MDRW2100010138.1.16",
          "form": "여자분이시죠?",
          "original_form": "여자분이시죠?",
          "speaker_id": "2",
          "time": "20210823 14:51"
        },
        {
          "id": "MDRW2100010138.1.17",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 14:51"
        },
        {
          "id": "MDRW2100010138.1.18",
          "form": "생리통에도 효과있더라구요",
          "original_form": "생리통에도 효과있더라구요",
          "speaker_id": "2",
          "time": "20210823 14:51"
        },
        {
          "id": "MDRW2100010138.1.19",
          "form": "반가운 소식이네요",
          "original_form": "반가운 소식이네요",
