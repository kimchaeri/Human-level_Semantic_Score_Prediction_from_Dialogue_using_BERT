      "utterance": [
        {
          "id": "MDRW2100011062.1.1",
          "form": "어떤 음식 제일 좋아하세요?",
          "original_form": "어떤 음식 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 00:18"
        },
        {
          "id": "MDRW2100011062.1.2",
          "form": "식탐이 없어서.. 싸고 눈에 보이는거요ㅡㅡㅋ",
          "original_form": "식탐이 없어서.. 싸고 눈에 보이는거요ㅡㅡㅋ",
          "speaker_id": "1",
          "time": "20210824 00:18"
        },
        {
          "id": "MDRW2100011062.1.3",
          "form": "어떤 음식 조아하세여?",
          "original_form": "어떤 음식 조아하세여?",
          "speaker_id": "1",
          "time": "20210824 00:19"
        },
        {
          "id": "MDRW2100011062.1.4",
          "form": "저는 초밥이랑 회 좋아해요! 고기 종류도 좋아하구요",
          "original_form": "저는 초밥이랑 회 좋아해요! 고기 종류도 좋아하구요",
          "speaker_id": "2",
          "time": "20210824 00:21"
        },
        {
          "id": "MDRW2100011062.1.5",
          "form": "아.. 그거슨 안주?",
          "original_form": "아.. 그거슨 안주?",
          "speaker_id": "1",
          "time": "20210824 00:21"
        },
        {
          "id": "MDRW2100011062.1.6",
          "form": "어 제가 술은 못 먹어서 보통 밥으로 많이 먹어요",
          "original_form": "어 제가 술은 못 먹어서 보통 밥으로 많이 먹어요",
          "speaker_id": "2",
          "time": "20210824 00:22"
        },
        {
          "id": "MDRW2100011062.1.7",
          "form": "술 좋아하세요?",
          "original_form": "술 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 00:22"
        },
        {
          "id": "MDRW2100011062.1.8",
          "form": "ㅎㅎㅎ 좋아하지는 않는데.. \r\n그냥 친구같고.. 항상 옆에있네요.. 퍼런술병이..ㅋㅋ",
          "original_form": "ㅎㅎㅎ 좋아하지는 않는데.. \r\n그냥 친구같고.. 항상 옆에있네요.. 퍼런술병이..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 00:23"
        },
        {
          "id": "MDRW2100011062.1.9",
          "form": "회는 맨입에 먹어도 맛잇나여?",
          "original_form": "회는 맨입에 먹어도 맛잇나여?",
          "speaker_id": "1",
          "time": "20210824 00:23"
        },
        {
          "id": "MDRW2100011062.1.10",
          "form": "술엄시 상상이 안되요",
          "original_form": "술엄시 상상이 안되요",
          "speaker_id": "1",
          "time": "20210824 00:23"
        },
        {
          "id": "MDRW2100011062.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ 항상 옆에 있는거면 좋아하는 거겠죠..?ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 항상 옆에 있는거면 좋아하는 거겠죠..?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:24"
        },
        {
          "id": "MDRW2100011062.1.12",
          "form": "전 보통 탄산음료 아님 물이랑 그냥 먹어요ㅋㅋㅋ 전 술이랑 회를 같이 먹어본 적이 없어서..",
          "original_form": "전 보통 탄산음료 아님 물이랑 그냥 먹어요ㅋㅋㅋ 전 술이랑 회를 같이 먹어본 적이 없어서..",
          "speaker_id": "2",
          "time": "20210824 00:24"
        },
        {
          "id": "MDRW2100011062.1.13",
          "form": "회맛도 잘 아시겠어여?",
          "original_form": "회맛도 잘 아시겠어여?",
          "speaker_id": "1",
          "time": "20210824 00:25"
        },
        {
          "id": "MDRW2100011062.1.14",
          "form": "잘 아는 수준...? 까진 아닌 것 같아요ㅋㅋㅋ 안 먹어본 회 말고는 그냥 구분 가능한 정도..?",
          "original_form": "잘 아는 수준...? 까진 아닌 것 같아요ㅋㅋㅋ 안 먹어본 회 말고는 그냥 구분 가능한 정도..?",
          "speaker_id": "2",
          "time": "20210824 00:26"
        },
        {
          "id": "MDRW2100011062.1.15",
          "form": "와.. 전 안주로만 먹어서 사람들이 욕해요..",
          "original_form": "와.. 전 안주로만 먹어서 사람들이 욕해요..",
          "speaker_id": "1",
          "time": "20210824 00:28"
        },
        {
          "id": "MDRW2100011062.1.16",
          "form": "ㅋㅋ 그럴꺼면 왜 먹냐거..",
          "original_form": "ㅋㅋ 그럴꺼면 왜 먹냐거..",
          "speaker_id": "1",
          "time": "20210824 00:28"
        },
        {
          "id": "MDRW2100011062.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 보통 어른들 보면 회랑 술 많이 드시더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 보통 어른들 보면 회랑 술 많이 드시더라구요",
          "speaker_id": "2",
          "time": "20210824 00:29"
        },
        {
          "id": "MDRW2100011062.1.18",
          "form": "뭐 입맛차이인데 그럴 수 있죠",
          "original_form": "뭐 입맛차이인데 그럴 수 있죠",
          "speaker_id": "2",
          "time": "20210824 00:30"
        },
        {
          "id": "MDRW2100011062.1.19",
          "form": "초밥은 저도 맛알아요..",
          "original_form": "초밥은 저도 맛알아요..",
          "speaker_id": "1",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011062.1.20",
          "form": "그 와사비의 알싸한.. ㅋㅋ",
          "original_form": "그 와사비의 알싸한.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011062.1.21",
          "form": "맞아요ㅋㅋㅋ 그리고 밥도 단촛물로 간 해서 달달새콤하구요",
          "original_form": "맞아요ㅋㅋㅋ 그리고 밥도 단촛물로 간 해서 달달새콤하구요",
          "speaker_id": "2",
          "time": "20210824 00:31"
        },
        {
          "id": "MDRW2100011062.1.22",
          "form": "회랑 잘 어울려요",
          "original_form": "회랑 잘 어울려요",
          "speaker_id": "2",
          "time": "20210824 00:32"
        }
      ]
    }
