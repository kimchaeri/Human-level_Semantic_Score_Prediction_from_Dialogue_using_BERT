      "utterance": [
        {
          "id": "MDRW2100046220.1.1",
          "form": "점심 하시려는거예여?ㅋㅋㅋ",
          "original_form": "점심 하시려는거예여?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046220.1.2",
          "form": "던킨은",
          "original_form": "던킨은",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046220.1.3",
          "form": "네! 지금 배가 그렇게",
          "original_form": "네! 지금 배가 그렇게",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046220.1.4",
          "form": "민트초코말고 또 뭐드시랴고요?",
          "original_form": "민트초코말고 또 뭐드시랴고요?",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046220.1.5",
          "form": "많이안고파서 음...",
          "original_form": "많이안고파서 음...",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046220.1.6",
          "form": "생각 안해봤는데.ㅎㅎㅎ.",
          "original_form": "생각 안해봤는데.ㅎㅎㅎ.",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046220.1.7",
          "form": "민트초코만 생각하다보니",
          "original_form": "민트초코만 생각하다보니",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046220.1.8",
          "form": "ㅋㅋㅋㅋ간식겸 점심으로",
          "original_form": "ㅋㅋㅋㅋ간식겸 점심으로",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046220.1.9",
          "form": "드시는거군요",
          "original_form": "드시는거군요",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046220.1.10",
          "form": "네! 시간이 없어서...",
          "original_form": "네! 시간이 없어서...",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046220.1.11",
          "form": "오랜만에 도넛먹는거라 ㅎㅎ",
          "original_form": "오랜만에 도넛먹는거라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046220.1.12",
          "form": "던킨 커피도 맛있잖아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "던킨 커피도 맛있잖아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046220.1.13",
          "form": "커피 앤 도넛#",
          "original_form": "커피 앤 도넛#",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046220.1.14",
          "form": "헉 코피는 안먹어봤는데",
          "original_form": "헉 코피는 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046220.1.15",
          "form": "사가야겠네요!!",
          "original_form": "사가야겠네요!!",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046220.1.16",
          "form": "커피!!",
          "original_form": "커피!!",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046220.1.17",
          "form": "던킨이 커피맛집이에요~",
          "original_form": "던킨이 커피맛집이에요~",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046220.1.18",
          "form": "커피는 잘안먹어서....음",
          "original_form": "커피는 잘안먹어서....음",
          "speaker_id": "2",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100046220.1.19",
          "form": "아메리카노 하나 사야하나...",
          "original_form": "아메리카노 하나 사야하나...",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046220.1.20",
          "form": "저는 스타벅스 갔다 오려고요~ 밥먹기전에",
          "original_form": "저는 스타벅스 갔다 오려고요~ 밥먹기전에",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046220.1.21",
          "form": "스벅 요번에 신메뉴 나왔는데!!",
          "original_form": "스벅 요번에 신메뉴 나왔는데!!",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046220.1.22",
          "form": "녹차에요!! 심지어!",
          "original_form": "녹차에요!! 심지어!",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046220.1.23",
          "form": "글레이즈드인가?",
          "original_form": "글레이즈드인가?",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046220.1.24",
          "form": "스벅은 신메뉴 참 많이내여",
          "original_form": "스벅은 신메뉴 참 많이내여",
          "speaker_id": "1",
          "time": "20210908 11:29"
        }
      ]
