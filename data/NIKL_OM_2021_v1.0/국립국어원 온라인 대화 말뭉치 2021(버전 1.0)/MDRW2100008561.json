      "utterance": [
        {
          "id": "MDRW2100008561.1.1",
          "form": "커뮤니티하는거 있으세요?",
          "original_form": "커뮤니티하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100008561.1.2",
          "form": "네이버 카페? 정도요 ㅋㅋ",
          "original_form": "네이버 카페? 정도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100008561.1.3",
          "form": "오 무슨 카페인가요?",
          "original_form": "오 무슨 카페인가요?",
          "speaker_id": "1",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100008561.1.4",
          "form": "긴급헌혈단 가입 했어요 얼마전에",
          "original_form": "긴급헌혈단 가입 했어요 얼마전에",
          "speaker_id": "2",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100008561.1.5",
          "form": "말했던가요",
          "original_form": "말했던가요",
          "speaker_id": "2",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100008561.1.6",
          "form": "되게 여기저기 홍보하는중 ㅋㅋ",
          "original_form": "되게 여기저기 홍보하는중 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100008561.1.7",
          "form": "처음듣네요!",
          "original_form": "처음듣네요!",
          "speaker_id": "1",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100008561.1.8",
          "form": "우와 지정헌혈 필요하신분들 올라오는 곳인가요?",
          "original_form": "우와 지정헌혈 필요하신분들 올라오는 곳인가요?",
          "speaker_id": "1",
          "time": "20210824 19:45"
        },
        {
          "id": "MDRW2100008561.1.9",
          "form": "네네 맞아요 특히 혈소판헌혈이요",
          "original_form": "네네 맞아요 특히 혈소판헌혈이요",
          "speaker_id": "2",
          "time": "20210824 19:59"
        },
        {
          "id": "MDRW2100008561.1.10",
          "form": "코로나라서 10개 맞아야할거",
          "original_form": "코로나라서 10개 맞아야할거",
          "speaker_id": "2",
          "time": "20210824 19:59"
        },
        {
          "id": "MDRW2100008561.1.11",
          "form": "3개 나눠서 맞고 버티고 그런대여ㅜㅜ",
          "original_form": "3개 나눠서 맞고 버티고 그런대여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 19:59"
        },
        {
          "id": "MDRW2100008561.1.12",
          "form": "아이고 ㅠㅠㅠㅠ",
          "original_form": "아이고 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100008561.1.13",
          "form": "요즘 진짜 피가 부족하긴 하죠..",
          "original_form": "요즘 진짜 피가 부족하긴 하죠..",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100008561.1.14",
          "form": "저도 열심히 하러다녀야지",
          "original_form": "저도 열심히 하러다녀야지",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100008561.1.15",
          "form": "정말 좋은 카페네요",
          "original_form": "정말 좋은 카페네요",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100008561.1.16",
          "form": "혹시 가입하신 인터넷 카페나 블로그? sns하시나요?",
          "original_form": "혹시 가입하신 인터넷 카페나 블로그? sns하시나요?",
          "speaker_id": "2",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100008561.1.17",
          "form": "저는 오픈카톡 많이들어가요!",
          "original_form": "저는 오픈카톡 많이들어가요!",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100008561.1.18",
          "form": "스터디같은것도 많이 찾아들어가고~",
          "original_form": "스터디같은것도 많이 찾아들어가고~",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100008561.1.19",
          "form": "오픈카톡으로 스터디도 하나요? 신기해라",
          "original_form": "오픈카톡으로 스터디도 하나요? 신기해라",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100008561.1.20",
          "form": "영어작문이나 경제스터디같은게 있더라고요",
          "original_form": "영어작문이나 경제스터디같은게 있더라고요",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100008561.1.21",
          "form": "친구들과는 오히려 스터디하기가 힘들어서",
          "original_form": "친구들과는 오히려 스터디하기가 힘들어서",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100008561.1.22",
          "form": "적당한 익명성이 차라리 나을때도 있죠 알것같아요 ㅎㅎ",
          "original_form": "적당한 익명성이 차라리 나을때도 있죠 알것같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100008561.1.23",
          "form": "아 그러고보니 저는 네이버에 제가 사는 동네 맛집 카페도 가입했어요",
          "original_form": "아 그러고보니 저는 네이버에 제가 사는 동네 맛집 카페도 가입했어요",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100008561.1.24",
          "form": "절찬 눈팅중 ㅋㄱ",
          "original_form": "절찬 눈팅중 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100008561.1.25",
          "form": "와..",
          "original_form": "와..",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100008561.1.26",
          "form": "그런 방법이",
          "original_form": "그런 방법이",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100008561.1.27",
          "form": "저도 한수 배워갑니다ㅋㅋㅋㅋ",
          "original_form": "저도 한수 배워갑니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100008561.1.28",
          "form": "저희동네는 뭐 없나 달려가는중",
          "original_form": "저희동네는 뭐 없나 달려가는중",
          "speaker_id": "1",
          "time": "20210824 23:03"
        }
      ]
    }
