      "utterance": [
        {
          "id": "MDRW2100042171.1.1",
          "form": "우리강아지는.어린나이에 집과 차를 모두 ㅋㅋㅋ",
          "original_form": "우리강아지는.어린나이에 집과 차를 모두 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042171.1.2",
          "form": "ㅎㅎㅎ위너군요",
          "original_form": "ㅎㅎㅎ위너군요",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042171.1.3",
          "form": "여자에요?남자에요?",
          "original_form": "여자에요?남자에요?",
          "speaker_id": "1",
          "time": "20210906 20:23"
        },
        {
          "id": "MDRW2100042171.1.4",
          "form": "여자아이에요",
          "original_form": "여자아이에요",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042171.1.5",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042171.1.6",
          "form": "미용은직접하시나요?",
          "original_form": "미용은직접하시나요?",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042171.1.7",
          "form": "어머니가 해주시거나 맡겨요",
          "original_form": "어머니가 해주시거나 맡겨요",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042171.1.8",
          "form": "제가 잘해주고픈데 난리를 피워서",
          "original_form": "제가 잘해주고픈데 난리를 피워서",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042171.1.9",
          "form": "ㅎㅎ보통그렇죠",
          "original_form": "ㅎㅎ보통그렇죠",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042171.1.10",
          "form": "어머님이능력자시네요",
          "original_form": "어머님이능력자시네요",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042171.1.11",
          "form": "그냥 삐뚤빼뚤 잘려요 ㅋㅋㅋ",
          "original_form": "그냥 삐뚤빼뚤 잘려요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042171.1.12",
          "form": "미용 배우고파요",
          "original_form": "미용 배우고파요",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042171.1.13",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042171.1.14",
          "form": "삐뚤빼뚤해도귀엽더라고요",
          "original_form": "삐뚤빼뚤해도귀엽더라고요",
          "speaker_id": "1",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042171.1.15",
          "form": "맞아요 뭘 해도 이쁘죠",
          "original_form": "맞아요 뭘 해도 이쁘죠",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042171.1.16",
          "form": "활발한성격이에요?",
          "original_form": "활발한성격이에요?",
          "speaker_id": "1",
          "time": "20210906 20:38"
        },
        {
          "id": "MDRW2100042171.1.17",
