      "utterance": [
        {
          "id": "MDRW2100039184.1.1",
          "form": "맛잘알이군요",
          "original_form": "맛잘알이군요",
          "speaker_id": "2",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100039184.1.2",
          "form": "찌개보다 찜이 더 어려울 것 같은덷ㄷ",
          "original_form": "찌개보다 찜이 더 어려울 것 같은덷ㄷ",
          "speaker_id": "2",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100039184.1.3",
          "form": "찜은 그냥",
          "original_form": "찜은 그냥",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100039184.1.4",
          "form": "고춧가루만 추가하고 푹끓이면",
          "original_form": "고춧가루만 추가하고 푹끓이면",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100039184.1.5",
          "form": "고기랑 김치가 알아서 해줘요",
          "original_form": "고기랑 김치가 알아서 해줘요",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100039184.1.6",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100039184.1.7",
          "form": "그런데 찌개는 막 김치볶고 넣을게많더라구요..",
          "original_form": "그런데 찌개는 막 김치볶고 넣을게많더라구요..",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100039184.1.8",
          "form": "그건..찌개도 그렇지 않나요ㅎ",
          "original_form": "그건..찌개도 그렇지 않나요ㅎ",
          "speaker_id": "2",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100039184.1.9",
          "form": "우리집은 김치 안볶고 그냥 해요ㅎ",
          "original_form": "우리집은 김치 안볶고 그냥 해요ㅎ",
          "speaker_id": "2",
          "time": "20210903 13:07"
        },
        {
          "id": "MDRW2100039184.1.10",
          "form": "의외로 그런집도 많더라구요!",
          "original_form": "의외로 그런집도 많더라구요!",
          "speaker_id": "2",
          "time": "20210903 13:07"
        },
        {
          "id": "MDRW2100039184.1.11",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100039184.1.12",
          "form": "맞아요 집마다 다 맛과 방법이 다른 법이져!",
          "original_form": "맞아요 집마다 다 맛과 방법이 다른 법이져!",
          "speaker_id": "1",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100039184.1.13",
          "form": "맛있으면 장땡이에여!",
          "original_form": "맛있으면 장땡이에여!",
          "speaker_id": "1",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100039184.1.14",
          "form": "근데 제 찌개는 맛이없죠",
          "original_form": "근데 제 찌개는 맛이없죠",
          "speaker_id": "1",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100039184.1.15",
          "form": "그래도 찜은 잘하시니ㅜ",
          "original_form": "그래도 찜은 잘하시니ㅜ",
          "speaker_id": "2",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039184.1.16",
          "form": "능력자시죠",
          "original_form": "능력자시죠",
          "speaker_id": "2",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039184.1.17",
          "form": "다른 장기요리는 없나요",
          "original_form": "다른 장기요리는 없나요",
          "speaker_id": "2",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039184.1.18",
          "form": "저 투움바파스타 잘합니다",
          "original_form": "저 투움바파스타 잘합니다",
          "speaker_id": "1",
          "time": "20210903 13:16"
        },
        {
          "id": "MDRW2100039184.1.19",
          "form": "제 투움바를 먹고나면 다신 아웃백에 갈 수 없죠",
          "original_form": "제 투움바를 먹고나면 다신 아웃백에 갈 수 없죠",
          "speaker_id": "1",
          "time": "20210903 13:16"
        },
        {
          "id": "MDRW2100039184.1.20",
          "form": "헉ㄷㄷㄷ금손",
          "original_form": "헉ㄷㄷㄷ금손",
          "speaker_id": "2",
          "time": "20210903 13:29"
        },
        {
          "id": "MDRW2100039184.1.21",
          "form": "친하게 지냅시다!!!",
          "original_form": "친하게 지냅시다!!!",
          "speaker_id": "2",
          "time": "20210903 13:29"
        },
        {
          "id": "MDRW2100039184.1.22",
          "form": "호호호",
          "original_form": "호호호",
          "speaker_id": "1",
          "time": "20210903 13:30"
        },
        {
          "id": "MDRW2100039184.1.23",
          "form": "스테이크 잘 구우시나요?",
          "original_form": "스테이크 잘 구우시나요?",
          "speaker_id": "1",
          "time": "20210903 13:30"
        },
        {
          "id": "MDRW2100039184.1.24",
          "form": "그러면 친하게지낼수있어요!!",
          "original_form": "그러면 친하게지낼수있어요!!",
          "speaker_id": "1",
          "time": "20210903 13:30"
        },
        {
          "id": "MDRW2100039184.1.25",
          "form": "어...님과 저는 평행선이군요ㅜ",
          "original_form": "어...님과 저는 평행선이군요ㅜ",
          "speaker_id": "2",
          "time": "20210903 13:32"
        },
        {
          "id": "MDRW2100039184.1.26",
          "form": "그 누구보다 돼지처럼 잘 먹어드릴수는 있어요ㅜㅇㅜ",
          "original_form": "그 누구보다 돼지처럼 잘 먹어드릴수는 있어요ㅜㅇㅜ",
          "speaker_id": "2",
          "time": "20210903 13:32"
        },
        {
          "id": "MDRW2100039184.1.27",
          "form": "그거 아시나요",
          "original_form": "그거 아시나요",
          "speaker_id": "1",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100039184.1.28",
          "form": "평행선도",
          "original_form": "평행선도",
          "speaker_id": "1",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100039184.1.29",
          "form": "가까이 붙어있으면 ︎한몸︎이라는 것",
          "original_form": "가까이 붙어있으면 {emoji:♥}︎한몸{emoji:♥}︎이라는 것",
          "speaker_id": "1",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100039184.1.30",
          "form": "ㅋㅋ 하지만 저랑 안 친할 거잖아욧!",
          "original_form": "ㅋㅋ 하지만 저랑 안 친할 거잖아욧!",
