      "utterance": [
        {
          "id": "MDRW2100008773.1.1",
          "form": "요즘 뭐해 수강신청 했어?",
          "original_form": "요즘 뭐해 수강신청 했어?",
          "speaker_id": "1",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100008773.1.2",
          "form": "난 졸업했어",
          "original_form": "난 졸업했어",
          "speaker_id": "2",
          "time": "20210822 18:54"
        },
        {
          "id": "MDRW2100008773.1.3",
          "form": "졸업했어도 말할수잇지 뭐!",
          "original_form": "졸업했어도 말할수잇지 뭐!",
          "speaker_id": "2",
          "time": "20210822 18:54"
        },
        {
          "id": "MDRW2100008773.1.4",
          "form": "넌 대학생이야?",
          "original_form": "넌 대학생이야?",
          "speaker_id": "2",
          "time": "20210822 18:54"
        },
        {
          "id": "MDRW2100008773.1.5",
          "form": "응응 1학년",
          "original_form": "응응 1학년",
          "speaker_id": "1",
          "time": "20210822 18:55"
        },
        {
          "id": "MDRW2100008773.1.6",
          "form": "21학번이야?!",
          "original_form": "21학번이야?!",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008773.1.7",
          "form": "완전 애기네",
          "original_form": "완전 애기네",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008773.1.8",
          "form": "대학생활 못해봐서 어떡해",
          "original_form": "대학생활 못해봐서 어떡해",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008773.1.9",
          "form": "그러니깐~~",
          "original_form": "그러니깐~~",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008773.1.10",
          "form": "나도 캠퍼스 라이프를 즐기고 싶다",
          "original_form": "나도 캠퍼스 라이프를 즐기고 싶다",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008773.1.11",
          "form": "그럼 수업은 아예 비대면으로만 듣는거야?",
          "original_form": "그럼 수업은 아예 비대면으로만 듣는거야?",
          "speaker_id": "2",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008773.1.12",
          "form": "줌으로 듣고 시험도 온라인으로?",
          "original_form": "줌으로 듣고 시험도 온라인으로?",
          "speaker_id": "2",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008773.1.13",
          "form": "난 코로나 터지기 전에 졸업해서 요즘은 어떻게 하는지 궁금하다",
          "original_form": "난 코로나 터지기 전에 졸업해서 요즘은 어떻게 하는지 궁금하다",
          "speaker_id": "2",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008773.1.14",
          "form": "응응 비대면으로 시험 볼 때 대면하자고 말 나오면 투표로 진행했었는데 거의 다 비대면이었어",
          "original_form": "응응 비대면으로 시험 볼 때 대면하자고 말 나오면 투표로 진행했었는데 거의 다 비대면이었어",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008773.1.15",
          "form": "줌으로 듣기도 하고 녹화된 영상으로 듣기도 하고",
          "original_form": "줌으로 듣기도 하고 녹화된 영상으로 듣기도 하고",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008773.1.16",
          "form": "시험은 과제로 대체 된다든지 등등",
          "original_form": "시험은 과제로 대체 된다든지 등등",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008773.1.17",
          "form": "ㅋㅋㅋ그치 시험본다고 대면하면 그것도 좀 웃기긴하다... 수업은 기껏 비대면으로 다 해놓고",
          "original_form": "ㅋㅋㅋ그치 시험본다고 대면하면 그것도 좀 웃기긴하다... 수업은 기껏 비대면으로 다 해놓고",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008773.1.18",
          "form": "난 실강보다 인강듣는게 더 힘들던데 고생많았네ㅋㅋ큐ㅠㅠ",
          "original_form": "난 실강보다 인강듣는게 더 힘들던데 고생많았네ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008773.1.19",
          "form": "동기들끼리는 서로 알긴 해?",
          "original_form": "동기들끼리는 서로 알긴 해?",
          "speaker_id": "2",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008773.1.20",
          "form": "맞아 나도 실강이 더 좋을 거 같아, ..",
          "original_form": "맞아 나도 실강이 더 좋을 거 같아, ..",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008773.1.21",
          "form": "동기들 서로 잘 몰라ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "동기들 서로 잘 몰라ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008773.1.22",
          "form": "만날 일이 없으니까ㅋㅋ",
          "original_form": "만날 일이 없으니까ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008773.1.23",
          "form": "기껏등록금내는데 줌으로만 수업들으면 좀 돈아깝긴하겠다ㅠ",
          "original_form": "기껏등록금내는데 줌으로만 수업들으면 좀 돈아깝긴하겠다ㅠ",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100008773.1.24",
          "form": "그렇겠네ㅠㅠㅠㅠㅠ",
          "original_form": "그렇겠네ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100008773.1.25",
          "form": "백신 예약은 했어? 내년엔 대면 수업 하지않을까?",
          "original_form": "백신 예약은 했어? 내년엔 대면 수업 하지않을까?",
          "speaker_id": "2",
          "time": "20210822 21:41"
        },
        {
          "id": "MDRW2100008773.1.26",
          "form": "맞아 직접 들어야 의미가 있는 듯ㅜㅜ",
          "original_form": "맞아 직접 들어야 의미가 있는 듯ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100008773.1.27",
          "form": "내년에 대면 수업 하게 될 지도,,?",
          "original_form": "내년에 대면 수업 하게 될 지도,,?",
          "speaker_id": "1",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100008773.1.28",
          "form": "사실 이번 10월달에 대면한다는 말이 있거든",
          "original_form": "사실 이번 10월달에 대면한다는 말이 있거든",
