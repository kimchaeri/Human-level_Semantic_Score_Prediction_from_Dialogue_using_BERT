      "utterance": [
        {
          "id": "MDRW2100027084.1.1",
          "form": "저는 갤이요",
          "original_form": "저는 갤이요",
          "speaker_id": "2",
          "time": "20210902 09:19"
        },
        {
          "id": "MDRW2100027084.1.2",
          "form": "갤이면 디씨인가요?",
          "original_form": "갤이면 디씨인가요?",
          "speaker_id": "1",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100027084.1.3",
          "form": "네 눈팅만 해요",
          "original_form": "네 눈팅만 해요",
          "speaker_id": "2",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100027084.1.4",
          "form": "어디갤하세요?",
          "original_form": "어디갤하세요?",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100027084.1.5",
          "form": "좋아하는 티비프로나 드라마요",
          "original_form": "좋아하는 티비프로나 드라마요",
          "speaker_id": "2",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100027084.1.6",
          "form": "드갤러시군요 저는 뽐뿌 재테크포럼 다녀요",
          "original_form": "드갤러시군요 저는 뽐뿌 재테크포럼 다녀요",
          "speaker_id": "1",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100027084.1.7",
          "form": "재테크면 투자쪽인가여",
          "original_form": "재테크면 투자쪽인가여",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100027084.1.8",
          "form": "음 아뇨 카드체리피킹쪽요",
          "original_form": "음 아뇨 카드체리피킹쪽요",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027084.1.9",
          "form": "뽐뿌 이벤트게시판은 가끔 가요",
          "original_form": "뽐뿌 이벤트게시판은 가끔 가요",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100027084.1.10",
          "form": "이벤트 좋죠 검소하시네요",
          "original_form": "이벤트 좋죠 검소하시네요",
          "speaker_id": "1",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100027084.1.11",
          "form": "네 당첨이 소소한 기쁨이에요",
          "original_form": "네 당첨이 소소한 기쁨이에요",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100027084.1.12",
          "form": "ㅋㅋ 당첨되면 좋죠",
          "original_form": "ㅋㅋ 당첨되면 좋죠",
          "speaker_id": "1",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100027084.1.13",
          "form": "엠엘비파크는 안가세요",
          "original_form": "엠엘비파크는 안가세요",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027084.1.14",
          "form": "아 저는 뽐뿌만",
          "original_form": "아 저는 뽐뿌만",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027084.1.15",
          "form": "오늘의유머도 재밌대요",
          "original_form": "오늘의유머도 재밌대요",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027084.1.16",
          "form": "음 저는 보배드림 유머게시판은 다녀요",
          "original_form": "음 저는 보배드림 유머게시판은 다녀요",
