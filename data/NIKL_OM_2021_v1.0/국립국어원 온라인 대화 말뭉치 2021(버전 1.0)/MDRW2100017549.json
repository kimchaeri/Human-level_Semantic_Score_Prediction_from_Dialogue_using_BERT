      "utterance": [
        {
          "id": "MDRW2100017549.1.1",
          "form": "동아리 하시나요?",
          "original_form": "동아리 하시나요?",
          "speaker_id": "1",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017549.1.2",
          "form": "아뇨 딱히 안하고있어요",
          "original_form": "아뇨 딱히 안하고있어요",
          "speaker_id": "2",
          "time": "20210826 20:02"
        },
        {
          "id": "MDRW2100017549.1.3",
          "form": "앞으로도 가입하실 생각은 없나요?",
          "original_form": "앞으로도 가입하실 생각은 없나요?",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017549.1.4",
          "form": "음 네 아마 안할것같아요",
          "original_form": "음 네 아마 안할것같아요",
          "speaker_id": "2",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100017549.1.5",
          "form": "곧 개강이라 벌써부터 종강을 원하시는건가요?",
          "original_form": "곧 개강이라 벌써부터 종강을 원하시는건가요?",
          "speaker_id": "1",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017549.1.6",
          "form": "그렇죠..",
          "original_form": "그렇죠..",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017549.1.7",
          "form": "개강하기도 전에 종강을 원하고 있습니다",
          "original_form": "개강하기도 전에 종강을 원하고 있습니다",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017549.1.8",
          "form": "출근 전부터 퇴근을 원하는 제 마음과 같네요",
          "original_form": "출근 전부터 퇴근을 원하는 제 마음과 같네요",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017549.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ아 왠지 알것같네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 왠지 알것같네요",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017549.1.10",
          "form": "종강하려면 아직 한참 멀었는데",
          "original_form": "종강하려면 아직 한참 멀었는데",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017549.1.11",
          "form": "어서 종강하고픈 마음,,",
          "original_form": "어서 종강하고픈 마음,,",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017549.1.12",
          "form": "일요일임에도 다시 금욜 저녁이엇으면 하는 마음,,",
          "original_form": "일요일임에도 다시 금욜 저녁이엇으면 하는 마음,,",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017549.1.13",
          "form": "수강신청은 성공하셨나요",
          "original_form": "수강신청은 성공하셨나요",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017549.1.14",
          "form": "일욜밤의 그 마음22",
          "original_form": "일욜밤의 그 마음22",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017549.1.15",
          "form": "꿀교양 하나 못잡아서 아쉽긴한데ㅠㅠ",
          "original_form": "꿀교양 하나 못잡아서 아쉽긴한데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017549.1.16",
          "form": "그래도 이정도면 만족하고있어요",
          "original_form": "그래도 이정도면 만족하고있어요",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017549.1.17",
          "form": "하나만 못잡은거면 성공하신거네요",
          "original_form": "하나만 못잡은거면 성공하신거네요",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017549.1.18",
          "form": "저는 똥손이라 전공필수 빼고는 늘 실패였는데ㅠ",
          "original_form": "저는 똥손이라 전공필수 빼고는 늘 실패였는데ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017549.1.19",
          "form": "앗ㅠㅠ",
          "original_form": "앗ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017549.1.20",
          "form": "자리 나오려나 싶어서 계속 확인하고 그랬는데",
          "original_form": "자리 나오려나 싶어서 계속 확인하고 그랬는데",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017549.1.21",
          "form": "이정도 해봤으면 포기할때 된거같아서ㅠ",
          "original_form": "이정도 해봤으면 포기할때 된거같아서ㅠ",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017549.1.22",
          "form": "나름 선방했다고 생각하고,,",
          "original_form": "나름 선방했다고 생각하고,,",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017549.1.23",
          "form": "담학기엔 꼭 성공하고 싶네요ㅎㅎㅠ",
          "original_form": "담학기엔 꼭 성공하고 싶네요ㅎㅎㅠ",
          "speaker_id": "2",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017549.1.24",
          "form": "성공하실거에요!!",
          "original_form": "성공하실거에요!!",
          "speaker_id": "1",
          "time": "20210826 20:22"
        }
      ]
    }
