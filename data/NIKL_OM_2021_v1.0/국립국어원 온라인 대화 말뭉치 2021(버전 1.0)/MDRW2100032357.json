      "utterance": [
        {
          "id": "MDRW2100032357.1.1",
          "form": "곰표맥주 드셔보셨어요?",
          "original_form": "곰표맥주 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100032357.1.2",
          "form": "헐랭 아직이요",
          "original_form": "헐랭 아직이요",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100032357.1.3",
          "form": "곰표...",
          "original_form": "곰표...",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100032357.1.4",
          "form": "근데 곰표가 뭐죠",
          "original_form": "근데 곰표가 뭐죠",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100032357.1.5",
          "form": "밀가루 만드는 회사 있잖아요",
          "original_form": "밀가루 만드는 회사 있잖아요",
          "speaker_id": "1",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032357.1.6",
          "form": "곰표밀가루",
          "original_form": "곰표밀가루",
          "speaker_id": "1",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032357.1.7",
          "form": "이런 콜라보 처음엔 신선했는데 요즘은 너무 많은것 같인요",
          "original_form": "이런 콜라보 처음엔 신선했는데 요즘은 너무 많은것 같인요",
          "speaker_id": "1",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100032357.1.8",
          "form": "편의점가면 엄청 많던데",
          "original_form": "편의점가면 엄청 많던데",
          "speaker_id": "1",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100032357.1.9",
          "form": "엌ㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032357.1.10",
          "form": "곰표밀가루구나",
          "original_form": "곰표밀가루구나",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032357.1.11",
          "form": "저는 진로토닉좋아해요",
          "original_form": "저는 진로토닉좋아해요",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032357.1.12",
          "form": "아 저도 요즘 위스키 조금씩 마시는데",
          "original_form": "아 저도 요즘 위스키 조금씩 마시는데",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032357.1.13",
          "form": "진로토닉워터 섞어마시면 마시기 편해져서 좋아요",
          "original_form": "진로토닉워터 섞어마시면 마시기 편해져서 좋아요",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032357.1.14",
          "form": "토닉워터 뭐랑 마셔요?",
          "original_form": "토닉워터 뭐랑 마셔요?",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032357.1.15",
          "form": "오오 역시 샷이죠",
          "original_form": "오오 역시 샷이죠",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032357.1.16",
          "form": "위스키랑먹으면",
          "original_form": "위스키랑먹으면",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032357.1.17",
          "form": "거기에레몬즙싸지~",
          "original_form": "거기에레몬즙싸지~",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032357.1.18",
          "form": "위스키 좋아하세요?",
          "original_form": "위스키 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032357.1.19",
          "form": "저는 작년부터 맛들여서 조금씩 마시는데",
          "original_form": "저는 작년부터 맛들여서 조금씩 마시는데",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032357.1.20",
          "form": "위스키 향이 너무좋인요",
          "original_form": "위스키 향이 너무좋인요",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032357.1.21",
          "form": "숙취도 없고",
          "original_form": "숙취도 없고",
          "speaker_id": "1",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032357.1.22",
          "form": "오오 그쵸그쵸",
          "original_form": "오오 그쵸그쵸",
          "speaker_id": "2",
          "time": "20210906 02:16"
        },
        {
          "id": "MDRW2100032357.1.23",
          "form": "꽐라되지만않으면",
          "original_form": "꽐라되지만않으면",
          "speaker_id": "2",
          "time": "20210906 02:17"
        },
        {
          "id": "MDRW2100032357.1.24",
          "form": "막마셔도잘안취하더라구요",
          "original_form": "막마셔도잘안취하더라구요",
          "speaker_id": "2",
          "time": "20210906 02:17"
        },
        {
          "id": "MDRW2100032357.1.25",
          "form": "또마시고싶네요...",
          "original_form": "또마시고싶네요...",
          "speaker_id": "2",
          "time": "20210906 02:17"
        },
        {
          "id": "MDRW2100032357.1.26",
          "form": "어떤거 좋아하세요?",
          "original_form": "어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 07:07"
        },
        {
          "id": "MDRW2100032357.1.27",
          "form": "오옹 저는역시 소맥일까요",
          "original_form": "오옹 저는역시 소맥일까요",
          "speaker_id": "2",
          "time": "20210906 15:00"
        },
        {
          "id": "MDRW2100032357.1.28",
          "form": "위스키는 사실",
          "original_form": "위스키는 사실",
          "speaker_id": "2",
          "time": "20210906 15:00"
        },
        {
          "id": "MDRW2100032357.1.29",
          "form": "아는게없는데",
          "original_form": "아는게없는데",
          "speaker_id": "2",
          "time": "20210906 15:00"
        },
        {
          "id": "MDRW2100032357.1.30",
          "form": "그냥 다먹어도맛나더라구요",
          "original_form": "그냥 다먹어도맛나더라구요",
          "speaker_id": "2",
          "time": "20210906 15:00"
        },
        {
          "id": "MDRW2100032357.1.31",
          "form": "아 그냥 술을 좋아하시는 거구나ㅋㅋㄱ",
          "original_form": "아 그냥 술을 좋아하시는 거구나ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210906 15:05"
        },
        {
          "id": "MDRW2100032357.1.32",
          "form": "술 적당히 마시면 좋죠 기분 좋아지고",
          "original_form": "술 적당히 마시면 좋죠 기분 좋아지고",
          "speaker_id": "1",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100032357.1.33",
          "form": "술자리 분위기도 좋고",
          "original_form": "술자리 분위기도 좋고",
          "speaker_id": "1",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100032357.1.34",
          "form": "ㅋㅋㅋㅋ맞아요...술을..좋아한답니다",
          "original_form": "ㅋㅋㅋㅋ맞아요...술을..좋아한답니다",
          "speaker_id": "2",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100032357.1.35",
          "form": "그런데 코로나때문에 그럴일이 없어서",
          "original_form": "그런데 코로나때문에 그럴일이 없어서",
          "speaker_id": "1",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100032357.1.36",
          "form": "아아그쵸",
          "original_form": "아아그쵸",
          "speaker_id": "2",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100032357.1.37",
          "form": "재미없긴해요",
          "original_form": "재미없긴해요",
          "speaker_id": "1",
          "time": "20210906 15:06"
        }
      ]
