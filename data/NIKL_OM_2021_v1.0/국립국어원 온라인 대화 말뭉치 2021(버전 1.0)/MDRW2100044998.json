      "utterance": [
        {
          "id": "MDRW2100044998.1.1",
          "form": "스트레스 받는일 있으신가요? 업무상?",
          "original_form": "스트레스 받는일 있으신가요? 업무상?",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044998.1.2",
          "form": "요즘에는 사람 스트레스 이외에는 없어요!!",
          "original_form": "요즘에는 사람 스트레스 이외에는 없어요!!",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044998.1.3",
          "form": "그게 가장 큰거 같긴 하네여...",
          "original_form": "그게 가장 큰거 같긴 하네여...",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044998.1.4",
          "form": "name1님은 뭐 있으신가요??",
          "original_form": "&name1&님은 뭐 있으신가요??",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044998.1.5",
          "form": "저도 이리저리 치인다는게ㅋㅋㅋㅋ",
          "original_form": "저도 이리저리 치인다는게ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044998.1.6",
          "form": "그게 제일 짜증나죠",
          "original_form": "그게 제일 짜증나죠",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044998.1.7",
          "form": "청내공때문에 머리아플거같아요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "청내공때문에 머리아플거같아요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044998.1.8",
          "form": "아 그거 하세요??",
          "original_form": "아 그거 하세요??",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044998.1.9",
          "form": "나기지도 못하시겠네요 ㅠㅠ",
          "original_form": "나기지도 못하시겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044998.1.10",
          "form": "제가 하느게아니고",
          "original_form": "제가 하느게아니고",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044998.1.11",
          "form": "하는 사람때문에 피곤하신가봐여",
          "original_form": "하는 사람때문에 피곤하신가봐여",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100044998.1.12",
          "form": "들려는 직원이 있는데 이번에 청내공이 조건이 조금 바뀌어서ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "들려는 직원이 있는데 이번에 청내공이 조건이 조금 바뀌어서ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100044998.1.13",
          "form": "네네 일이니까요",
          "original_form": "네네 일이니까요",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100044998.1.14",
          "form": "아 조건이 까다로워 진거에요???",
          "original_form": "아 조건이 까다로워 진거에요???",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100044998.1.15",
          "form": "ㅠㅠ 왜 피곤하게 바뀌는 걸까여",
          "original_form": "ㅠㅠ 왜 피곤하게 바뀌는 걸까여",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100044998.1.16",
          "form": "네네 추경으로..하다구요",
          "original_form": "네네 추경으로..하다구요",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044998.1.17",
          "form": "ㅠㅠ 일하는사람들 귀찮게",
          "original_form": "ㅠㅠ 일하는사람들 귀찮게",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044998.1.18",
          "form": "상사가 괴롭히나요 부하가 힘들게 하나요?",
          "original_form": "상사가 괴롭히나요 부하가 힘들게 하나요?",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044998.1.19",
          "form": "아님 동료?",
          "original_form": "아님 동료?",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044998.1.20",
          "form": "ㅎㅎㅎ 보통 상사져",
          "original_form": "ㅎㅎㅎ 보통 상사져",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100044998.1.21",
