        },
        {
          "id": "MDRW2100007772.1.2",
          "form": "엉 그나마 먹고오니",
          "original_form": "엉 그나마 먹고오니",
          "speaker_id": "2",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007772.1.3",
          "form": "좀 낫다",
          "original_form": "좀 낫다",
          "speaker_id": "2",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007772.1.4",
          "form": "지도 맛있다고 하고",
          "original_form": "지도 맛있다고 하고",
          "speaker_id": "2",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007772.1.5",
          "form": "잘먹는다니~다행이구만요^^",
          "original_form": "잘먹는다니~다행이구만요^^",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007772.1.6",
          "form": "유치원때는 매일 지도 힘들고 나도 불쌍하고",
          "original_form": "유치원때는 매일 지도 힘들고 나도 불쌍하고",
          "speaker_id": "2",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007772.1.7",
          "form": "짜다라 집밥도 맛없는디....",
          "original_form": "짜다라 집밥도 맛없는디....",
          "speaker_id": "2",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007772.1.8",
          "form": "맛없는거 먹는갓도 힘들쥬ㅠㅠ",
          "original_form": "맛없는거 먹는갓도 힘들쥬ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007772.1.9",
          "form": "근데 학교 메뉴봤나??",
          "original_form": "근데 학교 메뉴봤나??",
          "speaker_id": "2",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007772.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 욱겨라 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 욱겨라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007772.1.11",
          "form": "맛있는거 정말 많이 나오더라",
          "original_form": "맛있는거 정말 많이 나오더라",
          "speaker_id": "2",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007772.1.12",
          "form": "후식도 잘 나오고",
          "original_form": "후식도 잘 나오고",
          "speaker_id": "2",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007772.1.13",
          "form": "아뇨 ㅋㅋㅋㅋ지는 메뉴 일절 안범 ㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋㅋ지는 메뉴 일절 안범 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007772.1.14",
          "form": "캐가 맨날 메뉴 겹쳐요 ㅋㅋㅋ",
          "original_form": "캐가 맨날 메뉴 겹쳐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007772.1.15",
          "form": "샤베트 이런것도 후식으로 나오고 ㅋㅋㅋㅋ아스크림 나왔다고 엄청 좋아함",
          "original_form": "샤베트 이런것도 후식으로 나오고 ㅋㅋㅋㅋ아스크림 나왔다고 엄청 좋아함",
          "speaker_id": "2",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007772.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 구절판도 있음",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 구절판도 있음",
          "speaker_id": "2",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007772.1.17",
          "form": "메뉴대로 얼마나 잘 나올지는 모르겠지만",
          "original_form": "메뉴대로 얼마나 잘 나올지는 모르겠지만",
          "speaker_id": "2",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007772.1.18",
          "form": "일단 식단 구성은 아주 마음에 든다",
          "original_form": "일단 식단 구성은 아주 마음에 든다",
          "speaker_id": "2",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007772.1.19",
          "form": "구절판요??",
          "original_form": "구절판요??",
          "speaker_id": "1",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007772.1.20",
          "form": "어 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007772.1.21",
          "form": "이야 멋진데??그건 겹칠일 업긋네여 ㅋㅋ",
          "original_form": "이야 멋진데??그건 겹칠일 업긋네여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007772.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그래 없지없지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그래 없지없지",
          "speaker_id": "2",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007772.1.23",
          "form": "밖에서 아버님 또 성질을 슬슬 내네",
          "original_form": "밖에서 아버님 또 성질을 슬슬 내네",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007772.1.24",
          "form": "뭘 해줘도 맛있게 먹음 더 해주고 싶을낀데~0",
          "original_form": "뭘 해줘도 맛있게 먹음 더 해주고 싶을낀데~0",
          "speaker_id": "1",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007772.1.25",
          "form": "나가야 될 시간이 다가온다",
          "original_form": "나가야 될 시간이 다가온다",
          "speaker_id": "2",
          "time": "20210822 10:27"
        },
        {
          "id": "MDRW2100007772.1.26",
          "form": "이제 언니 일어날 시간된거쥬 ㅋㅋㅋㅋ",
          "original_form": "이제 언니 일어날 시간된거쥬 ㅋㅋㅋㅋ",
