      "utterance": [
        {
          "id": "MDRW2100009754.1.1",
          "form": "즐겨하는 게임이 있으신가요",
          "original_form": "즐겨하는 게임이 있으신가요",
          "speaker_id": "2",
          "time": "20210823 09:29"
        },
        {
          "id": "MDRW2100009754.1.2",
          "form": "저 예전에 오버워치랑 돈스타브랑 메이플 했었어요! 아시나요?",
          "original_form": "저 예전에 오버워치랑 돈스타브랑 메이플 했었어요! 아시나요?",
          "speaker_id": "1",
          "time": "20210823 10:27"
        },
        {
          "id": "MDRW2100009754.1.3",
          "form": "어 저 메이플했었어요!",
          "original_form": "어 저 메이플했었어요!",
          "speaker_id": "2",
          "time": "20210823 10:28"
        },
        {
          "id": "MDRW2100009754.1.4",
          "form": "어떤 직업으로 플레이 하셨나요?",
          "original_form": "어떤 직업으로 플레이 하셨나요?",
          "speaker_id": "2",
          "time": "20210823 10:28"
        },
        {
          "id": "MDRW2100009754.1.5",
          "form": "저 전사요 ㅋㅋㅋㅋ",
          "original_form": "저 전사요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:28"
        },
        {
          "id": "MDRW2100009754.1.6",
          "form": "뭐 하셨었어요?",
          "original_form": "뭐 하셨었어요?",
          "speaker_id": "1",
          "time": "20210823 10:29"
        },
        {
          "id": "MDRW2100009754.1.7",
          "form": "메카닉도 했었어요",
          "original_form": "메카닉도 했었어요",
          "speaker_id": "1",
          "time": "20210823 10:29"
        },
        {
          "id": "MDRW2100009754.1.8",
          "form": "용감한직업을 선택하셨군요",
          "original_form": "용감한직업을 선택하셨군요",
          "speaker_id": "2",
          "time": "20210823 10:29"
        },
        {
          "id": "MDRW2100009754.1.9",
          "form": "전 마법사했어요",
          "original_form": "전 마법사했어요",
          "speaker_id": "2",
          "time": "20210823 10:29"
        },
        {
          "id": "MDRW2100009754.1.10",
          "form": "육성이 쉬워서요",
          "original_form": "육성이 쉬워서요",
          "speaker_id": "2",
          "time": "20210823 10:29"
        },
        {
          "id": "MDRW2100009754.1.11",
          "form": "마법사 텔레포트 좋아보여요",
          "original_form": "마법사 텔레포트 좋아보여요",
          "speaker_id": "1",
          "time": "20210823 10:29"
        },
        {
          "id": "MDRW2100009754.1.12",
          "form": "기본 키로 이동하는거 답답해서",
          "original_form": "기본 키로 이동하는거 답답해서",
          "speaker_id": "1",
          "time": "20210823 10:29"
        },
        {
          "id": "MDRW2100009754.1.13",
          "form": "맞아요 사실 그거 때문에도 선택한게있어요",
          "original_form": "맞아요 사실 그거 때문에도 선택한게있어요",
          "speaker_id": "2",
          "time": "20210823 10:29"
        },
        {
          "id": "MDRW2100009754.1.14",
          "form": "좋아하는 맵은 있었나요?",
          "original_form": "좋아하는 맵은 있었나요?",
          "speaker_id": "2",
          "time": "20210823 10:30"
        },
        {
          "id": "MDRW2100009754.1.15",
          "form": "맞아요 ㅋㅋㅋ 매직클로 인가요?",
          "original_form": "맞아요 ㅋㅋㅋ 매직클로 인가요?",
          "speaker_id": "1",
          "time": "20210823 10:30"
        },
        {
          "id": "MDRW2100009754.1.16",
          "form": "그거 기본공격처럼 쓰고",
          "original_form": "그거 기본공격처럼 쓰고",
          "speaker_id": "1",
          "time": "20210823 10:30"
        },
        {
          "id": "MDRW2100009754.1.17",
          "form": "아 맞아요ㅋㅋ",
          "original_form": "아 맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:30"
        },
        {
          "id": "MDRW2100009754.1.18",
          "form": "가장먼저 배우는 스킬이었던거 같아요",
          "original_form": "가장먼저 배우는 스킬이었던거 같아요",
          "speaker_id": "2",
          "time": "20210823 10:30"
        },
        {
          "id": "MDRW2100009754.1.19",
          "form": "엘리니아 기억나시나요",
          "original_form": "엘리니아 기억나시나요",
          "speaker_id": "1",
          "time": "20210823 10:31"
        },
        {
          "id": "MDRW2100009754.1.20",
          "form": "네 마법사 전직하는 마을이잖아요",
          "original_form": "네 마법사 전직하는 마을이잖아요",
          "speaker_id": "2",
          "time": "20210823 10:31"
        },
        {
          "id": "MDRW2100009754.1.21",
          "form": "초록숲의 기운이 느껴지는 마을이었어요",
          "original_form": "초록숲의 기운이 느껴지는 마을이었어요",
          "speaker_id": "2",
          "time": "20210823 10:32"
        },
        {
          "id": "MDRW2100009754.1.22",
          "form": "초록숲ㅋㅋㅋㅋ 요정마을이죠",
          "original_form": "초록숲ㅋㅋㅋㅋ 요정마을이죠",
          "speaker_id": "1",
          "time": "20210823 10:32"
        },
        {
          "id": "MDRW2100009754.1.23",
          "form": "슬라임 막 잡고 그랬었는데",
          "original_form": "슬라임 막 잡고 그랬었는데",
          "speaker_id": "1",
          "time": "20210823 10:33"
        },
        {
          "id": "MDRW2100009754.1.24",
          "form": "맞아요 슬 라 임의 방울 얻으러 다니고",
          "original_form": "맞아요 슬 라 임의 방울 얻으러 다니고",
          "speaker_id": "2",
          "time": "20210823 10:33"
        }
      ]
    }
  ]
