      "utterance": [
        {
          "id": "MDRW2100000571.1.1",
          "form": "저는 최근에 아이패드 샀어요",
          "original_form": "저는 최근에 아이패드 샀어요",
          "speaker_id": "1",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000571.1.2",
          "form": "오 프로사셨어요?",
          "original_form": "오 프로사셨어요?",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000571.1.3",
          "form": "네 프로 12.9인치 샀어요! 후덜덜",
          "original_form": "네 프로 12.9인치 샀어요! 후덜덜",
          "speaker_id": "1",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000571.1.4",
          "form": "오오 저는 프로 11인치 쓰고 있어요",
          "original_form": "오오 저는 프로 11인치 쓰고 있어요",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000571.1.5",
          "form": "어떤 목적으로 사셨어요?",
          "original_form": "어떤 목적으로 사셨어요?",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000571.1.6",
          "form": "저는 그림그리고 싶어서 샀어요!",
          "original_form": "저는 그림그리고 싶어서 샀어요!",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000571.1.7",
          "form": "name2님은 무슨 목적으로 사셧나요?",
          "original_form": "&name2&님은 무슨 목적으로 사셧나요?",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000571.1.8",
          "form": "저도 웹툰 그려보고 싶어서 샀는데 그냥 일기작성용으로만 쓰게 되네요",
          "original_form": "저도 웹툰 그려보고 싶어서 샀는데 그냥 일기작성용으로만 쓰게 되네요",
          "speaker_id": "2",
          "time": "20210819 17:14"
        },
        {
          "id": "MDRW2100000571.1.9",
          "form": "오 웹툰 대단하신데요",
          "original_form": "오 웹툰 대단하신데요",
          "speaker_id": "1",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000571.1.10",
          "form": "저는 그림을 아예몰라서 큰화면으로 만화나 보네욬ㅋㅋㅋ",
          "original_form": "저는 그림을 아예몰라서 큰화면으로 만화나 보네욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000571.1.11",
          "form": "ㅋㅋ보통 넷플이나 만화보게 된다고 하더라구요",
          "original_form": "ㅋㅋ보통 넷플이나 만화보게 된다고 하더라구요",
          "speaker_id": "2",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000571.1.12",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000571.1.13",
          "form": "구매할때 휠수있다구 해서",
          "original_form": "구매할때 휠수있다구 해서",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000571.1.14",
          "form": "애플케어 들었는데 name2님도 드셨나요?",
          "original_form": "애플케어 들었는데 &name2&님도 드셨나요?",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000571.1.15",
          "form": "아 저도 그거 걱정했는데 초반 보증도 있어서 안 들었어요",
          "original_form": "아 저도 그거 걱정했는데 초반 보증도 있어서 안 들었어요",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000571.1.16",
          "form": "맞아요 좀 비싸죠ㅠㅠ 저도 손떨면서 결제했어요",
          "original_form": "맞아요 좀 비싸죠ㅠㅠ 저도 손떨면서 결제했어요",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000571.1.17",
          "form": "저 2달 고민하다 샀잖아요ㅎㅎ",
          "original_form": "저 2달 고민하다 샀잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000571.1.18",
          "form": "근데 좋긴한거 같아요!",
          "original_form": "근데 좋긴한거 같아요!",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000571.1.19",
          "form": "저도 고민했는데 배송만 늦춰졌네요 ㅋㅋ",
          "original_form": "저도 고민했는데 배송만 늦춰졌네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000571.1.20",
          "form": "맞아요 좋긴 좋아요!",
          "original_form": "맞아요 좋긴 좋아요!",
