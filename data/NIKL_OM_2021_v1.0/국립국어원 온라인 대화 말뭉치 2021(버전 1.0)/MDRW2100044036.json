      "utterance": [
        {
          "id": "MDRW2100044036.1.1",
          "form": "그게.사진으로 보는거랑 다르대요",
          "original_form": "그게.사진으로 보는거랑 다르대요",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044036.1.2",
          "form": "맞아요 그렇다더라구요",
          "original_form": "맞아요 그렇다더라구요",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044036.1.3",
          "form": "그 규모에 압도 당한대요",
          "original_form": "그 규모에 압도 당한대요",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044036.1.4",
          "form": "저는 사진기도 들고가서",
          "original_form": "저는 사진기도 들고가서",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044036.1.5",
          "form": "한번 사진 직접 찍어보고 싶어요",
          "original_form": "한번 사진 직접 찍어보고 싶어요",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044036.1.6",
          "form": "사진 잘 찍으시나봐요~",
          "original_form": "사진 잘 찍으시나봐요~",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044036.1.7",
          "form": "아뇨 ㅋㅋㅋㅋ 그냥 이렇게 생각해두면",
          "original_form": "아뇨 ㅋㅋㅋㅋ 그냥 이렇게 생각해두면",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044036.1.8",
          "form": "언젠간 배우지 않을까 해서요",
          "original_form": "언젠간 배우지 않을까 해서요",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044036.1.9",
          "form": "ㅋㅋㅋㅋ 저는.신랑이.사진 좋아해서 여행갈때 그 큰 카메라를 들고 가요 ㅡ.ㅡ 완전 짐 ㅠ",
          "original_form": "ㅋㅋㅋㅋ 저는.신랑이.사진 좋아해서 여행갈때 그 큰 카메라를 들고 가요 ㅡ.ㅡ 완전 짐 ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044036.1.10",
          "form": "앗 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044036.1.11",
          "form": "그래도 잘 찍으시면 나중에 다시 볼 때 좋겠네요",
          "original_form": "그래도 잘 찍으시면 나중에 다시 볼 때 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044036.1.12",
          "form": "아무래도 그렇겠죠?? 오로라 보러 갈 생각하면 막 신나요^-^",
          "original_form": "아무래도 그렇겠죠?? 오로라 보러 갈 생각하면 막 신나요^-^",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044036.1.13",
          "form": "코로나 끝나고 함 갔다오세요",
          "original_form": "코로나 끝나고 함 갔다오세요",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044036.1.14",
          "form": "안끝나도 23년에.가려고요 ㅋ",
          "original_form": "안끝나도 23년에.가려고요 ㅋ",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044036.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ 벌써 위드 코로나 얘기 나오는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 벌써 위드 코로나 얘기 나오는데",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044036.1.16",
          "form": "23년이면 여행 활발히 다닐거같아요",
          "original_form": "23년이면 여행 활발히 다닐거같아요",
          "speaker_id": "1",
          "time": "20210907 21:29"
