      "utterance": [
        {
          "id": "MDRW2100032699.1.1",
          "form": "저녁드셨나요??",
          "original_form": "저녁드셨나요??",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100032699.1.2",
          "form": "이제 먹으려구요.",
          "original_form": "이제 먹으려구요.",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100032699.1.3",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100032699.1.4",
          "form": "네 저는 라면끓여먹었어요!",
          "original_form": "네 저는 라면끓여먹었어요!",
          "speaker_id": "1",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100032699.1.5",
          "form": "오랜만은아니고 짜파게티 먹었는데 맛있더라구요",
          "original_form": "오랜만은아니고 짜파게티 먹었는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100032699.1.6",
          "form": "짜파게티, 오랜만에 먹으면 맛있죠",
          "original_form": "짜파게티, 오랜만에 먹으면 맛있죠",
          "speaker_id": "2",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100032699.1.7",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100032699.1.8",
          "form": "그래서 더 맛있더라구요 ㅎㅎ",
          "original_form": "그래서 더 맛있더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100032699.1.9",
          "form": "전 걍 밑반찬-장아찌에 김치-해서 밥 먹었어요",
          "original_form": "전 걍 밑반찬-장아찌에 김치-해서 밥 먹었어요",
          "speaker_id": "2",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100032699.1.10",
          "form": "짱아찌 맛있죠 ㅎㅎㅎ",
          "original_form": "짱아찌 맛있죠 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100032699.1.11",
          "form": "저는 주로 깻잎좋아해요~!!",
          "original_form": "저는 주로 깻잎좋아해요~!!",
          "speaker_id": "1",
          "time": "20210906 18:52"
        },
        {
          "id": "MDRW2100032699.1.12",
          "form": "깻잎 맛있죠",
          "original_form": "깻잎 맛있죠",
          "speaker_id": "2",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100032699.1.13",
          "form": "전 마늘쫑장아찌 좋아해요 ㅎㅎ",
          "original_form": "전 마늘쫑장아찌 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100032699.1.14",
          "form": "와 마늘쫑도 맛있죠!!",
          "original_form": "와 마늘쫑도 맛있죠!!",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100032699.1.15",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100032699.1.16",
          "form": "저는 내일 저녁은 나가서 먹을 예정이에요",
          "original_form": "저는 내일 저녁은 나가서 먹을 예정이에요",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100032699.1.17",
          "form": "내일 드실거 정한거 있나요??",
          "original_form": "내일 드실거 정한거 있나요??",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100032699.1.18",
          "form": "순두부 끓였어요",
          "original_form": "순두부 끓였어요",
          "speaker_id": "2",
          "time": "20210906 23:28"
        },
        {
          "id": "MDRW2100032699.1.19",
          "form": "그거랑 계란후라이 정도나 할까해요 ㅎㅎ",
          "original_form": "그거랑 계란후라이 정도나 할까해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 23:30"
        },
        {
          "id": "MDRW2100032699.1.20",
          "form": "순두부 ㅠㅠ 계란 한뭉터기로 들어가있는게 잔 그렇게 맛있더라구요",
          "original_form": "순두부 ㅠㅠ 계란 한뭉터기로 들어가있는게 잔 그렇게 맛있더라구요",
          "speaker_id": "1",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100032699.1.21",
          "form": "너무 맛있겠어요!!",
          "original_form": "너무 맛있겠어요!!",
          "speaker_id": "1",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100032699.1.22",
          "form": "순두부열라면도 드셔보셨나요??",
          "original_form": "순두부열라면도 드셔보셨나요??",
          "speaker_id": "1",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100032699.1.23",
          "form": "아! ㅋㅋㅋㅋ 해봤어요ㅈ",
          "original_form": "아! ㅋㅋㅋㅋ 해봤어요ㅈ",
          "speaker_id": "2",
          "time": "20210907 08:43"
        },
        {
          "id": "MDRW2100032699.1.24",
          "form": "근데 그거 하고나서 바로 먹어야하더라구요",
          "original_form": "근데 그거 하고나서 바로 먹어야하더라구요",
          "speaker_id": "2",
          "time": "20210907 08:43"
        },
        {
          "id": "MDRW2100032699.1.25",
          "form": "면만 다 건져먹은뒤 순두부는 다음 끼니에 밥이랑 먹어야지 했더니",
          "original_form": "면만 다 건져먹은뒤 순두부는 다음 끼니에 밥이랑 먹어야지 했더니",
          "speaker_id": "2",
          "time": "20210907 08:44"
        },
        {
          "id": "MDRW2100032699.1.26",
          "form": "이상하게 뒷맛이 느끼해졌다고 해야하나...?",
          "original_form": "이상하게 뒷맛이 느끼해졌다고 해야하나...?",
          "speaker_id": "2",
          "time": "20210907 08:44"
        },
        {
          "id": "MDRW2100032699.1.27",
          "form": "헉 그렇군요",
          "original_form": "헉 그렇군요",
          "speaker_id": "1",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100032699.1.28",
          "form": "ㅠㅠㅠ그건 너무 아쉽네요",
          "original_form": "ㅠㅠㅠ그건 너무 아쉽네요",
          "speaker_id": "1",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100032699.1.29",
          "form": "한번에 다먹는게 가장 좋겠어요",
          "original_form": "한번에 다먹는게 가장 좋겠어요",
          "speaker_id": "1",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100032699.1.30",
          "form": "점심은 드셨나요??",
          "original_form": "점심은 드셨나요??",
          "speaker_id": "1",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100032699.1.31",
          "form": "네, 좀전에 먹었어요",
          "original_form": "네, 좀전에 먹었어요",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100032699.1.32",
          "form": "name1맛님은요?",
          "original_form": "&name1&맛님은요?",
          "speaker_id": "2",
          "time": "20210907 13:56"
        },
        {
          "id": "MDRW2100032699.1.33",
          "form": "저도 먹었어요!",
          "original_form": "저도 먹었어요!",
          "speaker_id": "1",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100032699.1.34",
          "form": "계란프라이해먹었어요 ㅎㅎ",
          "original_form": "계란프라이해먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100032699.1.35",
          "form": "뭐 드셨나요??",
          "original_form": "뭐 드셨나요??",
          "speaker_id": "1",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100032699.1.36",
          "form": "굴비 전에 구운거 쬐금 남아서 구르길래 ㅎㅎ",
          "original_form": "굴비 전에 구운거 쬐금 남아서 구르길래 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:57"
        },
        {
          "id": "MDRW2100032699.1.37",
          "form": "그거 뎁혀서 밑반찬이랑 먹었어요 ㅎㅎ",
          "original_form": "그거 뎁혀서 밑반찬이랑 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:58"
        },
        {
          "id": "MDRW2100032699.1.38",
          "form": "굴비 너무 맛있죠 ㅎㅎ",
          "original_form": "굴비 너무 맛있죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100032699.1.39",
          "form": "저는 저녁에 보쌈을 먹을 예정이에요 ㅎㅎ",
          "original_form": "저는 저녁에 보쌈을 먹을 예정이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100032699.1.40",
          "form": "신나네요",
          "original_form": "신나네요",
          "speaker_id": "1",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100032699.1.41",
          "form": "우왕, 맛있겠어요",
          "original_form": "우왕, 맛있겠어요",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100032699.1.42",
          "form": "네 낙지가 올라가있는데 저번에 한번 먹으러갔ㄷ너무 맛있어서 다시 가려구요",
          "original_form": "네 낙지가 올라가있는데 저번에 한번 먹으러갔ㄷ너무 맛있어서 다시 가려구요",
          "speaker_id": "1",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100032699.1.43",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100032699.1.44",
          "form": "비싸고 약간 느끼하지만 ㄱㅇ래도 먹을때 맛있으니까 잘먹고오려구요",
          "original_form": "비싸고 약간 느끼하지만 ㄱㅇ래도 먹을때 맛있으니까 잘먹고오려구요",
          "speaker_id": "1",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100032699.1.45",
          "form": "헉? 낙지도 있어요?",
          "original_form": "헉? 낙지도 있어요?",
          "speaker_id": "2",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100032699.1.46",
          "form": "진심 존맛이겠다",
          "original_form": "진심 존맛이겠다",
          "speaker_id": "2",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100032699.1.47",
          "form": "그죠 ㅠㅠ",
          "original_form": "그죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100032699.1.48",
          "form": "낙지가 있어서 덜느끼하고 너무맛있어요",
          "original_form": "낙지가 있어서 덜느끼하고 너무맛있어요",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100032699.1.49",
          "form": "진짜...쩝쩝박사님들 너무 많아서 좋아요!!",
          "original_form": "진짜...쩝쩝박사님들 너무 많아서 좋아요!!",
          "speaker_id": "1",
          "time": "20210907 15:42"
        }
