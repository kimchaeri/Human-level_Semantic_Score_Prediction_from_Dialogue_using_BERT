        {
          "id": "MDRW2100014872.1.49",
          "form": "많이 다르니",
          "original_form": "많이 다르니",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100014872.1.50",
          "form": "아 근데 아까 내가 말핰",
          "original_form": "아 근데 아까 내가 말핰",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100014872.1.51",
          "form": "AI를 활용하는 것을 키운다고 하면",
          "original_form": "AI를 활용하는 것을 키운다고 하면",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
