      "utterance": [
        {
          "id": "MDRW2100040007.1.1",
          "form": "외관상 늙기전까진",
          "original_form": "외관상 늙기전까진",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100040007.1.2",
          "form": "그걸 못알아채",
          "original_form": "그걸 못알아채",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100040007.1.3",
          "form": "이게 군대냐 진짜",
          "original_form": "이게 군대냐 진짜",
          "speaker_id": "2",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100040007.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100040007.1.5",
          "form": "난 솔직히",
          "original_form": "난 솔직히",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100040007.1.6",
          "form": "텟페이 미화되는 것도 싫은게",
          "original_form": "텟페이 미화되는 것도 싫은게",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100040007.1.7",
          "form": "걔 처음 등장할 때부터",
          "original_form": "걔 처음 등장할 때부터",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100040007.1.8",
          "form": "ㅇㅇ애초에 걘",
          "original_form": "ㅇㅇ애초에 걘",
          "speaker_id": "2",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100040007.1.9",
          "form": "주인공한테 자기가 선배라고 가르쳐주고싶어하고",
          "original_form": "주인공한테 자기가 선배라고 가르쳐주고싶어하고",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100040007.1.10",
          "form": "주인공이랑 경쟁할 급도 아니었는데 자꾸",
          "original_form": "주인공이랑 경쟁할 급도 아니었는데 자꾸",
          "speaker_id": "2",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100040007.1.11",
          "form": "주인공이 대장직 얻으니까 자기도 한자리 꿰타고 싶어하고",
          "original_form": "주인공이 대장직 얻으니까 자기도 한자리 꿰타고 싶어하고",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100040007.1.12",
          "form": "지 혼자 라이벌선언해대는거",
          "original_form": "지 혼자 라이벌선언해대는거",
          "speaker_id": "2",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100040007.1.13",
          "form": "ㅇㅇ 애초에 경쟁할 급이 아닌데 ㅋㅋㅋ?",
          "original_form": "ㅇㅇ 애초에 경쟁할 급이 아닌데 ㅋㅋㅋ?",
          "speaker_id": "1",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100040007.1.14",
          "form": "아주 꼴보기싫었음",
          "original_form": "아주 꼴보기싫었음",
          "speaker_id": "2",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100040007.1.15",
          "form": "근데 신의눈 가지고",
          "original_form": "근데 신의눈 가지고",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100040007.1.16",
          "form": "인간의 성장 한정 시키겠단거 진짜면",
          "original_form": "인간의 성장 한정 시키겠단거 진짜면",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100040007.1.17",
          "form": "크게 보면 잘 먹히고 있는거 맞는듯",
          "original_form": "크게 보면 잘 먹히고 있는거 맞는듯",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100040007.1.18",
          "form": "신의눈 안줬다고 저러고 죽어가잖아",
          "original_form": "신의눈 안줬다고 저러고 죽어가잖아",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100040007.1.19",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100040007.1.20",
          "form": "더 보면",
          "original_form": "더 보면",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100040007.1.21",
          "form": "ㅎ ㅏ",
          "original_form": "ㅎ ㅏ",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100040007.1.22",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100040007.1.23",
          "form": "이 스토리가 왜 욕먹는지 알 수 잇음....",
          "original_form": "이 스토리가 왜 욕먹는지 알 수 잇음....",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100040007.1.24",
          "form": "뭔일이 일어나는것이지",
          "original_form": "뭔일이 일어나는것이지",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100040007.1.25",
          "form": "그래도 이전 업뎃까지는",
          "original_form": "그래도 이전 업뎃까지는",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100040007.1.26",
          "form": "일단 당장은 주인공이",
          "original_form": "일단 당장은 주인공이",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100040007.1.27",
          "form": "우인단 이놈들...!",
          "original_form": "우인단 이놈들...!",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100040007.1.28",
          "form": "하고 깐지나게",
          "original_form": "하고 깐지나게",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100040007.1.29",
          "form": "초반이니까 이런거겠지 햇는데...",
          "original_form": "초반이니까 이런거겠지 햇는데...",
          "speaker_id": "1",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100040007.1.30",
          "form": "달려갔는데",
          "original_form": "달려갔는데",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100040007.1.31",
          "form": "스카라무슈 만남?",
          "original_form": "스카라무슈 만남?",
          "speaker_id": "1",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100040007.1.32",
          "form": "아니 걍",
          "original_form": "아니 걍",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100040007.1.33",
          "form": "뛰어가는것까지 봤어",
          "original_form": "뛰어가는것까지 봤어",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100040007.1.34",
          "form": "그러고 잠깐 쉬는중",
          "original_form": "그러고 잠깐 쉬는중",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100040007.1.35",
          "form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100040007.1.36",
          "form": "사안공장은 아직 안들어간거지?",
          "original_form": "사안공장은 아직 안들어간거지?",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100040007.1.37",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100040007.1.38",
          "form": "설마 또",
          "original_form": "설마 또",
          "speaker_id": "2",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100040007.1.39",
          "form": "나쁜건 모두 우인단이었고",
          "original_form": "나쁜건 모두 우인단이었고",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.40",
          "form": "우리는 서로 오해가 있었을뿐",
          "original_form": "우리는 서로 오해가 있었을뿐",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.41",
          "form": "이런 전개는",
          "original_form": "이런 전개는",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.42",
          "form": "아니겠지",
          "original_form": "아니겠지",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.43",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.44",
          "form": "아주 틀린건 아닌데",
          "original_form": "아주 틀린건 아닌데",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.45",
          "form": "약간... 다르고",
          "original_form": "약간... 다르고",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.46",
          "form": "잰장",
          "original_form": "잰장",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.47",
          "form": "암튼 봐바 ㅎ",
          "original_form": "암튼 봐바 ㅎ",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.48",
          "form": "왜지",
          "original_form": "왜지",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.49",
          "form": "이게 대충",
          "original_form": "이게 대충",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.50",
          "form": "틀리길 바랐는데",
          "original_form": "틀리길 바랐는데",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.51",
          "form": "사안공장 들어가서 스카라무슈 만나는거 기쥰으로",
          "original_form": "사안공장 들어가서 스카라무슈 만나는거 기쥰으로",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100040007.1.52",
          "form": "1부/2부 나뉘는 느낌임",
          "original_form": "1부/2부 나뉘는 느낌임",
          "speaker_id": "1",
          "time": "20210904 00:20"
        },
        {
          "id": "MDRW2100040007.1.53",
          "form": "ㅇㅋ.......",
          "original_form": "ㅇㅋ.......",
          "speaker_id": "2",
          "time": "20210904 00:20"
        }
      ]
