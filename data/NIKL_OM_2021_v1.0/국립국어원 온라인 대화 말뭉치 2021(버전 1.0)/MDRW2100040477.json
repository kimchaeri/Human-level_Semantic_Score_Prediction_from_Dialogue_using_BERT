      "utterance": [
        {
          "id": "MDRW2100040477.1.1",
          "form": "백신 맞고나니 식욕이 왕성해져요",
          "original_form": "백신 맞고나니 식욕이 왕성해져요",
          "speaker_id": "1",
          "time": "20210904 21:29"
        },
        {
          "id": "MDRW2100040477.1.2",
          "form": "특히 간식류를 엄청 먹었어요",
          "original_form": "특히 간식류를 엄청 먹었어요",
          "speaker_id": "1",
          "time": "20210904 21:29"
        },
        {
          "id": "MDRW2100040477.1.3",
          "form": "달달구리 좋아하시나요?",
          "original_form": "달달구리 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100040477.1.4",
          "form": "달달한 게 당기는 시기가 있는 것 같아요",
          "original_form": "달달한 게 당기는 시기가 있는 것 같아요",
          "speaker_id": "1",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100040477.1.5",
          "form": "백신 맞고서는 1차때도 이번에도 달달한 게 당기네요",
          "original_form": "백신 맞고서는 1차때도 이번에도 달달한 게 당기네요",
          "speaker_id": "1",
          "time": "20210904 21:34"
        },
        {
          "id": "MDRW2100040477.1.6",
          "form": "어떤거 드셨나요??",
          "original_form": "어떤거 드셨나요??",
          "speaker_id": "2",
          "time": "20210904 21:38"
        },
        {
          "id": "MDRW2100040477.1.7",
          "form": "아이스크림을 2개나 먹었어요",
          "original_form": "아이스크림을 2개나 먹었어요",
          "speaker_id": "1",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100040477.1.8",
          "form": "아이스크림 좋아하시나요??",
          "original_form": "아이스크림 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100040477.1.9",
          "form": "당길 때가 있더라고요",
          "original_form": "당길 때가 있더라고요",
          "speaker_id": "1",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100040477.1.10",
          "form": "주로 마법에 걸릴때 먹어요",
          "original_form": "주로 마법에 걸릴때 먹어요",
          "speaker_id": "1",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100040477.1.11",
          "form": "아하 그때 단게 자주 땡기죠 ;)",
          "original_form": "아하 그때 단게 자주 땡기죠 ;)",
          "speaker_id": "2",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100040477.1.12",
          "form": "바? 쭈쭈바? 콘? 어떤거 좋아하세요?",
          "original_form": "바? 쭈쭈바? 콘? 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100040477.1.13",
          "form": "네 그땐 초코과자나 아이스크림이 당기더라고요",
          "original_form": "네 그땐 초코과자나 아이스크림이 당기더라고요",
          "speaker_id": "1",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100040477.1.14",
          "form": "돼지 바",
          "original_form": "돼지 바",
          "speaker_id": "1",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100040477.1.15",
          "form": "가 당기더라고요 오늘은",
          "original_form": "가 당기더라고요 오늘은",
          "speaker_id": "1",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100040477.1.16",
          "form": "초콜릿 들어간게 되게 좋죠 그 시기에",
          "original_form": "초콜릿 들어간게 되게 좋죠 그 시기에",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100040477.1.17",
          "form": "저는 오늘은 메로나요 ㅎㅎ",
          "original_form": "저는 오늘은 메로나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100040477.1.18",
          "form": "베라에서 어떤맛 주로 드시나요?",
          "original_form": "베라에서 어떤맛 주로 드시나요?",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100040477.1.19",
          "form": "바람과 함께 사라지다",
          "original_form": "바람과 함께 사라지다",
          "speaker_id": "1",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100040477.1.20",
          "form": "아몬드봉봉",
          "original_form": "아몬드봉봉",
          "speaker_id": "1",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100040477.1.21",
          "form": "제일 좋아해요",
          "original_form": "제일 좋아해요",
          "speaker_id": "1",
          "time": "20210904 22:20"
        },
        {
          "id": "MDRW2100040477.1.22",
          "form": "나머진 그때그때 조금씩 다르고요",
          "original_form": "나머진 그때그때 조금씩 다르고요",
          "speaker_id": "1",
          "time": "20210904 22:20"
        },
        {
          "id": "MDRW2100040477.1.23",
          "form": "제 최애 아이스크림이 바람과 함께 사라지다에요",
          "original_form": "제 최애 아이스크림이 바람과 함께 사라지다에요{emoji:♡}",
          "speaker_id": "2",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100040477.1.24",
          "form": "진짜 최곤데 ㅠ 다들 초코 아닌데 왜 먹냐고 자주 그래요 ㅠ",
          "original_form": "진짜 최곤데 ㅠ 다들 초코 아닌데 왜 먹냐고 자주 그래요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100040477.1.25",
          "form": "바람과 함께 사라지다가 얼마나 맛있는데요",
          "original_form": "바람과 함께 사라지다가 얼마나 맛있는데요",
          "speaker_id": "1",
          "time": "20210904 22:24"
