      "utterance": [
        {
          "id": "MDRW2100004147.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004147.1.2",
          "form": "안녕하세요 ~!! 저는 요즘 소고기국밥에 빠져있어요 ㅎㅎ",
          "original_form": "안녕하세요 ~!! 저는 요즘 소고기국밥에 빠져있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004147.1.3",
          "form": "오 저 소고기 국밥 좋아해요",
          "original_form": "오 저 소고기 국밥 좋아해요",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004147.1.4",
          "form": "전 빨간거로요 ㅎㅎ",
          "original_form": "전 빨간거로요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004147.1.5",
          "form": "하얀거 좋아하세요?",
          "original_form": "하얀거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004147.1.6",
          "form": "전 옛날 시골 잔치에 먹던 빨간 국밥용 ㅎㅎ",
          "original_form": "전 옛날 시골 잔치에 먹던 빨간 국밥용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004147.1.7",
          "form": "점심때 메뉴로 드세요? 오늘도 드셨어요?",
          "original_form": "점심때 메뉴로 드세요? 오늘도 드셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004147.1.8",
          "form": "전 며칠전 순대국밥 사먹었어요 ㅎㅎ",
          "original_form": "전 며칠전 순대국밥 사먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004147.1.9",
          "form": "빨간거요! 드실줄 아시네요 ㅎㅎㅎ",
          "original_form": "빨간거요! 드실줄 아시네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004147.1.10",
          "form": "오 제대로시네요",
          "original_form": "오 제대로시네요",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004147.1.11",
          "form": "오늘 점심은 그냥 집에서 반찬 꺼내 먹었어요",
          "original_form": "오늘 점심은 그냥 집에서 반찬 꺼내 먹었어요",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004147.1.12",
          "form": "저 어릴적 시골맛을 못잊어요",
          "original_form": "저 어릴적 시골맛을 못잊어요",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004147.1.13",
          "form": "집에선 그렇죠 저도 대충먹어요",
          "original_form": "집에선 그렇죠 저도 대충먹어요",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004147.1.14",
          "form": "헐.. name1님이 드신 국밥맛이 너무 궁금하네요",
          "original_form": "헐.. &name1&님이 드신 국밥맛이 너무 궁금하네요",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004147.1.15",
          "form": "엄청 맛있을거같은.. ㅎㅎ",
          "original_form": "엄청 맛있을거같은.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004147.1.16",
          "form": "그게 진짜 바로잡은고기를 넣어 큰솥애 아",
          "original_form": "그게 진짜 바로잡은고기를 넣어 큰솥애 아",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004147.1.17",
          "form": "진짜 말로 못해요",
          "original_form": "진짜 말로 못해요",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004147.1.18",
          "form": "최고입니다",
          "original_form": "최고입니다",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004147.1.19",
          "form": "솥이 틀려요",
          "original_form": "솥이 틀려요",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004147.1.20",
          "form": "ㅋㅋㅋㅋㅋ 으아 말만 들어도 군침이",
          "original_form": "ㅋㅋㅋㅋㅋ 으아 말만 들어도 군침이",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004147.1.21",
          "form": "예전에 해운대가서 먹었던 소고기국밥이랑 약간 비슷한 느낌이었을지도",
          "original_form": "예전에 해운대가서 먹었던 소고기국밥이랑 약간 비슷한 느낌이었을지도",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004147.1.22",
          "form": "지금은 맛이 변했더라구여 ㅎ",
          "original_form": "지금은 맛이 변했더라구여 ㅎ",
