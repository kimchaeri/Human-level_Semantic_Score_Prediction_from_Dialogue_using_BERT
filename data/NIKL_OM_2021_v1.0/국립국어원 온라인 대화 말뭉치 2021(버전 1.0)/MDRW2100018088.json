        {
          "id": "MDRW2100018088.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018088.1.3",
          "form": "종교가 있으신가요",
          "original_form": "종교가 있으신가요",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018088.1.4",
          "form": "저는 무교에요",
          "original_form": "저는 무교에요",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018088.1.5",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018088.1.6",
          "form": "저도 무교입니다",
          "original_form": "저도 무교입니다",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018088.1.7",
          "form": "종교활동에 대해서",
          "original_form": "종교활동에 대해서",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018088.1.8",
          "form": "어떻게 생각하세요?",
          "original_form": "어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018088.1.9",
          "form": "글쎄요 요즘에는 줄이는 게 좋다고 생각해요",
          "original_form": "글쎄요 요즘에는 줄이는 게 좋다고 생각해요",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018088.1.10",
          "form": "종교를 안믿으시는 이유라도 따로 있나요",
          "original_form": "종교를 안믿으시는 이유라도 따로 있나요",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018088.1.11",
          "form": "그냥 집 자체가 무교라서 그런거같아요",
          "original_form": "그냥 집 자체가 무교라서 그런거같아요",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018088.1.12",
          "form": "저도 집 자체가 무교라서 그런거같아요",
          "original_form": "저도 집 자체가 무교라서 그런거같아요",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018088.1.13",
          "form": "저랑 같으시네요 ㅋㅋ",
          "original_form": "저랑 같으시네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018088.1.14",
          "form": "가족 모두 무교인가요?",
          "original_form": "가족 모두 무교인가요?",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018088.1.15",
          "form": "예전에는 천주교였는데 어쩌다보니",
          "original_form": "예전에는 천주교였는데 어쩌다보니",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018088.1.16",
          "form": "다들 무교가 된거 같아요",
          "original_form": "다들 무교가 된거 같아요",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100018088.1.17",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018088.1.18",
          "form": "없는게 더 나으신가요?",
          "original_form": "없는게 더 나으신가요?",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018088.1.19",
          "form": "어릴때는 종교에 별 생각이 없었어서",
          "original_form": "어릴때는 종교에 별 생각이 없었어서",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100018088.1.20",
          "form": "없는게 맘편한거 같아요",
          "original_form": "없는게 맘편한거 같아요",
