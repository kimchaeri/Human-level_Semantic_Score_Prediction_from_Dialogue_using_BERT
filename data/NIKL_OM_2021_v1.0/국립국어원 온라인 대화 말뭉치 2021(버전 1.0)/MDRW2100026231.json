        {
          "id": "MDRW2100026231.1.1",
          "form": "코로나 끝나고 어디로 여행가고 싶으신가요?",
          "original_form": "코로나 끝나고 어디로 여행가고 싶으신가요?",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026231.1.2",
          "form": "전 해외로 나가보고 싶어요~",
          "original_form": "전 해외로 나가보고 싶어요~",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026231.1.3",
          "form": "비행기 타고 싶어요ㅋㅋㅋ",
          "original_form": "비행기 타고 싶어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100026231.1.4",
          "form": "그쵸 못가니 뭔가 더 가고 싶은 욕구가 생겨요",
          "original_form": "그쵸 못가니 뭔가 더 가고 싶은 욕구가 생겨요",
          "speaker_id": "1",
          "time": "20210901 20:57"
        },
        {
          "id": "MDRW2100026231.1.5",
          "form": "맞아요 코로나 끝나고 해외 가면",
          "original_form": "맞아요 코로나 끝나고 해외 가면",
          "speaker_id": "2",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100026231.1.6",
          "form": "얼마나 좋을까요?",
          "original_form": "얼마나 좋을까요?",
          "speaker_id": "2",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100026231.1.7",
          "form": "여행 계획 있으세요?",
          "original_form": "여행 계획 있으세요?",
          "speaker_id": "2",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026231.1.8",
          "form": "전 가족이 해외에 있어서 얼른 보고 싶어요",
          "original_form": "전 가족이 해외에 있어서 얼른 보고 싶어요",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026231.1.9",
          "form": "중국하고 베트남 찍고 유럽갔다 한국오는 코스요ㅋㅋ",
          "original_form": "중국하고 베트남 찍고 유럽갔다 한국오는 코스요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026231.1.10",
          "form": "아 그러시구나~",
          "original_form": "아 그러시구나~",
          "speaker_id": "2",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026231.1.11",
          "form": "안되면 중국 이태리 한국이요",
          "original_form": "안되면 중국 이태리 한국이요",
          "speaker_id": "1",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026231.1.12",
          "form": "너무 보고 싶겠어요ㅜ",
          "original_form": "너무 보고 싶겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100026231.1.13",
          "form": "코스가 디테일하군요ㅋㅋㅋㅋ",
          "original_form": "코스가 디테일하군요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026231.1.14",
          "form": "ㅋㅋ 미리 짜보고 변경하고 그러거든요ㅋㅋ",
          "original_form": "ㅋㅋ 미리 짜보고 변경하고 그러거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026231.1.15",
          "form": "가고 싶으니 계획만 한가득이네요",
          "original_form": "가고 싶으니 계획만 한가득이네요",
          "speaker_id": "1",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026231.1.16",
          "form": "여행은 계획하는 것만 해도 엄청 신나요 그죠??",
          "original_form": "여행은 계획하는 것만 해도 엄청 신나요 그죠??",
