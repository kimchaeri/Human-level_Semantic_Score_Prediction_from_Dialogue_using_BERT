      "utterance": [
        {
          "id": "MDRW2100033055.1.1",
          "form": "아아아~~",
          "original_form": "아아아~~",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100033055.1.2",
          "form": "좋네요 ㅋㅋㅋ",
          "original_form": "좋네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100033055.1.3",
          "form": "저는 기다리고 있는건 없는데 ㅋㅋㅋ 후기 한번 보고 보러 가보려구요",
          "original_form": "저는 기다리고 있는건 없는데 ㅋㅋㅋ 후기 한번 보고 보러 가보려구요",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100033055.1.4",
          "form": "다니엘크레이그 나온 007을 너무 재밌게봐서",
          "original_form": "다니엘크레이그 나온 007을 너무 재밌게봐서",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100033055.1.5",
          "form": "개봉 기다리는 영화 있나요?",
          "original_form": "개봉 기다리는 영화 있나요?",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100033055.1.6",
          "form": "아니요 ㅋㅋ 기다리고 있는건 없어요 ㅋㅋ",
          "original_form": "아니요 ㅋㅋ 기다리고 있는건 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100033055.1.7",
          "form": "예매1위라고 하면 ㅋㅋ 그때 궁금해서 보는편이에요",
          "original_form": "예매1위라고 하면 ㅋㅋ 그때 궁금해서 보는편이에요",
          "speaker_id": "2",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100033055.1.8",
          "form": "아 그렇구나 주로 누구랑 영화보는 편이에요?",
          "original_form": "아 그렇구나 주로 누구랑 영화보는 편이에요?",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100033055.1.9",
          "form": "요즘에는 신랑이랑 한번씩 영화관가요",
          "original_form": "요즘에는 신랑이랑 한번씩 영화관가요",
          "speaker_id": "2",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100033055.1.10",
          "form": "저는 아내랑 작년에 영화관에서 1917 봤는데",
          "original_form": "저는 아내랑 작년에 영화관에서 1917 봤는데",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100033055.1.11",
          "form": "작년 올해 합쳐서 유일하게 영화관 간 거에요",
          "original_form": "작년 올해 합쳐서 유일하게 영화관 간 거에요",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100033055.1.12",
          "form": "좋아하는 배우 있나요?",
          "original_form": "좋아하는 배우 있나요?",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100033055.1.13",
          "form": "신작 나올때마다 챙겨보는",
          "original_form": "신작 나올때마다 챙겨보는",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100033055.1.14",
          "form": "네네 좋아하는 배우는 너무 많아요",
          "original_form": "네네 좋아하는 배우는 너무 많아요",
          "speaker_id": "2",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100033055.1.15",
          "form": "누구 좋아하세요?",
          "original_form": "누구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 15:05"
        },
        {
          "id": "MDRW2100033055.1.16",
          "form": "음..........ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ송중기요",
          "original_form": "음..........ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ송중기요",
          "speaker_id": "2",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100033055.1.17",
          "form": "누구좋아하세요?",
          "original_form": "누구좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 15:07"
        },
        {
          "id": "MDRW2100033055.1.18",
          "form": "저는 음... 떠오르는 사람이 없네요...",
          "original_form": "저는 음... 떠오르는 사람이 없네요...",
          "speaker_id": "1",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100033055.1.19",
          "form": "그때그때 자꾸 바뀌어서",
          "original_form": "그때그때 자꾸 바뀌어서",
          "speaker_id": "1",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100033055.1.20",
          "form": "히로스에 료코",
          "original_form": "히로스에 료코",
          "speaker_id": "1",
          "time": "20210906 15:08"
        },
        {
          "id": "MDRW2100033055.1.21",
          "form": "우에노 쥬리",
          "original_form": "우에노 쥬리",
          "speaker_id": "1",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100033055.1.22",
          "form": "이런 느낌인데",
          "original_form": "이런 느낌인데",
          "speaker_id": "1",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100033055.1.23",
          "form": "ㅋㅋㅋ저도 ㅋㅋㅋ 그때 그때 달라요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ저도 ㅋㅋㅋ 그때 그때 달라요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:09"
        },
        {
          "id": "MDRW2100033055.1.24",
          "form": "영화관은 얼마나 자주 다니세요?",
          "original_form": "영화관은 얼마나 자주 다니세요?",
          "speaker_id": "1",
          "time": "20210906 15:23"
        },
        {
          "id": "MDRW2100033055.1.25",
          "form": "코로나때문에 자주 못갔는데",
          "original_form": "코로나때문에 자주 못갔는데",
          "speaker_id": "2",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100033055.1.26",
          "form": "최근에 한번 가니깐은 사람들 거의 없더라구요",
          "original_form": "최근에 한번 가니깐은 사람들 거의 없더라구요",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100033055.1.27",
          "form": "그래서 신랑이랑 이정도면은 한번씩 영화관에 가도 되겠다고 생각을 했어욬 ㅋ",
          "original_form": "그래서 신랑이랑 이정도면은 한번씩 영화관에 가도 되겠다고 생각을 했어욬 ㅋ",
          "speaker_id": "2",
          "time": "20210906 15:25"
        }
      ]
