      "utterance": [
        {
          "id": "MDRW2100018404.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 01:10"
        },
        {
          "id": "MDRW2100018404.1.2",
          "form": "네 한때 클래식공연 많이봤어요",
          "original_form": "네 한때 클래식공연 많이봤어요",
          "speaker_id": "2",
          "time": "20210827 01:31"
        },
        {
          "id": "MDRW2100018404.1.3",
          "form": "아하 클래식 좋아하시는구나.",
          "original_form": "아하 클래식 좋아하시는구나.",
          "speaker_id": "1",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018404.1.4",
          "form": "저는 딱 한번 봤는데 이름이 잘 기억이 안나네요 ㅠㅠ 피아니스트 내한공연이었는데",
          "original_form": "저는 딱 한번 봤는데 이름이 잘 기억이 안나네요 ㅠㅠ 피아니스트 내한공연이었는데",
          "speaker_id": "1",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100018404.1.5",
          "form": "그러사군요 어떤 장르 공연 좋아하세요?",
          "original_form": "그러사군요 어떤 장르 공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018404.1.6",
          "form": "저는 뮤지컬 좋아해요. 요즘엔 코로나 때문에 잘 못갔네요 ㅠㅠ",
          "original_form": "저는 뮤지컬 좋아해요. 요즘엔 코로나 때문에 잘 못갔네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018404.1.7",
          "form": "그러시군요 코로나때매 공연쪽이 힘들겠더군요",
          "original_form": "그러시군요 코로나때매 공연쪽이 힘들겠더군요",
          "speaker_id": "2",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100018404.1.8",
          "form": "맞아요 잘 버텨주길 ㅠㅠ",
          "original_form": "맞아요 잘 버텨주길 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018404.1.9",
          "form": "전시회는 안보시나요?",
          "original_form": "전시회는 안보시나요?",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018404.1.10",
          "form": "네 볼 기회가 많이 없네요",
          "original_form": "네 볼 기회가 많이 없네요",
          "speaker_id": "2",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100018404.1.11",
          "form": "저는 최근에 이건희 전시회 다녀왔는데 볼만했어요",
          "original_form": "저는 최근에 이건희 전시회 다녀왔는데 볼만했어요",
          "speaker_id": "1",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100018404.1.12",
          "form": "작품많다던데 괜찮은가봐요",
          "original_form": "작품많다던데 괜찮은가봐요",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100018404.1.13",
          "form": "네 서울 전시회도 궁금해지더라구요",
          "original_form": "네 서울 전시회도 궁금해지더라구요",
          "speaker_id": "1",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100018404.1.14",
          "form": "어떤 시대 작품 좋아하세요?",
          "original_form": "어떤 시대 작품 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100018404.1.15",
          "form": "저는 이건희 서울전에 이중섭 작품 궁금하더라구요ㅎㅎㅎ",
          "original_form": "저는 이건희 서울전에 이중섭 작품 궁금하더라구요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:07"
        }
