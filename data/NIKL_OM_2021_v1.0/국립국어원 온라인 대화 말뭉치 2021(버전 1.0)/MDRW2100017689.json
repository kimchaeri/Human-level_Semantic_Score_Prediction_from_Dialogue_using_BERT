        {
          "id": "MDRW2100017689.1.1",
          "form": "배달음식 자주 드시나요",
          "original_form": "배달음식 자주 드시나요",
          "speaker_id": "2",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017689.1.2",
          "form": "네 자주먹어요",
          "original_form": "네 자주먹어요",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017689.1.3",
          "form": "뭐 주로 시켜드세요?",
          "original_form": "뭐 주로 시켜드세요?",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017689.1.4",
          "form": "치느님이죠ㅎㅎ",
          "original_form": "치느님이죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017689.1.5",
          "form": "ㅎㅎ  어느 브랜드 주로드세요? 저는 bhc요",
          "original_form": "ㅎㅎ  어느 브랜드 주로드세요? 저는 bhc요",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017689.1.6",
          "form": "저는 교촌ㅎㅎ",
          "original_form": "저는 교촌ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100017689.1.7",
          "form": "교촌 맛있는데 양이 좀 더 많았으면 좋겠어요",
          "original_form": "교촌 맛있는데 양이 좀 더 많았으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100017689.1.8",
          "form": "허니콤보 맛있긴한데ㅠ",
          "original_form": "허니콤보 맛있긴한데ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100017689.1.9",
          "form": "저도 그거 좋아해요 단짠단짠",
          "original_form": "저도 그거 좋아해요 단짠단짠",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100017689.1.10",
          "form": "레드소스 찍어먹으면ㅎㅎ",
          "original_form": "레드소스 찍어먹으면ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:10"
        },
        {
