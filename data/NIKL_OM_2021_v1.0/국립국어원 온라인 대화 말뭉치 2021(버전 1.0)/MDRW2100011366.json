      "utterance": [
        {
          "id": "MDRW2100011366.1.1",
          "form": "가장 최근에 산건 아이돌 앨범이요",
          "original_form": "가장 최근에 산건 아이돌 앨범이요",
          "speaker_id": "2",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100011366.1.2",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100011366.1.3",
          "form": "누구껀가요?",
          "original_form": "누구껀가요?",
          "speaker_id": "1",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100011366.1.4",
          "form": "투모로우바이투게더라고 남자 아이돌이에요",
          "original_form": "투모로우바이투게더라고 남자 아이돌이에요",
          "speaker_id": "2",
          "time": "20210824 11:52"
        },
        {
          "id": "MDRW2100011366.1.5",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210824 12:14"
        },
        {
          "id": "MDRW2100011366.1.6",
          "form": "신인인가요?",
          "original_form": "신인인가요?",
          "speaker_id": "1",
          "time": "20210824 12:14"
        },
        {
          "id": "MDRW2100011366.1.7",
          "form": "2019년에 데뷔해서 신인은 아니에요ㅋㅋ",
          "original_form": "2019년에 데뷔해서 신인은 아니에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:56"
        },
        {
          "id": "MDRW2100011366.1.8",
          "form": "그렇군요 제가잘몰라서요",
          "original_form": "그렇군요 제가잘몰라서요",
          "speaker_id": "1",
          "time": "20210824 13:00"
        },
        {
          "id": "MDRW2100011366.1.9",
          "form": "관심을 가지지 않으면 그 어떤거든 알기 쉽진 않죠",
          "original_form": "관심을 가지지 않으면 그 어떤거든 알기 쉽진 않죠",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011366.1.10",
          "form": "누누님은 최근에 뭐 사셨어요?",
          "original_form": "누누님은 최근에 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011366.1.11",
          "form": "저는 운동화요ㅎㅎㅎ!",
          "original_form": "저는 운동화요ㅎㅎㅎ!",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011366.1.12",
          "form": "어떤 운동화요? 저는 지금 나이키에 눈독들이고 있는데",
          "original_form": "어떤 운동화요? 저는 지금 나이키에 눈독들이고 있는데",
          "speaker_id": "2",
          "time": "20210824 13:36"
        },
        {
          "id": "MDRW2100011366.1.13",
          "form": "전 아디다스요ㅋㅋㅋ",
          "original_form": "전 아디다스요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100011366.1.14",
          "form": "오즈위고샀답니다",
          "original_form": "오즈위고샀답니다",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100011366.1.15",
          "form": "색깔이 전체적으로 차분한게 많네요",
          "original_form": "색깔이 전체적으로 차분한게 많네요",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100011366.1.16",
          "form": "ㅋㅋㅋ 보셨나요?",
          "original_form": "ㅋㅋㅋ 보셨나요?",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100011366.1.17",
          "form": "찾아봤어요ㅎㅎ",
          "original_form": "찾아봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 08:26"
        },
        {
          "id": "MDRW2100011366.1.18",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100011366.1.19",
          "form": "전요즘",
          "original_form": "전요즘",
          "speaker_id": "1",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100011366.1.20",
          "form": "가디건이 사고싶더라고요",
