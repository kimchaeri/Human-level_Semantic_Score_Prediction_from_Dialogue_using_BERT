        },
        {
          "id": "MDRW2100028665.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028665.1.3",
          "form": "계시는 곳은 비가 오나요?",
          "original_form": "계시는 곳은 비가 오나요?",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028665.1.4",
          "form": "여기는 오지는 않고 바람이 많이 불었어요",
          "original_form": "여기는 오지는 않고 바람이 많이 불었어요",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028665.1.5",
          "form": "비오셨나요?",
          "original_form": "비오셨나요?",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028665.1.6",
          "form": "비가 오다가 해가 떴다가",
          "original_form": "비가 오다가 해가 떴다가",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028665.1.7",
          "form": "좀 잡을 수 없는 날씨네요;",
          "original_form": "좀 잡을 수 없는 날씨네요;",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028665.1.8",
          "form": "그쵸 요근래 날씨가 정말 이상합니다",
          "original_form": "그쵸 요근래 날씨가 정말 이상합니다",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028665.1.9",
          "form": "다음주 화요일에는 전국에 비온데요",
          "original_form": "다음주 화요일에는 전국에 비온데요",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028665.1.10",
          "form": "비가 정말 지겹게 오네요",
          "original_form": "비가 정말 지겹게 오네요",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028665.1.11",
          "form": "바로 겨울 오는 거 아닌가 몰라요;",
          "original_form": "바로 겨울 오는 거 아닌가 몰라요;",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028665.1.12",
          "form": "그쵸 가을이 순식간에 지나갈거 같아요",
          "original_form": "그쵸 가을이 순식간에 지나갈거 같아요",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028665.1.13",
          "form": "새벽에 춥지 않으셨어요?",
          "original_form": "새벽에 춥지 않으셨어요?",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028665.1.14",
          "form": "맞아요 새벽에는 춥죠~",
          "original_form": "맞아요 새벽에는 춥죠~",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028665.1.15",
          "form": "감기 걸리기 딱 좋은 날씨네요",
          "original_form": "감기 걸리기 딱 좋은 날씨네요",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028665.1.16",
          "form": "네 가디건 잘챙겨 다녀야 할거 같아요",
          "original_form": "네 가디건 잘챙겨 다녀야 할거 같아요",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028665.1.17",
          "form": "내 우산과 가디건 필수인 것 같아요~",
          "original_form": "내 우산과 가디건 필수인 것 같아요~",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028665.1.18",
          "form": "감기 조심하세요~",
          "original_form": "감기 조심하세요~",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028665.1.19",
          "form": "네 그래도 주말은 날씨가 좋을거 같았어요 뉴스보니",
          "original_form": "네 그래도 주말은 날씨가 좋을거 같았어요 뉴스보니",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028665.1.20",
          "form": "미세먼지도 없어서 좋네요",
          "original_form": "미세먼지도 없어서 좋네요",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100028665.1.21",
          "form": "올여름 너무 더웠어서",
          "original_form": "올여름 너무 더웠어서",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028665.1.22",
          "form": "비가 지겹기는 하지만",
          "original_form": "비가 지겹기는 하지만",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028665.1.23",
          "form": "좀 선선해져서  숨통은 트이네요",
          "original_form": "좀 선선해져서  숨통은 트이네요",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028665.1.24",
          "form": "네 너무 더운 여름 이였어요",
          "original_form": "네 너무 더운 여름 이였어요",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028665.1.25",
          "form": "이제 가을겨울이 코앞이네요",
          "original_form": "이제 가을겨울이 코앞이네요",
          "speaker_id": "1",
