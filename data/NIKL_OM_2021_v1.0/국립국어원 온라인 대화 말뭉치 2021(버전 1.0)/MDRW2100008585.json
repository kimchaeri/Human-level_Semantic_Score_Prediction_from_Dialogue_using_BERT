      "utterance": [
        {
          "id": "MDRW2100008585.1.1",
          "form": "김피탕은 어떤맛이던가요",
          "original_form": "김피탕은 어떤맛이던가요",
          "speaker_id": "1",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008585.1.2",
          "form": "새콤한 탕수육 느낌이요!!",
          "original_form": "새콤한 탕수육 느낌이요!!",
          "speaker_id": "2",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008585.1.3",
          "form": "아무래도 김치가 있어서 그렇게 느껴졌나봐요",
          "original_form": "아무래도 김치가 있어서 그렇게 느껴졌나봐요",
          "speaker_id": "2",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008585.1.4",
          "form": "김피탕은 부먹으로 먹나요 찍먹으로 먹나요",
          "original_form": "김피탕은 부먹으로 먹나요 찍먹으로 먹나요",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008585.1.5",
          "form": "소스가 이미 부어져 있어서 부먹이었어요",
          "original_form": "소스가 이미 부어져 있어서 부먹이었어요",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008585.1.6",
          "form": "근데 전 평소에도 부먹을 좋아해요 ㅎㅎㅎ",
          "original_form": "근데 전 평소에도 부먹을 좋아해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008585.1.7",
          "form": "찍먹파신가요?",
          "original_form": "찍먹파신가요?",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008585.1.8",
          "form": "전 사실 막먹습니다",
          "original_form": "전 사실 막먹습니다",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008585.1.9",
          "form": "부어도좋고 찍어도 좋고 가리지않아요",
          "original_form": "부어도좋고 찍어도 좋고 가리지않아요",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008585.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그게 정답이죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그게 정답이죠",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008585.1.11",
          "form": "김피탕에 김치는 어때요?",
          "original_form": "김피탕에 김치는 어때요?",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008585.1.12",
          "form": "새콤해요",
          "original_form": "새콤해요",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008585.1.13",
          "form": "약간 소스에 김치가 들어간다는게 상상이 안되는거같아요",
          "original_form": "약간 소스에 김치가 들어간다는게 상상이 안되는거같아요",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008585.1.14",
          "form": "그래서 새콤한 걸 싫어하시는 분은",
          "original_form": "그래서 새콤한 걸 싫어하시는 분은",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008585.1.15",
          "form": "별로라고 할 것 같아여",
          "original_form": "별로라고 할 것 같아여",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008585.1.16",
          "form": "식감은요?",
          "original_form": "식감은요?",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008585.1.17",
          "form": "그냥 익힌 김치같나요?",
          "original_form": "그냥 익힌 김치같나요?",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008585.1.18",
          "form": "물렁한 편은 아니고 살짝 식감이 있는 아삭아삭이요!!",
          "original_form": "물렁한 편은 아니고 살짝 식감이 있는 아삭아삭이요!!",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008585.1.19",
          "form": "오 신선한데",
          "original_form": "오 신선한데",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008585.1.20",
          "form": "타 지역에서도 파는 곳 있던데",
          "original_form": "타 지역에서도 파는 곳 있던데",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008585.1.21",
          "form": "아삭하다니 꼭 먹어보겠습니다",
          "original_form": "아삭하다니 꼭 먹어보겠습니다",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008585.1.22",
