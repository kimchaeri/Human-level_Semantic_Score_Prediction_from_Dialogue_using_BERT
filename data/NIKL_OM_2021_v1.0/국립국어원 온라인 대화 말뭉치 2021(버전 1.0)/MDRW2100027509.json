      "utterance": [
        {
          "id": "MDRW2100027509.1.1",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210902 13:17"
        },
        {
          "id": "MDRW2100027509.1.2",
          "form": "아직 개학한지 얼마안돼서",
          "original_form": "아직 개학한지 얼마안돼서",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027509.1.3",
          "form": "어제부터 학기 시작됏어",
          "original_form": "어제부터 학기 시작됏어",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027509.1.4",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027509.1.5",
          "form": "공부시간이 확 느니깐 적응이안되네",
          "original_form": "공부시간이 확 느니깐 적응이안되네",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027509.1.6",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027509.1.7",
          "form": "늘엇어?",
          "original_form": "늘엇어?",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027509.1.8",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027509.1.9",
          "form": "확실해?",
          "original_form": "확실해?",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027509.1.10",
          "form": "안됐다...",
          "original_form": "안됐다...",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027509.1.11",
          "form": "그리구",
          "original_form": "그리구",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100027509.1.12",
          "form": "시험있자나",
          "original_form": "시험있자나",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100027509.1.13",
          "form": "아직 수행평가 기간은 아니지만...",
          "original_form": "아직 수행평가 기간은 아니지만...",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100027509.1.14",
          "form": "시험이 제일 스트레스지",
          "original_form": "시험이 제일 스트레스지",
          "speaker_id": "1",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027509.1.15",
          "form": "수행평가도 좀 많이 보는거같구.",
          "original_form": "수행평가도 좀 많이 보는거같구.",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027509.1.16",
          "form": "맞아 ㅎㅎ",
          "original_form": "맞아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027509.1.17",
          "form": "한 학기에 시험이 두번이나 잇다니...",
          "original_form": "한 학기에 시험이 두번이나 잇다니...",
          "speaker_id": "1",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027509.1.18",
          "form": "정말 끔찍해",
          "original_form": "정말 끔찍해",
          "speaker_id": "1",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027509.1.19",
          "form": "그러게..ㅜㅡㅜ",
          "original_form": "그러게..ㅜㅡㅜ",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027509.1.20",
          "form": "중간고사는 게다가 방학끝나고 거의 바로보니까",
          "original_form": "중간고사는 게다가 방학끝나고 거의 바로보니까",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027509.1.21",
          "form": "더 싫은거같아",
          "original_form": "더 싫은거같아",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027509.1.22",
          "form": "시간이 완전 빨리 가지",
          "original_form": "시간이 완전 빨리 가지",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027509.1.23",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027509.1.24",
          "form": "막상 학교가면 시간이 느리게가는데..",
          "original_form": "막상 학교가면 시간이 느리게가는데..",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027509.1.25",
          "form": "좀 공부할만 하면 시험을 보자나",
          "original_form": "좀 공부할만 하면 시험을 보자나",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027509.1.26",
          "form": "모의고사도 보고",
          "original_form": "모의고사도 보고",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027509.1.27",
          "form": "그래서 이번에는 한달전부터 시험 공부를 하려구!",
          "original_form": "그래서 이번에는 한달전부터 시험 공부를 하려구!",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.28",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.29",
          "form": "잘 생각햇어!!",
          "original_form": "잘 생각햇어!!",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.30",
          "form": "모의고사는 너무 힘들어",
          "original_form": "모의고사는 너무 힘들어",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.31",
          "form": "웅!!",
          "original_form": "웅!!",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.32",
          "form": "근데 막상 미리 시험공부를 하려고 해도",
          "original_form": "근데 막상 미리 시험공부를 하려고 해도",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.33",
          "form": "언니도 미리 공부를 하는건어때?",
          "original_form": "언니도 미리 공부를 하는건어때?",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.34",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.35",
          "form": "할게 없는거 잇지",
          "original_form": "할게 없는거 잇지",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.36",
          "form": "나는 벼락치기가 좋아",
          "original_form": "나는 벼락치기가 좋아",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.37",
          "form": "그렇긴해",
          "original_form": "그렇긴해",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.38",
          "form": "짜릿하잔아",
          "original_form": "짜릿하잔아",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.39",
          "form": "나두 사실 그래",
          "original_form": "나두 사실 그래",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.40",
          "form": "공부하면서",
          "original_form": "공부하면서",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027509.1.41",
          "form": "스릴을 느낀다니",
          "original_form": "스릴을 느낀다니",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027509.1.42",
          "form": "시험 전날엔",
          "original_form": "시험 전날엔",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027509.1.43",
          "form": "잠을 많이 자야되는데...",
          "original_form": "잠을 많이 자야되는데...",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027509.1.44",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027509.1.45",
          "form": "하지만",
          "original_form": "하지만",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027509.1.46",
          "form": "시험보기 3시간전에는",
          "original_form": "시험보기 3시간전에는",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027509.1.47",
          "form": "미리 깨어있어야 한다구 하더라.",
          "original_form": "미리 깨어있어야 한다구 하더라.",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027509.1.48",
          "form": "헐 왜??",
          "original_form": "헐 왜??",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027509.1.49",
          "form": "뇌가 그래야 깰수 있다고 하더라구",
          "original_form": "뇌가 그래야 깰수 있다고 하더라구",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027509.1.50",
          "form": "일어난지 얼마 안되면",
          "original_form": "일어난지 얼마 안되면",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027509.1.51",
          "form": "뇌가 안돌아간대.",
          "original_form": "뇌가 안돌아간대.",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027509.1.52",
          "form": "물론 과학적인건 아니야",
          "original_form": "물론 과학적인건 아니야",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027509.1.53",
          "form": "줭말 놀라운 사실이다 ㅎㅎ",
          "original_form": "줭말 놀라운 사실이다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027509.1.54",
          "form": "하지만 과학적일수도 있지",
          "original_form": "하지만 과학적일수도 있지",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027509.1.55",
          "form": "그냥 들은거니깐.",
          "original_form": "그냥 들은거니깐.",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027509.1.56",
          "form": "웃기는 소리 하지마",
          "original_form": "웃기는 소리 하지마",
          "speaker_id": "1",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027509.1.57",
          "form": "알겠어",
          "original_form": "알겠어",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027509.1.58",
          "form": "공부를 아무튼 열심히해",
          "original_form": "공부를 아무튼 열심히해",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027509.1.59",
          "form": "너부터 열심히 해!",
          "original_form": "너부터 열심히 해!",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027509.1.60",
          "form": "알겠어",
          "original_form": "알겠어",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027509.1.61",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027509.1.62",
          "form": "학교 얘기나 하자구",
          "original_form": "학교 얘기나 하자구",
          "speaker_id": "2",
