      "utterance": [
        {
          "id": "MDRW2100027156.1.1",
          "form": "최근에쇼핑한거 있나요?",
          "original_form": "최근에쇼핑한거 있나요?",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027156.1.2",
          "form": "어제 바람떡을 사봤어요ㅋㅋ",
          "original_form": "어제 바람떡을 사봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:50"
        },
        {
          "id": "MDRW2100027156.1.3",
          "form": "바람떡 맛있죠 ~음료랑 먹으면 딱이죠",
          "original_form": "바람떡 맛있죠 ~음료랑 먹으면 딱이죠",
          "speaker_id": "2",
          "time": "20210902 11:51"
        },
        {
          "id": "MDRW2100027156.1.4",
          "form": "요즘은 굳지않는 떡이란게 잇나봐요",
          "original_form": "요즘은 굳지않는 떡이란게 잇나봐요",
          "speaker_id": "1",
          "time": "20210902 11:51"
        },
        {
          "id": "MDRW2100027156.1.5",
          "form": "알아요.저도 잘사먹어요",
          "original_form": "알아요.저도 잘사먹어요",
          "speaker_id": "2",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100027156.1.6",
          "form": "맛있으면 계속 사보려고요ㅋㅋ",
          "original_form": "맛있으면 계속 사보려고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027156.1.7",
          "form": "아리울떡 맛있어요",
          "original_form": "아리울떡 맛있어요",
          "speaker_id": "2",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027156.1.8",
          "form": "그이름 많이 보이던데요",
          "original_form": "그이름 많이 보이던데요",
          "speaker_id": "1",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100027156.1.9",
          "form": "아침방송이랑 홈쇼핑에 많이 나왔다고 하더라구요",
          "original_form": "아침방송이랑 홈쇼핑에 많이 나왔다고 하더라구요",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100027156.1.10",
          "form": "아 홈쇼핑까지 나왓군여\n담엔 그거사봐야겠네요",
          "original_form": "아 홈쇼핑까지 나왓군여\n담엔 그거사봐야겠네요",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100027156.1.11",
          "form": "종류도 다양하니까 찾아보세요",
          "original_form": "종류도 다양하니까 찾아보세요",
          "speaker_id": "2",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100027156.1.12",
          "form": "종류가 넘 많은것도 결정이 어려워서 안좋아요ㅋ",
          "original_form": "종류가 넘 많은것도 결정이 어려워서 안좋아요ㅋ",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027156.1.13",
          "form": "제일 인기많은거. 후기 많은걸로 사세요",
          "original_form": "제일 인기많은거. 후기 많은걸로 사세요",
          "speaker_id": "2",
          "time": "20210902 11:59"
        },
        {
          "id": "MDRW2100027156.1.14",
          "form": "좋은방법이네요",
          "original_form": "좋은방법이네요",
          "speaker_id": "1",
