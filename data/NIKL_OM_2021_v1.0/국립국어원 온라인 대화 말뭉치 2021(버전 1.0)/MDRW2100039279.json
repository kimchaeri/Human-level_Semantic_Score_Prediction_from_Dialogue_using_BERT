      "utterance": [
        {
          "id": "MDRW2100039279.1.1",
          "form": "앗 바뀌었다 먼가 이상하지",
          "original_form": "앗 바뀌었다 먼가 이상하지",
          "speaker_id": "2",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039279.1.2",
          "form": "만근 후에 쉴 수 있는지 물어봤는데",
          "original_form": "만근 후에 쉴 수 있는지 물어봤는데",
          "speaker_id": "2",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039279.1.3",
          "form": "정확하게 모르시는 눈치",
          "original_form": "정확하게 모르시는 눈치",
          "speaker_id": "2",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039279.1.4",
          "form": "엥....",
          "original_form": "엥....",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039279.1.5",
          "form": "이게 기관 몇개가 연계해서 일자리 만들어둔거라 더 그런가",
          "original_form": "이게 기관 몇개가 연계해서 일자리 만들어둔거라 더 그런가",
          "speaker_id": "2",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039279.1.6",
          "form": "정확하게 뭘로 취업한거야?",
          "original_form": "정확하게 뭘로 취업한거야?",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039279.1.7",
          "form": "기간직?",
          "original_form": "기간직?",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039279.1.8",
          "form": "기간직 인턴?",
          "original_form": "기간직 인턴?",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039279.1.9",
          "form": "일단 말로는 인턴이야",
          "original_form": "일단 말로는 인턴이야",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039279.1.10",
          "form": "체험형 인턴?",
          "original_form": "체험형 인턴?",
          "speaker_id": "1",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039279.1.11",
          "form": "일경험 프로그램으로 하는 체험..형 인턴이겠지?",
          "original_form": "일경험 프로그램으로 하는 체험..형 인턴이겠지?",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039279.1.12",
          "form": "일경험인지 체험인지 맨날 바뀌어서 헷갈리네 암튼 ㅇㅇ",
          "original_form": "일경험인지 체험인지 맨날 바뀌어서 헷갈리네 암튼 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039279.1.13",
          "form": "ㅇㅎ....",
          "original_form": "ㅇㅎ....",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039279.1.14",
          "form": "그럼 자회사가 뽑아서 줬나?",
          "original_form": "그럼 자회사가 뽑아서 줬나?",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100039279.1.15",
          "form": "이게 일단 자소서랑 거머야 그거",
          "original_form": "이게 일단 자소서랑 거머야 그거",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039279.1.16",
          "form": "시간 날때 화장실 가는 척 하고 공고 올렸던 업체한테 전화해봐...",
          "original_form": "시간 날때 화장실 가는 척 하고 공고 올렸던 업체한테 전화해봐...",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039279.1.17",
          "form": "이력서",
          "original_form": "이력서",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039279.1.18",
          "form": "근데 내가 알기론 쉬는건 일하는 곳 재량으로 알고있는데...",
          "original_form": "근데 내가 알기론 쉬는건 일하는 곳 재량으로 알고있는데...",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039279.1.19",
          "form": "이력서가 생각안났음 ㅋㅋㅋ 암튼 이력서 내고 하는건데..",
          "original_form": "이력서가 생각안났음 ㅋㅋㅋ 암튼 이력서 내고 하는건데..",
          "speaker_id": "2",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039279.1.20",
          "form": "이게 일경험 프로그램이라고 국민취업지원제도랑 연계해서 주는거라",
          "original_form": "이게 일경험 프로그램이라고 국민취업지원제도랑 연계해서 주는거라",
          "speaker_id": "2",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039279.1.21",
          "form": "거치는 것만 내 담당 선생님->일경험 프로그램 담당->우체국 이래서",
          "original_form": "거치는 것만 내 담당 선생님->일경험 프로그램 담당->우체국 이래서",
          "speaker_id": "2",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039279.1.22",
          "form": "내 담당도 모르는 거 많고 우체국도 모르는거 많아 환장",
          "original_form": "내 담당도 모르는 거 많고 우체국도 모르는거 많아 환장",
          "speaker_id": "2",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039279.1.23",
          "form": "ㅇㅎ... 단계를 많이 거쳤구나...",
          "original_form": "ㅇㅎ... 단계를 많이 거쳤구나...",
