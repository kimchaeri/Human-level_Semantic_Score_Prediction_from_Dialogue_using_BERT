      "utterance": [
        {
          "id": "MDRW2100018830.1.1",
          "form": "여행가고싶네요",
          "original_form": "여행가고싶네요",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018830.1.2",
          "form": "최근에 다녀오셧나요?",
          "original_form": "최근에 다녀오셧나요?",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018830.1.3",
          "form": "아니요ㅠ",
          "original_form": "아니요ㅠ",
          "speaker_id": "2",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018830.1.4",
          "form": "다녀오셨어요?",
          "original_form": "다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018830.1.5",
          "form": "최근엔 여름 휴가 다녀왓어요",
          "original_form": "최근엔 여름 휴가 다녀왓어요",
          "speaker_id": "1",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018830.1.6",
          "form": "어디로 다녀오셨어요?",
          "original_form": "어디로 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018830.1.7",
          "form": "울릉도 독도 다녀왓어요",
          "original_form": "울릉도 독도 다녀왓어요",
          "speaker_id": "1",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018830.1.8",
          "form": "우와 부러워요",
          "original_form": "우와 부러워요",
          "speaker_id": "2",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018830.1.9",
          "form": "꼭 가보고 싶은곳이에요",
          "original_form": "꼭 가보고 싶은곳이에요",
          "speaker_id": "2",
          "time": "20210827 13:24"
        },
        {
          "id": "MDRW2100018830.1.10",
          "form": "독도 입도는 진짜 인상깊었어요",
          "original_form": "독도 입도는 진짜 인상깊었어요",
          "speaker_id": "1",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018830.1.11",
          "form": "추천함니다",
          "original_form": "추천함니다",
          "speaker_id": "1",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100018830.1.12",
          "form": "오오 더 궁금하네요",
          "original_form": "오오 더 궁금하네요",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018830.1.13",
          "form": "기억에남는 여행지잇으신가요",
          "original_form": "기억에남는 여행지잇으신가요",
          "speaker_id": "1",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100018830.1.14",
          "form": "부산이요!",
          "original_form": "부산이요!",
          "speaker_id": "2",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018830.1.15",
          "form": "더운날 갔었는데 더우면서도 재밌던 기억이 있네요",
          "original_form": "더운날 갔었는데 더우면서도 재밌던 기억이 있네요",
          "speaker_id": "2",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018830.1.16",
          "form": "저는 부산살아서",
          "original_form": "저는 부산살아서",
          "speaker_id": "1",
          "time": "20210827 14:26"
        },
        {
          "id": "MDRW2100018830.1.17",
          "form": "서울놀러가고싶어요ㅠㅠ",
          "original_form": "서울놀러가고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:26"
        },
        {
          "id": "MDRW2100018830.1.18",
          "form": "코시국끝나구ㅜㅜ",
          "original_form": "코시국끝나구ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 14:26"
        },
        {
          "id": "MDRW2100018830.1.19",
          "form": "아ㅠ 후딱 끝나야 놀러라도 가보는데 말이죠",
          "original_form": "아ㅠ 후딱 끝나야 놀러라도 가보는데 말이죠",
          "speaker_id": "2",
          "time": "20210827 14:39"
        }
