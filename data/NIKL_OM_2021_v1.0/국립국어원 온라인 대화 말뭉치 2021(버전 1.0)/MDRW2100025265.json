      "utterance": [
        {
          "id": "MDRW2100025265.1.1",
          "form": "경상도 와보셨나요 ㅋㅋㅋ먹거리가 많이 다른데 ㅋㅋㅋ",
          "original_form": "경상도 와보셨나요 ㅋㅋㅋ먹거리가 많이 다른데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 03:38"
        },
        {
          "id": "MDRW2100025265.1.2",
          "form": "저 부산 가봤는데요",
          "original_form": "저 부산 가봤는데요",
          "speaker_id": "1",
          "time": "20210901 03:39"
        },
        {
          "id": "MDRW2100025265.1.3",
          "form": "돼지국밥!!!",
          "original_form": "돼지국밥!!!",
          "speaker_id": "1",
          "time": "20210901 03:39"
        },
        {
          "id": "MDRW2100025265.1.4",
          "form": "서울은 종종가지만 추어탕을 안먹어봐서 산초 안넣는다니 충격이네요",
          "original_form": "서울은 종종가지만 추어탕을 안먹어봐서 산초 안넣는다니 충격이네요",
          "speaker_id": "2",
          "time": "20210901 03:39"
        },
        {
          "id": "MDRW2100025265.1.5",
          "form": "씨앗호떡",
          "original_form": "씨앗호떡",
          "speaker_id": "1",
          "time": "20210901 03:39"
        },
        {
          "id": "MDRW2100025265.1.6",
          "form": "돼지국밥맛있죠!",
          "original_form": "돼지국밥맛있죠!",
          "speaker_id": "2",
          "time": "20210901 03:39"
        },
        {
          "id": "MDRW2100025265.1.7",
          "form": "그걸 누가먹습니까",
          "original_form": "그걸 누가먹습니까",
          "speaker_id": "2",
          "time": "20210901 03:39"
        },
        {
          "id": "MDRW2100025265.1.8",
          "form": "저 그 민락수변공원에서",
          "original_form": "저 그 민락수변공원에서",
          "speaker_id": "1",
          "time": "20210901 03:39"
        },
        {
          "id": "MDRW2100025265.1.9",
          "form": "진짜 이해안되는 음식중 하납니다",
          "original_form": "진짜 이해안되는 음식중 하납니다",
          "speaker_id": "2",
          "time": "20210901 03:39"
        },
        {
          "id": "MDRW2100025265.1.10",
          "form": "회도 먹은 사람에요",
          "original_form": "회도 먹은 사람에요",
          "speaker_id": "1",
          "time": "20210901 03:39"
        },
        {
          "id": "MDRW2100025265.1.11",
          "form": "그 방파제 같은데서",
          "original_form": "그 방파제 같은데서",
          "speaker_id": "1",
          "time": "20210901 03:39"
        },
        {
          "id": "MDRW2100025265.1.12",
          "form": "그거 블로그에서 보고 먹은건데",
          "original_form": "그거 블로그에서 보고 먹은건데",
          "speaker_id": "1",
          "time": "20210901 03:40"
        },
        {
          "id": "MDRW2100025265.1.13",
          "form": "씨앗호떡 그거 다 관광객들만 먹습니다",
          "original_form": "씨앗호떡 그거 다 관광객들만 먹습니다",
          "speaker_id": "2",
          "time": "20210901 03:40"
        },
        {
          "id": "MDRW2100025265.1.14",
          "form": "관광객1 입니다",
          "original_form": "관광객1 입니다",
          "speaker_id": "1",
          "time": "20210901 03:40"
        },
        {
          "id": "MDRW2100025265.1.15",
          "form": "아 밀면도 맛있던디",
          "original_form": "아 밀면도 맛있던디",
          "speaker_id": "1",
          "time": "20210901 03:40"
        },
        {
          "id": "MDRW2100025265.1.16",
          "form": "ㅋㅋㅋㅋ 진짜로 여기 사는사람들은 안먹습니다 ㅋㅋㅋ 맛이 없지 않나요",
          "original_form": "ㅋㅋㅋㅋ 진짜로 여기 사는사람들은 안먹습니다 ㅋㅋㅋ 맛이 없지 않나요",
          "speaker_id": "2",
          "time": "20210901 03:40"
        },
        {
          "id": "MDRW2100025265.1.17",
          "form": "이거도 관광객만먹습니까",
          "original_form": "이거도 관광객만먹습니까",
          "speaker_id": "1",
          "time": "20210901 03:40"
        },
        {
          "id": "MDRW2100025265.1.18",
          "form": "밀면 괜찮죠",
          "original_form": "밀면 괜찮죠",
          "speaker_id": "2",
          "time": "20210901 03:40"
        },
        {
          "id": "MDRW2100025265.1.19",
          "form": "밀면은 많이 먹습니다",
          "original_form": "밀면은 많이 먹습니다",
          "speaker_id": "2",
          "time": "20210901 03:41"
        },
        {
          "id": "MDRW2100025265.1.20",
          "form": "휴 이건 안당했군",
          "original_form": "휴 이건 안당했군",
          "speaker_id": "1",
          "time": "20210901 03:41"
        },
        {
          "id": "MDRW2100025265.1.21",
          "form": "전 부산은 아니고 그 옆동네 울산 사는데 밀면은 맛이  경상도 안에서도 좀씩 다릅니다",
          "original_form": "전 부산은 아니고 그 옆동네 울산 사는데 밀면은 맛이  경상도 안에서도 좀씩 다릅니다",
          "speaker_id": "2",
          "time": "20210901 03:41"
        }
      ]
