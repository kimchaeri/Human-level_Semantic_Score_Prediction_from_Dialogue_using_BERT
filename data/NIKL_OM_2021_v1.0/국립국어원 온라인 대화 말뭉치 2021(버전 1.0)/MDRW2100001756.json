        },
        {
          "id": "MDRW2100001756.1.2",
          "form": "안녕하세요 무슨 게임 좋아하세요?",
          "original_form": "안녕하세요 무슨 게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001756.1.3",
          "form": "예전에 디아랑 카트했어요",
          "original_form": "예전에 디아랑 카트했어요",
          "speaker_id": "1",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001756.1.4",
          "form": "카트 저도 좋아해요",
          "original_form": "카트 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001756.1.5",
          "form": "근데 너무 못해서...",
          "original_form": "근데 너무 못해서...",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001756.1.6",
          "form": "저는 많이 했어요",
          "original_form": "저는 많이 했어요",
          "speaker_id": "1",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100001756.1.7",
          "form": "많이하면 실력이 늘죠",
          "original_form": "많이하면 실력이 늘죠",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001756.1.8",
          "form": "혹시 심즈 아시나요?",
          "original_form": "혹시 심즈 아시나요?",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001756.1.9",
          "form": "들어는 봤어요",
          "original_form": "들어는 봤어요",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100001756.1.10",
          "form": "전 심즈 게임에 한때 빠져있었어요",
          "original_form": "전 심즈 게임에 한때 빠져있었어요",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001756.1.11",
          "form": "마을 짓는거 맞죠?",
          "original_form": "마을 짓는거 맞죠?",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001756.1.12",
          "form": "네 맞아요 캐릭터도 꾸미고요",
          "original_form": "네 맞아요 캐릭터도 꾸미고요",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001756.1.13",
          "form": "이거 말고는 메이플도 했었는데 접었어요",
          "original_form": "이거 말고는 메이플도 했었는데 접었어요",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001756.1.14",
          "form": "아  저도",
          "original_form": "아  저도",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001756.1.15",
          "form": "메이플 했었어요",
          "original_form": "메이플 했었어요",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001756.1.16",
          "form": "메이플 직업 뭐였어요?",
          "original_form": "메이플 직업 뭐였어요?",
