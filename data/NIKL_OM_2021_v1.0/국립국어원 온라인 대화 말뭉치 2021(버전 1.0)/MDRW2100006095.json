      "utterance": [
        {
          "id": "MDRW2100006095.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100006095.1.2",
          "form": "좋아하는 음료가 있으세요?",
          "original_form": "좋아하는 음료가 있으세요?",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100006095.1.3",
          "form": "네 전 코카콜라 완전 좋아해요",
          "original_form": "네 전 코카콜라 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006095.1.4",
          "form": "전 사이다요!",
          "original_form": "전 사이다요!",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006095.1.5",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006095.1.6",
          "form": "사이다 좋죠",
          "original_form": "사이다 좋죠",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100006095.1.7",
          "form": "칠성 가장 좋아하시는건가요?",
          "original_form": "칠성 가장 좋아하시는건가요?",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100006095.1.8",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006095.1.9",
          "form": "칠성이 최고죠~",
          "original_form": "칠성이 최고죠~",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006095.1.10",
          "form": "스프라이트 따위 ㅋㅋ",
          "original_form": "스프라이트 따위 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006095.1.11",
          "form": "오ㅋㅋㅋ 신조가있으시군요",
          "original_form": "오ㅋㅋㅋ 신조가있으시군요",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006095.1.12",
          "form": "저도 코카콜라가 최고에요",
          "original_form": "저도 코카콜라가 최고에요",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006095.1.13",
          "form": "펩시따위",
          "original_form": "펩시따위",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006095.1.14",
          "form": "저도 콜라는 코카콩라라 생각합니다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 콜라는 코카콩라라 생각합니다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006095.1.15",
          "form": "펩시는 비비적댈때가안되죠",
          "original_form": "펩시는 비비적댈때가안되죠",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006095.1.16",
          "form": "배달음식 먹을때",
          "original_form": "배달음식 먹을때",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006095.1.17",
          "form": "펩시오면 화나요ㅠㅜ",
          "original_form": "펩시오면 화나요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006095.1.18",
          "form": "맞아요....차라이오지를말던가",
          "original_form": "맞아요....차라이오지를말던가",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006095.1.19",
          "form": "맞아요ㅠㅜ차라리 조금 더 받고 코카콜라 줬으면해요",
          "original_form": "맞아요ㅠㅜ차라리 조금 더 받고 코카콜라 줬으면해요",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006095.1.20",
          "form": "맞아요 ㅠㅠㅠ 그게 은근 서비스라는걸 모르더라구요",
          "original_form": "맞아요 ㅠㅠㅠ 그게 은근 서비스라는걸 모르더라구요",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006095.1.21",
          "form": "맞아요 코카콜라 주면 전 일단 호감가더라구요",
          "original_form": "맞아요 코카콜라 주면 전 일단 호감가더라구요",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006095.1.22",
          "form": "맞아요!!@@",
          "original_form": "맞아요!!@@",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006095.1.23",
          "form": "엄청크다구요 그게",
          "original_form": "엄청크다구요 그게",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006095.1.24",
          "form": "맞아요ㅠㅜ 왜 사장님은 모를까요?",
          "original_form": "맞아요ㅠㅜ 왜 사장님은 모를까요?",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006095.1.25",
          "form": "ㅠㅠㅠ그니까요 소비자맴을 몰라 ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ그니까요 소비자맴을 몰라 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006095.1.26",
          "form": "그런데 저는 펩시랑 코카콜라는 당연히 구분이 가는데",
          "original_form": "그런데 저는 펩시랑 코카콜라는 당연히 구분이 가는데",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006095.1.27",
          "form": "콜라파여서 잘몰라서",
          "original_form": "콜라파여서 잘몰라서",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006095.1.28",
          "form": "칠성이랑 스프라이트도 차이점같은게 느껴지시나요?",
          "original_form": "칠성이랑 스프라이트도 차이점같은게 느껴지시나요?",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006095.1.29",
          "form": "완전 맛이달라요!!",
          "original_form": "완전 맛이달라요!!",
          "speaker_id": "1",
          "time": "20210907 20:53"
        }
      ]
