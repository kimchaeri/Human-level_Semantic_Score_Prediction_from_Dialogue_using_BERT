      "utterance": [
        {
          "id": "MDRW2100039341.1.1",
          "form": "최근에 뭐사셨어요~?",
          "original_form": "최근에 뭐사셨어요~?",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100039341.1.2",
          "form": "밴드 샀어요",
          "original_form": "밴드 샀어요",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100039341.1.3",
          "form": "모기에 하도 물려서요ㅠ",
          "original_form": "모기에 하도 물려서요ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100039341.1.4",
          "form": "ㅠㅜ 날이 시원해졌는데",
          "original_form": "ㅠㅜ 날이 시원해졌는데",
          "speaker_id": "1",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039341.1.5",
          "form": "아직 모기가 있나요?",
          "original_form": "아직 모기가 있나요?",
          "speaker_id": "1",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039341.1.6",
          "form": "가을모기가 좀 있네요",
          "original_form": "가을모기가 좀 있네요",
          "speaker_id": "2",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039341.1.7",
          "form": "가을 모기는 한번 물리면 너무 독해서",
          "original_form": "가을 모기는 한번 물리면 너무 독해서",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039341.1.8",
          "form": "부어오르더라구요ㅜㅜ",
          "original_form": "부어오르더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039341.1.9",
          "form": "모기밴드는 붙이면 덜 간지럽나요?",
          "original_form": "모기밴드는 붙이면 덜 간지럽나요?",
          "speaker_id": "1",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039341.1.10",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039341.1.11",
          "form": "잘 모르겠네요",
          "original_form": "잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039341.1.12",
          "form": "앗.",
          "original_form": "앗.",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039341.1.13",
          "form": "그럼 왜사는거죠?. ㅜㅜ",
          "original_form": "그럼 왜사는거죠?. ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039341.1.14",
          "form": "그냥..안가지러웠으면 하는 맘에?",
          "original_form": "그냥..안가지러웠으면 하는 맘에?",
          "speaker_id": "1",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039341.1.15",
          "form": "손 댔을때 긁으면 안된다는 생각 들게끔 하려구요...ㅎㅎ",
          "original_form": "손 댔을때 긁으면 안된다는 생각 들게끔 하려구요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039341.1.16",
          "form": "아! 손독 안오르는게 중요하죠",
          "original_form": "아! 손독 안오르는게 중요하죠",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039341.1.17",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039341.1.18",
          "form": "모기 처음 물렸을때보다",
          "original_form": "모기 처음 물렸을때보다",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039341.1.19",
          "form": "한번 긁었을때가 더 상처가 커지죠",
          "original_form": "한번 긁었을때가 더 상처가 커지죠",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039341.1.20",
          "form": "물렸을때 비누로 씻으니 안가렵더라구요",
          "original_form": "물렸을때 비누로 씻으니 안가렵더라구요",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039341.1.21",
          "form": "오 그런 방법도 있군요",
          "original_form": "오 그런 방법도 있군요",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039341.1.22",
          "form": "담에 물리실때 비누로 씻어보세요~^^",
          "original_form": "담에 물리실때 비누로 씻어보세요~^^",
