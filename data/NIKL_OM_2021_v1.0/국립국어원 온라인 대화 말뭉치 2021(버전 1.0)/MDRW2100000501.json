      "utterance": [
        {
          "id": "MDRW2100000501.1.1",
          "form": "안녕하세요~\n오늘 날씨가 엄청 좋은 거 같아요~",
          "original_form": "안녕하세요~\n오늘 날씨가 엄청 좋은 거 같아요~",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000501.1.2",
          "form": "네! 이렇게 해가 쨍쨍인데",
          "original_form": "네! 이렇게 해가 쨍쨍인데",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000501.1.3",
          "form": "주말부터 비라니...",
          "original_form": "주말부터 비라니...",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000501.1.4",
          "form": "아.. 주말부터 비가 오는 군요..ㅠㅠ",
          "original_form": "아.. 주말부터 비가 오는 군요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000501.1.5",
          "form": "계속 오더라고요",
          "original_form": "계속 오더라고요",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000501.1.6",
          "form": "일주일내내",
          "original_form": "일주일내내",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000501.1.7",
          "form": "태풍이 오는건지..",
          "original_form": "태풍이 오는건지..",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000501.1.8",
          "form": "아~ 다음주까지 오는 건가 보네요...",
          "original_form": "아~ 다음주까지 오는 건가 보네요...",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000501.1.9",
          "form": "비로 인한 피해가 크지 않았음 좋겠네요~",
          "original_form": "비로 인한 피해가 크지 않았음 좋겠네요~",
          "speaker_id": "1",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000501.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000501.1.11",
          "form": "비 오기 전에 더운 느낌으로",
          "original_form": "비 오기 전에 더운 느낌으로",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000501.1.12",
          "form": "오늘도 덥네요!",
          "original_form": "오늘도 덥네요!",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000501.1.13",
          "form": "긴바지 입었는데 다리가 타는 느낌이에요",
          "original_form": "긴바지 입었는데 다리가 타는 느낌이에요",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000501.1.14",
          "form": "습도 자체가 높나 보네요,,ㅠㅠ",
          "original_form": "습도 자체가 높나 보네요,,ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000501.1.15",
          "form": "이런 후덥지근한 날씨가 빨리 지나갔으면 좋겠어요~",
          "original_form": "이런 후덥지근한 날씨가 빨리 지나갔으면 좋겠어요~",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000501.1.16",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000501.1.17",
          "form": "밤에는 썰렁하던데",
          "original_form": "밤에는 썰렁하던데",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000501.1.18",
          "form": "아유 오늘은 덥네요!",
          "original_form": "아유 오늘은 덥네요!",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000501.1.19",
          "form": "그래도 열대야는 끝나서 다행인 거 같은데,ㅠㅠㅠ",
          "original_form": "그래도 열대야는 끝나서 다행인 거 같은데,ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000501.1.20",
          "form": "활동시간에 더우니 힘든 거 같아요~",
          "original_form": "활동시간에 더우니 힘든 거 같아요~",
          "speaker_id": "1",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000501.1.21",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000501.1.22",
          "form": "사계절이 아니라 점점 겨울-여름만 있는 것 같은 느낌이에요",
          "original_form": "사계절이 아니라 점점 겨울-여름만 있는 것 같은 느낌이에요",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000501.1.23",
          "form": "맞아요~ 4계절이 사라지는 거 같더라고요..ㅠ",
          "original_form": "맞아요~ 4계절이 사라지는 거 같더라고요..ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000501.1.24",
          "form": "와중에 일기예보는 계속 틀려서",
          "original_form": "와중에 일기예보는 계속 틀려서",
          "speaker_id": "2",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000501.1.25",
          "form": "더 헷갈려요",
          "original_form": "더 헷갈려요",
          "speaker_id": "2",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000501.1.26",
          "form": "꼭 우산 안 들고 나오면 비가 오고",
          "original_form": "꼭 우산 안 들고 나오면 비가 오고",
          "speaker_id": "2",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000501.1.27",
          "form": "그래서 들고 나오면 안 오고",
          "original_form": "그래서 들고 나오면 안 오고",
          "speaker_id": "2",
          "time": "20210819 16:46"
        },
        {
          "id": "MDRW2100000501.1.28",
          "form": "그러니까요~ 저도 저번에 우산 하루종일 들고 다녔던,,;;",
          "original_form": "그러니까요~ 저도 저번에 우산 하루종일 들고 다녔던,,;;",
          "speaker_id": "1",
