      "utterance": [
        {
          "id": "MDRW2100011017.1.1",
          "form": "너 담다디라는 가게 알아?",
          "original_form": "너 담다디라는 가게 알아?",
          "speaker_id": "1",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100011017.1.2",
          "form": "내가 소개해줬잖아",
          "original_form": "내가 소개해줬잖아",
          "speaker_id": "2",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100011017.1.3",
          "form": "벌써 알코올성 치매가 온 거야?",
          "original_form": "벌써 알코올성 치매가 온 거야?",
          "speaker_id": "2",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100011017.1.4",
          "form": "아맞다 순간 헷갈렸어",
          "original_form": "아맞다 순간 헷갈렸어",
          "speaker_id": "1",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100011017.1.5",
          "form": "그러게 몸에 좋은걸 좀 먹어야겠다",
          "original_form": "그러게 몸에 좋은걸 좀 먹어야겠다",
          "speaker_id": "1",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100011017.1.6",
          "form": "닭볶음탕이 사고싶은거지?",
          "original_form": "닭볶음탕이 사고싶은거지?",
          "speaker_id": "2",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100011017.1.7",
          "form": "영양제는 산거야?",
          "original_form": "영양제는 산거야?",
          "speaker_id": "2",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100011017.1.8",
          "form": "영양제는 그냥 없이 살래",
          "original_form": "영양제는 그냥 없이 살래",
          "speaker_id": "1",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100011017.1.9",
          "form": "아무래도 우리는 균형잡힌 식사를 하지않으니 영양제는 정말 필수인 거 같아.",
          "original_form": "아무래도 우리는 균형잡힌 식사를 하지않으니 영양제는 정말 필수인 거 같아.",
          "speaker_id": "2",
          "time": "20210823 23:46"
        },
        {
          "id": "MDRW2100011017.1.10",
          "form": "그냥 살면 좀 그럴까? 건강에 많이? 안좋을까?",
          "original_form": "그냥 살면 좀 그럴까? 건강에 많이? 안좋을까?",
          "speaker_id": "1",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100011017.1.11",
          "form": "흠 그러다가 나는 된통 당했거든.",
          "original_form": "흠 그러다가 나는 된통 당했거든.",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100011017.1.12",
          "form": "왜 맛있는건 몸에 안좋을까",
          "original_form": "왜 맛있는건 몸에 안좋을까",
          "speaker_id": "1",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100011017.1.13",
          "form": "음식을 잘 챙겨먹어.",
          "original_form": "음식을 잘 챙겨먹어.",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100011017.1.14",
          "form": "알겠어 조언 잘 새겨들을께",
          "original_form": "알겠어 조언 잘 새겨들을께",
          "speaker_id": "1",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100011017.1.15",
          "form": "아냐 우린 그래도 꽤 좋아하는 나물이 많아",
          "original_form": "아냐 우린 그래도 꽤 좋아하는 나물이 많아",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100011017.1.16",
          "form": "청경채를 불닭소스에 볶아먹으면 맛있어",
          "original_form": "청경채를 불닭소스에 볶아먹으면 맛있어",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100011017.1.17",
          "form": "흠 맞아 나는 숙주좋아해",
          "original_form": "흠 맞아 나는 숙주좋아해",
          "speaker_id": "1",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100011017.1.18",
          "form": "자취할 때 꿀팁이야",
          "original_form": "자취할 때 꿀팁이야",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100011017.1.19",
          "form": "좋아 불닭소스는 내가산거니까 집에서 들고와야겠어",
          "original_form": "좋아 불닭소스는 내가산거니까 집에서 들고와야겠어",
          "speaker_id": "1",
          "time": "20210823 23:48"
        }
      ]
