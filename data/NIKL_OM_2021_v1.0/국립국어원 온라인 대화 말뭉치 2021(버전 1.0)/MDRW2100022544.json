      "utterance": [
        {
          "id": "MDRW2100022544.1.1",
          "form": "음료 뭐좋아하세요?",
          "original_form": "음료 뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022544.1.2",
          "form": "저는 마시는건 안 가리는 편이라",
          "original_form": "저는 마시는건 안 가리는 편이라",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022544.1.3",
          "form": "카페음료요 아니면 일반 음료수요?",
          "original_form": "카페음료요 아니면 일반 음료수요?",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022544.1.4",
          "form": "카페음료요!",
          "original_form": "카페음료요!",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022544.1.5",
          "form": "전 스타벅스 애용해요",
          "original_form": "전 스타벅스 애용해요",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022544.1.6",
          "form": "이디야도 좋아하고 스벅도 좋아해요ㅋㅋㅋ",
          "original_form": "이디야도 좋아하고 스벅도 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:52"
        },
        {
          "id": "MDRW2100022544.1.7",
          "form": "스벅 최애는 뭐예요?",
          "original_form": "스벅 최애는 뭐예요?",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022544.1.8",
          "form": "민트초코프라프치노요!",
          "original_form": "민트초코프라프치노요!",
          "speaker_id": "1",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022544.1.9",
          "form": "용과도 맛있는데 단종되서 못먹어요ㅠㅠ",
          "original_form": "용과도 맛있는데 단종되서 못먹어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022544.1.10",
          "form": "아 그거 맛있었는데ㅜㅜㅜㅜ",
          "original_form": "아 그거 맛있었는데ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022544.1.11",
          "form": "스벅 아니면 어디서 용과를 맛보겠어요..............ㅜㅜ",
          "original_form": "스벅 아니면 어디서 용과를 맛보겠어요..............ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022544.1.12",
          "form": "그거 나왔을때 가장큰걸로 먹었는데",
          "original_form": "그거 나왔을때 가장큰걸로 먹었는데",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022544.1.13",
          "form": "일주일에 3번이상 사먹었어요ㅠㅠ",
          "original_form": "일주일에 3번이상 사먹었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022544.1.14",
          "form": "저는 스벅 쿨라임피지오 좋아해요",
          "original_form": "저는 스벅 쿨라임피지오 좋아해요",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022544.1.15",
          "form": "근데 집근처 매장에 피지오 기계가 없어져서  못마셔요ㅠㅠㅠ",
          "original_form": "근데 집근처 매장에 피지오 기계가 없어져서  못마셔요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022544.1.16",
          "form": "라임은 셔서 못먹는데 먹어본적은 있어요!",
          "original_form": "라임은 셔서 못먹는데 먹어본적은 있어요!",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022544.1.17",
          "form": "엄청엄청 맛있는데요???????????",
          "original_form": "엄청엄청 맛있는데요???????????",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022544.1.18",
          "form": "톡!!!!!!쏘고 시원하고ㅋㅋㅋ",
          "original_form": "톡!!!!!!쏘고 시원하고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022544.1.19",
          "form": "자몽허니블랙티만 먹어서 ㅋㅋㅋㅋㅋ",
          "original_form": "자몽허니블랙티만 먹어서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022544.1.20",
          "form": "톡쏘는거 잘 못먹어요ㅎ",
          "original_form": "톡쏘는거 잘 못먹어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022544.1.21",
          "form": "먹을줄 아시네요 ㅎㅎ",
          "original_form": "먹을줄 아시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:00"
        },
        {
          "id": "MDRW2100022544.1.22",
          "form": "자허블 존맛 ㅜㅜ",
          "original_form": "자허블 존맛 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 16:00"
        },
        {
          "id": "MDRW2100022544.1.23",
          "form": "원래 밀크티를 좋아하는데 자허블도 맛있더라고요",
