      "utterance": [
        {
          "id": "MDRW2100004573.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004573.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004573.1.3",
          "form": "name1님은 키우시나요?",
          "original_form": "&name1&님은 키우시나요?",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004573.1.4",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004573.1.5",
          "form": "그럼 키우실생각이신가요?",
          "original_form": "그럼 키우실생각이신가요?",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004573.1.6",
          "form": "네 여건되몀\r\nㄴ요",
          "original_form": "네 여건되몀\r\nㄴ요",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004573.1.7",
          "form": "어떤 동물 키우시고 싶으신가요?",
          "original_form": "어떤 동물 키우시고 싶으신가요?",
          "speaker_id": "2",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004573.1.8",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004573.1.9",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004573.1.10",
          "form": "저는 동물 싫어하는 건아닌데 키우고싶지는 않아요",
          "original_form": "저는 동물 싫어하는 건아닌데 키우고싶지는 않아요",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004573.1.11",
          "form": "이것저것 신경써줄게 많잖아요",
          "original_form": "이것저것 신경써줄게 많잖아요",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004573.1.12",
          "form": "그냥 친구가 키우고있는 고양이 한번씩 구경하는것만해도 만족해요",
          "original_form": "그냥 친구가 키우고있는 고양이 한번씩 구경하는것만해도 만족해요",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004573.1.13",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004573.1.14",
          "form": "관리가 ㅎ힘들죠",
          "original_form": "관리가 ㅎ힘들죠",
          "speaker_id": "1",
          "time": "20210821 18:52"
        },
        {
          "id": "MDRW2100004573.1.15",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004573.1.16",
          "form": "털날림이라던지 관리 잘 못해도 나는 특유의 냄새때문에 힘들더라구요",
          "original_form": "털날림이라던지 관리 잘 못해도 나는 특유의 냄새때문에 힘들더라구요",
          "speaker_id": "2",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004573.1.17",
          "form": "그래도 아주 어릴때 병아리 키운적은 있어요",
          "original_form": "그래도 아주 어릴때 병아리 키운적은 있어요",
          "speaker_id": "2",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004573.1.18",
          "form": "초등학교 앞에파는 병아리 있잖아요. 그거요ㅋ",
          "original_form": "초등학교 앞에파는 병아리 있잖아요. 그거요ㅋ",
          "speaker_id": "2",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004573.1.19",
          "form": "얼른 죽던데요",
          "original_form": "얼른 죽던데요",
          "speaker_id": "1",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004573.1.20",
          "form": "맞아요ㅠ 하루만에 죽었어요ㅠ",
          "original_form": "맞아요ㅠ 하루만에 죽었어요ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004573.1.21",
          "form": "넘 슬펐어요",
          "original_form": "넘 슬펐어요",
          "speaker_id": "1",
          "time": "20210821 19:23"
        }
      ]
