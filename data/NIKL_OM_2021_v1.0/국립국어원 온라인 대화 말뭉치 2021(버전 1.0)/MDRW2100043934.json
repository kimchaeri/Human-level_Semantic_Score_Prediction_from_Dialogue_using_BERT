      "utterance": [
        {
          "id": "MDRW2100043934.1.1",
          "form": "포기하고 그냥 드립만 마시고있어요ㅜㅜ",
          "original_form": "포기하고 그냥 드립만 마시고있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043934.1.2",
          "form": "드립이 더쉬운가요?",
          "original_form": "드립이 더쉬운가요?",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043934.1.3",
          "form": "드립고 어렵지않나요?",
          "original_form": "드립고 어렵지않나요?",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043934.1.4",
          "form": "어렵긴어려운데",
          "original_form": "어렵긴어려운데",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043934.1.5",
          "form": "대애충 흉내는 내겠더라구요",
          "original_form": "대애충 흉내는 내겠더라구요",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043934.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043934.1.7",
          "form": "일하면서 조금 배우긴했어요",
          "original_form": "일하면서 조금 배우긴했어요",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043934.1.8",
          "form": "아 일하시면서 ㅎㅎ",
          "original_form": "아 일하시면서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043934.1.9",
          "form": "좋네요 ㅎㅎㅎ",
          "original_form": "좋네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043934.1.10",
          "form": "원하는 맛을 잘 내고있나요?",
          "original_form": "원하는 맛을 잘 내고있나요?",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043934.1.11",
          "form": "그 경지는 아직 못온거같아요ㅜㅜ",
          "original_form": "그 경지는 아직 못온거같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043934.1.12",
          "form": "역시사먹는게짱이더라구요",
          "original_form": "역시사먹는게짱이더라구요",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043934.1.13",
          "form": "맞아요...사먹는게 최고에요.....돈좀아끼고싶지만",
          "original_form": "맞아요...사먹는게 최고에요.....돈좀아끼고싶지만",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043934.1.14",
          "form": "사먹는게최고...",
          "original_form": "사먹는게최고...",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043934.1.15",
          "form": "아니면 요즘은 캡슐을 사볼까도싶어요",
          "original_form": "아니면 요즘은 캡슐을 사볼까도싶어요",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043934.1.16",
          "form": "그런데 또 플라스틱쓰레기가 많이나오는거같아서ㅜㅜ",
          "original_form": "그런데 또 플라스틱쓰레기가 많이나오는거같아서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043934.1.17",
          "form": "아 맛은 보장되니 캡슐이 더 좋긴할거같아요",
          "original_form": "아 맛은 보장되니 캡슐이 더 좋긴할거같아요",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043934.1.18",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043934.1.19",
          "form": "뭔가 애쓸필요없는것도 좋고",
          "original_form": "뭔가 애쓸필요없는것도 좋고",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043934.1.20",
          "form": "맞아요 버튼만누르면~",
          "original_form": "맞아요 버튼만누르면~",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043934.1.21",
          "form": "커피가 와르르 ㅎㅎ",
          "original_form": "커피가 와르르 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043934.1.22",
