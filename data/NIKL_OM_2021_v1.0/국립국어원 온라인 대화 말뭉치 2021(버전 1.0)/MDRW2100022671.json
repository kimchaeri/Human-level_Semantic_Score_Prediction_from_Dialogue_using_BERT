          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022671.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022671.1.30",
          "form": "맞아요 자리 진짜 중요함",
          "original_form": "맞아요 자리 진짜 중요함",
