        },
        {
          "id": "MDRW2100022671.1.2",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022671.1.3",
          "form": "최근에 초밥이 많이 먹고 싶어졌어요",
          "original_form": "최근에 초밥이 많이 먹고 싶어졌어요",
          "speaker_id": "1",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022671.1.4",
          "form": "그래서 얼마전에 초밥 뷔페 갔거든요...?",
          "original_form": "그래서 얼마전에 초밥 뷔페 갔거든요...?",
          "speaker_id": "1",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022671.1.5",
          "form": "그것도 점심 안먹고 배고픔 참고 갔는데...ㅠㅠㅠㅠㅠㅠ",
          "original_form": "그것도 점심 안먹고 배고픔 참고 갔는데...ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022671.1.6",
          "form": "코로나 4단계로 단축영업해서 못먹었어요 ^.^",
          "original_form": "코로나 4단계로 단축영업해서 못먹었어요 ^.^",
          "speaker_id": "1",
          "time": "20210829 16:52"
        },
        {
          "id": "MDRW2100022671.1.7",
          "form": "아....진짜 절망적이엇겟는데요 ㅠㅠ",
          "original_form": "아....진짜 절망적이엇겟는데요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022671.1.8",
          "form": "그래서 뭐드셧어요 ㅠ",
          "original_form": "그래서 뭐드셧어요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022671.1.9",
          "form": "맞아요 ㅋㅋㅋ 그래서 그냥 거기 근처에 있는 맛집 검색해서 갔거든용 ?!!",
          "original_form": "맞아요 ㅋㅋㅋ 그래서 그냥 거기 근처에 있는 맛집 검색해서 갔거든용 ?!!",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022671.1.10",
          "form": "근데 브레이크 타임이래욬ㅋㅋㅋㅋ",
          "original_form": "근데 브레이크 타임이래욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022671.1.11",
          "form": "아닠ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022671.1.12",
          "form": "진짜 운도 지지리 없죠..?",
          "original_form": "진짜 운도 지지리 없죠..?",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022671.1.13",
          "form": "진짜 그날은 좀 안될날이엇나봐요 ㅠ",
          "original_form": "진짜 그날은 좀 안될날이엇나봐요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022671.1.14",
          "form": "그래서 그날 그냥 후참 먹었어요..",
          "original_form": "그래서 그날 그냥 후참 먹었어요..",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022671.1.15",
          "form": "그래서 뭐드셧우요 ㅠㅠ",
          "original_form": "그래서 뭐드셧우요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022671.1.16",
          "form": "돌고 돌아 후참으로...",
          "original_form": "돌고 돌아 후참으로...",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022671.1.17",
          "form": "앜ㅋㅋㅋㅋㅋ결국후참ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ결국후참ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022671.1.18",
          "form": "혹시 초밥 회전초밥으로 하는 데 가봤어요?",
          "original_form": "혹시 초밥 회전초밥으로 하는 데 가봤어요?",
