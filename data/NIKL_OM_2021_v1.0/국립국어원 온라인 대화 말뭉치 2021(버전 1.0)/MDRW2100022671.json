        {
          "id": "MDRW2100022671.1.1",
          "form": "네 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
