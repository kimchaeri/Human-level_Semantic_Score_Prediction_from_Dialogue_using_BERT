      "utterance": [
        {
          "id": "MDRW2100022505.1.1",
          "form": "다이소 가본 적 있어요?",
          "original_form": "다이소 가본 적 있어요?",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022505.1.2",
          "form": "네 당연하죠!",
          "original_form": "네 당연하죠!",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022505.1.3",
          "form": "잘 안가세요?",
          "original_form": "잘 안가세요?",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022505.1.4",
          "form": "거기서 주로 뭐사요?",
          "original_form": "거기서 주로 뭐사요?",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022505.1.5",
          "form": "저는 면봉 주기적으로 사요 ㅋㅋㅋㅋ",
          "original_form": "저는 면봉 주기적으로 사요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022505.1.6",
          "form": "아 면봉 ㅋㅋ ㅋ그 원통에 담긴거요?",
          "original_form": "아 면봉 ㅋㅋ ㅋ그 원통에 담긴거요?",
          "speaker_id": "1",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022505.1.7",
          "form": "어디에다 써요?",
          "original_form": "어디에다 써요?",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022505.1.8",
          "form": "아니요 그 종이면봉있는데 좋더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "아니요 그 종이면봉있는데 좋더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022505.1.9",
          "form": "아 종이면봉이라는 것도 있구나~",
          "original_form": "아 종이면봉이라는 것도 있구나~",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022505.1.10",
          "form": "처음 알았네요 신기해요 ㅎㅎ",
          "original_form": "처음 알았네요 신기해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022505.1.11",
          "form": "네네 나무면봉보다 좋더라구요",
          "original_form": "네네 나무면봉보다 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 15:12"
        },
        {
          "id": "MDRW2100022505.1.12",
          "form": "다음에 면봉 살 일 있으면 저도 그거 사봐야겠어요",
          "original_form": "다음에 면봉 살 일 있으면 저도 그거 사봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022505.1.13",
          "form": "다른거는 좀 불량이 많아서요 ㅋㅋㅋ",
          "original_form": "다른거는 좀 불량이 많아서요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022505.1.14",
          "form": "얼마에요??",
          "original_form": "얼마에요??",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022505.1.15",
          "form": "400개에 이천원인가?",
          "original_form": "400개에 이천원인가?",
          "speaker_id": "2",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022505.1.16",
          "form": "그 정도면 괜찮네요 !",
          "original_form": "그 정도면 괜찮네요 !",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022505.1.17",
          "form": "품질도 좋은 편인가봐요?",
          "original_form": "품질도 좋은 편인가봐요?",
          "speaker_id": "1",
          "time": "20210829 15:13"
        },
        {
          "id": "MDRW2100022505.1.18",
          "form": "전 다이소 가면 비닐백 같은거 많이 사요",
          "original_form": "전 다이소 가면 비닐백 같은거 많이 사요",
          "speaker_id": "1",
          "time": "20210829 15:13"
        }
