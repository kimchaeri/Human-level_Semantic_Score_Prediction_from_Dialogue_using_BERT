      "utterance": [
        {
          "id": "MDRW2100017247.1.1",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.2",
          "form": "보통 1년이내 성장이 끝나니까",
          "original_form": "보통 1년이내 성장이 끝나니까",
          "speaker_id": "1",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.3",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.4",
          "form": "산책한번갔다가",
          "original_form": "산책한번갔다가",
          "speaker_id": "1",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.5",
          "form": "보기만 해서 동물 잘 몰라요",
          "original_form": "보기만 해서 동물 잘 몰라요",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.6",
          "form": "집가기싫다고 드러누우면",
          "original_form": "집가기싫다고 드러누우면",
          "speaker_id": "1",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.7",
          "form": "너무 난감해요",
          "original_form": "너무 난감해요",
          "speaker_id": "1",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.8",
          "form": "ㅋㅋㅋㅋㅋ 웃기다",
          "original_form": "ㅋㅋㅋㅋㅋ 웃기다",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.9",
          "form": "그럼 어떡하세요",
          "original_form": "그럼 어떡하세요",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.10",
          "form": "달래야죠",
          "original_form": "달래야죠",
          "speaker_id": "1",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.11",
          "form": "들고 가세요?",
          "original_form": "들고 가세요?",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.12",
          "form": "우리 집 가자 응?",
          "original_form": "우리 집 가자 응?",
          "speaker_id": "1",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017247.1.13",
          "form": "달래 ㅋㅋㅋㅋ",
          "original_form": "달래 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017247.1.14",
          "form": "못들어요ㅋㅋㅋ",
          "original_form": "못들어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017247.1.15",
          "form": "둘다 귀엽네요",
          "original_form": "둘다 귀엽네요",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017247.1.16",
          "form": "이름이 뭐예요?",
          "original_form": "이름이 뭐예요?",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
