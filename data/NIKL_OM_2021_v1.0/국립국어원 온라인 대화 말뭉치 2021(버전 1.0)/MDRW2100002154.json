      "utterance": [
        {
          "id": "MDRW2100002154.1.1",
          "form": "어제 친구 생일이었는데",
          "original_form": "어제 친구 생일이었는데",
          "speaker_id": "2",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002154.1.2",
          "form": "선물이 엄청 고민되더라",
          "original_form": "선물이 엄청 고민되더라",
          "speaker_id": "2",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002154.1.3",
          "form": "뭐 사줬어?",
          "original_form": "뭐 사줬어?",
          "speaker_id": "1",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002154.1.4",
          "form": "나도 생일때마다 고민되더라",
          "original_form": "나도 생일때마다 고민되더라",
          "speaker_id": "1",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002154.1.5",
          "form": "그래도 술을 좋아하는 친구라서",
          "original_form": "그래도 술을 좋아하는 친구라서",
          "speaker_id": "2",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002154.1.6",
          "form": "술을 사줬어 근데 종류가 너무 많아서 고르기 힘들더라",
          "original_form": "술을 사줬어 근데 종류가 너무 많아서 고르기 힘들더라",
          "speaker_id": "2",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002154.1.7",
          "form": "아 그렇겠다",
          "original_form": "아 그렇겠다",
          "speaker_id": "1",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002154.1.8",
          "form": "술을 사주다니 정말 어른같아.",
          "original_form": "술을 사주다니 정말 어른같아.",
          "speaker_id": "1",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002154.1.9",
          "form": "양주를 그렇게 오래본게 처음이였어",
          "original_form": "양주를 그렇게 오래본게 처음이였어",
          "speaker_id": "2",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002154.1.10",
          "form": "아무래도 어른이니까.",
          "original_form": "아무래도 어른이니까.",
          "speaker_id": "2",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002154.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어른같다. 양주선물.",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어른같다. 양주선물.",
          "speaker_id": "1",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002154.1.12",
          "form": "나는 최근에 올리브영 상품권을 선물했어.",
          "original_form": "나는 최근에 올리브영 상품권을 선물했어.",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002154.1.13",
          "form": "나는 사실 올리브영을 잘 가지안ㄹ지만",
          "original_form": "나는 사실 올리브영을 잘 가지안ㄹ지만",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002154.1.14",
          "form": "근데 백화점이랑 이마트랑 가격차이가 엄청나더라",
          "original_form": "근데 백화점이랑 이마트랑 가격차이가 엄청나더라",
          "speaker_id": "2",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002154.1.15",
          "form": "술은 가격 비슷할줄 알았는데",
          "original_form": "술은 가격 비슷할줄 알았는데",
          "speaker_id": "2",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002154.1.16",
          "form": "화장품 관련 선물에서는 무난한 거 같더라",
          "original_form": "화장품 관련 선물에서는 무난한 거 같더라",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002154.1.17",
          "form": "백화점이 많이 비싸지?",
          "original_form": "백화점이 많이 비싸지?",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002154.1.18",
          "form": "맞아 올리브영에는 뭐든 있으니까",
          "original_form": "맞아 올리브영에는 뭐든 있으니까",
          "speaker_id": "2",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002154.1.19",
          "form": "어 엄청",
          "original_form": "어 엄청",
          "speaker_id": "2",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002154.1.20",
          "form": "이런..",
          "original_form": "이런..",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002154.1.21",
          "form": "근데 백화점에서 선물하면 백화점 브랜드파워가 있어서",
          "original_form": "근데 백화점에서 선물하면 백화점 브랜드파워가 있어서",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002154.1.22",
          "form": "기분이 좋긴 하더라.",
          "original_form": "기분이 좋긴 하더라.",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002154.1.23",
          "form": "맞아 근데 술은 그럴필요가 없으니까",
          "original_form": "맞아 근데 술은 그럴필요가 없으니까",
          "speaker_id": "2",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002154.1.24",
          "form": "이마트에서 샀어",
          "original_form": "이마트에서 샀어",
          "speaker_id": "2",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002154.1.25",
          "form": "하지만 백화점 쇼핑백이 있잖아.",
          "original_form": "하지만 백화점 쇼핑백이 있잖아.",
          "speaker_id": "1",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002154.1.26",
          "form": "오 그렇네",
          "original_form": "오 그렇네",
          "speaker_id": "2",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002154.1.27",
          "form": "하지만 양주는 비싸니까",
          "original_form": "하지만 양주는 비싸니까",
          "speaker_id": "1",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002154.1.28",
          "form": "넘어가자.",
          "original_form": "넘어가자.",
          "speaker_id": "1",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002154.1.29",
          "form": "다음에는 쇼핑백만 사서 넣어줘야겠다",
          "original_form": "다음에는 쇼핑백만 사서 넣어줘야겠다",
          "speaker_id": "2",
          "time": "20210820 00:08"
        }
      ]
