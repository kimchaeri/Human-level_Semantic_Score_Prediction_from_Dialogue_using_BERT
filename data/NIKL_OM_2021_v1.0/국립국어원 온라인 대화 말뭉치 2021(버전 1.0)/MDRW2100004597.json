      "utterance": [
        {
          "id": "MDRW2100004597.1.1",
          "form": "요즘 서울 댕로에서 해적이 유명하다던데 보셨나요?",
          "original_form": "요즘 서울 댕로에서 해적이 유명하다던데 보셨나요?",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004597.1.2",
          "form": "하..해적 보고싶엇는데요....",
          "original_form": "하..해적 보고싶엇는데요....",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004597.1.3",
          "form": "하..코로나쉑이...",
          "original_form": "하..코로나쉑이...",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004597.1.4",
          "form": "그리고 제 자리가 ..",
          "original_form": "그리고 제 자리가 ..",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004597.1.5",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004597.1.6",
          "form": "저 뮤지컬좋아해요...",
          "original_form": "저 뮤지컬좋아해요...",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004597.1.7",
          "form": "이렇게 급발진하는거 너무 오타쿠같지않나요...",
          "original_form": "이렇게 급발진하는거 너무 오타쿠같지않나요...",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004597.1.8",
          "form": "하지만 좋은걸어떡해...",
          "original_form": "하지만 좋은걸어떡해...",
          "speaker_id": "2",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004597.1.9",
          "form": "괜찮아요 저도 트친이 맨날 해적보라고 바짓가랑이 잡고 안 놔줘요",
          "original_form": "괜찮아요 저도 트친이 맨날 해적보라고 바짓가랑이 잡고 안 놔줘요",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004597.1.10",
          "form": "부산 사는 사람한테 말이야",
          "original_form": "부산 사는 사람한테 말이야",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004597.1.11",
          "form": "지방은 인프라가 너무 후져요..",
          "original_form": "지방은 인프라가 너무 후져요..",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004597.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ부산은 진짜 끝에서끝이네요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ부산은 진짜 끝에서끝이네요...",
          "speaker_id": "2",
          "time": "20210821 17:25"
        },
        {
          "id": "MDRW2100004597.1.13",
          "form": "뮤..가끔지방 오면 미친듯이 돌아야해요...",
          "original_form": "뮤..가끔지방 오면 미친듯이 돌아야해요...",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004597.1.14",
          "form": "전에 어쩌다 표를 두개 예매해버려서 친구한테 같이 보러가자했는데 거절당했었어요",
          "original_form": "전에 어쩌다 표를 두개 예매해버려서 친구한테 같이 보러가자했는데 거절당했었어요",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004597.1.15",
          "form": "안그래도 인프라 적은데 목마른 지방인들 몰려서 서울보다 티켓팅 힘들어요",
          "original_form": "안그래도 인프라 적은데 목마른 지방인들 몰려서 서울보다 티켓팅 힘들어요",
          "speaker_id": "1",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004597.1.16",
          "form": "그친구가 대학원생이라....",
          "original_form": "그친구가 대학원생이라....",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004597.1.17",
          "form": "와 저한테 물어봐주세요 당장갈게요",
          "original_form": "와 저한테 물어봐주세요 당장갈게요",
          "speaker_id": "1",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004597.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004597.1.19",
          "form": "그때 자리도 진짜 좋았는데..",
          "original_form": "그때 자리도 진짜 좋았는데..",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004597.1.20",
          "form": "결국 한자리취소하고 혼자봤어요...",
          "original_form": "결국 한자리취소하고 혼자봤어요...",
          "speaker_id": "2",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004597.1.21",
          "form": "너무 슬퍼요.......",
          "original_form": "너무 슬퍼요.......",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004597.1.22",
          "form": "저를 불러주세요........",
          "original_form": "저를 불러주세요........",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004597.1.23",
          "form": "대학원생 배가불러가지고말야",
          "original_form": "대학원생 배가불러가지고말야",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004597.1.24",
          "form": "떼잉~",
          "original_form": "떼잉~",
          "speaker_id": "1",
          "time": "20210821 17:27"
        },
        {
          "id": "MDRW2100004597.1.25",
          "form": "마저요...떼잉~~~~~~",
          "original_form": "마저요...떼잉~~~~~~",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004597.1.26",
          "form": "그 때 봤던 뮤는 무슨 극이었나요?",
          "original_form": "그 때 봤던 뮤는 무슨 극이었나요?",
          "speaker_id": "1",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004597.1.27",
          "form": "키다리아저씨....",
          "original_form": "키다리아저씨....",
          "speaker_id": "2",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004597.1.28",
          "form": "아니들어봐요...제루샤가요....",
          "original_form": "아니들어봐요...제루샤가요....",
          "speaker_id": "2",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004597.1.29",
          "form": "우리제루샤가...",
          "original_form": "우리제루샤가...",
          "speaker_id": "2",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004597.1.30",
          "form": "첫 넘버가",
          "original_form": "첫 넘버가",
          "speaker_id": "2",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004597.1.31",
          "form": "제루샤가 극에서 무슨 역인가요?",
          "original_form": "제루샤가 극에서 무슨 역인가요?",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004597.1.32",
          "form": "자기가 고아원 제일큰언니라고...",
          "original_form": "자기가 고아원 제일큰언니라고...",
          "speaker_id": "2",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004597.1.33",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004597.1.34",
          "form": "내여친",
          "original_form": "내여친",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004597.1.35",
          "form": "눈물이 좍좍",
          "original_form": "눈물이 좍좍",
          "speaker_id": "2",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004597.1.36",
          "form": "키다리 누가 일상힐링극이라고 했어...!!!!",
          "original_form": "키다리 누가 일상힐링극이라고 했어...!!!!",
          "speaker_id": "2",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004597.1.37",
          "form": "시작부터 눈물좍좍뽑고",
          "original_form": "시작부터 눈물좍좍뽑고",
          "speaker_id": "2",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004597.1.38",
          "form": "끝날때까지울었어요...",
          "original_form": "끝날때까지울었어요...",
          "speaker_id": "2",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004597.1.39",
