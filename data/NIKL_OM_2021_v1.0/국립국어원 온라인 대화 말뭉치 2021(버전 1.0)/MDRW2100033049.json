      "utterance": [
        {
          "id": "MDRW2100033049.1.1",
          "form": "빌려서만 봐요",
          "original_form": "빌려서만 봐요",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033049.1.2",
          "form": "근로장려금이 뭐죠",
          "original_form": "근로장려금이 뭐죠",
          "speaker_id": "2",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033049.1.3",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033049.1.4",
          "form": "취업전에 장려금주는건가",
          "original_form": "취업전에 장려금주는건가",
          "speaker_id": "2",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033049.1.5",
          "form": "근로장려금이라는게",
          "original_form": "근로장려금이라는게",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033049.1.6",
          "form": "네 그런건가봐요?...",
          "original_form": "네 그런건가봐요?...",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033049.1.7",
          "form": "아니근데 이번에",
          "original_form": "아니근데 이번에",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033049.1.8",
          "form": "지원금도...",
          "original_form": "지원금도...",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033049.1.9",
          "form": "기준이 이상하던데요...",
          "original_form": "기준이 이상하던데요...",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033049.1.10",
          "form": "맞죠",
          "original_form": "맞죠",
          "speaker_id": "2",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033049.1.11",
          "form": "불만인사람 많더라구요",
          "original_form": "불만인사람 많더라구요",
          "speaker_id": "2",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033049.1.12",
          "form": "그걸로 또 다들 너무싸워대서",
          "original_form": "그걸로 또 다들 너무싸워대서",
          "speaker_id": "2",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033049.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033049.1.14",
          "form": "아니 이걸로 국민들",
          "original_form": "아니 이걸로 국민들",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033049.1.15",
          "form": "편가르기 하는 것 같아요 진짜",
          "original_form": "편가르기 하는 것 같아요 진짜",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033049.1.16",
          "form": "그냥다주지 싶었어요",
          "original_form": "그냥다주지 싶었어요",
          "speaker_id": "2",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033049.1.17",
          "form": "근로장려금도 비슷할걸요?",
          "original_form": "근로장려금도 비슷할걸요?",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033049.1.18",
          "form": "저도 그렇게 생각해요",
          "original_form": "저도 그렇게 생각해요",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033049.1.19",
          "form": "안그래도 동사무소가서",
          "original_form": "안그래도 동사무소가서",
          "speaker_id": "2",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.20",
          "form": "근로장려금도 소득분위 따져서",
          "original_form": "근로장려금도 소득분위 따져서",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.21",
          "form": "어떤 청년은 받고 누구는 못받고 그럴텐데;;;",
          "original_form": "어떤 청년은 받고 누구는 못받고 그럴텐데;;;",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.22",
          "form": "돈왜안주냐고 그렇게 따진다네요",
          "original_form": "돈왜안주냐고 그렇게 따진다네요",
          "speaker_id": "2",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.23",
          "form": "근데 그소득분위도",
          "original_form": "근데 그소득분위도",
          "speaker_id": "2",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.24",
          "form": "얼마든지 속일수있어서",
          "original_form": "얼마든지 속일수있어서",
          "speaker_id": "2",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.26",
          "form": "아니 조국만 봐도 ㅋㅋㅋㅋ",
          "original_form": "아니 조국만 봐도 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.27",
          "form": "너무 정책에 구멍이 많은거같아요",
          "original_form": "너무 정책에 구멍이 많은거같아요",
          "speaker_id": "2",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.28",
          "form": "조국 걔네도 잘사는데",
          "original_form": "조국 걔네도 잘사는데",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.29",
          "form": "장학금이란 장학금은 다 받았잖아요",
          "original_form": "장학금이란 장학금은 다 받았잖아요",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.30",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.31",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "2",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.32",
          "form": "조민이 받은 장학금도",
          "original_form": "조민이 받은 장학금도",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033049.1.33",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 14:43"
        }
      ]
    }
