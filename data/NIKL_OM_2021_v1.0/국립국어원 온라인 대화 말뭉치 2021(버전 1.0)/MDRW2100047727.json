      "utterance": [
        {
          "id": "MDRW2100047727.1.1",
          "form": "아 그래서 궁금함 워킹화 어디꺼가좋음?",
          "original_form": "아 그래서 궁금함 워킹화 어디꺼가좋음?",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100047727.1.2",
          "form": "아시나요 name2님",
          "original_form": "아시나요 &name2&님",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100047727.1.3",
          "form": "그 내친구가 신던거 머엿지",
          "original_form": "그 내친구가 신던거 머엿지",
          "speaker_id": "2",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100047727.1.4",
          "form": "스케쳐스",
          "original_form": "스케쳐스",
          "speaker_id": "2",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100047727.1.5",
          "form": "아 그거본 것 같다",
          "original_form": "아 그거본 것 같다",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100047727.1.6",
          "form": "세상은 넓고 갖고 싶은건 많아",
          "original_form": "세상은 넓고 갖고 싶은건 많아",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100047727.1.7",
          "form": "그게 좋대 신발이 온라인가입하믄 마넌할인쿠폰주더라",
          "original_form": "그게 좋대 신발이 온라인가입하믄 마넌할인쿠폰주더라",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100047727.1.8",
          "form": "비 오는 날도 신을 수 있나염",
          "original_form": "비 오는 날도 신을 수 있나염",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100047727.1.9",
          "form": "그건몰랑 안신어봤엉",
          "original_form": "그건몰랑 안신어봤엉",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100047727.1.10",
          "form": "좋은 워킹화가 갖고 싶음",
          "original_form": "좋은 워킹화가 갖고 싶음",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100047727.1.11",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100047727.1.12",
          "form": "나는 당근에서 나이키샀오",
          "original_form": "나는 당근에서 나이키샀오",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100047727.1.13",
          "form": "오 신발 깨끗함?",
          "original_form": "오 신발 깨끗함?",
          "speaker_id": "1",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100047727.1.14",
          "form": "흰색 새거 삼처넌",
          "original_form": "흰색 새거 삼처넌",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100047727.1.15",
          "form": "와 말도 안되는가격이다",
          "original_form": "와 말도 안되는가격이다",
          "speaker_id": "1",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100047727.1.16",
          "form": "나랑 다른앱인가",
          "original_form": "나랑 다른앱인가",
          "speaker_id": "1",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100047727.1.17",
          "form": "난 그런거없던데 ㅋㅋ",
          "original_form": "난 그런거없던데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100047727.1.18",
          "form": "아직 안신어봄 여름엔",
          "original_form": "아직 안신어봄 여름엔",
          "speaker_id": "2",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100047727.1.19",
          "form": "샌들신으니까",
          "original_form": "샌들신으니까",
          "speaker_id": "2",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100047727.1.20",
          "form": "그래도 나이키가 삼만원도 아니고 삼천원이라니ㄷㄷㄷ",
          "original_form": "그래도 나이키가 삼만원도 아니고 삼천원이라니ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100047727.1.21",
          "form": "웅 싸게올라와서 좋았당 구형이지만 깨끗하구",
          "original_form": "웅 싸게올라와서 좋았당 구형이지만 깨끗하구",
          "speaker_id": "2",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100047727.1.22",
          "form": "ㅇㅇ 그가격이면 진짜 득템했네요",
          "original_form": "ㅇㅇ 그가격이면 진짜 득템했네요",
          "speaker_id": "1",
          "time": "20210827 13:55"
        }
      ]
