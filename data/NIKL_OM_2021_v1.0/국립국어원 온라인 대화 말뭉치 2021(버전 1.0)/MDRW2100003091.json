      "utterance": [
        {
          "id": "MDRW2100003091.1.1",
          "form": "엉 가봤지 경북궁이랑",
          "original_form": "엉 가봤지 경북궁이랑",
          "speaker_id": "2",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003091.1.2",
          "form": "덕수궁 수문장교대의식 봤어?",
          "original_form": "덕수궁 수문장교대의식 봤어?",
          "speaker_id": "1",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003091.1.3",
          "form": "아니 기다리기에는 계획이 있어서리",
          "original_form": "아니 기다리기에는 계획이 있어서리",
          "speaker_id": "2",
          "time": "20210820 18:32"
        },
        {
          "id": "MDRW2100003091.1.4",
          "form": "이게 96년 고증을 해서 하는거란다",
          "original_form": "이게 96년 고증을 해서 하는거란다",
          "speaker_id": "1",
          "time": "20210820 18:32"
        },
        {
          "id": "MDRW2100003091.1.5",
          "form": "고증까지 대단한데 그럼 한번 봐야겠다",
          "original_form": "고증까지 대단한데 그럼 한번 봐야겠다",
          "speaker_id": "2",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003091.1.6",
          "form": "응 영국 왕실 근위병교대도 보는데 우리나라껀 꼭 봐야지",
          "original_form": "응 영국 왕실 근위병교대도 보는데 우리나라껀 꼭 봐야지",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003091.1.7",
          "form": "응 정해진 시간이 있겠네",
          "original_form": "응 정해진 시간이 있겠네",
          "speaker_id": "2",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003091.1.8",
          "form": "응 정해진 의상입고 하루 3회 교대의식한다네",
          "original_form": "응 정해진 의상입고 하루 3회 교대의식한다네",
          "speaker_id": "1",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100003091.1.9",
          "form": "3회나 생각보다 많네",
          "original_form": "3회나 생각보다 많네",
          "speaker_id": "2",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003091.1.10",
          "form": "11시부터 40분간",
          "original_form": "11시부터 40분간",
          "speaker_id": "1",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003091.1.11",
          "form": "40분이나 엄청 긴데",
          "original_form": "40분이나 엄청 긴데",
          "speaker_id": "2",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003091.1.12",
          "form": "그러게 생각보다 길다",
          "original_form": "그러게 생각보다 길다",
          "speaker_id": "1",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003091.1.13",
          "form": "나머지는",
          "original_form": "나머지는",
          "speaker_id": "2",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003091.1.14",
          "form": "2시 3시",
          "original_form": "2시 3시",
          "speaker_id": "1",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003091.1.15",
          "form": "아~오후에는 거의 연달아하네",
          "original_form": "아~오후에는 거의 연달아하네",
          "speaker_id": "2",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003091.1.16",
          "form": "관광객 때문에 그렇겠지",
          "original_form": "관광객 때문에 그렇겠지",
          "speaker_id": "1",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003091.1.17",
          "form": "그럴수도",
          "original_form": "그럴수도",
          "speaker_id": "2",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003091.1.18",
          "form": "3시는 3시30분이다",
          "original_form": "3시는 3시30분이다",
          "speaker_id": "1",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003091.1.19",
          "form": "응 365일 하는거야",
          "original_form": "응 365일 하는거야",
          "speaker_id": "2",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003091.1.20",
          "form": "아니 월요일휴무",
          "original_form": "아니 월요일휴무",
          "speaker_id": "1",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003091.1.21",
          "form": "수문장교대의식이 여러군데서 한다",
          "original_form": "수문장교대의식이 여러군데서 한다",
