      "utterance": [
        {
          "id": "MDRW2100011532.1.1",
          "form": "미국 대학을 가는 여러가지 방편이 있구나",
          "original_form": "미국 대학을 가는 여러가지 방편이 있구나",
          "speaker_id": "2",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011532.1.2",
          "form": "찾아보면나갈방법은",
          "original_form": "찾아보면나갈방법은",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011532.1.3",
          "form": "다있긴한듯",
          "original_form": "다있긴한듯",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011532.1.4",
          "form": "해외나간사례가있음?",
          "original_form": "해외나간사례가있음?",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011532.1.5",
          "form": "ㅇㅇㅇ학비때문에못나가는사람들많은데",
          "original_form": "ㅇㅇㅇ학비때문에못나가는사람들많은데",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011532.1.6",
          "form": "선배중에?",
          "original_form": "선배중에?",
          "speaker_id": "1",
          "time": "20210824 13:39"
        },
        {
          "id": "MDRW2100011532.1.7",
          "form": "선배를 본 적이 없어서 모르겠는데",
          "original_form": "선배를 본 적이 없어서 모르겠는데",
          "speaker_id": "2",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011532.1.8",
          "form": "에타에 물어보고 올게",
          "original_form": "에타에 물어보고 올게",
          "speaker_id": "2",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011532.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아",
          "speaker_id": "1",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011532.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ선배중에얘기들으녹없음",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ선배중에얘기들으녹없음",
          "speaker_id": "1",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011532.1.11",
          "form": "들은적없음?",
          "original_form": "들은적없음?",
          "speaker_id": "1",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011532.1.12",
          "form": "톡같은걸로",
          "original_form": "톡같은걸로",
          "speaker_id": "1",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011532.1.13",
          "form": "일단 선배를 못만났고 코로나가 2년 정도 지속되면서",
          "original_form": "일단 선배를 못만났고 코로나가 2년 정도 지속되면서",
          "speaker_id": "2",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011532.1.14",
          "form": "신입생들도 아무것도 모른 채 2학년이 되어버려",
          "original_form": "신입생들도 아무것도 모른 채 2학년이 되어버려",
          "speaker_id": "2",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011532.1.15",
          "form": "그런 말을 들을 틈이 없었음",
          "original_form": "그런 말을 들을 틈이 없었음",
          "speaker_id": "2",
          "time": "20210824 13:41"
        },
        {
          "id": "MDRW2100011532.1.16",
          "form": "아 하긴",
          "original_form": "아 하긴",
          "speaker_id": "1",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100011532.1.17",
          "form": "학과에서선배랑톡방안만들어줬어?",
          "original_form": "학과에서선배랑톡방안만들어줬어?",
          "speaker_id": "1",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100011532.1.18",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "1",
          "time": "20210824 13:42"
        },
        {
          "id": "MDRW2100011532.1.19",
          "form": "오티때 뭐 물어보라고 하는게 다였어 아마",
          "original_form": "오티때 뭐 물어보라고 하는게 다였어 아마",
          "speaker_id": "2",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011532.1.20",
          "form": "내가 학교를 안나가서 그런가",
          "original_form": "내가 학교를 안나가서 그런가",
          "speaker_id": "2",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011532.1.21",
          "form": "가려고 정한 학교 있어?",
          "original_form": "가려고 정한 학교 있어?",
          "speaker_id": "2",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100011532.1.22",
          "form": "ㄴㄴㄴ그냥 받아주는 학교에 우선 원서 다 넣고",
          "original_form": "ㄴㄴㄴ그냥 받아주는 학교에 우선 원서 다 넣고",
          "speaker_id": "1",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011532.1.23",
          "form": "합격하는데중에학비",
          "original_form": "합격하는데중에학비",
          "speaker_id": "1",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011532.1.24",
          "form": "그나마싼데로",
          "original_form": "그나마싼데로",
          "speaker_id": "1",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011532.1.25",
          "form": "갈예정임",
          "original_form": "갈예정임",
          "speaker_id": "1",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011532.1.26",
          "form": "아 미국 학비 비싸다던데",
          "original_form": "아 미국 학비 비싸다던데",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011532.1.27",
          "form": "ㅇㅇㅇ나는펀딩받을수도없어서",
          "original_form": "ㅇㅇㅇ나는펀딩받을수도없어서",
          "speaker_id": "1",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011532.1.28",
          "form": "1년 1억 생각하고 가래",
          "original_form": "1년 1억 생각하고 가래",
          "speaker_id": "1",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011532.1.29",
          "form": "생활비도합쳐서",
          "original_form": "생활비도합쳐서",
          "speaker_id": "1",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011532.1.30",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210824 13:45"
        },
        {
          "id": "MDRW2100011532.1.31",
          "form": "진짜 비싸다 1억?? 이야",
          "original_form": "진짜 비싸다 1억?? 이야",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011532.1.32",
          "form": "ㅇㅇㅇ학비도비싸고방값도비싸서",
          "original_form": "ㅇㅇㅇ학비도비싸고방값도비싸서",
          "speaker_id": "1",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011532.1.33",
          "form": "그래서 너 나가는 시기랑 비슷하면 룸메하려고 했는데",
          "original_form": "그래서 너 나가는 시기랑 비슷하면 룸메하려고 했는데",
          "speaker_id": "1",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011532.1.34",
          "form": "아 나가게 되면 27년도 즈음에 나가게 되는 거임??",
          "original_form": "아 나가게 되면 27년도 즈음에 나가게 되는 거임??",
          "speaker_id": "2",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011532.1.35",
          "form": "만약에 그렇다면 그때까지 일단 한국에서 자리 잡아 놓고 한 밑천 잡은 다음에 나가야할 듯",
          "original_form": "만약에 그렇다면 그때까지 일단 한국에서 자리 잡아 놓고 한 밑천 잡은 다음에 나가야할 듯",
          "speaker_id": "2",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011532.1.36",
          "form": "ㅇㅇ이대로계속다니면",
          "original_form": "ㅇㅇ이대로계속다니면",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011532.1.37",
          "form": "돈벌고나가려고?",
          "original_form": "돈벌고나가려고?",
          "speaker_id": "1",
          "time": "20210824 13:49"
        },
        {
          "id": "MDRW2100011532.1.38",
          "form": "근데수의학과가미국내에서",
          "original_form": "근데수의학과가미국내에서",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011532.1.39",
          "form": "응응 27년이면 졸업마치고 4년 정도 지났을 거니까 아마",
          "original_form": "응응 27년이면 졸업마치고 4년 정도 지났을 거니까 아마",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011532.1.40",
          "form": "특히학비가더비싸긴함",
          "original_form": "특히학비가더비싸긴함",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011532.1.41",
          "form": "오 보통 의학 들어가면 다 비싸진다더라",
          "original_form": "오 보통 의학 들어가면 다 비싸진다더라",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011532.1.42",
          "form": "다른학과는조금더나을수도있음",
          "original_form": "다른학과는조금더나을수도있음",
          "speaker_id": "1",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011532.1.43",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210824 13:50"
        },
        {
          "id": "MDRW2100011532.1.44",
          "form": "근데4년맞음?ㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데4년맞음?ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011532.1.45",
          "form": "휴학할거아님?",
          "original_form": "휴학할거아님?",
          "speaker_id": "1",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011532.1.46",
          "form": "휴헉 안라면",
          "original_form": "휴헉 안라면",
          "speaker_id": "2",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011532.1.47",
          "form": "아니 저긴ㅅ",
          "original_form": "아니 저긴ㅅ",
          "speaker_id": "2",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011532.1.48",
          "form": "안할 걸?",
          "original_form": "안할 걸?",
          "speaker_id": "2",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011532.1.49",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ한학기마다휴학할거",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ한학기마다휴학할거",
          "speaker_id": "1",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011532.1.50",
          "form": "아니었음?",
          "original_form": "아니었음?",
          "speaker_id": "1",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011532.1.51",
          "form": "아니야 나 그러다가 쫓겨나",
          "original_form": "아니야 나 그러다가 쫓겨나",
          "speaker_id": "2",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011532.1.52",
          "form": "이왕 나갈거 내 의지로 나갈래",
          "original_form": "이왕 나갈거 내 의지로 나갈래",
          "speaker_id": "2",
          "time": "20210824 13:51"
        }
