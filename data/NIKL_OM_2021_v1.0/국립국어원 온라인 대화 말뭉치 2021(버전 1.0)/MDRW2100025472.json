      "utterance": [
        {
          "id": "MDRW2100025472.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025472.1.2",
          "form": "가벼운 걷기운동 밖에 안해요",
          "original_form": "가벼운 걷기운동 밖에 안해요",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025472.1.3",
          "form": "무슨 운동 하시나요?",
          "original_form": "무슨 운동 하시나요?",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025472.1.4",
          "form": "홈트요",
          "original_form": "홈트요",
          "speaker_id": "1",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100025472.1.5",
          "form": "요즘엔 홈트가 안전하고 편하지요",
          "original_form": "요즘엔 홈트가 안전하고 편하지요",
          "speaker_id": "2",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100025472.1.6",
          "form": "저도 실내 자전거 사고싶어요",
          "original_form": "저도 실내 자전거 사고싶어요",
          "speaker_id": "2",
          "time": "20210901 11:55"
        },
        {
          "id": "MDRW2100025472.1.7",
          "form": "전 플랭크요",
          "original_form": "전 플랭크요",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025472.1.8",
          "form": "와우 찾아보고 왔는데 보기만 해도 힘드네요",
          "original_form": "와우 찾아보고 왔는데 보기만 해도 힘드네요",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025472.1.9",
          "form": "몸이 좋으신가 봐요",
          "original_form": "몸이 좋으신가 봐요",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025472.1.10",
          "form": "아니여 살이 너무 쪄서",
          "original_form": "아니여 살이 너무 쪄서",
          "speaker_id": "1",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100025472.1.11",
          "form": "저두요 체지방이 너무 높더라구요",
          "original_form": "저두요 체지방이 너무 높더라구요",
          "speaker_id": "2",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100025472.1.12",
          "form": "체지방 측정 체중계 질렀는데 충격",
          "original_form": "체지방 측정 체중계 질렀는데 충격",
          "speaker_id": "2",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100025472.1.13",
          "form": "잘되나요",
          "original_form": "잘되나요",
          "speaker_id": "1",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100025472.1.14",
          "form": "잘 맞지 않을까 싶어요  살이 찌긴 해서",
          "original_form": "잘 맞지 않을까 싶어요  살이 찌긴 해서",
          "speaker_id": "2",
          "time": "20210901 12:07"
        },
        {
          "id": "MDRW2100025472.1.15",
          "form": "유투브 운동 영상도 잘 되있던데 그거라도 해야겠어요",
          "original_form": "유투브 운동 영상도 잘 되있던데 그거라도 해야겠어요",
          "speaker_id": "2",
          "time": "20210901 12:07"
        },
        {
          "id": "MDRW2100025472.1.16",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210901 12:08"
        },
        {
          "id": "MDRW2100025472.1.17",
          "form": "다이어트는 식단 조절도 중요한거같아요",
