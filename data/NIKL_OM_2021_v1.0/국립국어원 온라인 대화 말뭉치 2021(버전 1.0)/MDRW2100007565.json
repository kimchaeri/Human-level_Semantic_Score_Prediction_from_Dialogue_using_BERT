      "utterance": [
        {
          "id": "MDRW2100007565.1.1",
          "form": "염색이나 탈색 해봤나요",
          "original_form": "염색이나 탈색 해봤나요",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007565.1.2",
          "form": "앗 네네",
          "original_form": "앗 네네",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007565.1.3",
          "form": "근데 하",
          "original_form": "근데 하",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007565.1.4",
          "form": "예전에는 즐겼는데 요즘은 못즐겨요",
          "original_form": "예전에는 즐겼는데 요즘은 못즐겨요",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007565.1.5",
          "form": "탈모가 오는 것 같아서",
          "original_form": "탈모가 오는 것 같아서",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007565.1.6",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007565.1.7",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007565.1.8",
          "form": "탈모 무섭죠",
          "original_form": "탈모 무섭죠",
          "speaker_id": "1",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007565.1.9",
          "form": "염색 좋아하시나요",
          "original_form": "염색 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 03:18"
        },
        {
          "id": "MDRW2100007565.1.10",
          "form": "네 저도 자주해요",
          "original_form": "네 저도 자주해요",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007565.1.11",
          "form": "쿨톤? 웜톤?",
          "original_form": "쿨톤? 웜톤?",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007565.1.12",
          "form": "셀프염색으로",
          "original_form": "셀프염색으로",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007565.1.13",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007565.1.14",
          "form": "무슨색으로 제일 많이 하세요?",
          "original_form": "무슨색으로 제일 많이 하세요?",
          "speaker_id": "2",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007565.1.15",
          "form": "저는 다양한 색 많이 해요",
          "original_form": "저는 다양한 색 많이 해요",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007565.1.16",
          "form": "최근에는 보라색 했었어요",
          "original_form": "최근에는 보라색 했었어요",
          "speaker_id": "1",
          "time": "20210822 03:19"
        },
        {
          "id": "MDRW2100007565.1.17",
          "form": "홀",
          "original_form": "홀",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007565.1.18",
          "form": "너무 이뻣겠다",
          "original_form": "너무 이뻣겠다",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007565.1.19",
          "form": "진짜 이쁜데 유지력이 아쉽더라구요ㅠ",
          "original_form": "진짜 이쁜데 유지력이 아쉽더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007565.1.20",
          "form": "애쉬 섞인걸로요?!",
          "original_form": "애쉬 섞인걸로요?!",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007565.1.21",
          "form": "아 그 러쉬에 보라색 샴푸쓰면 염색 오래간다던데 맞나요?!",
          "original_form": "아 그 러쉬에 보라색 샴푸쓰면 염색 오래간다던데 맞나요?!",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007565.1.22",
          "form": "애쉬가 섞였나..? 약간 청보라색",
          "original_form": "애쉬가 섞였나..? 약간 청보라색",
          "speaker_id": "1",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007565.1.23",
          "form": "대디오였나",
          "original_form": "대디오였나",
          "speaker_id": "2",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007565.1.24",
          "form": "아 그거 쓰면 조금 더 오래가긴하는데",
          "original_form": "아 그거 쓰면 조금 더 오래가긴하는데",
          "speaker_id": "1",
          "time": "20210822 03:20"
        },
        {
          "id": "MDRW2100007565.1.25",
          "form": "어.. 머릿결이 진짜 골로가요",
          "original_form": "어.. 머릿결이 진짜 골로가요",
          "speaker_id": "1",
