      "utterance": [
        {
          "id": "MDRW2100022375.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210829 13:14"
        },
        {
          "id": "MDRW2100022375.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022375.1.3",
          "form": "요즘 선선해져서 좋죠",
          "original_form": "요즘 선선해져서 좋죠",
          "speaker_id": "2",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022375.1.4",
          "form": "더운 여름이 지나서 좋은데 도로 장마 같아서 좀 습하네요",
          "original_form": "더운 여름이 지나서 좋은데 도로 장마 같아서 좀 습하네요",
          "speaker_id": "1",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022375.1.5",
          "form": "맞아요 습해서 빨래가 잘 안말라요",
          "original_form": "맞아요 습해서 빨래가 잘 안말라요",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022375.1.6",
          "form": "이번주도 비소식 있던대",
          "original_form": "이번주도 비소식 있던대",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022375.1.7",
          "form": "맞습니다 건조기가 필요해요",
          "original_form": "맞습니다 건조기가 필요해요",
          "speaker_id": "1",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100022375.1.8",
          "form": "왜이리 날씨가 이상해졌는지",
          "original_form": "왜이리 날씨가 이상해졌는지",
          "speaker_id": "1",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100022375.1.9",
          "form": "요즘은 정말 기상이 무서워요",
          "original_form": "요즘은 정말 기상이 무서워요",
          "speaker_id": "2",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100022375.1.10",
          "form": "미국은 50도까지 올라가고",
          "original_form": "미국은 50도까지 올라가고",
          "speaker_id": "2",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100022375.1.11",
          "form": "남미는 폭설내리고",
          "original_form": "남미는 폭설내리고",
          "speaker_id": "2",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100022375.1.12",
          "form": "이상기후현상이 점점 심해질듯 합니다",
          "original_form": "이상기후현상이 점점 심해질듯 합니다",
          "speaker_id": "1",
          "time": "20210830 06:09"
        },
        {
          "id": "MDRW2100022375.1.13",
          "form": "우리나라도 점점 아열대서 기후 되어 가는데 좋은건지 모르겠어요",
          "original_form": "우리나라도 점점 아열대서 기후 되어 가는데 좋은건지 모르겠어요",
          "speaker_id": "2",
          "time": "20210830 07:14"
        },
        {
          "id": "MDRW2100022375.1.14",
          "form": "여름 좋아하세요?",
          "original_form": "여름 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 07:15"
        },
        {
          "id": "MDRW2100022375.1.15",
          "form": "제가 며칠 입원하는 바람에 답이 늦었습니다..",
          "original_form": "제가 며칠 입원하는 바람에 답이 늦었습니다..",
          "speaker_id": "1",
          "time": "20210903 14:53"
        },
        {
          "id": "MDRW2100022375.1.16",
          "form": "저는 여름 좋아합니다",
          "original_form": "저는 여름 좋아합니다",
          "speaker_id": "1",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100022375.1.17",
          "form": "무슨일 있으셨나요?",
          "original_form": "무슨일 있으셨나요?",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100022375.1.18",
          "form": "아.. 몸이 안좋아서 주기적으로 입퇴원을 하고 있어요",
          "original_form": "아.. 몸이 안좋아서 주기적으로 입퇴원을 하고 있어요",
          "speaker_id": "1",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100022375.1.19",
          "form": "한달에 한번정도",
          "original_form": "한달에 한번정도",
          "speaker_id": "1",
          "time": "20210903 15:58"
        },
        {
          "id": "MDRW2100022375.1.20",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100022375.1.21",
          "form": "음 여름은 어떤점이 좋으신거에요?",
          "original_form": "음 여름은 어떤점이 좋으신거에요?",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100022375.1.22",
          "form": "옷을 가볍게 입을 수 있는 점이요",
          "original_form": "옷을 가볍게 입을 수 있는 점이요",
          "speaker_id": "1",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100022375.1.23",
          "form": "아 저는 가을겨울 더 좋아해요",
          "original_form": "아 저는 가을겨울 더 좋아해요",
          "speaker_id": "2",
          "time": "20210903 20:46"
        }
      ]
