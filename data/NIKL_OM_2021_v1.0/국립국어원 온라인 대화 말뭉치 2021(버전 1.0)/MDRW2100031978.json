      "utterance": [
        {
          "id": "MDRW2100031978.1.1",
          "form": "백신 예약하실 땐 여유있게 하신편이신가요??",
          "original_form": "백신 예약하실 땐 여유있게 하신편이신가요??",
          "speaker_id": "1",
          "time": "20210905 10:14"
        },
        {
          "id": "MDRW2100031978.1.2",
          "form": "저는 이미 맞았어요",
          "original_form": "저는 이미 맞았어요",
          "speaker_id": "2",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100031978.1.3",
          "form": "십부제로 예약했나요??",
          "original_form": "십부제로 예약했나요??",
          "speaker_id": "2",
          "time": "20210905 11:49"
        },
        {
          "id": "MDRW2100031978.1.4",
          "form": "네 10부제때 예약을 했어요!",
          "original_form": "네 10부제때 예약을 했어요!",
          "speaker_id": "1",
          "time": "20210905 11:57"
        },
        {
          "id": "MDRW2100031978.1.5",
          "form": "예약하실때 어렵지 않으셨나요??",
          "original_form": "예약하실때 어렵지 않으셨나요??",
          "speaker_id": "1",
          "time": "20210905 11:57"
        },
        {
          "id": "MDRW2100031978.1.6",
          "form": "서버가 엄청 터졌었잖아요",
          "original_form": "서버가 엄청 터졌었잖아요",
          "speaker_id": "1",
          "time": "20210905 11:58"
        },
        {
          "id": "MDRW2100031978.1.7",
          "form": "저는 10부제 전에 해당되는 나이라 맞았어요",
          "original_form": "저는 10부제 전에 해당되는 나이라 맞았어요",
          "speaker_id": "2",
          "time": "20210905 11:58"
        },
        {
          "id": "MDRW2100031978.1.8",
          "form": "10부제 전에도 신청할때 빡셌다고 들었어요",
          "original_form": "10부제 전에도 신청할때 빡셌다고 들었어요",
          "speaker_id": "1",
          "time": "20210905 11:59"
        },
        {
          "id": "MDRW2100031978.1.9",
          "form": "수월하게 신청해서 맞으셨나봐요 :)",
          "original_form": "수월하게 신청해서 맞으셨나봐요 :)",
          "speaker_id": "1",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100031978.1.10",
          "form": "그래서 전 아직 1차 까지 밖에 못 맞았는데",
          "original_form": "그래서 전 아직 1차 까지 밖에 못 맞았는데",
          "speaker_id": "1",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100031978.1.11",
          "form": "2차때 엄청 아프다고해서 초긴장중이에요 ㅠㅠ",
          "original_form": "2차때 엄청 아프다고해서 초긴장중이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 12:00"
        },
        {
          "id": "MDRW2100031978.1.12",
          "form": "십부제때 일찍 신청했나보네요",
          "original_form": "십부제때 일찍 신청했나보네요",
          "speaker_id": "2",
          "time": "20210905 12:01"
        },
        {
          "id": "MDRW2100031978.1.13",
          "form": "벌써 1차를 맞았으니",
          "original_form": "벌써 1차를 맞았으니",
          "speaker_id": "2",
          "time": "20210905 12:02"
        },
        {
          "id": "MDRW2100031978.1.14",
          "form": "화이자 맞은거죠?",
          "original_form": "화이자 맞은거죠?",
          "speaker_id": "2",
          "time": "20210905 12:02"
        },
        {
          "id": "MDRW2100031978.1.15",
          "form": "네 화이자 맞았어요 :)",
          "original_form": "네 화이자 맞았어요 :)",
          "speaker_id": "1",
          "time": "20210905 12:02"
        },
        {
          "id": "MDRW2100031978.1.16",
          "form": "공교롭게 생일 날짜별로 하는데",
          "original_form": "공교롭게 생일 날짜별로 하는데",
          "speaker_id": "1",
          "time": "20210905 12:02"
        },
        {
          "id": "MDRW2100031978.1.17",
          "form": "1부터라 아니라서 이번엔 빨리 맞았어요 XD",
          "original_form": "1부터라 아니라서 이번엔 빨리 맞았어요 XD",
          "speaker_id": "1",
          "time": "20210905 12:02"
        },
        {
          "id": "MDRW2100031978.1.18",
          "form": "오 맞아요 이번에 그랬죠? 9부터였을텐데",
          "original_form": "오 맞아요 이번에 그랬죠? 9부터였을텐데",
          "speaker_id": "2",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100031978.1.19",
          "form": "네 ㅎㅎ 1부터였으면 아마 꼴찌로 맞았을거에요..",
          "original_form": "네 ㅎㅎ 1부터였으면 아마 꼴찌로 맞았을거에요..",
          "speaker_id": "1",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100031978.1.20",
          "form": "하루라도 빨리 맞는게 좋으니 잘된거네요",
          "original_form": "하루라도 빨리 맞는게 좋으니 잘된거네요",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100031978.1.21",
          "form": "이번에 처음으로 날짜 특혜를 받았어요 ㅋㅋ",
          "original_form": "이번에 처음으로 날짜 특혜를 받았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100031978.1.22",
          "form": "1차때 많이 아팠어요?",
          "original_form": "1차때 많이 아팠어요?",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100031978.1.23",
          "form": "1차때 첫날은 많이 덥다고 느꼈고",
          "original_form": "1차때 첫날은 많이 덥다고 느꼈고",
          "speaker_id": "1",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100031978.1.24",
          "form": "둘째 셋째날엔 팔이 좀 무겁다 정도였는데",
          "original_form": "둘째 셋째날엔 팔이 좀 무겁다 정도였는데",
          "speaker_id": "1",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100031978.1.25",
          "form": "넷째날에.. 팔이 좀 아파서 살짝 고생했어요 ㅠㅠ",
          "original_form": "넷째날에.. 팔이 좀 아파서 살짝 고생했어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 12:06"
        },
        {
          "id": "MDRW2100031978.1.26",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100031978.1.27",
          "form": "저는 아제 맞았는데 다행이 1차 2차 다 별 문제 없었어요",
          "original_form": "저는 아제 맞았는데 다행이 1차 2차 다 별 문제 없었어요",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100031978.1.28",
          "form": "부작용이 없으셨다고 하니 다행이에요 ㅎㅎ",
          "original_form": "부작용이 없으셨다고 하니 다행이에요 ㅎㅎ",
