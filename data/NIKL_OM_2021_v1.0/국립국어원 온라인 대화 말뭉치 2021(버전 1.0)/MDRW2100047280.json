      "utterance": [
        {
          "id": "MDRW2100047280.1.1",
          "form": "name2님 즐겨보는 tv프로 있으세요?",
          "original_form": "&name2&님 즐겨보는 tv프로 있으세요?",
          "speaker_id": "1",
