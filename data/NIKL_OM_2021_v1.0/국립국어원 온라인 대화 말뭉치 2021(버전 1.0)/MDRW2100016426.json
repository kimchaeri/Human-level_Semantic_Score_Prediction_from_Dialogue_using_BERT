      "utterance": [
        {
          "id": "MDRW2100016426.1.1",
          "form": "ㅇㅇㅇ 지금 새로운 시즌 나왔을걸",
          "original_form": "ㅇㅇㅇ 지금 새로운 시즌 나왔을걸",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016426.1.2",
          "form": "시즌몇까지있는데",
          "original_form": "시즌몇까지있는데",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016426.1.3",
          "form": "그앤프는 7까지",
          "original_form": "그앤프는 7까지",
          "speaker_id": "2",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016426.1.4",
          "form": "엄청많네",
          "original_form": "엄청많네",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016426.1.5",
          "form": "시즌하나에",
          "original_form": "시즌하나에",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016426.1.6",
          "form": "먗화까지있음?",
          "original_form": "먗화까지있음?",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016426.1.7",
          "form": "13화 정도",
          "original_form": "13화 정도",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016426.1.8",
          "form": "근데 나도 아직 시즌 1밖에 못봄",
          "original_form": "근데 나도 아직 시즌 1밖에 못봄",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016426.1.9",
          "form": "ㅋㅋㅋㅋ시즌7까지나와씅면",
          "original_form": "ㅋㅋㅋㅋ시즌7까지나와씅면",
          "speaker_id": "1",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016426.1.10",
          "form": "예전드라마아님?",
          "original_form": "예전드라마아님?",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100016426.1.11",
          "form": "15년도 부터 나왔음",
          "original_form": "15년도 부터 나왔음",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016426.1.12",
          "form": "근데 나름 괜찮았음",
          "original_form": "근데 나름 괜찮았음",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016426.1.13",
          "form": "거의6년됐네",
          "original_form": "거의6년됐네",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016426.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016426.1.15",
          "form": "그거말고넷플볼만한거있음?",
          "original_form": "그거말고넷플볼만한거있음?",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016426.1.16",
          "form": "남편들 부분 넘기며 봐서인지 몰라도",
          "original_form": "남편들 부분 넘기며 봐서인지 몰라도",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016426.1.17",
          "form": "이번에",
          "original_form": "이번에",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016426.1.18",
          "form": "더 체어 나옴",
          "original_form": "더 체어 나옴",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016426.1.19",
          "form": "더체어는뭐임?",
          "original_form": "더체어는뭐임?",
          "speaker_id": "1",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016426.1.20",
          "form": "영화?",
          "original_form": "영화?",
          "speaker_id": "1",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016426.1.21",
          "form": "시리즈인데",
          "original_form": "시리즈인데",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016426.1.22",
          "form": "기울어져가는 영문학과에 유색인종 여성이 학과장이 임명되면서 벌어지는 이야기래",
          "original_form": "기울어져가는 영문학과에 유색인종 여성이 학과장이 임명되면서 벌어지는 이야기래",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016426.1.23",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016426.1.24",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016426.1.25",
          "form": "전시리즈봉적있암",
          "original_form": "전시리즈봉적있암",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016426.1.26",
          "form": "본적있음?",
          "original_form": "본적있음?",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016426.1.27",
          "form": "아니 나도 봐야지 하고 담아놓기만 함",
          "original_form": "아니 나도 봐야지 하고 담아놓기만 함",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016426.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016426.1.29",
          "form": "그럴수있지",
          "original_form": "그럴수있지",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016426.1.30",
          "form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅌ",
          "original_form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016426.1.31",
          "form": "확실히넷플릭스말고는",
          "original_form": "확실히넷플릭스말고는",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016426.1.32",
          "form": "공중파드라마중에서는",
          "original_form": "공중파드라마중에서는",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016426.1.33",
          "form": "볼만한게없긴한듯",
          "original_form": "볼만한게없긴한듯",
          "speaker_id": "1",
          "time": "20210826 12:50"
        }
      ]
