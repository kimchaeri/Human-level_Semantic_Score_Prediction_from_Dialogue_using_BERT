      "utterance": [
        {
          "id": "MDRW2100045687.1.1",
          "form": "아기 성별 물어봐도 될까요?",
          "original_form": "아기 성별 물어봐도 될까요?",
          "speaker_id": "1",
          "time": "20210908 05:25"
        },
        {
          "id": "MDRW2100045687.1.2",
          "form": "네네 아기는 여아에요!",
          "original_form": "네네 아기는 여아에요!",
          "speaker_id": "2",
          "time": "20210908 05:26"
        },
        {
          "id": "MDRW2100045687.1.3",
          "form": "어머어머",
          "original_form": "어머어머",
          "speaker_id": "1",
          "time": "20210908 05:26"
        },
        {
          "id": "MDRW2100045687.1.4",
          "form": "너무 예쁘겟다",
          "original_form": "너무 예쁘겟다",
          "speaker_id": "1",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045687.1.5",
          "form": "새벽에 주책맞아지고잇네여 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "새벽에 주책맞아지고잇네여 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045687.1.6",
          "form": "아니에욬ㅋㅋㅋㅋ 애기는 다 이쁘잖아요 ㅎㅎ",
          "original_form": "아니에욬ㅋㅋㅋㅋ 애기는 다 이쁘잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 05:28"
        },
        {
          "id": "MDRW2100045687.1.7",
          "form": "임신부시면 더 조심스러웟겟어요 그동안 ㅠㅠ",
          "original_form": "임신부시면 더 조심스러웟겟어요 그동안 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:29"
        },
        {
          "id": "MDRW2100045687.1.8",
          "form": "코로나때문에ㅠㅠ",
          "original_form": "코로나때문에ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:29"
        },
        {
          "id": "MDRW2100045687.1.9",
          "form": "네ㅠㅠ 아무래도 어디 가기가 꺼려지더라구요ㅠㅠ 그래서 외식도 많이 줄이고 집에서 많이 해먹었어요",
          "original_form": "네ㅠㅠ 아무래도 어디 가기가 꺼려지더라구요ㅠㅠ 그래서 외식도 많이 줄이고 집에서 많이 해먹었어요",
          "speaker_id": "2",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045687.1.10",
          "form": "고생많으셧겟어요 ㅠㅠ",
          "original_form": "고생많으셧겟어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:31"
        },
        {
          "id": "MDRW2100045687.1.11",
          "form": "아니에요ㅠㅠ 그냥 쪼끔 답답하긴 했지만 이렇게 쉬어보건도 처음이라 좋았어요",
          "original_form": "아니에요ㅠㅠ 그냥 쪼끔 답답하긴 했지만 이렇게 쉬어보건도 처음이라 좋았어요",
          "speaker_id": "2",
          "time": "20210908 05:32"
        },
        {
          "id": "MDRW2100045687.1.12",
          "form": "오오 다행이에요",
          "original_form": "오오 다행이에요",
          "speaker_id": "1",
          "time": "20210908 05:32"
        },
        {
          "id": "MDRW2100045687.1.13",
          "form": "이번기회로 천상 집순인가보다 하고 있어요ㅎㅎ",
          "original_form": "이번기회로 천상 집순인가보다 하고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045687.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도 집순이라서 코로나 시국에 그나마 버틸만한것같아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도 집순이라서 코로나 시국에 그나마 버틸만한것같아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045687.1.15",
          "form": "집순이가 아니었다면 사람이 엄청 초췌해졌울 것 같아욬ㅋㅋㅋ",
          "original_form": "집순이가 아니었다면 사람이 엄청 초췌해졌울 것 같아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100045687.1.16",
          "form": "맞아요 집순이인데도 버티기 쉽지 않은데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 집순이인데도 버티기 쉽지 않은데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:41"
        },
        {
          "id": "MDRW2100045687.1.17",
          "form": "그니까욬ㅋㅋㅋ 처음엔 금방 지나서 본가 시댁도 자주가고 그럴 줄 알았는데 전혀 그러지 못하네요ㅠㅠ",
          "original_form": "그니까욬ㅋㅋㅋ 처음엔 금방 지나서 본가 시댁도 자주가고 그럴 줄 알았는데 전혀 그러지 못하네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:42"
        },
        {
          "id": "MDRW2100045687.1.18",
          "form": "ㅠㅠ저도 진짜 이렇게 길어질줄 몰랏어요 코로나가",
          "original_form": "ㅠㅠ저도 진짜 이렇게 길어질줄 몰랏어요 코로나가",
          "speaker_id": "1",
          "time": "20210908 05:43"
        },
        {
          "id": "MDRW2100045687.1.19",
          "form": "가족여행도 가고 싶고 멀리 계시는 할머니댁도 가고 싶은데",
          "original_form": "가족여행도 가고 싶고 멀리 계시는 할머니댁도 가고 싶은데",
          "speaker_id": "2",
          "time": "20210908 05:44"
        },
        {
          "id": "MDRW2100045687.1.20",
          "form": "본가는 그렇다쳐도 할머니댁은 진짜 못가겠어여ㅠㅠ",
          "original_form": "본가는 그렇다쳐도 할머니댁은 진짜 못가겠어여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:44"
        },
        {
          "id": "MDRW2100045687.1.21",
          "form": "ㅠㅠ 맞아요 조심스럽죠",
          "original_form": "ㅠㅠ 맞아요 조심스럽죠",
          "speaker_id": "1",
          "time": "20210908 05:45"
        },
        {
          "id": "MDRW2100045687.1.22",
          "form": "아무래도 연세가 있으시다보니까 더 그런거 같아요ㅠㅠ",
          "original_form": "아무래도 연세가 있으시다보니까 더 그런거 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:46"
        }
      ]
    }
