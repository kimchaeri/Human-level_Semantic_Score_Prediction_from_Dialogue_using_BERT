      "utterance": [
        {
          "id": "MDRW2100040579.1.1",
          "form": "네네 피부과 다녀요",
          "original_form": "네네 피부과 다녀요",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040579.1.2",
          "form": "평소엔 팩 해요",
          "original_form": "평소엔 팩 해요",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040579.1.3",
          "form": "피부과 가면 뭐 해야하나요? 전 가본적이 없는데 가고 싶어요",
          "original_form": "피부과 가면 뭐 해야하나요? 전 가본적이 없는데 가고 싶어요",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040579.1.4",
          "form": "저는 우선 기미잡티 제거하는데",
          "original_form": "저는 우선 기미잡티 제거하는데",
          "speaker_id": "2",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100040579.1.5",
          "form": "써마지 좋다하더라고요",
          "original_form": "써마지 좋다하더라고요",
          "speaker_id": "2",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100040579.1.6",
          "form": "명칭이 써마지예요? 가서 써마지 하고 싶어요 하면 되나요?",
          "original_form": "명칭이 써마지예요? 가서 써마지 하고 싶어요 하면 되나요?",
          "speaker_id": "1",
          "time": "20210905 11:13"
        },
        {
          "id": "MDRW2100040579.1.7",
          "form": "네네 그렇더라고요",
          "original_form": "네네 그렇더라고요",
          "speaker_id": "2",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100040579.1.8",
          "form": "아기 써마지가 좋대요 ㅋㅋ",
          "original_form": "아기 써마지가 좋대요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100040579.1.9",
          "form": "저도 들은거",
          "original_form": "저도 들은거",
          "speaker_id": "2",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100040579.1.10",
          "form": "저는잡티제거랑 아쿠아필만 해봤는데",
          "original_form": "저는잡티제거랑 아쿠아필만 해봤는데",
          "speaker_id": "2",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100040579.1.11",
          "form": "아쿠아필은 트러블만 더나고ㅠ",
          "original_form": "아쿠아필은 트러블만 더나고ㅠ",
          "speaker_id": "2",
          "time": "20210905 11:41"
        },
        {
          "id": "MDRW2100040579.1.12",
          "form": "아쿠아필은 물로 하는건가요??",
          "original_form": "아쿠아필은 물로 하는건가요??",
          "speaker_id": "1",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100040579.1.13",
          "form": "네네 치치칙 하는 물로 날같은 계 진동 이용해서 필링 하능거에요",
          "original_form": "네네 치치칙 하는 물로 날같은 계 진동 이용해서 필링 하능거에요",
          "speaker_id": "2",
          "time": "20210905 11:58"
        },
        {
          "id": "MDRW2100040579.1.14",
          "form": "좋을것 같은데 이건 별로인거봐요 ㅡ.ㅡ",
          "original_form": "좋을것 같은데 이건 별로인거봐요 ㅡ.ㅡ",
          "speaker_id": "1",
          "time": "20210905 13:08"
        },
        {
          "id": "MDRW2100040579.1.15",
          "form": "효과는 다 다릉데 전 트러블 났어요ㅠ",
          "original_form": "효과는 다 다릉데 전 트러블 났어요ㅠ",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040579.1.16",
          "form": "써마지는 뭐로 어떻게 하는걸까요?",
          "original_form": "써마지는 뭐로 어떻게 하는걸까요?",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100040579.1.17",
          "form": "저도 안해봐서 몰라요ㅠㅠ 추천을 받아서 저도 가서 말해보려고요 ㅋㅋ",
          "original_form": "저도 안해봐서 몰라요ㅠㅠ 추천을 받아서 저도 가서 말해보려고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100040579.1.18",
          "form": "가면 레이저 한다는데 뭐가 뭔지.모르겠더라고요 ㅠ",
          "original_form": "가면 레이저 한다는데 뭐가 뭔지.모르겠더라고요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100040579.1.19",
          "form": "구쵸ㅠㅠ 레이저를 정해서 가야 덜 바가지 써요ㅠㅠ",
          "original_form": "구쵸ㅠㅠ 레이저를 정해서 가야 덜 바가지 써요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100040579.1.20",
          "form": "레이저는 뭘 한다고 해야하나요?",
          "original_form": "레이저는 뭘 한다고 해야하나요?",
          "speaker_id": "1",
          "time": "20210905 15:03"
        },
        {
          "id": "MDRW2100040579.1.21",
          "form": "피부과 가서 레이저 뭐 해주세요~ 라고 해야할까요?? 그냥 피부가 좋아졌으면 하는 막연란 바람만 있어서..",
