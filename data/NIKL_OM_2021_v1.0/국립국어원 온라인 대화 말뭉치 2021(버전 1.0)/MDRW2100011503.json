      "utterance": [
        {
          "id": "MDRW2100011503.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100011503.1.2",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100011503.1.3",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100011503.1.4",
          "form": "음 요샌 온라인 쇼핑요",
          "original_form": "음 요샌 온라인 쇼핑요",
          "speaker_id": "2",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100011503.1.5",
          "form": "쇼핑을 좋아 하는거 같아요",
          "original_form": "쇼핑을 좋아 하는거 같아요",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011503.1.6",
          "form": "꼭 한번씩 셔핑몰 둘러봐요 ㅋㅋ",
          "original_form": "꼭 한번씩 셔핑몰 둘러봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011503.1.7",
          "form": "최근에 어떤거 구매하셨나요ㅎㅎ",
          "original_form": "최근에 어떤거 구매하셨나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011503.1.8",
          "form": "가방하고 바지요",
          "original_form": "가방하고 바지요",
          "speaker_id": "2",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011503.1.9",
          "form": "아들이 맘에 안들어 할까 걱정했는데",
          "original_form": "아들이 맘에 안들어 할까 걱정했는데",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100011503.1.10",
          "form": "음 좀 괜찮은데 이러네요 ㅡㅡ",
          "original_form": "음 좀 괜찮은데 이러네요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100011503.1.11",
          "form": "아들분 귀엽네요ㅎㅎ 저는 신발샀어요",
          "original_form": "아들분 귀엽네요ㅎㅎ 저는 신발샀어요",
          "speaker_id": "1",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100011503.1.12",
          "form": "오 저도 ㅋㅋ오늘 신발와요 ㅋㅋ",
          "original_form": "오 저도 ㅋㅋ오늘 신발와요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100011503.1.13",
          "form": "어떤거 사셨어요?",
          "original_form": "어떤거 사셨어요?",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100011503.1.14",
          "form": "좋아하는 색 있으세요?",
          "original_form": "좋아하는 색 있으세요?",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011503.1.15",
          "form": "저는 회색신발 좋아해요ㅎㅎ",
          "original_form": "저는 회색신발 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011503.1.16",
          "form": "오 저도 회색 검정요",
          "original_form": "오 저도 회색 검정요",
          "speaker_id": "2",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011503.1.17",
          "form": "무난하죠 코디하기에도ㅎㅎ",
          "original_form": "무난하죠 코디하기에도ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100011503.1.18",
          "form": "그렇죠 어디에도 어울리고",
          "original_form": "그렇죠 어디에도 어울리고",
          "speaker_id": "2",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100011503.1.19",
          "form": "흰색은 부담되고",
          "original_form": "흰색은 부담되고",
          "speaker_id": "2",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100011503.1.20",
          "form": "오맞아요 때도 잘타고",
          "original_form": "오맞아요 때도 잘타고",
          "speaker_id": "1",
          "time": "20210824 13:32"
        },
        {
          "id": "MDRW2100011503.1.21",
          "form": "빨면 운동화 망가지고 ㅋㅋ",
          "original_form": "빨면 운동화 망가지고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100011503.1.22",
          "form": "흰색 비추요 ㅋㅋ",
          "original_form": "흰색 비추요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:33"
        },
        {
          "id": "MDRW2100011503.1.23",
          "form": "마자요ㅎㅎㅎ",
          "original_form": "마자요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:34"
        }
      ]
