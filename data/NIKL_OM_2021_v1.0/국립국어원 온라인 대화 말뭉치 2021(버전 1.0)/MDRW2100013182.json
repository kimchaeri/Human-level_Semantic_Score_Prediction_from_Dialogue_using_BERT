      "utterance": [
        {
          "id": "MDRW2100013182.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100013182.1.2",
          "form": "그쪽에도 비가 오나요?",
          "original_form": "그쪽에도 비가 오나요?",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100013182.1.3",
          "form": "비가 오다가다하네요",
          "original_form": "비가 오다가다하네요",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013182.1.4",
          "form": "name1 지역은 어떤가요",
          "original_form": "&name1& 지역은 어떤가요",
          "speaker_id": "2",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013182.1.5",
          "form": "저희도 오다말다 하는데 이제는 안오네요!",
          "original_form": "저희도 오다말다 하는데 이제는 안오네요!",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100013182.1.6",
          "form": "비가 오니까 확실히 선선해서 좋지않나요?",
          "original_form": "비가 오니까 확실히 선선해서 좋지않나요?",
          "speaker_id": "1",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100013182.1.7",
          "form": "비만 안오면 최고의 날씨죠ㅎ",
          "original_form": "비만 안오면 최고의 날씨죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100013182.1.8",
          "form": "여름 간 게 확 느껴져요",
          "original_form": "여름 간 게 확 느껴져요",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100013182.1.9",
          "form": "어느 계절 좋아하세요",
          "original_form": "어느 계절 좋아하세요",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100013182.1.10",
          "form": "저는 여름~가을 좋아해요ㅎㅎ",
          "original_form": "저는 여름~가을 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100013182.1.11",
          "form": "어느 계절 좋아하시나요?",
          "original_form": "어느 계절 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100013182.1.12",
          "form": "여름이요",
          "original_form": "여름이요",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100013182.1.13",
          "form": "그래서 선선한 날씨가 좋으면서도 조금 아쉬워요",
          "original_form": "그래서 선선한 날씨가 좋으면서도 조금 아쉬워요",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100013182.1.14",
          "form": "오 여름 좋아하시는구나!",
          "original_form": "오 여름 좋아하시는구나!",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100013182.1.15",
          "form": "이번 여름에 덥긴 했는데 미세먼지 없고 화창해서 너무 좋더라구요!",
          "original_form": "이번 여름에 덥긴 했는데 미세먼지 없고 화창해서 너무 좋더라구요!",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100013182.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100013182.1.17",
          "form": "그리고 3년 전 정도로 열대야가 심하지 않아서 살만했어요",
          "original_form": "그리고 3년 전 정도로 열대야가 심하지 않아서 살만했어요",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100013182.1.18",
          "form": "더위와 추위 중 뭐에 강하신가요",
          "original_form": "더위와 추위 중 뭐에 강하신가요",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100013182.1.19",
          "form": "저는 더위요!!",
          "original_form": "저는 더위요!!",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100013182.1.20",
          "form": "추운거 싫어해서ㅠ퓨",
          "original_form": "추운거 싫어해서ㅠ퓨",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100013182.1.21",
          "form": "더운거vs추운거 뭐가 나으신가요?",
          "original_form": "더운거vs추운거 뭐가 나으신가요?",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100013182.1.22",
          "form": "더위요!",
          "original_form": "더위요!",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100013182.1.23",
          "form": "추우면 껴입어도 춥잖아요ㅜ",
          "original_form": "추우면 껴입어도 춥잖아요ㅜ",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100013182.1.24",
          "form": "더우면 얇게 입으면 되는데",
          "original_form": "더우면 얇게 입으면 되는데",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100013182.1.25",
          "form": "그쵸ㅠㅠㅠㅠㅠ",
          "original_form": "그쵸ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100013182.1.26",
          "form": "저도 더운게 나아요...ㅎㅎ",
          "original_form": "저도 더운게 나아요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100013182.1.27",
          "form": "요즘 미세먼지도 많이 없어서 너무 좋지않나요?",
          "original_form": "요즘 미세먼지도 많이 없어서 너무 좋지않나요?",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100013182.1.28",
          "form": "그러게요. 좀 살 것 같아요",
          "original_form": "그러게요. 좀 살 것 같아요",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100013182.1.29",
          "form": "마스크 써서 공기가 나아보이나 했는데",
          "original_form": "마스크 써서 공기가 나아보이나 했는데",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100013182.1.30",
          "form": "실제 공기질이 좋더라고요 요근래ㅎ",
          "original_form": "실제 공기질이 좋더라고요 요근래ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100013182.1.31",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
