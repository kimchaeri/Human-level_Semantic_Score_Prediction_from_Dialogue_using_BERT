      "utterance": [
        {
          "id": "MDRW2100029753.1.1",
          "form": "최근에 영화보셨나요?",
          "original_form": "최근에 영화보셨나요?",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100029753.1.2",
          "form": "영화관은 안갔구",
          "original_form": "영화관은 안갔구",
          "speaker_id": "2",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100029753.1.3",
          "form": "최근에 해리포터 재탕했어요ㅋㅋ",
          "original_form": "최근에 해리포터 재탕했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100029753.1.4",
          "form": "오 저도 해리포터 봐야지 하고는있는데 ㅠㅠ",
          "original_form": "오 저도 해리포터 봐야지 하고는있는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 13:29"
        },
        {
          "id": "MDRW2100029753.1.5",
          "form": "이번 주말에 도전해봐야겟네요",
          "original_form": "이번 주말에 도전해봐야겟네요",
          "speaker_id": "1",
          "time": "20210903 13:29"
        },
        {
          "id": "MDRW2100029753.1.6",
          "form": "처음엔 이게뭐야했는데",
          "original_form": "처음엔 이게뭐야했는데",
          "speaker_id": "2",
          "time": "20210903 13:51"
        },
        {
          "id": "MDRW2100029753.1.7",
          "form": "다보니까 재밌더라구요ㅋㅋ",
          "original_form": "다보니까 재밌더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:52"
        },
        {
          "id": "MDRW2100029753.1.8",
          "form": "ㅋㅋㅋ처음부터 재밌던데!! 말하다보니 보고싶네영",
          "original_form": "ㅋㅋㅋ처음부터 재밌던데!! 말하다보니 보고싶네영",
          "speaker_id": "1",
          "time": "20210903 13:53"
        },
        {
          "id": "MDRW2100029753.1.9",
          "form": "ㅋㅋㅋ저는 원작을 안봐서 그랬나봐영",
          "original_form": "ㅋㅋㅋ저는 원작을 안봐서 그랬나봐영",
          "speaker_id": "2",
          "time": "20210903 13:54"
        },
        {
          "id": "MDRW2100029753.1.10",
          "form": "영화 뭐 재밌게보신거있으세요?",
          "original_form": "영화 뭐 재밌게보신거있으세요?",
          "speaker_id": "2",
          "time": "20210903 13:54"
        },
        {
          "id": "MDRW2100029753.1.11",
          "form": "저는 로맨스 좋아해서",
          "original_form": "저는 로맨스 좋아해서",
          "speaker_id": "1",
          "time": "20210903 13:55"
        },
        {
          "id": "MDRW2100029753.1.12",
          "form": "웬만한 로맨스는 거의다본듯용",
          "original_form": "웬만한 로맨스는 거의다본듯용",
          "speaker_id": "1",
          "time": "20210903 13:55"
        },
        {
          "id": "MDRW2100029753.1.13",
          "form": "아 그 범죄도시 보셨어요?",
          "original_form": "아 그 범죄도시 보셨어요?",
          "speaker_id": "1",
          "time": "20210903 13:55"
        },
        {
          "id": "MDRW2100029753.1.14",
          "form": "2 제작중이래요",
          "original_form": "2 제작중이래요",
          "speaker_id": "1",
          "time": "20210903 13:55"
        },
        {
          "id": "MDRW2100029753.1.15",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210903 14:03"
        },
        {
          "id": "MDRW2100029753.1.16",
          "form": "아 그 유명한 장첸이ㅋㅋㅋ",
          "original_form": "아 그 유명한 장첸이ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:03"
        },
        {
          "id": "MDRW2100029753.1.17",
          "form": "범죄도시 엄청 재밌게봣거든요 ㅋㅋ",
          "original_form": "범죄도시 엄청 재밌게봣거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100029753.1.18",
          "form": "기대중",
          "original_form": "기대중",
          "speaker_id": "1",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100029753.1.19",
          "form": "많이 잔인하거나 그런가요?",
          "original_form": "많이 잔인하거나 그런가요?",
          "speaker_id": "2",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029753.1.20",
          "form": "좀 잔인한내용있는데 액션 좋더라구용 ㅎㅎ",
          "original_form": "좀 잔인한내용있는데 액션 좋더라구용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029753.1.21",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029753.1.22",
          "form": "봐봐야겠어요",
          "original_form": "봐봐야겠어요",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029753.1.23",
          "form": "내일 모가디슈 보러가려구용!",
          "original_form": "내일 모가디슈 보러가려구용!",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029753.1.24",
          "form": "아 그것도 보고싶은데!",
          "original_form": "아 그것도 보고싶은데!",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029753.1.25",
          "form": "만식배우님 좋아요ㅋㅋ",
