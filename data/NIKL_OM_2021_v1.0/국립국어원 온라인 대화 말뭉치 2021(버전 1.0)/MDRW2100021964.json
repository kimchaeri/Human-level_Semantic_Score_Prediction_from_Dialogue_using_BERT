      "utterance": [
        {
          "id": "MDRW2100021964.1.1",
          "form": "ㅋㅋㅋ 건강하려면... \r\n역시 탄수화물엔 탄수화물이져",
          "original_form": "ㅋㅋㅋ 건강하려면... \r\n역시 탄수화물엔 탄수화물이져",
          "speaker_id": "1",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021964.1.2",
          "form": "ㅋㅋㅋㅋ진짜 건강생각하면 아무것도 못먹어요",
          "original_form": "ㅋㅋㅋㅋ진짜 건강생각하면 아무것도 못먹어요",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021964.1.3",
          "form": "뭐 영양제 같은거 챙겨드시는거 있나요?",
          "original_form": "뭐 영양제 같은거 챙겨드시는거 있나요?",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021964.1.4",
          "form": "저는 비타민만 챙겨먹어요",
          "original_form": "저는 비타민만 챙겨먹어요",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021964.1.5",
          "form": "드시는거 있으세요?",
          "original_form": "드시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021964.1.6",
          "form": "종합비타민이여? 저는 그냥 골고루 잘 먹어서 딱히 안챙겨넉어여",
          "original_form": "종합비타민이여? 저는 그냥 골고루 잘 먹어서 딱히 안챙겨넉어여",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021964.1.7",
          "form": "종합비타민 직구해서 쟁여둔거 있어서 먹고있어요",
          "original_form": "종합비타민 직구해서 쟁여둔거 있어서 먹고있어요",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021964.1.8",
          "form": "근데 귀찮아서 생각날때만 먹고...ㅋㅋ",
          "original_form": "근데 귀찮아서 생각날때만 먹고...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021964.1.9",
          "form": "먹음 효과있나요?",
          "original_form": "먹음 효과있나요?",
          "speaker_id": "1",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021964.1.10",
          "form": "음 효과 크게는 모르겠어요",
          "original_form": "음 효과 크게는 모르겠어요",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021964.1.11",
          "form": "그냥 양심상 먹는..?",
          "original_form": "그냥 양심상 먹는..?",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021964.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021964.1.13",
          "form": "그 플라시보? ㅋㅋㅋ",
          "original_form": "그 플라시보? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021964.1.14",
          "form": "설마 밀가루덩어리 팔겠어요? 몸에 좋은거니까 팔겠죠.. ㅋㅋ",
          "original_form": "설마 밀가루덩어리 팔겠어요? 몸에 좋은거니까 팔겠죠.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021964.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ그렇겠죠...?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그렇겠죠...?",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100021964.1.16",
          "form": "꾸준히 챙겨 먹어야죠 머...",
          "original_form": "꾸준히 챙겨 먹어야죠 머...",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100021964.1.17",
          "form": "혹시 영양제 잘 아시나여?",
          "original_form": "혹시 영양제 잘 아시나여?",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100021964.1.18",
          "form": "잘은 몰라용 그냥 추천받는거 사요!!",
          "original_form": "잘은 몰라용 그냥 추천받는거 사요!!",
          "speaker_id": "2",
          "time": "20210830 08:42"
        },
        {
          "id": "MDRW2100021964.1.19",
          "form": "비타민은 어디꺼 드세요?",
          "original_form": "비타민은 어디꺼 드세요?",
          "speaker_id": "1",
