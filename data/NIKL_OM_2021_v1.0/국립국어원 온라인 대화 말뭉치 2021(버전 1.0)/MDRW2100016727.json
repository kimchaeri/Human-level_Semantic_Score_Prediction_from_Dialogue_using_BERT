      "utterance": [
        {
          "id": "MDRW2100016727.1.1",
          "form": "최근에 머리 한적 있어?",
          "original_form": "최근에 머리 한적 있어?",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016727.1.2",
          "form": "아니 나 1월에 커트하구서 줄곧 기르기만 했어ㅎ",
          "original_form": "아니 나 1월에 커트하구서 줄곧 기르기만 했어ㅎ",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016727.1.3",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016727.1.4",
          "form": "오 완전 생머리~",
          "original_form": "오 완전 생머리~",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016727.1.5",
          "form": "나..나 결혼식 가기전에 머리한번 했다 ㅋㅋ",
          "original_form": "나..나 결혼식 가기전에 머리한번 했다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016727.1.6",
          "form": "지금 c컬",
          "original_form": "지금 c컬",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016727.1.7",
          "form": "생머리보단 더벅머리지..?",
          "original_form": "생머리보단 더벅머리지..?",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016727.1.8",
          "form": "오 길이는 어떻게 돼??",
          "original_form": "오 길이는 어떻게 돼??",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016727.1.9",
          "form": "길이 좀 짧게했더니 아직도 어깨 안닿아 ㅋㅋㅋㅋ",
          "original_form": "길이 좀 짧게했더니 아직도 어깨 안닿아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016727.1.10",
          "form": "그전에 숏컷이었어서 ㅋㅋㅋ 많이 잛았오",
          "original_form": "그전에 숏컷이었어서 ㅋㅋㅋ 많이 잛았오",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016727.1.11",
          "form": "어깨 안닿는 c컬 여름에 하면 완전 가1볍고 시원해보여서 좋은거 같아~~",
          "original_form": "어깨 안닿는 c컬 여름에 하면 완전 가1볍고 시원해보여서 좋은거 같아~~",
          "speaker_id": "1",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100016727.1.12",
          "form": "당분간은 그러면 머리 기를 예정이야?",
          "original_form": "당분간은 그러면 머리 기를 예정이야?",
          "speaker_id": "1",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100016727.1.13",
          "form": "맞아 머리 한번 자르면 못기르겠더라",
          "original_form": "맞아 머리 한번 자르면 못기르겠더라",
          "speaker_id": "2",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100016727.1.14",
          "form": "가볍고 너무 편해",
          "original_form": "가볍고 너무 편해",
          "speaker_id": "2",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100016727.1.15",
          "form": "음 응우선 이대로 두고 또 길면 자를 것 같아 ㅋㅋ 넌?",
          "original_form": "음 응우선 이대로 두고 또 길면 자를 것 같아 ㅋㅋ 넌?",
          "speaker_id": "2",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100016727.1.16",
          "form": "머리 어디까지와?",
          "original_form": "머리 어디까지와?",
          "speaker_id": "2",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100016727.1.17",
          "form": "나는 쇄골보다 쫌더 아래 정도??",
          "original_form": "나는 쇄골보다 쫌더 아래 정도??",
          "speaker_id": "1",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100016727.1.18",
          "form": "무겁고 답답하긴 한데 여름이 다 지나서",
          "original_form": "무겁고 답답하긴 한데 여름이 다 지나서",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016727.1.19",
          "form": "1월에 어디까지 잘랐었는데?",
          "original_form": "1월에 어디까지 잘랐었는데?",
          "speaker_id": "2",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016727.1.20",
          "form": "이제와 자르자니 아까워서 길러보려고ㅎ",
          "original_form": "이제와 자르자니 아까워서 길러보려고ㅎ",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016727.1.21",
          "form": "1월에 어깨 안닿게?? 스무살 이후로 제일 짧았던거같아",
          "original_form": "1월에 어깨 안닿게?? 스무살 이후로 제일 짧았던거같아",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016727.1.22",
          "form": "오 글쿠만",
          "original_form": "오 글쿠만",
          "speaker_id": "2",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016727.1.23",
          "form": "너 단발 한적 없나??",
          "original_form": "너 단발 한적 없나??",
          "speaker_id": "2",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100016727.1.24",
          "form": "ㅋㅋㅋㅋ 오래길렀음 아깝긴하지",
          "original_form": "ㅋㅋㅋㅋ 오래길렀음 아깝긴하지",
          "speaker_id": "2",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016727.1.25",
          "form": "단발이라고 할 정도로의 길이는 없었던 거 같아ㅋㅋㅋ",
          "original_form": "단발이라고 할 정도로의 길이는 없었던 거 같아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016727.1.26",
          "form": "오래 길러서 아깝다기 보단..",
          "original_form": "오래 길러서 아깝다기 보단..",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016727.1.27",
          "form": "한창 더운 시절을 버텼는데 이제와서..?",
          "original_form": "한창 더운 시절을 버텼는데 이제와서..?",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016727.1.28",
          "form": "라는 기분이야",
          "original_form": "라는 기분이야",
          "speaker_id": "1",
          "time": "20210826 15:06"
        }
      ]
    }
