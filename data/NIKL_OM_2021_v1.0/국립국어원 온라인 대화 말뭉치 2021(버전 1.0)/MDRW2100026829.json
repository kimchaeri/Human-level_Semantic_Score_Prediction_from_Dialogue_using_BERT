      "utterance": [
        {
          "id": "MDRW2100026829.1.1",
          "form": "최근 산거있으세요?",
          "original_form": "최근 산거있으세요?",
          "speaker_id": "1",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026829.1.2",
          "form": "네 1일 1쇼핑합니다",
          "original_form": "네 1일 1쇼핑합니다",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026829.1.3",
          "form": "어제는 닭발을 샀습니다",
          "original_form": "어제는 닭발을 샀습니다",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026829.1.4",
          "form": "뭐사셨나요",
          "original_form": "뭐사셨나요",
          "speaker_id": "1",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026829.1.5",
          "form": "맛있었겠군요",
          "original_form": "맛있었겠군요",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026829.1.6",
          "form": "오늘은 웍 샀습니다",
          "original_form": "오늘은 웍 샀습니다",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026829.1.7",
          "form": "전 가방을 삿어요",
          "original_form": "전 가방을 삿어요",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026829.1.8",
          "form": "전문용어로 무슨팬이신디",
          "original_form": "전문용어로 무슨팬이신디",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026829.1.9",
          "form": "줄석이벤트로 할인해서 싸게샀어요",
          "original_form": "줄석이벤트로 할인해서 싸게샀어요",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026829.1.10",
          "form": "어디서 사셨지요?",
          "original_form": "어디서 사셨지요?",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026829.1.11",
          "form": "아 그 이벤트 좋군요",
          "original_form": "아 그 이벤트 좋군요",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026829.1.12",
          "form": "현대몰에서요",
          "original_form": "현대몰에서요",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026829.1.13",
          "form": "한번씩해요 싸게사서 좋아요",
          "original_form": "한번씩해요 싸게사서 좋아요",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026829.1.14",
          "form": "저도 가방인줄 알았는데 이민가방이 왔습니다",
          "original_form": "저도 가방인줄 알았는데 이민가방이 왔습니다",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026829.1.15",
          "form": "구매 실패",
          "original_form": "구매 실패",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026829.1.16",
          "form": "아그렇군요 실물을 보고 사는게 좋지요",
          "original_form": "아그렇군요 실물을 보고 사는게 좋지요",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026829.1.17",
          "form": "사이즈 잘보고 사야합니다",
          "original_form": "사이즈 잘보고 사야합니다",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026829.1.18",
          "form": "인터넷쇼핑은 그게 단점",
          "original_form": "인터넷쇼핑은 그게 단점",
