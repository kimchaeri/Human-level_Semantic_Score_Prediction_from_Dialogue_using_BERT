        },
        {
          "id": "MDRW2100044169.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044169.1.3",
          "form": "오늘 저희 동네는 비가 많이 왔는데",
          "original_form": "오늘 저희 동네는 비가 많이 왔는데",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044169.1.4",
          "form": "날씨가 어땠나요?",
          "original_form": "날씨가 어땠나요?",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044169.1.5",
          "form": "오늘은 살짝 쌀랑하고 어두웠어요",
          "original_form": "오늘은 살짝 쌀랑하고 어두웠어요",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044169.1.6",
          "form": "옷은 든든히 입으셨나요?",
          "original_form": "옷은 든든히 입으셨나요?",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044169.1.7",
          "form": "아 비는 안오고요?",
          "original_form": "아 비는 안오고요?",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044169.1.8",
          "form": "네 긴팔셔츠 입었어요",
          "original_form": "네 긴팔셔츠 입었어요",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044169.1.9",
          "form": "네 비가 안오고 공기는 촉촉해서 좋았어요ㅎㅎ",
          "original_form": "네 비가 안오고 공기는 촉촉해서 좋았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044169.1.10",
          "form": "아 비만 안와도 좋죠",
          "original_form": "아 비만 안와도 좋죠",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044169.1.11",
          "form": "네 그래서 반팔 가디건 입었어요ㅎㅎ",
          "original_form": "네 그래서 반팔 가디건 입었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044169.1.12",
          "form": "딱좋더라고요",
          "original_form": "딱좋더라고요",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044169.1.13",
          "form": "요즘은 비가 갑자기 오기도해서",
          "original_form": "요즘은 비가 갑자기 오기도해서",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044169.1.14",
          "form": "우산을 챙기긴 해야겠더라고요",
          "original_form": "우산을 챙기긴 해야겠더라고요",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044169.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044169.1.16",
          "form": "진짜 우산 예비로 두고 다니거나",
          "original_form": "진짜 우산 예비로 두고 다니거나",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044169.1.17",
          "form": "그쳐ㅋㅋ",
          "original_form": "그쳐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044169.1.18",
          "form": "챙겨야 해요",
          "original_form": "챙겨야 해요",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044169.1.19",
          "form": "안그러면 갑자기 비맞아요",
          "original_form": "안그러면 갑자기 비맞아요",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044169.1.20",
