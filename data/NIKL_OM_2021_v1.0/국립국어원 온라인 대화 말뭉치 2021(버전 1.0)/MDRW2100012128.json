      "utterance": [
        {
          "id": "MDRW2100012128.1.1",
          "form": "군대에서 뭐해?",
          "original_form": "군대에서 뭐해?",
          "speaker_id": "1",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012128.1.2",
          "form": "나 군인아닌데",
          "original_form": "나 군인아닌데",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012128.1.3",
          "form": "군인 맞잖아",
          "original_form": "군인 맞잖아",
          "speaker_id": "1",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012128.1.4",
          "form": "난 민간인이야",
          "original_form": "난 민간인이야",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012128.1.5",
          "form": "전역했어?",
          "original_form": "전역했어?",
          "speaker_id": "1",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012128.1.6",
          "form": "순수한 시민이라구",
          "original_form": "순수한 시민이라구",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012128.1.7",
          "form": "했지했지",
          "original_form": "했지했지",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012128.1.8",
          "form": "거기서 뭐가 가장 기억에 남아?",
          "original_form": "거기서 뭐가 가장 기억에 남아?",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012128.1.9",
          "form": "나 밖이야 밖 군대 기억 하나도안나",
          "original_form": "나 밖이야 밖 군대 기억 하나도안나",
          "speaker_id": "2",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012128.1.10",
          "form": "언제 전역했어?",
          "original_form": "언제 전역했어?",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012128.1.11",
          "form": "꽤됐어",
          "original_form": "꽤됐어",
          "speaker_id": "2",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012128.1.12",
          "form": "너는 군인이야 민간인이야",
          "original_form": "너는 군인이야 민간인이야",
          "speaker_id": "2",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012128.1.13",
          "form": "나는 여자야",
          "original_form": "나는 여자야",
          "speaker_id": "1",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012128.1.14",
          "form": "군인할생각없어?",
          "original_form": "군인할생각없어?",
          "speaker_id": "2",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012128.1.15",
          "form": "없어",
          "original_form": "없어",
          "speaker_id": "1",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012128.1.16",
          "form": "왜없어",
          "original_form": "왜없어",
          "speaker_id": "2",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012128.1.17",
          "form": "몰라 너는 아직까지 연락하는 동기 있어?",
          "original_form": "몰라 너는 아직까지 연락하는 동기 있어?",
          "speaker_id": "1",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012128.1.18",
          "form": "없어",
          "original_form": "없어",
          "speaker_id": "2",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012128.1.19",
          "form": "군생활을 못했거든",
          "original_form": "군생활을 못했거든",
          "speaker_id": "2",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012128.1.20",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210824 19:30"
        },
        {
          "id": "MDRW2100012128.1.21",
          "form": "그냥 이래저래 많은일이 있었어",
          "original_form": "그냥 이래저래 많은일이 있었어",
          "speaker_id": "2",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012128.1.22",
          "form": "무슨일?",
          "original_form": "무슨일?",
          "speaker_id": "1",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012128.1.23",
          "form": "그냥 자그만 실수들",
          "original_form": "그냥 자그만 실수들",
          "speaker_id": "2",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012128.1.24",
          "form": "요즘 후임이 참 나빠",
          "original_form": "요즘 후임이 참 나빠",
          "speaker_id": "2",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012128.1.25",
          "form": "그렇구나 그래도 무사히 전역한거지?",
          "original_form": "그렇구나 그래도 무사히 전역한거지?",
          "speaker_id": "1",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012128.1.26",
          "form": "역빨이 대세야",
          "original_form": "역빨이 대세야",
          "speaker_id": "2",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012128.1.27",
          "form": "그치그치 무사전역했어",
          "original_form": "그치그치 무사전역했어",
          "speaker_id": "2",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012128.1.28",
          "form": "후임한테 갈굼당했어?",
          "original_form": "후임한테 갈굼당했어?",
          "speaker_id": "1",
          "time": "20210824 19:31"
        },
        {
          "id": "MDRW2100012128.1.29",
          "form": "후임이 맛있는거 달라고 쪼아대",
          "original_form": "후임이 맛있는거 달라고 쪼아대",
          "speaker_id": "2",
          "time": "20210824 19:32"
        },
