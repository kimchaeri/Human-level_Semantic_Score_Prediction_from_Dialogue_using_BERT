      "utterance": [
        {
          "id": "MDRW2100004690.1.1",
          "form": "어제 펜트하우스 했나요?",
          "original_form": "어제 펜트하우스 했나요?",
          "speaker_id": "1",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004690.1.2",
          "form": "펜하 오늘하는거아니예요?",
          "original_form": "펜하 오늘하는거아니예요?",
          "speaker_id": "2",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004690.1.3",
          "form": "요즘 맨날 재방만봐서 ㅎ",
          "original_form": "요즘 맨날 재방만봐서 ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004690.1.4",
          "form": "금욜아니고 토욜이예요?",
          "original_form": "금욜아니고 토욜이예요?",
          "speaker_id": "1",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004690.1.5",
          "form": "시즌3부터 거의 안봐가지고",
          "original_form": "시즌3부터 거의 안봐가지고",
          "speaker_id": "1",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004690.1.6",
          "form": "토욜 10시였던가...금욜 10시였던갸",
          "original_form": "토욜 10시였던가...금욜 10시였던갸",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004690.1.7",
          "form": "한번만했던거같아요",
          "original_form": "한번만했던거같아요",
          "speaker_id": "2",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004690.1.8",
          "form": "끝날때 된거같긴 한데",
          "original_form": "끝날때 된거같긴 한데",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004690.1.9",
          "form": "몰아보기해야될지 ㅋ",
          "original_form": "몰아보기해야될지 ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004690.1.10",
          "form": "이젠 진짜 끝물같아요",
          "original_form": "이젠 진짜 끝물같아요",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004690.1.11",
          "form": "저도 돌리다 나오면보고 아님말고 그렇게 보고 있어서요 ㅎ",
          "original_form": "저도 돌리다 나오면보고 아님말고 그렇게 보고 있어서요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004690.1.12",
          "form": "그냥 막방때 결말이나 봐야겠어요",
          "original_form": "그냥 막방때 결말이나 봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004690.1.13",
          "form": "ㅎㅎ 이제 다 밝혀진거같던데요",
          "original_form": "ㅎㅎ 이제 다 밝혀진거같던데요",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004690.1.14",
          "form": "아그래요? 로건리도 살았던데",
          "original_form": "아그래요? 로건리도 살았던데",
          "speaker_id": "1",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004690.1.15",
          "form": "네 살앟고 이제 둘이서 정리단계같더라근요",
          "original_form": "네 살앟고 이제 둘이서 정리단계같더라근요",
          "speaker_id": "2",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100004690.1.16",
          "form": "둘이 살아요?",
          "original_form": "둘이 살아요?",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004690.1.17",
          "form": "유진은 진짜죽은거죠?",
          "original_form": "유진은 진짜죽은거죠?",
          "speaker_id": "1",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004690.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004690.1.19",
          "form": "로건리랑 심수련이랑 유진죽인사람들 복수한다고",
          "original_form": "로건리랑 심수련이랑 유진죽인사람들 복수한다고",
          "speaker_id": "2",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100004690.1.20",
          "form": "그러면서 하나씩 정리하더라구요",
          "original_form": "그러면서 하나씩 정리하더라구요",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004690.1.21",
          "form": "해피엔딩이겠네요",
          "original_form": "해피엔딩이겠네요",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004690.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004690.1.23",
          "form": "이미 주단테는 일본정신병원으로 보내졌더라구요",
          "original_form": "이미 주단테는 일본정신병원으로 보내졌더라구요",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004690.1.24",
          "form": "어머 진짜요?",
          "original_form": "어머 진짜요?",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004690.1.25",
          "form": "오늘 본방봐야겠네요",
          "original_form": "오늘 본방봐야겠네요",
          "speaker_id": "1",
          "time": "20210821 20:14"
        }
      ]
    }
