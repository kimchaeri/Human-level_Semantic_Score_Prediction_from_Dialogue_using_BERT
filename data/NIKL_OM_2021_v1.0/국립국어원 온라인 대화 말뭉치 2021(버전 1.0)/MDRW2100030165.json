      "utterance": [
        {
          "id": "MDRW2100030165.1.1",
          "form": "전 스파게티 먹었어요!",
          "original_form": "전 스파게티 먹었어요!",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030165.1.2",
          "form": "오호 스파게티 자주드시네요",
          "original_form": "오호 스파게티 자주드시네요",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030165.1.3",
          "form": "혹시 직접 만드세요??",
          "original_form": "혹시 직접 만드세요??",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030165.1.4",
          "form": "요즘 꽂혀서 해먹고 있네요ㅋㅋㅋㅋ",
          "original_form": "요즘 꽂혀서 해먹고 있네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030165.1.5",
          "form": "휴 오늘 불금이네용.. 힘듭니다",
          "original_form": "휴 오늘 불금이네용.. 힘듭니다",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030165.1.6",
          "form": "불금인 오늘저녁을 뭐로 마무리 하셧나유",
          "original_form": "불금인 오늘저녁을 뭐로 마무리 하셧나유",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030165.1.7",
          "form": "저녁으론 국수 먹었네요!",
          "original_form": "저녁으론 국수 먹었네요!",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030165.1.8",
          "form": "님은 뭐드셨나용?",
          "original_form": "님은 뭐드셨나용?",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030165.1.9",
          "form": "면류를 좋아하시나봐요~",
          "original_form": "면류를 좋아하시나봐요~",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030165.1.10",
          "form": "저는 제육덮밥 먹었어요",
          "original_form": "저는 제육덮밥 먹었어요",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030165.1.11",
          "form": "후식으로 프로바이오틱스 한포 먹어줬습니당",
          "original_form": "후식으로 프로바이오틱스 한포 먹어줬습니당",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030165.1.12",
          "form": "네 어쩌다보니 오늘 면류를 먹었네요ㅋㅋㅋ",
          "original_form": "네 어쩌다보니 오늘 면류를 먹었네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030165.1.13",
          "form": "제육덮밥 맛있었겠어요!",
          "original_form": "제육덮밥 맛있었겠어요!",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030165.1.14",
          "form": "네에 맛있었어요",
          "original_form": "네에 맛있었어요",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030165.1.15",
          "form": "오랜반에 초밥먹고싶네요 연어 넘맛있어유",
          "original_form": "오랜반에 초밥먹고싶네요 연어 넘맛있어유",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030165.1.16",
          "form": "연어는 언제 먹어도 맛있죠!",
          "original_form": "연어는 언제 먹어도 맛있죠!",
          "speaker_id": "1",
          "time": "20210904 02:06"
        },
        {
          "id": "MDRW2100030165.1.17",
          "form": "마자요 근데 여름에는 회 조심해서 먹어야되더라고요 ㅠ",
          "original_form": "마자요 근데 여름에는 회 조심해서 먹어야되더라고요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030165.1.18",
          "form": "아 맞아요 한번 배탈나면 고생하는거 같더라고요ㅠㅠ",
          "original_form": "아 맞아요 한번 배탈나면 고생하는거 같더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100030165.1.19",
          "form": "혹시 카페에 좋아하는 음료 있으세요?",
          "original_form": "혹시 카페에 좋아하는 음료 있으세요?",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030165.1.20",
          "form": "전 초코 들어간거라면 다 잘 먹어요",
          "original_form": "전 초코 들어간거라면 다 잘 먹어요",
          "speaker_id": "1",
          "time": "20210904 12:39"
        },
        {
          "id": "MDRW2100030165.1.21",
          "form": "님은요?!",
          "original_form": "님은요?!",
          "speaker_id": "1",
          "time": "20210904 12:39"
        },
        {
          "id": "MDRW2100030165.1.22",
          "form": "저는 달달한 밀크쉐이크가 제 취향이더라고요",
          "original_form": "저는 달달한 밀크쉐이크가 제 취향이더라고요",
          "speaker_id": "2",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100030165.1.23",
          "form": "커피는 잘 안마시게 되서요",
          "original_form": "커피는 잘 안마시게 되서요",
          "speaker_id": "2",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100030165.1.24",
          "form": "밀크쉐이크 맛있죠!!",
          "original_form": "밀크쉐이크 맛있죠!!",
          "speaker_id": "1",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030165.1.25",
          "form": "겨울에는 뭐 마시세요?",
          "original_form": "겨울에는 뭐 마시세요?",
          "speaker_id": "1",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030165.1.26",
          "form": "겨울에도 따뜻한 히터맞으며 쉐이크 한잔합니다 ㅎㅎ",
          "original_form": "겨울에도 따뜻한 히터맞으며 쉐이크 한잔합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100030165.1.27",
          "form": "사실 카페에가서 자주마시진 않는타입이고",
          "original_form": "사실 카페에가서 자주마시진 않는타입이고",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100030165.1.28",
          "form": "토레타를자주마시는거같아용",
          "original_form": "토레타를자주마시는거같아용",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100030165.1.29",
          "form": "토레타 맛있어요??",
          "original_form": "토레타 맛있어요??",
          "speaker_id": "1",
          "time": "20210904 13:58"
        },
        {
          "id": "MDRW2100030165.1.30",
          "form": "네 저는 음료수살때 토레타 자주사요",
          "original_form": "네 저는 음료수살때 토레타 자주사요",
          "speaker_id": "2",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100030165.1.31",
          "form": "티비에서 몇년전에 박보영이 토레타 광고하는거 봤던거 같아요",
          "original_form": "티비에서 몇년전에 박보영이 토레타 광고하는거 봤던거 같아요",
          "speaker_id": "1",
          "time": "20210904 16:39"
        }
      ]
    }
