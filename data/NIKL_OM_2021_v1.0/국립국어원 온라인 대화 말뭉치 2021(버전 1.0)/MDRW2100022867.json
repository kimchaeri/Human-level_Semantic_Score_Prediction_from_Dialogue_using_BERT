      "utterance": [
        {
          "id": "MDRW2100022867.1.1",
          "form": "여행가고싶은곳 있으세요?",
          "original_form": "여행가고싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100022867.1.2",
          "form": "알레스카요",
          "original_form": "알레스카요",
          "speaker_id": "2",
          "time": "20210829 20:10"
        },
        {
          "id": "MDRW2100022867.1.3",
          "form": "오로라를 제 눈으로 꼭 보고싶어요",
          "original_form": "오로라를 제 눈으로 꼭 보고싶어요",
          "speaker_id": "2",
          "time": "20210829 20:10"
        },
        {
          "id": "MDRW2100022867.1.4",
          "form": "와 멋지네요",
          "original_form": "와 멋지네요",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100022867.1.5",
          "form": "알래스카여행 진짜 황홀하겠어요",
          "original_form": "알래스카여행 진짜 황홀하겠어요",
          "speaker_id": "1",
          "time": "20210830 11:39"
        },
        {
          "id": "MDRW2100022867.1.6",
          "form": "맞아요 한번쯤 보고싶어요",
          "original_form": "맞아요 한번쯤 보고싶어요",
          "speaker_id": "2",
          "time": "20210830 13:02"
        },
        {
          "id": "MDRW2100022867.1.7",
          "form": "name1님은 어디가보고싶으세요?",
          "original_form": "&name1&님은 어디가보고싶으세요?",
          "speaker_id": "2",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100022867.1.8",
          "form": "저는 아일랜드요!",
          "original_form": "저는 아일랜드요!",
          "speaker_id": "1",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100022867.1.9",
          "form": "더블린에 가보고싶어요 :)",
          "original_form": "더블린에 가보고싶어요 :)",
          "speaker_id": "1",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100022867.1.10",
          "form": "오 더블린",
          "original_form": "오 더블린",
          "speaker_id": "2",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100022867.1.11",
          "form": "우와 뷰가 너무 예쁜데요",
          "original_form": "우와 뷰가 너무 예쁜데요",
          "speaker_id": "2",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100022867.1.12",
          "form": "한달살이 해보고싶어요",
          "original_form": "한달살이 해보고싶어요",
          "speaker_id": "2",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100022867.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100022867.1.14",
          "form": "현실은.. 한달 여행 언제 갈 수 있을까요 ㅠ",
          "original_form": "현실은.. 한달 여행 언제 갈 수 있을까요 ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100022867.1.15",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "2",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100022867.1.16",
          "form": "당장 코로나만해도ㅜ",
          "original_form": "당장 코로나만해도ㅜ",
          "speaker_id": "2",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100022867.1.17",
          "form": "현실적으로 한달 빼기 쉽지않고요",
          "original_form": "현실적으로 한달 빼기 쉽지않고요",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100022867.1.18",
          "form": "은퇴후엔 코로나같은거 없겠죠?",
          "original_form": "은퇴후엔 코로나같은거 없겠죠?",
          "speaker_id": "1",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100022867.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100022867.1.20",
          "form": "하.. name2님은 여행가면 기념품 뭐사오세요?",
          "original_form": "하.. &name2&님은 여행가면 기념품 뭐사오세요?",
          "speaker_id": "1",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100022867.1.21",
          "form": "전 엽서같은거 좋아해서",
          "original_form": "전 엽서같은거 좋아해서",
          "speaker_id": "2",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100022867.1.22",
          "form": "보통 엽서 사와요",
          "original_form": "보통 엽서 사와요",
          "speaker_id": "2",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100022867.1.23",
          "form": "그외엔 막 기념품 딱히 챙겨오는건 없어요",
          "original_form": "그외엔 막 기념품 딱히 챙겨오는건 없어요",
          "speaker_id": "2",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100022867.1.24",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100022867.1.25",
          "form": "와 저도 엽서요",
          "original_form": "와 저도 엽서요",
          "speaker_id": "1",
          "time": "20210830 13:50"
        },
        {
          "id": "MDRW2100022867.1.26",
          "form": "엽서랑 지인선물용 먹을거 정도",
          "original_form": "엽서랑 지인선물용 먹을거 정도",
          "speaker_id": "1",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100022867.1.27",
          "form": "가 맞아요 지인선물용",
          "original_form": "가 맞아요 지인선물용",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100022867.1.28",
          "form": "그정도가 다인것 같아요",
          "original_form": "그정도가 다인것 같아요",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100022867.1.29",
          "form": "지인선물사는것도 은근 고민이 많이되더라구요",
          "original_form": "지인선물사는것도 은근 고민이 많이되더라구요",
          "speaker_id": "2",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100022867.1.30",
          "form": "예쁜.여행 엽서보면",
          "original_form": "예쁜.여행 엽서보면",
          "speaker_id": "1",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100022867.1.31",
          "form": "세상 행복해요",
          "original_form": "세상 행복해요",
          "speaker_id": "1",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100022867.1.32",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100022867.1.33",
          "form": "맞아요 그게 가격보다도 짐이라..",
          "original_form": "맞아요 그게 가격보다도 짐이라..",
          "speaker_id": "1",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100022867.1.34",
          "form": "직접쓰지도 못하지만",
          "original_form": "직접쓰지도 못하지만",
          "speaker_id": "2",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100022867.1.35",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100022867.1.36",
          "form": "ㅋㅋㅋ계속 들고다녀야한다는",
          "original_form": "ㅋㅋㅋ계속 들고다녀야한다는",
          "speaker_id": "1",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100022867.1.37",
          "form": "맞아요 짐이죠",
          "original_form": "맞아요 짐이죠",
          "speaker_id": "2",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100022867.1.38",
          "form": "유리제품 사고싶어도",
          "original_form": "유리제품 사고싶어도",
          "speaker_id": "2",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100022867.1.39",
          "form": "깨질까봐 못사요",
          "original_form": "깨질까봐 못사요",
          "speaker_id": "2",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100022867.1.40",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100022867.1.41",
          "form": "그릇하나샀다가 모시고 다녔어요",
          "original_form": "그릇하나샀다가 모시고 다녔어요",
          "speaker_id": "1",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100022867.1.42",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100022867.1.43",
          "form": "앜ㅋㅋㅋ맞아요",
          "original_form": "앜ㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100022867.1.44",
          "form": "엽서가 진짜 가지고다니기도 편하도 딱 좋죠",
          "original_form": "엽서가 진짜 가지고다니기도 편하도 딱 좋죠",
          "speaker_id": "2",
          "time": "20210830 14:49"
        },
        {
