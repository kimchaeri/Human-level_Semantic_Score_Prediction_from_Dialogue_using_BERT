      "utterance": [
        {
          "id": "MDRW2100044394.1.1",
          "form": "해보고싶네요",
          "original_form": "해보고싶네요",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044394.1.2",
          "form": "직장사람중에친한사람있나요",
          "original_form": "직장사람중에친한사람있나요",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044394.1.3",
          "form": "그닥...? 없는 거같아요",
          "original_form": "그닥...? 없는 거같아요",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044394.1.4",
          "form": "별로 친해지고 싶은 사람이 없네요 ㅋㅋ",
          "original_form": "별로 친해지고 싶은 사람이 없네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044394.1.5",
          "form": "동료들이랑 친하게 지내시나요?",
          "original_form": "동료들이랑 친하게 지내시나요?",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044394.1.6",
          "form": "운이좋게도요ㅠ",
          "original_form": "운이좋게도요ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044394.1.7",
          "form": "부러워요~ 스트레스 조금이라도 덜 받을 거같아요",
          "original_form": "부러워요~ 스트레스 조금이라도 덜 받을 거같아요",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044394.1.8",
          "form": "약간그걸로풀어요",
          "original_form": "약간그걸로풀어요",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044394.1.9",
          "form": "그럼 이직생각은 딱히 없으신가요?",
          "original_form": "그럼 이직생각은 딱히 없으신가요?",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044394.1.10",
          "form": "있어욪!",
          "original_form": "있어욪!",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044394.1.11",
          "form": "한2년뒤에",
          "original_form": "한2년뒤에",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044394.1.12",
          "form": "오 계획이 있으시군여",
          "original_form": "오 계획이 있으시군여",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044394.1.13",
          "form": "어느계열인지 물어봐도돼요?",
          "original_form": "어느계열인지 물어봐도돼요?",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044394.1.14",
          "form": "저 보건쪽이요",
          "original_form": "저 보건쪽이요",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044394.1.15",
          "form": "아하 요즘 바쁘시겠어요",
          "original_form": "아하 요즘 바쁘시겠어요",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044394.1.16",
          "form": "조끔요ㅎㅎ",
          "original_form": "조끔요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044394.1.17",
          "form": "고생이 많으십니다 정말로..",
          "original_form": "고생이 많으십니다 정말로..",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044394.1.18",
          "form": "ㅎㅎ아닙니당",
          "original_form": "ㅎㅎ아닙니당",
          "speaker_id": "1",
          "time": "20210907 22:53"
        }
