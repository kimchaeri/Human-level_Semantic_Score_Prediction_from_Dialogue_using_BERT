      "utterance": [
        {
          "id": "MDRW2100041062.1.1",
          "form": "레인보우 로망스로 데뷔했나봐",
          "original_form": "레인보우 로망스로 데뷔했나봐",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100041062.1.2",
          "form": "엄현경 말하는거죠?",
          "original_form": "엄현경 말하는거죠?",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100041062.1.3",
          "form": "엉 엄현경 출연작이 엄청 많아",
          "original_form": "엉 엄현경 출연작이 엄청 많아",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100041062.1.4",
          "form": "해피투게더 엠씨도 했어요",
          "original_form": "해피투게더 엠씨도 했어요",
          "speaker_id": "1",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100041062.1.5",
          "form": "일단뛰어 청춘드라마",
          "original_form": "일단뛰어 청춘드라마",
          "speaker_id": "2",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100041062.1.6",
          "form": "근데 일일드라마 나오는건 의외다",
          "original_form": "근데 일일드라마 나오는건 의외다",
          "speaker_id": "1",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100041062.1.7",
          "form": "맞어 2007년도 해피투게더 3에서 나왓네",
          "original_form": "맞어 2007년도 해피투게더 3에서 나왓네",
          "speaker_id": "2",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100041062.1.8",
          "form": "일일드라마는 신인들 많이 나오는데ㅣ",
          "original_form": "일일드라마는 신인들 많이 나오는데ㅣ",
          "speaker_id": "1",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100041062.1.9",
          "form": "착한여자 백일홍에서 조연",
          "original_form": "착한여자 백일홍에서 조연",
          "speaker_id": "2",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100041062.1.10",
          "form": "저번에 당나귀라는 프로여 토니안이 기획사 사장으로 나오던데",
          "original_form": "저번에 당나귀라는 프로여 토니안이 기획사 사장으로 나오던데",
          "speaker_id": "1",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100041062.1.11",
          "form": "강력반도 조연인듯",
          "original_form": "강력반도 조연인듯",
          "speaker_id": "2",
          "time": "20210905 16:23"
        },
        {
          "id": "MDRW2100041062.1.12",
          "form": "많이 나오긴 했네요",
          "original_form": "많이 나오긴 했네요",
          "speaker_id": "1",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100041062.1.13",
          "form": "코르나때문에 영화제작이 어려워 쟁쟁한 영화배우들이 다 드라마출연해서 경쟁이 심하다",
          "original_form": "코르나때문에 영화제작이 어려워 쟁쟁한 영화배우들이 다 드라마출연해서 경쟁이 심하다",
          "speaker_id": "2",
          "time": "20210905 16:24"
        }
      ]
