      "utterance": [
        {
          "id": "MDRW2100002027.1.1",
          "form": "오늘 저녁 맛있었다",
          "original_form": "오늘 저녁 맛있었다",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100002027.1.2",
          "form": "저녁에 뭐?",
          "original_form": "저녁에 뭐?",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100002027.1.3",
          "form": "찌게 말이야",
          "original_form": "찌게 말이야",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100002027.1.4",
          "form": "글쿤 ㅋ 내가 좀 맛있게 하지",
          "original_form": "글쿤 ㅋ 내가 좀 맛있게 하지",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100002027.1.5",
          "form": "무얼 넣었는데?",
          "original_form": "무얼 넣었는데?",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100002027.1.6",
          "form": "애호박 만땅에 양념을 비법양념으로 하면 이렇게 나온다",
          "original_form": "애호박 만땅에 양념을 비법양념으로 하면 이렇게 나온다",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100002027.1.7",
          "form": "배터질 정도로 먹었다",
          "original_form": "배터질 정도로 먹었다",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100002027.1.8",
          "form": "굿굿 ㅎㅎ 내일도 먹자",
          "original_form": "굿굿 ㅎㅎ 내일도 먹자",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100002027.1.9",
          "form": "엄마도 먹었냐?",
          "original_form": "엄마도 먹었냐?",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100002027.1.10",
          "form": "아니 ㅠㅠ 엄마 안먹음",
          "original_form": "아니 ㅠㅠ 엄마 안먹음",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100002027.1.11",
          "form": "왜 맛있는데...",
          "original_form": "왜 맛있는데...",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100002027.1.12",
          "form": "글게 ㅋ 안떙기나봄",
          "original_form": "글게 ㅋ 안떙기나봄",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100002027.1.13",
          "form": "그렇게 맵지도 않고",
          "original_form": "그렇게 맵지도 않고",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100002027.1.14",
          "form": "딱 아빠 맞춤이다",
          "original_form": "딱 아빠 맞춤이다",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100002027.1.15",
          "form": "아무튼 잘 었었다.",
          "original_form": "아무튼 잘 었었다.",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100002027.1.16",
          "form": "아무튼 잘 먹었다",
          "original_form": "아무튼 잘 먹었다",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100002027.1.17",
          "form": "ㅎㅎ 잘먹으니 좋군욤",
          "original_form": "ㅎㅎ 잘먹으니 좋군욤",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100002027.1.18",
          "form": "내일 점심엔 뭐 먹을까",
          "original_form": "내일 점심엔 뭐 먹을까",
