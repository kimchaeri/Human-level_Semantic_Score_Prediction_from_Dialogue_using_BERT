      "utterance": [
        {
          "id": "MDRW2100017583.1.1",
          "form": "동물 키운 적 있으세요?",
          "original_form": "동물 키운 적 있으세요?",
          "speaker_id": "1",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017583.1.2",
          "form": "강아지 잠시 맡은적은있어요",
          "original_form": "강아지 잠시 맡은적은있어요",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017583.1.3",
          "form": "너무 좋아합니다",
          "original_form": "너무 좋아합니다",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017583.1.4",
          "form": "강아지 돌볼 줄 아시나봐요 보통 키운 사람한테 맡기잖아요",
          "original_form": "강아지 돌볼 줄 아시나봐요 보통 키운 사람한테 맡기잖아요",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017583.1.5",
          "form": "키워본적은 없는데 좋아하는걸 알아선지",
          "original_form": "키워본적은 없는데 좋아하는걸 알아선지",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017583.1.6",
          "form": "그리고 짧게 여행갔다온거라서",
          "original_form": "그리고 짧게 여행갔다온거라서",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017583.1.7",
          "form": "저도 좋고 그친구도 좋았죠",
          "original_form": "저도 좋고 그친구도 좋았죠",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017583.1.8",
          "form": "name1은 동물 기르시나요?",
          "original_form": "&name1&은 동물 기르시나요?",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017583.1.9",
          "form": "아뇨 저는 길러본 적 없어요",
          "original_form": "아뇨 저는 길러본 적 없어요",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017583.1.10",
          "form": "근데 동물은 좋아해요",
          "original_form": "근데 동물은 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017583.1.11",
          "form": "무슨 동물 좋아하세요?",
          "original_form": "무슨 동물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017583.1.12",
          "form": "토끼 좋아해요",
          "original_form": "토끼 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017583.1.13",
          "form": "강아지랑 고양이도 좋고",
          "original_form": "강아지랑 고양이도 좋고",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017583.1.14",
          "form": "최근에 페럿 키우고 싶었어요",
          "original_form": "최근에 페럿 키우고 싶었어요",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017583.1.15",
          "form": "아 다 너무귀엽죠",
          "original_form": "아 다 너무귀엽죠",
          "speaker_id": "2",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017583.1.16",
          "form": "페럿도 난이도는 좀있다고 들었는데",
          "original_form": "페럿도 난이도는 좀있다고 들었는데",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017583.1.17",
          "form": "너무 사랑스럽게 생겼어요",
          "original_form": "너무 사랑스럽게 생겼어요",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017583.1.18",
          "form": "맞아요 찾아보니까 쉽지 않더라고요",
          "original_form": "맞아요 찾아보니까 쉽지 않더라고요",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017583.1.19",
          "form": "보는 모습만이 다가 아니니까요",
          "original_form": "보는 모습만이 다가 아니니까요",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017583.1.20",
          "form": "페럿 너무 이뻐요,,",
          "original_form": "페럿 너무 이뻐요,,",
          "speaker_id": "1",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017583.1.21",
          "form": "그래도 여우보다는 사육 난이도 가 낮은거같아요",
          "original_form": "그래도 여우보다는 사육 난이도 가 낮은거같아요",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017583.1.22",
          "form": "여우 키울 생각은 한번도 안해봤는데",
          "original_form": "여우 키울 생각은 한번도 안해봤는데",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017583.1.23",
          "form": "되게 독특하네요",
          "original_form": "되게 독특하네요",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017583.1.24",
          "form": "제가 동물을 좋아해서 동물유투버영상들을 자주보는데",
          "original_form": "제가 동물을 좋아해서 동물유투버영상들을 자주보는데",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017583.1.25",
          "form": "고양이 보다도 더 알기힘든 성격이고 낯을 많이가리더라구요",
          "original_form": "고양이 보다도 더 알기힘든 성격이고 낯을 많이가리더라구요",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017583.1.26",
          "form": "여우가 낯을 많이 가리는 줄은 처음 알았네요",
          "original_form": "여우가 낯을 많이 가리는 줄은 처음 알았네요",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017583.1.27",
          "form": "저보다 동물 더 많이많이 좋아하시는 거 같아요ㅋㅋㅋㅋ",
          "original_form": "저보다 동물 더 많이많이 좋아하시는 거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017583.1.28",
          "form": "ㅋㅋㅋㅋㅋ동물을 못키우는 상황이라서",
          "original_form": "ㅋㅋㅋㅋㅋ동물을 못키우는 상황이라서",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017583.1.29",
          "form": "온갖 동물 영상들만 빠삭하게 알고있어요ㅋㅋㅋㅋㅋ",
          "original_form": "온갖 동물 영상들만 빠삭하게 알고있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017583.1.30",
          "form": "기본지식이 이미 쌓여있어서 따로 공부할 필요 없겠어요ㅋㅋㅋㅋ",
          "original_form": "기본지식이 이미 쌓여있어서 따로 공부할 필요 없겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017583.1.31",
          "form": "멋져요",
          "original_form": "멋져요",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017583.1.32",
          "form": "보는것만으로도 사랑스럽기 쉽지 않은데",
