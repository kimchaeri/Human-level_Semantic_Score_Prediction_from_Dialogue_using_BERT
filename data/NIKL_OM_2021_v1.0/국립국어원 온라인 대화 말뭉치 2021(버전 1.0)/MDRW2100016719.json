      "utterance": [
        {
          "id": "MDRW2100016719.1.1",
          "form": "긴팔 꺼내셨나요!",
          "original_form": "긴팔 꺼내셨나요!",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016719.1.2",
          "form": "오 저는 아직요! 더위를 많이타서",
          "original_form": "오 저는 아직요! 더위를 많이타서",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100016719.1.3",
          "form": "name2님 긴팔 개장하셨나요?!",
          "original_form": "&name2&님 긴팔 개장하셨나요?!",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100016719.1.4",
          "form": "어제 꺼냈어요! 근데 오늘은 좀 덥더라고요 ㅠ",
          "original_form": "어제 꺼냈어요! 근데 오늘은 좀 덥더라고요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100016719.1.5",
          "form": "맞아요 오늘은 해가 나서 그런가봐요",
          "original_form": "맞아요 오늘은 해가 나서 그런가봐요",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100016719.1.6",
          "form": "전 한 9월쯤 가면 보고 슬슬 맨투맨도 꺼내려고요!",
          "original_form": "전 한 9월쯤 가면 보고 슬슬 맨투맨도 꺼내려고요!",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100016719.1.7",
          "form": "이러다 훅 추워질수도 있으니까..!",
          "original_form": "이러다 훅 추워질수도 있으니까..!",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100016719.1.8",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100016719.1.9",
          "form": "감기 조심하셔야돼요 !!",
          "original_form": "감기 조심하셔야돼요 !!",
          "speaker_id": "2",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100016719.1.10",
          "form": "네네 name2님두요!",
          "original_form": "네네 &name2&님두요!",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100016719.1.11",
          "form": "그래도 가을은 미세먼지도 덜하고..",
          "original_form": "그래도 가을은 미세먼지도 덜하고..",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100016719.1.12",
          "form": "코로나만 아니면 동네방네 돌아다녔을것 같아요",
          "original_form": "코로나만 아니면 동네방네 돌아다녔을것 같아요",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100016719.1.13",
          "form": "저도요 ㅠㅠ",
          "original_form": "저도요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100016719.1.14",
          "form": "나들이가기 딱인데 슬퍼요",
          "original_form": "나들이가기 딱인데 슬퍼요",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100016719.1.15",
          "form": "맞아요ㅠ~~~ 진짜 아쉽네요",
          "original_form": "맞아요ㅠ~~~ 진짜 아쉽네요",
          "speaker_id": "1",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100016719.1.16",
          "form": "일기예보엔 비온댔는데 오늘은 또 엄청 맑네요",
          "original_form": "일기예보엔 비온댔는데 오늘은 또 엄청 맑네요",
          "speaker_id": "1",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100016719.1.17",
          "form": "아쉬운대로 빨래나 말리는중ㅋㅋ",
          "original_form": "아쉬운대로 빨래나 말리는중ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100016719.1.18",
          "form": "날씨가 정말 지멋대로 ㅋㅋㅋㅋㅋ",
          "original_form": "날씨가 정말 지멋대로 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:00"
        },
        {
          "id": "MDRW2100016719.1.19",
          "form": "전 이따 산책이나 나가려고요",
          "original_form": "전 이따 산책이나 나가려고요",
          "speaker_id": "2",
          "time": "20210829 14:00"
        },
        {
          "id": "MDRW2100016719.1.20",
          "form": "오 저도 산책 다녀왔어요~~~ 은근 덥고 해가 길었어요!",
          "original_form": "오 저도 산책 다녀왔어요~~~ 은근 덥고 해가 길었어요!",
          "speaker_id": "1",
          "time": "20210829 20:58"
        },
        {
          "id": "MDRW2100016719.1.21",
          "form": "맞아요 은근 그렇더라고요",
          "original_form": "맞아요 은근 그렇더라고요",
          "speaker_id": "2",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100016719.1.22",
          "form": "저는 이제 개강이네요 ㅎㅎ.",
          "original_form": "저는 이제 개강이네요 ㅎㅎ.",
          "speaker_id": "2",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100016719.1.23",
          "form": "가을..개강.....",
          "original_form": "가을..개강.....",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100016719.1.24",
          "form": "가을이 잠시 싫어질것 같으시겠어요...",
          "original_form": "가을이 잠시 싫어질것 같으시겠어요...",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100016719.1.25",
          "form": "하지만 비대면이라 ㅎ.. 아직 실감이 안나요",
          "original_form": "하지만 비대면이라 ㅎ.. 아직 실감이 안나요",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100016719.1.26",
          "form": "아직 방학갘은대 분명",
          "original_form": "아직 방학갘은대 분명",
          "speaker_id": "2",
          "time": "20210830 18:57"
        },
        {
          "id": "MDRW2100016719.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100016719.1.28",
          "form": "비대면 최고...",
          "original_form": "비대면 최고...",
          "speaker_id": "1",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100016719.1.29",
          "form": "이대로 종강합시다..",
          "original_form": "이대로 종강합시다..",
          "speaker_id": "1",
          "time": "20210831 13:43"
        }
      ]
