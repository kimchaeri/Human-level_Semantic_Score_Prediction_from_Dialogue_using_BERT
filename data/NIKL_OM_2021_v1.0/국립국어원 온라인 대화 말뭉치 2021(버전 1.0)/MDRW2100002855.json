      "utterance": [
        {
          "id": "MDRW2100002855.1.1",
          "form": "세상은 날씬한 사람만 있는듯 돌아가요",
          "original_form": "세상은 날씬한 사람만 있는듯 돌아가요",
          "speaker_id": "2",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002855.1.2",
          "form": "ㅠㅠ 맞아요",
          "original_form": "ㅠㅠ 맞아요",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002855.1.3",
          "form": "그래오 건강위해서 빼는게 맞긴 한데",
          "original_form": "그래오 건강위해서 빼는게 맞긴 한데",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002855.1.4",
          "form": "늘 입 따로 머리 따로 ㅎㅎ",
          "original_form": "늘 입 따로 머리 따로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 16:00"
        },
        {
          "id": "MDRW2100002855.1.5",
          "form": "맞아요 건강을 위해선 해야하는데",
          "original_form": "맞아요 건강을 위해선 해야하는데",
          "speaker_id": "2",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002855.1.6",
          "form": "저도 따로따로예요",
          "original_form": "저도 따로따로예요",
          "speaker_id": "2",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002855.1.7",
          "form": "인내를 배우고 싶네요",
          "original_form": "인내를 배우고 싶네요",
          "speaker_id": "2",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002855.1.8",
          "form": "냉장고를 비워야 겠어요",
          "original_form": "냉장고를 비워야 겠어요",
          "speaker_id": "2",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002855.1.9",
          "form": "사실 군것질만 안해도 빠지죠",
          "original_form": "사실 군것질만 안해도 빠지죠",
          "speaker_id": "2",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002855.1.10",
          "form": "맞아요  ㅋ ㄱ\r\n근데 맛있는더 너무 많음 ㅠㅠㅋ",
          "original_form": "맞아요  ㅋ ㄱ\r\n근데 맛있는더 너무 많음 ㅠㅠㅋ",
          "speaker_id": "1",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002855.1.11",
          "form": "너무 많아요 달달이는 왜이리 좋은지",
          "original_form": "너무 많아요 달달이는 왜이리 좋은지",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002855.1.12",
          "form": "디저트 배는 따로 있다고 하죠",
          "original_form": "디저트 배는 따로 있다고 하죠",
          "speaker_id": "2",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002855.1.13",
          "form": "거짓이 아니예요",
          "original_form": "거짓이 아니예요",
          "speaker_id": "2",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002855.1.14",
          "form": "최고의적은 남편",
          "original_form": "최고의적은 남편",
          "speaker_id": "1",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002855.1.15",
          "form": "그다음 적은 떡볶이 입니다 ㅋ",
          "original_form": "그다음 적은 떡볶이 입니다 ㅋ",
          "speaker_id": "1",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002855.1.16",
          "form": "디저트 배는 원래 따로에요 ㅋㅋ",
          "original_form": "디저트 배는 원래 따로에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002855.1.17",
          "form": "맞아요 저 떡볶이 넘 좋아해요",
          "original_form": "맞아요 저 떡볶이 넘 좋아해요",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002855.1.18",
          "form": "디저트 아 케이크 못끊겠어요",
          "original_form": "디저트 아 케이크 못끊겠어요",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002855.1.19",
          "form": "커피솝에서 디저트 꼭먹어요",
          "original_form": "커피솝에서 디저트 꼭먹어요",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002855.1.20",
          "form": "아 낼부터 걷기라도 할까봐요",
          "original_form": "아 낼부터 걷기라도 할까봐요",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002855.1.21",
          "form": "가을옷입으려면 그래야겠죠 ㅜㅜ",
          "original_form": "가을옷입으려면 그래야겠죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002855.1.22",
          "form": "네네 ㅠㅜ 옷을 크게 사면 되는데 우리나라는\r\n크면 안예쁜옷만",
          "original_form": "네네 ㅠㅜ 옷을 크게 사면 되는데 우리나라는\r\n크면 안예쁜옷만",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002855.1.23",
          "form": "옷에 몸을 맞춰야 하는 슬픔 ㅋ",
          "original_form": "옷에 몸을 맞춰야 하는 슬픔 ㅋ",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002855.1.24",
          "form": "맞아요 큰옷이 이뿌지도 않고 잘없죠",
          "original_form": "맞아요 큰옷이 이뿌지도 않고 잘없죠",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002855.1.25",
          "form": "몸을 밀어 넣어 봐야죠",
          "original_form": "몸을 밀어 넣어 봐야죠",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002855.1.26",
          "form": "주말엔 아무래도 어디가거나 집에 있어서 외식이나 배달을 하니 다욧 힘들어요",
          "original_form": "주말엔 아무래도 어디가거나 집에 있어서 외식이나 배달을 하니 다욧 힘들어요",
          "speaker_id": "2",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002855.1.27",
          "form": "그래도 가울전엔 해보려고요",
          "original_form": "그래도 가울전엔 해보려고요",
          "speaker_id": "2",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002855.1.28",
          "form": "가을",
          "original_form": "가을",
          "speaker_id": "2",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002855.1.29",
          "form": "네네 저도 도전 해야죠 ㅎㅎ",
          "original_form": "네네 저도 도전 해야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002855.1.30",
          "form": "우리 가을엔 살짝 빼보죠 뭐 ㅋㅋ",
          "original_form": "우리 가을엔 살짝 빼보죠 뭐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002855.1.31",
          "form": "도전",
          "original_form": "도전",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002855.1.32",
          "form": "아 이렇게 날짜 미루는거 부터가 좀 불길하지만요 ㅋㅋ",
          "original_form": "아 이렇게 날짜 미루는거 부터가 좀 불길하지만요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002855.1.33",
          "form": "ㅎㅎㅎ 전 담주부터 해보려구요",
          "original_form": "ㅎㅎㅎ 전 담주부터 해보려구요",
          "speaker_id": "1",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002855.1.34",
          "form": "네 저도 그래 볼래요",
          "original_form": "네 저도 그래 볼래요",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002855.1.35",
          "form": "걷기 실내 자전거",
          "original_form": "걷기 실내 자전거",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002855.1.36",
          "form": "디저트 끊기 까지만 해볼래요",
          "original_form": "디저트 끊기 까지만 해볼래요",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002855.1.37",
          "form": "전 유산균은 꼭먹어요",
          "original_form": "전 유산균은 꼭먹어요",
          "speaker_id": "2",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002855.1.38",
          "form": "식사량을 줄이면 변비가 생겨서요",
          "original_form": "식사량을 줄이면 변비가 생겨서요",
