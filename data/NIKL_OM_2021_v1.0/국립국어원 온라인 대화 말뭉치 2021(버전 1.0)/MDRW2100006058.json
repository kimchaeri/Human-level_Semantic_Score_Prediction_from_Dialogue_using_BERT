        {
          "id": "MDRW2100006058.1.1",
          "form": "학창시절 즐거우셨낭6",
          "original_form": "학창시절 즐거우셨낭6",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100006058.1.2",
          "form": "나요",
          "original_form": "나요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100006058.1.3",
          "form": "네 재밋엇죠",
          "original_form": "네 재밋엇죠",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100006058.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100006058.1.5",
          "form": "저는 맞은 기억이...",
          "original_form": "저는 맞은 기억이...",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100006058.1.6",
          "form": "누구에게요?",
          "original_form": "누구에게요?",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100006058.1.7",
          "form": "선생님들이요..체벌 없으셨나요?",
          "original_form": "선생님들이요..체벌 없으셨나요?",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100006058.1.8",
          "form": "저도 잇엇죠",
          "original_form": "저도 잇엇죠",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100006058.1.9",
          "form": "아직도 잊을 수 없는 몇명 잇어요",
          "original_form": "아직도 잊을 수 없는 몇명 잇어요",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100006058.1.10",
          "form": "저는 맞다가 뛰쳐나간 기억이....",
          "original_form": "저는 맞다가 뛰쳐나간 기억이....",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100006058.1.11",
          "form": "끔찍한 기억으로 남아잇겟네요 지금도...",
          "original_form": "끔찍한 기억으로 남아잇겟네요 지금도...",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100006058.1.12",
          "form": "네 남고라 그런지 체벌이 일상화였어요",
          "original_form": "네 남고라 그런지 체벌이 일상화였어요",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100006058.1.13",
          "form": "왜그렇게까지 폭력을 휘둘러야햇을까요",
          "original_form": "왜그렇게까지 폭력을 휘둘러야햇을까요",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100006058.1.14",
          "form": "참...",
          "original_form": "참...",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100006058.1.15",
          "form": "저는 초등학생때 특히 폭력을 휘두르던 선생이 잊혀지질 않아요",
          "original_form": "저는 초등학생때 특히 폭력을 휘두르던 선생이 잊혀지질 않아요",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100006058.1.16",
          "form": "초등학생이면 어릴땐데 ㅠ",
          "original_form": "초등학생이면 어릴땐데 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100006058.1.17",
          "form": "네 지금도 또렷하게 기억나요",
          "original_form": "네 지금도 또렷하게 기억나요",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100006058.1.18",
          "form": "제가 잘못한 게 아닌데도 무조건 맞앗어요",
          "original_form": "제가 잘못한 게 아닌데도 무조건 맞앗어요",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100006058.1.19",
          "form": "늘 단체로 맞고 단체로 기합받고...",
          "original_form": "늘 단체로 맞고 단체로 기합받고...",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100006058.1.20",
          "form": "아 맞아요..저도 초등학교때 전체 의자들고 책상위에 무릎꿇었던 기억도...ㅠ",
          "original_form": "아 맞아요..저도 초등학교때 전체 의자들고 책상위에 무릎꿇었던 기억도...ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        }
      ]
