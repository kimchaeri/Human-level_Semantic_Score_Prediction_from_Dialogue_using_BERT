      "utterance": [
        {
          "id": "MDRW2100045684.1.1",
          "form": "name2님 안녕하세요, 떡볶이 좋아하시나요?",
          "original_form": "&name2&님 안녕하세요, 떡볶이 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100045684.1.2",
          "form": "안녕하세요 name1님~!",
          "original_form": "안녕하세요 &name1&님~!",
          "speaker_id": "2",
          "time": "20210908 05:22"
        },
        {
          "id": "MDRW2100045684.1.3",
          "form": "싫어하진 않는데 엄청 좋아하진 않아요 ㅋㅋㅋ",
          "original_form": "싫어하진 않는데 엄청 좋아하진 않아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:23"
        },
        {
          "id": "MDRW2100045684.1.4",
          "form": "name1님은 떡볶이 좋아하시나요?",
          "original_form": "&name1&님은 떡볶이 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 05:23"
        },
        {
          "id": "MDRW2100045684.1.5",
          "form": "전 좋아해요!ㅋㅋㅋ근데 떡볶이 먹고나서 나오는",
          "original_form": "전 좋아해요!ㅋㅋㅋ근데 떡볶이 먹고나서 나오는",
          "speaker_id": "1",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100045684.1.6",
          "form": "치즈밥을 더 좋아하는 거 같아요ㄱㅋㅋ",
          "original_form": "치즈밥을 더 좋아하는 거 같아요ㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100045684.1.7",
          "form": "아하 ㅋㅋㅋㅋ 치즈밥은 뭔가요",
          "original_form": "아하 ㅋㅋㅋㅋ 치즈밥은 뭔가요",
          "speaker_id": "2",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100045684.1.8",
          "form": "볶음밥같은걸까요?",
          "original_form": "볶음밥같은걸까요?",
          "speaker_id": "2",
          "time": "20210908 05:24"
        },
        {
          "id": "MDRW2100045684.1.9",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210908 05:25"
        },
        {
          "id": "MDRW2100045684.1.10",
          "form": "남은 양념에 치즈랑 해서 볶아주는데",
          "original_form": "남은 양념에 치즈랑 해서 볶아주는데",
          "speaker_id": "1",
          "time": "20210908 05:25"
        },
        {
          "id": "MDRW2100045684.1.11",
          "form": "너무 맛있어요ㅠㅠ",
          "original_form": "너무 맛있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:25"
        },
        {
          "id": "MDRW2100045684.1.12",
          "form": "으악 맛없을 수가 없겠는데요",
          "original_form": "으악 맛없을 수가 없겠는데요",
          "speaker_id": "2",
          "time": "20210908 05:26"
        },
        {
          "id": "MDRW2100045684.1.13",
          "form": "저도 전에 떡볶이 국물에 밥비벼주는 거 먹어봤는데",
          "original_form": "저도 전에 떡볶이 국물에 밥비벼주는 거 먹어봤는데",
          "speaker_id": "2",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045684.1.14",
          "form": "진짜요ㅠㅠ",
          "original_form": "진짜요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045684.1.15",
          "form": "별로일 줄 알았는데 맛있더라구요",
          "original_form": "별로일 줄 알았는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045684.1.16",
          "form": "근데 거기에 치즈까지 얹었으니.. 캬",
          "original_form": "근데 거기에 치즈까지 얹었으니.. 캬",
          "speaker_id": "2",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045684.1.17",
          "form": "크으ㅠㅠㅠㅠㅠ",
          "original_form": "크으ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:28"
        },
        {
          "id": "MDRW2100045684.1.18",
          "form": "치즈는 정말 만능이에요",
          "original_form": "치즈는 정말 만능이에요",
          "speaker_id": "1",
          "time": "20210908 05:28"
        },
        {
          "id": "MDRW2100045684.1.19",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:29"
        },
        {
          "id": "MDRW2100045684.1.20",
          "form": "크림치즈는 별로 안좋아하는데",
          "original_form": "크림치즈는 별로 안좋아하는데",
          "speaker_id": "2",
          "time": "20210908 05:29"
        },
        {
          "id": "MDRW2100045684.1.21",
          "form": "피자치즈는 진짜 넘 맛있어요",
          "original_form": "피자치즈는 진짜 넘 맛있어요",
          "speaker_id": "2",
          "time": "20210908 05:29"
        },
        {
          "id": "MDRW2100045684.1.22",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045684.1.23",
          "form": "모짜렐라는 정말...",
          "original_form": "모짜렐라는 정말...",
          "speaker_id": "1",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045684.1.24",
          "form": "파파존스 치즈스틱 혹시 드셔보셨나요?",
          "original_form": "파파존스 치즈스틱 혹시 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045684.1.25",
          "form": "그것도 짱맛이에요",
          "original_form": "그것도 짱맛이에요",
          "speaker_id": "1",
          "time": "20210908 05:30"
        },
        {
          "id": "MDRW2100045684.1.26",
          "form": "헉 아니요 파파존스가 근처에 없어서 못먹어봤네요",
          "original_form": "헉 아니요 파파존스가 근처에 없어서 못먹어봤네요",
          "speaker_id": "2",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045684.1.27",
          "form": "먹어보고싶군요...",
          "original_form": "먹어보고싶군요...",
          "speaker_id": "2",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045684.1.28",
          "form": "꼭 드셔보세요! 도우에 치즈가 듬뿍 올라가있어요",
          "original_form": "꼭 드셔보세요! 도우에 치즈가 듬뿍 올라가있어요",
          "speaker_id": "1",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045684.1.29",
          "form": "으아아ㅏ ㅜ 파파존스 꼭 기억하겠습니다",
          "original_form": "으아아ㅏ ㅜ 파파존스 꼭 기억하겠습니다",
          "speaker_id": "2",
          "time": "20210908 05:36"
        },
        {
          "id": "MDRW2100045684.1.30",
          "form": "희희 후회하지 않으실거예요",
          "original_form": "희희 후회하지 않으실거예요",
          "speaker_id": "1",
