        },
        {
          "id": "MDRW2100020478.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210828 08:07"
        },
        {
          "id": "MDRW2100020478.1.3",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210828 08:07"
        },
        {
          "id": "MDRW2100020478.1.4",
          "form": "좀이따 하려구요",
          "original_form": "좀이따 하려구요",
          "speaker_id": "2",
          "time": "20210828 08:07"
        },
        {
          "id": "MDRW2100020478.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 08:07"
        },
        {
          "id": "MDRW2100020478.1.6",
          "form": "메뉴는 정하셨나요",
          "original_form": "메뉴는 정하셨나요",
          "speaker_id": "1",
          "time": "20210828 08:07"
        },
        {
          "id": "MDRW2100020478.1.7",
          "form": "전 아직이요",
          "original_form": "전 아직이요",
          "speaker_id": "1",
          "time": "20210828 08:07"
        },
        {
          "id": "MDRW2100020478.1.8",
          "form": "전 햄버거 먹을까 싶어요ㅋㅋㅋ",
          "original_form": "전 햄버거 먹을까 싶어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 08:07"
        },
        {
          "id": "MDRW2100020478.1.9",
          "form": "어제 먹은게 소화가 덜되서 아침은 거르려구요",
          "original_form": "어제 먹은게 소화가 덜되서 아침은 거르려구요",
          "speaker_id": "1",
          "time": "20210828 08:07"
        },
        {
          "id": "MDRW2100020478.1.10",
          "form": "완벽하네요",
          "original_form": "완벽하네요",
          "speaker_id": "1",
          "time": "20210828 08:07"
        },
        {
          "id": "MDRW2100020478.1.11",
          "form": "어떤버거 드시나요",
          "original_form": "어떤버거 드시나요",
          "speaker_id": "1",
          "time": "20210828 08:08"
        },
        {
          "id": "MDRW2100020478.1.12",
          "form": "아하 그럼 점심으로 맛난거 드세요",
          "original_form": "아하 그럼 점심으로 맛난거 드세요",
          "speaker_id": "2",
          "time": "20210828 08:08"
        },
        {
          "id": "MDRW2100020478.1.13",
          "form": "지금 고민중이에요 몬스터버거 먹을듯해요",
          "original_form": "지금 고민중이에요 몬스터버거 먹을듯해요",
          "speaker_id": "2",
          "time": "20210828 08:08"
        },
        {
          "id": "MDRW2100020478.1.14",
          "form": "버거킹 드시는구나",
          "original_form": "버거킹 드시는구나",
          "speaker_id": "1",
          "time": "20210828 08:09"
        },
        {
          "id": "MDRW2100020478.1.15",
          "form": "몬스터버거 너무 맛있어요",
          "original_form": "몬스터버거 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210828 08:09"
        },
        {
          "id": "MDRW2100020478.1.16",
          "form": "엇 딱 아시네요ㅋㅋㅋ",
          "original_form": "엇 딱 아시네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 08:09"
        },
        {
          "id": "MDRW2100020478.1.17",
          "form": "혹시 드실 때 토핑추가하시나요",
          "original_form": "혹시 드실 때 토핑추가하시나요",
          "speaker_id": "1",
          "time": "20210828 08:09"
        },
        {
          "id": "MDRW2100020478.1.18",
          "form": "아뇨 토핑 추가도 됐나요??",
          "original_form": "아뇨 토핑 추가도 됐나요??",
          "speaker_id": "2",
          "time": "20210828 08:09"
        },
        {
          "id": "MDRW2100020478.1.19",
          "form": "저는 치즈추가 꼭 하는편이에요 치즈가 2장이면 맛이 2배 ㅎ",
          "original_form": "저는 치즈추가 꼭 하는편이에요 치즈가 2장이면 맛이 2배 ㅎ",
          "speaker_id": "1",
          "time": "20210828 08:09"
        },
        {
          "id": "MDRW2100020478.1.20",
          "form": "첨알았어요",
          "original_form": "첨알았어요",
          "speaker_id": "2",
          "time": "20210828 08:09"
        },
        {
          "id": "MDRW2100020478.1.21",
          "form": "와 이번에 먹을 땐 그렇게 먹어봐야겠네요",
          "original_form": "와 이번에 먹을 땐 그렇게 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 08:10"
        },
        {
          "id": "MDRW2100020478.1.22",
          "form": "헉 그러셨구나 채소랑 소스가 추가금없었던걸로 기억하고 치즈가 500원인가 300원인가했던거같아요",
          "original_form": "헉 그러셨구나 채소랑 소스가 추가금없었던걸로 기억하고 치즈가 500원인가 300원인가했던거같아요",
