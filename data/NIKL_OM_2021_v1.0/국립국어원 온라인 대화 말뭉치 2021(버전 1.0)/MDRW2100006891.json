      "utterance": [
        {
          "id": "MDRW2100006891.1.1",
          "form": "요즘 다이어트 하시나요?",
          "original_form": "요즘 다이어트 하시나요?",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100006891.1.2",
          "form": "먹는걸 좋아해서 다이어트는 안해봤어요^^",
          "original_form": "먹는걸 좋아해서 다이어트는 안해봤어요^^",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006891.1.3",
          "form": "요즘 근데 해야겠더라구요",
          "original_form": "요즘 근데 해야겠더라구요",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006891.1.4",
          "form": "저는 살면서 다이어트를 해본적이 없거든요",
          "original_form": "저는 살면서 다이어트를 해본적이 없거든요",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006891.1.5",
          "form": "다이어트 쉽게 하는 방법 아시나요?",
          "original_form": "다이어트 쉽게 하는 방법 아시나요?",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006891.1.6",
          "form": "글쎄요 아무래도 운동과 식단병행하는게 젤 좋은거같아요",
          "original_form": "글쎄요 아무래도 운동과 식단병행하는게 젤 좋은거같아요",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006891.1.7",
          "form": "저도 다이어트안해봐서ㅋ",
          "original_form": "저도 다이어트안해봐서ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006891.1.8",
          "form": "그쵸 다이어트는 목표가 세워도 금방 포기하게되더라고요",
          "original_form": "그쵸 다이어트는 목표가 세워도 금방 포기하게되더라고요",
          "speaker_id": "1",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100006891.1.9",
          "form": "식단이 제일 중요한거같아요",
          "original_form": "식단이 제일 중요한거같아요",
          "speaker_id": "1",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100006891.1.10",
          "form": "어떤 운동을 좋아하시나요?",
          "original_form": "어떤 운동을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 10:07"
        },
        {
          "id": "MDRW2100006891.1.11",
          "form": "전 주로 음악들으면서 하는거 좋아해요",
          "original_form": "전 주로 음악들으면서 하는거 좋아해요",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100006891.1.12",
          "form": "줌바나 스피닝",
          "original_form": "줌바나 스피닝",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100006891.1.13",
          "form": "스피닝 들어보니까 무릎 통증이 생기는 상황이 많다고 들었어요",
          "original_form": "스피닝 들어보니까 무릎 통증이 생기는 상황이 많다고 들었어요",
          "speaker_id": "1",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100006891.1.14",
          "form": "허벅지에 자극도 가고 무릎에 통증이 심하다고 하던데요",
          "original_form": "허벅지에 자극도 가고 무릎에 통증이 심하다고 하던데요",
          "speaker_id": "1",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100006891.1.15",
          "form": "스피닝 할때  문제가 없으신가요?",
          "original_form": "스피닝 할때  문제가 없으신가요?",
          "speaker_id": "1",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100006891.1.16",
          "form": "요즘은 안해요 심하게안하면 괜찮더라구요",
          "original_form": "요즘은 안해요 심하게안하면 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100006891.1.17",
          "form": "그렇죠 건강은 항상 매순간 지켜야하고  무리하면 안되죠",
          "original_form": "그렇죠 건강은 항상 매순간 지켜야하고  무리하면 안되죠",
          "speaker_id": "1",
          "time": "20210822 11:23"
        },
        {
          "id": "MDRW2100006891.1.18",
          "form": "하시는 운동있으세요?",
          "original_form": "하시는 운동있으세요?",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100006891.1.19",
          "form": "저는 맨몸운동을 위주로 해서 푸쉬업이나 풀업을 합니다.",
          "original_form": "저는 맨몸운동을 위주로 해서 푸쉬업이나 풀업을 합니다.",
          "speaker_id": "1",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100006891.1.20",
          "form": "홈트하시나보네요",
          "original_form": "홈트하시나보네요",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100006891.1.21",
          "form": "홈트가 제일 쉽고 밖에 나가는걸 싫어하고 운동은 해야하니 어쩔 수없네요",
          "original_form": "홈트가 제일 쉽고 밖에 나가는걸 싫어하고 운동은 해야하니 어쩔 수없네요",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100006891.1.22",
          "form": "살이 없다보니 옷핏도 안맞아서 패션에 신경도 못쓰게 되네요",
          "original_form": "살이 없다보니 옷핏도 안맞아서 패션에 신경도 못쓰게 되네요",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100006891.1.23",
          "form": "요즘 코로나땜에 헬스장 가기도 겁나요",
          "original_form": "요즘 코로나땜에 헬스장 가기도 겁나요",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100006891.1.24",
          "form": "살이 없으세요? 마른체형이신가봐요",
          "original_form": "살이 없으세요? 마른체형이신가봐요",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100006891.1.25",
          "form": "그렇죠 ㅎㅎ 헬스장은 끈기로 . 가는거라서 ㅠ   대근육으로 근육을 키워야 하는데 끈기가 적네요",
          "original_form": "그렇죠 ㅎㅎ 헬스장은 끈기로 . 가는거라서 ㅠ   대근육으로 근육을 키워야 하는데 끈기가 적네요",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100006891.1.26",
          "form": "마른체형이라서 요즘에 맨몸운동부터 하고 있어요",
          "original_form": "마른체형이라서 요즘에 맨몸운동부터 하고 있어요",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100006891.1.27",
          "form": "근력운동위주로 하셔야겠네요",
          "original_form": "근력운동위주로 하셔야겠네요",
          "speaker_id": "2",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100006891.1.28",
          "form": "꾸준히 하는게 진짜 힘들더라구요",
          "original_form": "꾸준히 하는게 진짜 힘들더라구요",
