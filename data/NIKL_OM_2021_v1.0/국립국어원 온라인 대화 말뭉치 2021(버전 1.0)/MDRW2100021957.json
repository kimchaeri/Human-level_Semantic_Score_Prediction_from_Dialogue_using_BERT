      "utterance": [
        {
          "id": "MDRW2100021957.1.1",
          "form": "마음의 건강에 관심이 많아서",
          "original_form": "마음의 건강에 관심이 많아서",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021957.1.2",
          "form": "책을 많이 봤어여..ㅋㅋㅋ",
          "original_form": "책을 많이 봤어여..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021957.1.3",
          "form": "마음의 건강이 9할이요.. 신체는 1할이니라. ㅋㅋㅋ",
          "original_form": "마음의 건강이 9할이요.. 신체는 1할이니라. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021957.1.4",
          "form": "스무스하게 넘어가시네요 ㅋㅋㅋㅋㅋ",
          "original_form": "스무스하게 넘어가시네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021957.1.5",
          "form": "ㅋㅋㅋ술 땜에 어디 이상잇고 그러진 않으세요?",
          "original_form": "ㅋㅋㅋ술 땜에 어디 이상잇고 그러진 않으세요?",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021957.1.6",
          "form": "전 술을 정도이상 마시면 앓아누워요",
          "original_form": "전 술을 정도이상 마시면 앓아누워요",
          "speaker_id": "2",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021957.1.7",
          "form": "정도란게? 소주로치면 2병?",
          "original_form": "정도란게? 소주로치면 2병?",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021957.1.8",
          "form": "한병이요",
          "original_form": "한병이요",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021957.1.9",
          "form": "아.. 건강 생각하심 적게드셔야겟네요..",
          "original_form": "아.. 건강 생각하심 적게드셔야겟네요..",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021957.1.10",
          "form": "전 숙취가 아니라 정말 앓아누워요",
          "original_form": "전 숙취가 아니라 정말 앓아누워요",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021957.1.11",
          "form": "막 피부 뻘겋게 되고 그런건요?",
          "original_form": "막 피부 뻘겋게 되고 그런건요?",
          "speaker_id": "1",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021957.1.12",
          "form": "그것도 잇구요",
          "original_form": "그것도 잇구요",
          "speaker_id": "2",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021957.1.13",
          "form": "그런데 피부 뒤집어지는건 참고 마시면 되는데",
          "original_form": "그런데 피부 뒤집어지는건 참고 마시면 되는데",
          "speaker_id": "2",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021957.1.14",
          "form": "다음날에 진짜 병이나서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다음날에 진짜 병이나서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021957.1.15",
          "form": "몸살처럼 앓아요",
          "original_form": "몸살처럼 앓아요",
          "speaker_id": "2",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021957.1.16",
          "form": "그럼 왜 마시는거라요? ㅋㅋㅋㅋ",
          "original_form": "그럼 왜 마시는거라요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021957.1.17",
          "form": "미친거? ㅋㅋㅋ 라고 말하면 실례겠네요\r\n. ㅎㅎㅎ",
          "original_form": "미친거? ㅋㅋㅋ 라고 말하면 실례겠네요\r\n. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021957.1.18",
          "form": "넘기지만 않으면 괜찮거든요",
          "original_form": "넘기지만 않으면 괜찮거든요",
          "speaker_id": "2",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021957.1.19",
          "form": "그래서 적당히 마셔요",
          "original_form": "그래서 적당히 마셔요",
          "speaker_id": "2",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021957.1.20",
          "form": "실례라고 말하면서 넙죽 잘 말하시네요 ㅋㅋㅋㅋ",
          "original_form": "실례라고 말하면서 넙죽 잘 말하시네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021957.1.21",
          "form": "ㅋㅋㅋ 그러게요.. \r\n저 처럼 적당히.. 건강하게.. 잔졍없이..",
          "original_form": "ㅋㅋㅋ 그러게요.. \r\n저 처럼 적당히.. 건강하게.. 잔졍없이..",
          "speaker_id": "1",
          "time": "20210829 00:27"
        }
      ]
