      "utterance": [
        {
          "id": "MDRW2100006324.1.1",
          "form": "먹고싶지만 평일엔 일찍 자야되니까",
          "original_form": "먹고싶지만 평일엔 일찍 자야되니까",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006324.1.2",
          "form": "주말에 먹죠 ㅋㅋㅋ",
          "original_form": "주말에 먹죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006324.1.3",
          "form": "어떤 메뉴인가요 ㅎㅎ",
          "original_form": "어떤 메뉴인가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006324.1.4",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006324.1.5",
          "form": "술안주류요",
          "original_form": "술안주류요",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006324.1.6",
          "form": "족발",
          "original_form": "족발",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006324.1.7",
          "form": "맥주와 어울릴만한 피자?",
          "original_form": "맥주와 어울릴만한 피자?",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006324.1.8",
          "form": "아 술과 함께 드시는군요",
          "original_form": "아 술과 함께 드시는군요",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100006324.1.9",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006324.1.10",
          "form": "한캔정도",
          "original_form": "한캔정도",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006324.1.11",
          "form": "적당하네요 ㅎ",
          "original_form": "적당하네요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006324.1.12",
          "form": "술을 잘 못마셔요 제가ㅜㅜ",
          "original_form": "술을 잘 못마셔요 제가ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006324.1.13",
          "form": "과한건 좋지 않으니까요 ㅎㅎ",
          "original_form": "과한건 좋지 않으니까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006324.1.14",
          "form": "체질상 못마셔서",
          "original_form": "체질상 못마셔서",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006324.1.15",
          "form": "어쩔수 없이",
          "original_form": "어쩔수 없이",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006324.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006324.1.17",
          "form": "커피는 드시나요",
          "original_form": "커피는 드시나요",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006324.1.18",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006324.1.19",
          "form": "커피는 3잔은 마셔요",
          "original_form": "커피는 3잔은 마셔요",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006324.1.20",
          "form": "하루에",
          "original_form": "하루에",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006324.1.21",
          "form": "많이 드시네요..",
          "original_form": "많이 드시네요..",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006324.1.22",
          "form": "많은건가요",
          "original_form": "많은건가요",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006324.1.23",
          "form": "아침 점심 저녁 세쟈",
          "original_form": "아침 점심 저녁 세쟈",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006324.1.24",
          "form": "잔",
          "original_form": "잔",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006324.1.25",
          "form": "네 저는 안먹어서 ㅎㅎ",
          "original_form": "네 저는 안먹어서 ㅎㅎ",
          "speaker_id": "1",
