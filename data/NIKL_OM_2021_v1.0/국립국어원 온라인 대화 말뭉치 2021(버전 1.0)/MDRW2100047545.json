      "utterance": [
        {
          "id": "MDRW2100047545.1.1",
          "form": "지코 음료수도 드세요?",
          "original_form": "지코 음료수도 드세요?",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100047545.1.2",
          "form": "하 제가 음료엔 보수적이라서",
          "original_form": "하 제가 음료엔 보수적이라서",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100047545.1.3",
          "form": "그거 하수구맛 나던데요",
          "original_form": "그거 하수구맛 나던데요",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100047545.1.4",
          "form": "아아 vs 뜨아",
          "original_form": "아아 vs 뜨아",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100047545.1.5",
          "form": "전 아메리카노 못마셔서ㅎㅂㅎ....",
          "original_form": "전 아메리카노 못마셔서ㅎㅂㅎ....",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047545.1.6",
          "form": "아아파이신가요?",
          "original_form": "아아파이신가요?",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047545.1.7",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047545.1.8",
          "form": "네 저 얼죽아",
          "original_form": "네 저 얼죽아",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047545.1.9",
          "form": "아메리카노 못마시면",
          "original_form": "아메리카노 못마시면",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047545.1.10",
          "form": "어떤 음료 보통 사줄때 좋으신가요?",
          "original_form": "어떤 음료 보통 사줄때 좋으신가요?",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047545.1.11",
          "form": "복숭아티 이러거 괜찮나요?",
          "original_form": "복숭아티 이러거 괜찮나요?",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047545.1.12",
          "form": "아아로 통일할 때 좀 힘들다해서..",
          "original_form": "아아로 통일할 때 좀 힘들다해서..",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047545.1.13",
          "form": "전 커피 자체는 마실 수 있어서 라떼까진 괜찮은거같아요!!",
          "original_form": "전 커피 자체는 마실 수 있어서 라떼까진 괜찮은거같아요!!",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100047545.1.14",
          "form": "데자와도 드시나요?",
          "original_form": "데자와도 드시나요?",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100047545.1.15",
          "form": "어 네",
          "original_form": "어 네",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100047545.1.16",
          "form": "데자와는 맛있지 않나요??",
          "original_form": "데자와는 맛있지 않나요??",
          "speaker_id": "2",
          "time": "20210826 19:53"
        },
