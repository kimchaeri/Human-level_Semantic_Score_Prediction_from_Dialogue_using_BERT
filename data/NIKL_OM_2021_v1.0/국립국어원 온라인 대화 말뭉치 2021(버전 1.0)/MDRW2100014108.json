      "utterance": [
        {
          "id": "MDRW2100014108.1.1",
          "form": "설마 농담",
          "original_form": "설마 농담",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014108.1.2",
          "form": "ㅋㅋ 그건 아니고 섬 비슷무리",
          "original_form": "ㅋㅋ 그건 아니고 섬 비슷무리",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014108.1.3",
          "form": "내륙지방인데 섬이라 해서 놀랬다",
          "original_form": "내륙지방인데 섬이라 해서 놀랬다",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014108.1.4",
          "form": "ㅋㅋ응 금호강이 흐르잖아",
          "original_form": "ㅋㅋ응 금호강이 흐르잖아",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014108.1.5",
          "form": "어 금호강 있지",
          "original_form": "어 금호강 있지",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014108.1.6",
          "form": "그 금호강이 느리게 흘려가면서 흙이 쌍형서 한가운데 섬이 생겼데",
          "original_form": "그 금호강이 느리게 흘려가면서 흙이 쌍형서 한가운데 섬이 생겼데",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014108.1.7",
          "form": "아~하",
          "original_form": "아~하",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014108.1.8",
          "form": "응 그섬이름을 하중도라고 하는데 정말 좋다고 하더라",
          "original_form": "응 그섬이름을 하중도라고 하는데 정말 좋다고 하더라",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014108.1.9",
          "form": "하중도 청 들어봄",
          "original_form": "하중도 청 들어봄",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014108.1.10",
          "form": "여기 하중도에 여러가지 식물을 심어서 대구 핫 플레이스래",
          "original_form": "여기 하중도에 여러가지 식물을 심어서 대구 핫 플레이스래",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014108.1.11",
          "form": "뭐가 있는데",
          "original_form": "뭐가 있는데",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014108.1.12",
          "form": "봄에는 유채꽃",
          "original_form": "봄에는 유채꽃",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014108.1.13",
          "form": "유채꽃 이쁘지",
          "original_form": "유채꽃 이쁘지",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014108.1.14",
          "form": "가을에는 코스모스",
          "original_form": "가을에는 코스모스",
          "speaker_id": "1",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014108.1.15",
          "form": "여름하면 계곡이잖아",
          "original_form": "여름하면 계곡이잖아",
          "speaker_id": "1",
          "time": "20210825 17:23"
        }
