      "utterance": [
        {
          "id": "MDRW2100034988.1.1",
          "form": "혹시 몇분이서 사세요??",
          "original_form": "혹시 몇분이서 사세요??",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100034988.1.2",
          "form": "저는 엄마랑 살아용",
          "original_form": "저는 엄마랑 살아용",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100034988.1.3",
          "form": "ㅋㅋ 저는 쉐어하우스라서",
          "original_form": "ㅋㅋ 저는 쉐어하우스라서",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034988.1.4",
          "form": "친구들하고살아요",
          "original_form": "친구들하고살아요",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034988.1.5",
          "form": "헐! 우와",
          "original_form": "헐! 우와",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034988.1.6",
          "form": "부모님과사는거 좋네요",
          "original_form": "부모님과사는거 좋네요",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034988.1.7",
          "form": "그럼 관리비나 기타 수도세 이런거 각자 나누세요?",
          "original_form": "그럼 관리비나 기타 수도세 이런거 각자 나누세요?",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034988.1.8",
          "form": "한꺼번에 내요",
          "original_form": "한꺼번에 내요",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034988.1.9",
          "form": "각자내는비용이 있어서리 ㅎㅎ",
          "original_form": "각자내는비용이 있어서리 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034988.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100034988.1.11",
          "form": "친구분들이랑 같이 사실정도면",
          "original_form": "친구분들이랑 같이 사실정도면",
          "speaker_id": "1",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100034988.1.12",
          "form": "사이 엄청 좋으시겠다",
          "original_form": "사이 엄청 좋으시겠다",
          "speaker_id": "1",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100034988.1.13",
          "form": "음 들어와서 사귄분도 잏구요",
          "original_form": "음 들어와서 사귄분도 잏구요",
          "speaker_id": "2",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100034988.1.14",
          "form": "알던친구도 있어요",
          "original_form": "알던친구도 있어요",
          "speaker_id": "2",
          "time": "20210828 23:25"
        },
        {
          "id": "MDRW2100034988.1.15",
          "form": "오오 저도 마음에 맞는 친구랑 쉐어하우스로 살아보고싶어요",
          "original_form": "오오 저도 마음에 맞는 친구랑 쉐어하우스로 살아보고싶어요",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100034988.1.16",
          "form": "사이좋게 지내면 모두 편하니까 친하게지내요",
          "original_form": "사이좋게 지내면 모두 편하니까 친하게지내요",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100034988.1.17",
          "form": "이런것도 결혼하면 다 불가한 일이니까ㅠㅠ",
          "original_form": "이런것도 결혼하면 다 불가한 일이니까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100034988.1.18",
          "form": "오 좋은 경험인거같아요",
          "original_form": "오 좋은 경험인거같아요",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100034988.1.19",
          "form": "네네ㅋㅋㅋ 집안일도 나눠서 하세요?",
          "original_form": "네네ㅋㅋㅋ 집안일도 나눠서 하세요?",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100034988.1.20",
          "form": "음식이나 청소나",
          "original_form": "음식이나 청소나",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100034988.1.21",
          "form": "부모님과 사는거랑 많이 다른거같아요",
          "original_form": "부모님과 사는거랑 많이 다른거같아요",
          "speaker_id": "2",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100034988.1.22",
          "form": "네 청소구역 나눠서 일해요",
          "original_form": "네 청소구역 나눠서 일해요",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100034988.1.23",
          "form": "냉장고 따로에요 ㅎㅎ",
          "original_form": "냉장고 따로에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100034988.1.24",
          "form": "헉 대박",
          "original_form": "헉 대박",
          "speaker_id": "1",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100034988.1.25",
          "form": "냉장고 분리되면 싸울일도 없고",
          "original_form": "냉장고 분리되면 싸울일도 없고",
          "speaker_id": "1",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100034988.1.26",
          "form": "부모님이랑 사는것도 좋지만",
          "original_form": "부모님이랑 사는것도 좋지만",
          "speaker_id": "1",
          "time": "20210828 23:27"
        },
        {
          "id": "MDRW2100034988.1.27",
          "form": "저도 독립하고싶어서ㅋㅋㅋ",
          "original_form": "저도 독립하고싶어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:27"
        }
      ]
    }
  ]
