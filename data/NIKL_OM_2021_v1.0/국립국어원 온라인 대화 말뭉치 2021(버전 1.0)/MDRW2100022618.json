        {
          "id": "MDRW2100022618.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210829 16:25"
        },
        {
          "id": "MDRW2100022618.1.2",
          "form": "아직 안 했어요!ㅠㅠ",
          "original_form": "아직 안 했어요!ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022618.1.3",
          "form": "식사 하셨나요?",
          "original_form": "식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022618.1.4",
          "form": "칼국수요",
          "original_form": "칼국수요",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022618.1.5",
          "form": "오 저도 칼국수 좋아하는데ㅠㅠ",
          "original_form": "오 저도 칼국수 좋아하는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022618.1.6",
          "form": "맛있었나요?",
          "original_form": "맛있었나요?",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022618.1.7",
          "form": "바지락칼국수좋아요",
          "original_form": "바지락칼국수좋아요",
          "speaker_id": "1",
          "time": "20210901 07:29"
        },
        {
          "id": "MDRW2100022618.1.8",
          "form": "특별히 좋아하는 음식있으신가요?",
          "original_form": "특별히 좋아하는 음식있으신가요?",
          "speaker_id": "2",
          "time": "20210901 15:40"
        },
        {
          "id": "MDRW2100022618.1.9",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100022618.1.10",
          "form": "역시 치킨이 진리죠ㅋㅋㅋㅋ",
          "original_form": "역시 치킨이 진리죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100022618.1.11",
          "form": "어떤 치킨 좋아하세요?",
          "original_form": "어떤 치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100022618.1.12",
          "form": "네네요",
          "original_form": "네네요",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100022618.1.13",
          "form": "전 네네에서 스노윙 제일 좋아해요!",
          "original_form": "전 네네에서 스노윙 제일 좋아해요!",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100022618.1.14",
          "form": "맛나나여?",
          "original_form": "맛나나여?",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100022618.1.15",
          "form": "네네! 짭짤하고 맛있어요",
          "original_form": "네네! 짭짤하고 맛있어요",
          "speaker_id": "2",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100022618.1.16",
          "form": "먹어봐야겠네요",
          "original_form": "먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210901 18:10"
        }
