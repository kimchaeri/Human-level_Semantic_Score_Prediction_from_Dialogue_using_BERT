      "utterance": [
        {
          "id": "MDRW2100040069.1.1",
          "form": "얼렸다 불판에 올렸다를",
          "original_form": "얼렸다 불판에 올렸다를",
          "speaker_id": "2",
          "time": "20210904 02:52"
        },
        {
          "id": "MDRW2100040069.1.2",
          "form": "반복해줍니다",
          "original_form": "반복해줍니다",
          "speaker_id": "2",
          "time": "20210904 02:52"
        },
        {
          "id": "MDRW2100040069.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:52"
        },
        {
          "id": "MDRW2100040069.1.4",
          "form": "암튼 낼 점심때쯤",
          "original_form": "암튼 낼 점심때쯤",
          "speaker_id": "1",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.5",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "2",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.6",
          "form": "다시 스토리 밀면 연락줘",
          "original_form": "다시 스토리 밀면 연락줘",
          "speaker_id": "1",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.7",
          "form": "그건 자신 업서",
          "original_form": "그건 자신 업서",
          "speaker_id": "2",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.8",
          "form": "오후 언젠가.....",
          "original_form": "오후 언젠가.....",
          "speaker_id": "2",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.9",
          "form": "난 여기서",
          "original_form": "난 여기서",
          "speaker_id": "2",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.11",
          "form": "설거지도 해야하거든",
          "original_form": "설거지도 해야하거든",
          "speaker_id": "2",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.12",
          "form": "잘 자",
          "original_form": "잘 자",
          "speaker_id": "2",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.13",
          "form": "저런....",
          "original_form": "저런....",
          "speaker_id": "1",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.14",
          "form": "ㅇㅇ 일찍 자...",
          "original_form": "ㅇㅇ 일찍 자...",
          "speaker_id": "1",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.15",
          "form": "사실 이미",
          "original_form": "사실 이미",
          "speaker_id": "2",
          "time": "20210904 02:53"
        },
        {
          "id": "MDRW2100040069.1.16",
          "form": "일찍은 그른것같지만",
          "original_form": "일찍은 그른것같지만",
          "speaker_id": "2",
          "time": "20210904 02:54"
        },
        {
          "id": "MDRW2100040069.1.17",
          "form": "와 타탈던전 자주 깼구나...",
          "original_form": "와 타탈던전 자주 깼구나...",
          "speaker_id": "1",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100040069.1.18",
          "form": "나 요즘 주간보스 귀찮아서 안잡은지 한참됨...",
          "original_form": "나 요즘 주간보스 귀찮아서 안잡은지 한참됨...",
          "speaker_id": "1",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100040069.1.19",
          "form": "아냐 나도",
          "original_form": "아냐 나도",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100040069.1.20",
          "form": "저번주에 60렙 찍먹해봤는데",
          "original_form": "저번주에 60렙 찍먹해봤는데",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100040069.1.21",
          "form": "타탈이",
          "original_form": "타탈이",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100040069.1.22",
          "form": "줄줄 녹아서",
          "original_form": "줄줄 녹아서",
          "speaker_id": "2",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100040069.1.23",
          "form": "70렙 해봤더니",
          "original_form": "70렙 해봤더니",
          "speaker_id": "2",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100040069.1.24",
          "form": "되더라구",
          "original_form": "되더라구",
          "speaker_id": "2",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100040069.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100040069.1.26",
          "form": "80렙이 최고인가?",
          "original_form": "80렙이 최고인가?",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100040069.1.27",
          "form": "90렙이 최고일걸",
          "original_form": "90렙이 최고일걸",
          "speaker_id": "2",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100040069.1.28",
          "form": "ㅇㅎ...",
          "original_form": "ㅇㅎ...",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100040069.1.29",
          "form": "지난번에 50렙 찍고 애들 8~90렙 됐길래",
          "original_form": "지난번에 50렙 찍고 애들 8~90렙 됐길래",
          "speaker_id": "1",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100040069.1.30",
          "form": "신나서 80렙 갔다가 실패한 뒤로",
          "original_form": "신나서 80렙 갔다가 실패한 뒤로",
          "speaker_id": "1",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100040069.1.31",
          "form": "안감....",
          "original_form": "안감....",
          "speaker_id": "1",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100040069.1.32",
          "form": "거기서부터는 ㄹㅇ 컨트롤과 성유물 차이일듯.....",
          "original_form": "거기서부터는 ㄹㅇ 컨트롤과 성유물 차이일듯.....",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100040069.1.33",
          "form": "물속성 안맞아....",
          "original_form": "물속성 안맞아....",
          "speaker_id": "1",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100040069.1.34",
          "form": "그리고 조합",
          "original_form": "그리고 조합",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100040069.1.35",
          "form": "난 라이덴 다 키우면",
          "original_form": "난 라이덴 다 키우면",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100040069.1.36",
          "form": "타탈하고 면대면 1대1  맞딜",
          "original_form": "타탈하고 면대면 1대1  맞딜",
          "speaker_id": "2",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100040069.1.37",
          "form": "가능할것같아서",
          "original_form": "가능할것같아서",
          "speaker_id": "2",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100040069.1.38",
          "form": "좀 설레",
          "original_form": "좀 설레",
          "speaker_id": "2",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100040069.1.39",
          "form": "이속 왤케 빠른거야...",
          "original_form": "이속 왤케 빠른거야...",
          "speaker_id": "1",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100040069.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
