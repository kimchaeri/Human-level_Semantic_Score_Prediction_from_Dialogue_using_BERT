          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046137.1.12",
          "form": "나도 옷사고싶다",
          "original_form": "나도 옷사고싶다",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046137.1.13",
          "form": "요즘 안 산지 너무 오래되서",
          "original_form": "요즘 안 산지 너무 오래되서",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046137.1.14",
          "form": "계절 넘 애매해서 반팔 사기도 글코 긴팔 사기도 글코",
          "original_form": "계절 넘 애매해서 반팔 사기도 글코 긴팔 사기도 글코",
          "speaker_id": "1",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046137.1.15",
          "form": "애매하지",
          "original_form": "애매하지",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046137.1.16",
          "form": "간절기가 날씨는좋은에",
          "original_form": "간절기가 날씨는좋은에",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046137.1.17",
          "form": "데",
          "original_form": "데",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046137.1.18",
          "form": "넌 무슨 옷 사고 싶어?",
          "original_form": "넌 무슨 옷 사고 싶어?",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046137.1.19",
          "form": "그냥 새옷??",
          "original_form": "그냥 새옷??",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046137.1.20",
          "form": "내 옷의 반이 동생옷이야",
          "original_form": "내 옷의 반이 동생옷이야",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046137.1.21",
          "form": "동생이 안 입는거...",
          "original_form": "동생이 안 입는거...",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046137.1.22",
          "form": "그래도 스타일이란게 있자나",
          "original_form": "그래도 스타일이란게 있자나",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046137.1.23",
          "form": "난 캐주얼한게 좋아",
