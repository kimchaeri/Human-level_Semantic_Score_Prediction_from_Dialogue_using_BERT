          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046137.1.5",
          "form": "중학교 동창",
          "original_form": "중학교 동창",
          "speaker_id": "1",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046137.1.6",
          "form": "세월이 빠르네",
          "original_form": "세월이 빠르네",
          "speaker_id": "2",
          "time": "20210908 10:42"
        },
        {
          "id": "MDRW2100046137.1.7",
          "form": "어떤거 사려고",
          "original_form": "어떤거 사려고",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046137.1.8",
          "form": "생각해둔거 있어",
          "original_form": "생각해둔거 있어",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046137.1.9",
          "form": "걍 원피스 사거 싶다...",
          "original_form": "걍 원피스 사거 싶다...",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046137.1.10",
          "form": "끗",
          "original_form": "끗",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046137.1.11",
