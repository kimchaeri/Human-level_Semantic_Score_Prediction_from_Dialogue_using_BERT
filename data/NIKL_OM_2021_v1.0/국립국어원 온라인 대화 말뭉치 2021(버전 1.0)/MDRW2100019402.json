      "utterance": [
        {
          "id": "MDRW2100019402.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 18:49"
        },
        {
          "id": "MDRW2100019402.1.2",
          "form": "좋아하는 책 있으세요?",
          "original_form": "좋아하는 책 있으세요?",
          "speaker_id": "1",
          "time": "20210827 18:49"
        },
        {
          "id": "MDRW2100019402.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 18:49"
        },
        {
          "id": "MDRW2100019402.1.4",
          "form": "담 쌓고 살아서요 저는 ㅎㅎ.. 좋아하세요?",
          "original_form": "담 쌓고 살아서요 저는 ㅎㅎ.. 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 18:49"
        },
        {
          "id": "MDRW2100019402.1.5",
          "form": "저도 그랬는데",
          "original_form": "저도 그랬는데",
          "speaker_id": "1",
          "time": "20210827 18:50"
        },
        {
          "id": "MDRW2100019402.1.6",
          "form": "최근에한권 읽었어요ㅋㅋ!",
          "original_form": "최근에한권 읽었어요ㅋㅋ!",
          "speaker_id": "1",
          "time": "20210827 18:50"
        },
        {
          "id": "MDRW2100019402.1.7",
          "form": "오뭐요??",
          "original_form": "오뭐요??",
          "speaker_id": "2",
          "time": "20210827 18:50"
        },
        {
          "id": "MDRW2100019402.1.8",
          "form": "추리소설 좋아하시면 데드맨 이라고",
          "original_form": "추리소설 좋아하시면 데드맨 이라고",
          "speaker_id": "1",
          "time": "20210827 18:50"
        },
        {
          "id": "MDRW2100019402.1.9",
          "form": "일본소설인데 추천드려요!",
          "original_form": "일본소설인데 추천드려요!",
          "speaker_id": "1",
          "time": "20210827 18:50"
        },
        {
          "id": "MDRW2100019402.1.10",
          "form": "오 재밌을거같아여 추리",
          "original_form": "오 재밌을거같아여 추리",
          "speaker_id": "2",
          "time": "20210827 18:50"
        },
        {
          "id": "MDRW2100019402.1.11",
          "form": "긴편인가요?",
          "original_form": "긴편인가요?",
          "speaker_id": "2",
          "time": "20210827 18:50"
        },
        {
          "id": "MDRW2100019402.1.12",
          "form": "아뇨 ~~!!",
          "original_form": "아뇨 ~~!!",
          "speaker_id": "1",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019402.1.13",
          "form": "3시간정도면 다읽습니다!",
          "original_form": "3시간정도면 다읽습니다!",
          "speaker_id": "1",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019402.1.14",
          "form": "짧나보네요 ㅋㅋㅋㅋ",
          "original_form": "짧나보네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019402.1.15",
          "form": "참고할게요 ㅋㅋㅋ추천 감사해요",
          "original_form": "참고할게요 ㅋㅋㅋ추천 감사해요",
          "speaker_id": "2",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019402.1.16",
          "form": "네ㅎㅎ!!",
          "original_form": "네ㅎㅎ!!",
          "speaker_id": "1",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019402.1.17",
          "form": "그럼 좋아하는 장르는 잇으세요?",
          "original_form": "그럼 좋아하는 장르는 잇으세요?",
          "speaker_id": "1",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019402.1.18",
          "form": "아님 잡지두!",
          "original_form": "아님 잡지두!",
          "speaker_id": "1",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019402.1.19",
          "form": "흠 글쎄요 읽은지가 너무 오래돼서...ㅋㅋㅋㅋㅋ",
          "original_form": "흠 글쎄요 읽은지가 너무 오래돼서...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:52"
        },
        {
          "id": "MDRW2100019402.1.20",
          "form": "소설이면 장르 안가리고 일단 다 괜찮을거같아요",
          "original_form": "소설이면 장르 안가리고 일단 다 괜찮을거같아요",
          "speaker_id": "2",
          "time": "20210827 18:52"
        },
        {
          "id": "MDRW2100019402.1.21",
          "form": "그럼 보건교사 안은영은 어떠세요?",
          "original_form": "그럼 보건교사 안은영은 어떠세요?",
          "speaker_id": "1",
          "time": "20210827 18:52"
        },
        {
          "id": "MDRW2100019402.1.22",
          "form": "헉 그게 원래 책이 원조인가요?",
          "original_form": "헉 그게 원래 책이 원조인가요?",
          "speaker_id": "2",
          "time": "20210827 18:52"
        },
        {
          "id": "MDRW2100019402.1.23",
          "form": "드라마도 재밌는데 전 원작도 좋더라구요!",
          "original_form": "드라마도 재밌는데 전 원작도 좋더라구요!",
          "speaker_id": "1",
          "time": "20210827 18:52"
        },
        {
          "id": "MDRW2100019402.1.24",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210827 18:52"
        },
        {
          "id": "MDRW2100019402.1.25",
          "form": "헉 그렇군요 몰랐어요!",
          "original_form": "헉 그렇군요 몰랐어요!",
          "speaker_id": "2",
          "time": "20210827 18:53"
        },
        {
          "id": "MDRW2100019402.1.26",
          "form": "이것도 진짜 금방읽었어요ㅎㅎ",
          "original_form": "이것도 진짜 금방읽었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:53"
        },
        {
          "id": "MDRW2100019402.1.27",
          "form": "재밌을거같아요 찾아볼게요 ㅋㅋㅋ",
          "original_form": "재밌을거같아요 찾아볼게요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:53"
        }
      ]
    }
