      "utterance": [
        {
          "id": "MDRW2100009085.1.1",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 21:04"
        },
        {
          "id": "MDRW2100009085.1.2",
          "form": "채소나 회? 제외하고 다 좋아하는 거 같아요",
          "original_form": "채소나 회? 제외하고 다 좋아하는 거 같아요",
          "speaker_id": "2",
          "time": "20210822 21:09"
        },
        {
          "id": "MDRW2100009085.1.3",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 21:09"
        },
        {
          "id": "MDRW2100009085.1.4",
          "form": "저는 빵이랑 과자 같은 군것질거리 좋아해요ㅋㅋ 큰일이에요",
          "original_form": "저는 빵이랑 과자 같은 군것질거리 좋아해요ㅋㅋ 큰일이에요",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009085.1.5",
          "form": "빵이랑 과자 어떤 종류 좋아하세요?",
          "original_form": "빵이랑 과자 어떤 종류 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009085.1.6",
          "form": "전 매운 맛 말고는 다 좋아해요\r\n맛 없는게 없어요",
          "original_form": "전 매운 맛 말고는 다 좋아해요\r\n맛 없는게 없어요",
          "speaker_id": "1",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009085.1.7",
          "form": "맛 없는 게 없다는 부뷰",
          "original_form": "맛 없는 게 없다는 부뷰",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009085.1.8",
          "form": "분",
          "original_form": "분",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009085.1.9",
          "form": "너무 공감되네요",
          "original_form": "너무 공감되네요",
          "speaker_id": "2",
          "time": "20210822 23:57"
        },
        {
          "id": "MDRW2100009085.1.10",
          "form": "세상에 맛있는 게 너무 많아요ㅠㅜ",
          "original_form": "세상에 맛있는 게 너무 많아요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100009085.1.11",
          "form": "요즘엔 로제 떡볶이에 빠져있어요ㅜㅜ",
          "original_form": "요즘엔 로제 떡볶이에 빠져있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100009085.1.12",
          "form": "로제 떡볶이 맛있죠ㅠㅜ",
          "original_form": "로제 떡볶이 맛있죠ㅠㅜ",
          "speaker_id": "2",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100009085.1.13",
          "form": "배떡 유명할 때 먹어봤는데",
          "original_form": "배떡 유명할 때 먹어봤는데",
          "speaker_id": "2",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100009085.1.14",
          "form": "역시 떡볶이는 배신 안 하더라구요ㅠㅜ",
          "original_form": "역시 떡볶이는 배신 안 하더라구요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100009085.1.15",
          "form": "맞아요!! 최근엔 튀김 떡볶이 먹어봤는데 맛있었어요!",
          "original_form": "맞아요!! 최근엔 튀김 떡볶이 먹어봤는데 맛있었어요!",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100009085.1.16",
          "form": "다음에 한번 먹어봐야겠네요!",
          "original_form": "다음에 한번 먹어봐야겠네요!",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100009085.1.17",
          "form": "좋아요ㅎㅎ 저녁식사 하셨나요?",
          "original_form": "좋아요ㅎㅎ 저녁식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100009085.1.18",
          "form": "네 했어요!",
          "original_form": "네 했어요!",
          "speaker_id": "2",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100009085.1.19",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100009085.1.20",
          "form": "네! 저도 했어요!",
          "original_form": "네! 저도 했어요!",
