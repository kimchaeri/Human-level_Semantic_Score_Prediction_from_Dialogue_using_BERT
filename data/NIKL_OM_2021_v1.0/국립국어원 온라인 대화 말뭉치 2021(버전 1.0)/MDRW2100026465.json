      "utterance": [
        {
          "id": "MDRW2100026465.1.1",
          "form": "안녕하세요 ~\n비가많이옵니다",
          "original_form": "안녕하세요 ~\n비가많이옵니다",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026465.1.2",
          "form": "계신곳은 어떠신가요~~",
          "original_form": "계신곳은 어떠신가요~~",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026465.1.3",
          "form": "오늘은비 안왔어요\n어제 엄청왓네요",
          "original_form": "오늘은비 안왔어요\n어제 엄청왓네요",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026465.1.4",
          "form": "아하 여긴 방금 갑자기 쏟아집니다~",
          "original_form": "아하 여긴 방금 갑자기 쏟아집니다~",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026465.1.5",
          "form": "습해서 에어컨켰어요~~",
          "original_form": "습해서 에어컨켰어요~~",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026465.1.6",
          "form": "전 어제 그랬었져ㅋ",
          "original_form": "전 어제 그랬었져ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026465.1.7",
          "form": "낮에는 오락가락하더니만",
          "original_form": "낮에는 오락가락하더니만",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026465.1.8",
          "form": "결국 지금시작됩니다~",
          "original_form": "결국 지금시작됩니다~",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026465.1.9",
          "form": "선선한 날씨가",
          "original_form": "선선한 날씨가",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026465.1.10",
          "form": "오래가주면 좋겠어요^^",
          "original_form": "오래가주면 좋겠어요^^",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026465.1.11",
          "form": "비올땐 추울정도에요",
          "original_form": "비올땐 추울정도에요",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026465.1.12",
          "form": "그러셔요?\n전 비가오면 창문을 다닫으니",
          "original_form": "그러셔요?\n전 비가오면 창문을 다닫으니",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026465.1.13",
          "form": "오히려 습하고덥구요^^",
          "original_form": "오히려 습하고덥구요^^",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026465.1.14",
          "form": "내일은 날이 좋앗으면 하네요",
          "original_form": "내일은 날이 좋앗으면 하네요",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026465.1.15",
          "form": "밤새 에어컨을 돌려야할까봐요ㅜ",
          "original_form": "밤새 에어컨을 돌려야할까봐요ㅜ",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026465.1.16",
          "form": "저두요 내일날씨 좋았으면..",
          "original_form": "저두요 내일날씨 좋았으면..",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026465.1.17",
          "form": "날씨가 이래서 옷이 애매하네요",
          "original_form": "날씨가 이래서 옷이 애매하네요",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026465.1.18",
          "form": "전 아직 반팔입니다 ㅋ",
          "original_form": "전 아직 반팔입니다 ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026465.1.19",
          "form": "열이 많아서요^^",
          "original_form": "열이 많아서요^^",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026465.1.20",
          "form": "가디건이 필수에요",
          "original_form": "가디건이 필수에요",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026465.1.21",
          "form": "오~~추위많이 타시나봐요^^",
          "original_form": "오~~추위많이 타시나봐요^^",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026465.1.22",
          "form": "회사도 에어컨 하루종이 틀면 추워요",
          "original_form": "회사도 에어컨 하루종이 틀면 추워요",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026465.1.23",
          "form": "ㅋㅋ 그렇죠 맞죠 이런날씨엔 계속돌아가면 춥죠^^",
          "original_form": "ㅋㅋ 그렇죠 맞죠 이런날씨엔 계속돌아가면 춥죠^^",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026465.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026465.1.25",
          "form": "ㅋㅋ 그럴땐 꼭 따뜻한차를 드셔요",
          "original_form": "ㅋㅋ 그럴땐 꼭 따뜻한차를 드셔요",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026465.1.26",
          "form": "그래야겟네요",
          "original_form": "그래야겟네요",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026465.1.27",
          "form": "전 지금 후두염으로 고생중이예요 ㅋ",
          "original_form": "전 지금 후두염으로 고생중이예요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
