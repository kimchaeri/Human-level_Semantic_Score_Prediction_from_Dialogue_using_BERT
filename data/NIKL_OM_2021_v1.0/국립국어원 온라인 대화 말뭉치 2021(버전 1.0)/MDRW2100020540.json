      "utterance": [
        {
          "id": "MDRW2100020540.1.1",
          "form": "좋아하는 배우 있으세요?",
          "original_form": "좋아하는 배우 있으세요?",
          "speaker_id": "2",
          "time": "20210828 09:29"
        },
        {
          "id": "MDRW2100020540.1.2",
          "form": "송중기요!",
          "original_form": "송중기요!",
          "speaker_id": "1",
          "time": "20210828 09:29"
        },
        {
          "id": "MDRW2100020540.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 09:29"
        },
        {
          "id": "MDRW2100020540.1.4",
          "form": "저는 김남길이요!",
          "original_form": "저는 김남길이요!",
          "speaker_id": "2",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020540.1.5",
          "form": "송중기 전에 국제 영화제에서 늑대소년할때 봤었는데 너무 잘생겼더라구요 ㅠㅠ",
          "original_form": "송중기 전에 국제 영화제에서 늑대소년할때 봤었는데 너무 잘생겼더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020540.1.6",
          "form": "크으 김남길 멋지죠",
          "original_form": "크으 김남길 멋지죠",
          "speaker_id": "1",
          "time": "20210828 09:31"
        },
        {
          "id": "MDRW2100020540.1.7",
          "form": "저도 시사회때 한번 봤는데 피부가 빛나더라구요",
          "original_form": "저도 시사회때 한번 봤는데 피부가 빛나더라구요",
          "speaker_id": "1",
          "time": "20210828 09:31"
        },
        {
          "id": "MDRW2100020540.1.8",
          "form": "앜 김남길을 실제로 못봤네요 직접 보고싶은 배우에요..",
          "original_form": "앜 김남길을 실제로 못봤네요 직접 보고싶은 배우에요..",
          "speaker_id": "2",
          "time": "20210828 09:32"
        },
        {
          "id": "MDRW2100020540.1.9",
          "form": "영화랑 드라마 엄청 열일해서 필모따라가기도 좋고 ㅎㅎ",
          "original_form": "영화랑 드라마 엄청 열일해서 필모따라가기도 좋고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100020540.1.10",
          "form": "크으 맞아요",
          "original_form": "크으 맞아요",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020540.1.11",
          "form": "연기도 뒷받침 해주니 볼 때 즐거워요",
          "original_form": "연기도 뒷받침 해주니 볼 때 즐거워요",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020540.1.12",
          "form": "맞아요 일단 본업 잘하는 사람들 너무 좋아요",
          "original_form": "맞아요 일단 본업 잘하는 사람들 너무 좋아요",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020540.1.13",
          "form": "맞아욧! 최고죠",
          "original_form": "맞아욧! 최고죠",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020540.1.14",
          "form": "이번에 전도연이랑 김남길이랑 임시완이랑 송강호? 같이 나오는 영화 기다리고 있어요",
          "original_form": "이번에 전도연이랑 김남길이랑 임시완이랑 송강호? 같이 나오는 영화 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020540.1.15",
          "form": "오? 그런게 있었군요",
          "original_form": "오? 그런게 있었군요",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020540.1.16",
          "form": "제목이 뭐에요?",
          "original_form": "제목이 뭐에요?",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020540.1.17",
          "form": "비상선언이요! 근데 개봉이 계속 밀려서 언제 개봉하나 싶어요",
          "original_form": "비상선언이요! 근데 개봉이 계속 밀려서 언제 개봉하나 싶어요",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020540.1.18",
          "form": "크윽 코로나가 발목을 잡고 있는거군요ㅠ",
          "original_form": "크윽 코로나가 발목을 잡고 있는거군요ㅠ",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020540.1.19",
          "form": "네 배우진이 넘 탄탄해서 제작할때부터 기대했는데ㅠ",
          "original_form": "네 배우진이 넘 탄탄해서 제작할때부터 기대했는데ㅠ",
          "speaker_id": "2",
          "time": "20210828 11:00"
        },
        {
          "id": "MDRW2100020540.1.20",
          "form": "진짜 탄탄하네요",
          "original_form": "진짜 탄탄하네요",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020540.1.21",
          "form": "아쉬우시겠어요ㅠ",
          "original_form": "아쉬우시겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210828 11:01"
        }
      ]
    }
