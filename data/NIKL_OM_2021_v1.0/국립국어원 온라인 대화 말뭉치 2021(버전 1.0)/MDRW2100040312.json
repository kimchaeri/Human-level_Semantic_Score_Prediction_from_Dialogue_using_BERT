      "utterance": [
        {
          "id": "MDRW2100040312.1.1",
          "form": "또새롭게도전해보신거있으신가요",
          "original_form": "또새롭게도전해보신거있으신가요",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100040312.1.2",
          "form": "사실 이전에 말씀히ㅐ주셧던것들도",
          "original_form": "사실 이전에 말씀히ㅐ주셧던것들도",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100040312.1.3",
          "form": "신기한거많앗는데",
          "original_form": "신기한거많앗는데",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100040312.1.4",
          "form": "전 보이면 먹지만",
          "original_form": "전 보이면 먹지만",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040312.1.5",
          "form": "찾아먹지는 않아요! ㅎㅎ",
          "original_form": "찾아먹지는 않아요! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040312.1.6",
          "form": "귀찮아서ㅎㅎ",
          "original_form": "귀찮아서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040312.1.7",
          "form": "어..미더덕회? 드셔보셨나요",
          "original_form": "어..미더덕회? 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100040312.1.8",
          "form": "미더덕회..가있어요..?ㅇㅁㅇ오잉???!!띠용",
          "original_form": "미더덕회..가있어요..?ㅇㅁㅇ오잉???!!띠용",
          "speaker_id": "2",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100040312.1.9",
          "form": "ㅎㅎ 그 칼국수 동태탕속 미더덕이요ㅎㅎ",
          "original_form": "ㅎㅎ 그 칼국수 동태탕속 미더덕이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100040312.1.10",
          "form": "그것만 팔더라구요",
          "original_form": "그것만 팔더라구요",
          "speaker_id": "1",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100040312.1.11",
          "form": "와... 미더덕자체를먹지않는데..\r\n미더덕회라니이!!!!",
          "original_form": "와... 미더덕자체를먹지않는데..\r\n미더덕회라니이!!!!",
          "speaker_id": "2",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100040312.1.12",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100040312.1.13",
          "form": "아예 안 드시나요",
          "original_form": "아예 안 드시나요",
          "speaker_id": "1",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100040312.1.14",
          "form": "안의 물만 쪽 빨아먹는 건 봤는데",
          "original_form": "안의 물만 쪽 빨아먹는 건 봤는데",
          "speaker_id": "1",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100040312.1.15",
          "form": "아 멍게도 안 드시죠ㅜㅇㅜ",
          "original_form": "아 멍게도 안 드시죠ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210904 16:08"
        },
        {
          "id": "MDRW2100040312.1.16",
          "form": "네네ㅠㅁㅠ편식왕..\r\n여기 바다쪽사시면 자주먹으러가셨겠는데요ㅋㅋㅋㅋ",
          "original_form": "네네ㅠㅁㅠ편식왕..\r\n여기 바다쪽사시면 자주먹으러가셨겠는데요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100040312.1.17",
          "form": "ㅎㅎ 거기 살면 매일 갔죠! 정말",
          "original_form": "ㅎㅎ 거기 살면 매일 갔죠! 정말",
          "speaker_id": "1",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100040312.1.18",
          "form": "굳이 따지면 멍게 스타일?",
          "original_form": "굳이 따지면 멍게 스타일?",
          "speaker_id": "1",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100040312.1.19",
          "form": "달고 향긋합니다ㅎ",
          "original_form": "달고 향긋합니다ㅎ",
          "speaker_id": "1",
          "time": "20210904 16:11"
        },
        {
          "id": "MDRW2100040312.1.20",
          "form": "제 기준에선요ㅎㅎ",
          "original_form": "제 기준에선요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100040312.1.21",
          "form": "이거 파는 곳을 저도 한군데밖에 못봐서ㅜ",
          "original_form": "이거 파는 곳을 저도 한군데밖에 못봐서ㅜ",
          "speaker_id": "1",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100040312.1.22",
          "form": "그리고 다른 사람들과 시키기엔 호불호 갈려서 몇번 못 먹어봤네요",
          "original_form": "그리고 다른 사람들과 시키기엔 호불호 갈려서 몇번 못 먹어봤네요",
          "speaker_id": "1",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100040312.1.23",
          "form": "호불호갈리는거면 진짜 드실기회가 많이 없겠어요jnj  흐엉",
          "original_form": "호불호갈리는거면 진짜 드실기회가 많이 없겠어요jnj  흐엉",
          "speaker_id": "2",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100040312.1.24",
          "form": "그래서 불가피하게 고독한 대식가가 됐쥬ㅜ",
          "original_form": "그래서 불가피하게 고독한 대식가가 됐쥬ㅜ",
          "speaker_id": "1",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100040312.1.25",
          "form": "근데 미더덕회 파는데는 진짜 혼자 갈만한 데가 아니라ㅜ",
          "original_form": "근데 미더덕회 파는데는 진짜 혼자 갈만한 데가 아니라ㅜ",
          "speaker_id": "1",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100040312.1.26",
          "form": "고독한대식가 악ㅋㅋㅋ",
          "original_form": "고독한대식가 악ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100040312.1.27",
          "form": "근데 그런거파는집이 혼자가기엔 좀  무리가잇긴해요",
          "original_form": "근데 그런거파는집이 혼자가기엔 좀  무리가잇긴해요",
          "speaker_id": "2",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100040312.1.28",
          "form": ";-;",
          "original_form": ";-;",
          "speaker_id": "2",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100040312.1.29",
          "form": "제가 진짜 혼자 곱창도 잘 먹으러 가는데..",
          "original_form": "제가 진짜 혼자 곱창도 잘 먹으러 가는데..",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100040312.1.30",
          "form": "거긴 너어무 붐벼요ㅜ",
          "original_form": "거긴 너어무 붐벼요ㅜ",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100040312.1.31",
          "form": "혼자 테이블 잡기도 눈치 보이고",
          "original_form": "혼자 테이블 잡기도 눈치 보이고",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100040312.1.32",
          "form": "죄다 술마셔서 시끄럽고ㅜㅇㅜ",
          "original_form": "죄다 술마셔서 시끄럽고ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210904 16:22"
        },
        {
          "id": "MDRW2100040312.1.33",
          "form": "와 혼곱..!!!!!!",
          "original_form": "와 혼곱..!!!!!!",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100040312.1.34",
          "form": "최고..!!!!!",
          "original_form": "최고..!!!!!",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100040312.1.35",
          "form": "곱창집은 혼자 가면 더 잘해줘요",
          "original_form": "곱창집은 혼자 가면 더 잘해줘요",
