        },
        {
          "id": "MDRW2100036699.1.6",
          "form": "고칼로리다=JMT",
          "original_form": "고칼로리다=JMT",
          "speaker_id": "1",
          "time": "20210901 10:49"
        },
        {
          "id": "MDRW2100036699.1.7",
          "form": "노벨상감 진리죠ㅎㅎ",
          "original_form": "노벨상감 진리죠ㅎㅎ",
          "speaker_id": "1",
