      "utterance": [
        {
          "id": "MDRW2100036026.1.1",
          "form": "점심은 냉면 먹었어요 ㅎㅎ",
          "original_form": "점심은 냉면 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:37"
        },
        {
          "id": "MDRW2100036026.1.2",
          "form": "시원하니 맛있으셨겠어요",
          "original_form": "시원하니 맛있으셨겠어요",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036026.1.3",
          "form": "네 근데 위에 고명이 없어서 아쉽..ㅋㅋ 걍 대충 오이소박이랑 ㅋㅋ  먹었어요 배고프니 다 맛있네요 ㅎㅎ",
          "original_form": "네 근데 위에 고명이 없어서 아쉽..ㅋㅋ 걍 대충 오이소박이랑 ㅋㅋ  먹었어요 배고프니 다 맛있네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100036026.1.4",
          "form": "밀키트로 음식하시나요?",
          "original_form": "밀키트로 음식하시나요?",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100036026.1.5",
          "form": "방금 냉면은 냉동으로 된거 였고 밀키트 좋아해요",
          "original_form": "방금 냉면은 냉동으로 된거 였고 밀키트 좋아해요",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100036026.1.6",
          "form": "간편하고 ㅎㅎ",
          "original_form": "간편하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100036026.1.7",
          "form": "담꾹 아세요?",
          "original_form": "담꾹 아세요?",
          "speaker_id": "1",
          "time": "20210830 15:35"
        },
        {
          "id": "MDRW2100036026.1.8",
          "form": "온라인에서 누가 알려줬는데요",
          "original_form": "온라인에서 누가 알려줬는데요",
          "speaker_id": "1",
          "time": "20210830 15:35"
        },
        {
          "id": "MDRW2100036026.1.9",
          "form": "부대찌개랑 찜닭 해먹어보고 싶더라구요 ㅎㅎ",
          "original_form": "부대찌개랑 찜닭 해먹어보고 싶더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:35"
        },
        {
          "id": "MDRW2100036026.1.10",
          "form": "담꾹 밀키트에요??",
          "original_form": "담꾹 밀키트에요??",
          "speaker_id": "2",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100036026.1.11",
          "form": "넹 지점 많더라구요 네이버에서 함 찾아보세요",
          "original_form": "넹 지점 많더라구요 네이버에서 함 찾아보세요",
          "speaker_id": "1",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100036026.1.12",
          "form": "동네에서 파는 가게에요",
          "original_form": "동네에서 파는 가게에요",
          "speaker_id": "1",
          "time": "20210830 15:45"
        },
        {
          "id": "MDRW2100036026.1.13",
          "form": "아 그래요?? 오 한번 검색해봐야겠네요",
          "original_form": "아 그래요?? 오 한번 검색해봐야겠네요",
          "speaker_id": "2",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100036026.1.14",
          "form": "괜찮아보이더라구요",
          "original_form": "괜찮아보이더라구요",
          "speaker_id": "1",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100036026.1.15",
          "form": "어디서 많이 들어봤다했더니 지나다 본적 있네요 ㅎㅎ",
          "original_form": "어디서 많이 들어봤다했더니 지나다 본적 있네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 01:16"
        },
        {
          "id": "MDRW2100036026.1.16",
          "form": "동네에 있어요 ㅎㅎ",
          "original_form": "동네에 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 01:16"
        },
        {
          "id": "MDRW2100036026.1.17",
          "form": "아하 넹 ㅎㅎㅎ",
          "original_form": "아하 넹 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 01:16"
        },
        {
          "id": "MDRW2100036026.1.18",
          "form": "무인으로 한다고 ㅎㅎ",
          "original_form": "무인으로 한다고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 01:17"
        },
        {
          "id": "MDRW2100036026.1.19",
          "form": "사람이 뒤에서 포장하고 하긴 하는데 파는건 무인시스템 이라고 현수막 봤어요",
          "original_form": "사람이 뒤에서 포장하고 하긴 하는데 파는건 무인시스템 이라고 현수막 봤어요",
          "speaker_id": "2",
          "time": "20210831 01:17"
        }
      ]
