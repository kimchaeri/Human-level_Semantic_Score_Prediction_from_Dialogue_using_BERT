      "utterance": [
        {
          "id": "MDRW2100030262.1.1",
          "form": "공구우먼에서 이번에 셋업블라우스랑 바지 샀는데 편하고 괜찮더라구요",
          "original_form": "공구우먼에서 이번에 셋업블라우스랑 바지 샀는데 편하고 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030262.1.2",
          "form": "비슷한 제품 많이 나오면 좋겠어요",
          "original_form": "비슷한 제품 많이 나오면 좋겠어요",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030262.1.3",
          "form": "오 예쁘겠네요~",
          "original_form": "오 예쁘겠네요~",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030262.1.4",
          "form": "저는 대부분 쫙쫙늘어나고 통이큰걸 입게되더라고요",
          "original_form": "저는 대부분 쫙쫙늘어나고 통이큰걸 입게되더라고요",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030262.1.5",
          "form": "블라우스를 사도 안입게되고ㅠ",
          "original_form": "블라우스를 사도 안입게되고ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030262.1.6",
          "form": "사실 저도요....",
          "original_form": "사실 저도요....",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030262.1.7",
          "form": "블라우스 좋아하는데 불편해서 거의 모셔만 둬요 ㅋㅋㅋㅋ",
          "original_form": "블라우스 좋아하는데 불편해서 거의 모셔만 둬요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030262.1.8",
          "form": "그쵸 ㅎ 사실 그런 스타일은 참 좋아는하는데",
          "original_form": "그쵸 ㅎ 사실 그런 스타일은 참 좋아는하는데",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030262.1.9",
          "form": "막상 입는건 티셔츠 ㅋㅋㅋ",
          "original_form": "막상 입는건 티셔츠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030262.1.10",
          "form": "신발도 귀찮아서 크록스나 슬리퍼 신고다녀요",
          "original_form": "신발도 귀찮아서 크록스나 슬리퍼 신고다녀요",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030262.1.11",
          "form": "앗 크록스 ㅋㅋㅋㅋㅋㅋ 저도 그래요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 크록스 ㅋㅋㅋㅋㅋㅋ 저도 그래요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030262.1.12",
          "form": "크록스에 정착한 후로 다른 신발을 못신겠어요 ㅋㅋㅋㅋ",
          "original_form": "크록스에 정착한 후로 다른 신발을 못신겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030262.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그쵸",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030262.1.14",
          "form": "회사용 크록스 외출용 크록스 있어요ㅋㅋ",
          "original_form": "회사용 크록스 외출용 크록스 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030262.1.15",
          "form": "다른건 신을수가없어요ㅋㅋ",
          "original_form": "다른건 신을수가없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030262.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ진짜요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ진짜요",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030262.1.17",
          "form": "심지어 크록스에서 디자인도 다양하게 내줘서",
          "original_form": "심지어 크록스에서 디자인도 다양하게 내줘서",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030262.1.18",
          "form": "대충 원피스랑 신을 샌들까지도 커버가 되더라구요ㅋㅋㅋ",
          "original_form": "대충 원피스랑 신을 샌들까지도 커버가 되더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030262.1.19",
          "form": "ㅋㅋ맞아요",
          "original_form": "ㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030262.1.20",
          "form": "친구가 샌들샀는데 진짜 편하다면서 그러더라고요",
          "original_form": "친구가 샌들샀는데 진짜 편하다면서 그러더라고요",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030262.1.21",
          "form": "맞아요 은근 편하고 비맞아도 그냥 물로 닦아내면 마르고",
          "original_form": "맞아요 은근 편하고 비맞아도 그냥 물로 닦아내면 마르고",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030262.1.22",
          "form": "굽 낮은 샌들도 꽤 있어서 막 신기 좋더라구요",
          "original_form": "굽 낮은 샌들도 꽤 있어서 막 신기 좋더라구요",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030262.1.23",
          "form": "ㅋㅋ맞아요ㅋㅋㅋㅋ디자인도 은근 괜찮던데요",
          "original_form": "ㅋㅋ맞아요ㅋㅋㅋㅋ디자인도 은근 괜찮던데요",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030262.1.24",
          "form": "나이가 들면서 ㅋㅋ실용성을 우선시하게되네요",
          "original_form": "나이가 들면서 ㅋㅋ실용성을 우선시하게되네요",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030262.1.25",
          "form": "앗 그쵸그쵸",
          "original_form": "앗 그쵸그쵸",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030262.1.26",
          "form": "편하고 오래쓸만한 게 최곤거 같아요",
          "original_form": "편하고 오래쓸만한 게 최곤거 같아요",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030262.1.27",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030262.1.28",
          "form": "명품도 관심있으시나요?",
          "original_form": "명품도 관심있으시나요?",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030262.1.29",
          "form": "사실 명품은 몰라요 ㅋㅋㅋ",
          "original_form": "사실 명품은 몰라요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030262.1.30",
          "form": "좋아하시나요??",
          "original_form": "좋아하시나요??",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030262.1.31",
          "form": "아뇨ㅋㅋ좋아한다기보다 돈 있으면 사보고싶어요",
          "original_form": "아뇨ㅋㅋ좋아한다기보다 돈 있으면 사보고싶어요",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100030262.1.32",
          "form": "어떤 브랜드 좋아하세요??",
          "original_form": "어떤 브랜드 좋아하세요??",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030262.1.33",
          "form": "저는 딱히 선호하는 브랜드는 없어용",
          "original_form": "저는 딱히 선호하는 브랜드는 없어용",
          "speaker_id": "2",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100030262.1.34",
          "form": "사이즈 땜에 인쇼에서만 구입해요",
          "original_form": "사이즈 땜에 인쇼에서만 구입해요",
          "speaker_id": "2",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100030262.1.35",
          "form": "신발이나 가방은요?",
          "original_form": "신발이나 가방은요?",
          "speaker_id": "1",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100030262.1.36",
          "form": "명품은 옷은 사기 애매한데 신발이나 가방은 하나쯤 있으면 좋을 거 같더라구요",
          "original_form": "명품은 옷은 사기 애매한데 신발이나 가방은 하나쯤 있으면 좋을 거 같더라구요",
          "speaker_id": "1",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100030262.1.37",
          "form": "어디 가야할때 가방하나는 있어야겠더라고요",
          "original_form": "어디 가야할때 가방하나는 있어야겠더라고요",
          "speaker_id": "2",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100030262.1.38",
          "form": "워낙... 한국인들이 겉치레를 중요시 해서",
          "original_form": "워낙... 한국인들이 겉치레를 중요시 해서",
          "speaker_id": "2",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100030262.1.39",
          "form": "맞아요 결혼식 자리는 특히요 ㅋㅋㅋ",
          "original_form": "맞아요 결혼식 자리는 특히요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100030262.1.40",
          "form": "맞아요 ㅎ 결혼식 다니기도 귀찮은데 차려입어야 하는것도 별로에요",
          "original_form": "맞아요 ㅎ 결혼식 다니기도 귀찮은데 차려입어야 하는것도 별로에요",
          "speaker_id": "2",
          "time": "20210904 20:30"
        },
        {
          "id": "MDRW2100030262.1.41",
          "form": "맞아요 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100030262.1.42",
          "form": "기싸움의 장이 될 때가 있어서 힘들죠ㅠㅠ",
          "original_form": "기싸움의 장이 될 때가 있어서 힘들죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:37"
        }
      ]
