      "utterance": [
        {
          "id": "MDRW2100028688.1.1",
          "form": "회사생활 어때?",
          "original_form": "회사생활 어때?",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028688.1.2",
          "form": "회사생활 그 대리 없어져서 할만해",
          "original_form": "회사생활 그 대리 없어져서 할만해",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028688.1.3",
          "form": "새로온 직원은 어때",
          "original_form": "새로온 직원은 어때",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028688.1.4",
          "form": "새로운 직원 진짜 착해",
          "original_form": "새로운 직원 진짜 착해",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028688.1.5",
          "form": "아직 회사에 적응 중이라 둘다 헤매는 중이야",
          "original_form": "아직 회사에 적응 중이라 둘다 헤매는 중이야",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028688.1.6",
          "form": "다행이다 정말",
          "original_form": "다행이다 정말",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028688.1.7",
          "form": "급여는 만족하니?",
          "original_form": "급여는 만족하니?",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028688.1.8",
          "form": "아니지",
          "original_form": "아니지",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028688.1.9",
          "form": "연봉협상 빨리 하고 싶어",
          "original_form": "연봉협상 빨리 하고 싶어",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028688.1.10",
          "form": "나도 월급이 인상되면 좋겠어",
          "original_form": "나도 월급이 인상되면 좋겠어",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100028688.1.11",
          "form": "우리는 월급 동결될 각이야",
          "original_form": "우리는 월급 동결될 각이야",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028688.1.12",
          "form": "떡값도 못준다는디",
          "original_form": "떡값도 못준다는디",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028688.1.13",
          "form": "우리도 떡값 없어 상품권 하나 주ㅜ",
          "original_form": "우리도 떡값 없어 상품권 하나 주ㅜ",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028688.1.14",
          "form": "우리는 스팸 ㅜ",
          "original_form": "우리는 스팸 ㅜ",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028688.1.15",
          "form": "그걸로 땡이야",
          "original_form": "그걸로 땡이야",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028688.1.16",
          "form": "나쁜 회사",
          "original_form": "나쁜 회사",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028688.1.17",
          "form": "급여가 5000만 됐으면 좋겠더",
          "original_form": "급여가 5000만 됐으면 좋겠더",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028688.1.18",
          "form": "죽기전에 통장에 찍어볼 수 있을까ㅋㅋㅋㅋ",
          "original_form": "죽기전에 통장에 찍어볼 수 있을까ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028688.1.19",
          "form": "그럼 연봉 5000",
          "original_form": "그럼 연봉 5000",
          "speaker_id": "1",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028688.1.20",
          "form": "구래 그럼 오래 일해야겠어",
          "original_form": "구래 그럼 오래 일해야겠어",
          "speaker_id": "2",
