      "utterance": [
        {
          "id": "MDRW2100042771.1.1",
          "form": "반려동물 키워본 경험 있으세요?",
          "original_form": "반려동물 키워본 경험 있으세요?",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100042771.1.2",
          "form": "저 강아지 키우고 있어요!!!",
          "original_form": "저 강아지 키우고 있어요!!!",
          "speaker_id": "2",
          "time": "20210907 08:53"
        },
        {
          "id": "MDRW2100042771.1.3",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210907 08:53"
        },
        {
          "id": "MDRW2100042771.1.4",
          "form": "저는 예전에 키웠어요 말티즈",
          "original_form": "저는 예전에 키웠어요 말티즈",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042771.1.5",
          "form": "name2님은 어느 종을 키우나요?",
          "original_form": "&name2&님은 어느 종을 키우나요?",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042771.1.6",
          "form": "저는 시츄요!! 말티즈도 귀엽죠ㅠㅠ ㅋㅋㅋ",
          "original_form": "저는 시츄요!! 말티즈도 귀엽죠ㅠㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042771.1.7",
          "form": "시츄 키워보고 싶었는데 정말 귀엽죠?",
          "original_form": "시츄 키워보고 싶었는데 정말 귀엽죠?",
          "speaker_id": "1",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042771.1.8",
          "form": "말티즈는 털이 길어서 꼭 딸아이 머리 빗겨주는 느낌이었어요",
          "original_form": "말티즈는 털이 길어서 꼭 딸아이 머리 빗겨주는 느낌이었어요",
          "speaker_id": "1",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042771.1.9",
          "form": "시츄 진짜 순딩이에요 ㅜㅜㅜ",
          "original_form": "시츄 진짜 순딩이에요 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042771.1.10",
          "form": "멍충미가 있죠",
          "original_form": "멍충미가 있죠",
          "speaker_id": "1",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042771.1.11",
          "form": "악ㅋㅋㅋㅋ저희는 엄마가 털 기르는 꼴을 못보셔서 어느정도 길면 미용해요 ㅋㅋㅋㅋ 시츄도 털 길게 기르는애들 있더라구요 ㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋㅋ저희는 엄마가 털 기르는 꼴을 못보셔서 어느정도 길면 미용해요 ㅋㅋㅋㅋ 시츄도 털 길게 기르는애들 있더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042771.1.12",
          "form": "시츄랑 말티가 막상막하더라구요",
          "original_form": "시츄랑 말티가 막상막하더라구요",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042771.1.13",
          "form": "그래서 더 귀여워요",
          "original_form": "그래서 더 귀여워요",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042771.1.14",
          "form": "어우 네네 ㅜㅠ 제가 모르고 발밟은적있는데 아프다고 소리도 안내요ㅠㅠ",
          "original_form": "어우 네네 ㅜㅠ 제가 모르고 발밟은적있는데 아프다고 소리도 안내요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042771.1.15",
          "form": "저는 말티 머리 묶어주고 그랬는데요",
          "original_form": "저는 말티 머리 묶어주고 그랬는데요",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042771.1.16",
          "form": "삔 꼽아주는 재미가 있어요",
          "original_form": "삔 꼽아주는 재미가 있어요",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042771.1.17",
          "form": "막 털 땋기도 하고",
          "original_form": "막 털 땋기도 하고",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042771.1.18",
          "form": "악ㅋㅋㅋ오 강아지가 삔 꼽아줘도 가만히 있나요?!?!",
          "original_form": "악ㅋㅋㅋ오 강아지가 삔 꼽아줘도 가만히 있나요?!?!",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042771.1.19",
          "form": "오 땋기까지!!",
          "original_form": "오 땋기까지!!",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042771.1.20",
          "form": "말티는 가능하죠 ㅎㅎ",
          "original_form": "말티는 가능하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042771.1.21",
          "form": "저도 강아지 눈이 안보여서 이마쪽에 삔 꼽아준적있는데 계속 털더라구요 ㅋㅋㅋㅋㅋ첨이라 어색했나봐요 ㅋㅋㅋㅋ",
          "original_form": "저도 강아지 눈이 안보여서 이마쪽에 삔 꼽아준적있는데 계속 털더라구요 ㅋㅋㅋㅋㅋ첨이라 어색했나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042771.1.22",
          "form": "아.. 시츄는 그런가요?",
          "original_form": "아.. 시츄는 그런가요?",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042771.1.23",
          "form": "말티는 가만히 있던데",
          "original_form": "말티는 가만히 있던데",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042771.1.24",
          "form": "강아지 성향일까요?",
          "original_form": "강아지 성향일까요?",
