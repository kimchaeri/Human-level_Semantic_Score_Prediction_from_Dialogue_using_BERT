      "utterance": [
        {
          "id": "MDRW2100013315.1.1",
          "form": "공부하기 싫다ㅎㅎ",
          "original_form": "공부하기 싫다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013315.1.2",
          "form": "동감입니다!!",
          "original_form": "동감입니다!!",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013315.1.3",
          "form": "제일 싫은 과목 뭐예요?",
          "original_form": "제일 싫은 과목 뭐예요?",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013315.1.4",
          "form": "저는 영어요",
          "original_form": "저는 영어요",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013315.1.5",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013315.1.6",
          "form": "영어 완전 어렵죠ㅠㅠ",
          "original_form": "영어 완전 어렵죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013315.1.7",
          "form": "저는 화학이요 진짜 하나도 모르겠어요",
          "original_form": "저는 화학이요 진짜 하나도 모르겠어요",
          "speaker_id": "1",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013315.1.8",
          "form": "화학... 보이지 않는 원소 알게 무엇입니까. 흑흑",
          "original_form": "화학... 보이지 않는 원소 알게 무엇입니까. 흑흑",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013315.1.9",
          "form": "완전 공감ㅋㅋㅋ",
          "original_form": "완전 공감ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013315.1.10",
          "form": "고등학생이에요?",
          "original_form": "고등학생이에요?",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013315.1.11",
          "form": "아니요 저는 대학졸업 이후로 화학 쳐다도 안 본 일반인이예요ㅎㅎ",
          "original_form": "아니요 저는 대학졸업 이후로 화학 쳐다도 안 본 일반인이예요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013315.1.12",
          "form": "아 졸업하셨구나 저돈데",
          "original_form": "아 졸업하셨구나 저돈데",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013315.1.13",
          "form": "대학교 다닐때 동아리 하셨어요?",
          "original_form": "대학교 다닐때 동아리 하셨어요?",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013315.1.14",
          "form": "네 1학년때만 봉사동아리했어요 name1은 동아리활동 하셨어요?",
          "original_form": "네 1학년때만 봉사동아리했어요 &name1&은 동아리활동 하셨어요?",
          "speaker_id": "2",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013315.1.15",
          "form": "와 봉사동아리 좋은일 많이하셨겠다",
          "original_form": "와 봉사동아리 좋은일 많이하셨겠다",
          "speaker_id": "1",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013315.1.16",
          "form": "저는 댄스동아리했어요 2학년까지만 했지만",
          "original_form": "저는 댄스동아리했어요 2학년까지만 했지만",
          "speaker_id": "1",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013315.1.17",
          "form": "우와 멋지네요! 축제때 댄스동아리 젤 멋지잖아요",
          "original_form": "우와 멋지네요! 축제때 댄스동아리 젤 멋지잖아요",
