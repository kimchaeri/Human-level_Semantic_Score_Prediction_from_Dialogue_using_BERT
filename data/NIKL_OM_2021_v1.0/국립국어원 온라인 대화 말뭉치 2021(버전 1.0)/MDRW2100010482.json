      "utterance": [
        {
          "id": "MDRW2100010482.1.1",
          "form": "그래도 오전에 비 안 올 때 좀 걷다 왔어요!",
          "original_form": "그래도 오전에 비 안 올 때 좀 걷다 왔어요!",
          "speaker_id": "1",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100010482.1.2",
          "form": "미리 다녀오길 잘했네요",
          "original_form": "미리 다녀오길 잘했네요",
          "speaker_id": "1",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100010482.1.3",
          "form": "다행이네요~~",
          "original_form": "다행이네요~~",
          "speaker_id": "2",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010482.1.4",
          "form": "해보고 싶은 운동 있으세요?",
          "original_form": "해보고 싶은 운동 있으세요?",
          "speaker_id": "2",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010482.1.5",
          "form": "수영이랑 배구요!",
          "original_form": "수영이랑 배구요!",
          "speaker_id": "1",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010482.1.6",
          "form": "오 배구 멋있네요!!",
          "original_form": "오 배구 멋있네요!!",
          "speaker_id": "2",
          "time": "20210823 19:27"
        },
        {
          "id": "MDRW2100010482.1.7",
          "form": "이번에 배구 보셨어요??",
          "original_form": "이번에 배구 보셨어요??",
          "speaker_id": "2",
          "time": "20210823 19:27"
        },
        {
          "id": "MDRW2100010482.1.8",
          "form": "네 사실 이번에 여자 배구 보고 완전 빠졌어요ㅎㅎ",
          "original_form": "네 사실 이번에 여자 배구 보고 완전 빠졌어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:30"
        },
        {
          "id": "MDRW2100010482.1.9",
          "form": "그쵸 넘 멋있어요",
          "original_form": "그쵸 넘 멋있어요",
          "speaker_id": "2",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010482.1.10",
          "form": "ㅋㅋ다들 어찌 그리 키도 크고...멋있는지",
          "original_form": "ㅋㅋ다들 어찌 그리 키도 크고...멋있는지",
          "speaker_id": "2",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010482.1.11",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "1",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010482.1.12",
          "form": "name2 님은 운동 좋아하시나요?",
          "original_form": "&name2& 님은 운동 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010482.1.13",
          "form": "학교다닐 땐 못 했는데",
          "original_form": "학교다닐 땐 못 했는데",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010482.1.14",
          "form": "수영해보니까 다른것도 배우고 싶더라고요ㅋㅋ",
          "original_form": "수영해보니까 다른것도 배우고 싶더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010482.1.15",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010482.1.16",
          "form": "저도 조금씩 흥미를 만들어 가는 중이에요ㅎㅎ",
          "original_form": "저도 조금씩 흥미를 만들어 가는 중이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010482.1.17",
          "form": "건강을 위해서라도 친해져야죠",
          "original_form": "건강을 위해서라도 친해져야죠",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010482.1.18",
          "form": "그쵸ㅋㅋ전 무술 종류도 배워보고 싶어요",
          "original_form": "그쵸ㅋㅋ전 무술 종류도 배워보고 싶어요",
          "speaker_id": "2",
          "time": "20210823 19:50"
        },
        {
          "id": "MDRW2100010482.1.19",
          "form": "와 저도요!",
          "original_form": "와 저도요!",
          "speaker_id": "1",
          "time": "20210823 19:52"
        },
        {
          "id": "MDRW2100010482.1.20",
          "form": "주짓수랑 무에타이 배워보고 싶어요!",
          "original_form": "주짓수랑 무에타이 배워보고 싶어요!",
          "speaker_id": "1",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010482.1.21",
          "form": "저는 무에타이나 복싱이요!",
          "original_form": "저는 무에타이나 복싱이요!",
          "speaker_id": "2",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010482.1.22",
          "form": "근처에 도장 있어요??",
          "original_form": "근처에 도장 있어요??",
          "speaker_id": "2",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010482.1.23",
          "form": "다이어트 복싱 이런 건 있던데",
          "original_form": "다이어트 복싱 이런 건 있던데",
          "speaker_id": "1",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010482.1.24",
          "form": "막 찾아보지는 않았아요ㅎㅎ",
          "original_form": "막 찾아보지는 않았아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010482.1.25",
          "form": "그냥 막연하게 배워보고 싶다고만 생각한 거라",
          "original_form": "그냥 막연하게 배워보고 싶다고만 생각한 거라",
          "speaker_id": "1",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010482.1.26",
          "form": "뭔가 제대로 배우고 싶은데",
          "original_form": "뭔가 제대로 배우고 싶은데",
          "speaker_id": "2",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100010482.1.27",
          "form": "다이어트 복싱이 많더라고요ㅋㅋ",
          "original_form": "다이어트 복싱이 많더라고요ㅋㅋ",
