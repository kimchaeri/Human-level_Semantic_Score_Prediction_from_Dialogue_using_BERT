      "utterance": [
        {
          "id": "MDRW2100035580.1.1",
          "form": "지난주랑 이번주 이어지는 스토리인데",
          "original_form": "지난주랑 이번주 이어지는 스토리인데",
          "speaker_id": "1",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100035580.1.2",
          "form": "지난주께 진짜 대박 재밌었어요",
          "original_form": "지난주께 진짜 대박 재밌었어요",
          "speaker_id": "1",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100035580.1.3",
          "form": "어디탈출하나요",
          "original_form": "어디탈출하나요",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100035580.1.4",
          "form": "그 범죄 저지른 사람의 무의식에 들어가서",
          "original_form": "그 범죄 저지른 사람의 무의식에 들어가서",
          "speaker_id": "1",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100035580.1.5",
          "form": "저 좀비 무서워한다고 한 말이 신빙성이 없어지고 있는데ㅋㅋㅋ대탈출도 병원 탈출하는 좀비편은 봤어요",
          "original_form": "저 좀비 무서워한다고 한 말이 신빙성이 없어지고 있는데ㅋㅋㅋ대탈출도 병원 탈출하는 좀비편은 봤어요",
          "speaker_id": "2",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100035580.1.6",
          "form": "그 무의식속에서 탈출해용",
          "original_form": "그 무의식속에서 탈출해용",
          "speaker_id": "1",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100035580.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100035580.1.8",
          "form": "앜ㅋㅋㅋㅋ 이건 출연자들이 너무 무 서워해서",
          "original_form": "앜ㅋㅋㅋㅋ 이건 출연자들이 너무 무 서워해서",
          "speaker_id": "1",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100035580.1.9",
          "form": "그거 어디 영화 소재 였던거 같아요",
          "original_form": "그거 어디 영화 소재 였던거 같아요",
          "speaker_id": "2",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100035580.1.10",
          "form": "저까지 무서움이 상쇄되는 느낌...?",
          "original_form": "저까지 무서움이 상쇄되는 느낌...?",
          "speaker_id": "1",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100035580.1.11",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035580.1.12",
          "form": "들어본거같아요",
          "original_form": "들어본거같아요",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035580.1.13",
          "form": "인셉션이라고 하도라구여",
          "original_form": "인셉션이라고 하도라구여",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035580.1.14",
          "form": "막 유쾌한 소재는 아니지만",
          "original_form": "막 유쾌한 소재는 아니지만",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035580.1.15",
          "form": "무섭지도 않아서",
          "original_form": "무섭지도 않아서",
          "speaker_id": "1",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035580.1.16",
          "form": "맞죠?인셉션도 있고",
          "original_form": "맞죠?인셉션도 있고",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100035580.1.17",
          "form": "추천드려용",
          "original_form": "추천드려용",
          "speaker_id": "1",
          "time": "20210829 20:44"
        }
