      "utterance": [
        {
          "id": "MDRW2100038067.1.1",
          "form": "자~이제 지는 깔꼼허게 자로 갑니더 ㅋㅋㅋㅋ",
          "original_form": "자~이제 지는 깔꼼허게 자로 갑니더 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038067.1.2",
          "form": "적당히 보고 주무시라요 ㅋㅋㅋㅋ",
          "original_form": "적당히 보고 주무시라요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038067.1.3",
          "form": "낼 애들 밥줘야됭께 ㅋㅋㅋ",
          "original_form": "낼 애들 밥줘야됭께 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038067.1.4",
          "form": "엉ㅎㅎ낼보자이ㅋ",
          "original_form": "엉ㅎㅎ낼보자이ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038067.1.5",
          "form": "빠잉~~",
          "original_form": "빠잉~~",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038067.1.6",
          "form": "일어나요 언니 ㅋㅋㅋ",
          "original_form": "일어나요 언니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 06:26"
        },
        {
          "id": "MDRW2100038067.1.7",
          "form": "새아침이 밝았어요 ㅋㅋㅋ",
          "original_form": "새아침이 밝았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 06:26"
        },
        {
          "id": "MDRW2100038067.1.8",
          "form": "새벽이네ㅋㅋ",
          "original_form": "새벽이네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038067.1.9",
          "form": "오늘 뭐하노?",
          "original_form": "오늘 뭐하노?",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038067.1.10",
          "form": "오늘도 아가방 집에 가면 청소 그렇죠ㅠ",
          "original_form": "오늘도 아가방 집에 가면 청소 그렇죠ㅠ",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038067.1.11",
          "form": "맨날 쳇바퀴 ㅋㅋㅋ",
          "original_form": "맨날 쳇바퀴 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038067.1.12",
          "form": "내가 햄스터임 ㅋㅋㅋ",
          "original_form": "내가 햄스터임 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038067.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038067.1.14",
          "form": "아가방ㅋㅋ",
          "original_form": "아가방ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038067.1.15",
          "form": "난 집 엉망",
          "original_form": "난 집 엉망",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038067.1.16",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038067.1.17",
          "form": "난 첫찌 방분리한다고 뭐 이래저래 또 옮겨야되서ㅠ",
          "original_form": "난 첫찌 방분리한다고 뭐 이래저래 또 옮겨야되서ㅠ",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100038067.1.18",
          "form": "하...그래여 ㅋㅋㅋㅋㅋ",
          "original_form": "하...그래여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100038067.1.19",
          "form": "매장도 바쁘고..",
          "original_form": "매장도 바쁘고..",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100038067.1.20",
          "form": "요번주는 디네여 ㅋㅋㅋ",
          "original_form": "요번주는 디네여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100038067.1.21",
          "form": "방분리하나?",
          "original_form": "방분리하나?",
