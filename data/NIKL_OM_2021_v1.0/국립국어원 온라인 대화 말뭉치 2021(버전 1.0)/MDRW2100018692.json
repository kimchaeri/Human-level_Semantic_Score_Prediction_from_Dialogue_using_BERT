      "utterance": [
        {
          "id": "MDRW2100018692.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 10:50"
        },
        {
          "id": "MDRW2100018692.1.2",
          "form": "룰은잘모르는데",
          "original_form": "룰은잘모르는데",
          "speaker_id": "1",
          "time": "20210827 10:51"
        },
        {
          "id": "MDRW2100018692.1.3",
          "form": "보는거좋아해요",
          "original_form": "보는거좋아해요",
          "speaker_id": "1",
          "time": "20210827 10:51"
        },
        {
          "id": "MDRW2100018692.1.4",
          "form": "어떤거 자주 보세요?",
          "original_form": "어떤거 자주 보세요?",
          "speaker_id": "2",
          "time": "20210827 10:51"
        },
        {
          "id": "MDRW2100018692.1.5",
          "form": "올림픽 열심히 챙겨봣고",
          "original_form": "올림픽 열심히 챙겨봣고",
          "speaker_id": "1",
          "time": "20210827 10:51"
        },
        {
          "id": "MDRW2100018692.1.6",
          "form": "9월엔 축구보러갈까싶어요",
          "original_form": "9월엔 축구보러갈까싶어요",
          "speaker_id": "1",
          "time": "20210827 10:51"
        },
        {
          "id": "MDRW2100018692.1.7",
          "form": "오.. 올림픽 어떤게 제일 재밌으셨어요?",
          "original_form": "오.. 올림픽 어떤게 제일 재밌으셨어요?",
          "speaker_id": "2",
          "time": "20210827 10:52"
        },
        {
          "id": "MDRW2100018692.1.8",
          "form": "우와 직관이요?",
          "original_form": "우와 직관이요?",
          "speaker_id": "2",
          "time": "20210827 10:52"
        },
        {
          "id": "MDRW2100018692.1.9",
          "form": "아무래도 배구 한일전....",
          "original_form": "아무래도 배구 한일전....",
          "speaker_id": "1",
          "time": "20210827 10:52"
        },
        {
          "id": "MDRW2100018692.1.10",
          "form": "뭉클햇어요ㅠㅠ",
          "original_form": "뭉클햇어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 10:52"
        },
        {
          "id": "MDRW2100018692.1.11",
          "form": "아..ㅠ 그 감동을 저는 놓쳐서..!",
          "original_form": "아..ㅠ 그 감동을 저는 놓쳐서..!",
          "speaker_id": "2",
          "time": "20210827 10:53"
        },
        {
          "id": "MDRW2100018692.1.12",
          "form": "너무 아쉽네요",
          "original_form": "너무 아쉽네요",
          "speaker_id": "2",
          "time": "20210827 10:53"
        },
        {
          "id": "MDRW2100018692.1.13",
          "form": "앗 왜 못보셧어요?",
          "original_form": "앗 왜 못보셧어요?",
          "speaker_id": "1",
          "time": "20210827 10:55"
        },
        {
          "id": "MDRW2100018692.1.14",
          "form": "스포츠를 안챙겨보다보니ㅠ 생각을 못했어요",
          "original_form": "스포츠를 안챙겨보다보니ㅠ 생각을 못했어요",
          "speaker_id": "2",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018692.1.15",
          "form": "부모님 보실때 슬쩍 보는정도거든요",
          "original_form": "부모님 보실때 슬쩍 보는정도거든요",
          "speaker_id": "2",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018692.1.16",
          "form": "아ㅋㅋ저도 올림픽아니면 챙겨보진않아요",
          "original_form": "아ㅋㅋ저도 올림픽아니면 챙겨보진않아요",
          "speaker_id": "1",
          "time": "20210827 11:02"
        },
        {
          "id": "MDRW2100018692.1.17",
          "form": "ㅋㅋ오로지 고양이 생각뿐이시구나!!!!!!",
          "original_form": "ㅋㅋ오로지 고양이 생각뿐이시구나!!!!!!",
          "speaker_id": "1",
          "time": "20210827 11:02"
        },
        {
          "id": "MDRW2100018692.1.18",
          "form": "좋은집사님",
          "original_form": "좋은집사님",
          "speaker_id": "1",
          "time": "20210827 11:03"
        },
        {
          "id": "MDRW2100018692.1.19",
          "form": "아핫ㅋㅋㅋ 뭔가 부끄럽네요ㅋㅋ",
          "original_form": "아핫ㅋㅋㅋ 뭔가 부끄럽네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:04"
        },
        {
          "id": "MDRW2100018692.1.20",
          "form": "아! 그래도 가끔 보는 볼링은 좋아해요",
          "original_form": "아! 그래도 가끔 보는 볼링은 좋아해요",
          "speaker_id": "2",
          "time": "20210827 11:05"
        },
        {
          "id": "MDRW2100018692.1.21",
          "form": "올림픽 경기 보신거 잇으신가요",
          "original_form": "올림픽 경기 보신거 잇으신가요",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018692.1.22",
          "form": "양궁이요!",
          "original_form": "양궁이요!",
          "speaker_id": "2",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018692.1.23",
          "form": "보셨나요?",
