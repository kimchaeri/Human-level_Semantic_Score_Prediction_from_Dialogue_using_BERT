      "utterance": [
        {
          "id": "MDRW2100010239.1.1",
          "form": "즐겨먹는 음료 있으세요?",
          "original_form": "즐겨먹는 음료 있으세요?",
          "speaker_id": "1",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100010239.1.2",
          "form": "저는 폴바셋 라떼 좋아해요",
          "original_form": "저는 폴바셋 라떼 좋아해요",
          "speaker_id": "1",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100010239.1.3",
          "form": "오 구체적이시네용",
          "original_form": "오 구체적이시네용",
          "speaker_id": "2",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010239.1.4",
          "form": "저는 커피응 잘안마셔서",
          "original_form": "저는 커피응 잘안마셔서",
          "speaker_id": "2",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010239.1.5",
          "form": "정해진건 없어요 ㅋㅋ그냥 음료수위주로 먹네요",
          "original_form": "정해진건 없어요 ㅋㅋ그냥 음료수위주로 먹네요",
          "speaker_id": "2",
          "time": "20210823 16:02"
        },
        {
          "id": "MDRW2100010239.1.6",
          "form": "그럼 어떤 음료수 드세여?",
          "original_form": "그럼 어떤 음료수 드세여?",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010239.1.7",
          "form": "저는 주로 과일주스요 ㅎㅎ",
          "original_form": "저는 주로 과일주스요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010239.1.8",
          "form": "좋아하는 음식있으세요?",
          "original_form": "좋아하는 음식있으세요?",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010239.1.9",
          "form": "저는 쌀국수 좋아해요~",
          "original_form": "저는 쌀국수 좋아해요~",
          "speaker_id": "1",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010239.1.10",
          "form": "오 쌀국수 맛나죠",
          "original_form": "오 쌀국수 맛나죠",
          "speaker_id": "2",
          "time": "20210823 16:04"
        },
        {
          "id": "MDRW2100010239.1.11",
          "form": "샤브샤브도 좋아하고 요즘 비가 오고 좀 쌀쌀하니 국물이 땡겨요 ㅎㅎ",
          "original_form": "샤브샤브도 좋아하고 요즘 비가 오고 좀 쌀쌀하니 국물이 땡겨요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010239.1.12",
          "form": "월날쌈 먹고싶당 ㅠ",
          "original_form": "월날쌈 먹고싶당 ㅠ",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010239.1.13",
          "form": "저도 샤브샤브 좋아해요",
          "original_form": "저도 샤브샤브 좋아해요",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010239.1.14",
          "form": "베트남느낌 음식 좋아하시나봐요!",
          "original_form": "베트남느낌 음식 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010239.1.15",
          "form": "딱히 그런건 아닌데 그런가봐요 ㅎㅎㅎ",
          "original_form": "딱히 그런건 아닌데 그런가봐요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010239.1.16",
          "form": "고수 들어간건 싫어해요 ㅎㅎ",
          "original_form": "고수 들어간건 싫어해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010239.1.17",
          "form": "전 적당히는 괜찮더라구요 ㅎㅎ",
          "original_form": "전 적당히는 괜찮더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010239.1.18",
          "form": "매운음식 좋아하세요?",
          "original_form": "매운음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 16:05"
        },
        {
          "id": "MDRW2100010239.1.19",
          "form": "볶음면이나 분짜 기타등등 다른건 안먹어요 ㅎㅎ",
          "original_form": "볶음면이나 분짜 기타등등 다른건 안먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010239.1.20",
          "form": "매운음식은 잘 못먹어요",
          "original_form": "매운음식은 잘 못먹어요",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010239.1.21",
          "form": "요즘 매운맛 난이도가 너무 올라간 느낌이라 대중을 가늠하지 못하겠어요 ㅎㅎ",
          "original_form": "요즘 매운맛 난이도가 너무 올라간 느낌이라 대중을 가늠하지 못하겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010239.1.22",
          "form": "매운거 좋아하세요?",
          "original_form": "매운거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 16:06"
        },
        {
          "id": "MDRW2100010239.1.23",
          "form": "저는 좋아는하는데요 잘먹진못해요 ㅋㅋ",
          "original_form": "저는 좋아는하는데요 잘먹진못해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010239.1.24",
          "form": "적당히 잘먹는거같아용",
          "original_form": "적당히 잘먹는거같아용",
          "speaker_id": "2",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010239.1.25",
          "form": "어떤 음식 드세요?",
          "original_form": "어떤 음식 드세요?",
          "speaker_id": "1",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010239.1.26",
          "form": "매운거!",
          "original_form": "매운거!",
          "speaker_id": "1",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100010239.1.27",
          "form": "매운거 어떤종류 드시는지 궁금해욤",
          "original_form": "매운거 어떤종류 드시는지 궁금해욤",
          "speaker_id": "1",
          "time": "20210823 16:08"
        },
        {
          "id": "MDRW2100010239.1.28",
          "form": "저는 불닭볶음면까지는 먹어요!!",
          "original_form": "저는 불닭볶음면까지는 먹어요!!",
          "speaker_id": "1",
          "time": "20210823 16:08"
        },
        {
          "id": "MDRW2100010239.1.29",
          "form": "근데 매워하면서 먹는 편이에요 ㅎㅎ",
          "original_form": "근데 매워하면서 먹는 편이에요 ㅎㅎ",
          "speaker_id": "1",
