      "utterance": [
        {
          "id": "MDRW2100032456.1.1",
          "form": "저도 안아팠으면",
          "original_form": "저도 안아팠으면",
          "speaker_id": "2",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100032456.1.2",
          "form": "좋겠습니다 ㅋㅋㅋ",
          "original_form": "좋겠습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100032456.1.3",
          "form": "그냥 쉬게  ㅋㅋㅋ",
          "original_form": "그냥 쉬게  ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100032456.1.4",
          "form": "네 아마 name2님도 안 아프실듯ㅋㅋㅋ",
          "original_form": "네 아마 &name2&님도 안 아프실듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100032456.1.5",
          "form": "이게 막 아프다고 여기저기서 그러니까",
          "original_form": "이게 막 아프다고 여기저기서 그러니까",
          "speaker_id": "2",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100032456.1.6",
          "form": "겁먹고 있어서 ㅋㅋㅋ",
          "original_form": "겁먹고 있어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100032456.1.7",
          "form": "네ㅋㅋㅋㅋ 근데 안아픈 사람 여기!",
          "original_form": "네ㅋㅋㅋㅋ 근데 안아픈 사람 여기!",
          "speaker_id": "1",
          "time": "20210905 18:47"
        },
        {
          "id": "MDRW2100032456.1.8",
          "form": "의외로 좀 있나봐요 ㅎㅎ",
          "original_form": "의외로 좀 있나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 18:50"
        },
        {
          "id": "MDRW2100032456.1.9",
          "form": "네 근데 사실 저희 가족들 아무도 안 아프고 넘어갔어요",
          "original_form": "네 근데 사실 저희 가족들 아무도 안 아프고 넘어갔어요",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100032456.1.10",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210905 19:37"
        },
        {
          "id": "MDRW2100032456.1.11",
          "form": "유전적 요인이 있는건가",
          "original_form": "유전적 요인이 있는건가",
          "speaker_id": "2",
          "time": "20210905 19:37"
        },
        {
          "id": "MDRW2100032456.1.12",
          "form": "좀 면역력이 좋은 거 같아요 가족들이 ㅋㅋ",
          "original_form": "좀 면역력이 좋은 거 같아요 가족들이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100032456.1.13",
          "form": "제동생은 1차 맞고도",
          "original_form": "제동생은 1차 맞고도",
          "speaker_id": "2",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100032456.1.14",
          "form": "헤롱거리던데",
          "original_form": "헤롱거리던데",
          "speaker_id": "2",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100032456.1.15",
          "form": "앗 정말요 진짜 다 다르구나..",
          "original_form": "앗 정말요 진짜 다 다르구나..",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100032456.1.16",
          "form": "진짜 사람마다 이렇게 차이나는 백신 처음봤어요",
          "original_form": "진짜 사람마다 이렇게 차이나는 백신 처음봤어요",
          "speaker_id": "2",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100032456.1.17",
          "form": "이게 맞아도 항체가 생겼는지 모르겠고 ㅋㅋㅋ",
          "original_form": "이게 맞아도 항체가 생겼는지 모르겠고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100032456.1.18",
          "form": "또 계속 변이가 빠르게 생기고 ㅋㅋㅋ",
          "original_form": "또 계속 변이가 빠르게 생기고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100032456.1.19",
          "form": "또 맞으러가야할 수도 있자나요 그쵸",
          "original_form": "또 맞으러가야할 수도 있자나요 그쵸",
          "speaker_id": "1",
          "time": "20210905 21:22"
        },
        {
          "id": "MDRW2100032456.1.20",
          "form": "부스터샷?",
          "original_form": "부스터샷?",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032456.1.21",
          "form": "그거 또 맞으라고 할지도",
          "original_form": "그거 또 맞으라고 할지도",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032456.1.22",
          "form": "휴 이 백신 맞아도 변이 준비는 안된것이니 ㅠㅜ",
          "original_form": "휴 이 백신 맞아도 변이 준비는 안된것이니 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100032456.1.23",
          "form": "이거 치료약이 나와야하는데ㅜㅜ",
          "original_form": "이거 치료약이 나와야하는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 22:43"
        }
      ]
    }
