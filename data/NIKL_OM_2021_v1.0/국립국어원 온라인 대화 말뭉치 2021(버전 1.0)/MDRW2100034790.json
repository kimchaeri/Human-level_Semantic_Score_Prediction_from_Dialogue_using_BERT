      "utterance": [
        {
          "id": "MDRW2100034790.1.1",
          "form": "맛집 찾아다니는 거 좋아하시나요?",
          "original_form": "맛집 찾아다니는 거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100034790.1.2",
          "form": "집순이라 거의 근처만 가고 멀리갈때는 맛집가려고해요",
          "original_form": "집순이라 거의 근처만 가고 멀리갈때는 맛집가려고해요",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100034790.1.3",
          "form": "맛집 탐방 좋아하세요?",
          "original_form": "맛집 탐방 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100034790.1.4",
          "form": "저두 비슷하네요 ㅋㅋㅋ",
          "original_form": "저두 비슷하네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100034790.1.5",
          "form": "근데 또 줄이 길게 서있으면 꼭 기다려먹자는 주의는 아니라",
          "original_form": "근데 또 줄이 길게 서있으면 꼭 기다려먹자는 주의는 아니라",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100034790.1.6",
          "form": "저두요ㅋㅋ",
          "original_form": "저두요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034790.1.7",
          "form": "특히 덥거나 추울때는 못기다려요ㅋㅋ",
          "original_form": "특히 덥거나 추울때는 못기다려요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:45"
        },
        {
          "id": "MDRW2100034790.1.8",
          "form": "네 그 옆집을 가기도 하고 ㅋㅋㅋㅋ",
          "original_form": "네 그 옆집을 가기도 하고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100034790.1.9",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034790.1.10",
          "form": "가리시는 음식 있으세요?",
          "original_form": "가리시는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210828 19:56"
        },
        {
          "id": "MDRW2100034790.1.11",
          "form": "아니요 그냥 주면 다먹어요",
          "original_form": "아니요 그냥 주면 다먹어요",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034790.1.12",
          "form": "여행지가서도 크게 무리가 없었어요",
          "original_form": "여행지가서도 크게 무리가 없었어요",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034790.1.13",
          "form": "물론 대만 일본은 음식이 많이 난감한 곳은 아니지만 ㅋㅋ",
          "original_form": "물론 대만 일본은 음식이 많이 난감한 곳은 아니지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100034790.1.14",
          "form": "여행가기 좋은 친구이시네요!",
          "original_form": "여행가기 좋은 친구이시네요!",
          "speaker_id": "1",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034790.1.15",
          "form": "먹는게 진짜 큰걸요",
          "original_form": "먹는게 진짜 큰걸요",
          "speaker_id": "1",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034790.1.16",
          "form": "맞아요 먹는 거 진짜 중요하죠 여행에서ㅋㅋㅋ",
          "original_form": "맞아요 먹는 거 진짜 중요하죠 여행에서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:07"
        },
        {
          "id": "MDRW2100034790.1.17",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100034790.1.18",
          "form": "싸움의 시작은 음식 취향이라해도 과언이 아니라 생각합니닷",
          "original_form": "싸움의 시작은 음식 취향이라해도 과언이 아니라 생각합니닷",
          "speaker_id": "1",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100034790.1.19",
          "form": "마자요... 예전에 만났던 전남친도 너무 편식이 심해서 좀 식당찾기 너무 어렵더라구요 ㅋㅋㅋ",
          "original_form": "마자요... 예전에 만났던 전남친도 너무 편식이 심해서 좀 식당찾기 너무 어렵더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100034790.1.20",
          "form": "아..ㅋㅋ 그건 힘드셨겠어요",
          "original_form": "아..ㅋㅋ 그건 힘드셨겠어요",
          "speaker_id": "1",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100034790.1.21",
          "form": "편식하시나요?",
          "original_form": "편식하시나요?",
          "speaker_id": "2",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100034790.1.22",
          "form": "야채는 좀 하는편이에욥..ㅎㅎ",
          "original_form": "야채는 좀 하는편이에욥..ㅎㅎ",
