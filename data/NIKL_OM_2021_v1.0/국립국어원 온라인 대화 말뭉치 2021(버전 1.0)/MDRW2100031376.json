      "utterance": [
        {
          "id": "MDRW2100031376.1.1",
          "form": "백신 맞은 팔은 멀쩡해졌는데",
          "original_form": "백신 맞은 팔은 멀쩡해졌는데",
          "speaker_id": "1",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031376.1.2",
          "form": "2차.. 완전 걱정되지않냐...",
          "original_form": "2차.. 완전 걱정되지않냐...",
          "speaker_id": "1",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031376.1.3",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031376.1.4",
          "form": "1차는 이만하면 무난히 지나간것 같은데",
          "original_form": "1차는 이만하면 무난히 지나간것 같은데",
          "speaker_id": "2",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031376.1.5",
          "form": "2차때 아플 확률이 크다고 하니..",
          "original_form": "2차때 아플 확률이 크다고 하니..",
          "speaker_id": "2",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100031376.1.6",
          "form": "아 난 2차 접종일이 대체공휴일이라 날짜 3일 당겨졌다",
          "original_form": "아 난 2차 접종일이 대체공휴일이라 날짜 3일 당겨졌다",
          "speaker_id": "2",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100031376.1.7",
          "form": "잉 땡겨졌어?",
          "original_form": "잉 땡겨졌어?",
          "speaker_id": "1",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100031376.1.8",
          "form": "ㅇㅇ 원래 10월4일이었는데",
          "original_form": "ㅇㅇ 원래 10월4일이었는데",
          "speaker_id": "2",
          "time": "20210904 21:00"
        },
        {
          "id": "MDRW2100031376.1.9",
          "form": "자동으로 예약날짜 변경되서 문자오더라고 ㅋㅋㅋ",
          "original_form": "자동으로 예약날짜 변경되서 문자오더라고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:00"
        },
        {
          "id": "MDRW2100031376.1.10",
          "form": "넌 당겨 맞았자네...",
          "original_form": "넌 당겨 맞았자네...",
          "speaker_id": "1",
          "time": "20210904 21:01"
        },
        {
          "id": "MDRW2100031376.1.11",
          "form": "나도 2차건 잔여백신 노려야할까봐",
          "original_form": "나도 2차건 잔여백신 노려야할까봐",
          "speaker_id": "1",
          "time": "20210904 21:01"
        },
        {
          "id": "MDRW2100031376.1.12",
          "form": "10부제라 줄줄 밀려서",
          "original_form": "10부제라 줄줄 밀려서",
          "speaker_id": "1",
          "time": "20210904 21:01"
        },
        {
          "id": "MDRW2100031376.1.13",
          "form": "날짜가 어디까지 밀리는지 모르겠어",
          "original_form": "날짜가 어디까지 밀리는지 모르겠어",
          "speaker_id": "1",
          "time": "20210904 21:01"
        },
        {
          "id": "MDRW2100031376.1.14",
          "form": "1차도 당겨맞긴 했는데",
          "original_form": "1차도 당겨맞긴 했는데",
          "speaker_id": "2",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031376.1.15",
          "form": "1차 맞으면 2차는 자동으로 예약되니까",
          "original_form": "1차 맞으면 2차는 자동으로 예약되니까",
          "speaker_id": "2",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031376.1.16",
          "form": "2차도 잔여백신으로 맞을수 있데?",
          "original_form": "2차도 잔여백신으로 맞을수 있데?",
          "speaker_id": "2",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031376.1.17",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031376.1.18",
          "form": "된데",
          "original_form": "된데",
          "speaker_id": "1",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031376.1.19",
          "form": "4주 지난 후에 하면 할 수 이ㅛ데",
          "original_form": "4주 지난 후에 하면 할 수 이ㅛ데",
          "speaker_id": "1",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031376.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 21:06"
        },
        {
          "id": "MDRW2100031376.1.21",
          "form": "최소 4주는 지나야되서 그런가보구만",
          "original_form": "최소 4주는 지나야되서 그런가보구만",
          "speaker_id": "2",
          "time": "20210904 21:06"
        },
        {
          "id": "MDRW2100031376.1.22",
          "form": "ㄴㄴ 원래는 2주만에 맞아야해",
          "original_form": "ㄴㄴ 원래는 2주만에 맞아야해",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031376.1.23",
          "form": "보급량때문에 4주였는데",
          "original_form": "보급량때문에 4주였는데",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031376.1.24",
          "form": "그나마도 예약자체는 6주야",
          "original_form": "그나마도 예약자체는 6주야",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031376.1.25",
          "form": "그래서 땡겨 맞을라면 4주차 무렵에",
          "original_form": "그래서 땡겨 맞을라면 4주차 무렵에",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031376.1.26",
          "form": "잔여잡으려는거지",
          "original_form": "잔여잡으려는거지",
          "speaker_id": "1",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031376.1.27",
          "form": "엥 아냐",
          "original_form": "엥 아냐",
          "speaker_id": "2",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031376.1.28",
          "form": "원래 화이자가 3주 모더나가 4주였는데",
          "original_form": "원래 화이자가 3주 모더나가 4주였는데",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031376.1.29",
          "form": "보급 문제랑 이러저러 해가지고 밀리면서 6주까지 늦춰진걸로 알고있음",
          "original_form": "보급 문제랑 이러저러 해가지고 밀리면서 6주까지 늦춰진걸로 알고있음",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031376.1.30",
          "form": "엥 아니야 화이자도 2주임",
          "original_form": "엥 아니야 화이자도 2주임",
          "speaker_id": "1",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031376.1.31",
          "form": "고3들 2주간격으로 맞았어",
          "original_form": "고3들 2주간격으로 맞았어",
          "speaker_id": "1",
          "time": "20210904 21:20"
        },
        {
          "id": "MDRW2100031376.1.32",
          "form": "잉",
          "original_form": "잉",
          "speaker_id": "2",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100031376.1.33",
          "form": "권고는 3주로 알고 있는데",
          "original_form": "권고는 3주로 알고 있는데",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031376.1.34",
          "form": "3~4주가 권장 접종 간격이라고 들었음",
          "original_form": "3~4주가 권장 접종 간격이라고 들었음",
          "speaker_id": "2",
          "time": "20210904 21:23"
        }
      ]
