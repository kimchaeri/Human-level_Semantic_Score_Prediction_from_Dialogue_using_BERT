      "utterance": [
        {
          "id": "MDRW2100035473.1.1",
          "form": "뭐 재밌는거 보고 있나 ?",
          "original_form": "뭐 재밌는거 보고 있나 ?",
          "speaker_id": "1",
          "time": "20210830 15:46"
        },
        {
          "id": "MDRW2100035473.1.2",
          "form": "월요일인데 티비 재밌거 하겠나 주말에도 안하는데",
          "original_form": "월요일인데 티비 재밌거 하겠나 주말에도 안하는데",
          "speaker_id": "2",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100035473.1.3",
          "form": "그렇지 뭐. 여전히 티비조서 뉴스다",
          "original_form": "그렇지 뭐. 여전히 티비조서 뉴스다",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100035473.1.4",
          "form": "난 안싸우면 다행이야하길래 틀어봤는데",
          "original_form": "난 안싸우면 다행이야하길래 틀어봤는데",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100035473.1.5",
          "form": "ㅋㅋㅋ 예능 제목들이 왜이래",
          "original_form": "ㅋㅋㅋ 예능 제목들이 왜이래",
          "speaker_id": "1",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100035473.1.6",
          "form": "이번에 개그맨들이 출연이라서 안볼란다",
          "original_form": "이번에 개그맨들이 출연이라서 안볼란다",
          "speaker_id": "2",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100035473.1.7",
          "form": "아 저번에 안정환 나온거 맞지?",
          "original_form": "아 저번에 안정환 나온거 맞지?",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100035473.1.8",
          "form": "엉 그 500원 꽃거지로 나온 개그맨과 그일행들",
          "original_form": "엉 그 500원 꽃거지로 나온 개그맨과 그일행들",
          "speaker_id": "2",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100035473.1.9",
          "form": "아 허닭 갑자기 이름이 기억 안나네ㅋ",
          "original_form": "아 허닭 갑자기 이름이 기억 안나네ㅋ",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100035473.1.10",
          "form": "엉 무슨 경환이엇던거 같은데",
          "original_form": "엉 무슨 경환이엇던거 같은데",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100035473.1.11",
          "form": "허경환이네ㅋ",
          "original_form": "허경환이네ㅋ",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100035473.1.12",
          "form": "맞다 ㅋㅋ 허경환 꼭 한글자가 말썽이다",
          "original_form": "맞다 ㅋㅋ 허경환 꼭 한글자가 말썽이다",
          "speaker_id": "2",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100035473.1.13",
          "form": "난 닭가슴살 파는거 밖에 기억 안나드라",
          "original_form": "난 닭가슴살 파는거 밖에 기억 안나드라",
