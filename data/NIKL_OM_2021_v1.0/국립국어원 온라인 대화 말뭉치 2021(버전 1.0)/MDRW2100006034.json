      "utterance": [
        {
          "id": "MDRW2100006034.1.1",
          "form": "콘서트 가보셨나요",
          "original_form": "콘서트 가보셨나요",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100006034.1.2",
          "form": "넹 ㅋㅋㅋ아이돌",
          "original_form": "넹 ㅋㅋㅋ아이돌",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100006034.1.3",
          "form": "그러시군요 최근 보신 영화 있낭ㅅ",
          "original_form": "그러시군요 최근 보신 영화 있낭ㅅ",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100006034.1.4",
          "form": "나요",
          "original_form": "나요",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100006034.1.5",
          "form": "모가디슈요",
          "original_form": "모가디슈요",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006034.1.6",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006034.1.7",
          "form": "저두요 ㅎㅎ 마지막 영화입니다",
          "original_form": "저두요 ㅎㅎ 마지막 영화입니다",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006034.1.8",
          "form": "ㅋㅋ 재밋엇죠?",
          "original_form": "ㅋㅋ 재밋엇죠?",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006034.1.9",
          "form": "네 만족스럽더라구요",
          "original_form": "네 만족스럽더라구요",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006034.1.10",
          "form": "ㅋㅋ 오즘 영화 넘 비싸죠",
          "original_form": "ㅋㅋ 오즘 영화 넘 비싸죠",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006034.1.11",
          "form": "요즘",
          "original_form": "요즘",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006034.1.12",
          "form": "네.조조가 만원가까운거보고 놀랐네요",
          "original_form": "네.조조가 만원가까운거보고 놀랐네요",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006034.1.13",
          "form": "진짜ㅠㅠ 사람이 없어서 장사가  안될거같긴하지만",
          "original_form": "진짜ㅠㅠ 사람이 없어서 장사가  안될거같긴하지만",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100006034.1.14",
          "form": "아 맞네요..그부분을 생각하면 그런데 과거를 생각하면 넘 비싸죠..",
          "original_form": "아 맞네요..그부분을 생각하면 그런데 과거를 생각하면 넘 비싸죠..",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100006034.1.15",
          "form": "마자요 그래서 잘 안가요 무료티켓없으면 ㅋㅋ",
          "original_form": "마자요 그래서 잘 안가요 무료티켓없으면 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006034.1.16",
          "form": "무료티켓이 어떤 이유로 생기나요?",
          "original_form": "무료티켓이 어떤 이유로 생기나요?",
