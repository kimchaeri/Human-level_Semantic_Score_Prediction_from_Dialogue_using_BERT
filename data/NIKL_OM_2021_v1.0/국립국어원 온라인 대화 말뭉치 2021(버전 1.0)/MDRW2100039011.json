          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.14",
          "form": "완전별로",
          "original_form": "완전별로",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.15",
          "form": "진짜이율ㄹ 들었겠지",
          "original_form": "진짜이율ㄹ 들었겠지",
          "speaker_id": "2",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100039011.1.16",
          "form": "그랬더니 또 찾아와서",
          "original_form": "그랬더니 또 찾아와서",
          "speaker_id": "2",
          "time": "20210903 01:17"
        },
