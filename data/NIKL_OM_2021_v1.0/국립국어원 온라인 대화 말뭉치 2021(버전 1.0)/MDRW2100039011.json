        },
        {
          "id": "MDRW2100039011.1.2",
          "form": "계속 설득하더래",
          "original_form": "계속 설득하더래",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.3",
          "form": "아 짜증나",
          "original_form": "아 짜증나",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.4",
          "form": "엄마들도 문제다",
          "original_form": "엄마들도 문제다",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.5",
          "form": "둘이 데이트할때",
          "original_form": "둘이 데이트할때",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.6",
          "form": "였나",
          "original_form": "였나",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.8",
          "form": "뭐 담달 결혼을",
          "original_form": "뭐 담달 결혼을",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.9",
          "form": "하자는둥",
          "original_form": "하자는둥",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.10",
          "form": "몸만오라는둥",
          "original_form": "몸만오라는둥",
          "speaker_id": "2",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.11",
          "form": "설레발",
          "original_form": "설레발",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100039011.1.12",
          "form": "그런 얘기를 했대",
          "original_form": "그런 얘기를 했대",
