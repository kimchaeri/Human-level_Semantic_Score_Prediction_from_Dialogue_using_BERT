      "utterance": [
        {
          "id": "MDRW2100019386.1.1",
          "form": "넌 집안일 중에서 뭐가 제일 싫어",
          "original_form": "넌 집안일 중에서 뭐가 제일 싫어",
          "speaker_id": "2",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100019386.1.2",
          "form": "난 닦기..",
          "original_form": "난 닦기..",
          "speaker_id": "2",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100019386.1.3",
          "form": "빨래개기..",
          "original_form": "빨래개기..",
          "speaker_id": "1",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100019386.1.4",
          "form": "집안일분담은",
          "original_form": "집안일분담은",
          "speaker_id": "1",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100019386.1.5",
          "form": "어케하는편이야",
          "original_form": "어케하는편이야",
          "speaker_id": "1",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100019386.1.6",
          "form": "설거지랑 빨래개기 청소는 내가 하고",
          "original_form": "설거지랑 빨래개기 청소는 내가 하고",
          "speaker_id": "2",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100019386.1.7",
          "form": "밥하고 빨래 돌리는거 이런건 엄마가 ㅇㅇ",
          "original_form": "밥하고 빨래 돌리는거 이런건 엄마가 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100019386.1.8",
          "form": "아빠는 아주 드물게...정말 드물게...청소 정도?",
          "original_form": "아빠는 아주 드물게...정말 드물게...청소 정도?",
          "speaker_id": "2",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100019386.1.9",
          "form": "와 똑같애",
          "original_form": "와 똑같애",
          "speaker_id": "1",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100019386.1.10",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100019386.1.11",
          "form": "어머니는 반찬 얼마나자주하셔",
          "original_form": "어머니는 반찬 얼마나자주하셔",
          "speaker_id": "1",
          "time": "20210827 18:43"
        },
        {
          "id": "MDRW2100019386.1.12",
          "form": "떨어지면??",
          "original_form": "떨어지면??",
          "speaker_id": "2",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100019386.1.13",
          "form": "대용량으로",
          "original_form": "대용량으로",
          "speaker_id": "1",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019386.1.14",
          "form": "근데 부모님이 나물을 엄청 좋아하시는데다",
          "original_form": "근데 부모님이 나물을 엄청 좋아하시는데다",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019386.1.15",
          "form": "만드시는편?",
          "original_form": "만드시는편?",
          "speaker_id": "1",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019386.1.16",
          "form": "친가나 외가에서 뭐 보내주면 그거로 또 만드심",
          "original_form": "친가나 외가에서 뭐 보내주면 그거로 또 만드심",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019386.1.17",
          "form": "ㅇㅇ 좀?",
          "original_form": "ㅇㅇ 좀?",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019386.1.18",
          "form": "ㅋㅋㅋ 드시는거만계속 드시는편이야?",
          "original_form": "ㅋㅋㅋ 드시는거만계속 드시는편이야?",
          "speaker_id": "1",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019386.1.19",
          "form": "새로운시도해보시진않음?",
          "original_form": "새로운시도해보시진않음?",
          "speaker_id": "1",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019386.1.20",
          "form": "어 보통 먹던거만 ㅋㅋㅋ",
          "original_form": "어 보통 먹던거만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019386.1.21",
          "form": "우리엄만요새",
          "original_form": "우리엄만요새",
          "speaker_id": "1",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019386.1.22",
          "form": "유튜브보고 이것저것 해보던데",
          "original_form": "유튜브보고 이것저것 해보던데",
          "speaker_id": "1",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019386.1.23",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019386.1.24",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019386.1.25",
          "form": "울엄마는 전에 함 유튜브 보고 국물김치 만들었다가",
          "original_form": "울엄마는 전에 함 유튜브 보고 국물김치 만들었다가",
          "speaker_id": "2",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019386.1.26",
          "form": "엄마 레피시보다 맛없다고 ㅋㅋㅋ",
          "original_form": "엄마 레피시보다 맛없다고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019386.1.27",
          "form": "그뒤로 안보더라 다시",
          "original_form": "그뒤로 안보더라 다시",
          "speaker_id": "2",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019386.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019386.1.29",
          "form": "근데편차는확실히있더라",
          "original_form": "근데편차는확실히있더라",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019386.1.30",
          "form": "존맛도있고",
          "original_form": "존맛도있고",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019386.1.31",
          "form": "개노맛도있고",
          "original_form": "개노맛도있고",
          "speaker_id": "1",
          "time": "20210827 19:26"
        }
      ]
