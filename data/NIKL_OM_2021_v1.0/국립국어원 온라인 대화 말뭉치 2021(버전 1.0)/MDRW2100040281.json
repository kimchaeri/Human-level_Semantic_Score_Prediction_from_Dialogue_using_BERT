      "utterance": [
        {
          "id": "MDRW2100040281.1.1",
          "form": "죽이고 싶은놈 천지죠",
          "original_form": "죽이고 싶은놈 천지죠",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040281.1.2",
          "form": "갑자기 살남 욕구가...",
          "original_form": "갑자기 살남 욕구가...",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040281.1.3",
          "form": "운동하는거 있으세요?",
          "original_form": "운동하는거 있으세요?",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040281.1.4",
          "form": "ㅋㅋㅋ살남ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ살남ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040281.1.5",
          "form": "네네! 저 얼마 전부터",
          "original_form": "네네! 저 얼마 전부터",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040281.1.6",
          "form": "필라테스 시작했어요 ㅎㅎㅎ",
          "original_form": "필라테스 시작했어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040281.1.7",
          "form": "와 1대1이요?",
          "original_form": "와 1대1이요?",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040281.1.8",
          "form": "사실 저 이제 남자 안좋아하거든요,,",
          "original_form": "사실 저 이제 남자 안좋아하거든요,,",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040281.1.9",
          "form": "아뇨아뇨 가성비 넘치게 8대 1이요~!~!!",
          "original_form": "아뇨아뇨 가성비 넘치게 8대 1이요~!~!!",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040281.1.10",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040281.1.11",
          "form": "네넼ㅋㅋㅋ 아직 가난한 대학원생이라....",
          "original_form": "네넼ㅋㅋㅋ 아직 가난한 대학원생이라....",
          "speaker_id": "1",
          "time": "20210904 15:25"
        },
        {
          "id": "MDRW2100040281.1.12",
          "form": "하지만 8대 1로도 만족하는 중이에요!!!",
          "original_form": "하지만 8대 1로도 만족하는 중이에요!!!",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040281.1.13",
          "form": "저도 필라테스 관심있는데",
          "original_form": "저도 필라테스 관심있는데",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040281.1.14",
          "form": "요즘 코로나땜에 샤워장 폐쇄해서",
          "original_form": "요즘 코로나땜에 샤워장 폐쇄해서",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040281.1.15",
          "form": "고민중이에요",
          "original_form": "고민중이에요",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040281.1.16",
          "form": "앗 그쵸!!!",
          "original_form": "앗 그쵸!!!",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040281.1.17",
          "form": "근데 이제 날씨가 선선해서",
          "original_form": "근데 이제 날씨가 선선해서",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040281.1.18",
          "form": "집 가는 길에 땀 식히면서 가요 (?)",
          "original_form": "집 가는 길에 땀 식히면서 가요 (?)",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040281.1.19",
          "form": "ㅋㅋㅋㅋ 운동의 꽃은 끝나고샤워하는건데",
          "original_form": "ㅋㅋㅋㅋ 운동의 꽃은 끝나고샤워하는건데",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040281.1.20",
          "form": "아 그런건가요!!",
          "original_form": "아 그런건가요!!",
          "speaker_id": "1",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040281.1.21",
          "form": "제가 운동을",
          "original_form": "제가 운동을",
          "speaker_id": "1",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040281.1.22",
          "form": "그재미가없다니 흑흑ㅠㅠ",
          "original_form": "그재미가없다니 흑흑ㅠㅠ",
