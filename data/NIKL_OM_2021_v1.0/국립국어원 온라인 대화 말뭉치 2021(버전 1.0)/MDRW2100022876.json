      "utterance": [
        {
          "id": "MDRW2100022876.1.1",
          "form": "여기 섬나라야",
          "original_form": "여기 섬나라야",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022876.1.2",
          "form": "왜 섬나라야?",
          "original_form": "왜 섬나라야?",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022876.1.3",
          "form": "삼에 군대가이써",
          "original_form": "삼에 군대가이써",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022876.1.4",
          "form": "아 군대구나 거기서 뭐해?",
          "original_form": "아 군대구나 거기서 뭐해?",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022876.1.5",
          "form": "서북도서 절대사수",
          "original_form": "서북도서 절대사수",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022876.1.6",
          "form": "그게 뭐야?",
          "original_form": "그게 뭐야?",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022876.1.7",
          "form": "백령도 대청도 소청도 연평도 우도를 지키지요",
          "original_form": "백령도 대청도 소청도 연평도 우도를 지키지요",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.8",
          "form": "섬을 지키는거야?",
          "original_form": "섬을 지키는거야?",
          "speaker_id": "1",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.9",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.10",
          "form": "너는 어디야",
          "original_form": "너는 어디야",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.11",
          "form": "오늘은 뭐했어?",
          "original_form": "오늘은 뭐했어?",
          "speaker_id": "1",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.12",
          "form": "나는 집이야",
          "original_form": "나는 집이야",
          "speaker_id": "1",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.13",
          "form": "오늘은 휴일과업했어",
          "original_form": "오늘은 휴일과업했어",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.14",
          "form": "휴일과업이 뭐야?",
          "original_form": "휴일과업이 뭐야?",
          "speaker_id": "1",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.15",
          "form": "아무것도한게없어",
          "original_form": "아무것도한게없어",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.16",
          "form": "폰만 주구장창",
          "original_form": "폰만 주구장창",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.17",
          "form": "아 일 쉬는 날이야(",
          "original_form": "아 일 쉬는 날이야(",
          "speaker_id": "1",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.18",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022876.1.19",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022876.1.20",
          "form": "넌 군필이야?",
          "original_form": "넌 군필이야?",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022876.1.21",
          "form": "주말에 쉬는거야?",
          "original_form": "주말에 쉬는거야?",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022876.1.22",
          "form": "나 여자야",
          "original_form": "나 여자야",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022876.1.23",
          "form": "그치 주말에는 셔",
          "original_form": "그치 주말에는 셔",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022876.1.24",
          "form": "평일에는 모해?",
          "original_form": "평일에는 모해?",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022876.1.25",
          "form": "평일에는 작업!",
          "original_form": "평일에는 작업!",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022876.1.26",
          "form": "무슨 작업?",
          "original_form": "무슨 작업?",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022876.1.27",
          "form": "그냥 제초하고 이것저것",
          "original_form": "그냥 제초하고 이것저것",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022876.1.28",
          "form": "제초가 뭐야?",
          "original_form": "제초가 뭐야?",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022876.1.29",
          "form": "풀베는거 바보야",
          "original_form": "풀베는거 바보야",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022876.1.30",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210829 18:49"
        }
      ]
