      "utterance": [
        {
          "id": "MDRW2100034921.1.1",
          "form": "넹넹! 저 부대찌개 먹었어용",
          "original_form": "넹넹! 저 부대찌개 먹었어용",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034921.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034921.1.3",
          "form": "우왕 맛있겠네요",
          "original_form": "우왕 맛있겠네요",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034921.1.4",
          "form": "저는 칼국수요",
          "original_form": "저는 칼국수요",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034921.1.5",
          "form": "든든한 한국음식이네요 부대찌개 ㅋㅋ",
          "original_form": "든든한 한국음식이네요 부대찌개 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100034921.1.6",
          "form": "앗 칼국수 고기들어간거 드셨어요 아님 바지락 아님 야채 드셨어요?",
          "original_form": "앗 칼국수 고기들어간거 드셨어요 아님 바지락 아님 야채 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100034921.1.7",
          "form": "저 칼국수 완전 좋아해요ㅠㅜ",
          "original_form": "저 칼국수 완전 좋아해요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100034921.1.8",
          "form": "바지락인데",
          "original_form": "바지락인데",
          "speaker_id": "1",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100034921.1.9",
          "form": "샘표에서 라면처럼나온 제품이에요",
          "original_form": "샘표에서 라면처럼나온 제품이에요",
          "speaker_id": "1",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100034921.1.10",
          "form": "어머 저 처음알았어요ㅋㅋㅋㅋ",
          "original_form": "어머 저 처음알았어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100034921.1.11",
          "form": "어디서 구매하셨어요?",
          "original_form": "어디서 구매하셨어요?",
          "speaker_id": "2",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100034921.1.12",
          "form": "이마트?",
          "original_form": "이마트?",
          "speaker_id": "2",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100034921.1.13",
          "form": "내일 당장 사러갑니다",
          "original_form": "내일 당장 사러갑니다",
          "speaker_id": "2",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100034921.1.14",
          "form": "싸구 편해요",
          "original_form": "싸구 편해요",
          "speaker_id": "1",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100034921.1.15",
          "form": "네 이마트에",
          "original_form": "네 이마트에",
          "speaker_id": "1",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100034921.1.16",
          "form": "팔아요 천원씩이라 싸구 맛있어요",
          "original_form": "팔아요 천원씩이라 싸구 맛있어요",
          "speaker_id": "1",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100034921.1.17",
          "form": "오 감사해요ㅋㅋㅋㅋ 오늘 날이 우중충해서 안그래도 칼국수 땡겼거든요",
          "original_form": "오 감사해요ㅋㅋㅋㅋ 오늘 날이 우중충해서 안그래도 칼국수 땡겼거든요",
          "speaker_id": "2",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100034921.1.18",
          "form": "아니 지금사시게요?",
          "original_form": "아니 지금사시게요?",
          "speaker_id": "1",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034921.1.19",
          "form": "문닫았는디 ㅋㅋ",
          "original_form": "문닫았는디 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034921.1.20",
          "form": "아 내일 사러 가려구요ㅋㅋㅋㅋ",
          "original_form": "아 내일 사러 가려구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034921.1.21",
          "form": "이마트 피자도 맛있는데",
          "original_form": "이마트 피자도 맛있는데",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034921.1.22",
          "form": "아 그래요",
          "original_form": "아 그래요",
          "speaker_id": "1",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034921.1.23",
          "form": "가는길에 겸사겸사 사야겠어요",
          "original_form": "가는길에 겸사겸사 사야겠어요",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034921.1.24",
          "form": "가성비 소문들었어요",
          "original_form": "가성비 소문들었어요",
          "speaker_id": "1",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034921.1.25",
          "form": "엄청 커요ㅋㅋㅋ 저는 소분해서 냉동실에 얼려두고 먹어요",
          "original_form": "엄청 커요ㅋㅋㅋ 저는 소분해서 냉동실에 얼려두고 먹어요",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100034921.1.26",
          "form": "아 그런방법도 좋네요",
          "original_form": "아 그런방법도 좋네요",
          "speaker_id": "1",
          "time": "20210828 22:48"
        }
      ]
    }
