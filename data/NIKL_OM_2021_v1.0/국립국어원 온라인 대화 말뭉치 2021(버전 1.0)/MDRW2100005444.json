      "utterance": [
        {
          "id": "MDRW2100005444.1.1",
          "form": "평소에 다이어트 하는 편이에요?",
          "original_form": "평소에 다이어트 하는 편이에요?",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005444.1.2",
          "form": "다어이트 하는걸 귀찮아서 안하고 있어요",
          "original_form": "다어이트 하는걸 귀찮아서 안하고 있어요",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005444.1.3",
          "form": "건강관리는 하고 계세요?",
          "original_form": "건강관리는 하고 계세요?",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005444.1.4",
          "form": "빼는건 지옥같아요",
          "original_form": "빼는건 지옥같아요",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005444.1.5",
          "form": "건강관리는 평소에 하고있어서 몸에 문제 있는건 없어요",
          "original_form": "건강관리는 평소에 하고있어서 몸에 문제 있는건 없어요",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005444.1.6",
          "form": "아 평소에 건강하신가봐요",
          "original_form": "아 평소에 건강하신가봐요",
          "speaker_id": "1",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005444.1.7",
          "form": "그쪽은 건강하다고 생각안하시나요?",
          "original_form": "그쪽은 건강하다고 생각안하시나요?",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005444.1.8",
          "form": "되게 건강한거같아요",
          "original_form": "되게 건강한거같아요",
          "speaker_id": "1",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005444.1.9",
          "form": "혹시 성형에 관심있으세요?",
          "original_form": "혹시 성형에 관심있으세요?",
          "speaker_id": "1",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005444.1.10",
          "form": "성형엔 관심이 하나도 없어요 어떻게 건강관리 하시나요?",
          "original_form": "성형엔 관심이 하나도 없어요 어떻게 건강관리 하시나요?",
          "speaker_id": "2",
