      "utterance": [
        {
          "id": "MDRW2100006787.1.1",
          "form": "제철음식 챙겨드리나요?",
          "original_form": "제철음식 챙겨드리나요?",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100006787.1.2",
          "form": "제철음식을 좋아하는 편입니다",
          "original_form": "제철음식을 좋아하는 편입니다",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100006787.1.3",
          "form": "사실 요즘은 4계절이 무색하게 아무때나  먹거리가 나오지만",
          "original_form": "사실 요즘은 4계절이 무색하게 아무때나  먹거리가 나오지만",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100006787.1.4",
          "form": "그래도 제철에 먹어야 맛이 있지요",
          "original_form": "그래도 제철에 먹어야 맛이 있지요",
          "speaker_id": "2",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100006787.1.5",
          "form": "그렇죠ㅡ 챙겨드시는거 있나요? ㅎㅎ",
          "original_form": "그렇죠ㅡ 챙겨드시는거 있나요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:28"
        },
        {
          "id": "MDRW2100006787.1.6",
          "form": "아직 감이 안나왔는데 저는 감을 아주 좋아합니다",
          "original_form": "아직 감이 안나왔는데 저는 감을 아주 좋아합니다",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100006787.1.7",
          "form": "단감 홍시 곶감 다 좋아하지요",
          "original_form": "단감 홍시 곶감 다 좋아하지요",
          "speaker_id": "2",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100006787.1.8",
          "form": "그 중 제일은요??",
          "original_form": "그 중 제일은요??",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100006787.1.9",
          "form": "곶감 좋아하지요",
          "original_form": "곶감 좋아하지요",
          "speaker_id": "2",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100006787.1.10",
          "form": "어디서 사드세요??",
          "original_form": "어디서 사드세요??",
          "speaker_id": "1",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100006787.1.11",
          "form": "저희는 친척중에 농사짓는 분이 계셔서 보내줍니다",
          "original_form": "저희는 친척중에 농사짓는 분이 계셔서 보내줍니다",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100006787.1.12",
          "form": "좋으시겠어요 곶감 고급 음식이잖습니까",
          "original_form": "좋으시겠어요 곶감 고급 음식이잖습니까",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100006787.1.13",
          "form": "그렇지요",
          "original_form": "그렇지요",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006787.1.14",
          "form": "감사하고 있습니다",
          "original_form": "감사하고 있습니다",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006787.1.15",
          "form": "항상 먹거리를 자주 보내주세요",
          "original_form": "항상 먹거리를 자주 보내주세요",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006787.1.16",
          "form": "또 어떤걸 보내주시나요? 쌀?",
          "original_form": "또 어떤걸 보내주시나요? 쌀?",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100006787.1.17",
          "form": "맞아요 쌀은 해마다 보내주셔서 돈주고 안사먹고 있습니다",
          "original_form": "맞아요 쌀은 해마다 보내주셔서 돈주고 안사먹고 있습니다",
          "speaker_id": "2",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100006787.1.18",
          "form": "아 든든하시겠어요",
          "original_form": "아 든든하시겠어요",
          "speaker_id": "1",
          "time": "20210908 09:47"
        }
      ]
