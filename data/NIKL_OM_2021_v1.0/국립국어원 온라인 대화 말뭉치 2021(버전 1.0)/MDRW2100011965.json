        },
        {
          "id": "MDRW2100011965.1.2",
          "form": "나 무화과 먹고 싶어",
          "original_form": "나 무화과 먹고 싶어",
          "speaker_id": "1",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100011965.1.3",
          "form": "사먹어 ㅎㅎ",
          "original_form": "사먹어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011965.1.4",
          "form": "그래서 요즘 배민지켜보고 있어",
          "original_form": "그래서 요즘 배민지켜보고 있어",
          "speaker_id": "1",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011965.1.5",
          "form": "근데 난 식감이 내 스타일 아냐",
          "original_form": "근데 난 식감이 내 스타일 아냐",
          "speaker_id": "2",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011965.1.6",
          "form": "그럴 거 같기는 해",
          "original_form": "그럴 거 같기는 해",
          "speaker_id": "1",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011965.1.7",
          "form": "맛은 괜찮았는데",
          "original_form": "맛은 괜찮았는데",
          "speaker_id": "2",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011965.1.8",
          "form": "그래? 난 싫어할 줄 알았어",
          "original_form": "그래? 난 싫어할 줄 알았어",
          "speaker_id": "1",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011965.1.9",
          "form": "근데 내 돈주고 안사먹음 누가 사주면 먹고",
          "original_form": "근데 내 돈주고 안사먹음 누가 사주면 먹고",
          "speaker_id": "2",
          "time": "20210824 17:53"
        },
        {
          "id": "MDRW2100011965.1.10",
          "form": "누가 무화과를 사줘",
          "original_form": "누가 무화과를 사줘",
          "speaker_id": "1",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011965.1.11",
          "form": "ㅋㅋㅋㅋㅋ 사줄 사람 구함",
          "original_form": "ㅋㅋㅋㅋㅋ 사줄 사람 구함",
          "speaker_id": "2",
          "time": "20210824 17:54"
        },
        {
          "id": "MDRW2100011965.1.12",
          "form": "속히 구해지길 바람",
          "original_form": "속히 구해지길 바람",
          "speaker_id": "1",
          "time": "20210824 17:54"
        },
