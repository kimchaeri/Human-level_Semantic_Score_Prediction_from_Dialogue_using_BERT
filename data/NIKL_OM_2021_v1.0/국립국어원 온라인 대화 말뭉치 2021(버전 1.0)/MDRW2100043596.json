      "utterance": [
        {
          "id": "MDRW2100043596.1.1",
          "form": "남자들은 지가 안낳으니 쉽게 말하죠",
          "original_form": "남자들은 지가 안낳으니 쉽게 말하죠",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043596.1.2",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043596.1.3",
          "form": "동성이시면 기저귀만 사시면 되겠군요 ㅋㅋㅋ",
          "original_form": "동성이시면 기저귀만 사시면 되겠군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043596.1.4",
          "form": "그래도 힘들다고 하면 첫째 챙겨주니 그걸로 만족하려구요",
          "original_form": "그래도 힘들다고 하면 첫째 챙겨주니 그걸로 만족하려구요",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043596.1.5",
          "form": "네~ 형아 입던거 물려입을 수 있을꺼 같아요",
          "original_form": "네~ 형아 입던거 물려입을 수 있을꺼 같아요",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043596.1.6",
          "form": "그런점이 동성의 좋은 점이죠 ㅎㅎ",
          "original_form": "그런점이 동성의 좋은 점이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043596.1.7",
          "form": "둘이 노는 시간이 많았으면 좋겠어요",
          "original_form": "둘이 노는 시간이 많았으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043596.1.8",
          "form": "저를 찾지 않고 ㅋㅋㅋ",
          "original_form": "저를 찾지 않고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043596.1.9",
          "form": "그러려면 둘째가 두돌은 되야..... ㅠ",
          "original_form": "그러려면 둘째가 두돌은 되야..... ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043596.1.10",
          "form": "아... 둘돌..  멀었다 ㅋㅋㅋ",
          "original_form": "아... 둘돌..  멀었다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043596.1.11",
          "form": "두돌",
          "original_form": "두돌",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043596.1.12",
          "form": "사부님은 애들과 잘 놀아주세요?",
          "original_form": "사부님은 애들과 잘 놀아주세요?",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043596.1.13",
          "form": "지금 몇개월이죠?",
          "original_form": "지금 몇개월이죠?",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043596.1.14",
          "form": "네~ 그나마 남자 셋이 좀 노네요",
          "original_form": "네~ 그나마 남자 셋이 좀 노네요",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043596.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043596.1.16",
          "form": "전 이제 7개월이요",
          "original_form": "전 이제 7개월이요",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043596.1.17",
          "form": "얼마 안남았네요~ 두근두근",
          "original_form": "얼마 안남았네요~ 두근두근",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043596.1.18",
          "form": "진짜 곧...",
          "original_form": "진짜 곧...",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043596.1.19",
