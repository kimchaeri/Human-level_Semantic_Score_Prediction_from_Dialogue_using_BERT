      "utterance": [
        {
          "id": "MDRW2100019258.1.1",
          "form": "저 요즘은 감정 따라가기 벅차서 예능만 보는데",
          "original_form": "저 요즘은 감정 따라가기 벅차서 예능만 보는데",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019258.1.2",
          "form": "유퀴즈는 꼭 챙겨봐요",
          "original_form": "유퀴즈는 꼭 챙겨봐요",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019258.1.3",
          "form": "놀토랑 놀뭐랑 런닝맨도 보고",
          "original_form": "놀토랑 놀뭐랑 런닝맨도 보고",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019258.1.4",
          "form": "저도 유퀴즈 즐겨봐요 ㅋㅋㅋㅋㅋ놀토도 종종보고",
          "original_form": "저도 유퀴즈 즐겨봐요 ㅋㅋㅋㅋㅋ놀토도 종종보고",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019258.1.5",
          "form": "예전 예능도 다시 보고 ㅋㅋㅋㅋㅋ",
          "original_form": "예전 예능도 다시 보고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019258.1.6",
          "form": "유퀴즈 너무 따뜻해서 볼때마다 눈물 줄줄이에요",
          "original_form": "유퀴즈 너무 따뜻해서 볼때마다 눈물 줄줄이에요",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019258.1.7",
          "form": "최근에 인상깊게 본 편 있어요?",
          "original_form": "최근에 인상깊게 본 편 있어요?",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019258.1.8",
          "form": "최근에는 국가대표 하고 있으니 국가대표편도 좋더라구요",
          "original_form": "최근에는 국가대표 하고 있으니 국가대표편도 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019258.1.9",
          "form": "뭔가 인간미가 느껴지고ㅋㅋㅋ",
          "original_form": "뭔가 인간미가 느껴지고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019258.1.10",
          "form": "맞아요 국가대표편 좋았죠",
          "original_form": "맞아요 국가대표편 좋았죠",
          "speaker_id": "1",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019258.1.11",
          "form": "전 원래 스포츠 좋아하거든요",
          "original_form": "전 원래 스포츠 좋아하거든요",
          "speaker_id": "1",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019258.1.12",
          "form": "아 물론 하는거말고 보는거...ㅋㅋㅋㅋㅋ",
          "original_form": "아 물론 하는거말고 보는거...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:50"
        },
        {
          "id": "MDRW2100019258.1.13",
          "form": "어우 이번에 진짜 올림픽 폐인같이 봤거든요",
          "original_form": "어우 이번에 진짜 올림픽 폐인같이 봤거든요",
          "speaker_id": "2",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019258.1.14",
          "form": "그래서 선수들의 진솔한 인터뷰 너무 좋앗어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 선수들의 진솔한 인터뷰 너무 좋앗어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019258.1.15",
          "form": "너무 몰입해서 봐서 인터뷰 너무 좋았어요",
          "original_form": "너무 몰입해서 봐서 인터뷰 너무 좋았어요",
          "speaker_id": "2",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019258.1.16",
          "form": "name2님도 올림픽 폐인이셧구나 ㅋㅋㅋㅋㅋ",
          "original_form": "&name2&님도 올림픽 폐인이셧구나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:51"
        },
        {
          "id": "MDRW2100019258.1.17",
          "form": "진짜 온종일 봤어요",
          "original_form": "진짜 온종일 봤어요",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019258.1.18",
          "form": "특히 재밋게 보신 종목 잇으세요?",
          "original_form": "특히 재밋게 보신 종목 잇으세요?",
          "speaker_id": "1",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019258.1.19",
          "form": "일단 올해는 배구가 너무 재밌었고",
          "original_form": "일단 올해는 배구가 너무 재밌었고",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019258.1.20",
          "form": "하이큐 올해초에 몰아봤더니 룰을 좀 알겠더라구요ㅋㅋㅋㅋ",
          "original_form": "하이큐 올해초에 몰아봤더니 룰을 좀 알겠더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019258.1.21",
          "form": "우상혁 선수도 너무 인상깊었어요",
          "original_form": "우상혁 선수도 너무 인상깊었어요",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019258.1.22",
          "form": "맞아요 배구 너무 재밌었죠",
          "original_form": "맞아요 배구 너무 재밌었죠",
          "speaker_id": "1",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019258.1.23",
          "form": "높이뛰기도 진짜 멋있었어요",
          "original_form": "높이뛰기도 진짜 멋있었어요",
          "speaker_id": "1",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019258.1.24",
          "form": "전 클라이밍도 인상깊게봤어요",
          "original_form": "전 클라이밍도 인상깊게봤어요",
          "speaker_id": "1",
          "time": "20210827 17:13"
        },
        {
          "id": "MDRW2100019258.1.25",
          "form": "그러게요 클라이밍 배우고 싶더라구요ㅋㅋㅋ",
          "original_form": "그러게요 클라이밍 배우고 싶더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100019258.1.26",
          "form": "입 쩍 벌리면서 봣어요 ㅋㅋㅋㅋ심지어 엄청 어린선수여서 ㅋㅋㅋㅋㅋ",
          "original_form": "입 쩍 벌리면서 봣어요 ㅋㅋㅋㅋ심지어 엄청 어린선수여서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:25"
        },
        {
          "id": "MDRW2100019258.1.27",
          "form": "맞아요 완전 애기선수ㅜㅜ 결승이 넘 어려워서 좀 재미없게 되어서 아쉽더라구요",
          "original_form": "맞아요 완전 애기선수ㅜㅜ 결승이 넘 어려워서 좀 재미없게 되어서 아쉽더라구요",
          "speaker_id": "2",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100019258.1.28",
          "form": "맞아요 너무 아쉬웟어요 결승 전까지 너무 좋았는데 ㅠㅠ",
          "original_form": "맞아요 너무 아쉬웟어요 결승 전까지 너무 좋았는데 ㅠㅠ",
