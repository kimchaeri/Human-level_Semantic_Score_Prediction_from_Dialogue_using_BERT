      "utterance": [
        {
          "id": "MDRW2100018123.1.1",
          "form": "공연 좋아하시나여?",
          "original_form": "공연 좋아하시나여?",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018123.1.2",
          "form": "영화관람 좋아해여!",
          "original_form": "영화관람 좋아해여!",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018123.1.3",
          "form": "어떤 장르요?",
          "original_form": "어떤 장르요?",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100018123.1.4",
          "form": "액션이요ㅎㅎ",
          "original_form": "액션이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100018123.1.5",
          "form": "재밌게 본 영화 있으신가요",
          "original_form": "재밌게 본 영화 있으신가요",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100018123.1.6",
          "form": "마블 영화 좋아합니다",
          "original_form": "마블 영화 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100018123.1.7",
          "form": "마블 최애 캐릭 뭔가요",
          "original_form": "마블 최애 캐릭 뭔가요",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100018123.1.8",
          "form": "아이언맨이요ㅠㅜ",
          "original_form": "아이언맨이요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100018123.1.9",
          "form": "아 ㅠㅠ 멋있죠",
          "original_form": "아 ㅠㅠ 멋있죠",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100018123.1.10",
          "form": "전 잘 안봤는데 이름하고는 알아요ㅋㅋ",
          "original_form": "전 잘 안봤는데 이름하고는 알아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100018123.1.11",
          "form": "최근에 어떤영화보셨나요",
          "original_form": "최근에 어떤영화보셨나요",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100018123.1.12",
          "form": "저 아까전에",
          "original_form": "저 아까전에",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100018123.1.13",
          "form": "호텔뭄바이 봤어요..",
          "original_form": "호텔뭄바이 봤어요..",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100018123.1.14",
          "form": "아대박 인도 무서워지는..",
          "original_form": "아대박 인도 무서워지는..",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100018123.1.15",
          "form": "진짜 무서워요 ㅜㅠ",
          "original_form": "진짜 무서워요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210829 12:03"
