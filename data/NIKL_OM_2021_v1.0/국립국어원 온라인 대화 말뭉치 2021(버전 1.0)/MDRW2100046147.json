      "utterance": [
        {
          "id": "MDRW2100046147.1.1",
          "form": "우아 너무 좋겟어요 ㅋㅋㅋㅋㅋ",
          "original_form": "우아 너무 좋겟어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046147.1.2",
          "form": "바베큐!",
          "original_form": "바베큐!",
          "speaker_id": "2",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046147.1.3",
          "form": "뭐 먹기살기위해 하는겁니다",
          "original_form": "뭐 먹기살기위해 하는겁니다",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046147.1.4",
          "form": "전 못하니 ㅠ",
          "original_form": "전 못하니 ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046147.1.5",
          "form": "ㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046147.1.6",
          "form": "정말 부엌이랑 안친하거든요",
          "original_form": "정말 부엌이랑 안친하거든요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046147.1.7",
          "form": "요리 잘하세요??",
          "original_form": "요리 잘하세요??",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046147.1.8",
          "form": "저는 못해욬ㅋㅋㅋ라면 물도 잘 못맞춰요",
          "original_form": "저는 못해욬ㅋㅋㅋ라면 물도 잘 못맞춰요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046147.1.9",
          "form": "ㄹㅋㅋㅋㅋㄱㄱㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㄹㅋㅋㅋㅋㄱㄱㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046147.1.10",
          "form": "나중에 요리 잘하시는 분 만나세요",
          "original_form": "나중에 요리 잘하시는 분 만나세요",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100046147.1.11",
          "form": "오 그래야겟어요 ㅋㅋㅋㅋㅋㅋㅋ 필수네요",
          "original_form": "오 그래야겟어요 ㅋㅋㅋㅋㅋㅋㅋ 필수네요",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046147.1.12",
          "form": "맛집 자주 다니세요?",
          "original_form": "맛집 자주 다니세요?",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046147.1.13",
          "form": "아니요 요새는",
          "original_form": "아니요 요새는",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046147.1.14",
          "form": "코로나 때문에",
          "original_form": "코로나 때문에",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046147.1.15",
          "form": "회사에 나가서 식당을 갈 때 빼고는",
          "original_form": "회사에 나가서 식당을 갈 때 빼고는",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046147.1.16",
          "form": "따로 맛집을 가거나",
          "original_form": "따로 맛집을 가거나",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046147.1.17",
          "form": "외식을 하지는 않아요",
          "original_form": "외식을 하지는 않아요",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046147.1.18",
          "form": "아 맞아요 ㅠㅠ",
          "original_form": "아 맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046147.1.19",
          "form": "코로나 ㅠ",
          "original_form": "코로나 ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046147.1.20",
          "form": "줄스는것도 고생이구요 ㅠ",
          "original_form": "줄스는것도 고생이구요 ㅠ",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046147.1.21",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046147.1.22",
          "form": "줄서서 먹는데는",
          "original_form": "줄서서 먹는데는",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046147.1.23",
          "form": "아예 도전도 할 수가 없어요",
          "original_form": "아예 도전도 할 수가 없어요",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046147.1.24",
          "form": "맞아요 그리고.. 그렇게 맛있지도 않아요 ㅋㅋㅋ",
          "original_form": "맞아요 그리고.. 그렇게 맛있지도 않아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046147.1.25",
          "form": "ㅋㅋㅋㅋㄱㄱㄱㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㄱㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046147.1.26",
          "form": "너무 기대하면",
          "original_form": "너무 기대하면",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046147.1.27",
          "form": "또 그만큼 맛이 없죠",
          "original_form": "또 그만큼 맛이 없죠",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046147.1.28",
          "form": "전 요즘 어탕을 많이 먹으러 갔었는데",
          "original_form": "전 요즘 어탕을 많이 먹으러 갔었는데",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046147.1.29",
          "form": "어탕 드시나요??",
          "original_form": "어탕 드시나요??",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046147.1.30",
          "form": "오 어탕",
          "original_form": "오 어탕",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100046147.1.31",
          "form": "처음들어봐요!",
          "original_form": "처음들어봐요!",
          "speaker_id": "2",
          "time": "20210908 10:49"
        }
      ]
    }
