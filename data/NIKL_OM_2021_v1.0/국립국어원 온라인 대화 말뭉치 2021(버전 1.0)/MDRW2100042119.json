      "utterance": [
        {
          "id": "MDRW2100042119.1.1",
          "form": "아침 일찍 나갔는데 왜 지각했어",
          "original_form": "아침 일찍 나갔는데 왜 지각했어",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042119.1.2",
          "form": "차가 진짜 너무 막히더라",
          "original_form": "차가 진짜 너무 막히더라",
          "speaker_id": "1",
          "time": "20210907 00:07"
        },
        {
          "id": "MDRW2100042119.1.3",
          "form": "원래 안막히면 10분인데 40분 걸렸어",
          "original_form": "원래 안막히면 10분인데 40분 걸렸어",
          "speaker_id": "1",
          "time": "20210907 00:08"
        },
        {
          "id": "MDRW2100042119.1.4",
          "form": "그래서 어떻게 했어",
          "original_form": "그래서 어떻게 했어",
          "speaker_id": "2",
          "time": "20210907 00:10"
        },
        {
          "id": "MDRW2100042119.1.5",
          "form": "그래서 중간에 버스에서 내려서 신도림역을 갔거든",
          "original_form": "그래서 중간에 버스에서 내려서 신도림역을 갔거든",
          "speaker_id": "1",
          "time": "20210907 00:10"
        },
        {
          "id": "MDRW2100042119.1.6",
          "form": "와 아침에 신도림역 대박이야",
          "original_form": "와 아침에 신도림역 대박이야",
          "speaker_id": "1",
          "time": "20210907 00:10"
        },
        {
          "id": "MDRW2100042119.1.7",
          "form": "아침에 사람 진짜 많지",
          "original_form": "아침에 사람 진짜 많지",
          "speaker_id": "2",
          "time": "20210907 00:11"
        },
        {
          "id": "MDRW2100042119.1.8",
          "form": "지하철에서 발뒷꿈치로 서서갔어",
          "original_form": "지하철에서 발뒷꿈치로 서서갔어",
          "speaker_id": "1",
          "time": "20210907 00:13"
        },
        {
          "id": "MDRW2100042119.1.9",
          "form": "사람이 너믄 많아서",
          "original_form": "사람이 너믄 많아서",
          "speaker_id": "1",
          "time": "20210907 00:13"
        },
        {
          "id": "MDRW2100042119.1.10",
          "form": "내일 비와서 더 일찍 나가야겠는데",
          "original_form": "내일 비와서 더 일찍 나가야겠는데",
          "speaker_id": "2",
          "time": "20210907 00:14"
        },
        {
          "id": "MDRW2100042119.1.11",
          "form": "비오는 날이 제일 좋았는데 이제는 싫다...",
          "original_form": "비오는 날이 제일 좋았는데 이제는 싫다...",
          "speaker_id": "1",
          "time": "20210907 00:14"
        },
        {
          "id": "MDRW2100042119.1.12",
          "form": "아침 안머고갔는데 배안고팠어??",
          "original_form": "아침 안머고갔는데 배안고팠어??",
          "speaker_id": "2",
          "time": "20210907 00:15"
        },
        {
          "id": "MDRW2100042119.1.13",
          "form": "배고파서 우유사먹었어",
          "original_form": "배고파서 우유사먹었어",
          "speaker_id": "1",
          "time": "20210907 00:15"
        },
        {
          "id": "MDRW2100042119.1.14",
          "form": "내일은 뭐 좀 먹고가",
          "original_form": "내일은 뭐 좀 먹고가",
          "speaker_id": "2",
          "time": "20210907 00:16"
        },
        {
          "id": "MDRW2100042119.1.15",
          "form": "먹고가고싶은데 난 잠을 더 자고싶어",
          "original_form": "먹고가고싶은데 난 잠을 더 자고싶어",
          "speaker_id": "1",
          "time": "20210907 00:16"
        },
        {
          "id": "MDRW2100042119.1.16",
          "form": "아침에 먹을걸 좀 사놔야겠네",
          "original_form": "아침에 먹을걸 좀 사놔야겠네",
          "speaker_id": "2",
          "time": "20210907 00:17"
        },
        {
          "id": "MDRW2100042119.1.17",
          "form": "이따가 점심에 감자탕먹어",
          "original_form": "이따가 점심에 감자탕먹어",
          "speaker_id": "2",
          "time": "20210907 00:46"
        },
        {
          "id": "MDRW2100042119.1.18",
          "form": "감자탕 만들어놨어",
          "original_form": "감자탕 만들어놨어",
