      "utterance": [
        {
          "id": "MDRW2100023160.1.1",
          "form": "보시는 예능 있으세요?",
          "original_form": "보시는 예능 있으세요?",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023160.1.2",
          "form": "대탈출이요!",
          "original_form": "대탈출이요!",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023160.1.3",
          "form": "오!! 저두요!",
          "original_form": "오!! 저두요!",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023160.1.4",
          "form": "본방 챙겨보세요?",
          "original_form": "본방 챙겨보세요?",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023160.1.5",
          "form": "본방은 못보고 재방으로보고있어요",
          "original_form": "본방은 못보고 재방으로보고있어요",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023160.1.6",
          "form": "그러시구나!",
          "original_form": "그러시구나!",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023160.1.7",
          "form": "저는 본방 보려하는데 11시부터 봐서 앞은 늘 못봐요ㅠ",
          "original_form": "저는 본방 보려하는데 11시부터 봐서 앞은 늘 못봐요ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023160.1.8",
          "form": "앗...왜죠?",
          "original_form": "앗...왜죠?",
          "speaker_id": "2",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100023160.1.9",
          "form": "부모님 미우새 보시거든요ㅎㅎ",
          "original_form": "부모님 미우새 보시거든요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023160.1.10",
          "form": "아..그렇구나ㅠ",
          "original_form": "아..그렇구나ㅠ",
          "speaker_id": "2",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023160.1.11",
          "form": "네ㅠ",
          "original_form": "네ㅠ",
          "speaker_id": "1",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023160.1.12",
          "form": "지금 보고있어요!",
          "original_form": "지금 보고있어요!",
          "speaker_id": "1",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023160.1.13",
          "form": "오늘도 벌써 흥미진진ㅋㅋㅋ",
          "original_form": "오늘도 벌써 흥미진진ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023160.1.14",
          "form": "오! 저는자야되서 재방을보는데ㅠ",
          "original_form": "오! 저는자야되서 재방을보는데ㅠ",
          "speaker_id": "2",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023160.1.15",
          "form": "일찍 주무시는군요ㅠ",
          "original_form": "일찍 주무시는군요ㅠ",
          "speaker_id": "1",
          "time": "20210829 23:27"
        },
        {
          "id": "MDRW2100023160.1.16",
          "form": "재방은 어떤거로 보세요?",
          "original_form": "재방은 어떤거로 보세요?",
          "speaker_id": "1",
          "time": "20210829 23:27"
        },
        {
          "id": "MDRW2100023160.1.17",
          "form": "인터넷이요ㅋㅋㅋ",
          "original_form": "인터넷이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023160.1.18",
          "form": "아항ㅋㅋ 넷플이런건 안쓰세요?",
