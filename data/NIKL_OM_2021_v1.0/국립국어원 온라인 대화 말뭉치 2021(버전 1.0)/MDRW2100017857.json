      "utterance": [
        {
          "id": "MDRW2100017857.1.1",
          "form": "여행은 좋아하세요?",
          "original_form": "여행은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017857.1.2",
          "form": "네네! 너무 좋죠!",
          "original_form": "네네! 너무 좋죠!",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017857.1.3",
          "form": "요즘은 그냥 랜선투어 하지만..",
          "original_form": "요즘은 그냥 랜선투어 하지만..",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017857.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ코로나 끝나면 어디로 여행가고싶으세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ코로나 끝나면 어디로 여행가고싶으세요?",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017857.1.5",
          "form": "일단 저 서울에 가려고 했던 곳들이 좀 있어서 서울을 가려고 하구요",
          "original_form": "일단 저 서울에 가려고 했던 곳들이 좀 있어서 서울을 가려고 하구요",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017857.1.6",
          "form": "그 외에는 해외여행 맘놓고 가고싶네요",
          "original_form": "그 외에는 해외여행 맘놓고 가고싶네요",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017857.1.7",
          "form": "어디가실거에요?",
          "original_form": "어디가실거에요?",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017857.1.8",
          "form": "저도 해외여행 너무 오래 못가서 어디든 해외여행 가고싶어요 ㅠㅠ ㅋㅋㅋㅋㅋ",
          "original_form": "저도 해외여행 너무 오래 못가서 어디든 해외여행 가고싶어요 ㅠㅠ ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017857.1.9",
          "form": "저 코로나 터지기 직전에 연말에 타이페이 다녀왔었는데",
          "original_form": "저 코로나 터지기 직전에 연말에 타이페이 다녀왔었는데",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017857.1.10",
          "form": "그냥 그 기억으로 살고 있어요..",
          "original_form": "그냥 그 기억으로 살고 있어요..",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017857.1.11",
          "form": "ㅠㅠ아련하네요",
          "original_form": "ㅠㅠ아련하네요",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017857.1.12",
          "form": "누가크래커 먹고싶어요 ㅠㅠ",
          "original_form": "누가크래커 먹고싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017857.1.13",
          "form": "아ㅠㅠ 저 계속 트레이더스 가서 누가 크래커 들었다 놨다 하는데",
          "original_form": "아ㅠㅠ 저 계속 트레이더스 가서 누가 크래커 들었다 놨다 하는데",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017857.1.14",
          "form": "여기서 사서 먹는건 의미없어 하면서 ㅠㅠ",
          "original_form": "여기서 사서 먹는건 의미없어 하면서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017857.1.15",
          "form": "맞아요 거기서먹어야의미가잇죠ㅠㅠ",
          "original_form": "맞아요 거기서먹어야의미가잇죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017857.1.16",
          "form": "야시장 지파이도 맛잇엇는데ㅠㅠ",
          "original_form": "야시장 지파이도 맛잇엇는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017857.1.17",
          "form": "마자요 아 지파이도 ㅠㅠ 다 한국서 사먹을 수는 있어도 다 의미없어요 흑흑",
          "original_form": "마자요 아 지파이도 ㅠㅠ 다 한국서 사먹을 수는 있어도 다 의미없어요 흑흑",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017857.1.18",
          "form": "고수 얹은 땅콩아이스크림도 먹어줘야하는뎈ㅋㅋㅋㅋㅋㅋ",
          "original_form": "고수 얹은 땅콩아이스크림도 먹어줘야하는뎈ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017857.1.19",
          "form": "앗 어쩌다보니 먹는것얘기만ㅋㅋㅋㅋ",
          "original_form": "앗 어쩌다보니 먹는것얘기만ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017857.1.20",
          "form": "ㅋㅋㅋ여행지의 기억은 먹는 것으로 !",
          "original_form": "ㅋㅋㅋ여행지의 기억은 먹는 것으로 !",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017857.1.21",
          "form": "어디가 제일 기억에 남으세요",
          "original_form": "어디가 제일 기억에 남으세요",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017857.1.22",
          "form": "대만에서는 지우펀이 좋앗어요 ㅋㅋㅋㅋㅋ",
          "original_form": "대만에서는 지우펀이 좋앗어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017857.1.23",
          "form": "제가 센과치히로의행방불명 좋아햇어서 ㅋㅋㅋㅋㅋ",
          "original_form": "제가 센과치히로의행방불명 좋아햇어서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017857.1.24",
          "form": "아 지우펀 예쁘죠 사진으로 예쁘더라구요",
          "original_form": "아 지우펀 예쁘죠 사진으로 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017857.1.25",
          "form": "저는 갔을때 비가와서...너무 지옥이었어요 ..ㅎㅎ",
          "original_form": "저는 갔을때 비가와서...너무 지옥이었어요 ..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017857.1.26",
          "form": "아이고 ㅠㅠ",
          "original_form": "아이고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017857.1.27",
          "form": "고생하셧겟네요ㅠㅠ",
          "original_form": "고생하셧겟네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017857.1.28",
          "form": "그럼 어디가 좋으셧어요?",
          "original_form": "그럼 어디가 좋으셧어요?",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017857.1.29",
          "form": "전 거기 송상문화공원이었나",
          "original_form": "전 거기 송상문화공원이었나",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017857.1.30",
          "form": "담배공장을 공원으로 만든곳이 있었어요",
          "original_form": "담배공장을 공원으로 만든곳이 있었어요",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017857.1.31",
          "form": "아 알아요",
          "original_form": "아 알아요",
          "speaker_id": "1",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017857.1.32",
          "form": "거기도 좋죠 ㅠㅠ",
          "original_form": "거기도 좋죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100017857.1.33",
          "form": "네네 너무 분위기가 좋더라구요",
          "original_form": "네네 너무 분위기가 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100017857.1.34",
          "form": "타이페이는 다시 또 가고싶어요 ㅋㅋ",
          "original_form": "타이페이는 다시 또 가고싶어요 ㅋㅋ",
