      "utterance": [
        {
          "id": "MDRW2100044044.1.1",
          "form": "일본 여행을 가고 싶어",
          "original_form": "일본 여행을 가고 싶어",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044044.1.2",
          "form": "웅웅ㅋㅋ",
          "original_form": "웅웅ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044044.1.3",
          "form": "안 된다 지금은",
          "original_form": "안 된다 지금은",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044044.1.4",
          "form": "그렇지.. 나중에 코로나 끝나면 말이야~",
          "original_form": "그렇지.. 나중에 코로나 끝나면 말이야~",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044044.1.5",
          "form": "그때 같이 가자",
          "original_form": "그때 같이 가자",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044044.1.6",
          "form": "오 그럴까용?",
          "original_form": "오 그럴까용?",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044044.1.7",
          "form": "그래 나도가 보고 싶다",
          "original_form": "그래 나도가 보고 싶다",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044044.1.8",
          "form": "오 좋다좋다 ㅎㅎ 같이가자",
          "original_form": "오 좋다좋다 ㅎㅎ 같이가자",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044044.1.9",
          "form": "일본은 한 번도 안가 봤네",
          "original_form": "일본은 한 번도 안가 봤네",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044044.1.10",
          "form": "ㅋㅋ 나는 오사카만 두번 가봤다",
          "original_form": "ㅋㅋ 나는 오사카만 두번 가봤다",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044044.1.11",
          "form": "그래 회사에서 다녀 왔잖아",
          "original_form": "그래 회사에서 다녀 왔잖아",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044044.1.12",
          "form": "웅웅 이번엔 도쿄로 가보자",
          "original_form": "웅웅 이번엔 도쿄로 가보자",
