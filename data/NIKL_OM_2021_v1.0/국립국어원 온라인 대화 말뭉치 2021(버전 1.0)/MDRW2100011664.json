      "utterance": [
        {
          "id": "MDRW2100011664.1.1",
          "form": "공연보는거 좋아하세요?",
          "original_form": "공연보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011664.1.2",
          "form": "네네 코로나라서 자주못가게되었지만",
          "original_form": "네네 코로나라서 자주못가게되었지만",
          "speaker_id": "2",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011664.1.3",
          "form": "그전에는 잘 댕겼어요",
          "original_form": "그전에는 잘 댕겼어요",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011664.1.4",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011664.1.5",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011664.1.6",
          "form": "클래식 위주로만 편식한게 문제지 보는건 좋아합니다 ㅋㅋ",
          "original_form": "클래식 위주로만 편식한게 문제지 보는건 좋아합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011664.1.7",
          "form": "발레도 보러갔었어요 부퍼탈텐즈테아터 내한했을때 오래됐네요",
          "original_form": "발레도 보러갔었어요 부퍼탈텐즈테아터 내한했을때 오래됐네요",
          "speaker_id": "2",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011664.1.8",
          "form": "봄의제전 봤어요",
          "original_form": "봄의제전 봤어요",
          "speaker_id": "2",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011664.1.9",
          "form": "아 스트라빈스키요..",
          "original_form": "아 스트라빈스키요..",
          "speaker_id": "1",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011664.1.10",
          "form": "네 그음악에 춤췄어요",
          "original_form": "네 그음악에 춤췄어요",
          "speaker_id": "2",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011664.1.11",
          "form": "엄청 좋으셨나봐요",
          "original_form": "엄청 좋으셨나봐요",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011664.1.12",
          "form": "저도 스트라빈스키 좋아해요",
          "original_form": "저도 스트라빈스키 좋아해요",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011664.1.13",
          "form": "피나바우쉬는 돌아가셨지만 작품은 대단했던거 같아요 스트라빈스키의 변주곡에 춤짜는게 어렵다나봐요",
          "original_form": "피나바우쉬는 돌아가셨지만 작품은 대단했던거 같아요 스트라빈스키의 변주곡에 춤짜는게 어렵다나봐요",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011664.1.14",
          "form": "저는 창극도 좋아해요 이자람팬입니다",
          "original_form": "저는 창극도 좋아해요 이자람팬입니다",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011664.1.15",
          "form": "오오 창극 좋아하시는군요",
          "original_form": "오오 창극 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011664.1.16",
          "form": "저는 공연 한번 본게 전부라 잘은 모르는데",
          "original_form": "저는 공연 한번 본게 전부라 잘은 모르는데",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011664.1.17",
          "form": "잘됐으면좋겠어요",
          "original_form": "잘됐으면좋겠어요",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011664.1.18",
          "form": "코로나로 다 힘들죠ㅜㅜ",
          "original_form": "코로나로 다 힘들죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011664.1.19",
          "form": "창극도 온라인공연으로 돌리더래요",
          "original_form": "창극도 온라인공연으로 돌리더래요",
          "speaker_id": "2",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011664.1.20",
          "form": "어디서 해요?",
          "original_form": "어디서 해요?",
          "speaker_id": "1",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011664.1.21",
          "form": "국립국악원에서요?",
          "original_form": "국립국악원에서요?",
          "speaker_id": "1",
          "time": "20210824 15:24"
        },
        {
          "id": "MDRW2100011664.1.22",
          "form": "국립극장 유툽에서 하던거같아요",
          "original_form": "국립극장 유툽에서 하던거같아요",
          "speaker_id": "2",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100011664.1.23",
          "form": "그렇군요 좋은 정보 감사합니다",
          "original_form": "그렇군요 좋은 정보 감사합니다",
