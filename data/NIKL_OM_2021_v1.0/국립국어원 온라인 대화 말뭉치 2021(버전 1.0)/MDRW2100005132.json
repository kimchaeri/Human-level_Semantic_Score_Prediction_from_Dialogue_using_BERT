      "utterance": [
        {
          "id": "MDRW2100005132.1.1",
          "form": "펜하 보세요?",
          "original_form": "펜하 보세요?",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005132.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005132.1.3",
          "form": "티비를 잘 안봐요",
          "original_form": "티비를 잘 안봐요",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005132.1.4",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005132.1.5",
          "form": "아 그러시군요...",
          "original_form": "아 그러시군요...",
          "speaker_id": "1",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005132.1.6",
          "form": "영화는요?",
          "original_form": "영화는요?",
          "speaker_id": "1",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005132.1.7",
          "form": "영화는 볼시간이 안되시죠?",
          "original_form": "영화는 볼시간이 안되시죠?",
          "speaker_id": "1",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005132.1.8",
          "form": "사람이 많아서 피하는데",
          "original_form": "사람이 많아서 피하는데",
          "speaker_id": "2",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005132.1.9",
          "form": "집에 티비가 75인치라 보고싶으면 티비로봐요",
          "original_form": "집에 티비가 75인치라 보고싶으면 티비로봐요",
          "speaker_id": "2",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005132.1.10",
          "form": "영화 헬릭스보셨나요",
          "original_form": "영화 헬릭스보셨나요",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005132.1.11",
          "form": "저도 코로나땜에 못가긴해요",
          "original_form": "저도 코로나땜에 못가긴해요",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005132.1.12",
          "form": "영화는 무슨종류 좋아해요?",
          "original_form": "영화는 무슨종류 좋아해요?",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005132.1.13",
          "form": "헬릭스는 첨들어요",
          "original_form": "헬릭스는 첨들어요",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005132.1.14",
          "form": "저는 SF요 저것도 SF예요",
          "original_form": "저는 SF요 저것도 SF예요",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005132.1.15",
          "form": "저는 추리 좋아해요",
          "original_form": "저는 추리 좋아해요",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005132.1.16",
          "form": "16년도에 나온영화인데 재밋어요",
          "original_form": "16년도에 나온영화인데 재밋어요",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005132.1.17",
          "form": "아하, 드라마 비밀의숲보셨어요?",
          "original_form": "아하, 드라마 비밀의숲보셨어요?",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005132.1.18",
          "form": "그게 추리같은거인가요?",
          "original_form": "그게 추리같은거인가요?",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005132.1.19",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005132.1.20",
          "form": "그런거 좋아해요 ㅎㅎ",
          "original_form": "그런거 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005132.1.21",
          "form": "비밀의 숲 1,2둘다 봤어요",
          "original_form": "비밀의 숲 1,2둘다 봤어요",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005132.1.22",
          "form": "인생드라마예요",
          "original_form": "인생드라마예요",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005132.1.23",
          "form": "시그널같은것두요",
          "original_form": "시그널같은것두요",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005132.1.24",
          "form": "조승우 넘 멋있..ㅋㅋ",
          "original_form": "조승우 넘 멋있..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005132.1.25",
          "form": "시그널도 봤죠!",
          "original_form": "시그널도 봤죠!",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005132.1.26",
          "form": "제가 추리를 좋아하는건가봐요",
          "original_form": "제가 추리를 좋아하는건가봐요",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005132.1.27",
          "form": "진짜 저도 조승우 넘좋아해요",
          "original_form": "진짜 저도 조승우 넘좋아해요",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005132.1.28",
          "form": "님 덕에 알게됬네요",
          "original_form": "님 덕에 알게됬네요",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005132.1.29",
          "form": "요즘은 그런 드라마 없죠?",
          "original_form": "요즘은 그런 드라마 없죠?",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005132.1.30",
          "form": "네 없어요 ㅜㅠ",
          "original_form": "네 없어요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210821 20:50"
        }
      ]
