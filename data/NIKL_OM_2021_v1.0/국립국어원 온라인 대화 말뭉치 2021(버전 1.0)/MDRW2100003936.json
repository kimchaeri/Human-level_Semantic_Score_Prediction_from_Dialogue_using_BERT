        {
          "id": "MDRW2100003936.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210821 14:02"
        },
        {
          "id": "MDRW2100003936.1.2",
          "form": "그냥 저냥 중간 정도요 ㅎㅎ",
          "original_form": "그냥 저냥 중간 정도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003936.1.3",
          "form": "여행 잘다니세요?",
          "original_form": "여행 잘다니세요?",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003936.1.4",
          "form": "코로나라 갈수가 없어소 그건 아쉬워요",
          "original_form": "코로나라 갈수가 없어소 그건 아쉬워요",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003936.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100003936.1.6",
          "form": "어디가나요",
          "original_form": "어디가나요",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100003936.1.7",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100003936.1.8",
          "form": "가까운 산밑에 절이나 계곡에 발만 담그고와요 ㅎㅎ",
          "original_form": "가까운 산밑에 절이나 계곡에 발만 담그고와요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100003936.1.9",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100003936.1.10",
          "form": "언제 꼭 가보세요",
          "original_form": "언제 꼭 가보세요",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100003936.1.11",
          "form": "사람 없는곳 찾아 다녀요",
          "original_form": "사람 없는곳 찾아 다녀요",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100003936.1.12",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100003936.1.13",
          "form": "아이 대학붙으면 제주도 가려고요",
          "original_form": "아이 대학붙으면 제주도 가려고요",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100003936.1.14",
          "form": "오 추억이 되겠다",
          "original_form": "오 추억이 되겠다",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100003936.1.15",
          "form": "네 해외는 코로나로 안되니 제주도 가게요",
          "original_form": "네 해외는 코로나로 안되니 제주도 가게요",
          "speaker_id": "2",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100003936.1.16",
          "form": "저도 제주",
          "original_form": "저도 제주",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100003936.1.17",
          "form": "아 가시게요?",
          "original_form": "아 가시게요?",
