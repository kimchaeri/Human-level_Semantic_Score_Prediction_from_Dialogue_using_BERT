      "utterance": [
        {
          "id": "MDRW2100009977.1.1",
          "form": "이번주 토요일모임 아시죠?",
          "original_form": "이번주 토요일모임 아시죠?",
          "speaker_id": "1",
          "time": "20210823 19:22"
        },
        {
          "id": "MDRW2100009977.1.2",
          "form": "물론 알지\r\n친구들 만나야 하잖아",
          "original_form": "물론 알지\r\n친구들 만나야 하잖아",
          "speaker_id": "2",
          "time": "20210824 06:12"
        },
        {
          "id": "MDRW2100009977.1.3",
          "form": "줌으로요",
          "original_form": "줌으로요",
          "speaker_id": "1",
          "time": "20210824 06:13"
        },
        {
          "id": "MDRW2100009977.1.4",
          "form": "알지 오후2시던가?",
          "original_form": "알지 오후2시던가?",
          "speaker_id": "2",
          "time": "20210824 06:14"
        },
        {
          "id": "MDRW2100009977.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 06:15"
        },
        {
          "id": "MDRW2100009977.1.6",
          "form": "벌써  기대가 되는군",
          "original_form": "벌써  기대가 되는군",
          "speaker_id": "2",
          "time": "20210824 06:16"
        },
        {
          "id": "MDRW2100009977.1.7",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210824 06:16"
        },
        {
          "id": "MDRW2100009977.1.8",
          "form": "직접 못만나도 화상으로나마 얼굴을 볼 수 있다니",
          "original_form": "직접 못만나도 화상으로나마 얼굴을 볼 수 있다니",
          "speaker_id": "2",
          "time": "20210824 06:16"
        },
        {
          "id": "MDRW2100009977.1.9",
          "form": "기술 발전이 대단해요",
          "original_form": "기술 발전이 대단해요",
          "speaker_id": "1",
          "time": "20210824 06:17"
        },
        {
          "id": "MDRW2100009977.1.10",
          "form": "몇명온대?",
          "original_form": "몇명온대?",
          "speaker_id": "2",
          "time": "20210824 06:18"
        },
        {
          "id": "MDRW2100009977.1.11",
          "form": "30명쯤?",
          "original_form": "30명쯤?",
          "speaker_id": "1",
          "time": "20210824 06:18"
        },
        {
          "id": "MDRW2100009977.1.12",
          "form": "와 직접 모엿다면장소 찾고 난리였겠구만",
          "original_form": "와 직접 모엿다면장소 찾고 난리였겠구만",
          "speaker_id": "2",
          "time": "20210824 06:18"
        },
        {
          "id": "MDRW2100009977.1.13",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210824 06:18"
        },
        {
          "id": "MDRW2100009977.1.14",
          "form": "외국에 있는 멤버들도 오지?",
          "original_form": "외국에 있는 멤버들도 오지?",
          "speaker_id": "2",
          "time": "20210824 06:19"
        },
        {
          "id": "MDRW2100009977.1.15",
          "form": "네 미국요",
          "original_form": "네 미국요",
          "speaker_id": "1",
          "time": "20210824 06:19"
        },
        {
          "id": "MDRW2100009977.1.16",
          "form": "오프라인이라면 불가능햇갯지",
          "original_form": "오프라인이라면 불가능햇갯지",
          "speaker_id": "2",
          "time": "20210824 06:19"
        },
        {
          "id": "MDRW2100009977.1.17",
          "form": "그래도 같이 봉사활동 다니던 때가 그립네요",
          "original_form": "그래도 같이 봉사활동 다니던 때가 그립네요",
          "speaker_id": "1",
          "time": "20210824 06:20"
        },
