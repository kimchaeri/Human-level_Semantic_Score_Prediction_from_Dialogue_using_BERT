      "utterance": [
        {
          "id": "MDRW2100002245.1.1",
          "form": "저 애낳을때 수술이라 백만원도 더 나왔는데",
          "original_form": "저 애낳을때 수술이라 백만원도 더 나왔는데",
          "speaker_id": "2",
          "time": "20210820 01:15"
        },
        {
          "id": "MDRW2100002245.1.2",
          "form": "요새는 출산비 비싼가요?",
          "original_form": "요새는 출산비 비싼가요?",
          "speaker_id": "2",
          "time": "20210820 01:15"
        },
        {
          "id": "MDRW2100002245.1.3",
          "form": "전 자분인데 70정도",
          "original_form": "전 자분인데 70정도",
          "speaker_id": "1",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002245.1.4",
          "form": "나온거같아요",
          "original_form": "나온거같아요",
          "speaker_id": "1",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002245.1.5",
          "form": "아이 용품은 성별이 같아서 나이차도 덜나고 많이 있으시죠",
          "original_form": "아이 용품은 성별이 같아서 나이차도 덜나고 많이 있으시죠",
          "speaker_id": "2",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002245.1.6",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002245.1.7",
          "form": "무통을 못맞고나아서",
          "original_form": "무통을 못맞고나아서",
          "speaker_id": "1",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002245.1.8",
          "form": "아 아프셨겠다 ㅜㅜ",
          "original_form": "아 아프셨겠다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002245.1.9",
          "form": "아파죽는줄알앗어요ㅋ별이 보엿어요",
          "original_form": "아파죽는줄알앗어요ㅋ별이 보엿어요",
          "speaker_id": "1",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002245.1.10",
          "form": "전 수술이라",
          "original_form": "전 수술이라",
          "speaker_id": "2",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002245.1.11",
          "form": "헉 또 생각하니 무섭네요",
          "original_form": "헉 또 생각하니 무섭네요",
          "speaker_id": "2",
          "time": "20210820 01:16"
        },
        {
          "id": "MDRW2100002245.1.12",
          "form": "정말 여자는 불쌍해요",
          "original_form": "정말 여자는 불쌍해요",
          "speaker_id": "2",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002245.1.13",
          "form": "그니까요ㅜㅜㅡ",
          "original_form": "그니까요ㅜㅜㅡ",
          "speaker_id": "1",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002245.1.14",
          "form": "둘째는 지금 벌써부터 밑에 내려와잇다고해서",
          "original_form": "둘째는 지금 벌써부터 밑에 내려와잇다고해서",
          "speaker_id": "1",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002245.1.15",
          "form": "근데 또 지나면 잊게 되니 신기해요",
          "original_form": "근데 또 지나면 잊게 되니 신기해요",
          "speaker_id": "2",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002245.1.16",
          "form": "37주쯤 낳자하더라구요",
          "original_form": "37주쯤 낳자하더라구요",
          "speaker_id": "1",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002245.1.17",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002245.1.18",
          "form": "빠르네요",
          "original_form": "빠르네요",
          "speaker_id": "2",
          "time": "20210820 01:17"
        },
        {
          "id": "MDRW2100002245.1.19",
          "form": "유도분만 하겠어요",
          "original_form": "유도분만 하겠어요",
          "speaker_id": "2",
          "time": "20210820 01:18"
        },
        {
          "id": "MDRW2100002245.1.20",
          "form": "그리고 첫째도 2시간만에 낳아서",
          "original_form": "그리고 첫째도 2시간만에 낳아서",
          "speaker_id": "1",
          "time": "20210820 01:18"
        },
        {
          "id": "MDRW2100002245.1.21",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210820 01:18"
        },
        {
          "id": "MDRW2100002245.1.22",
          "form": "미리 유도하자고 하더라구요",
          "original_form": "미리 유도하자고 하더라구요",
          "speaker_id": "1",
          "time": "20210820 01:18"
        },
        {
          "id": "MDRW2100002245.1.23",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210820 01:18"
        },
        {
          "id": "MDRW2100002245.1.24",
          "form": "네 조심해서 낳으세요",
          "original_form": "네 조심해서 낳으세요",
          "speaker_id": "2",
          "time": "20210820 01:18"
        },
        {
          "id": "MDRW2100002245.1.25",
          "form": "전 양수가 터지고 자궁이 안열려서 수술했어요",
          "original_form": "전 양수가 터지고 자궁이 안열려서 수술했어요",
          "speaker_id": "2",
          "time": "20210820 01:18"
        },
        {
          "id": "MDRW2100002245.1.26",
          "form": "진통도 없고 위험해서",
          "original_form": "진통도 없고 위험해서",
          "speaker_id": "2",
          "time": "20210820 01:19"
        },
        {
          "id": "MDRW2100002245.1.27",
          "form": "네 힘내야조ㅎㅎ",
          "original_form": "네 힘내야조ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 01:19"
        },
        {
          "id": "MDRW2100002245.1.28",
          "form": "순산하실것 같아요",
          "original_form": "순산하실것 같아요",
          "speaker_id": "2",
          "time": "20210820 01:19"
        },
        {
          "id": "MDRW2100002245.1.29",
          "form": "힘드셧겠어요ㅠ",
          "original_form": "힘드셧겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210820 01:19"
        },
        {
          "id": "MDRW2100002245.1.30",
          "form": "뭔지도 모르고 낳어요",
          "original_form": "뭔지도 모르고 낳어요",
          "speaker_id": "2",
          "time": "20210820 01:19"
        },
        {
          "id": "MDRW2100002245.1.31",
          "form": "곡정 안하셔도 될것 같아요 ㅎㅎ요번엔 더 빨리 잘 낳으실거예요",
          "original_form": "곡정 안하셔도 될것 같아요 ㅎㅎ요번엔 더 빨리 잘 낳으실거예요",
          "speaker_id": "2",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002245.1.32",
          "form": "여자는 대단하죠 엄마짱",
          "original_form": "여자는 대단하죠 엄마짱",
          "speaker_id": "1",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002245.1.33",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002245.1.34",
          "form": "이번엔 무통 맞고싶어요ㅎ",
          "original_form": "이번엔 무통 맞고싶어요ㅎ",
          "speaker_id": "1",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002245.1.35",
          "form": "울엄마 4 대단하세요",
          "original_form": "울엄마 4 대단하세요",
          "speaker_id": "2",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002245.1.36",
          "form": "꼭 맞으세요",
          "original_form": "꼭 맞으세요",
          "speaker_id": "2",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002245.1.37",
          "form": "와 대단하시다",
          "original_form": "와 대단하시다",
          "speaker_id": "1",
          "time": "20210820 01:20"
        },
        {
          "id": "MDRW2100002245.1.38",
          "form": "그땐 많이 낳았죠",
          "original_form": "그땐 많이 낳았죠",
          "speaker_id": "2",
          "time": "20210820 01:20"
        }
      ]
    }
