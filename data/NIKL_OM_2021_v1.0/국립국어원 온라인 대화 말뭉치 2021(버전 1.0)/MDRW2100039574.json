        {
          "id": "MDRW2100039574.1.1",
          "form": "흑",
          "original_form": "흑",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039574.1.2",
          "form": "아니그거 남기관사가 보고",
          "original_form": "아니그거 남기관사가 보고",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039574.1.3",
          "form": "열어줄 수 있지 않아요?",
          "original_form": "열어줄 수 있지 않아요?",
          "speaker_id": "1",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100039574.1.4",
          "form": "ㅋㅋㅋ후..급행은 너무 많아서 사람이",
          "original_form": "ㅋㅋㅋ후..급행은 너무 많아서 사람이",
          "speaker_id": "2",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039574.1.5",
          "form": "기다려주면 끝도 없긴하죠..에혀",
          "original_form": "기다려주면 끝도 없긴하죠..에혀",
          "speaker_id": "2",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039574.1.6",
          "form": "달리기를 더 빨리 뛰었어야햇다",
          "original_form": "달리기를 더 빨리 뛰었어야햇다",
          "speaker_id": "2",
          "time": "20210903 19:30"
        },
        {
          "id": "MDRW2100039574.1.7",
          "form": "흑 그렇군요ㅠ 사람들이 많았다면...ㅜ",
          "original_form": "흑 그렇군요ㅠ 사람들이 많았다면...ㅜ",
          "speaker_id": "1",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100039574.1.8",
          "form": "저도 방금",
          "original_form": "저도 방금",
          "speaker_id": "1",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100039574.1.9",
          "form": "비슷한 일을 겪었어요,,",
          "original_form": "비슷한 일을 겪었어요,,",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039574.1.10",
          "form": "지금 퇴근하는데",
          "original_form": "지금 퇴근하는데",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039574.1.11",
          "form": "2호선을 코앞에서 놓쳤답니다!",
          "original_form": "2호선을 코앞에서 놓쳤답니다!",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039574.1.12",
          "form": "아오...",
          "original_form": "아오...",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039574.1.13",
          "form": "저희 타면 문 닫으시라구요.",
          "original_form": "저희 타면 문 닫으시라구요.",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039574.1.14",
          "form": "아.시.겠.어.요?",
          "original_form": "아.시.겠.어.요?",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039574.1.15",
          "form": "맞 소 맞 소",
          "original_form": "맞 소 맞 소",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039574.1.16",
          "form": "name2님 집에 잘",
          "original_form": "&name2&님 집에 잘",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039574.1.17",
          "form": "도착하셨나요?!?!",
          "original_form": "도착하셨나요?!?!",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039574.1.18",
          "form": "네네 저 오자마자 저녁도 바로 호로록 했답니다",
          "original_form": "네네 저 오자마자 저녁도 바로 호로록 했답니다",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039574.1.19",
          "form": "name1님도 오늘하루 고생하셨어요",
          "original_form": "&name1&님도 오늘하루 고생하셨어요",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039574.1.20",
          "form": "이제 주말이다!!!!!",
          "original_form": "이제 주말이다!!!!!",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039574.1.21",
          "form": "우와아악 name2님도 고생하셨어요!!!!!",
          "original_form": "우와아악 &name2&님도 고생하셨어요!!!!!",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039574.1.22",
          "form": "저는 이제 다음역에서 내리는데",
          "original_form": "저는 이제 다음역에서 내리는데",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039574.1.23",
          "form": "필라테스에 늦을 확률이 아주 높아서 심장이",
          "original_form": "필라테스에 늦을 확률이 아주 높아서 심장이",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100039574.1.24",
          "form": "벌렁벌렁하답니다!!",
          "original_form": "벌렁벌렁하답니다!!",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100039574.1.25",
          "form": "우와 진짜 하루를 알차게 보내시네요",
          "original_form": "우와 진짜 하루를 알차게 보내시네요",
          "speaker_id": "2",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039574.1.26",
          "form": "오늘 필테 이기고 오세요!!",
          "original_form": "오늘 필테 이기고 오세요!!",
          "speaker_id": "2",
          "time": "20210903 20:55"
        },
        {
