      "utterance": [
        {
          "id": "MDRW2100036144.1.1",
          "form": "스트레스가많죠",
          "original_form": "스트레스가많죠",
          "speaker_id": "2",
          "time": "20210830 17:31"
        },
        {
          "id": "MDRW2100036144.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210830 17:31"
        },
        {
          "id": "MDRW2100036144.1.3",
          "form": "차장급 일을 왜나에게 주는거에요 돈을 주든가",
          "original_form": "차장급 일을 왜나에게 주는거에요 돈을 주든가",
          "speaker_id": "1",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100036144.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ때려치울래요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ때려치울래요",
          "speaker_id": "1",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100036144.1.5",
          "form": "왜월급안올려주고뽕뽑아먹을라구하죠",
          "original_form": "왜월급안올려주고뽕뽑아먹을라구하죠",
          "speaker_id": "2",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100036144.1.6",
          "form": "ㅅㆍ람짱나게",
          "original_form": "ㅅㆍ람짱나게",
          "speaker_id": "2",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100036144.1.7",
          "form": "그니까요 짜증나요",
          "original_form": "그니까요 짜증나요",
          "speaker_id": "1",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100036144.1.8",
          "form": "맨날 나만 초과근무하고",
          "original_form": "맨날 나만 초과근무하고",
          "speaker_id": "1",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100036144.1.9",
          "form": "차장부장은 칼퇴하고",
          "original_form": "차장부장은 칼퇴하고",
          "speaker_id": "1",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100036144.1.10",
          "form": "받는만큼 일하면 안돼요?ㅠㅠㅍ",
          "original_form": "받는만큼 일하면 안돼요?ㅠㅠㅍ",
          "speaker_id": "1",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100036144.1.11",
          "form": "아니 언제퇴근해요 그래가지구\n자기들은 왜집에가요",
          "original_form": "아니 언제퇴근해요 그래가지구\n자기들은 왜집에가요",
          "speaker_id": "2",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100036144.1.12",
          "form": "8시부터 근무하는 사람은 5시에도ㅠ가요ㅠㅠㅠ",
          "original_form": "8시부터 근무하는 사람은 5시에도ㅠ가요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100036144.1.13",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100036144.1.14",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100036144.1.15",
          "form": "와서 개인 공부 하다가 가는 사람도 잇어요",
          "original_form": "와서 개인 공부 하다가 가는 사람도 잇어요",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100036144.1.16",
          "form": "별",
          "original_form": "별",
          "speaker_id": "2",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100036144.1.17",
          "form": "맘에안드네여",
          "original_form": "맘에안드네여",
          "speaker_id": "2",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100036144.1.18",
          "form": "그쵸 짜증나요",
          "original_form": "그쵸 짜증나요",
          "speaker_id": "1",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100036144.1.19",
          "form": "퇴근하셨나요",
          "original_form": "퇴근하셨나요",
          "speaker_id": "1",
          "time": "20210830 17:38"
        },
        {
          "id": "MDRW2100036144.1.20",
          "form": "네 벌을피해 집으로왔습니다",
          "original_form": "네 벌을피해 집으로왔습니다",
          "speaker_id": "2",
          "time": "20210830 17:38"
        },
        {
          "id": "MDRW2100036144.1.21",
          "form": "ㅋㅋㅋㅋ..",
          "original_form": "ㅋㅋㅋㅋ..",
          "speaker_id": "2",
          "time": "20210830 17:38"
        },
        {
          "id": "MDRW2100036144.1.22",
          "form": "벌이 어디서 날아왔어요",
          "original_form": "벌이 어디서 날아왔어요",
          "speaker_id": "1",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100036144.1.23",
          "form": "그러게요 이놈의벌어디서 그렇게나타나는지  미쳐버리겠어요",
          "original_form": "그러게요 이놈의벌어디서 그렇게나타나는지  미쳐버리겠어요",
          "speaker_id": "2",
          "time": "20210830 17:40"
        },
        {
          "id": "MDRW2100036144.1.24",
          "form": "ㄱㅋㅋㄱㅈㅋ",
          "original_form": "ㄱㅋㅋㄱㅈㅋ",
          "speaker_id": "2",
          "time": "20210830 17:40"
        },
        {
          "id": "MDRW2100036144.1.25",
          "form": "저는 벌 하나만 봐도 무서운데",
          "original_form": "저는 벌 하나만 봐도 무서운데",
          "speaker_id": "1",
          "time": "20210830 17:40"
        }
      ]
