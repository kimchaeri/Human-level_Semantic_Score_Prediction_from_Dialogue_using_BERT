      "utterance": [
        {
          "id": "MDRW2100019417.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100019417.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210827 19:07"
        },
        {
          "id": "MDRW2100019417.1.3",
          "form": "쇼핑과 상품..",
          "original_form": "쇼핑과 상품..",
          "speaker_id": "1",
          "time": "20210827 19:08"
        },
        {
          "id": "MDRW2100019417.1.4",
          "form": "혹시 구매하고 싶은 물건은 있으세요?",
          "original_form": "혹시 구매하고 싶은 물건은 있으세요?",
          "speaker_id": "1",
          "time": "20210827 19:08"
        },
        {
          "id": "MDRW2100019417.1.5",
          "form": "전 요즘 아이폰 출시일만 기다리고 있거든요",
          "original_form": "전 요즘 아이폰 출시일만 기다리고 있거든요",
          "speaker_id": "1",
          "time": "20210827 19:08"
        },
        {
          "id": "MDRW2100019417.1.6",
          "form": "저는 요즘 z플립3요!",
          "original_form": "저는 요즘 z플립3요!",
          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019417.1.7",
          "form": "오! 구매하실 예정이신가요?",
          "original_form": "오! 구매하실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019417.1.8",
          "form": "안그래도 유튜브에 가끔 뜨는데 예쁘긴 하더라고요",
          "original_form": "안그래도 유튜브에 가끔 뜨는데 예쁘긴 하더라고요",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019417.1.9",
          "form": "살짝 마음이 흔들리긴 했는데...",
          "original_form": "살짝 마음이 흔들리긴 했는데...",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019417.1.10",
          "form": "고민중이에요",
          "original_form": "고민중이에요",
          "speaker_id": "2",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019417.1.11",
          "form": "색깔도 너무 예쁘게 나와서ㅎㅎ",
          "original_form": "색깔도 너무 예쁘게 나와서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019417.1.12",
          "form": "아이폰 출시일은 언제인가요?",
          "original_form": "아이폰 출시일은 언제인가요?",
          "speaker_id": "2",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019417.1.13",
          "form": "아직 정확하게 안나왔어요!",
          "original_form": "아직 정확하게 안나왔어요!",
          "speaker_id": "1",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019417.1.14",
          "form": "다음 달 중반 정도에 나오고 아마 한국엔 그보다 좀 후에 출시하니까..",
          "original_form": "다음 달 중반 정도에 나오고 아마 한국엔 그보다 좀 후에 출시하니까..",
          "speaker_id": "1",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019417.1.15",
          "form": "아직 많이 남았네요",
          "original_form": "아직 많이 남았네요",
          "speaker_id": "1",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019417.1.16",
          "form": "아 아직 정해지진 않았군요",
          "original_form": "아 아직 정해지진 않았군요",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019417.1.17",
          "form": "지금 사용하는 핸드폰이 점점 고장나고 있어서",
          "original_form": "지금 사용하는 핸드폰이 점점 고장나고 있어서",
          "speaker_id": "1",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019417.1.18",
          "form": "얼른 얼른 출시했으면 좋겠어요..",
          "original_form": "얼른 얼른 출시했으면 좋겠어요..",
          "speaker_id": "1",
          "time": "20210827 19:12"
        },
        {
          "id": "MDRW2100019417.1.19",
          "form": "저도요ㅠ",
          "original_form": "저도요ㅠ",
          "speaker_id": "2",
          "time": "20210827 19:12"
        },
        {
          "id": "MDRW2100019417.1.20",
          "form": "계속 아이폰 사용하셨나요?",
          "original_form": "계속 아이폰 사용하셨나요?",
          "speaker_id": "2",
          "time": "20210827 19:12"
        },
        {
          "id": "MDRW2100019417.1.21",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210827 19:12"
        },
        {
          "id": "MDRW2100019417.1.22",
          "form": "매번 안드로이드만 쓰다가 처음 바꿔보려구요",
          "original_form": "매번 안드로이드만 쓰다가 처음 바꿔보려구요",
          "speaker_id": "1",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100019417.1.23",
          "form": "사실 12때 바꾸려다가 일년을 더 버텼네요!",
          "original_form": "사실 12때 바꾸려다가 일년을 더 버텼네요!",
          "speaker_id": "1",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100019417.1.24",
          "form": "오! 저도 계속 안드로이드만 사용해봤어요",
          "original_form": "오! 저도 계속 안드로이드만 사용해봤어요",
          "speaker_id": "2",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100019417.1.25",
          "form": "오~ 아이폰 써보실 생각은 없으셨어요?",
          "original_form": "오~ 아이폰 써보실 생각은 없으셨어요?",
          "speaker_id": "1",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100019417.1.26",
          "form": "오래기다리셨네요ㅇ",
          "original_form": "오래기다리셨네요ㅇ",
          "speaker_id": "2",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100019417.1.27",
          "form": "그러게요..ㅠ",
          "original_form": "그러게요..ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100019417.1.28",
          "form": "또 버티다 보니까 어찌저찌 되더라구요",
          "original_form": "또 버티다 보니까 어찌저찌 되더라구요",
          "speaker_id": "1",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100019417.1.29",
          "form": "뭔가 계속 안드로이드만 사용하니까 불편할것같고 삼성페이 때문에ㅠ",
          "original_form": "뭔가 계속 안드로이드만 사용하니까 불편할것같고 삼성페이 때문에ㅠ",
          "speaker_id": "2",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100019417.1.30",
          "form": "얼레벌레 여기까지 왔어요",
          "original_form": "얼레벌레 여기까지 왔어요",
          "speaker_id": "1",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100019417.1.31",
          "form": "맞아요! 삼성페이 놓치기 너무 아쉽긴한데..",
          "original_form": "맞아요! 삼성페이 놓치기 너무 아쉽긴한데..",
          "speaker_id": "1",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100019417.1.32",
          "form": "지금 아이폰 한 번 안써보면 영원히 안쓸거 같아서",
          "original_form": "지금 아이폰 한 번 안써보면 영원히 안쓸거 같아서",
          "speaker_id": "1",
          "time": "20210827 19:15"
        },
        {
          "id": "MDRW2100019417.1.33",
          "form": "맘 먹었을 때 바꿔보려구요",
          "original_form": "맘 먹었을 때 바꿔보려구요",
          "speaker_id": "1",
          "time": "20210827 19:15"
        },
        {
          "id": "MDRW2100019417.1.34",
          "form": "오 결심하셨네요 저는 아마 아이폰 못써볼거같아요ㅋㅋ",
          "original_form": "오 결심하셨네요 저는 아마 아이폰 못써볼거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:16"
        },
        {
          "id": "MDRW2100019417.1.35",
          "form": "저도 솔직히 계속 안드로이드만 써서 불안하긴 해요",
          "original_form": "저도 솔직히 계속 안드로이드만 써서 불안하긴 해요",
          "speaker_id": "1",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019417.1.36",
          "form": "이번에 넘어간 친구가 있는데 그 친구도 쭉 갤럭시만 쓰다가 아이폰으로 넘어갔거든요",
          "original_form": "이번에 넘어간 친구가 있는데 그 친구도 쭉 갤럭시만 쓰다가 아이폰으로 넘어갔거든요",
          "speaker_id": "1",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019417.1.37",
          "form": "적응하는데 조금 힘들어 보이긴 하더라구요..",
          "original_form": "적응하는데 조금 힘들어 보이긴 하더라구요..",
          "speaker_id": "1",
          "time": "20210827 19:18"
        },
        {
          "id": "MDRW2100019417.1.38",
          "form": "저도 주변에 보면 친구들 다 아이폰 써서 가끔 구경할 때 조작하기 너무 어렵더라고요",
          "original_form": "저도 주변에 보면 친구들 다 아이폰 써서 가끔 구경할 때 조작하기 너무 어렵더라고요",
          "speaker_id": "2",
          "time": "20210827 19:19"
        },
        {
          "id": "MDRW2100019417.1.39",
          "form": "뭔가 복잡해요ㅎㅎ",
          "original_form": "뭔가 복잡해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:19"
        },
        {
          "id": "MDRW2100019417.1.40",
          "form": "그러니까요..",
          "original_form": "그러니까요..",
          "speaker_id": "1",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019417.1.41",
          "form": "아이폰 쓰는 사람들도 안드로 넘어오면 그런건지 궁금하네요",
          "original_form": "아이폰 쓰는 사람들도 안드로 넘어오면 그런건지 궁금하네요",
          "speaker_id": "1",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019417.1.42",
          "form": "보통 갤럭시에서 넘어가는건 많이 봤는데",
          "original_form": "보통 갤럭시에서 넘어가는건 많이 봤는데",
          "speaker_id": "1",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019417.1.43",
          "form": "반대는 잘 없더라구요",
          "original_form": "반대는 잘 없더라구요",
          "speaker_id": "1",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019417.1.44",
          "form": "로고때문도 있더라고요 디자인도 이유겠지만ㅎㅎ",
          "original_form": "로고때문도 있더라고요 디자인도 이유겠지만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100019417.1.45",
          "form": "확실히 갤럭시는 삼성로고가 별로라고 많이 말하긴 하죠",
          "original_form": "확실히 갤럭시는 삼성로고가 별로라고 많이 말하긴 하죠",
          "speaker_id": "1",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019417.1.46",
          "form": "근데 전 로고는 크게 신경 안쓰이더라구요",
          "original_form": "근데 전 로고는 크게 신경 안쓰이더라구요",
          "speaker_id": "1",
          "time": "20210827 19:37"
        }
      ]
