      "utterance": [
        {
          "id": "MDRW2100041714.1.1",
          "form": "청귤에이드 짱맛있어요",
          "original_form": "청귤에이드 짱맛있어요",
          "speaker_id": "2",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041714.1.2",
          "form": "그렇겠어요ㅠㅠㅠ 직접해먹으면 더 맛잇을듯",
          "original_form": "그렇겠어요ㅠㅠㅠ 직접해먹으면 더 맛잇을듯",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100041714.1.3",
          "form": "님은 커피나 음료 어떤거 주로 드세요",
          "original_form": "님은 커피나 음료 어떤거 주로 드세요",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100041714.1.4",
          "form": "커피는 집에서는 아메먹고",
          "original_form": "커피는 집에서는 아메먹고",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041714.1.5",
          "form": "카페가면 프라푸치노요",
          "original_form": "카페가면 프라푸치노요",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041714.1.6",
          "form": "캡슐머쉰같은거있으신가여",
          "original_form": "캡슐머쉰같은거있으신가여",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100041714.1.7",
          "form": "그런건 업고요ㅠ",
          "original_form": "그런건 업고요ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041714.1.8",
          "form": "저 영화보러 가서 또 이야기해야 남갸쥬세여",
          "original_form": "저 영화보러 가서 또 이야기해야 남갸쥬세여",
          "speaker_id": "2",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041714.1.9",
          "form": "가는길에 저도 커피한잔 해야겠어요",
          "original_form": "가는길에 저도 커피한잔 해야겠어요",
          "speaker_id": "2",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041714.1.10",
          "form": "네네  맛커하세요",
          "original_form": "네네  맛커하세요",
          "speaker_id": "1",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100041714.1.11",
          "form": "저녁은 뭐 드셨나요?",
          "original_form": "저녁은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100041714.1.12",
          "form": "저 중식이요 볶음밥이랑 완탕",
          "original_form": "저 중식이요 볶음밥이랑 완탕",
          "speaker_id": "2",
          "time": "20210906 20:44"
        },
        {
          "id": "MDRW2100041714.1.13",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100041714.1.14",
          "form": "오 맛난거 드셨네요ㅠ전부대찌개요ㅠ",
          "original_form": "오 맛난거 드셨네요ㅠ전부대찌개요ㅠ",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100041714.1.15",
          "form": "짜지는 않았어요?",
          "original_form": "짜지는 않았어요?",
          "speaker_id": "2",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100041714.1.16",
          "form": "조금요ㅋㅋ",
          "original_form": "조금요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100041714.1.17",
          "form": "전짠것도 잘먹어요ㅋ",
          "original_form": "전짠것도 잘먹어요ㅋ",
          "speaker_id": "1",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100041714.1.18",
          "form": "놀부부대찌개??",
          "original_form": "놀부부대찌개??",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100041714.1.19",
          "form": "지에스에서 편키트라고 나왓더라구요",
          "original_form": "지에스에서 편키트라고 나왓더라구요",
          "speaker_id": "1",
          "time": "20210906 21:00"
        }
