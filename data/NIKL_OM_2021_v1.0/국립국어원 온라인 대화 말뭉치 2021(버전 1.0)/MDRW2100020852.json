      "utterance": [
        {
          "id": "MDRW2100020852.1.1",
          "form": "저번에 올림픽 보셨어요?",
          "original_form": "저번에 올림픽 보셨어요?",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020852.1.2",
          "form": "네네 봤어요! 넘 재밌더라구요",
          "original_form": "네네 봤어요! 넘 재밌더라구요",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020852.1.3",
          "form": "양궁이랑 배구 기억에남네요",
          "original_form": "양궁이랑 배구 기억에남네요",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020852.1.4",
          "form": "그쵸 ㅎㅎㅎ",
          "original_form": "그쵸 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020852.1.5",
          "form": "뭐로 보셨어요? 티비? 태블릿?",
          "original_form": "뭐로 보셨어요? 티비? 태블릿?",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020852.1.6",
          "form": "티비로 봤어요 ㅋㅋㅋㅋname2님은요?",
          "original_form": "티비로 봤어요 ㅋㅋㅋㅋ&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020852.1.7",
          "form": "저는 노트북으로 봤어요~",
          "original_form": "저는 노트북으로 봤어요~",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020852.1.8",
          "form": "이런거 직관해야되는데 집관했네요 ㅜㅜ",
          "original_form": "이런거 직관해야되는데 집관했네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020852.1.9",
          "form": "집관 ㅋㅋㅋㅋㅋ맞아요 실제로 보면 재밌을고같아요",
          "original_form": "집관 ㅋㅋㅋㅋㅋ맞아요 실제로 보면 재밌을고같아요",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020852.1.10",
          "form": "직접 관람하러 가보신적 잇으세요?",
          "original_form": "직접 관람하러 가보신적 잇으세요?",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020852.1.11",
          "form": "있죠 ㅎㅎ 근데 다 코로나 이전 이야기에요",
          "original_form": "있죠 ㅎㅎ 근데 다 코로나 이전 이야기에요",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020852.1.12",
          "form": "그땐 티켓 같은거 뿌리고 그랬어요..",
          "original_form": "그땐 티켓 같은거 뿌리고 그랬어요..",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020852.1.13",
          "form": "아 그래요?? ㅠㅠ",
          "original_form": "아 그래요?? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020852.1.14",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020852.1.15",
          "form": "보는 건 좋아하는데 하는건 안좋아하네요...",
          "original_form": "보는 건 좋아하는데 하는건 안좋아하네요...",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020852.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그저 구경꾼일 뿐..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그저 구경꾼일 뿐..",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020852.1.17",
          "form": "ㅋㅋㅋㅋㅋ아 그렇군요 어떤 스포츠 가장 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋㅋ아 그렇군요 어떤 스포츠 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020852.1.18",
          "form": "요즘 배구가 재밌어지긴 했는데..",
          "original_form": "요즘 배구가 재밌어지긴 했는데..",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020852.1.19",
          "form": "그래도 펜싱이 제일 좋아요!",
          "original_form": "그래도 펜싱이 제일 좋아요!",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020852.1.20",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020852.1.21",
          "form": "저는 스포츠는 크게 관심은 없어요ㅋㅋㅋㅋ",
          "original_form": "저는 스포츠는 크게 관심은 없어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020852.1.22",
          "form": "요번에 배구 제대로봤는데 재밌어서 관심가지는중이에요!",
          "original_form": "요번에 배구 제대로봤는데 재밌어서 관심가지는중이에요!",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020852.1.23",
          "form": "배구 ㅎㅎ 어떤게 인상적이였어요?",
          "original_form": "배구 ㅎㅎ 어떤게 인상적이였어요?",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020852.1.24",
          "form": "점수차 좁히며 역전할때요 ㅠㅠ 멋잇더라구요",
          "original_form": "점수차 좁히며 역전할때요 ㅠㅠ 멋잇더라구요",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020852.1.25",
          "form": "아 ㅋㅋ 그 점수 뒤쳐지고 있었는데 따라잡는거",
          "original_form": "아 ㅋㅋ 그 점수 뒤쳐지고 있었는데 따라잡는거",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100020852.1.26",
          "form": "그거 진짜 보통 멘탈 아닌 것 같아요 그쵸?",
          "original_form": "그거 진짜 보통 멘탈 아닌 것 같아요 그쵸?",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100020852.1.27",
          "form": "맞아요 진짜 다들 멘탈 대단해요 ㅠㅠ",
          "original_form": "맞아요 진짜 다들 멘탈 대단해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:35"
        }
      ]
