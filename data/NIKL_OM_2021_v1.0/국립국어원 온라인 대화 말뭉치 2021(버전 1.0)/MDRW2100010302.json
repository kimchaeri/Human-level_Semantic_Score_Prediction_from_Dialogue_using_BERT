      "utterance": [
        {
          "id": "MDRW2100010302.1.1",
          "form": "안녕하세요~ 점심 뭐드셨어요?",
          "original_form": "안녕하세요~ 점심 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210823 16:34"
        },
        {
          "id": "MDRW2100010302.1.2",
          "form": "저는 육회비빔밥을 먹었어요",
          "original_form": "저는 육회비빔밥을 먹었어요",
          "speaker_id": "2",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010302.1.3",
          "form": "오늘 저녁은 뭐드실 예정이에요?",
          "original_form": "오늘 저녁은 뭐드실 예정이에요?",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010302.1.4",
          "form": "헐 육비 ㅠㅠ맛있었겠어요 저도 육비좋아하는데",
          "original_form": "헐 육비 ㅠㅠ맛있었겠어요 저도 육비좋아하는데",
          "speaker_id": "1",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100010302.1.5",
          "form": "점심을 너무 잘먹어서 저녁은 안먹으려구요 ㅎㅎ",
          "original_form": "점심을 너무 잘먹어서 저녁은 안먹으려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100010302.1.6",
          "form": "그러시구나ㅎ",
          "original_form": "그러시구나ㅎ",
          "speaker_id": "2",
          "time": "20210823 16:42"
        },
        {
          "id": "MDRW2100010302.1.7",
          "form": "요리하는거 좋아하세요?",
          "original_form": "요리하는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 16:42"
        },
        {
          "id": "MDRW2100010302.1.8",
          "form": "하는것보단 먹는거좋아해요 ㅋㅋㅋㅋ",
          "original_form": "하는것보단 먹는거좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:44"
        },
        {
          "id": "MDRW2100010302.1.9",
          "form": "맛있게 하는게 자신이없어섷",
          "original_form": "맛있게 하는게 자신이없어섷",
          "speaker_id": "1",
          "time": "20210823 16:44"
        },
        {
          "id": "MDRW2100010302.1.10",
          "form": "생각보다 어렵지 않은 음식들도 많아요",
          "original_form": "생각보다 어렵지 않은 음식들도 많아요",
          "speaker_id": "2",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100010302.1.11",
          "form": "저는 지난주에",
          "original_form": "저는 지난주에",
          "speaker_id": "2",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100010302.1.12",
          "form": "집에 감자가 많아서 감자수프 만들고",
          "original_form": "집에 감자가 많아서 감자수프 만들고",
          "speaker_id": "2",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010302.1.13",
          "form": "두부 유통기한이 조금 지났길래 두부강정 만들어먹었어요",
          "original_form": "두부 유통기한이 조금 지났길래 두부강정 만들어먹었어요",
          "speaker_id": "2",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010302.1.14",
          "form": "유튜브보면 많아요",
          "original_form": "유튜브보면 많아요",
          "speaker_id": "2",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100010302.1.15",
          "form": "우와 요리잘하시는구나",
          "original_form": "우와 요리잘하시는구나",
          "speaker_id": "1",
          "time": "20210823 16:52"
        },
        {
          "id": "MDRW2100010302.1.16",
          "form": "잘하지 못하는데",
          "original_form": "잘하지 못하는데",
          "speaker_id": "2",
          "time": "20210823 16:53"
        },
        {
          "id": "MDRW2100010302.1.17",
          "form": "요리해본것중에 제일 맛있었던거 있으세요?",
          "original_form": "요리해본것중에 제일 맛있었던거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 16:53"
        },
        {
          "id": "MDRW2100010302.1.18",
          "form": "최근에한것 중에 감자수프 맛있었어요",
          "original_form": "최근에한것 중에 감자수프 맛있었어요",
          "speaker_id": "2",
          "time": "20210823 16:53"
        },
        {
          "id": "MDRW2100010302.1.19",
          "form": "커피는좋아하세요",
          "original_form": "커피는좋아하세요",
          "speaker_id": "2",
          "time": "20210823 16:53"
        },
        {
          "id": "MDRW2100010302.1.20",
          "form": "저 수프별로 안좋아하는데 감자여서 맛있었겠어요! 감자좋아하거든요 ㅎㅎ",
          "original_form": "저 수프별로 안좋아하는데 감자여서 맛있었겠어요! 감자좋아하거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:54"
        },
        {
          "id": "MDRW2100010302.1.21",
          "form": "넹 커피 필수죠~ 무조건 아아",
          "original_form": "넹 커피 필수죠~ 무조건 아아",
          "speaker_id": "1",
          "time": "20210823 16:54"
        },
        {
          "id": "MDRW2100010302.1.22",
          "form": "마스터 쉐프 코리아인가? 거기나온 닭봉조림 친구가해줘서 먹어봣는데 완전맛있어요",
          "original_form": "마스터 쉐프 코리아인가? 거기나온 닭봉조림 친구가해줘서 먹어봣는데 완전맛있어요",
          "speaker_id": "1",
          "time": "20210823 16:55"
        },
        {
          "id": "MDRW2100010302.1.23",
          "form": "오늘 저는 저녁에 오리탕을 처음 먹어봤는데",
          "original_form": "오늘 저는 저녁에 오리탕을 처음 먹어봤는데",
          "speaker_id": "2",
          "time": "20210823 21:36"
        },
        {
          "id": "MDRW2100010302.1.24",
          "form": "시원하고 맛잇더라고요",
          "original_form": "시원하고 맛잇더라고요",
          "speaker_id": "2",
          "time": "20210823 21:36"
        },
        {
          "id": "MDRW2100010302.1.25",
          "form": "오리도 맛있어요",
          "original_form": "오리도 맛있어요",
          "speaker_id": "2",
          "time": "20210823 21:36"
        },
        {
          "id": "MDRW2100010302.1.26",
          "form": "우와 오리탕",
          "original_form": "우와 오리탕",
          "speaker_id": "1",
          "time": "20210823 21:37"
        }
      ]
    }
