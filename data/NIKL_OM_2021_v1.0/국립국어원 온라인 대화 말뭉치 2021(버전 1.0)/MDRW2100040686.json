      "utterance": [
        {
          "id": "MDRW2100040686.1.1",
          "form": "어 그렇지 ㅋㅋㄱㅋㅋㅋㄱ",
          "original_form": "어 그렇지 ㅋㅋㄱㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.2",
          "form": "공평하게 ㅋㅋㅋㅋㄱ",
          "original_form": "공평하게 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.3",
          "form": "그래서 아스크림 하나 사먹고",
          "original_form": "그래서 아스크림 하나 사먹고",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.4",
          "form": "말랑이 사왔대 ㅋㅋㅋ",
          "original_form": "말랑이 사왔대 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.5",
          "form": "3000원은 쎄다. ㅋㅋ2000원이면됐지뭐ㅋㅋ잘됐넥",
          "original_form": "3000원은 쎄다. ㅋㅋ2000원이면됐지뭐ㅋㅋ잘됐넥",
          "speaker_id": "1",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.6",
          "form": "ㅋㅋㅋㅋㅋ맞어",
          "original_form": "ㅋㅋㅋㅋㅋ맞어",
          "speaker_id": "1",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.7",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.8",
          "form": "애들 그렇게 잘 사더라ㅋ",
          "original_form": "애들 그렇게 잘 사더라ㅋ",
          "speaker_id": "1",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.9",
          "form": "천원으로는 할 게 없고",
          "original_form": "천원으로는 할 게 없고",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.10",
          "form": "3000원은 많으니",
          "original_form": "3000원은 많으니",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.11",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.12",
          "form": "2000원이 제일 적당한 듯",
          "original_form": "2000원이 제일 적당한 듯",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.13",
          "form": "그렇지ㅋㅋ",
          "original_form": "그렇지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.14",
          "form": "근데 일주일에 2000원이면 괜찮은데",
          "original_form": "근데 일주일에 2000원이면 괜찮은데",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100040686.1.15",
          "form": "내일 토 2000원 달라고 할 거 아니야",
          "original_form": "내일 토 2000원 달라고 할 거 아니야",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
