      "utterance": [
        {
          "id": "MDRW2100006188.1.1",
          "form": "가을 그만타시고",
          "original_form": "가을 그만타시고",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100006188.1.2",
          "form": "밥은 드세요",
          "original_form": "밥은 드세요",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100006188.1.3",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100006188.1.4",
          "form": "name2님은 어느 계절 좋아해요?",
          "original_form": "&name2&님은 어느 계절 좋아해요?",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100006188.1.5",
          "form": "저 여름빼고 다요",
          "original_form": "저 여름빼고 다요",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100006188.1.6",
          "form": "봄 가을이 제일 좋지만",
          "original_form": "봄 가을이 제일 좋지만",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100006188.1.7",
          "form": "눈만 많이 안오면 겨울도 좋아해요",
          "original_form": "눈만 많이 안오면 겨울도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100006188.1.8",
          "form": "오 역시 30대는 봄가을이죠 ㅋㅋ",
          "original_form": "오 역시 30대는 봄가을이죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100006188.1.9",
          "form": "여자친구랑 처음 만난\r\n계절은요?",
          "original_form": "여자친구랑 처음 만난\r\n계절은요?",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100006188.1.10",
          "form": "6월이니까",
          "original_form": "6월이니까",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100006188.1.11",
          "form": "초여름?",
          "original_form": "초여름?",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100006188.1.12",
          "form": "오 초여름...ㅋㅋ",
          "original_form": "오 초여름...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006188.1.13",
          "form": "저는 생각해보니\r\n다 봄에 만난거 같아요ㅋㅋ",
          "original_form": "저는 생각해보니\r\n다 봄에 만난거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006188.1.14",
          "form": "긴팔입긴 더운",
          "original_form": "긴팔입긴 더운",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006188.1.15",
          "form": "남친 있고 없고가 가을타는데 영향이 있었나요?",
          "original_form": "남친 있고 없고가 가을타는데 영향이 있었나요?",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006188.1.16",
          "form": "없었던거 같아요ㅋㅋ",
          "original_form": "없었던거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006188.1.17",
          "form": "별개였던거 같음",
          "original_form": "별개였던거 같음",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006188.1.18",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100006188.1.19",
          "form": "그럼 가을 제대로 타시네",
          "original_form": "그럼 가을 제대로 타시네",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100006188.1.20",
          "form": "그럼 가을탈때마다",
          "original_form": "그럼 가을탈때마다",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100006188.1.21",
          "form": "모해요?",
          "original_form": "모해요?",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100006188.1.22",
          "form": "음악들으면서 많이 걷고",
          "original_form": "음악들으면서 많이 걷고",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100006188.1.23",
          "form": "혼자 멍도 많이 때려요ㅋㅋ",
          "original_form": "혼자 멍도 많이 때려요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100006188.1.24",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006188.1.25",
          "form": "진짜 제대로 타시는거네",
          "original_form": "진짜 제대로 타시는거네",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006188.1.26",
          "form": "가을비까지 오니...",
          "original_form": "가을비까지 오니...",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006188.1.27",
          "form": "타는 계절 없으세요?",
          "original_form": "타는 계절 없으세요?",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006188.1.28",
          "form": "지금까지 살아오면서",
          "original_form": "지금까지 살아오면서",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006188.1.29",
          "form": "타본건 썸바께 없죠? ㅋㅋ",
          "original_form": "타본건 썸바께 없죠? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006188.1.30",
          "form": "네 안타요 ㅋㅋ",
          "original_form": "네 안타요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006188.1.31",
          "form": "타는거요?",
          "original_form": "타는거요?",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006188.1.32",
          "form": "차타요",
          "original_form": "차타요",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006188.1.33",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006188.1.34",
          "form": "올 지지 않으시네ㅋㅋ",
          "original_form": "올 지지 않으시네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:37"
        }
      ]
    }
