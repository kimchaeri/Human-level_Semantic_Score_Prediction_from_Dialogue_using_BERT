      "utterance": [
        {
          "id": "MDRW2100030083.1.1",
          "form": "건강관리는 어떻게 하고 있나요?",
          "original_form": "건강관리는 어떻게 하고 있나요?",
          "speaker_id": "1",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100030083.1.2",
          "form": "홈트와 비타민...? ㅎㅎ",
          "original_form": "홈트와 비타민...? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100030083.1.3",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100030083.1.4",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100030083.1.5",
          "form": "전 산책을 하고 있습니다",
          "original_form": "전 산책을 하고 있습니다",
          "speaker_id": "1",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100030083.1.6",
          "form": "산책도 좋죠!",
          "original_form": "산책도 좋죠!",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100030083.1.7",
          "form": "예전엔 헬스장도 다녔는데",
          "original_form": "예전엔 헬스장도 다녔는데",
          "speaker_id": "1",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030083.1.8",
          "form": "저도 산책도 해요 ㅋㅋ",
          "original_form": "저도 산책도 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030083.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030083.1.10",
          "form": "코로나 이후로 못가고 있지요",
          "original_form": "코로나 이후로 못가고 있지요",
          "speaker_id": "1",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030083.1.11",
          "form": "그르쵸 ㅠㅠㅠ 코로나 ㅠㅠㅠ",
          "original_form": "그르쵸 ㅠㅠㅠ 코로나 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030083.1.12",
          "form": "수도권은 여전히 4단계이니...",
          "original_form": "수도권은 여전히 4단계이니...",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030083.1.13",
          "form": "불가능하죠 ㅠ",
          "original_form": "불가능하죠 ㅠ",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100030083.1.14",
          "form": "네 위험해요",
          "original_form": "네 위험해요",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100030083.1.15",
          "form": "뭐 여기도 3단계라...별다를바없긴하지만",
          "original_form": "뭐 여기도 3단계라...별다를바없긴하지만",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100030083.1.16",
          "form": "비타민도 챙겨먹습니다",
          "original_form": "비타민도 챙겨먹습니다",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100030083.1.17",
          "form": "네 ㅎㅎ 비타민 좋죠!",
          "original_form": "네 ㅎㅎ 비타민 좋죠!",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100030083.1.18",
          "form": "이젠 밥심이 아니라 약심으로 사는 듯 하네요",
          "original_form": "이젠 밥심이 아니라 약심으로 사는 듯 하네요",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100030083.1.19",
          "form": "?!!!!",
          "original_form": "?!!!!",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100030083.1.20",
          "form": "밥심 플러스 약심..?",
          "original_form": "밥심 플러스 약심..?",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030083.1.21",
          "form": "인거죠???",
          "original_form": "인거죠???",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030083.1.22",
          "form": "밥의 힘이 아니라",
          "original_form": "밥의 힘이 아니라",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030083.1.23",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030083.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030083.1.25",
          "form": "저는 정말 밥심...",
          "original_form": "저는 정말 밥심...",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030083.1.26",
          "form": "약으로 버티는 나이인거죠",
          "original_form": "약으로 버티는 나이인거죠",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030083.1.27",
          "form": "밥안먹으몀 아무것도 못하겠어요",
          "original_form": "밥안먹으몀 아무것도 못하겠어요",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030083.1.28",
          "form": "뇌가 안돌아가요 ㅠㅠ",
          "original_form": "뇌가 안돌아가요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030083.1.29",
          "form": "name2님은 아직 젊으니까요",
          "original_form": "&name2&님은 아직 젊으니까요",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100030083.1.30",
          "form": "미리미리 건강관리해야",
          "original_form": "미리미리 건강관리해야",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100030083.1.31",
          "form": "나중에도 좋답니다",
          "original_form": "나중에도 좋답니다",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100030083.1.32",
          "form": "안...젊은데요?",
          "original_form": "안...젊은데요?",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100030083.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100030083.1.34",
          "form": "젊은 나이는 어디까지인가요?",
          "original_form": "젊은 나이는 어디까지인가요?",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100030083.1.35",
          "form": "그런가요? 다 저보다 젊어보이네요 ㅎㅎ",
          "original_form": "그런가요? 다 저보다 젊어보이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100030083.1.36",
          "form": "ㅋㅋㅋㅋㅋ 아니에요 가끔 오십대분들도 있어요!",
          "original_form": "ㅋㅋㅋㅋㅋ 아니에요 가끔 오십대분들도 있어요!",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100030083.1.37",
          "form": "그에비하면 다 젊은거일거에요!",
          "original_form": "그에비하면 다 젊은거일거에요!",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100030083.1.38",
          "form": "ㅎㅎ 그렇군요",
          "original_form": "ㅎㅎ 그렇군요",
          "speaker_id": "1",
          "time": "20210903 21:16"
        }
      ]
