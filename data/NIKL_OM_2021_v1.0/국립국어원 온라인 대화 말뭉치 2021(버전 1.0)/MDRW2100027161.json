      "utterance": [
        {
          "id": "MDRW2100027161.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100027161.1.2",
          "form": "안녕하세요~반려동물 있으세요?",
          "original_form": "안녕하세요~반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100027161.1.3",
          "form": "네 고양이 한 마리 키우고있습니다~",
          "original_form": "네 고양이 한 마리 키우고있습니다~",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027161.1.4",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027161.1.5",
          "form": "저는 안 키우는데",
          "original_form": "저는 안 키우는데",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027161.1.6",
          "form": "친구들이 몇마리 키웁니다",
          "original_form": "친구들이 몇마리 키웁니다",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027161.1.7",
          "form": "고양이 이름이 뭐예요?",
          "original_form": "고양이 이름이 뭐예요?",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027161.1.8",
          "form": "name3 이에요!",
          "original_form": "&name3& 이에요!",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027161.1.9",
          "form": "고양이 좋아하세요?",
          "original_form": "고양이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027161.1.10",
          "form": "name3! 정말 특이하네요?",
          "original_form": "&name3&! 정말 특이하네요?",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027161.1.11",
          "form": "고양이 정말 좋아해요",
          "original_form": "고양이 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027161.1.12",
          "form": "근데 어떻게 만지는지는 잘 몰라서...먼 발치에서 보기만 한답니다",
          "original_form": "근데 어떻게 만지는지는 잘 몰라서...먼 발치에서 보기만 한답니다",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027161.1.13",
          "form": "고양이 처음 만질 때 좀 난감하죠 ㅋㅋㅋㅋ 만졌다가 화 내는 부위도 있고 그래서 ㅎㅎ",
          "original_form": "고양이 처음 만질 때 좀 난감하죠 ㅋㅋㅋㅋ 만졌다가 화 내는 부위도 있고 그래서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027161.1.14",
          "form": "고양이가 다가오면 뒷통수나 등부분 긁어주시면 좋아할거에요 ㅎㅎ",
          "original_form": "고양이가 다가오면 뒷통수나 등부분 긁어주시면 좋아할거에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027161.1.15",
          "form": "근데 이 친구가 무는 습관이 있어서",
          "original_form": "근데 이 친구가 무는 습관이 있어서",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027161.1.16",
          "form": "전에 팔에 빵꾸 몇 개 뚫린 후로는 그저 눈으로만 예뻐해주고 있어요 ㅋㅋㅋ",
          "original_form": "전에 팔에 빵꾸 몇 개 뚫린 후로는 그저 눈으로만 예뻐해주고 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100027161.1.17",
          "form": "아이고ㅠㅠ 애기가 사냥습관이 남아있나보네요 ㅠㅠ",
          "original_form": "아이고ㅠㅠ 애기가 사냥습관이 남아있나보네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027161.1.18",
          "form": "그 쪼끄만 이빨로 물겠다고 무는거 어이없을 때 있어요 ㅋㅋㅋ",
          "original_form": "그 쪼끄만 이빨로 물겠다고 무는거 어이없을 때 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027161.1.19",
          "form": "그 쪼끄만 이빨이 증말 세도라고요 골골...",
          "original_form": "그 쪼끄만 이빨이 증말 세도라고요 골골...",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027161.1.20",
          "form": "근데 전 알러지가 있어서",
          "original_form": "근데 전 알러지가 있어서",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027161.1.21",
          "form": "강아지 고양이 둘 다 있어가지고 앞으로도 못 키워요",
          "original_form": "강아지 고양이 둘 다 있어가지고 앞으로도 못 키워요",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027161.1.22",
          "form": "슬픈 외사랑",
          "original_form": "슬픈 외사랑",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027161.1.23",
          "form": "아이고ㅠㅠ 그럼 바라만 보셔야죠ㅠㅠ\r\n한 공간에 있는 것도 힘드실텐데 많이 예뻐하시나봐요",
          "original_form": "아이고ㅠㅠ 그럼 바라만 보셔야죠ㅠㅠ\r\n한 공간에 있는 것도 힘드실텐데 많이 예뻐하시나봐요",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100027161.1.24",
          "form": "그래도 건강이 먼저에요ㅜㅜ",
          "original_form": "그래도 건강이 먼저에요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027161.1.25",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027161.1.26",
          "form": "제 친구네 고양인 식탐이 거의 없도라고요...",
          "original_form": "제 친구네 고양인 식탐이 거의 없도라고요...",
