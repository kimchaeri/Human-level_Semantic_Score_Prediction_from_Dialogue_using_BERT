      "utterance": [
        {
          "id": "MDRW2100026987.1.1",
          "form": "오늘 먹은 알볼로피자어땠어",
          "original_form": "오늘 먹은 알볼로피자어땠어",
          "speaker_id": "1",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100026987.1.2",
          "form": "간이 심심한게 맛있었어",
          "original_form": "간이 심심한게 맛있었어",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100026987.1.3",
          "form": "다른 피자는 너무 짜",
          "original_form": "다른 피자는 너무 짜",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100026987.1.4",
          "form": "맞아 난 다른 피자도 좋긴한데 가끔 너무 짜더라",
          "original_form": "맞아 난 다른 피자도 좋긴한데 가끔 너무 짜더라",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100026987.1.5",
          "form": "다음에는 다른 맛 먹어보자",
          "original_form": "다음에는 다른 맛 먹어보자",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100026987.1.6",
          "form": "다음에는 치즈 많은 걸로",
          "original_form": "다음에는 치즈 많은 걸로",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100026987.1.7",
          "form": "알겠어",
          "original_form": "알겠어",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100026987.1.8",
          "form": "아 나 오늘 조선호텔 석박지 샀어",
          "original_form": "아 나 오늘 조선호텔 석박지 샀어",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100026987.1.9",
          "form": "석박지 집에 있잖아",
          "original_form": "석박지 집에 있잖아",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100026987.1.10",
          "form": "없어 내가 다먹었어",
          "original_form": "없어 내가 다먹었어",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100026987.1.11",
          "form": "만들어먹지 뭐하러사",
          "original_form": "만들어먹지 뭐하러사",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100026987.1.12",
          "form": "힘들게 뭘 만들어먹어",
          "original_form": "힘들게 뭘 만들어먹어",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100026987.1.13",
          "form": "그리고 조선호텔 김치 엄청 맛있대",
          "original_form": "그리고 조선호텔 김치 엄청 맛있대",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100026987.1.14",
          "form": "그럼 먹어보고 나중에 사먹자",
          "original_form": "그럼 먹어보고 나중에 사먹자",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100026987.1.15",
          "form": "종류도 엄청 많더라 배추김치총각김치 겉절이도 있던데",
          "original_form": "종류도 엄청 많더라 배추김치총각김치 겉절이도 있던데",
          "speaker_id": "1",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100026987.1.16",
          "form": "콩국수랑 먹게 겉절이로 사야겠네 가격은",
          "original_form": "콩국수랑 먹게 겉절이로 사야겠네 가격은",
          "speaker_id": "2",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100026987.1.17",
          "form": "김치를 안사봐서 비싼지 안비싼지 모르겠네",
          "original_form": "김치를 안사봐서 비싼지 안비싼지 모르겠네",
