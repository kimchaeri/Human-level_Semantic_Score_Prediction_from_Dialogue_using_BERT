        {
          "id": "MDRW2100034654.1.1",
          "form": "안대",
          "original_form": "안대",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100034654.1.2",
          "form": "글케열심히 다녔는데",
          "original_form": "글케열심히 다녔는데",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100034654.1.3",
          "form": "ㅇㅇ근데 수업듣고 바로 뽝해서 따야한대요",
          "original_form": "ㅇㅇ근데 수업듣고 바로 뽝해서 따야한대요",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034654.1.4",
          "form": "안그럼 흐지부지되서 못딴다고",
          "original_form": "안그럼 흐지부지되서 못딴다고",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034654.1.5",
          "form": "수업 쫒아가기 어렵다니 힘들겠다",
          "original_form": "수업 쫒아가기 어렵다니 힘들겠다",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034654.1.6",
          "form": "그부분은 캐공감",
          "original_form": "그부분은 캐공감",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034654.1.7",
          "form": "첨하는거자너",
          "original_form": "첨하는거자너",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034654.1.8",
          "form": "어렵긴한대 해야한다 붙어야한다란 압박에 ㅋㅋ",
          "original_form": "어렵긴한대 해야한다 붙어야한다란 압박에 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034654.1.9",
          "form": "더 정신적으로 괴롭고이쑴",
          "original_form": "더 정신적으로 괴롭고이쑴",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034654.1.10",
          "form": "힘들겠다",
          "original_form": "힘들겠다",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100034654.1.11",
          "form": "시험때 너무 압박되서 잘하겠남",
          "original_form": "시험때 너무 압박되서 잘하겠남",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100034654.1.12",
          "form": "그래도 해야하는거니까ㅜㅜ",
          "original_form": "그래도 해야하는거니까ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034654.1.13",
          "form": "잘하믄좋지만",
          "original_form": "잘하믄좋지만",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034654.1.14",
          "form": "근데 또 딴애들은 얼마나 해봤게씀",
          "original_form": "근데 또 딴애들은 얼마나 해봤게씀",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034654.1.15",
          "form": "만족스럽지못해도 첨하는거니까 너그럽게 봐주라",
          "original_form": "만족스럽지못해도 첨하는거니까 너그럽게 봐주라",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034654.1.16",
          "form": "구랭 다른거하다왔응께",
          "original_form": "구랭 다른거하다왔응께",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034654.1.17",
          "form": "물론 애들 얼굴보니까졸업반이나 갓졸업한애들같든데",
          "original_form": "물론 애들 얼굴보니까졸업반이나 갓졸업한애들같든데",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034654.1.18",
          "form": "갸들도 뭐 우리처럼 인문계열이면 비슷한수준아니겠음",
          "original_form": "갸들도 뭐 우리처럼 인문계열이면 비슷한수준아니겠음",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034654.1.19",
          "form": "공대생임몰라도",
          "original_form": "공대생임몰라도",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034654.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100034654.1.21",
          "form": "대화를안해봐서 모르제..",
          "original_form": "대화를안해봐서 모르제..",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100034654.1.22",
          "form": "아니 전에 실수로 선생님이",
          "original_form": "아니 전에 실수로 선생님이",
          "speaker_id": "1",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100034654.1.23",
          "form": "하지만 니가 생소한건알게씀 학교서 안쓰능가엑셀",
          "original_form": "하지만 니가 생소한건알게씀 학교서 안쓰능가엑셀",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100034654.1.24",
          "form": "애들명단을 화면에 띄웠는디",
          "original_form": "애들명단을 화면에 띄웠는디",
          "speaker_id": "1",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100034654.1.25",
