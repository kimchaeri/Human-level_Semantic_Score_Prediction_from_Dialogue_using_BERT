      "utterance": [
        {
          "id": "MDRW2100041921.1.1",
          "form": "이대 교직원도",
          "original_form": "이대 교직원도",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041921.1.2",
          "form": "계약직인가요?!",
          "original_form": "계약직인가요?!",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041921.1.3",
          "form": "근데 저희 과사무실에서 일하셨던 분 되게 오래 일하고 계셔요! 계약직이라고 들었는데",
          "original_form": "근데 저희 과사무실에서 일하셨던 분 되게 오래 일하고 계셔요! 계약직이라고 들었는데",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041921.1.4",
          "form": "2017년에도 계셨는데 지금도 계시거든요",
          "original_form": "2017년에도 계셨는데 지금도 계시거든요",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041921.1.5",
          "form": "대부분 계약직이 많아요ㅠ",
          "original_form": "대부분 계약직이 많아요ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041921.1.6",
          "form": "17년도부터 계속계신거면",
          "original_form": "17년도부터 계속계신거면",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041921.1.7",
          "form": "국고사업이신가,,,",
          "original_form": "국고사업이신가,,,",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041921.1.8",
          "form": "오오 국고사업?!",
          "original_form": "오오 국고사업?!",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041921.1.9",
          "form": "어려운,, 말이네요,, (숙연",
          "original_form": "어려운,, 말이네요,, (숙연",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041921.1.10",
          "form": "팀이나 직책변경하면서 2년넘게 계신분들도 있으시더라고요",
          "original_form": "팀이나 직책변경하면서 2년넘게 계신분들도 있으시더라고요",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041921.1.11",
          "form": "대부분 2년!",
          "original_form": "대부분 2년!",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041921.1.12",
          "form": "근데 중간에 직책 변경하셨어요!",
          "original_form": "근데 중간에 직책 변경하셨어요!",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041921.1.13",
          "form": "오오 네 맞아요!!! 약간 그런식으로?!",
          "original_form": "오오 네 맞아요!!! 약간 그런식으로?!",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041921.1.14",
          "form": "에이스사업 링크사업같은게 국고사업이에요!",
          "original_form": "에이스사업 링크사업같은게 국고사업이에요!",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041921.1.15",
          "form": "(그래도 못알아들음",
          "original_form": "(그래도 못알아들음",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041921.1.16",
          "form": "저....저도 내년에 취준해야하는데",
          "original_form": "저....저도 내년에 취준해야하는데",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041921.1.17",
          "form": "ㄱㅋㄱㄱㄱㄱㄱㅋㅋㄱㅋㅋㄱㅋㄱㅋ",
          "original_form": "ㄱㅋㄱㄱㄱㄱㄱㅋㅋㄱㅋㅋㄱㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041921.1.18",
          "form": "어떡하죠... (멍청",
          "original_form": "어떡하죠... (멍청",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041921.1.19",
          "form": "흐어어엉.....",
          "original_form": "흐어어엉.....",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041921.1.20",
          "form": "name1님은 지금 배우고있는분야쪽으로 준비하시면 되죠 ㅎㅎ",
          "original_form": "&name1&님은 지금 배우고있는분야쪽으로 준비하시면 되죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041921.1.21",
          "form": "히힛 감사해요,,,",
          "original_form": "히힛 감사해요,,,",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041921.1.22",
          "form": "취업이 잘 되어야할텐데 말이죠,,,",
          "original_form": "취업이 잘 되어야할텐데 말이죠,,,",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041921.1.23",
          "form": "어학성적이 자꾸",
          "original_form": "어학성적이 자꾸",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041921.1.24",
          "form": "2년이면 만기되니까 속상해요!!!",
          "original_form": "2년이면 만기되니까 속상해요!!!",
