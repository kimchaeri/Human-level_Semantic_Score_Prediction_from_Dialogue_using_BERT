      "utterance": [
        {
          "id": "MDRW2100005232.1.1",
          "form": "일은 안 힘드나",
          "original_form": "일은 안 힘드나",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005232.1.2",
          "form": "요즘 바쁜일 없어서 조금 괜찮아요",
          "original_form": "요즘 바쁜일 없어서 조금 괜찮아요",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100005232.1.3",
          "form": "저는 휴가가 이제 끝나요",
          "original_form": "저는 휴가가 이제 끝나요",
          "speaker_id": "1",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100005232.1.4",
          "form": "일하기싫네요",
          "original_form": "일하기싫네요",
          "speaker_id": "1",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100005232.1.5",
          "form": "저두 출근하기 싫어요ㅠㅠ",
          "original_form": "저두 출근하기 싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100005232.1.6",
          "form": "출근은 재미없지요",
          "original_form": "출근은 재미없지요",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100005232.1.7",
          "form": "네 내일 출근하는 날이네요",
          "original_form": "네 내일 출근하는 날이네요",
          "speaker_id": "2",
          "time": "20210822 20:03"
        },
        {
          "id": "MDRW2100005232.1.8",
          "form": "우울합니다ㅜ",
          "original_form": "우울합니다ㅜ",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100005232.1.9",
          "form": "비오는 날 일하니 싫네욤",
          "original_form": "비오는 날 일하니 싫네욤",
          "speaker_id": "2",
          "time": "20210824 10:12"
        },
        {
          "id": "MDRW2100005232.1.10",
          "form": "음 태풍 온다더니 금방갔네요",
          "original_form": "음 태풍 온다더니 금방갔네요",
          "speaker_id": "1",
          "time": "20210824 10:12"
        },
        {
          "id": "MDRW2100005232.1.11",
          "form": "부산은 물난리라던데요",
          "original_form": "부산은 물난리라던데요",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100005232.1.12",
          "form": "아하.. 업무 스트레스가 많겠네요",
          "original_form": "아하.. 업무 스트레스가 많겠네요",
          "speaker_id": "2",
          "time": "20210824 10:14"
        },
        {
          "id": "MDRW2100005232.1.13",
          "form": "그러게요 날씨도 좋아야 일도 잘 될텐데요",
          "original_form": "그러게요 날씨도 좋아야 일도 잘 될텐데요",
          "speaker_id": "1",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100005232.1.14",
          "form": "가을 장마래요ㅠㅠ",
          "original_form": "가을 장마래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100005232.1.15",
          "form": "비오니까 습해서 일이 더 안되네요",
          "original_form": "비오니까 습해서 일이 더 안되네요",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100005232.1.16",
          "form": "그래서 일하기 힘들어욤",
          "original_form": "그래서 일하기 힘들어욤",
          "speaker_id": "2",
          "time": "20210826 06:12"
        },
        {
          "id": "MDRW2100005232.1.17",
          "form": "그래도 힘내서 일해야죠!",
          "original_form": "그래도 힘내서 일해야죠!",
          "speaker_id": "1",
          "time": "20210826 07:40"
        },
        {
          "id": "MDRW2100005232.1.18",
          "form": "일이 집중이 안되네요",
          "original_form": "일이 집중이 안되네요",
          "speaker_id": "1",
          "time": "20210904 15:30"
        }
      ]
