      "utterance": [
        {
          "id": "MDRW2100035430.1.1",
          "form": "저기에서도 뭐라고 가난 어쩌고  심사위원이 뭐하고 한다",
          "original_form": "저기에서도 뭐라고 가난 어쩌고  심사위원이 뭐하고 한다",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100035430.1.2",
          "form": "에고 웃긴다",
          "original_form": "에고 웃긴다",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100035430.1.3",
          "form": "긴장해서리",
          "original_form": "긴장해서리",
          "speaker_id": "2",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100035430.1.4",
          "form": "다른 음악으로 할거 같은데",
          "original_form": "다른 음악으로 할거 같은데",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100035430.1.5",
          "form": "어 하네ㅋ",
          "original_form": "어 하네ㅋ",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100035430.1.6",
          "form": "ㅋㅋ엄청 열심히 한다",
          "original_form": "ㅋㅋ엄청 열심히 한다",
          "speaker_id": "2",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100035430.1.7",
          "form": "너무 긴장했다",
          "original_form": "너무 긴장했다",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100035430.1.8",
          "form": "심사위원들 표정보면 하던 춤도 들어가겠다",
          "original_form": "심사위원들 표정보면 하던 춤도 들어가겠다",
          "speaker_id": "2",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100035430.1.9",
          "form": "똥씹은 표정",
          "original_form": "똥씹은 표정",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100035430.1.10",
          "form": "에궁 내년 기회도 빌리처럼 가난하면 힘들지",
          "original_form": "에궁 내년 기회도 빌리처럼 가난하면 힘들지",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100035430.1.11",
          "form": "그래도 되겠지?",
          "original_form": "그래도 되겠지?",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100035430.1.12",
          "form": "성질 나니까 애도 때리네",
          "original_form": "성질 나니까 애도 때리네",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100035430.1.13",
          "form": "엉 이제 상담을 한다 거기서 얘기 잘해서",
          "original_form": "엉 이제 상담을 한다 거기서 얘기 잘해서",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100035430.1.14",
          "form": "말을 엄청 잘했나?",
          "original_form": "말을 엄청 잘했나?",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100035430.1.15",
          "form": "아빠의 저 답답하다는 표정",
          "original_form": "아빠의 저 답답하다는 표정",
          "speaker_id": "2",
          "time": "20210829 16:58"
        }
