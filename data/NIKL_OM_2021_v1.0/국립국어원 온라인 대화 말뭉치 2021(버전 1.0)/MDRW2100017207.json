      "utterance": [
        {
          "id": "MDRW2100017207.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017207.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017207.1.3",
          "form": "OTT 이용하는거 있으세요?",
          "original_form": "OTT 이용하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017207.1.4",
          "form": "OTT가 뭔가요? 넷플릭스같은건가요?",
          "original_form": "OTT가 뭔가요? 넷플릭스같은건가요?",
          "speaker_id": "2",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017207.1.5",
          "form": "네!! 웨이브, 왓챠 등등",
          "original_form": "네!! 웨이브, 왓챠 등등",
          "speaker_id": "1",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100017207.1.6",
          "form": "저는 얼마전부터 오션을 써요!",
          "original_form": "저는 얼마전부터 오션을 써요!",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017207.1.7",
          "form": "주말에 하루종일 미드를봤어요ㅎㅎ 님은요?",
          "original_form": "주말에 하루종일 미드를봤어요ㅎㅎ 님은요?",
          "speaker_id": "2",
          "time": "20210826 18:08"
        },
        {
          "id": "MDRW2100017207.1.8",
          "form": "오션이요?! 찾아봐야겠네요ㅎㅎ",
          "original_form": "오션이요?! 찾아봐야겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017207.1.9",
          "form": "주말엔 주로 한국꺼봐요! 예능이랑 드라맣ㅎㅎㅎ",
          "original_form": "주말엔 주로 한국꺼봐요! 예능이랑 드라맣ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017207.1.10",
          "form": "아!오션이라해서 뭔가했어요ㅎㅎㅎbtv채널인가 맞죠?",
          "original_form": "아!오션이라해서 뭔가했어요ㅎㅎㅎbtv채널인가 맞죠?",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017207.1.11",
          "form": "네 맞아요ㅎㅎ",
          "original_form": "네 맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017207.1.12",
          "form": "예능 드라마건 예전거 보세요? 아님 요즘 하는거보세요?",
          "original_form": "예능 드라마건 예전거 보세요? 아님 요즘 하는거보세요?",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017207.1.13",
          "form": "둘다요ㅋㅋㅋ솔직히 예전게 재밌지않나요?",
          "original_form": "둘다요ㅋㅋㅋ솔직히 예전게 재밌지않나요?",
          "speaker_id": "1",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017207.1.14",
          "form": "거침없이하이킥 많이봐요 무도는 유튜브 편집본으로보고",
          "original_form": "거침없이하이킥 많이봐요 무도는 유튜브 편집본으로보고",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017207.1.15",
          "form": "ㅋㅋ맞아요 저번에 헬스장에서 무한도전보다가 넘어질뻔햇어요",
          "original_form": "ㅋㅋ맞아요 저번에 헬스장에서 무한도전보다가 넘어질뻔햇어요",
          "speaker_id": "2",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017207.1.16",
          "form": "아는데도 너무 재밌잖아요!",
          "original_form": "아는데도 너무 재밌잖아요!",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017207.1.17",
          "form": "런닝맨 1박2일도 재밌게보는데 어떻게보면 이 프로들도 예전꺼니까ㅜ",
          "original_form": "런닝맨 1박2일도 재밌게보는데 어떻게보면 이 프로들도 예전꺼니까ㅜ",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017207.1.18",
          "form": "맞아요 요즘은 놀라운 토요일?이 유행인거같아요",
          "original_form": "맞아요 요즘은 놀라운 토요일?이 유행인거같아요",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017207.1.19",
          "form": "노래 가사 맞추는거요ㅎㅎ",
          "original_form": "노래 가사 맞추는거요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017207.1.20",
          "form": "막귀라 그런지 잘 안들려서 간식퀴즈 부분만 봐요ㅋㅋㅋ",
          "original_form": "막귀라 그런지 잘 안들려서 간식퀴즈 부분만 봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017207.1.21",
          "form": "아! 초반에 분장쇼랑",
          "original_form": "아! 초반에 분장쇼랑",
          "speaker_id": "1",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017207.1.22",
          "form": "ㅋㅋ분장쇼 웃겨요",
          "original_form": "ㅋㅋ분장쇼 웃겨요",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017207.1.23",
          "form": "아 신서유기도 좋아하세요? 저 요즘엔 안보는데",
          "original_form": "아 신서유기도 좋아하세요? 저 요즘엔 안보는데",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017207.1.24",
          "form": "한 시즌 5까지는 진짜 많이봤어요 넘 웃겨서",
          "original_form": "한 시즌 5까지는 진짜 많이봤어요 넘 웃겨서",
          "speaker_id": "2",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017207.1.25",
          "form": "신서유기 본방도 챙겨봤어요!",
          "original_form": "신서유기 본방도 챙겨봤어요!",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017207.1.26",
          "form": "근데 최근에 한 캠핑편은 별로였어요ㅠ",
          "original_form": "근데 최근에 한 캠핑편은 별로였어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 18:19"
        }
      ]
    }
