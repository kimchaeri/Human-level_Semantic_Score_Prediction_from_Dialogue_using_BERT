      "utterance": [
        {
          "id": "MDRW2100028069.1.1",
          "form": "아 혹시 손바닥만한 나방이 사는곳",
          "original_form": "아 혹시 손바닥만한 나방이 사는곳",
          "speaker_id": "2",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028069.1.2",
          "form": "거기가어딜까",
          "original_form": "거기가어딜까",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028069.1.3",
          "form": "눈이 비듬처럼 내리는곳!",
          "original_form": "눈이 비듬처럼 내리는곳!",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028069.1.4",
          "form": "그건좀...",
          "original_form": "그건좀...",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028069.1.5",
          "form": "강원도 철원입니다1l",
          "original_form": "강원도 철원입니다1l",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028069.1.6",
          "form": "눈이 엄청왓서요 ㅠ",
          "original_form": "눈이 엄청왓서요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028069.1.7",
          "form": "아하....고생하셨겠군요",
          "original_form": "아하....고생하셨겠군요",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028069.1.8",
          "form": "추워 죽는줄",
          "original_form": "추워 죽는줄",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028069.1.9",
          "form": "그당시",
          "original_form": "그당시",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028069.1.10",
          "form": "20도는 기본으로 내려갓서용",
          "original_form": "20도는 기본으로 내려갓서용",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028069.1.11",
          "form": "스타킹도 신어봤습니다",
          "original_form": "스타킹도 신어봤습니다",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028069.1.12",
          "form": "여성분들 스타킹신고 다니는거 존경합니다",
          "original_form": "여성분들 스타킹신고 다니는거 존경합니다",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028069.1.13",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028069.1.14",
          "form": "그 추분걸 우째 신고 다닌대요",
          "original_form": "그 추분걸 우째 신고 다닌대요",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028069.1.15",
          "form": "ㅋㅋㅋㅋㅋ 저도 이해가안되네요",
          "original_form": "ㅋㅋㅋㅋㅋ 저도 이해가안되네요",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028069.1.16",
          "form": "스타킹 신으시나요?",
          "original_form": "스타킹 신으시나요?",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028069.1.17",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028069.1.18",
          "form": "사격 잘하심 ?",
          "original_form": "사격 잘하심 ?",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028069.1.19",
          "form": "아 제가",
          "original_form": "아 제가",
          "speaker_id": "2",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100028069.1.20",
          "form": "사격으로 말할것같으면",
          "original_form": "사격으로 말할것같으면",
          "speaker_id": "2",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100028069.1.21",
          "form": "250미터 거리에 있는",
          "original_form": "250미터 거리에 있는",
          "speaker_id": "2",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100028069.1.22",
          "form": "사과를 박살냈엇죠",
          "original_form": "사과를 박살냈엇죠",
          "speaker_id": "2",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100028069.1.23",
          "form": "한 오십발 쏘니까 터지더라구요",
          "original_form": "한 오십발 쏘니까 터지더라구요",
          "speaker_id": "2",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100028069.1.24",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100028069.1.25",
          "form": "올림픽 나가셔야겟네",
          "original_form": "올림픽 나가셔야겟네",
          "speaker_id": "1",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100028069.1.26",
          "form": "포상휴가 마니받으셧나요",
          "original_form": "포상휴가 마니받으셧나요",
          "speaker_id": "1",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100028069.1.27",
          "form": "포상휴가하니까 슬픈이야기가 생각나네요",
          "original_form": "포상휴가하니까 슬픈이야기가 생각나네요",
          "speaker_id": "2",
          "time": "20210902 17:26"
        }
      ]
