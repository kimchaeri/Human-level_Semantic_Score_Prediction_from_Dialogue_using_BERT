      "utterance": [
        {
          "id": "MDRW2100015349.1.1",
          "form": "나 요즘 왓챠 한달 끊어서 열심히 보고있다",
          "original_form": "나 요즘 왓챠 한달 끊어서 열심히 보고있다",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015349.1.2",
          "form": "왓챠 괜찮아?",
          "original_form": "왓챠 괜찮아?",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100015349.1.3",
          "form": "왓챠 어때?",
          "original_form": "왓챠 어때?",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100015349.1.4",
          "form": "왓챠 좋아 ㅎㅎ 영화가 많음",
          "original_form": "왓챠 좋아 ㅎㅎ 영화가 많음",
          "speaker_id": "1",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100015349.1.5",
          "form": "해외드라마도 많고",
          "original_form": "해외드라마도 많고",
          "speaker_id": "1",
          "time": "20210826 10:13"
        },
        {
          "id": "MDRW2100015349.1.6",
          "form": "영화관 안가도 될만큼?",
          "original_form": "영화관 안가도 될만큼?",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100015349.1.7",
          "form": "화질도 괜찮아?",
          "original_form": "화질도 괜찮아?",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100015349.1.8",
          "form": "ㅇㅇ 화질은 폰으로 보면 당연히 볼만하고 티비도 괜찮음",
          "original_form": "ㅇㅇ 화질은 폰으로 보면 당연히 볼만하고 티비도 괜찮음",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100015349.1.9",
          "form": "프리미엄은 4k야",
          "original_form": "프리미엄은 4k야",
          "speaker_id": "1",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100015349.1.10",
          "form": "오 프리미엄 끊었어?",
          "original_form": "오 프리미엄 끊었어?",
          "speaker_id": "2",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100015349.1.11",
          "form": "ㅇㅇ무료 ㅋㅋ",
          "original_form": "ㅇㅇ무료 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100015349.1.12",
          "form": "이벤트했을때 참여함",
          "original_form": "이벤트했을때 참여함",
          "speaker_id": "1",
          "time": "20210826 11:41"
        },
        {
          "id": "MDRW2100015349.1.13",
          "form": "더 대박이네",
          "original_form": "더 대박이네",
          "speaker_id": "2",
          "time": "20210826 11:44"
        },
        {
          "id": "MDRW2100015349.1.14",
          "form": "프리미엄을 무료로ㅎ",
          "original_form": "프리미엄을 무료로ㅎ",
          "speaker_id": "2",
          "time": "20210826 11:44"
        },
        {
          "id": "MDRW2100015349.1.15",
          "form": "원래 무료는 프리미엄 주더라",
          "original_form": "원래 무료는 프리미엄 주더라",
          "speaker_id": "1",
          "time": "20210826 11:44"
        },
        {
          "id": "MDRW2100015349.1.16",
          "form": "호객 잘하네ㅎ",
          "original_form": "호객 잘하네ㅎ",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100015349.1.17",
          "form": "그러게 ㅋㅋ 왓챠는 이벤트 좀 하더라",
          "original_form": "그러게 ㅋㅋ 왓챠는 이벤트 좀 하더라",
          "speaker_id": "1",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100015349.1.18",
          "form": "넷플릭스는 무료체험도 없애버림",
          "original_form": "넷플릭스는 무료체험도 없애버림",
          "speaker_id": "1",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100015349.1.19",
          "form": "오메 넷플릭스 왜 그래",
          "original_form": "오메 넷플릭스 왜 그래",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100015349.1.20",
          "form": "그래도 이벤트 있어야 체험해보고 구매할텐데",
          "original_form": "그래도 이벤트 있어야 체험해보고 구매할텐데",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100015349.1.21",
          "form": "넷플릭스를 많이 보나보다",
          "original_form": "넷플릭스를 많이 보나보다",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100015349.1.22",
          "form": "그러게 한국 점유율 1위라 자신만만한듯",
          "original_form": "그러게 한국 점유율 1위라 자신만만한듯",
          "speaker_id": "1",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100015349.1.23",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100015349.1.24",
          "form": "1위라?",
          "original_form": "1위라?",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100015349.1.25",
          "form": "하긴 숙박하는곳에 있기도하니까",
          "original_form": "하긴 숙박하는곳에 있기도하니까",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100015349.1.26",
          "form": "그럴수밖에 없겠다",
          "original_form": "그럴수밖에 없겠다",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100015349.1.27",
          "form": "난 왓챠가 영화가 많아서 좋더라",
          "original_form": "난 왓챠가 영화가 많아서 좋더라",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100015349.1.28",
          "form": "넷플은 오리지널 때메 보고싶은듯",
          "original_form": "넷플은 오리지널 때메 보고싶은듯",
          "speaker_id": "1",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100015349.1.29",
          "form": "플랫폼 별 성향이 달라서 둘다 괜찮은거같아ㅎ",
          "original_form": "플랫폼 별 성향이 달라서 둘다 괜찮은거같아ㅎ",
          "speaker_id": "2",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100015349.1.30",
          "form": "그것도 그렇지 ㅎㅎ",
          "original_form": "그것도 그렇지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:15"
        }
      ]
