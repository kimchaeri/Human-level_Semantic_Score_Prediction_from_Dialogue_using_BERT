      "utterance": [
        {
          "id": "MDRW2100026922.1.1",
          "form": "흠...게임",
          "original_form": "흠...게임",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100026922.1.2",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100026922.1.3",
          "form": "저의남자는 게임에 살고있어요",
          "original_form": "저의남자는 게임에 살고있어요",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100026922.1.4",
          "form": "어머 .......",
          "original_form": "어머 .......",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100026922.1.5",
          "form": "님근데요즘 게임하나요?",
          "original_form": "님근데요즘 게임하나요?",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100026922.1.6",
          "form": "좋피위피의 건넛편 사장?",
          "original_form": "좋피위피의 건넛편 사장?",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100026922.1.7",
          "form": "설마요??",
          "original_form": "설마요??",
          "speaker_id": "2",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100026922.1.8",
          "form": "오타쿠 게임은 안하는디",
          "original_form": "오타쿠 게임은 안하는디",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100026922.1.9",
          "form": "아뇨 설마요????",
          "original_form": "아뇨 설마요????",
          "speaker_id": "1",
          "time": "20210902 01:16"
        },
        {
          "id": "MDRW2100026922.1.10",
          "form": "그럼 누군데요???",
          "original_form": "그럼 누군데요???",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100026922.1.11",
          "form": "데못죽도 판소속 게임이란 점에서",
          "original_form": "데못죽도 판소속 게임이란 점에서",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100026922.1.12",
          "form": "들도 게임속 인물들인거죠",
          "original_form": "{censored}들도 게임속 인물들인거죠",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100026922.1.13",
          "form": "아니 그런세계관이",
          "original_form": "아니 그런세계관이",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100026922.1.14",
          "form": "그래서 죽는건가요?",
          "original_form": "그래서 죽는건가요?",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100026922.1.15",
          "form": "사실 구라구요",
          "original_form": "사실 구라구요",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100026922.1.16",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100026922.1.17",
          "form": "무캄코 생각하고 한말인데",
          "original_form": "무캄코 생각하고 한말인데",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100026922.1.18",
          "form": "역시 사장이맞는거죠",
          "original_form": "역시 사장이맞는거죠",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026922.1.19",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026922.1.20",
          "form": "아니라고요",
          "original_form": "아니라고요",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026922.1.21",
          "form": "먹던거 뿜을뻔했네",
          "original_form": "먹던거 뿜을뻔했네",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026922.1.22",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026922.1.23",
          "form": "자본주의에 굴복해서",
          "original_form": "자본주의에 굴복해서",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026922.1.24",
          "form": "결국 스카우트 당한줄 알았어요",
          "original_form": "결국 스카우트 당한줄 알았어요",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026922.1.25",
          "form": "그 사장은",
          "original_form": "그 사장은",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026922.1.26",
          "form": "수염이 있잖아요",
          "original_form": "수염이 있잖아요",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026922.1.27",
          "form": "님이 갂아버려요",
          "original_form": "님이 갂아버려요",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026922.1.28",
          "form": "그런 노력을 들일 정도로",
          "original_form": "그런 노력을 들일 정도로",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100026922.1.29",
          "form": "정성 잇는 얼굴이 아니라서",
          "original_form": "정성 잇는 얼굴이 아니라서",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026922.1.30",
          "form": "엄격하시군요",
          "original_form": "엄격하시군요",
          "speaker_id": "2",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026922.1.31",
          "form": "저도 그렇다고생각해요",
          "original_form": "저도 그렇다고생각해요",
          "speaker_id": "2",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026922.1.32",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026922.1.33",
          "form": "좋피위피 안들어간지 좀 된듯",
          "original_form": "좋피위피 안들어간지 좀 된듯",
          "speaker_id": "2",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026922.1.34",
          "form": "저 아까 하루치 함",
          "original_form": "저 아까 하루치 함",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026922.1.35",
          "form": "꿈왕국만 하고 있어서...",
          "original_form": "꿈왕국만 하고 있어서...",
          "speaker_id": "2",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026922.1.36",
          "form": "한달넘지않았어요??",
          "original_form": "한달넘지않았어요??",
          "speaker_id": "2",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026922.1.37",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026922.1.38",
          "form": "기종바꾼지 얼마 안됐으면서",
          "original_form": "기종바꾼지 얼마 안됐으면서",
          "speaker_id": "2",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100026922.1.39",
          "form": "무서운사람",
          "original_form": "무서운사람",
          "speaker_id": "2",
          "time": "20210902 01:20"
        }
      ]
    }
