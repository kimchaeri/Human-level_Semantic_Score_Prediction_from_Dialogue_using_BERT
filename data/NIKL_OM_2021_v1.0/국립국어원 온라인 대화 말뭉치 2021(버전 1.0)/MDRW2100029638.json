      "utterance": [
        {
          "id": "MDRW2100029638.1.1",
          "form": "디스코드 같은걸로",
          "original_form": "디스코드 같은걸로",
          "speaker_id": "2",
          "time": "20210903 11:09"
        },
        {
          "id": "MDRW2100029638.1.2",
          "form": "같이 대화하면서",
          "original_form": "같이 대화하면서",
          "speaker_id": "2",
          "time": "20210903 11:09"
        },
        {
          "id": "MDRW2100029638.1.3",
          "form": "많이해요",
          "original_form": "많이해요",
          "speaker_id": "2",
          "time": "20210903 11:09"
        },
        {
          "id": "MDRW2100029638.1.4",
          "form": "디스코드가 무엇이죠",
          "original_form": "디스코드가 무엇이죠",
          "speaker_id": "1",
          "time": "20210903 11:09"
        },
        {
          "id": "MDRW2100029638.1.5",
          "form": "음성채팅 프로그램이요",
          "original_form": "음성채팅 프로그램이요",
          "speaker_id": "2",
          "time": "20210903 11:09"
        },
        {
          "id": "MDRW2100029638.1.6",
          "form": "대화할수록 전문가같군요",
          "original_form": "대화할수록 전문가같군요",
          "speaker_id": "1",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.7",
          "form": "하고 있는 게임목록ㄷ뜨구요",
          "original_form": "하고 있는 게임목록ㄷ뜨구요",
          "speaker_id": "2",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.8",
          "form": "카트라이더도",
          "original_form": "카트라이더도",
          "speaker_id": "2",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.9",
          "form": "남친과 대화하며 겜하시나오",
          "original_form": "남친과 대화하며 겜하시나오",
          "speaker_id": "1",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.10",
          "form": "음성같이 할수 있게",
          "original_form": "음성같이 할수 있게",
          "speaker_id": "2",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.11",
          "form": "만들어놨더라구요",
          "original_form": "만들어놨더라구요",
          "speaker_id": "2",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.12",
          "form": "싸움도 나겠네요",
          "original_form": "싸움도 나겠네요",
          "speaker_id": "1",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.13",
          "form": "네 음성하면서 싸웁니다",
          "original_form": "네 음성하면서 싸웁니다",
          "speaker_id": "2",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.14",
          "form": "엄청싸워요",
          "original_form": "엄청싸워요",
          "speaker_id": "2",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.15",
          "form": "역쉬",
          "original_form": "역쉬",
          "speaker_id": "1",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.16",
          "form": "왜싸우죠 게임인데ㅣ",
          "original_form": "왜싸우죠 게임인데ㅣ",
          "speaker_id": "1",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.17",
          "form": "내탓하기 싫고",
          "original_form": "내탓하기 싫고",
          "speaker_id": "2",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.18",
          "form": "남자가 져주나요",
          "original_form": "남자가 져주나요",
          "speaker_id": "1",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.19",
          "form": "내가 못한것도 니탓",
          "original_form": "내가 못한것도 니탓",
          "speaker_id": "2",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029638.1.20",
          "form": "이런거죠",
          "original_form": "이런거죠",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029638.1.21",
          "form": "저런 게임에 진지하군요",
          "original_form": "저런 게임에 진지하군요",
          "speaker_id": "1",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029638.1.22",
          "form": "지가잘해야죠",
          "original_form": "지가잘해야죠",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029638.1.23",
          "form": "나이도어린게",
          "original_form": "나이도어린게",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029638.1.24",
          "form": "욕하며 정드는군요",
          "original_form": "욕하며 정드는군요",
          "speaker_id": "1",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029638.1.25",
          "form": "손이 저보다 빠르잗ㅇ아요",
          "original_form": "손이 저보다 빠르잗ㅇ아요",
          "speaker_id": "2",
          "time": "20210903 11:11"
        }
      ]
