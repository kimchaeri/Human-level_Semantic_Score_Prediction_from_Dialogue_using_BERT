        {
          "id": "MDRW2100003606.1.1",
          "form": "여기는 비가오네요~ name2님이 사는 곳은 어떤가요?",
          "original_form": "여기는 비가오네요~ &name2&님이 사는 곳은 어떤가요?",
          "speaker_id": "1",
          "time": "20210821 08:12"
        },
        {
          "id": "MDRW2100003606.1.2",
          "form": "저희쪽도 아침부터 비가 계속 내려요 ~",
          "original_form": "저희쪽도 아침부터 비가 계속 내려요 ~",
          "speaker_id": "2",
          "time": "20210821 08:37"
        },
        {
          "id": "MDRW2100003606.1.3",
          "form": "비오는 날 좋아하시나요?",
          "original_form": "비오는 날 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 08:38"
        },
        {
          "id": "MDRW2100003606.1.4",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "2",
          "time": "20210821 08:40"
        },
        {
          "id": "MDRW2100003606.1.5",
          "form": "비오는소리가 좋아서",
          "original_form": "비오는소리가 좋아서",
          "speaker_id": "2",
          "time": "20210821 08:40"
        },
        {
          "id": "MDRW2100003606.1.6",
          "form": "name1님은여?",
          "original_form": "&name1&님은여?",
          "speaker_id": "2",
          "time": "20210821 08:40"
        },
        {
          "id": "MDRW2100003606.1.7",
          "form": "저는 비는 별로 좋아하지않아요",
          "original_form": "저는 비는 별로 좋아하지않아요",
          "speaker_id": "1",
          "time": "20210821 08:42"
        },
        {
          "id": "MDRW2100003606.1.8",
          "form": "약간 습한걸 안좋아해서",
          "original_form": "약간 습한걸 안좋아해서",
          "speaker_id": "1",
          "time": "20210821 08:43"
        },
        {
          "id": "MDRW2100003606.1.9",
          "form": "비오는 날엔 뭐 하시나요?",
          "original_form": "비오는 날엔 뭐 하시나요?",
          "speaker_id": "1",
          "time": "20210821 08:46"
        },
        {
          "id": "MDRW2100003606.1.10",
          "form": "비오는날엔",
          "original_form": "비오는날엔",
          "speaker_id": "2",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003606.1.11",
          "form": "파전많이해먹어염ㅋㅋ",
          "original_form": "파전많이해먹어염ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:55"
        },
        {
          "id": "MDRW2100003606.1.12",
          "form": "파전좋아해요 너무 맛있잖아요",
          "original_form": "파전좋아해요 너무 맛있잖아요",
          "speaker_id": "1",
          "time": "20210821 08:56"
        },
        {
          "id": "MDRW2100003606.1.13",
          "form": "파전에 뭐 찍어드세요",
          "original_form": "파전에 뭐 찍어드세요",
          "speaker_id": "1",
          "time": "20210821 08:56"
        },
        {
          "id": "MDRW2100003606.1.14",
          "form": "저는 간장이요",
          "original_form": "저는 간장이요",
          "speaker_id": "2",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003606.1.15",
          "form": "간장말고 다른게있나욤?ㅋㅋ",
          "original_form": "간장말고 다른게있나욤?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003606.1.16",
          "form": "음 초장찍어드시는 분도 계시더라구요ㅋ기",
          "original_form": "음 초장찍어드시는 분도 계시더라구요ㅋ기",
          "speaker_id": "1",
          "time": "20210821 09:00"
        },
        {
          "id": "MDRW2100003606.1.17",
          "form": "아~ 그럴수도있겠구나 ㅎㅎ",
          "original_form": "아~ 그럴수도있겠구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003606.1.18",
          "form": "그럼오늘",
          "original_form": "그럼오늘",
          "speaker_id": "2",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003606.1.19",
          "form": "비도오니",
          "original_form": "비도오니",
          "speaker_id": "2",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003606.1.20",
          "form": "전을 한장",
          "original_form": "전을 한장",
          "speaker_id": "1",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003606.1.21",
          "form": "부쳐먹는것도ㅎ",
          "original_form": "부쳐먹는것도ㅎ",
          "speaker_id": "1",
          "time": "20210821 09:10"
        },
        {
          "id": "MDRW2100003606.1.22",
          "form": "네 ㅎㅎ 그래야겠어여",
          "original_form": "네 ㅎㅎ 그래야겠어여",
          "speaker_id": "2",
          "time": "20210821 09:11"
        },
        {
          "id": "MDRW2100003606.1.23",
          "form": "생각보다",
          "original_form": "생각보다",
          "speaker_id": "2",
          "time": "20210821 09:11"
        },
        {
          "id": "MDRW2100003606.1.24",
          "form": "오래 내릴거같아요",
