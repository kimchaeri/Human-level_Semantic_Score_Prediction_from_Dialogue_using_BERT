      "utterance": [
        {
          "id": "MDRW2100018777.1.1",
          "form": "저 오늘 봉사활동 하러 가요",
          "original_form": "저 오늘 봉사활동 하러 가요",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018777.1.2",
          "form": "오 좋은 일 하시네요 ㅎㅎ",
          "original_form": "오 좋은 일 하시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018777.1.3",
          "form": "어떤 봉사인가요?",
          "original_form": "어떤 봉사인가요?",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100018777.1.4",
          "form": "지역아동센터에서 하는 일이에요",
          "original_form": "지역아동센터에서 하는 일이에요",
          "speaker_id": "1",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100018777.1.5",
          "form": "마음 맞는 사람들끼리 봉사 동호회? 동아리 같은거 만들었거든요",
          "original_form": "마음 맞는 사람들끼리 봉사 동호회? 동아리 같은거 만들었거든요",
          "speaker_id": "1",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100018777.1.6",
          "form": "아하 그렇구나 뿌듯하시겠어요",
          "original_form": "아하 그렇구나 뿌듯하시겠어요",
          "speaker_id": "2",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018777.1.7",
          "form": "저는 봉사는 따로 안하고 산악 동호회 해요",
          "original_form": "저는 봉사는 따로 안하고 산악 동호회 해요",
          "speaker_id": "2",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018777.1.8",
          "form": "우와 산악 동호회면 같이 등산가고 그래요?",
          "original_form": "우와 산악 동호회면 같이 등산가고 그래요?",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018777.1.9",
          "form": "네 주기적으로 등산도 가고 술도 마시고",
          "original_form": "네 주기적으로 등산도 가고 술도 마시고",
          "speaker_id": "2",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018777.1.10",
          "form": "신기하네요 어느 산 가보셨어요?",
          "original_form": "신기하네요 어느 산 가보셨어요?",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018777.1.11",
          "form": "가본 산은 되게 많아요 가장 인상깊었던 산은",
          "original_form": "가본 산은 되게 많아요 가장 인상깊었던 산은",
          "speaker_id": "2",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018777.1.12",
          "form": "설악산이네요",
          "original_form": "설악산이네요",
          "speaker_id": "2",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018777.1.13",
          "form": "허걱 설악산도 가보셨어요??",
          "original_form": "허걱 설악산도 가보셨어요??",
          "speaker_id": "1",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018777.1.14",
          "form": "멋져요 대박",
          "original_form": "멋져요 대박",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018777.1.15",
          "form": "네 10년 전에 한번 가봤어요",
          "original_form": "네 10년 전에 한번 가봤어요",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018777.1.16",
          "form": "설악산 오르기 힘들지 않나요? 어떠셨어요?",
          "original_form": "설악산 오르기 힘들지 않나요? 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018777.1.17",
          "form": "많이 힘들기는 했어요ㅠㅠ",
          "original_form": "많이 힘들기는 했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018777.1.18",
          "form": "동호회에서 같이 갔으니 망정이지",
          "original_form": "동호회에서 같이 갔으니 망정이지",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018777.1.19",
          "form": "혼자로는 무리 같아요",
          "original_form": "혼자로는 무리 같아요",
          "speaker_id": "2",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018777.1.20",
          "form": "그쵸 산이 아무래도 험하니까ㅠㅠ",
          "original_form": "그쵸 산이 아무래도 험하니까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018777.1.21",
          "form": "취미든 봉사든 여러 사람들이랑 같이 하는게 좋은거 같아요",
          "original_form": "취미든 봉사든 여러 사람들이랑 같이 하는게 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100018777.1.22",
          "form": "맞아요 뭐든 혼자면 외롭잖아요",
          "original_form": "맞아요 뭐든 혼자면 외롭잖아요",
          "speaker_id": "2",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018777.1.23",
          "form": "단체로 같이 가는게 더 일정짜기 편하기도 하고요",
          "original_form": "단체로 같이 가는게 더 일정짜기 편하기도 하고요",
          "speaker_id": "2",
          "time": "20210827 12:09"
        }
      ]
    }
  ]
