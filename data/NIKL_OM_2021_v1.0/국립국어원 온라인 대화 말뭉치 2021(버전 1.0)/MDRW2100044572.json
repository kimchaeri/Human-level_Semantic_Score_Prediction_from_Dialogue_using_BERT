      "utterance": [
        {
          "id": "MDRW2100044572.1.1",
          "form": "넵 들어갑니다~",
          "original_form": "넵 들어갑니다~",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044572.1.2",
          "form": "아 정답! 칼국수!",
          "original_form": "아 정답! 칼국수!",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044572.1.3",
          "form": "넵 맞아요!",
          "original_form": "넵 맞아요!",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044572.1.4",
          "form": "오늘 날이 추워서 땡기더라구요 ㅋㅋㅋ",
          "original_form": "오늘 날이 추워서 땡기더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044572.1.5",
          "form": "맛있겠다... 칼국수 시원하고 개운하고 너무 좋아요",
          "original_form": "맛있겠다... 칼국수 시원하고 개운하고 너무 좋아요",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044572.1.6",
          "form": "이번엔 제 점심 메뉴 맞춰보실래요?ㅋㅋㅋㅋ",
          "original_form": "이번엔 제 점심 메뉴 맞춰보실래요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044572.1.7",
          "form": "넵 좋아요 ㅋㅋㅋㅋ",
          "original_form": "넵 좋아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044572.1.8",
          "form": "국물이 잇나요?",
          "original_form": "국물이 잇나요?",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044572.1.9",
          "form": "아니요 국물이 거의 없어요!",
          "original_form": "아니요 국물이 거의 없어요!",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044572.1.10",
          "form": "매운가요?",
          "original_form": "매운가요?",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044572.1.11",
          "form": "아니요 하나도 안 매운 요리예요!",
          "original_form": "아니요 하나도 안 매운 요리예요!",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044572.1.12",
          "form": "면요리인가요?",
          "original_form": "면요리인가요?",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044572.1.13",
          "form": "아니요~~~ 면 안 들어간답니다",
          "original_form": "아니요~~~ 면 안 들어간답니다",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044572.1.14",
          "form": "김치가 들어가나요?",
          "original_form": "김치가 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044572.1.15",
          "form": "아뇨 김치도 안 들어가요 후후",
          "original_form": "아뇨 김치도 안 들어가요 후후",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044572.1.16",
          "form": "쌀은 들어가나요?",
          "original_form": "쌀은 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044572.1.17",
          "form": "쌀이랑 같이 먹어요!",
          "original_form": "쌀이랑 같이 먹어요!",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044572.1.18",
          "form": "어떤 야채가 들어가나요?",
          "original_form": "어떤 야채가 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044572.1.19",
          "form": "앗.. 토마토가 들어갑니다 너무 쉬워지는데요ㅌㅋㅋㅋ",
          "original_form": "앗.. 토마토가 들어갑니다 너무 쉬워지는데요ㅌㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044572.1.20",
          "form": "아니 저 요리에 너무 문외한인가봐요 ㅋㅋㅋㅋ",
          "original_form": "아니 저 요리에 너무 문외한인가봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:13"
        }
