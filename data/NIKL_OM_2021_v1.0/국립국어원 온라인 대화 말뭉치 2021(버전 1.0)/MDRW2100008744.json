      "utterance": [
        {
          "id": "MDRW2100008744.1.1",
          "form": "저도 갤럭시로 풀셋하고 싶어요...",
          "original_form": "저도 갤럭시로 풀셋하고 싶어요...",
          "speaker_id": "1",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100008744.1.2",
          "form": "저는 노트북이랑 핸드폰이 삼성꺼라 ㅋㅋㅋ",
          "original_form": "저는 노트북이랑 핸드폰이 삼성꺼라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100008744.1.3",
          "form": "아하 그러시군요",
          "original_form": "아하 그러시군요",
          "speaker_id": "2",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008744.1.4",
          "form": "그럼 삼성페이도 쓰시겠네요",
          "original_form": "그럼 삼성페이도 쓰시겠네요",
          "speaker_id": "2",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008744.1.5",
          "form": "삼성페이기반으로 한 카드사어플 쓰고 있어요 ㅎㅎ",
          "original_form": "삼성페이기반으로 한 카드사어플 쓰고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008744.1.6",
          "form": "카드사 혜택받으려구욯ㅎ",
          "original_form": "카드사 혜택받으려구욯ㅎ",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008744.1.7",
          "form": "삼성페이 편해요! ㅎㅎㅎ",
          "original_form": "삼성페이 편해요! ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008744.1.8",
          "form": "넹넹 ㅎㅎ",
          "original_form": "넹넹 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008744.1.9",
          "form": "이번에 갤럭시 플립도 너무 이뻐서 사고 싶더라구요ㅜㅜㅜㅜㅜㅜ",
          "original_form": "이번에 갤럭시 플립도 너무 이뻐서 사고 싶더라구요ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008744.1.10",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008744.1.11",
          "form": "저는 비싸서 ㅎㅎㅎ",
          "original_form": "저는 비싸서 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008744.1.12",
          "form": "맞아여..... 비싸요 ...ㅠㅋㅋㅋ",
          "original_form": "맞아여..... 비싸요 ...ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008744.1.13",
          "form": "핸드폰이 이렇게 비쌀필요가 있나싶어요..",
          "original_form": "핸드폰이 이렇게 비쌀필요가 있나싶어요..",
          "speaker_id": "1",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008744.1.14",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008744.1.15",
          "form": "제 동생은 중고로도 사더라구요.",
          "original_form": "제 동생은 중고로도 사더라구요.",
          "speaker_id": "1",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008744.1.16",
          "form": "비싸긴 해요",
          "original_form": "비싸긴 해요",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008744.1.17",
          "form": "쓰던거 팔기도 하고..",
          "original_form": "쓰던거 팔기도 하고..",
          "speaker_id": "1",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008744.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008744.1.19",
          "form": "그게요 2년에 한번씩 사는데",
          "original_form": "그게요 2년에 한번씩 사는데",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008744.1.20",
          "form": "비싼건 백만원 넘으니까요",
          "original_form": "비싼건 백만원 넘으니까요",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008744.1.21",
          "form": "저희집 쇼파가 이십년전에 이백만원정도로 샀거든요",
          "original_form": "저희집 쇼파가 이십년전에 이백만원정도로 샀거든요",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008744.1.22",
          "form": "폰으로 생각해보니 이십년치 폰기계값이 원금만 천오백은 되겠더라구요 ㅡㅡ;;",
          "original_form": "폰으로 생각해보니 이십년치 폰기계값이 원금만 천오백은 되겠더라구요 ㅡㅡ;;",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008744.1.23",
          "form": "너무 비싸요ㅠㅠ 저는 50도 비싸다고 생각해요 진짜...",
          "original_form": "너무 비싸요ㅠㅠ 저는 50도 비싸다고 생각해요 진짜...",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008744.1.24",
          "form": "네 ㅜ",
          "original_form": "네 ㅜ",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008744.1.25",
          "form": "저 지금쓰는 폰은 중국꺼에요",
          "original_form": "저 지금쓰는 폰은 중국꺼에요",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008744.1.26",
          "form": "포코폰이라는건데 이년정도 쓴거같거든요",
          "original_form": "포코폰이라는건데 이년정도 쓴거같거든요",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008744.1.27",
          "form": "오~ 괜찮나요?",
          "original_form": "오~ 괜찮나요?",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008744.1.28",
          "form": "원금 38만원 정도로샀는데 쓸만해요",
          "original_form": "원금 38만원 정도로샀는데 쓸만해요",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008744.1.29",
          "form": "저도 홍미... 뭐드라 찾아본적있어요",
          "original_form": "저도 홍미... 뭐드라 찾아본적있어요",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008744.1.30",
          "form": "이거 용량이 120기가정도되고",
          "original_form": "이거 용량이 120기가정도되고",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008744.1.31",
          "form": "외부 sd카드 넣을수있어서요",
          "original_form": "외부 sd카드 넣을수있어서요",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008744.1.32",
          "form": "다른기능은 잘안써서요",
          "original_form": "다른기능은 잘안써서요",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008744.1.33",
          "form": "저는 여태 나온지 일년정도 지난 모델 2ㅡ30에 완납으로 사서 썼거든요.",
          "original_form": "저는 여태 나온지 일년정도 지난 모델 2ㅡ30에 완납으로 사서 썼거든요.",
          "speaker_id": "1",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008744.1.34",
          "form": "근데 이번엔 5g요금제도 안쓰고 싶고",
          "original_form": "근데 이번엔 5g요금제도 안쓰고 싶고",
          "speaker_id": "1",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008744.1.35",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008744.1.36",
          "form": "약정도 안하고 싶어서 찾다가 s21 자급제로 샀어요~",
          "original_form": "약정도 안하고 싶어서 찾다가 s21 자급제로 샀어요~",
          "speaker_id": "1",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008744.1.37",
          "form": "제인생 최초로 최신폰 구매..",
          "original_form": "제인생 최초로 최신폰 구매..",
          "speaker_id": "1",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008744.1.38",
          "form": "진짜 너무비싸여...",
          "original_form": "진짜 너무비싸여...",
          "speaker_id": "1",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008744.1.39",
          "form": "네 너무비싸요 ㅜㅜ",
          "original_form": "네 너무비싸요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008744.1.40",
          "form": "원래는 중저가 라인 모델 사려고 헀는데 곧 나온다고 해서 계속 기다렸거든요~",
          "original_form": "원래는 중저가 라인 모델 사려고 헀는데 곧 나온다고 해서 계속 기다렸거든요~",
          "speaker_id": "1",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008744.1.41",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008744.1.42",
          "form": "두달 기다리다가 결국 s21...",
          "original_form": "두달 기다리다가 결국 s21...",
          "speaker_id": "1",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008744.1.43",
          "form": "근데 반전은 그 폰 아직도 안나왔어요 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 반전은 그 폰 아직도 안나왔어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008744.1.44",
          "form": "그렇군요 ㅋㅋ",
          "original_form": "그렇군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008744.1.45",
          "form": "그르쳐...",
          "original_form": "그르쳐...",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008744.1.46",
          "form": "요즘 당근마켓도 핫한데~",
          "original_form": "요즘 당근마켓도 핫한데~",
          "speaker_id": "1",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008744.1.47",
          "form": "이용해본적 있으세여?",
          "original_form": "이용해본적 있으세여?",
          "speaker_id": "1",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008744.1.48",
          "form": "네 눈팅만 해봤어요",
          "original_form": "네 눈팅만 해봤어요",
          "speaker_id": "2",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008744.1.49",
          "form": "지금은 어플지웠구요",
          "original_form": "지금은 어플지웠구요",
          "speaker_id": "2",
          "time": "20210822 18:18"
        }
