      "utterance": [
        {
          "id": "MDRW2100002916.1.1",
          "form": "시원하다",
          "original_form": "시원하다",
          "speaker_id": "1",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002916.1.2",
          "form": "나는 지금 선풍기 엎이거든",
          "original_form": "나는 지금 선풍기 엎이거든",
          "speaker_id": "1",
          "time": "20210820 16:32"
        },
        {
          "id": "MDRW2100002916.1.3",
          "form": "나는 에어컨이 틀려있는곳에 있어",
          "original_form": "나는 에어컨이 틀려있는곳에 있어",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002916.1.4",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002916.1.5",
          "form": "더 시원하겠구나",
          "original_form": "더 시원하겠구나",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002916.1.6",
          "form": "에어컨은 여름에 정말 중요한 가구같아",
          "original_form": "에어컨은 여름에 정말 중요한 가구같아",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002916.1.7",
          "form": "요즘 날이 좀 풀린 거 같아",
          "original_form": "요즘 날이 좀 풀린 거 같아",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002916.1.8",
          "form": "그치만 지금은 가을이야",
          "original_form": "그치만 지금은 가을이야",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002916.1.9",
          "form": "초가을이라그런지 더위를 타는구나",
          "original_form": "초가을이라그런지 더위를 타는구나",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002916.1.10",
          "form": "벌써?",
          "original_form": "벌써?",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002916.1.11",
          "form": "9월은 되어야 가을이지않나",
          "original_form": "9월은 되어야 가을이지않나",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002916.1.12",
          "form": "입추가 지난지",
          "original_form": "입추가 지난지",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002916.1.13",
          "form": "좀 되었다",
          "original_form": "좀 되었다",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002916.1.14",
          "form": "공식적으로는 가을이지 않을까?",
          "original_form": "공식적으로는 가을이지 않을까?",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002916.1.15",
          "form": "흠 너는 음력으로 따지는구나",
          "original_form": "흠 너는 음력으로 따지는구나",
          "speaker_id": "2",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002916.1.16",
          "form": "하지만 날씨가 이상해서 장마는 또 온 거 같더라",
          "original_form": "하지만 날씨가 이상해서 장마는 또 온 거 같더라",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002916.1.17",
          "form": "나는 양력으로 9월부터 쳐줘!",
          "original_form": "나는 양력으로 9월부터 쳐줘!",
          "speaker_id": "2",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002916.1.18",
          "form": "나는 조상님들의 지혜에 따르는 편이야",
          "original_form": "나는 조상님들의 지혜에 따르는 편이야",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002916.1.19",
          "form": "실내에 에어컨을 켜고 지낸다면 겉옷을 입어야겠구나",
          "original_form": "실내에 에어컨을 켜고 지낸다면 겉옷을 입어야겠구나",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002916.1.20",
          "form": "맞아 하얀색 겉옷을 입고있어",
          "original_form": "맞아 하얀색 겉옷을 입고있어",
          "speaker_id": "2",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002916.1.21",
          "form": "이걸 입으면 온도가 딱 적당해",
          "original_form": "이걸 입으면 온도가 딱 적당해",
          "speaker_id": "2",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002916.1.22",
          "form": "정말 좋구나.",
          "original_form": "정말 좋구나.",
          "speaker_id": "1",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002916.1.23",
          "form": "근무환경도 좋구나.",
          "original_form": "근무환경도 좋구나.",
          "speaker_id": "1",
          "time": "20210820 16:42"
        }
      ]
