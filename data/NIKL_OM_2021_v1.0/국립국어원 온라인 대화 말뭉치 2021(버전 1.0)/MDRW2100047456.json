      "utterance": [
        {
          "id": "MDRW2100047456.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100047456.1.2",
          "form": "네 여행 좋아해요~",
          "original_form": "네 여행 좋아해요~",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100047456.1.3",
          "form": "요즘은 자주 못가지만",
          "original_form": "요즘은 자주 못가지만",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100047456.1.4",
          "form": "기억에 남는 여행지 있으신가요?",
          "original_form": "기억에 남는 여행지 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100047456.1.5",
          "form": "음 전 다 좋았는데",
          "original_form": "음 전 다 좋았는데",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100047456.1.6",
          "form": "취직하고 다음해에 서유럽 배낭여행 다녀온 기억이 나네요",
          "original_form": "취직하고 다음해에 서유럽 배낭여행 다녀온 기억이 나네요",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100047456.1.7",
          "form": "우와 유럽 배낭여행! 어떠셨어요?",
          "original_form": "우와 유럽 배낭여행! 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100047456.1.8",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100047456.1.9",
          "form": "걷다보면 유명한 곳들 나오고",
          "original_form": "걷다보면 유명한 곳들 나오고",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100047456.1.10",
          "form": "여름에 가서 매일 야경 보러 나가고",
          "original_form": "여름에 가서 매일 야경 보러 나가고",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100047456.1.11",
          "form": "우와.... 가장 좋았던 나라는 어디였어요?",
          "original_form": "우와.... 가장 좋았던 나라는 어디였어요?",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100047456.1.12",
          "form": "다 나름의 매력이 있었는데",
          "original_form": "다 나름의 매력이 있었는데",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100047456.1.13",
          "form": "영국 벨기에 프랑스 이탈리아",
          "original_form": "영국 벨기에 프랑스 이탈리아",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100047456.1.14",
          "form": "다 좋았어요",
          "original_form": "다 좋았어요",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100047456.1.15",
          "form": "기간은 얼마나 되셨나요?",
          "original_form": "기간은 얼마나 되셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100047456.1.16",
          "form": "25일이요",
          "original_form": "25일이요",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100047456.1.17",
          "form": "휴가를 좀 길게 쓸 수 있었어요",
          "original_form": "휴가를 좀 길게 쓸 수 있었어요",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100047456.1.18",
          "form": "와....부러워요 시간과 재정이 맞는 타이밍이 좋았네요",
          "original_form": "와....부러워요 시간과 재정이 맞는 타이밍이 좋았네요",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100047456.1.19",
          "form": "혼자가셨나요?",
          "original_form": "혼자가셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100047456.1.20",
          "form": "취업스터디 같이했던 친구들이랑 셋이요",
          "original_form": "취업스터디 같이했던 친구들이랑 셋이요",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100047456.1.21",
          "form": "좋으셨겠다",
          "original_form": "좋으셨겠다",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100047456.1.22",
          "form": "네 딱 시기도 좋았고",
          "original_form": "네 딱 시기도 좋았고",
          "speaker_id": "2",
          "time": "20210826 18:30"
        }
      ]
