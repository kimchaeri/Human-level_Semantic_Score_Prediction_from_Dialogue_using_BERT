      "utterance": [
        {
          "id": "MDRW2100045113.1.1",
          "form": "아직 아까 그드라마보는데 재밌어서",
          "original_form": "아직 아까 그드라마보는데 재밌어서",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045113.1.2",
          "form": "밤새서볼거같아",
          "original_form": "밤새서볼거같아",
          "speaker_id": "1",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045113.1.3",
          "form": "대만사람들도 미역국먹네",
          "original_form": "대만사람들도 미역국먹네",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045113.1.4",
          "form": "신기",
          "original_form": "신기",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045113.1.5",
          "form": "체력대단하디",
          "original_form": "체력대단하디",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045113.1.6",
          "form": "낸 인자 놀아도 밤샘불가",
          "original_form": "낸 인자 놀아도 밤샘불가",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045113.1.7",
          "form": "쉬는것도 밤샘불가",
          "original_form": "쉬는것도 밤샘불가",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045113.1.8",
          "form": "ㅋㅋ ㅋ 난요즘 잠이없어",
          "original_form": "ㅋㅋ ㅋ 난요즘 잠이없어",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045113.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045113.1.10",
          "form": "낮잠도안잠",
          "original_form": "낮잠도안잠",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045113.1.11",
          "form": "늙었나",
          "original_form": "늙었나",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045113.1.12",
          "form": "해조류 안먹는 나라 많던데 대만도 미역먹구나",
          "original_form": "해조류 안먹는 나라 많던데 대만도 미역먹구나",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045113.1.13",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045113.1.14",
          "form": "계란말이에 야채넣네",
          "original_form": "계란말이에 야채넣네",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045113.1.15",
          "form": "우리랑 똑같다",
          "original_form": "우리랑 똑같다",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045113.1.16",
          "form": "낸 자도 자도 자고픔ㅋㅋ",
          "original_form": "낸 자도 자도 자고픔ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045113.1.17",
          "form": "일본은 안 넣던데",
          "original_form": "일본은 안 넣던데",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045113.1.18",
          "form": "한국왔다가셨남ㅋ",
          "original_form": "한국왔다가셨남ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045113.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045113.1.20",
          "form": "일본은 설탕넣지ㅋ",
          "original_form": "일본은 설탕넣지ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045113.1.21",
          "form": "달달구리계란",
          "original_form": "달달구리계란",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045113.1.22",
          "form": "걔네는 두가지패턴이더라 소금넣거나 설탕넣거나",
          "original_form": "걔네는 두가지패턴이더라 소금넣거나 설탕넣거나",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045113.1.23",
          "form": "기호에따라",
          "original_form": "기호에따라",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045113.1.24",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045113.1.25",
          "form": "가게가면 달게나와서",
          "original_form": "가게가면 달게나와서",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045113.1.26",
          "form": "여주 요리잘하네",
          "original_form": "여주 요리잘하네",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045113.1.27",
          "form": "미역국에두부넣었다",
          "original_form": "미역국에두부넣었다",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045113.1.28",
          "form": "된장미역국인가",
          "original_form": "된장미역국인가",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045113.1.29",
          "form": "미소시루",
          "original_form": "미소시루",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045113.1.30",
          "form": "대만애들 일본문화 많이 익숙하니까",
          "original_form": "대만애들 일본문화 많이 익숙하니까",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045113.1.31",
