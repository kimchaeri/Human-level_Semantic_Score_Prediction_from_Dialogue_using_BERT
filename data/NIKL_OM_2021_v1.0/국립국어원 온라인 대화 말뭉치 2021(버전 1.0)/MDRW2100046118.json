      "utterance": [
        {
          "id": "MDRW2100046118.1.1",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046118.1.2",
          "form": "넓어서??",
          "original_form": "넓어서??",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046118.1.3",
          "form": "아뇨ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046118.1.4",
          "form": "다른 지점보다 싸요",
          "original_form": "다른 지점보다 싸요",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046118.1.5",
          "form": "오그건좀 탐난다",
          "original_form": "오그건좀 탐난다",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046118.1.6",
          "form": "학생들만요?",
          "original_form": "학생들만요?",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046118.1.7",
          "form": "저도 가본건 아닌데",
          "original_form": "저도 가본건 아닌데",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046118.1.8",
          "form": "아마 다 될거예요ㅋㅋ",
          "original_form": "아마 다 될거예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046118.1.9",
          "form": "저희는 협동조합카페라....",
          "original_form": "저희는 협동조합카페라....",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046118.1.10",
          "form": "맛은그냥그랬어요..ㅋㅋ",
          "original_form": "맛은그냥그랬어요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046118.1.11",
          "form": "갑자기 교수님이",
          "original_form": "갑자기 교수님이",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046118.1.12",
          "form": "과제 내주시면서",
          "original_form": "과제 내주시면서",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046118.1.13",
          "form": "10분내로 제출하래요..",
          "original_form": "10분내로 제출하래요..",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046118.1.14",
          "form": "잉?ㅋㅋㅋㅋ창의수업이요?",
          "original_form": "잉?ㅋㅋㅋㅋ창의수업이요?",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046118.1.15",
          "form": "네....ㅋ",
          "original_form": "네....ㅋ",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046118.1.16",
          "form": "수업 대충들었는데",
          "original_form": "수업 대충들었는데",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046118.1.17",
          "form": "망했다",
          "original_form": "망했다",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046118.1.18",
          "form": "아는거라도 빨리 쓰세욬ㅋㅋㅋㅋ",
          "original_form": "아는거라도 빨리 쓰세욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046118.1.19",
          "form": "다음부터는...",
          "original_form": "다음부터는...",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046118.1.20",
          "form": "이교수님 수업 안들으러고요",
          "original_form": "이교수님 수업 안들으러고요",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046118.1.21",
          "form": "너무 즉흥적이셔",
          "original_form": "너무 즉흥적이셔",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046118.1.22",
          "form": "ㅋㅋ근데 또 그런분이",
          "original_form": "ㅋㅋ근데 또 그런분이",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046118.1.23",
          "form": "성적잘주세요",
          "original_form": "성적잘주세요",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046118.1.24",
          "form": "저번학기 성적을 생각하면...",
          "original_form": "저번학기 성적을 생각하면...",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046118.1.25",
          "form": "맞는거 같기도 하네요ㅋㅋ",
          "original_form": "맞는거 같기도 하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046118.1.26",
          "form": "전공교수님은",
          "original_form": "전공교수님은",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046118.1.27",
          "form": "몇명이세요?",
          "original_form": "몇명이세요?",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046118.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ저희 전공은",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저희 전공은",
          "speaker_id": "1",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046118.1.29",
          "form": "1명이요",
          "original_form": "1명이요",
          "speaker_id": "1",
          "time": "20210908 10:35"
        }
      ]
    }
