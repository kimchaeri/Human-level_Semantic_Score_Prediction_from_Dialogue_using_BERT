      "utterance": [
        {
          "id": "MDRW2100016587.1.1",
          "form": "요즘 배달음식 너무 먹어서 문제예요ㅠㅜ",
          "original_form": "요즘 배달음식 너무 먹어서 문제예요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016587.1.2",
          "form": "편하긴한데 쓰레기가 너무나와서",
          "original_form": "편하긴한데 쓰레기가 너무나와서",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016587.1.3",
          "form": "배달음식 최대의 단점이죠ㅋㅋ 뭐 자주 시키세요?",
          "original_form": "배달음식 최대의 단점이죠ㅋㅋ 뭐 자주 시키세요?",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016587.1.4",
          "form": "한식 패스트푸드 디저트류 회 등등 종류안가려욬ㅋㅋ",
          "original_form": "한식 패스트푸드 디저트류 회 등등 종류안가려욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016587.1.5",
          "form": "집에서 해 드시기 보다는 주로 배달해서 드시나봐요",
          "original_form": "집에서 해 드시기 보다는 주로 배달해서 드시나봐요",
          "speaker_id": "2",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016587.1.6",
          "form": "배달앱은 어디꺼 쓰세요?",
          "original_form": "배달앱은 어디꺼 쓰세요?",
          "speaker_id": "2",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016587.1.7",
          "form": "주방이 좁아서 잘 안쓰게돼요",
          "original_form": "주방이 좁아서 잘 안쓰게돼요",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016587.1.8",
          "form": "배달의민족이요! 이게제일 많더라고요 가게가",
          "original_form": "배달의민족이요! 이게제일 많더라고요 가게가",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016587.1.9",
          "form": "역시 선두주자 ㅋㅋ 저는 배민만 써봤어요",
          "original_form": "역시 선두주자 ㅋㅋ 저는 배민만 써봤어요",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016587.1.10",
          "form": "배달음식 뭐 많이드세요?",
          "original_form": "배달음식 뭐 많이드세요?",
          "speaker_id": "1",
          "time": "20210826 14:24"
        },
        {
          "id": "MDRW2100016587.1.11",
          "form": "전 지금도 햄버거시켜서 먹고있어요",
          "original_form": "전 지금도 햄버거시켜서 먹고있어요",
          "speaker_id": "1",
          "time": "20210826 14:24"
        },
        {
          "id": "MDRW2100016587.1.12",
          "form": "저는 자주 시키는 편은 아니고 치킨 피자 정도?",
          "original_form": "저는 자주 시키는 편은 아니고 치킨 피자 정도?",
          "speaker_id": "2",
          "time": "20210826 14:24"
        },
        {
          "id": "MDRW2100016587.1.13",
          "form": "햄버거는 어디꺼 좋아하세요?",
          "original_form": "햄버거는 어디꺼 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016587.1.14",
          "form": "치킨은 기본이죠ㅋㅋㅋ",
          "original_form": "치킨은 기본이죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016587.1.15",
          "form": "맥도날드요! 진짜자주 시켜먹어요",
          "original_form": "맥도날드요! 진짜자주 시켜먹어요",
          "speaker_id": "1",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016587.1.16",
          "form": "얼마 전에 맥도날드 망고 칠러 먹었는데 맛있더라구요",
          "original_form": "얼마 전에 맥도날드 망고 칠러 먹었는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210826 14:28"
        },
        {
          "id": "MDRW2100016587.1.17",
          "form": "그것도 가끔먹어요~",
          "original_form": "그것도 가끔먹어요~",
          "speaker_id": "1",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016587.1.18",
          "form": "버거는 뭐 좋아하세요? 맥날은 다 맛있긴한데",
          "original_form": "버거는 뭐 좋아하세요? 맥날은 다 맛있긴한데",
          "speaker_id": "1",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016587.1.19",
          "form": "맥날 가면 베토디나 빅맥 주로 먹어요",
          "original_form": "맥날 가면 베토디나 빅맥 주로 먹어요",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016587.1.20",
          "form": "취향이 저랑반대네요! 제가 안 먹는 버거가 그 두개...ㅎㅎ",
          "original_form": "취향이 저랑반대네요! 제가 안 먹는 버거가 그 두개...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016587.1.21",
          "form": "슈슈 슈비버거랑 불고기버거가 취향이라ㅎㅎ",
          "original_form": "슈슈 슈비버거랑 불고기버거가 취향이라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:38"
        }
      ]
    }
