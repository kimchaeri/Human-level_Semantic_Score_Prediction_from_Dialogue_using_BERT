          "speaker_id": "2",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003089.1.15",
          "form": "카페라떼도 좋아",
          "original_form": "카페라떼도 좋아",
          "speaker_id": "2",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003089.1.16",
          "form": "나도 좋아해",
          "original_form": "나도 좋아해",
          "speaker_id": "1",
          "time": "20210820 18:31"
        },
