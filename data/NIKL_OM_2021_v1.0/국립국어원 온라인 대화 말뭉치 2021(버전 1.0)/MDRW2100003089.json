          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100003089.1.12",
          "form": "흰우유 ㅋㅋ",
          "original_form": "흰우유 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100003089.1.13",
          "form": "딸기우유는 좀 그래",
          "original_form": "딸기우유는 좀 그래",
