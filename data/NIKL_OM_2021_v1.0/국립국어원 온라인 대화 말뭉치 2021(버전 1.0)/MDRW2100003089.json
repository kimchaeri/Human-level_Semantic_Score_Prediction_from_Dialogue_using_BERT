        {
          "id": "MDRW2100003089.1.1",
          "form": "마시는 건 뭐 좋아해?",
          "original_form": "마시는 건 뭐 좋아해?",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100003089.1.2",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100003089.1.3",
          "form": "여름은 아이스 아메리카노지",
          "original_form": "여름은 아이스 아메리카노지",
          "speaker_id": "1",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100003089.1.4",
          "form": "겨울에도 ㅋㅋ",
          "original_form": "겨울에도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100003089.1.5",
          "form": "얼죽아 ㅋㅋㅋㅋ",
          "original_form": "얼죽아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100003089.1.6",
          "form": "나도야 ㅋㅋㅋ",
          "original_form": "나도야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100003089.1.7",
          "form": "근데 쥬스는 잘 안마셔 ㅋㅋ",
          "original_form": "근데 쥬스는 잘 안마셔 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100003089.1.8",
          "form": "나도 요즘 안마셨네",
          "original_form": "나도 요즘 안마셨네",
          "speaker_id": "1",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100003089.1.9",
          "form": "우유는 마셔?",
          "original_form": "우유는 마셔?",
          "speaker_id": "2",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100003089.1.10",
          "form": "우유도 좋아해",
          "original_form": "우유도 좋아해",
          "speaker_id": "1",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100003089.1.11",
          "form": "무슨 우유?",
          "original_form": "무슨 우유?",
