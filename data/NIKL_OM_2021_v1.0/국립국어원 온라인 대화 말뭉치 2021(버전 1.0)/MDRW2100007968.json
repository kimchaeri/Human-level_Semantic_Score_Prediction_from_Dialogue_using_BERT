      "utterance": [
        {
          "id": "MDRW2100007968.1.1",
          "form": "오늘 저",
          "original_form": "오늘 저",
          "speaker_id": "2",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007968.1.2",
          "form": "아이스크림 먹었는데",
          "original_form": "아이스크림 먹었는데",
          "speaker_id": "2",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007968.1.3",
          "form": "빠삐코 밀크맛 맛있어욧",
          "original_form": "빠삐코 밀크맛 맛있어욧",
          "speaker_id": "2",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007968.1.4",
          "form": "어 빠삐코가 밀크맛도 나왔어?",
          "original_form": "어 빠삐코가 밀크맛도 나왔어?",
          "speaker_id": "1",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007968.1.5",
          "form": "나 오리지널 초코맛만 있는줄 알았는데",
          "original_form": "나 오리지널 초코맛만 있는줄 알았는데",
          "speaker_id": "1",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007968.1.6",
          "form": "있다가 마트갈 때 확인해봐야겠다",
          "original_form": "있다가 마트갈 때 확인해봐야겠다",
          "speaker_id": "1",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007968.1.7",
          "form": "나온지 오래됐답니다...",
          "original_form": "나온지 오래됐답니다...",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007968.1.8",
          "form": "하지만 저도 빠삐코는 역시 초코지 하고 오리지널 고집하느라",
          "original_form": "하지만 저도 빠삐코는 역시 초코지 하고 오리지널 고집하느라",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007968.1.9",
          "form": "이번에 밀크맛 첨먹어봐요 ㅋㅋㅋㅋㅋ",
          "original_form": "이번에 밀크맛 첨먹어봐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007968.1.10",
          "form": "평소에는 먹던거만 먹어서 ㅋㅋㅋㅋ 잘 몰랐어",
          "original_form": "평소에는 먹던거만 먹어서 ㅋㅋㅋㅋ 잘 몰랐어",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007968.1.11",
          "form": "우유맛은 빵빠레먹고",
          "original_form": "우유맛은 빵빠레먹고",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007968.1.12",
          "form": "초코는 빠삐코먹고... 그러는 사람이라 ㅋㅋㅋ",
          "original_form": "초코는 빠삐코먹고... 그러는 사람이라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007968.1.13",
          "form": "헐 전 빵빠레도",
          "original_form": "헐 전 빵빠레도",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007968.1.14",
          "form": "묻지도 따지지도 않고",
          "original_form": "묻지도 따지지도 않고",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007968.1.15",
          "form": "초코맛으로만 먹는데...",
          "original_form": "초코맛으로만 먹는데...",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007968.1.16",
          "form": "집 앞 슈퍼 사장님이",
          "original_form": "집 앞 슈퍼 사장님이",
          "speaker_id": "1",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007968.1.17",
          "form": "빵빠라 우유맛 극단 주의자인가봐",
          "original_form": "빵빠라 우유맛 극단 주의자인가봐",
          "speaker_id": "1",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007968.1.18",
          "form": "가면 맨날 메로나랑 빵빠레 우유맛이 쌓여있어",
          "original_form": "가면 맨날 메로나랑 빵빠레 우유맛이 쌓여있어",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007968.1.19",
          "form": "아닛ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닛ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007968.1.20",
          "form": "말하다보니 생각났는데 빵빠레랑 더블비안코 딸기맛 나온 거 아세요???",
          "original_form": "말하다보니 생각났는데 빵빠레랑 더블비안코 딸기맛 나온 거 아세요???",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007968.1.21",
          "form": "노맛입니다... 비추",
          "original_form": "노맛입니다... 비추",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007968.1.22",
          "form": "어떤 아이스크림이던간에",
          "original_form": "어떤 아이스크림이던간에",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007968.1.23",
          "form": "오리지널 맛이 가장 맛있고",
          "original_form": "오리지널 맛이 가장 맛있고",
          "speaker_id": "1",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007968.1.24",
          "form": "나중에 추가로 나오는건 늘 기대에 못미치는거같아",
          "original_form": "나중에 추가로 나오는건 늘 기대에 못미치는거같아",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007968.1.25",
          "form": "그치만... 빠삐코 밀크맛은",
          "original_form": "그치만... 빠삐코 밀크맛은",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007968.1.26",
          "form": "예상외로 맛있었어요",
          "original_form": "예상외로 맛있었어요",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007968.1.27",
          "form": "더 달콤한 설레임맛임여",
          "original_form": "더 달콤한 설레임맛임여",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007968.1.28",
          "form": "오 설레임도 꽤 달달한 맛 아니었어?",
          "original_form": "오 설레임도 꽤 달달한 맛 아니었어?",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007968.1.29",
          "form": "그거보다 더 달콤하면",
          "original_form": "그거보다 더 달콤하면",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007968.1.30",
          "form": "음 나한테는 넘 달지도 모르겠다 ㅋㅋㅋ",
          "original_form": "음 나한테는 넘 달지도 모르겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007968.1.31",
          "form": "앗 그렇다면...어쩔수없구요",
          "original_form": "앗 그렇다면...어쩔수없구요",
          "speaker_id": "2",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007968.1.32",
          "form": "소다맛도 있는데",
          "original_form": "소다맛도 있는데",
          "speaker_id": "2",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007968.1.33",
          "form": "소다맛도 맛있을런지 궁금하네요",
          "original_form": "소다맛도 맛있을런지 궁금하네요",
          "speaker_id": "2",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007968.1.34",
          "form": "쭈쭈바계열 소다맛은",
          "original_form": "쭈쭈바계열 소다맛은",
          "speaker_id": "1",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007968.1.35",
          "form": "뽕따라는 최강자가 있어서",
          "original_form": "뽕따라는 최강자가 있어서",
          "speaker_id": "1",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007968.1.36",
          "form": "그거보다 더 맛있을지 궁금하네 ㅋㅋ",
          "original_form": "그거보다 더 맛있을지 궁금하네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007968.1.37",
          "form": "근데 전 사실",
          "original_form": "근데 전 사실",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007968.1.38",
          "form": "소다맛을 그렇게 좋아하진 않아요...",
          "original_form": "소다맛을 그렇게 좋아하진 않아요...",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007968.1.39",
          "form": "뭔가 손이 잘 안가는 맛",
          "original_form": "뭔가 손이 잘 안가는 맛",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007968.1.40",
          "form": "ㅋㅋㅋㅋ 뭔가 딸기나 초코, 바닐라는 아 이게 무슨 맛이구나! 이걸 아는데",
          "original_form": "ㅋㅋㅋㅋ 뭔가 딸기나 초코, 바닐라는 아 이게 무슨 맛이구나! 이걸 아는데",
          "speaker_id": "1",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007968.1.41",
          "form": "소다는 갑자기 튀어나온",
          "original_form": "소다는 갑자기 튀어나온",
          "speaker_id": "1",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007968.1.42",
          "form": "인위적인 달콤한 맛이라 그런가봐",
          "original_form": "인위적인 달콤한 맛이라 그런가봐",
          "speaker_id": "1",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007968.1.43",
          "form": "벌써 점심시간이네",
          "original_form": "벌써 점심시간이네",
          "speaker_id": "1",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007968.1.44",
          "form": "점심 먹었어?",
          "original_form": "점심 먹었어?",
          "speaker_id": "1",
          "time": "20210822 12:34"
        },
        {
          "id": "MDRW2100007968.1.45",
          "form": "난 라면 먹고 싶은데 집에 없어서 고민이야...",
          "original_form": "난 라면 먹고 싶은데 집에 없어서 고민이야...",
          "speaker_id": "1",
          "time": "20210822 12:35"
        }
      ]
