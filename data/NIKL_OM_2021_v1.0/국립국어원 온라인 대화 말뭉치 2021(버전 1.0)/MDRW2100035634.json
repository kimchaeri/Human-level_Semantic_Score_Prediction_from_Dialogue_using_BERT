      "utterance": [
        {
          "id": "MDRW2100035634.1.1",
          "form": "오 좋으셨겠어요",
          "original_form": "오 좋으셨겠어요",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035634.1.2",
          "form": "근데 거기에 까마귀가 진짜 엄청 많았어요",
          "original_form": "근데 거기에 까마귀가 진짜 엄청 많았어요",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035634.1.3",
          "form": "멋지던가요?",
          "original_form": "멋지던가요?",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035634.1.4",
          "form": "으앙 무시라",
          "original_form": "으앙 무시라",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035634.1.5",
          "form": "까마귀가 성위에서 까악까악하고",
          "original_form": "까마귀가 성위에서 까악까악하고",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035634.1.6",
          "form": "떼처럼 몰려서",
          "original_form": "떼처럼 몰려서",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035634.1.7",
          "form": "안그래도 그때 비오고 날도 흐렸는데",
          "original_form": "안그래도 그때 비오고 날도 흐렸는데",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035634.1.8",
          "form": "무서웠어요ㅠㅠ",
          "original_form": "무서웠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035634.1.9",
          "form": "ㅋㅋㅋㅋ약간 새우깡을 노리는 갈매기들처럼",
          "original_form": "ㅋㅋㅋㅋ약간 새우깡을 노리는 갈매기들처럼",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035634.1.10",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035634.1.11",
          "form": "오사카성 벚꽃필때 가면 완전히 다른 느낌이라던데",
          "original_form": "오사카성 벚꽃필때 가면 완전히 다른 느낌이라던데",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035634.1.12",
          "form": "제 기억속엔 약간",
          "original_form": "제 기억속엔 약간",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035634.1.13",
          "form": "폐가같은....ㅋㅋㅋㅋㅋ",
          "original_form": "폐가같은....ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035634.1.14",
          "form": "벚꽃핀 폐가ㅋㅋ언제  또 가볼수있을까싶어요ㅋㅋ",
          "original_form": "벚꽃핀 폐가ㅋㅋ언제  또 가볼수있을까싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035634.1.15",
          "form": "그러게용",
          "original_form": "그러게용",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035634.1.16",
          "form": "한 3년은 못갈거같은데ㅜ",
          "original_form": "한 3년은 못갈거같은데ㅜ",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035634.1.17",
          "form": "저는 음식이 입에 맞지않아서",
          "original_form": "저는 음식이 입에 맞지않아서",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035634.1.18",
          "form": "갈 의사는 없지만",
          "original_form": "갈 의사는 없지만",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035634.1.19",
          "form": "음식 빼고는 나쁘진 않았어용!",
          "original_form": "음식 빼고는 나쁘진 않았어용!",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035634.1.20",
          "form": "ㅋㅋ 또 가고싶은데 있으세요?",
          "original_form": "ㅋㅋ 또 가고싶은데 있으세요?",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035634.1.21",
          "form": "달고 짜서ㅠㅠㅠㅋㅋㅋㅋ",
          "original_form": "달고 짜서ㅠㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035634.1.22",
          "form": "저 대만가고싶어요",
          "original_form": "저 대만가고싶어요",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035634.1.23",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035634.1.24",
          "form": "저 거기 시장 가보고싶어요",
          "original_form": "저 거기 시장 가보고싶어요",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035634.1.25",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035634.1.26",
          "form": "시장 유명한데 맞죠?",
          "original_form": "시장 유명한데 맞죠?",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035634.1.27",
          "form": "망고빙수도 먹고싶고",
          "original_form": "망고빙수도 먹고싶고",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035634.1.28",
          "form": "네네! 그 관광지 이름이 기억이 안나는뎅",
          "original_form": "네네! 그 관광지 이름이 기억이 안나는뎅",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035634.1.29",
          "form": "유명한 관광지 시장 하나 있어요",
          "original_form": "유명한 관광지 시장 하나 있어요",
          "speaker_id": "1",
          "time": "20210829 21:42"
        }
      ]
    }
  ]
