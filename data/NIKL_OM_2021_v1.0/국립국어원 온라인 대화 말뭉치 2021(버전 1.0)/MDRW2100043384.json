      "utterance": [
        {
          "id": "MDRW2100043384.1.1",
          "form": "찌개는 그러려고 끓이는게 아닌가요ㅋㅋㅋ",
          "original_form": "찌개는 그러려고 끓이는게 아닌가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043384.1.2",
          "form": "엄마 놀러갈때 찌개 한솥하고 감",
          "original_form": "엄마 놀러갈때 찌개 한솥하고 감",
          "speaker_id": "2",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043384.1.3",
          "form": "아오 ㅋㅋ 글킨하죠",
          "original_form": "아오 ㅋㅋ 글킨하죠",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043384.1.4",
          "form": "그래도 매일 먹으면 질리니까",
          "original_form": "그래도 매일 먹으면 질리니까",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043384.1.5",
          "form": "오늘내일은 찌개드시고",
          "original_form": "오늘내일은 찌개드시고",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043384.1.6",
          "form": "3~4일째는 앞에 고기두고 보면서 찌개드세요 ㅋㅋ",
          "original_form": "3~4일째는 앞에 고기두고 보면서 찌개드세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043384.1.7",
          "form": "다먹어가면 라면사리넣어서 김치라면 가능",
          "original_form": "다먹어가면 라면사리넣어서 김치라면 가능",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043384.1.8",
          "form": "아놔 자린고비냐구요",
          "original_form": "아놔 자린고비냐구요",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043384.1.9",
          "form": "매달아놓고 쳐다보기",
          "original_form": "매달아놓고 쳐다보기",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043384.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 김치라면도 맛있겠다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 김치라면도 맛있겠다",
          "speaker_id": "1",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043384.1.11",
          "form": "약간 2d남친... 그런 거죠",
          "original_form": "약간 2d남친... 그런 거죠",
          "speaker_id": "1",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043384.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100043384.1.13",
          "form": "없는..거잖아",
          "original_form": "없는..거잖아",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100043384.1.14",
          "form": "그럼... 2.5d? ㅎㅎ;; 암튼요 ㅋ",
          "original_form": "그럼... 2.5d? ㅎㅎ;; 암튼요 ㅋ",
          "speaker_id": "1",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100043384.1.15",
          "form": "암튼..  찌개를하던지..",
          "original_form": "암튼..  찌개를하던지..",
          "speaker_id": "2",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043384.1.16",
          "form": "배달의 민족을 훔쳐보기로",
          "original_form": "배달의 민족을 훔쳐보기로",
          "speaker_id": "2",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043384.1.17",
          "form": "아오 ㅋㅋ",
          "original_form": "아오 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043384.1.18",
          "form": "귀찮아요",
          "original_form": "귀찮아요",
          "speaker_id": "2",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043384.1.19",
          "form": "배달도 맛있긴 함....",
          "original_form": "배달도 맛있긴 함....",
          "speaker_id": "1",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043384.1.20",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043384.1.21",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043384.1.22",
          "form": "배달이..  짱이죠 돈이없어서 글치",
          "original_form": "배달이..  짱이죠 돈이없어서 글치",
          "speaker_id": "2",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043384.1.23",
          "form": "ㅋㅋ레알",
          "original_form": "ㅋㅋ레알",
          "speaker_id": "1",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.24",
          "form": "내가 고기구워먹는거보다 시켜먹는 찜닭이 건강에도 좋을거예요",
          "original_form": "내가 고기구워먹는거보다 시켜먹는 찜닭이 건강에도 좋을거예요",
          "speaker_id": "2",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.25",
          "form": "지금 그지라서 라면먹는 거지....",
          "original_form": "지금 그지라서 라면먹는 거지....",
          "speaker_id": "1",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.26",
          "form": "거긴 야채도있잖아",
          "original_form": "거긴 야채도있잖아",
          "speaker_id": "2",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.27",
          "form": "이 사람... 똑똑해",
          "original_form": "이 사람... 똑똑해",
          "speaker_id": "1",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.28",
          "form": "야채를 먹지않으면 위험합니다",
          "original_form": "야채를 먹지않으면 위험합니다",
          "speaker_id": "2",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.29",
          "form": "야채",
          "original_form": "야채",
          "speaker_id": "1",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.30",
          "form": "안경척",
          "original_form": "안경척",
          "speaker_id": "2",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.31",
          "form": "전 빼고먹는데",
          "original_form": "전 빼고먹는데",
          "speaker_id": "1",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.32",
          "form": "이거맞음?",
          "original_form": "이거맞음?",
          "speaker_id": "1",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.33",
          "form": "야채국물 먹으니 괜찮을듯",
          "original_form": "야채국물 먹으니 괜찮을듯",
          "speaker_id": "2",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.34",
          "form": "와.. 솔로몬이네",
          "original_form": "와.. 솔로몬이네",
          "speaker_id": "1",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100043384.1.35",
          "form": "따봉드립니다 ^^ 부상으로 찜닭 시켜드십쇼 ^^",
          "original_form": "따봉드립니다 ^^ 부상으로 찜닭 시켜드십쇼 ^^",
          "speaker_id": "1",
          "time": "20210907 18:42"
        },
        {
          "id": "MDRW2100043384.1.36",
          "form": "와 신난다~",
          "original_form": "와 신난다~",
