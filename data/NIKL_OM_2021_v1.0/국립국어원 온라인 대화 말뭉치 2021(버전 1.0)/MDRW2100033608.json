      "utterance": [
        {
          "id": "MDRW2100033608.1.1",
          "form": "오징어도 직접 만지고 보고",
          "original_form": "오징어도 직접 만지고 보고",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033608.1.2",
          "form": "둘째는 배우는거 없어요?",
          "original_form": "둘째는 배우는거 없어요?",
          "speaker_id": "1",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033608.1.3",
          "form": "그거보고 그리고 붙이고 색칠하고",
          "original_form": "그거보고 그리고 붙이고 색칠하고",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033608.1.4",
          "form": "오징어 관련 동화책보고 그런곳이예요 ㅎㅎ",
          "original_form": "오징어 관련 동화책보고 그런곳이예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100033608.1.5",
          "form": "둘째는 내년에 보내려고요",
          "original_form": "둘째는 내년에 보내려고요",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100033608.1.6",
          "form": "촉감놀이도 하는곳인가봐요",
          "original_form": "촉감놀이도 하는곳인가봐요",
          "speaker_id": "1",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100033608.1.7",
          "form": "근데 누나하는거보고 맨날",
          "original_form": "근데 누나하는거보고 맨날",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100033608.1.8",
          "form": "나도 미술학원가고싶다 이래요 ㅎ",
          "original_form": "나도 미술학원가고싶다 이래요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100033608.1.9",
          "form": "보통 첫째보고 배우는게 많으니",
          "original_form": "보통 첫째보고 배우는게 많으니",
          "speaker_id": "1",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033608.1.10",
          "form": "좀 미술재료로만하는곳이 아니라 다른면으로 확장시켜서 하는곳이더라구요",
          "original_form": "좀 미술재료로만하는곳이 아니라 다른면으로 확장시켜서 하는곳이더라구요",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033608.1.11",
          "form": "누나누나 부르는거보면 귀여울듯요",
          "original_form": "누나누나 부르는거보면 귀여울듯요",
          "speaker_id": "1",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033608.1.12",
          "form": "차가 없어서 맨날 데려다주고 델고오고해야하는게 좀 불편해요",
          "original_form": "차가 없어서 맨날 데려다주고 델고오고해야하는게 좀 불편해요",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033608.1.13",
          "form": "귀여워요 ㅎㅎ",
          "original_form": "귀여워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033608.1.14",
          "form": "너라고 하면 엄청 혼내요 ㅎㅎ",
          "original_form": "너라고 하면 엄청 혼내요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:02"
        },
        {
          "id": "MDRW2100033608.1.15",
          "form": "둘이 한살차이인데 둘째가 잘먹어서 몸무게는 같아요 ㅎ",
          "original_form": "둘이 한살차이인데 둘째가 잘먹어서 몸무게는 같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100033608.1.16",
          "form": "나중에 아마 너 야 할걸요ㅎ",
          "original_form": "나중에 아마 너 야 할걸요ㅎ",
          "speaker_id": "1",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100033608.1.17",
          "form": "그렇겠죠 ㅎㅎ",
          "original_form": "그렇겠죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100033608.1.18",
          "form": "남매는 글케되더라구요",
          "original_form": "남매는 글케되더라구요",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100033608.1.19",
          "form": "그래도 습관안되게하려고",
          "original_form": "그래도 습관안되게하려고",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100033608.1.20",
          "form": "전언니라 한번도 너라고한적은 없는데",
          "original_form": "전언니라 한번도 너라고한적은 없는데",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100033608.1.21",
          "form": "그건 확실히 혼내요 ㅎ",
          "original_form": "그건 확실히 혼내요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100033608.1.22",
          "form": "때리는거나 너라고 하는거는",
          "original_form": "때리는거나 너라고 하는거는",
          "speaker_id": "2",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100033608.1.23",
          "form": "그래도 오빠아니라서 다행ㅋ",
          "original_form": "그래도 오빠아니라서 다행ㅋ",
          "speaker_id": "1",
          "time": "20210824 17:05"
        }
      ]
    }
