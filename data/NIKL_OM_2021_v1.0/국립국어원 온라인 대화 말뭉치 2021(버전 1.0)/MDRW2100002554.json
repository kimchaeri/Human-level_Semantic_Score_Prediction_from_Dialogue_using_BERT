      "utterance": [
        {
          "id": "MDRW2100002554.1.1",
          "form": "name2님은 기억에 남는\r\n여행지가 있나요?",
          "original_form": "&name2&님은 기억에 남는\r\n여행지가 있나요?",
          "speaker_id": "1",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002554.1.2",
          "form": "전 스위스 제일 좋았어요",
          "original_form": "전 스위스 제일 좋았어요",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002554.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002554.1.4",
          "form": "저는 하와이요...ㅋㅋㅋㅋㅋ",
          "original_form": "저는 하와이요...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002554.1.5",
          "form": "오 하와이~ 가보고싶었는데",
          "original_form": "오 하와이~ 가보고싶었는데",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002554.1.6",
          "form": "아직못가봤네요",
          "original_form": "아직못가봤네요",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002554.1.7",
          "form": "스위스는 아직 못가봤는데\r\n저도 가보고 싶어요:)",
          "original_form": "스위스는 아직 못가봤는데\r\n저도 가보고 싶어요:)",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002554.1.8",
          "form": "저희 다시 갈 날 오겠죠? ㅠㅠㅋ",
          "original_form": "저희 다시 갈 날 오겠죠? ㅠㅠㅋ",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002554.1.9",
          "form": "와야할텐데요....ㅜㅜㅜ",
          "original_form": "와야할텐데요....ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002554.1.10",
          "form": "코로나가 끝나도...인종차별무섭고...",
          "original_form": "코로나가 끝나도...인종차별무섭고...",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002554.1.11",
          "form": "예전에 좀 다녀놓은게",
          "original_form": "예전에 좀 다녀놓은게",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002554.1.12",
          "form": "그나마 다행이다 싶어요",
          "original_form": "그나마 다행이다 싶어요",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002554.1.13",
          "form": "더 다닐걸 그랬어요ㅠ",
          "original_form": "더 다닐걸 그랬어요ㅠ",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002554.1.14",
          "form": "이렇게 일상이 많이 바뀔줄 몰랐는데",
          "original_form": "이렇게 일상이 많이 바뀔줄 몰랐는데",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002554.1.15",
          "form": "ㅜㅜ너무 갑작스러웠죠",
          "original_form": "ㅜㅜ너무 갑작스러웠죠",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002554.1.16",
          "form": "아직 가고픈곳이",
          "original_form": "아직 가고픈곳이",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002554.1.17",
          "form": "참 많은데ㅜㅜ",
          "original_form": "참 많은데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002554.1.18",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002554.1.19",
          "form": "모리셔스도 꼭 가보고 싶었는데ㅋㅋ",
          "original_form": "모리셔스도 꼭 가보고 싶었는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002554.1.20",
          "form": "이젠 괌이라도 다시 갈 수 있음 좋겠다 싶어요",
          "original_form": "이젠 괌이라도 다시 갈 수 있음 좋겠다 싶어요",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002554.1.21",
          "form": "휴양지 좋아하시나봐요",
          "original_form": "휴양지 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002554.1.22",
          "form": "저는 휴양지쪽은 거의 못가봤어요",
          "original_form": "저는 휴양지쪽은 거의 못가봤어요",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002554.1.23",
          "form": "체력이 저질이라\r\n휴양지를 찾게 되는거 같아여ㅋㅋ",
          "original_form": "체력이 저질이라\r\n휴양지를 찾게 되는거 같아여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002554.1.24",
          "form": "저도 체력 저질인데ㅎㅎ 휴양지가서 느긋하게 누워있고싶네요",
          "original_form": "저도 체력 저질인데ㅎㅎ 휴양지가서 느긋하게 누워있고싶네요",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002554.1.25",
          "form": "지금까진 엄청 걷는 곳들만...",
          "original_form": "지금까진 엄청 걷는 곳들만...",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002554.1.26",
          "form": "갔어서..",
          "original_form": "갔어서..",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002554.1.27",
          "form": "ㅋㅋ 더 격렬히 아무것도 안하고픈ㅋㅋ",
          "original_form": "ㅋㅋ 더 격렬히 아무것도 안하고픈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002554.1.28",
          "form": "근데 것도 젊을때 가야죠 ㅋㅋ",
          "original_form": "근데 것도 젊을때 가야죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002554.1.29",
          "form": "ㅋㅋ 그건그래요",
          "original_form": "ㅋㅋ 그건그래요",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002554.1.30",
          "form": "고생하면서 다니는것도",
          "original_form": "고생하면서 다니는것도",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002554.1.31",
          "form": "다추억이죠",
          "original_form": "다추억이죠",
          "speaker_id": "2",
          "time": "20210820 14:03"
        }
      ]
    }
