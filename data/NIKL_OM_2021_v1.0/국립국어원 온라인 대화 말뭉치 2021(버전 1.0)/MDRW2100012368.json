        {
          "id": "MDRW2100012368.1.1",
          "form": "겜 좋아하시나요",
          "original_form": "겜 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 12:52"
        },
        {
          "id": "MDRW2100012368.1.2",
          "form": "게임.....좋아하는데\r\n한번 빠짐 못 나올걸 알아서\r\n지금은 안하고 있어요",
          "original_form": "게임.....좋아하는데\r\n한번 빠짐 못 나올걸 알아서\r\n지금은 안하고 있어요",
          "speaker_id": "2",
          "time": "20210825 12:54"
        },
        {
          "id": "MDRW2100012368.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 12:54"
        },
        {
          "id": "MDRW2100012368.1.4",
          "form": "겜 좋아해요ㅎㅎ",
          "original_form": "겜 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100012368.1.5",
          "form": "어떤 게임 하세요?",
          "original_form": "어떤 게임 하세요?",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100012368.1.6",
          "form": "피파온라인이요",
          "original_form": "피파온라인이요",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100012368.1.7",
          "form": "축구겜 좋아하시는군용 ㅋㅋㅈ",
          "original_form": "축구겜 좋아하시는군용 ㅋㅋㅈ",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100012368.1.8",
          "form": "겜 아예 안하시나요",
          "original_form": "겜 아예 안하시나요",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100012368.1.9",
          "form": "지금은 하고 있는 게임 없어요",
          "original_form": "지금은 하고 있는 게임 없어요",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100012368.1.10",
          "form": "모바일 게임은 안하세요?",
          "original_form": "모바일 게임은 안하세요?",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100012368.1.11",
          "form": "넵 안해요ㅎㅎ",
          "original_form": "넵 안해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100012368.1.12",
          "form": "저는 마지막 게임이",
          "original_form": "저는 마지막 게임이",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100012368.1.13",
          "form": "애니팡 쿠키런",
          "original_form": "애니팡 쿠키런",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100012368.1.14",
          "form": "이었던거 같네요ㅋㅋ",
          "original_form": "이었던거 같네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100012368.1.15",
          "form": "오 추억의게임들ㅎㅎ",
          "original_form": "오 추억의게임들ㅎㅎ",
