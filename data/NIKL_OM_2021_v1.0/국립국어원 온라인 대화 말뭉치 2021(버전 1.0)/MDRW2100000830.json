      "utterance": [
        {
          "id": "MDRW2100000830.1.1",
          "form": "헐.. 책도 많이 보세요?",
          "original_form": "헐.. 책도 많이 보세요?",
          "speaker_id": "1",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000830.1.2",
          "form": "저는 책과 등지고 살아요..",
          "original_form": "저는 책과 등지고 살아요..",
          "speaker_id": "1",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000830.1.3",
          "form": "저는 한창 책 많이 봤었는데",
          "original_form": "저는 한창 책 많이 봤었는데",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000830.1.4",
          "form": "요새는 안보고 있어요",
          "original_form": "요새는 안보고 있어요",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000830.1.5",
          "form": "봐야되는데 흑....",
          "original_form": "봐야되는데 흑....",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000830.1.6",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000830.1.7",
          "form": "봐야한다니..?",
          "original_form": "봐야한다니..?",
          "speaker_id": "1",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000830.1.8",
          "form": "굳이..?",
          "original_form": "굳이..?",
          "speaker_id": "1",
          "time": "20210819 20:18"
        },
        {
          "id": "MDRW2100000830.1.9",
          "form": "물론 보면 좋긴한데에..",
          "original_form": "물론 보면 좋긴한데에..",
          "speaker_id": "1",
          "time": "20210819 20:18"
        },
        {
          "id": "MDRW2100000830.1.10",
          "form": "재미없잔아요! ㅋㅋㅎㅋ",
          "original_form": "재미없잔아요! ㅋㅋㅎㅋ",
          "speaker_id": "1",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000830.1.11",
          "form": "책도 재밌는 책은 되게 재밌더라구요",
          "original_form": "책도 재밌는 책은 되게 재밌더라구요",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000830.1.12",
          "form": "약간 상상력도 좋아지고ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "약간 상상력도 좋아지고ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000830.1.13",
          "form": "마지막으로 본 책이 언제에요?",
          "original_form": "마지막으로 본 책이 언제에요?",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000830.1.14",
          "form": "2주전?",
          "original_form": "2주전?",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000830.1.15",
          "form": "오늘은 이만 좀 쉴게요",
          "original_form": "오늘은 이만 좀 쉴게요",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000830.1.16",
          "form": "라는 책이에요",
          "original_form": "라는 책이에요",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000830.1.17",
          "form": "우와 어떤 주제에요?",
          "original_form": "우와 어떤 주제에요?",
          "speaker_id": "2",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000830.1.18",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000830.1.19",
          "form": "약간 위로하는 책?",
          "original_form": "약간 위로하는 책?",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000830.1.20",
          "form": "인생에 도움되는 이야기",
          "original_form": "인생에 도움되는 이야기",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000830.1.21",
          "form": "이런 느낌이에요",
          "original_form": "이런 느낌이에요",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000830.1.22",
          "form": "인스타에서 보고 샀엉ㅅ",
          "original_form": "인스타에서 보고 샀엉ㅅ",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000830.1.23",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000830.1.24",
          "form": "물론 다는 못봤지만",
          "original_form": "물론 다는 못봤지만",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000830.1.25",
          "form": "우와 좋을 거 같아요",
          "original_form": "우와 좋을 거 같아요",
          "speaker_id": "2",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000830.1.26",
          "form": "위로하는 느낌의 책 너무 좋죠",
          "original_form": "위로하는 느낌의 책 너무 좋죠",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000830.1.27",
          "form": "맞아요 위로가 필요할때 한번씩 보죠",
          "original_form": "맞아요 위로가 필요할때 한번씩 보죠",
          "speaker_id": "1",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000830.1.28",
          "form": "무슨 책 많이 보세요?",
          "original_form": "무슨 책 많이 보세요?",
          "speaker_id": "1",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000830.1.29",
          "form": "전 그냥 판타지 스릴러 뭐 이런 거 봐요 ㅎ",
          "original_form": "전 그냥 판타지 스릴러 뭐 이런 거 봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000830.1.30",
          "form": "나미야 잡화점의 기적 이것도 봤어요!",
          "original_form": "나미야 잡화점의 기적 이것도 봤어요!",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000830.1.31",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000830.1.32",
          "form": "역시 소설이 짱이죠!!",
          "original_form": "역시 소설이 짱이죠!!",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000830.1.33",
          "form": "아 나미야 저거 친구가 보던데",
          "original_form": "아 나미야 저거 친구가 보던데",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000830.1.34",
          "form": "그 책표지가 집에 위에 고양이있는거!",
          "original_form": "그 책표지가 집에 위에 고양이있는거!",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000830.1.35",
          "form": "맞아요 ㅎ 이 작가분 유명하시잖라요!",
          "original_form": "맞아요 ㅎ 이 작가분 유명하시잖라요!",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000830.1.36",
          "form": "헐 저도 담애 한번 봐야겠어요!!",
          "original_form": "헐 저도 담애 한번 봐야겠어요!!",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000830.1.37",
          "form": "저도 소설은 많이 봤었는데",
          "original_form": "저도 소설은 많이 봤었는데",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000830.1.38",
          "form": "물론 좀 됐지만..",
          "original_form": "물론 좀 됐지만..",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000830.1.39",
          "form": "봐보세요 나름 잔잔하니 재밌더라구요",
          "original_form": "봐보세요 나름 잔잔하니 재밌더라구요",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000830.1.40",
          "form": "타라덩컨 이거 많이 봤었어요",
          "original_form": "타라덩컨 이거 많이 봤었어요",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000830.1.41",
          "form": "시리즈로",
          "original_form": "시리즈로",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000830.1.42",
          "form": "약간의 감동도 있었어요!!",
          "original_form": "약간의 감동도 있었어요!!",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000830.1.43",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000830.1.44",
          "form": "오 타라덩컨이요? 궁금하네요 어떤 내용일지",
          "original_form": "오 타라덩컨이요? 궁금하네요 어떤 내용일지",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000830.1.45",
          "form": "전 감동있는거 좋아해요",
          "original_form": "전 감동있는거 좋아해요",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000830.1.46",
          "form": "타라덩컨은 약간 해리포터 느낌이에요",
          "original_form": "타라덩컨은 약간 해리포터 느낌이에요",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000830.1.47",
          "form": "호빗? 이런 판타지 마법쓰는 아이의 내용 소설",
          "original_form": "호빗? 이런 판타지 마법쓰는 아이의 내용 소설",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000830.1.48",
          "form": "헐 해리포터요? 심장이 뛰네요",
          "original_form": "헐 해리포터요? 심장이 뛰네요",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000830.1.49",
          "form": "저 그런 거 좋아해요",
          "original_form": "저 그런 거 좋아해요",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000830.1.50",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000830.1.51",
          "form": "판타지!! 재밌죠",
          "original_form": "판타지!! 재밌죠",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000830.1.52",
          "form": "네 완전이요,, 몰입감 장난아니잖아요,, 판타지 장르 좋아요",
          "original_form": "네 완전이요,, 몰입감 장난아니잖아요,, 판타지 장르 좋아요",
          "speaker_id": "2",
