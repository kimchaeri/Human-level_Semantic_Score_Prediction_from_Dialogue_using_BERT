      "utterance": [
        {
          "id": "MDRW2100037793.1.1",
          "form": "아... 그렇진않아요ㅋㅋㅋㅋ",
          "original_form": "아... 그렇진않아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037793.1.2",
          "form": "근데 보통 명절에 폭식한다고 하더라구욬ㅋㅋ",
          "original_form": "근데 보통 명절에 폭식한다고 하더라구욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037793.1.3",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100037793.1.4",
          "form": "명절을 휴일로 보내는편이라 그냥.. 특별하지 않고 똑같아요ㅜㅜ",
          "original_form": "명절을 휴일로 보내는편이라 그냥.. 특별하지 않고 똑같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100037793.1.5",
          "form": "전 설에는 폭식해요 ㅋㅋㅋ",
          "original_form": "전 설에는 폭식해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100037793.1.6",
          "form": "만두 킬러ㅋㅋㅋ",
          "original_form": "만두 킬러ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100037793.1.7",
          "form": "근데 송편은 그닥 ㅠ",
          "original_form": "근데 송편은 그닥 ㅠ",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100037793.1.8",
          "form": "ㅋㅋㅋㅋㅋ 아아 떡국은 인정이요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 아아 떡국은 인정이요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100037793.1.9",
          "form": "떡만두국맛있어요",
          "original_form": "떡만두국맛있어요{emoji:♡}",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100037793.1.10",
          "form": "추석음식은 손만 많이가고 맛있진 않은것 같아요",
          "original_form": "추석음식은 손만 많이가고 맛있진 않은것 같아요",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100037793.1.11",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100037793.1.12",
          "form": "갈비나 전을 좋아하긴하는데",
          "original_form": "갈비나 전을 좋아하긴하는데",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100037793.1.13",
          "form": "이젠 얘들은 명절음식이라기엔 좀...",
          "original_form": "이젠 얘들은 명절음식이라기엔 좀...",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100037793.1.14",
          "form": "걔넨 음식! ㅋㅋㅋㅋㅋ",
          "original_form": "걔넨 음식! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037793.1.15",
          "form": "갈비는 언제먹어도 맛있쥬...",
          "original_form": "갈비는 언제먹어도 맛있쥬...",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037793.1.16",
          "form": "근데 송편은 메인이긴한지 시장지나가면 떡집만 줄이 엄청 길더리구요!",
          "original_form": "근데 송편은 메인이긴한지 시장지나가면 떡집만 줄이 엄청 길더리구요!",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037793.1.17",
          "form": "하... 송편에 꿀송편만 있으면 안좋아 할 일도 없을텐데",
          "original_form": "하... 송편에 꿀송편만 있으면 안좋아 할 일도 없을텐데",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100037793.1.18",
          "form": "왜 그렇게 콩송편을 만드는지",
          "original_form": "왜 그렇게 콩송편을 만드는지",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100037793.1.19",
          "form": "근데 만들기가 압도적으로 쉽긴 해요...",
          "original_form": "근데 만들기가 압도적으로 쉽긴 해요...",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100037793.1.20",
          "form": "ㅋㅋㅋㅋ꿀송편이 맛있는데 요리알못이 생각해봐도 콩송편이 더 쉬울것 같네욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ꿀송편이 맛있는데 요리알못이 생각해봐도 콩송편이 더 쉬울것 같네욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100037793.1.21",
          "form": "그것두 있잖아요 그 생선으로만든 전인데..",
          "original_form": "그것두 있잖아요 그 생선으로만든 전인데..",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100037793.1.22",
          "form": "동태전?",
          "original_form": "동태전?",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100037793.1.23",
          "form": "그런가요..? 왜ㅜ갑자기 이름생각이 안나지ㅜㅜ",
          "original_form": "그런가요..? 왜ㅜ갑자기 이름생각이 안나지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100037793.1.24",
          "form": "그럼 명절에 명절음식 잘 안드시는건가용?",
          "original_form": "그럼 명절에 명절음식 잘 안드시는건가용?",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100037793.1.25",
          "form": "좋아하는 것만..?",
          "original_form": "좋아하는 것만..?",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037793.1.26",
          "form": "요즘엔 명절음식도 거의 사서 써요",
          "original_form": "요즘엔 명절음식도 거의 사서 써요",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037793.1.27",
          "form": "큰집에서",
          "original_form": "큰집에서",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037793.1.28",
          "form": "맞아유~ ㅎㅎ 저희도 아예 안해먹다가",
          "original_form": "맞아유~ ㅎㅎ 저희도 아예 안해먹다가",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037793.1.29",
          "form": "그래서 사실 조금 집어먹으면 딱히 남는건 없어요",
          "original_form": "그래서 사실 조금 집어먹으면 딱히 남는건 없어요",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037793.1.30",
          "form": "요즘엔 분위기라도 내자고 ㅋㅋㅋ 시장에서 송편사다 먹구 갈비먹구...",
          "original_form": "요즘엔 분위기라도 내자고 ㅋㅋㅋ 시장에서 송편사다 먹구 갈비먹구...",
          "speaker_id": "2",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100037793.1.31",
          "form": "만두가 부족한게 안타깝지만...",
          "original_form": "만두가 부족한게 안타깝지만...",
          "speaker_id": "1",
          "time": "20210901 18:50"
        }
      ]
    }
