        {
          "id": "MDRW2100033190.1.3",
          "form": "내일 하루종일비온대",
          "original_form": "내일 하루종일비온대",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033190.1.4",
          "form": "요즘 환절기라 그런지",
          "original_form": "요즘 환절기라 그런지",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033190.1.5",
          "form": "이게.. 가을?",
          "original_form": "이게.. 가을?",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033190.1.6",
          "form": "날씨가 확 추워지니까",
          "original_form": "날씨가 확 추워지니까",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033190.1.7",
          "form": "비염이 엄청 심해졌어",
          "original_form": "비염이 엄청 심해졌어",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033190.1.8",
          "form": "비염..",
          "original_form": "비염..",
          "speaker_id": "2",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100033190.1.9",
          "form": "그냥달고살아서",
          "original_form": "그냥달고살아서",
          "speaker_id": "2",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100033190.1.10",
          "form": "원래부터 콧구멍이 하나였다는 느낌으로 살아왔지만",
          "original_form": "원래부터 콧구멍이 하나였다는 느낌으로 살아왔지만",
          "speaker_id": "2",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100033190.1.11",
          "form": "이게왜나쁜말",
          "original_form": "이게왜나쁜말",
          "speaker_id": "2",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100033190.1.12",
          "form": "하이고야...",
          "original_form": "하이고야...",
          "speaker_id": "1",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100033190.1.13",
          "form": "여름에는 좀 괜찮았었는데",
          "original_form": "여름에는 좀 괜찮았었는데",
          "speaker_id": "1",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100033190.1.14",
          "form": "아무튼 계절영향은 거의 못받아옴",
          "original_form": "아무튼 계절영향은 거의 못받아옴",
          "speaker_id": "2",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100033190.1.15",
          "form": "싸늘해지니까 비염이 다시 왔네",
          "original_form": "싸늘해지니까 비염이 다시 왔네",
          "speaker_id": "1",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100033190.1.16",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100033190.1.17",
          "form": "근데 요즘 비 많이오니까",
          "original_form": "근데 요즘 비 많이오니까",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100033190.1.18",
          "form": "공기가 너무 좋아",
          "original_form": "공기가 너무 좋아",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100033190.1.19",
          "form": "비 조아",
          "original_form": "비 조아",
          "speaker_id": "2",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100033190.1.20",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "2",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100033190.1.21",
          "form": "미세먼지 맨날 좋음이야",
          "original_form": "미세먼지 맨날 좋음이야",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100033190.1.22",
          "form": "미세먼지 싫어",
          "original_form": "미세먼지 싫어",
          "speaker_id": "2",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100033190.1.23",
          "form": "비 조아",
          "original_form": "비 조아",
          "speaker_id": "2",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100033190.1.24",
          "form": "비오면 맨날 창문 열어놓고 숨쉬잖아 그래서",
          "original_form": "비오면 맨날 창문 열어놓고 숨쉬잖아 그래서",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100033190.1.25",
          "form": "좋은 공기 열심히 맡게....",
          "original_form": "좋은 공기 열심히 맡게....",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100033190.1.26",
          "form": "바람도 나름 많이불어서 시원해서 좋음",
          "original_form": "바람도 나름 많이불어서 시원해서 좋음",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100033190.1.27",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100033190.1.28",
          "form": "학교에 에어컨안틀고 시원함",
          "original_form": "학교에 에어컨안틀고 시원함",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100033190.1.29",
          "form": "이것이... 가을?",
          "original_form": "이것이... 가을?",
          "speaker_id": "1",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100033190.1.30",
          "form": "기분굿",
          "original_form": "기분굿",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100033190.1.31",
          "form": "아니 근데 우리반은 맨날 에어컨틀더라",
          "original_form": "아니 근데 우리반은 맨날 에어컨틀더라",
          "speaker_id": "1",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100033190.1.32",
          "form": "에어컨 바람 쐬니까 비염 더 심해져서",
          "original_form": "에어컨 바람 쐬니까 비염 더 심해져서",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100033190.1.33",
          "form": "그건 원래그럼",
          "original_form": "그건 원래그럼",
          "speaker_id": "2",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100033190.1.34",
          "form": "쌤",
          "original_form": "쌤",
          "speaker_id": "2",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100033190.1.35",
          "form": "들도",
          "original_form": "들도",
          "speaker_id": "2",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100033190.1.36",
          "form": "오늘 하루죙일 훌쩍거림",
          "original_form": "오늘 하루죙일 훌쩍거림",
          "speaker_id": "1",
          "time": "20210906 18:34"
        },
        {
          "id": "MDRW2100033190.1.37",
