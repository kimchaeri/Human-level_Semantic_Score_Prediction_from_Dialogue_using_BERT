      "utterance": [
        {
          "id": "MDRW2100007595.1.1",
          "form": "학교 2학기엔 비대면 풀리나??",
          "original_form": "학교 2학기엔 비대면 풀리나??",
          "speaker_id": "2",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007595.1.2",
          "form": "그럼 경제 쪽 전공이야?!",
          "original_form": "그럼 경제 쪽 전공이야?!",
          "speaker_id": "1",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007595.1.3",
          "form": "웅 비슷하지 경영!!",
          "original_form": "웅 비슷하지 경영!!",
          "speaker_id": "2",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007595.1.4",
          "form": "4단계라 안 풀릴듯.....",
          "original_form": "4단계라 안 풀릴듯.....",
          "speaker_id": "1",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007595.1.5",
          "form": "ㅠㅠㅠㅠㅠ그치ㅠㅠ 줌 지겹다",
          "original_form": "ㅠㅠㅠㅠㅠ그치ㅠㅠ 줌 지겹다",
          "speaker_id": "2",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007595.1.6",
          "form": "ㅠㅠ마자 친구도 사귀고",
          "original_form": "ㅠㅠ마자 친구도 사귀고",
          "speaker_id": "1",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007595.1.7",
          "form": "술도 먹고 하고싶은뎅...",
          "original_form": "술도 먹고 하고싶은뎅...",
          "speaker_id": "1",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007595.1.8",
          "form": "와 경영!!!",
          "original_form": "와 경영!!!",
          "speaker_id": "1",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007595.1.9",
          "form": "수업도 이제 현강으로 좀 듣고싶어ㅠㅠ",
          "original_form": "수업도 이제 현강으로 좀 듣고싶어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007595.1.10",
          "form": "그러게ㅠㅠㅠㅠㅠ",
          "original_form": "그러게ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007595.1.11",
          "form": "경영에선 어떤 거 배워?!?",
          "original_form": "경영에선 어떤 거 배워?!?",
          "speaker_id": "1",
          "time": "20210822 03:47"
        },
