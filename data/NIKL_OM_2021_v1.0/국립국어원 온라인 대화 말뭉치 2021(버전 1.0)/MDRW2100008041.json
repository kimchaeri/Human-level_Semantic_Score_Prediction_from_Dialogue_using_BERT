      "utterance": [
        {
          "id": "MDRW2100008041.1.1",
          "form": "최근에 즐겨 읽으신 책이 있나요?",
          "original_form": "최근에 즐겨 읽으신 책이 있나요?",
          "speaker_id": "1",
          "time": "20210822 12:51"
        },
        {
          "id": "MDRW2100008041.1.2",
          "form": "삼국지요",
          "original_form": "삼국지요",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008041.1.3",
          "form": "삼국지 어렸을때 필독 도서였는데",
          "original_form": "삼국지 어렸을때 필독 도서였는데",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008041.1.4",
          "form": "정작 저는 안 읽어봤어요",
          "original_form": "정작 저는 안 읽어봤어요",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008041.1.5",
          "form": "무슨 책 좋아하시나요",
          "original_form": "무슨 책 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100008041.1.6",
          "form": "저는 히가시노 게이고 소설 좋아해요",
          "original_form": "저는 히가시노 게이고 소설 좋아해요",
          "speaker_id": "1",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100008041.1.7",
          "form": "오 이유가?",
          "original_form": "오 이유가?",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100008041.1.8",
          "form": "매 소설마다 반전이 있는데 그럼에도 불구하고 신선하더라구요",
          "original_form": "매 소설마다 반전이 있는데 그럼에도 불구하고 신선하더라구요",
          "speaker_id": "1",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100008041.1.9",
          "form": "오 재밌군요",
          "original_form": "오 재밌군요",
          "speaker_id": "2",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008041.1.10",
          "form": "무슨 장르의 책 좋아하세요?",
          "original_form": "무슨 장르의 책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 13:41"
        },
        {
          "id": "MDRW2100008041.1.11",
          "form": "저는 역사요",
          "original_form": "저는 역사요",
          "speaker_id": "2",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100008041.1.12",
          "form": "최근에 읽으신 책 추천해주신다면요?",
          "original_form": "최근에 읽으신 책 추천해주신다면요?",
          "speaker_id": "1",
          "time": "20210822 14:22"
        },
        {
          "id": "MDRW2100008041.1.13",
          "form": "음 기억에 남는게 없네요",
          "original_form": "음 기억에 남는게 없네요",
          "speaker_id": "2",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008041.1.14",
          "form": "요즘엔 인터넷으로 소설 보는 서비스도 있는데 이용해보신적 있으세요?",
          "original_form": "요즘엔 인터넷으로 소설 보는 서비스도 있는데 이용해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 14:29"
        },
        {
          "id": "MDRW2100008041.1.15",
          "form": "아니요? 사용중이신가요",
          "original_form": "아니요? 사용중이신가요",
          "speaker_id": "2",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008041.1.16",
          "form": "한번 써봤는데 화면으로 글 읽기가 불편하더라구요",
          "original_form": "한번 써봤는데 화면으로 글 읽기가 불편하더라구요",
          "speaker_id": "1",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008041.1.17",
          "form": "아 별로군요!",
          "original_form": "아 별로군요!",
          "speaker_id": "2",
          "time": "20210822 14:48"
        }
      ]
