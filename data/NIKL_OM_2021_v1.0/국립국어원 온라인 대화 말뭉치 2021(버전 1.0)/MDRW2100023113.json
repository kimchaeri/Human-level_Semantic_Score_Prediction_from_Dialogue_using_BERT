      "utterance": [
        {
          "id": "MDRW2100023113.1.1",
          "form": "ㅇㅇ요즘 컨텐츠는",
          "original_form": "ㅇㅇ요즘 컨텐츠는",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023113.1.2",
          "form": "심하게 동적이고 현란하고 자극적이야",
          "original_form": "심하게 동적이고 현란하고 자극적이야",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023113.1.3",
          "form": "난 좀 정적이고 조용한게 좋아....",
          "original_form": "난 좀 정적이고 조용한게 좋아....",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023113.1.4",
          "form": "ㅇㅇ 굳이 그럴필요 없는데서까지...",
          "original_form": "ㅇㅇ 굳이 그럴필요 없는데서까지...",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023113.1.5",
          "form": "ㄹㅇ나 텍스트 너무 사랑해서",
          "original_form": "ㄹㅇ나 텍스트 너무 사랑해서",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023113.1.6",
          "form": "유튜브로 정보찾는거",
          "original_form": "유튜브로 정보찾는거",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023113.1.7",
          "form": "여전히 힘들어",
          "original_form": "여전히 힘들어",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023113.1.8",
          "form": "나도 텍스트가 훨 좋아",
          "original_form": "나도 텍스트가 훨 좋아",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023113.1.9",
          "form": "걍 이렇게 하세요 하면 되는걸",
          "original_form": "걍 이렇게 하세요 하면 되는걸",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023113.1.10",
          "form": "ㅇㅇ 훨씬 간단한데",
          "original_form": "ㅇㅇ 훨씬 간단한데",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023113.1.11",
          "form": "3분간 말로 하고있음",
          "original_form": "3분간 말로 하고있음",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023113.1.12",
          "form": "2배속해도 1분 30초야",
          "original_form": "2배속해도 1분 30초야",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023113.1.13",
          "form": "그니까 ㅋㅋㅋㅋㅋ 강제시청 해야함",
          "original_form": "그니까 ㅋㅋㅋㅋㅋ 강제시청 해야함",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023113.1.14",
          "form": "ㅇㅇ.....근데 이제",
          "original_form": "ㅇㅇ.....근데 이제",
          "speaker_id": "2",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023113.1.15",
          "form": "아무도 글로 올려주지 않아.....",
          "original_form": "아무도 글로 올려주지 않아.....",
          "speaker_id": "2",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023113.1.16",
          "form": "맞아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023113.1.17",
          "form": "내가 용납할 수 있는 영상은",
          "original_form": "내가 용납할 수 있는 영상은",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023113.1.18",
          "form": "인터넷강의 뿐인데",
          "original_form": "인터넷강의 뿐인데",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023113.1.19",
          "form": "요즘은 별 게 다 영상이야...",
          "original_form": "요즘은 별 게 다 영상이야...",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023113.1.20",
          "form": "ㅇㅇ......",
          "original_form": "ㅇㅇ......",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023113.1.21",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023113.1.22",
          "form": "이것이 세대차이인가...?",
          "original_form": "이것이 세대차이인가...?",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023113.1.23",
          "form": "그런가봄 ㅋㅋㅋㅋ",
          "original_form": "그런가봄 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023113.1.24",
          "form": "ㄹㅇ키오스크 사용 못하는 노인들 뭐라할것이 아님......",
          "original_form": "ㄹㅇ키오스크 사용 못하는 노인들 뭐라할것이 아님......",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023113.1.25",
          "form": "나 벌써 뒤쳐지는데",
          "original_form": "나 벌써 뒤쳐지는데",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023113.1.26",
          "form": "이대로 30년 지나면",
          "original_form": "이대로 30년 지나면",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023113.1.27",
          "form": "나 혼자 책 부여잡고 울고잇을듯",
          "original_form": "나 혼자 책 부여잡고 울고잇을듯",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023113.1.28",
          "form": "내 택스트를 돌려달라고",
          "original_form": "내 택스트를 돌려달라고",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023113.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그니까 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그니까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023113.1.30",
          "form": "나 이번에 원신 공략 찾는데",
          "original_form": "나 이번에 원신 공략 찾는데",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023113.1.31",
          "form": "거의 다 유튜브로 올려놓은거야",
          "original_form": "거의 다 유튜브로 올려놓은거야",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023113.1.32",
          "form": "텍스트 돌려달라...",
          "original_form": "텍스트 돌려달라...",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023113.1.33",
          "form": "ㅇㅇ.....근데 유튜브는",
          "original_form": "ㅇㅇ.....근데 유튜브는",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023113.1.34",
          "form": "어그로만끌고",
          "original_form": "어그로만끌고",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023113.1.35",
          "form": "내용은 아닌것도 많음",
          "original_form": "내용은 아닌것도 많음",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023113.1.36",
          "form": "나 오로바시유사는",
          "original_form": "나 오로바시유사는",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023113.1.37",
          "form": "사진만가지고는 방향감각이 안잡혀서",
          "original_form": "사진만가지고는 방향감각이 안잡혀서",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023113.1.38",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023113.1.39",
          "form": "영상 찾아보는데",
          "original_form": "영상 찾아보는데",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023113.1.40",
          "form": "그게 젤 답답함",
          "original_form": "그게 젤 답답함",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023113.1.41",
          "form": "제목은 공략영상인데",
          "original_form": "제목은 공략영상인데",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023113.1.42",
          "form": "절반동안 딴짓하잖아",
          "original_form": "절반동안 딴짓하잖아",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023113.1.43",
          "form": "ㅇㅇ 시간 때우려고 ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 시간 때우려고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023113.1.44",
          "form": "짜증나서 싫어요 누르고 꺼버림",
          "original_form": "짜증나서 싫어요 누르고 꺼버림",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023113.1.45",
          "form": "영상컨텐츠는 내 시간 써가면서 다 감상해야만",
          "original_form": "영상컨텐츠는 내 시간 써가면서 다 감상해야만",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023113.1.46",
          "form": "그 안에 무슨 내용이 있는지 알 수 있다는게",
          "original_form": "그 안에 무슨 내용이 있는지 알 수 있다는게",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023113.1.47",
          "form": "최악의 단점임",
          "original_form": "최악의 단점임",
          "speaker_id": "1",
          "time": "20210829 21:58"
        }
      ]
    }
  ]
