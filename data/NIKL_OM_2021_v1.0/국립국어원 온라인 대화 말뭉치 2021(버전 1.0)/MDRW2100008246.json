      "utterance": [
        {
          "id": "MDRW2100008246.1.1",
          "form": "근데 그렇게 재밌지는 않다  저렇게 하는거 좀 봐서 그런지",
          "original_form": "근데 그렇게 재밌지는 않다  저렇게 하는거 좀 봐서 그런지",
          "speaker_id": "2",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008246.1.2",
          "form": "놀면뭐하니 스페셜",
          "original_form": "놀면뭐하니 스페셜",
          "speaker_id": "2",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008246.1.3",
          "form": "얼음라켓 엄청 차갑겠다",
          "original_form": "얼음라켓 엄청 차갑겠다",
          "speaker_id": "1",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008246.1.4",
          "form": "손으로 공을 잡을려고 한다 ㅋㅋ",
          "original_form": "손으로 공을 잡을려고 한다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008246.1.5",
          "form": "경기를 많이 하네",
          "original_form": "경기를 많이 하네",
          "speaker_id": "1",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008246.1.6",
          "form": "실전훈련은 또 뭔데 여태까지 한건",
          "original_form": "실전훈련은 또 뭔데 여태까지 한건",
          "speaker_id": "2",
          "time": "20210822 14:28"
        },
        {
          "id": "MDRW2100008246.1.7",
          "form": "재미없다 채널변경좀 하자",
          "original_form": "재미없다 채널변경좀 하자",
          "speaker_id": "2",
          "time": "20210822 14:29"
        },
        {
          "id": "MDRW2100008246.1.8",
          "form": "ㅋㅋ 뭐 볼래?  찾아봐",
          "original_form": "ㅋㅋ 뭐 볼래?  찾아봐",
          "speaker_id": "1",
          "time": "20210822 14:29"
        },
        {
          "id": "MDRW2100008246.1.9",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210822 14:29"
        },
        {
          "id": "MDRW2100008246.1.10",
          "form": "옥탑방 문제아들한다",
          "original_form": "옥탑방 문제아들한다",
          "speaker_id": "2",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008246.1.11",
          "form": "옥탑방의 문제아들 하네",
          "original_form": "옥탑방의 문제아들 하네",
          "speaker_id": "1",
          "time": "20210822 14:33"
        },
        {
          "id": "MDRW2100008246.1.12",
          "form": "같은말 했네 ㅋㅋㅋ 동시에",
          "original_form": "같은말 했네 ㅋㅋㅋ 동시에",
          "speaker_id": "2",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008246.1.13",
          "form": "이분 연기 잘하던데",
          "original_form": "이분 연기 잘하던데",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008246.1.14",
          "form": "찌 찌 뽕",
          "original_form": "찌 찌 뽕",
          "speaker_id": "2",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008246.1.15",
          "form": "ㅋㅋ  그러게 그러기 드문데",
          "original_form": "ㅋㅋ  그러게 그러기 드문데",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008246.1.16",
          "form": "그러게 김용만이 한 말 동감이다 카리스마",
          "original_form": "그러게 김용만이 한 말 동감이다 카리스마",
          "speaker_id": "2",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008246.1.17",
          "form": "진짜 어느 역할마다 카리스마 있게 잘함",
          "original_form": "진짜 어느 역할마다 카리스마 있게 잘함",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008246.1.18",
          "form": "배우라는게 절절 흘러 나온다",
          "original_form": "배우라는게 절절 흘러 나온다",
