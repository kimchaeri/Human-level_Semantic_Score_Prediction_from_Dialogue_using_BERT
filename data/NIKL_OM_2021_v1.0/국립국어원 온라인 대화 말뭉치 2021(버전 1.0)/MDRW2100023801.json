      "utterance": [
        {
          "id": "MDRW2100023801.1.1",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023801.1.2",
          "form": "그리고 약간 숨 막히는 느낌이야",
          "original_form": "그리고 약간 숨 막히는 느낌이야",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023801.1.3",
          "form": "습한거 같아",
          "original_form": "습한거 같아",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023801.1.4",
          "form": "그니까 갑자기 어제는 이런 느낌이 아니었는데",
          "original_form": "그니까 갑자기 어제는 이런 느낌이 아니었는데",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023801.1.5",
          "form": "전혀",
          "original_form": "전혀",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023801.1.6",
          "form": "왜 장 보러 가려니까 날씨가 이 모양이 되냐",
          "original_form": "왜 장 보러 가려니까 날씨가 이 모양이 되냐",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023801.1.7",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023801.1.8",
          "form": "가지 말라는 건가?",
          "original_form": "가지 말라는 건가?",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023801.1.9",
          "form": "오늘은 걸어가면 안 될 거 같은데",
          "original_form": "오늘은 걸어가면 안 될 거 같은데",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023801.1.10",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023801.1.11",
          "form": "버스타고 가야할 듯",
          "original_form": "버스타고 가야할 듯",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023801.1.12",
          "form": "꼭 오늘 가야 되나?",
          "original_form": "꼭 오늘 가야 되나?",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023801.1.13",
          "form": "모르겠어",
          "original_form": "모르겠어",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023801.1.14",
          "form": "올 때는 무조건 버슨데",
          "original_form": "올 때는 무조건 버슨데",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023801.1.15",
          "form": "갈 때도 버스타야 할 듯",
          "original_form": "갈 때도 버스타야 할 듯",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023801.1.16",
          "form": "그러니까 말이야",
          "original_form": "그러니까 말이야",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023801.1.17",
          "form": "내일 비 엄청 온다는데?",
