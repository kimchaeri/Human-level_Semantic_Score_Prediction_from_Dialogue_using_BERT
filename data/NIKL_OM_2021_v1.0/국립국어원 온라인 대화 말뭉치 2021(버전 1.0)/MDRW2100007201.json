      "utterance": [
        {
          "id": "MDRW2100007201.1.1",
          "form": "저 경주 여행 간 날 비가 많이 왔어요",
          "original_form": "저 경주 여행 간 날 비가 많이 왔어요",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007201.1.2",
          "form": "그게 아직도 아쉬워요 ㅋㅋㅋ",
          "original_form": "그게 아직도 아쉬워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007201.1.3",
          "form": "제가 사는 데는 일기예보가 일주일 내내 비예요",
          "original_form": "제가 사는 데는 일기예보가 일주일 내내 비예요",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007201.1.4",
          "form": "저는 비 싫어해요",
          "original_form": "저는 비 싫어해요",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007201.1.5",
          "form": "제가 사는 곳은 생각보다 날씨가 좋았어요",
          "original_form": "제가 사는 곳은 생각보다 날씨가 좋았어요",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007201.1.6",
          "form": "저도 비 싫어해요",
          "original_form": "저도 비 싫어해요",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007201.1.7",
          "form": "비오는날은 무조건 집",
          "original_form": "비오는날은 무조건 집",
          "speaker_id": "2",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007201.1.8",
          "form": "젖는 거 너무 싫어요 ㅠㅠ",
          "original_form": "젖는 거 너무 싫어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007201.1.9",
          "form": "그리고 꿉꿉하자나요 ㅠㅠ",
          "original_form": "그리고 꿉꿉하자나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007201.1.10",
          "form": "제습기 있으신가요?",
          "original_form": "제습기 있으신가요?",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007201.1.11",
          "form": "없답니다....",
          "original_form": "없답니다....",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007201.1.12",
          "form": "스타일러는 있어서 빨래 안 마를 땐 비상조치는 해요",
          "original_form": "스타일러는 있어서 빨래 안 마를 땐 비상조치는 해요",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007201.1.13",
          "form": "그치만 오늘도 빨래 널며 건조기를 꿈꿨죠 ㅋㅋㅋ",
          "original_form": "그치만 오늘도 빨래 널며 건조기를 꿈꿨죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007201.1.14",
          "form": "우와 스타일러가 있으세요? 부럽다 ㅠㅠ",
          "original_form": "우와 스타일러가 있으세요? 부럽다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007201.1.15",
          "form": "스타일러 어때요?",
          "original_form": "스타일러 어때요?",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007201.1.16",
          "form": "스타일러 제 인생 첫 장기할부였는데요",
          "original_form": "스타일러 제 인생 첫 장기할부였는데요",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007201.1.17",
          "form": "있기 전의 삶으로 못 돌아가요 정망",
          "original_form": "있기 전의 삶으로 못 돌아가요 정망",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007201.1.18",
          "form": "최고",
          "original_form": "최고",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007201.1.19",
          "form": "냄새 제거도 해주고",
          "original_form": "냄새 제거도 해주고",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007201.1.20",
          "form": "혹시 나중에 사실 거면 꼭 큰 걸로 사세요 그게 훨씬 활용도 높아요!!",
          "original_form": "혹시 나중에 사실 거면 꼭 큰 걸로 사세요 그게 훨씬 활용도 높아요!!",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007201.1.21",
          "form": "다리미 기능도 있는거죠?",
          "original_form": "다리미 기능도 있는거죠?",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007201.1.22",
          "form": "요즘 시국에",
          "original_form": "요즘 시국에",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007201.1.23",
          "form": "거거익선이군요ㅋㅋㅋㅋㅋ",
          "original_form": "거거익선이군요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007201.1.24",
          "form": "매일 빨기 어려운 옷",
          "original_form": "매일 빨기 어려운 옷",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007201.1.25",
          "form": "식세기처럼..",
          "original_form": "식세기처럼..",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007201.1.26",
          "form": "넣고 돌리면 맘이 편안.....",
          "original_form": "넣고 돌리면 맘이 편안.....",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007201.1.27",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007201.1.28",
          "form": "니트같은거 넣으면 참 좋겠네요 ㅠㅠㅠ",
          "original_form": "니트같은거 넣으면 참 좋겠네요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007201.1.29",
          "form": "스타일링 기능이 다리미 역할 비슷해서 생활 주름은 대강 정리가 되는 것 같아요",
          "original_form": "스타일링 기능이 다리미 역할 비슷해서 생활 주름은 대강 정리가 되는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007201.1.30",
          "form": "맞아요 겨울에 진가를 발휘해요",
          "original_form": "맞아요 겨울에 진가를 발휘해요",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007201.1.31",
          "form": "코트랑 니트",
          "original_form": "코트랑 니트",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007201.1.32",
          "form": "겨울은 좋아하세요?",
          "original_form": "겨울은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007201.1.33",
          "form": "저는 정말 싫어해요",
          "original_form": "저는 정말 싫어해요",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007201.1.34",
          "form": "전 겨울 괜찮아요",
          "original_form": "전 겨울 괜찮아요",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007201.1.35",
          "form": "추위 많이 타셔서 싫어하시나요?",
          "original_form": "추위 많이 타셔서 싫어하시나요?",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007201.1.36",
          "form": "그것도 그렇고",
          "original_form": "그것도 그렇고",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007201.1.37",
          "form": "저는 해가 짧은 게 무척 치명적이에요",
          "original_form": "저는 해가 짧은 게 무척 치명적이에요",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007201.1.38",
          "form": "우와 전 밤도 좋아하는데",
          "original_form": "우와 전 밤도 좋아하는데",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007201.1.39",
          "form": "해가 짧아져서 특별히 불편하신게 있나요?",
          "original_form": "해가 짧아져서 특별히 불편하신게 있나요?",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007201.1.40",
          "form": "하루가 더 짧은 느낌이라 마음이 더 조급해져요",
          "original_form": "하루가 더 짧은 느낌이라 마음이 더 조급해져요",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007201.1.41",
          "form": "괜히 ㅎㅎㅎ",
          "original_form": "괜히 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007201.1.42",
          "form": "아침에 일어나는데 아직 어두운 것도 너무 싫고요 ㅠㅠ",
          "original_form": "아침에 일어나는데 아직 어두운 것도 너무 싫고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007201.1.43",
          "form": "어두운 출근길 ㅠㅠ",
          "original_form": "어두운 출근길 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007201.1.44",
          "form": "우와 전 그런 생각 해본적이 없는데",
          "original_form": "우와 전 그런 생각 해본적이 없는데",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007201.1.45",
          "form": "엄청 부지런 하신 것 같아요",
          "original_form": "엄청 부지런 하신 것 같아요",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007201.1.46",
          "form": "어두운 출근길은 싫긴하다 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "어두운 출근길은 싫긴하다 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007201.1.47",
          "form": "안 그래도 울적한데 해도 없으면 더 다운돼요 ㅋㅋㅋㅋ",
          "original_form": "안 그래도 울적한데 해도 없으면 더 다운돼요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:13"
        }
      ]
