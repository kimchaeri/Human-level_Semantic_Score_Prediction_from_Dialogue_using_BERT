        },
        {
          "id": "MDRW2100011758.1.2",
          "form": "학교 (등교) 생활은 싫오",
          "original_form": "학교 (등교) 생활은 싫오",
          "speaker_id": "2",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011758.1.3",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011758.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나도..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나도..",
          "speaker_id": "1",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011758.1.5",
          "form": "이번방학은 어떻게 보냈니",
          "original_form": "이번방학은 어떻게 보냈니",
          "speaker_id": "1",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011758.1.6",
          "form": "약간의 공부와 많은 게임",
          "original_form": "약간의 공부와 많은 게임",
          "speaker_id": "2",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011758.1.7",
          "form": "+많은 코난",
          "original_form": "+많은 코난",
          "speaker_id": "2",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011758.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나랑똑같네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나랑똑같네",
          "speaker_id": "1",
          "time": "20210824 15:51"
        },
        {
          "id": "MDRW2100011758.1.9",
          "form": "약간의 독서와 아주약간의 공부와 많은 테런 +많은자빱",
          "original_form": "약간의 독서와 아주약간의 공부와 많은 테런 +많은자빱",
          "speaker_id": "1",
          "time": "20210824 15:51"
        },
        {
          "id": "MDRW2100011758.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100011758.1.11",
          "form": "진짜 계획 세운거 싸그리 무시",
          "original_form": "진짜 계획 세운거 싸그리 무시",
          "speaker_id": "2",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100011758.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011758.1.13",
          "form": "너 책 몇권읽는다 그랬더라??",
          "original_form": "너 책 몇권읽는다 그랬더라??",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011758.1.14",
          "form": "50권이었나",
          "original_form": "50권이었나",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011758.1.15",
          "form": "20권....",
          "original_form": "20권....",
          "speaker_id": "2",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011758.1.16",
          "form": "맞다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011758.1.17",
          "form": "몇권읽었어?",
          "original_form": "몇권읽었어?",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011758.1.18",
          "form": "50권은 무슨 5권도 겨우 읽음",
          "original_form": "50권은 무슨 5권도 겨우 읽음",
          "speaker_id": "2",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011758.1.19",
          "form": "지금 10몇권 읽었나",
          "original_form": "지금 10몇권 읽었나",
          "speaker_id": "2",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011758.1.20",
          "form": "오 그래도 많이읽은거아니야??",
          "original_form": "오 그래도 많이읽은거아니야??",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011758.1.21",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011758.1.22",
          "form": "코난 본 시간에 책을 읽고",
          "original_form": "코난 본 시간에 책을 읽고",
          "speaker_id": "2",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011758.1.23",
          "form": "난...10권도 안될듯?ㅋㅋㅋ",
          "original_form": "난...10권도 안될듯?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011758.1.24",
          "form": "책 본 시간애 코난을 봤었어야..",
          "original_form": "책 본 시간애 코난을 봤었어야..",
          "speaker_id": "2",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011758.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011758.1.26",
          "form": "넌 무슨 책 읽었어?",
          "original_form": "넌 무슨 책 읽었어?",
          "speaker_id": "2",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011758.1.27",
          "form": "내가 테런한 시간만큼 책읽었으면",
          "original_form": "내가 테런한 시간만큼 책읽었으면",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011758.1.28",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011758.1.29",
          "form": "난",
          "original_form": "난",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011758.1.30",
          "form": "그동안 집에 사두고 쌓아둔책 읽었지",
          "original_form": "그동안 집에 사두고 쌓아둔책 읽었지",
          "speaker_id": "1",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011758.1.31",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011758.1.32",
          "form": "정세랑 작가?",
          "original_form": "정세랑 작가?",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011758.1.33",
          "form": "눈앞이 컴컴하다",
          "original_form": "눈앞이 컴컴하다",
          "speaker_id": "2",
          "time": "20210827 18:34"
        }
      ]
    }
