      "utterance": [
        {
          "id": "MDRW2100037984.1.1",
          "form": "근데 내가알기론",
          "original_form": "근데 내가알기론",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037984.1.2",
          "form": "백신맞고 3일인가만 헌혈안되지않나여",
          "original_form": "백신맞고 3일인가만 헌혈안되지않나여",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037984.1.3",
          "form": "3일밖에 안됑?",
          "original_form": "3일밖에 안됑?",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037984.1.4",
          "form": "그런감 그럼괜찮을듯",
          "original_form": "그런감 그럼괜찮을듯",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037984.1.5",
          "form": "나는 어디선가 그렇게들은거같은데 출처가 확실치않음 ㅋㅋ",
          "original_form": "나는 어디선가 그렇게들은거같은데 출처가 확실치않음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100037984.1.6",
          "form": "의사선생님이 진단해주니까 갠차나",
          "original_form": "의사선생님이 진단해주니까 갠차나",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100037984.1.7",
          "form": "낼부터 의료노조파업이군 아푸지마세용",
          "original_form": "낼부터 의료노조파업이군 아푸지마세용",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037984.1.8",
          "form": "내동생말론",
          "original_form": "내동생말론",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037984.1.9",
          "form": "본인은참여안한다던데",
          "original_form": "본인은참여안한다던데",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037984.1.10",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037984.1.11",
          "form": "공가를 쓸수있긴하지만 동생입장에서는 딱히 메리트없다고",
          "original_form": "공가를 쓸수있긴하지만 동생입장에서는 딱히 메리트없다고",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037984.1.12",
          "form": "간호사 인권에 목소리를낸게 의미있는거지",
          "original_form": "간호사 인권에 목소리를낸게 의미있는거지",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100037984.1.13",
          "form": "하 다 진짜 월욜부터  다이어트중임",
          "original_form": "하 다 진짜 월욜부터  다이어트중임",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100037984.1.14",
          "form": "건강식 돌입!",
          "original_form": "건강식 돌입!",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100037984.1.15",
          "form": "오늘도걷기운동했나염",
          "original_form": "오늘도걷기운동했나염",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100037984.1.16",
          "form": "요새 식단조절 시작해서 우울해서 버스타고다녔음ㅜㅜ",
          "original_form": "요새 식단조절 시작해서 우울해서 버스타고다녔음ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100037984.1.17",
          "form": "또 운동 안하다보니 걷기가 시르네? 허허ㅜ",
          "original_form": "또 운동 안하다보니 걷기가 시르네? 허허ㅜ",
          "speaker_id": "2",
          "time": "20210902 09:26"
        },
        {
          "id": "MDRW2100037984.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210902 09:27"
        },
        {
          "id": "MDRW2100037984.1.19",
          "form": "마음이 안내키다보다",
          "original_form": "마음이 안내키다보다",
          "speaker_id": "1",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100037984.1.20",
          "form": "하지만 습관잡아야대용",
          "original_form": "하지만 습관잡아야대용",
          "speaker_id": "1",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100037984.1.21",
          "form": "왜 또 추적추적비가오고",
          "original_form": "왜 또 추적추적비가오고",
          "speaker_id": "2",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100037984.1.22",
          "form": "또 그동안 2만보 걸을땐 손에 1리터 프라페마시면서걸었음  ㅜㅜ",
          "original_form": "또 그동안 2만보 걸을땐 손에 1리터 프라페마시면서걸었음  ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100037984.1.23",
          "form": "보상이없으니 운동하기가싫네?ㅋㅋ",
          "original_form": "보상이없으니 운동하기가싫네?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100037984.1.24",
          "form": "ㅋㅋㅋ아유 물마셔야지요",
          "original_form": "ㅋㅋㅋ아유 물마셔야지요",
          "speaker_id": "1",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100037984.1.25",
          "form": "뭐랄까 핑계는 참많음 ㅋㅋㅋ",
          "original_form": "뭐랄까 핑계는 참많음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100037984.1.26",
          "form": "쉐이크 샀다면서?",
          "original_form": "쉐이크 샀다면서?",
          "speaker_id": "1",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100037984.1.27",
          "form": "하 맛이없어ㅜ",
          "original_form": "하 맛이없어ㅜ",
          "speaker_id": "2",
          "time": "20210902 09:29"
        }
      ]
    }
