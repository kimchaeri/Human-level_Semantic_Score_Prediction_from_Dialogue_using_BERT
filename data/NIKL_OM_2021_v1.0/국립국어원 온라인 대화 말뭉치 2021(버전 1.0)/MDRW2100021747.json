      "utterance": [
        {
          "id": "MDRW2100021747.1.1",
          "form": "저 충격이에요 ㅋㅋㅋ",
          "original_form": "저 충격이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021747.1.2",
          "form": "침펄풍이라는거 들어는 봤거든요 인터넷 커뮤같은 곳에서 ㅋㅋㅋ 그뜻인지몰랐어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "침펄풍이라는거 들어는 봤거든요 인터넷 커뮤같은 곳에서 ㅋㅋㅋ 그뜻인지몰랐어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021747.1.3",
          "form": "헐헐 ㅋㅋㅋ",
          "original_form": "헐헐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021747.1.4",
          "form": "진주같다해서 펄(주호민씨)",
          "original_form": "진주같다해서 펄(주호민씨)",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021747.1.5",
          "form": "김풍씨가 풍",
          "original_form": "김풍씨가 풍",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021747.1.6",
          "form": "셋이모이면 엄청웃겨요",
          "original_form": "셋이모이면 엄청웃겨요",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021747.1.7",
          "form": "중년이신대도",
          "original_form": "중년이신대도",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021747.1.8",
          "form": "건전하고 작가들이라",
          "original_form": "건전하고 작가들이라",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021747.1.9",
          "form": "상상력도 쩔고요",
          "original_form": "상상력도 쩔고요",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021747.1.10",
          "form": "ㅋㅋㅋㅋ 진짜 너무 충격이네요 그뜻일거라고 생각못했어요",
          "original_form": "ㅋㅋㅋㅋ 진짜 너무 충격이네요 그뜻일거라고 생각못했어요",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021747.1.11",
          "form": "왜 짤같은건 많이 본거같은데 늘 이말년씨와 주호민씨만 있었거든요 ㅋㅋㅋ",
          "original_form": "왜 짤같은건 많이 본거같은데 늘 이말년씨와 주호민씨만 있었거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021747.1.12",
          "form": "두분이서 더 친하고",
          "original_form": "두분이서 더 친하고",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021747.1.13",
          "form": "김풍님은 가끔나오시더라구요",
          "original_form": "김풍님은 가끔나오시더라구요",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021747.1.14",
          "form": "주호민작가님이랑은",
          "original_form": "주호민작가님이랑은",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021747.1.15",
          "form": "침펄토론이라는 콘텐츠가 있는데",
          "original_form": "침펄토론이라는 콘텐츠가 있는데",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021747.1.16",
          "form": "ㅋㅋㅋㅋ얘기만들어도 벌써웃기네요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ얘기만들어도 벌써웃기네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021747.1.17",
          "form": "그 막 말도안되는 주제로 하는거아닌가요 ㅋㅋ",
          "original_form": "그 막 말도안되는 주제로 하는거아닌가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021747.1.18",
          "form": "이상한 논리로 두분이서 정장입고",
          "original_form": "이상한 논리로 두분이서 정장입고",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021747.1.19",
          "form": "그니까요 ㅋㅋ그거 짤로 본거같아요 ㅋㅋ",
          "original_form": "그니까요 ㅋㅋ그거 짤로 본거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021747.1.20",
          "form": "네 진짜 웃겨요",
          "original_form": "네 진짜 웃겨요",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021747.1.21",
          "form": "ㅋㅋㅋ 저는 그 소영이? ㅋㅋ 나오는거 본적있어요",
          "original_form": "ㅋㅋㅋ 저는 그 소영이? ㅋㅋ 나오는거 본적있어요",
          "speaker_id": "1",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021747.1.22",
          "form": "출장십오야라구",
          "original_form": "출장십오야라구",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021747.1.23",
          "form": "나영석피디님이 출장가서",
          "original_form": "나영석피디님이 출장가서",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021747.1.24",
          "form": "퀴즈내고하는거도 나오셨는데",
          "original_form": "퀴즈내고하는거도 나오셨는데",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021747.1.25",
          "form": "아아 그거도 이름은 들어봤어요! ㅋㅋ",
          "original_form": "아아 그거도 이름은 들어봤어요! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021747.1.26",
          "form": "저 그거 진짜 많이 웃었어요",
          "original_form": "저 그거 진짜 많이 웃었어요",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021747.1.27",
          "form": "거기에 이말년이 나온거에요?",
          "original_form": "거기에 이말년이 나온거에요?",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021747.1.28",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021747.1.29",
          "form": "이말년 주호민 김풍 이종범 4분이서",
          "original_form": "이말년 주호민 김풍 이종범 4분이서",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021747.1.30",
          "form": "ㅋㅋㅋ 웃길거같네요 ㅋㅋㅋㅋㅋ보장된웃음ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 웃길거같네요 ㅋㅋㅋㅋㅋ보장된웃음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021747.1.31",
          "form": "나왔어요",
          "original_form": "나왔어요",
          "speaker_id": "2",
          "time": "20210828 22:12"
        }
      ]
    }
