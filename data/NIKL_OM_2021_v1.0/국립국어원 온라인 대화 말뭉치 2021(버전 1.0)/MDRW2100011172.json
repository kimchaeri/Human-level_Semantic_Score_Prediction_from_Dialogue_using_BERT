      "utterance": [
        {
          "id": "MDRW2100011172.1.1",
          "form": "안녕하세요 건강 어떻게 챙기세요?",
          "original_form": "안녕하세요 건강 어떻게 챙기세요?",
          "speaker_id": "1",
          "time": "20210824 02:20"
        },
        {
          "id": "MDRW2100011172.1.2",
          "form": "채식위주로 식사해요",
          "original_form": "채식위주로 식사해요",
          "speaker_id": "2",
          "time": "20210824 02:21"
        },
        {
          "id": "MDRW2100011172.1.3",
          "form": "저도요! 채식하는데 너무 만족스러워요",
          "original_form": "저도요! 채식하는데 너무 만족스러워요",
          "speaker_id": "1",
          "time": "20210824 02:22"
        },
        {
          "id": "MDRW2100011172.1.4",
          "form": "영양제는 드세요?",
          "original_form": "영양제는 드세요?",
          "speaker_id": "2",
          "time": "20210824 02:23"
        },
        {
          "id": "MDRW2100011172.1.5",
          "form": "비타민b6랑 비타민c 비타민d 열심히 먹고있어요",
          "original_form": "비타민b6랑 비타민c 비타민d 열심히 먹고있어요",
          "speaker_id": "1",
          "time": "20210824 02:23"
        },
        {
          "id": "MDRW2100011172.1.6",
          "form": "저는 종합비타민, 마그네슘 먹어요",
          "original_form": "저는 종합비타민, 마그네슘 먹어요",
          "speaker_id": "2",
          "time": "20210824 02:24"
        },
        {
          "id": "MDRW2100011172.1.7",
          "form": "저는 마그네슘 너무 졸려서 못먹겠더라고요",
          "original_form": "저는 마그네슘 너무 졸려서 못먹겠더라고요",
          "speaker_id": "1",
          "time": "20210824 02:24"
        },
        {
          "id": "MDRW2100011172.1.8",
          "form": "운동은 하시나요?",
          "original_form": "운동은 하시나요?",
          "speaker_id": "2",
          "time": "20210824 02:25"
        },
        {
          "id": "MDRW2100011172.1.9",
          "form": "해야하는데 못하고 있어요",
          "original_form": "해야하는데 못하고 있어요",
          "speaker_id": "1",
          "time": "20210824 02:25"
        },
        {
          "id": "MDRW2100011172.1.10",
          "form": "저는 못할때 자기전 스트레칭 해요",
          "original_form": "저는 못할때 자기전 스트레칭 해요",
          "speaker_id": "2",
          "time": "20210824 02:26"
        },
        {
          "id": "MDRW2100011172.1.11",
          "form": "저도 스트레칭 해봐야겠어요",
          "original_form": "저도 스트레칭 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 02:26"
        },
        {
          "id": "MDRW2100011172.1.12",
          "form": "크게 아파보신적 있으세요?",
          "original_form": "크게 아파보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210824 02:26"
        },
        {
          "id": "MDRW2100011172.1.13",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210824 02:26"
        },
        {
          "id": "MDRW2100011172.1.14",
          "form": "저는 독감 걸려봤는데 죽을 맛이었어요",
          "original_form": "저는 독감 걸려봤는데 죽을 맛이었어요",
          "speaker_id": "1",
          "time": "20210824 02:27"
        },
        {
          "id": "MDRW2100011172.1.15",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210824 02:27"
        },
        {
          "id": "MDRW2100011172.1.16",
          "form": "모더나 맞았어요!",
          "original_form": "모더나 맞았어요!",
          "speaker_id": "1",
          "time": "20210824 02:28"
        },
        {
          "id": "MDRW2100011172.1.17",
          "form": "저는  화이자요",
          "original_form": "저는  화이자요",
