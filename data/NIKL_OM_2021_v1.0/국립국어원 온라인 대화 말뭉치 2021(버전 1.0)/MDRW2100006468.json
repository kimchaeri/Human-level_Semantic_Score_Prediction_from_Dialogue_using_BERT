      "utterance": [
        {
          "id": "MDRW2100006468.1.1",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006468.1.2",
          "form": "그 헤드윅??",
          "original_form": "그 헤드윅??",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006468.1.3",
          "form": "맞아 인기 많아 빨리 빠져",
          "original_form": "맞아 인기 많아 빨리 빠져",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006468.1.4",
          "form": "친구가 조승우 보겠다고 해달라해서",
          "original_form": "친구가 조승우 보겠다고 해달라해서",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006468.1.5",
          "form": "아 엄청나게 유명한거지",
          "original_form": "아 엄청나게 유명한거지",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006468.1.6",
          "form": "나는 이규형꺼 더 보고 싶기도 한데 아니다 조승우도 좋아 하여튼",
          "original_form": "나는 이규형꺼 더 보고 싶기도 한데 아니다 조승우도 좋아 하여튼",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006468.1.7",
          "form": "내가 보려고 한건 아니구",
          "original_form": "내가 보려고 한건 아니구",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006468.1.8",
          "form": "친구꺼 해주는데 오우 진짜 대박이었어",
          "original_form": "친구꺼 해주는데 오우 진짜 대박이었어",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006468.1.9",
          "form": "하필 자리도 적어가지고",
          "original_form": "하필 자리도 적어가지고",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006468.1.10",
          "form": "그냥 쓱싹 빠지잖아 ㅋㅋㅋ",
          "original_form": "그냥 쓱싹 빠지잖아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006468.1.11",
          "form": "출연진에 아이돌 껴있으면 더 힘들어",
          "original_form": "출연진에 아이돌 껴있으면 더 힘들어",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006468.1.12",
          "form": "웅웅 근데 아이돌은 없었을껄 아닌가",
          "original_form": "웅웅 근데 아이돌은 없었을껄 아닌가",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006468.1.13",
          "form": "친구 말로는 그냥 조승우 뮤지컬은 별따기래",
          "original_form": "친구 말로는 그냥 조승우 뮤지컬은 별따기래",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006468.1.14",
          "form": "모르겠다 헤드윅 나오는 출연진 조승우밖에 몰라",
          "original_form": "모르겠다 헤드윅 나오는 출연진 조승우밖에 몰라",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006468.1.15",
          "form": "ㅋㅋㅋㅋㅋ나도 잘 기억안나",
          "original_form": "ㅋㅋㅋㅋㅋ나도 잘 기억안나",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006468.1.16",
          "form": "그냥 콘서트보다 티켓딩 빡세다 이런 말이양",
          "original_form": "그냥 콘서트보다 티켓딩 빡세다 이런 말이양",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006468.1.17",
          "form": "조승우 맨오브 라만차 나올 때",
          "original_form": "조승우 맨오브 라만차 나올 때",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006468.1.18",
          "form": "연습하는거 직관했었다",
          "original_form": "연습하는거 직관했었다",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006468.1.19",
          "form": "재밌었어 안 보는 척하면서 열심히 봤지",
          "original_form": "재밌었어 안 보는 척하면서 열심히 봤지",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006468.1.20",
          "form": "연습?? 우와 대박이네",
          "original_form": "연습?? 우와 대박이네",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006468.1.21",
          "form": "어떻게 본거야 신기",
          "original_form": "어떻게 본거야 신기",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006468.1.22",
          "form": "응응 충무아트센터에서 알바했거든 ㅋㅋㅋㅋ",
          "original_form": "응응 충무아트센터에서 알바했거든 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006468.1.23",
          "form": "뮤지컬 하는 곳에서 알바해서 훔쳐봤어",
          "original_form": "뮤지컬 하는 곳에서 알바해서 훔쳐봤어",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006468.1.24",
          "form": "아 오 좋은 알바햇네",
          "original_form": "아 오 좋은 알바햇네",
          "speaker_id": "1",
