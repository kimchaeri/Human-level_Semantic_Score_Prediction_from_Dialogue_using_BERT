      "utterance": [
        {
          "id": "MDRW2100006175.1.1",
          "form": "누나 나 오늘 한잔때림",
          "original_form": "누나 나 오늘 한잔때림",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100006175.1.2",
          "form": "소주 2병에 장어 고고함 ㅋㅋ",
          "original_form": "소주 2병에 장어 고고함 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100006175.1.3",
          "form": "헉! 무슨 장어 드셨어요!!!",
          "original_form": "헉! 무슨 장어 드셨어요!!!",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006175.1.4",
          "form": "대박! 완전 대박!",
          "original_form": "대박! 완전 대박!",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006175.1.5",
          "form": "바다장어임돠",
          "original_form": "바다장어임돠",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006175.1.6",
          "form": "민물아니구",
          "original_form": "민물아니구",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006175.1.7",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006175.1.8",
          "form": "붕장어ㅜㅇㅜ",
          "original_form": "붕장어ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006175.1.9",
          "form": "아나고?",
          "original_form": "아나고?",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006175.1.10",
          "form": "술 챕니당",
          "original_form": "술 챕니당",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006175.1.11",
          "form": "아나고가 어디가 어때서욧!",
          "original_form": "아나고가 어디가 어때서욧!",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006175.1.12",
          "form": "꼬리가 파닥파닥",
          "original_form": "꼬리가 파닥파닥",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006175.1.13",
          "form": "넘 좋네용",
          "original_form": "넘 좋네용",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006175.1.14",
          "form": "솔직히 둘 다 맛난데 왜 글케 가격차이가 나는지 이해불가",
          "original_form": "솔직히 둘 다 맛난데 왜 글케 가격차이가 나는지 이해불가",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006175.1.15",
          "form": "와 장어 진짜 좋아하는데",
          "original_form": "와 장어 진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006175.1.16",
          "form": "생강채 얹어서 꿀꺽ㅜㅜㅜ",
          "original_form": "생강채 얹어서 꿀꺽ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006175.1.17",
          "form": "저 빼고 드시니 맛나던가요",
          "original_form": "저 빼고 드시니 맛나던가요",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006175.1.18",
          "form": "맛업어요",
          "original_form": "맛업어요",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006175.1.19",
          "form": "흥이안남",
          "original_form": "흥이안남",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006175.1.20",
          "form": "ㅎㅎ 이 기만자!",
          "original_form": "ㅎㅎ 이 기만자!",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006175.1.21",
          "form": "배민 검색하니 장어 파는데 나오네요",
          "original_form": "배민 검색하니 장어 파는데 나오네요",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100006175.1.22",
          "form": "저도 장어 먹을 거임!",
          "original_form": "저도 장어 먹을 거임!",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100006175.1.23",
          "form": "누나",
          "original_form": "누나",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100006175.1.24",
          "form": "소주는?",
          "original_form": "소주는?",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100006175.1.25",
          "form": "소주는 집에 있쥬ㅎ",
          "original_form": "소주는 집에 있쥬ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100006175.1.26",
          "form": "오늘 하루 어땠서요?",
          "original_form": "오늘 하루 어땠서요?",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100006175.1.27",
          "form": "소주한잔 할만해요?",
          "original_form": "소주한잔 할만해요?",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100006175.1.28",
          "form": "갑자기 엄청 술 당기네요ㅎ",
          "original_form": "갑자기 엄청 술 당기네요ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006175.1.29",
          "form": "소주 맛 싫어하는데",
          "original_form": "소주 맛 싫어하는데",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006175.1.30",
          "form": "노비의 원동력은 술이네유ㅎ",
          "original_form": "노비의 원동력은 술이네유ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006175.1.31",
          "form": "누나 오늘 안힘들엇서요?",
          "original_form": "누나 오늘 안힘들엇서요?",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100006175.1.32",
          "form": "괜괜?",
          "original_form": "괜괜?",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100006175.1.33",
          "form": "장어시킴?",
          "original_form": "장어시킴?",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100006175.1.34",
          "form": "아직 집 도착 전이라  가게 고른중이요",
          "original_form": "아직 집 도착 전이라  가게 고른중이요",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100006175.1.35",
          "form": "노비인생은 언제나 시궁창이죠ㅎ",
          "original_form": "노비인생은 언제나 시궁창이죠ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100006175.1.36",
          "form": "언젠가 슈퍼볼 당첨되고 만다ㅂㄷㅂㄷ",
          "original_form": "언젠가 슈퍼볼 당첨되고 만다ㅂㄷㅂㄷ",
          "speaker_id": "1",
          "time": "20210907 21:39"
        }
      ]
    }
