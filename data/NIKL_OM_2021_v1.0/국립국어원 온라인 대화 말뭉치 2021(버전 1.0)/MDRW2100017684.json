      "utterance": [
        {
          "id": "MDRW2100017684.1.1",
          "form": "공연 보는거 좋아하세요?",
          "original_form": "공연 보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017684.1.2",
          "form": "네 좋아해요! 근데 많이는 못 봤어요ㅠㅠ",
          "original_form": "네 좋아해요! 근데 많이는 못 봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017684.1.3",
          "form": "어떤 공연 좋아하세요? )",
          "original_form": "어떤 공연 좋아하세요? )",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100017684.1.4",
          "form": "저는 생각보다 연극 보는게 재밌었어요!",
          "original_form": "저는 생각보다 연극 보는게 재밌었어요!",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100017684.1.5",
          "form": "뮤지컬도 좋은데 연극이 더 몰입하게 돼요",
          "original_form": "뮤지컬도 좋은데 연극이 더 몰입하게 돼요",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100017684.1.6",
          "form": "저는 연극은 한번밖에 못봤네요 ㅠㅠ",
          "original_form": "저는 연극은 한번밖에 못봤네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100017684.1.7",
          "form": "오 좀 재밌었던 연극 있으신가요 ?",
          "original_form": "오 좀 재밌었던 연극 있으신가요 ?",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100017684.1.8",
          "form": "진짜 옛날에 봤지만ㅋㅋㅋ 라이어라고 너무 재밌게 봤어요!",
          "original_form": "진짜 옛날에 봤지만ㅋㅋㅋ 라이어라고 너무 재밌게 봤어요!",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100017684.1.9",
          "form": "오 헐 검색해봤는데",
          "original_form": "오 헐 검색해봤는데",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100017684.1.10",
          "form": "거의 한 20년동안 했네요 !",
          "original_form": "거의 한 20년동안 했네요 !",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100017684.1.11",
          "form": "와 그렇게 오래한 줄은 몰랐어요",
          "original_form": "와 그렇게 오래한 줄은 몰랐어요",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100017684.1.12",
          "form": "인기 많았나봐요!!ㅋㅋㅋㅋ",
          "original_form": "인기 많았나봐요!!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:59"
        },
        {
          "id": "MDRW2100017684.1.13",
          "form": "저도 이렇게 오래한 연극을 첨 봤어요 ㅋㅋ !",
          "original_form": "저도 이렇게 오래한 연극을 첨 봤어요 ㅋㅋ !",
          "speaker_id": "1",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100017684.1.14",
          "form": "보통 혜화동 가서 보시나요?",
          "original_form": "보통 혜화동 가서 보시나요?",
          "speaker_id": "1",
          "time": "20210827 01:05"
        },
        {
          "id": "MDRW2100017684.1.15",
          "form": "네 서울 와서 연극은 혜화동에서만 봤어요!",
          "original_form": "네 서울 와서 연극은 혜화동에서만 봤어요!",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100017684.1.16",
          "form": "영화는 자주 보세요?",
          "original_form": "영화는 자주 보세요?",
          "speaker_id": "2",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100017684.1.17",
          "form": "영화관에서 보는거 좋아하는데 ㅠㅠ 안간지가 오래됐네요",
          "original_form": "영화관에서 보는거 좋아하는데 ㅠㅠ 안간지가 오래됐네요",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100017684.1.18",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100017684.1.19",
          "form": "저도 좋아해요! 친구랑 볼 만한 거 나오면 바로 약속 잡았는데ㅠㅠ 요즘 못 보네요ㅠ",
          "original_form": "저도 좋아해요! 친구랑 볼 만한 거 나오면 바로 약속 잡았는데ㅠㅠ 요즘 못 보네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100017684.1.20",
          "form": "맞죠 ㅠㅠ 또 영화관 가격도 계속 올라서",
          "original_form": "맞죠 ㅠㅠ 또 영화관 가격도 계속 올라서",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100017684.1.21",
          "form": "차라리 폰으로 보는게 나은거같기도 해요 ㅠ",
          "original_form": "차라리 폰으로 보는게 나은거같기도 해요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100017684.1.22",
          "form": "아 맞아요 집에서도 볼 수 있으니까요!",
          "original_form": "아 맞아요 집에서도 볼 수 있으니까요!",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100017684.1.23",
          "form": "근데 이제 기다리던 영화 나오면 못 참고 바로 예매하죠ㅋㅋㅋ",
          "original_form": "근데 이제 기다리던 영화 나오면 못 참고 바로 예매하죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:05"
        }
      ]
    }
