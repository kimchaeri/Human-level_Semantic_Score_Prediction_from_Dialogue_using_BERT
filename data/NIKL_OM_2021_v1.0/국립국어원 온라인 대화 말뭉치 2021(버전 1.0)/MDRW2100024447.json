      "utterance": [
        {
          "id": "MDRW2100024447.1.1",
          "form": "종교 있으신가용?",
          "original_form": "종교 있으신가용?",
          "speaker_id": "2",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100024447.1.2",
          "form": "저는 무교에요!",
          "original_form": "저는 무교에요!",
          "speaker_id": "1",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100024447.1.3",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210831 09:58"
        },
        {
          "id": "MDRW2100024447.1.4",
          "form": "저는 기독교에요!",
          "original_form": "저는 기독교에요!",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100024447.1.5",
          "form": "모태신앙인지라 계속 되고잇네요 핳",
          "original_form": "모태신앙인지라 계속 되고잇네요 핳",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100024447.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100024447.1.7",
          "form": "그럼 술안드세요?!",
          "original_form": "그럼 술안드세요?!",
          "speaker_id": "1",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100024447.1.8",
          "form": "아뇨ㅋㅋㅋㄲㄱㅋ",
          "original_form": "아뇨ㅋㅋㅋㄲㄱㅋ",
          "speaker_id": "2",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100024447.1.9",
          "form": "자주 마시는편은 아니지만 거절은 안합니닷",
          "original_form": "자주 마시는편은 아니지만 거절은 안합니닷",
          "speaker_id": "2",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100024447.1.10",
          "form": "항상묻는질문이에요ㅋㅋㅋ",
          "original_form": "항상묻는질문이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:02"
        },
        {
          "id": "MDRW2100024447.1.11",
          "form": "나이 있으신 분들은 안드시는 분들이 꽤 계시긴 하더라구요",
          "original_form": "나이 있으신 분들은 안드시는 분들이 꽤 계시긴 하더라구요",
          "speaker_id": "2",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100024447.1.12",
          "form": "교회는 주에 몇번나가세요?!",
          "original_form": "교회는 주에 몇번나가세요?!",
          "speaker_id": "1",
          "time": "20210831 10:06"
        },
        {
          "id": "MDRW2100024447.1.13",
          "form": "주말만 가고있어요!",
          "original_form": "주말만 가고있어요!",
          "speaker_id": "2",
          "time": "20210831 10:06"
        },
        {
          "id": "MDRW2100024447.1.14",
          "form": "요즘엔 유튜브라 두번듣고 있구요ㅋㅋ",
          "original_form": "요즘엔 유튜브라 두번듣고 있구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100024447.1.15",
          "form": "오 주일이군요",
          "original_form": "오 주일이군요",
          "speaker_id": "1",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100024447.1.16",
          "form": "아 비대면",
          "original_form": "아 비대면",
          "speaker_id": "1",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100024447.1.17",
          "form": "하시는구나,,,",
          "original_form": "하시는구나,,,",
          "speaker_id": "1",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100024447.1.18",
