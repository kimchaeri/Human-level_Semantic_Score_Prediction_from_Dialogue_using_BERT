      "utterance": [
        {
          "id": "MDRW2100044104.1.1",
          "form": "안녕하세요 오늘 날씨 어땠어요?",
          "original_form": "안녕하세요 오늘 날씨 어땠어요?",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044104.1.2",
          "form": "여기는 좀 흐렸네요",
          "original_form": "여기는 좀 흐렸네요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044104.1.3",
          "form": "name1님쪽은 어떠셨나요?",
          "original_form": "&name1&님쪽은 어떠셨나요?",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044104.1.4",
          "form": "요즘 날씨가 제멋대로죠?",
          "original_form": "요즘 날씨가 제멋대로죠?",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044104.1.5",
          "form": "여긴 흐리다가 맑다가 비오다가 변덕스러워요",
          "original_form": "여긴 흐리다가 맑다가 비오다가 변덕스러워요",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044104.1.6",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044104.1.7",
          "form": "진짜 가을이 오는 것 같아요",
          "original_form": "진짜 가을이 오는 것 같아요",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044104.1.8",
          "form": "가을 장마래요 새벽에 춥죠?",
          "original_form": "가을 장마래요 새벽에 춥죠?",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044104.1.9",
          "form": "맞아요 이제 진짜 춥더라공ㅅ",
          "original_form": "맞아요 이제 진짜 춥더라공ㅅ",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044104.1.10",
          "form": "고요",
          "original_form": "고요",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044104.1.11",
          "form": "옷 잘 챙겨입으시나요?",
          "original_form": "옷 잘 챙겨입으시나요?",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044104.1.12",
          "form": "아... 저는 열이 많아서 아직 긴팔 입진 않고 가볍게 입어요 님은요?",
          "original_form": "아... 저는 열이 많아서 아직 긴팔 입진 않고 가볍게 입어요 님은요?",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044104.1.13",
          "form": "저도 아직까지는 가볍게 입네요",
          "original_form": "저도 아직까지는 가볍게 입네요",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044104.1.14",
          "form": "어떤 계절 좋아하시나요?",
          "original_form": "어떤 계절 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044104.1.15",
          "form": "저는 가을 좋아해요 name2님은요?",
          "original_form": "저는 가을 좋아해요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044104.1.16",
          "form": "저는 겨울을 좋아해요",
          "original_form": "저는 겨울을 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044104.1.17",
          "form": "왜 가을을 좋아하시나요?",
          "original_form": "왜 가을을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044104.1.18",
          "form": "날씨가 선선하니깐요 딱 겨울 초입까지만요 님은 왜 겨울을 좋아하나요?",
          "original_form": "날씨가 선선하니깐요 딱 겨울 초입까지만요 님은 왜 겨울을 좋아하나요?",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044104.1.19",
          "form": "사실 안덥고 미세먼지 적어서요",
          "original_form": "사실 안덥고 미세먼지 적어서요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044104.1.20",
          "form": "가을 날씨를 좋아하시는군요",
          "original_form": "가을 날씨를 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044104.1.21",
          "form": "네 근데 가을은 시기가 짧아요",
          "original_form": "네 근데 가을은 시기가 짧아요",
          "speaker_id": "1",
          "time": "20210907 21:45"
        }
