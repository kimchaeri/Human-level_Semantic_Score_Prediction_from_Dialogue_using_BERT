      "utterance": [
        {
          "id": "MDRW2100015465.1.1",
          "form": "하트시그널 보는 느낌입니다",
          "original_form": "하트시그널 보는 느낌입니다",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015465.1.2",
          "form": "연애프로는 잘 안보시나봐요!?!",
          "original_form": "연애프로는 잘 안보시나봐요!?!",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015465.1.3",
          "form": "맞하는거죠 ㄹㅇ..",
          "original_form": "맞하는거죠 ㄹㅇ..",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100015465.1.4",
          "form": "크리스마스 때",
          "original_form": "크리스마스 때",
          "speaker_id": "2",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100015465.1.5",
          "form": "휴...",
          "original_form": "휴...",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100015465.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100015465.1.7",
          "form": "교회 가봤다했나",
          "original_form": "교회 가봤다했나",
          "speaker_id": "2",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100015465.1.8",
          "form": "저 실로폰 친거 말해줬었나",
          "original_form": "저 실로폰 친거 말해줬었나",
          "speaker_id": "2",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100015465.1.9",
          "form": "네 할머니한테 끌려가서",
          "original_form": "네 할머니한테 끌려가서",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100015465.1.10",
          "form": "ㅇㄴ 저 기억력 좋죠",
          "original_form": "ㅇㄴ 저 기억력 좋죠",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100015465.1.11",
          "form": "아 말해드렸구나",
          "original_form": "아 말해드렸구나",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100015465.1.12",
          "form": "저 어릴 때 할머니집 가면 주말마다 교회를 끌려갔거든요",
          "original_form": "저 어릴 때 할머니집 가면 주말마다 교회를 끌려갔거든요",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100015465.1.13",
          "form": "함 더 해바바요(?)",
          "original_form": "함 더 해바바요(?)",
          "speaker_id": "1",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100015465.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100015465.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100015465.1.16",
          "form": "내 의지와는 상관없이",
          "original_form": "내 의지와는 상관없이",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100015465.1.17",
          "form": "우리누나 안데려가고 나만데리고감",
          "original_form": "우리누나 안데려가고 나만데리고감",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100015465.1.18",
          "form": "싫다 그래도 나만 나만 데리고가는데",
          "original_form": "싫다 그래도 나만 나만 데리고가는데",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100015465.1.19",
          "form": "가면 이제 자랑을 하셨죠",
          "original_form": "가면 이제 자랑을 하셨죠",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100015465.1.20",
          "form": "얘가 내 손주다",
          "original_form": "얘가 내 손주다",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100015465.1.21",
          "form": "아들램이 이쁘셨나보다",
          "original_form": "아들램이 이쁘셨나보다",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100015465.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100015465.1.23",
          "form": "교화사람들도 이뻐하셨을듯",
          "original_form": "교화사람들도 이뻐하셨을듯",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100015465.1.24",
          "form": "그러면서 이제 온갖 손이 들어와서",
          "original_form": "그러면서 이제 온갖 손이 들어와서",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100015465.1.25",
          "form": "내 볼을 조물딱 거려요",
          "original_form": "내 볼을 조물딱 거려요",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100015465.1.26",
          "form": "그러고 이제 기억도 안나는 찬송가를 막 부릅니다",
          "original_form": "그러고 이제 기억도 안나는 찬송가를 막 부릅니다",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100015465.1.27",
          "form": "자매님의 손주? 나라도(?)",
          "original_form": "자매님의 손주? 나라도(?)",
          "speaker_id": "1",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100015465.1.28",
          "form": "박수도 막치고",
          "original_form": "박수도 막치고",
          "speaker_id": "2",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100015465.1.29",
          "form": "그러다 이제 끝나면 또 기가 막히게 점심시간이야",
          "original_form": "그러다 이제 끝나면 또 기가 막히게 점심시간이야",
          "speaker_id": "2",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100015465.1.30",
          "form": "맛난거 또 먹고 가야죠 교회에서",
          "original_form": "맛난거 또 먹고 가야죠 교회에서",
          "speaker_id": "1",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100015465.1.31",
          "form": "교회마다 다르겠지만 교회가면 점심 뭐 먹는지 알아요?",
          "original_form": "교회마다 다르겠지만 교회가면 점심 뭐 먹는지 알아요?",
          "speaker_id": "2",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100015465.1.32",
          "form": "뭐먹나요???",
          "original_form": "뭐먹나요???",
          "speaker_id": "1",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100015465.1.33",
          "form": "10번중 8번을 카레를먹습니다",
          "original_form": "10번중 8번을 카레를먹습니다",
          "speaker_id": "2",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100015465.1.34",
          "form": "갈 때마다 카레를 줘서 전 교회사람은 카레만 먹는 줄 알았어요",
          "original_form": "갈 때마다 카레를 줘서 전 교회사람은 카레만 먹는 줄 알았어요",
          "speaker_id": "2",
          "time": "20210826 12:53"
        }
      ]
