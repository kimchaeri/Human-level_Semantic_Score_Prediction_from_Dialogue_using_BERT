      "utterance": [
        {
          "id": "MDRW2100046114.1.1",
          "form": "귀여운 캐릭터도 많은데",
          "original_form": "귀여운 캐릭터도 많은데",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046114.1.2",
          "form": "이해가 안 가요",
          "original_form": "이해가 안 가요",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046114.1.3",
          "form": "고양이 아니에요?",
          "original_form": "고양이 아니에요?",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046114.1.4",
          "form": "헐..ㅡ",
          "original_form": "헐..ㅡ",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046114.1.5",
          "form": "아니에요?!",
          "original_form": "아니에요?!",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046114.1.6",
          "form": "헐 그럼 뭐지",
          "original_form": "헐 그럼 뭐지",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046114.1.7",
          "form": "저는잘몰라용",
          "original_form": "저는잘몰라용",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046114.1.8",
          "form": "먼지",
          "original_form": "먼지",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046114.1.9",
          "form": "알고싶지않이서안찾아봄 ㅋㅋㅋ",
          "original_form": "알고싶지않이서안찾아봄 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046114.1.10",
          "form": "검색해봤는데",
          "original_form": "검색해봤는데",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046114.1.11",
          "form": "고양이래요",
          "original_form": "고양이래요",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.12",
          "form": "맞네요 아니",
          "original_form": "맞네요 아니",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.13",
          "form": "근데 사진 뜬 거 보니까",
          "original_form": "근데 사진 뜬 거 보니까",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.14",
          "form": "무슨고양이가저래 ㅋㅋ",
          "original_form": "무슨고양이가저래 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.15",
          "form": "ㄹㅇ 뽕알같아서",
          "original_form": "ㄹㅇ 뽕알같아서",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.17",
          "form": "킹받아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "킹받아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.18",
          "form": "그것도 볼살도 너무 그렇고ㅜㅜ",
          "original_form": "그것도 볼살도 너무 그렇고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.19",
          "form": "저게어떻게고양이이죠...?",
          "original_form": "저게어떻게고양이이죠...?",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.20",
          "form": "수염이 근데 너무",
          "original_form": "수염이 근데 너무",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.21",
          "form": "돈벌레 다리같아요",
          "original_form": "돈벌레 다리같아요",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.22",
          "form": "마자여",
          "original_form": "마자여",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.23",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046114.1.24",
          "form": "으윽",
          "original_form": "으윽",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046114.1.25",
