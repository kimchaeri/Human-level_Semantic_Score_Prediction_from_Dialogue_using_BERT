      "utterance": [
        {
          "id": "MDRW2100042372.1.1",
          "form": "메인메뉴에 가까워",
          "original_form": "메인메뉴에 가까워",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042372.1.2",
          "form": "안 매운 음식이라구?",
          "original_form": "안 매운 음식이라구?",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042372.1.3",
          "form": "응 안매워",
          "original_form": "응 안매워",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042372.1.4",
          "form": "그 요리를 밥 없이 단독으로 먹기도 해?",
          "original_form": "그 요리를 밥 없이 단독으로 먹기도 해?",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042372.1.5",
          "form": "먹기도해",
          "original_form": "먹기도해",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042372.1.6",
          "form": "면 요리야?",
          "original_form": "면 요리야?",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042372.1.7",
          "form": "배달로도 많이 먹어",
          "original_form": "배달로도 많이 먹어",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042372.1.8",
          "form": "면요리는 아니야",
          "original_form": "면요리는 아니야",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042372.1.9",
          "form": "빵 요리야?",
          "original_form": "빵 요리야?",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042372.1.10",
          "form": "아니야 빵요리",
          "original_form": "아니야 빵요리",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042372.1.11",
          "form": "또띠아 들어가?",
          "original_form": "또띠아 들어가?",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042372.1.12",
          "form": "아니 탄수화물은 안들어가",
          "original_form": "아니 탄수화물은 안들어가",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042372.1.13",
          "form": "흐으으으으음 뭘까",
          "original_form": "흐으으으으음 뭘까",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042372.1.14",
          "form": "생선이 들어가?",
          "original_form": "생선이 들어가?",
          "speaker_id": "1",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042372.1.15",
          "form": "아니 생선아닌 육류들어가",
          "original_form": "아니 생선아닌 육류들어가",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042372.1.16",
          "form": "그걸 밥이랑 곁들여먹는 게 괴식이야?",
          "original_form": "그걸 밥이랑 곁들여먹는 게 괴식이야?",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042372.1.17",
          "form": "아니 전혀",
          "original_form": "아니 전혀",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042372.1.18",
          "form": "일반적이야",
          "original_form": "일반적이야",
          "speaker_id": "2",
          "time": "20210906 21:56"
        }
      ]
