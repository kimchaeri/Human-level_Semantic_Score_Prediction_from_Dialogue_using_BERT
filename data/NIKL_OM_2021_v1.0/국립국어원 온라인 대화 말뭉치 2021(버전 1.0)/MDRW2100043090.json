          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043090.1.5",
          "form": "언제까지해야하나",
          "original_form": "언제까지해야하나",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043090.1.6",
          "form": "운동은안해",
          "original_form": "운동은안해",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043090.1.7",
          "form": "이제는 목표도몰라",
          "original_form": "이제는 목표도몰라",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043090.1.8",
          "form": "평생..",
          "original_form": "평생..",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043090.1.9",
          "form": "그건 그냥 근손실아니냐",
          "original_form": "그건 그냥 근손실아니냐",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043090.1.10",
          "form": "나도..뭔목표..",
          "original_form": "나도..뭔목표..",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043090.1.11",
          "form": "라고하면서 저녁 안먹어서 근손실옴",
          "original_form": "라고하면서 저녁 안먹어서 근손실옴",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043090.1.12",
          "form": "그렇지...근데 내가 더 근육 많을듯...",
          "original_form": "그렇지...근데 내가 더 근육 많을듯...",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043090.1.13",
          "form": "목표도없이 그냥 하는 중",
          "original_form": "목표도없이 그냥 하는 중",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
