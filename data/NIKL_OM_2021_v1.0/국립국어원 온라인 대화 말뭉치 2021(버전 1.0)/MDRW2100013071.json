      "utterance": [
        {
          "id": "MDRW2100013071.1.1",
          "form": "저는 홍차!",
          "original_form": "저는 홍차!",
          "speaker_id": "2",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013071.1.2",
          "form": "아아vs뜨아?",
          "original_form": "아아vs뜨아?",
          "speaker_id": "1",
          "time": "20210825 12:13"
        },
        {
          "id": "MDRW2100013071.1.3",
          "form": "아아요!",
          "original_form": "아아요!",
          "speaker_id": "2",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100013071.1.4",
          "form": "차가운게 좋더라고요 ㅎㅎ",
          "original_form": "차가운게 좋더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100013071.1.5",
          "form": "삼겹살vs목살은요?",
          "original_form": "삼겹살vs목살은요?",
          "speaker_id": "1",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100013071.1.6",
          "form": "저는 삼겹살이요!",
          "original_form": "저는 삼겹살이요!",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100013071.1.7",
          "form": "이유는요?",
          "original_form": "이유는요?",
          "speaker_id": "1",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100013071.1.8",
          "form": "개인적으로 목살은 잘 안찾는데",
          "original_form": "개인적으로 목살은 잘 안찾는데",
          "speaker_id": "2",
          "time": "20210825 12:56"
        },
        {
          "id": "MDRW2100013071.1.9",
          "form": "삼겹살은 잘 찾게되더라고요",
          "original_form": "삼겹살은 잘 찾게되더라고요",
          "speaker_id": "2",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100013071.1.10",
          "form": "그리고 삼겹살 기름에 굽는 마늘과 김치또한 치트키인지라..",
          "original_form": "그리고 삼겹살 기름에 굽는 마늘과 김치또한 치트키인지라..",
          "speaker_id": "2",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100013071.1.11",
          "form": "ㅋㅋㅋ 미나리도 좋죠",
          "original_form": "ㅋㅋㅋ 미나리도 좋죠",
          "speaker_id": "1",
          "time": "20210825 13:01"
        },
        {
          "id": "MDRW2100013071.1.12",
          "form": "삼겸살vs항정살은요?",
          "original_form": "삼겸살vs항정살은요?",
          "speaker_id": "1",
          "time": "20210825 13:01"
        },
        {
          "id": "MDRW2100013071.1.13",
          "form": "그래도 삼겹살이죠!",
          "original_form": "그래도 삼겹살이죠!",
          "speaker_id": "2",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100013071.1.14",
          "form": "항정살보단 삼겹살을 더 자주먹거든요 ㅎㅎ",
          "original_form": "항정살보단 삼겹살을 더 자주먹거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100013071.1.15",
          "form": "ㅋㅋㅋ 흑돼지는",
          "original_form": "ㅋㅋㅋ 흑돼지는",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100013071.1.16",
          "form": "어떤가요?",
          "original_form": "어떤가요?",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100013071.1.17",
          "form": "흑돼지도 맛있죠!",
          "original_form": "흑돼지도 맛있죠!",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013071.1.18",
          "form": "제주도 가봤나요?",
          "original_form": "제주도 가봤나요?",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013071.1.19",
          "form": "2번 가봤어요!",
          "original_form": "2번 가봤어요!",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013071.1.20",
          "form": "흑돼지 먹었나요ㅋㅋㅋㅋ",
          "original_form": "흑돼지 먹었나요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013071.1.21",
          "form": "흑돼지도 먹었고",
          "original_form": "흑돼지도 먹었고",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013071.1.22",
          "form": "고기국수도 먹어봤어요!",
          "original_form": "고기국수도 먹어봤어요!",
          "speaker_id": "2",
          "time": "20210825 14:21"
        }
      ]
