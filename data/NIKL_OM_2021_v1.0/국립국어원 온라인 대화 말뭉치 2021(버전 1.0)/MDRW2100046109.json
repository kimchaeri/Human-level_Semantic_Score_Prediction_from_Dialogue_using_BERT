      "utterance": [
        {
          "id": "MDRW2100046109.1.1",
          "form": "그건 모르겠고",
          "original_form": "그건 모르겠고",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046109.1.2",
          "form": "건물이 낡았어요",
          "original_form": "건물이 낡았어요",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046109.1.3",
          "form": "엘리베이터도 없고",
          "original_form": "엘리베이터도 없고",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046109.1.4",
          "form": "전체적으로",
          "original_form": "전체적으로",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046109.1.5",
          "form": "ㅋㅋㅋㅋ무슨느낌인지",
          "original_form": "ㅋㅋㅋㅋ무슨느낌인지",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046109.1.6",
          "form": "알거같아여",
          "original_form": "알거같아여",
          "speaker_id": "1",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046109.1.7",
          "form": "님 학교는요?",
          "original_form": "님 학교는요?",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046109.1.8",
          "form": "저희과 건물은 엘베있었어요ㅋㅋㅋㅋ",
          "original_form": "저희과 건물은 엘베있었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046109.1.9",
          "form": "그리거 저 다닐때 공사해서 외관만 신축으로바꾸고",
          "original_form": "그리거 저 다닐때 공사해서 외관만 신축으로바꾸고",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046109.1.10",
          "form": "오 좋겠어요",
          "original_form": "오 좋겠어요",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046109.1.11",
          "form": "제가 실습할 건물은",
          "original_form": "제가 실습할 건물은",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046109.1.12",
          "form": "정말 별로래요...",
          "original_form": "정말 별로래요...",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046109.1.13",
          "form": "에이 그래도 엘베없는곳 많아요ㅋㅋㅋㅋㅋ",
          "original_form": "에이 그래도 엘베없는곳 많아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046109.1.14",
          "form": "강의실....",
          "original_form": "강의실....",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046109.1.15",
          "form": "5층인데....",
          "original_form": "5층인데....",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046109.1.16",
          "form": "아그건좀ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아그건좀ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046109.1.17",
          "form": "화이팅하세요ㅠ",
          "original_form": "화이팅하세요ㅠ",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046109.1.18",
          "form": "그나마 지금 비대면이라 다행이고ㅠ",
          "original_form": "그나마 지금 비대면이라 다행이고ㅠ",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046109.1.19",
          "form": "대면할 생각하니 끔찍하네요",
          "original_form": "대면할 생각하니 끔찍하네요",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046109.1.20",
          "form": "학교 장점은 없어요?ㅋㅋㅋㅋ",
          "original_form": "학교 장점은 없어요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046109.1.21",
          "form": "좋은거",
          "original_form": "좋은거",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046109.1.22",
          "form": "학교에 이디야 있는거?",
          "original_form": "학교에 이디야 있는거?",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046109.1.23",
          "form": "카페요",
          "original_form": "카페요",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046109.1.24",
          "form": "에이 그건 다있을걸요",
          "original_form": "에이 그건 다있을걸요",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046109.1.25",
          "form": "우리는 맘스터치랑 빵집도있었는뎈ㅋㅋㅋㅋ",
          "original_form": "우리는 맘스터치랑 빵집도있었는뎈ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046109.1.26",
          "form": "저희도",
          "original_form": "저희도",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046109.1.27",
          "form": "맘스터치는 있어요ㅋㅋㅋㅋ",
          "original_form": "맘스터치는 있어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:29"
        }
      ]
