      "utterance": [
        {
          "id": "MDRW2100005165.1.1",
          "form": "아 사료만 먹는건가요?",
          "original_form": "아 사료만 먹는건가요?",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005165.1.2",
          "form": "네 야채 사료요",
          "original_form": "네 야채 사료요",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005165.1.3",
          "form": "식성이 너무 달라요",
          "original_form": "식성이 너무 달라요",
          "speaker_id": "1",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005165.1.4",
          "form": "고양이 집은 타워인가요?",
          "original_form": "고양이 집은 타워인가요?",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005165.1.5",
          "form": "저 타워 우리 도마뱀 사주고 싶어요",
          "original_form": "저 타워 우리 도마뱀 사주고 싶어요",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005165.1.6",
          "form": "높은데 올하가서 잘놀것 같아서요",
          "original_form": "높은데 올하가서 잘놀것 같아서요",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005165.1.7",
          "form": "올라가서",
          "original_form": "올라가서",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005165.1.8",
          "form": "고양이 쿠션이있긴한데",
          "original_form": "고양이 쿠션이있긴한데",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005165.1.9",
          "form": "캣타워에서도 쉬구 여기저기 그냥 돌아다녀요 ㅋㅋ",
          "original_form": "캣타워에서도 쉬구 여기저기 그냥 돌아다녀요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005165.1.10",
          "form": "아 정말 활발한가봐요",
          "original_form": "아 정말 활발한가봐요",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005165.1.11",
          "form": "집이 따로없구 그냥 저랑 같이 침대에서 자요",
          "original_form": "집이 따로없구 그냥 저랑 같이 침대에서 자요",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005165.1.12",
          "form": "저희는 두마리 극과극요",
          "original_form": "저희는 두마리 극과극요",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005165.1.13",
          "form": "하난꼼짝도 안하고 하나는 막 돌아다녀요",
          "original_form": "하난꼼짝도 안하고 하나는 막 돌아다녀요",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005165.1.14",
          "form": "어머 포근 하겠어요",
          "original_form": "어머 포근 하겠어요",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005165.1.15",
          "form": "ㅋㅋㅋ 어쩜 그렇게다르죠",
          "original_form": "ㅋㅋㅋ 어쩜 그렇게다르죠",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005165.1.16",
          "form": "나이가 비슷한가요??",
          "original_form": "나이가 비슷한가요??",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005165.1.17",
          "form": "그러니까요 신기해요",
          "original_form": "그러니까요 신기해요",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005165.1.18",
          "form": "똑같아요",
          "original_form": "똑같아요",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005165.1.19",
          "form": "저희고양이도 얌전해요 거의 하루종일 자구",
          "original_form": "저희고양이도 얌전해요 거의 하루종일 자구",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005165.1.20",
          "form": "놀아줘도 시큰둥해요 ㅋㅋㅋ",
          "original_form": "놀아줘도 시큰둥해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005165.1.21",
          "form": "아 고양이는 그런것 같아요",
          "original_form": "아 고양이는 그런것 같아요",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005165.1.22",
          "form": "저희도요 ㅡㅡ",
          "original_form": "저희도요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005165.1.23",
          "form": "째려보고",
          "original_form": "째려보고",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005165.1.24",
          "form": "ㅋㅋㅋ 하루의 절반이상을 자더라구요 지금도 자고있어요",
          "original_form": "ㅋㅋㅋ 하루의 절반이상을 자더라구요 지금도 자고있어요",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005165.1.25",
          "form": "조용해서 좋기도해요 ㅋㅋㅋ",
          "original_form": "조용해서 좋기도해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005165.1.26",
          "form": "어머 저희 애들이랑 같아요 ㅋㅌ",
          "original_form": "어머 저희 애들이랑 같아요 ㅋㅌ",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005165.1.27",
          "form": "하루종일 자요",
          "original_form": "하루종일 자요",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005165.1.28",
          "form": "지금자요",
          "original_form": "지금자요",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005165.1.29",
          "form": "오늘은 일부러 먹이 안줬어요",
          "original_form": "오늘은 일부러 먹이 안줬어요",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005165.1.30",
          "form": "너무 안먹어서 낼 먹게 하려고요 ㅜㅜ",
          "original_form": "너무 안먹어서 낼 먹게 하려고요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005165.1.31",
          "form": "속상해요",
          "original_form": "속상해요",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005165.1.32",
          "form": "아이구 ㅜㅜ 정말 안먹는군요 하루정도는 안먹어도",
          "original_form": "아이구 ㅜㅜ 정말 안먹는군요 하루정도는 안먹어도",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005165.1.33",
          "form": "건강에 이상없나요??",
          "original_form": "건강에 이상없나요??",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005165.1.34",
          "form": "네 하루정도 안먹고 담날 좀 먹이게요",
          "original_form": "네 하루정도 안먹고 담날 좀 먹이게요",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005165.1.35",
          "form": "낼 어차피 비늘 벗겨져서 온욕해야해서 그러고 먹이게요",
          "original_form": "낼 어차피 비늘 벗겨져서 온욕해야해서 그러고 먹이게요",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005165.1.36",
          "form": "고양이 목욕할때 얌전해요?",
          "original_form": "고양이 목욕할때 얌전해요?",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005165.1.37",
          "form": "저희는 하나는 난리나고 하나는 세상 얌전해요 ㅎㅎ",
          "original_form": "저희는 하나는 난리나고 하나는 세상 얌전해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:07"
        }
      ]
