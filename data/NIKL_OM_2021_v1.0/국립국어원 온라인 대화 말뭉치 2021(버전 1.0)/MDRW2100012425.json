      "utterance": [
        {
          "id": "MDRW2100012425.1.1",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012425.1.2",
          "form": "운동 링피트 해요",
          "original_form": "운동 링피트 해요",
          "speaker_id": "1",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012425.1.3",
          "form": "백신맞으셨나요?",
          "original_form": "백신맞으셨나요?",
          "speaker_id": "1",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012425.1.4",
          "form": "오 재밌나요?",
          "original_form": "오 재밌나요?",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012425.1.5",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012425.1.6",
          "form": "맞으셨나요",
          "original_form": "맞으셨나요",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012425.1.7",
          "form": "네 재밌어요 좀더 쉽게 꾸준해져요",
          "original_form": "네 재밌어요 좀더 쉽게 꾸준해져요",
          "speaker_id": "1",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012425.1.8",
          "form": "저도 아직이요",
          "original_form": "저도 아직이요",
          "speaker_id": "1",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012425.1.9",
          "form": "예약은 하셨어요?",
          "original_form": "예약은 하셨어요?",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012425.1.10",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "2",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012425.1.11",
          "form": "해야하는데 불안하네요ㅋㅋㅋㅋ",
          "original_form": "해야하는데 불안하네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012425.1.12",
          "form": "아직안하셨구나 10부제 날이안오셨나요?",
          "original_form": "아직안하셨구나 10부제 날이안오셨나요?",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012425.1.13",
          "form": "그때 까먹고 안했어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그때 까먹고 안했어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012425.1.14",
          "form": "그러시구나ㅋㅋㅋㅋ급하신거아니면 뭐 괜찮겠죠!",
          "original_form": "그러시구나ㅋㅋㅋㅋ급하신거아니면 뭐 괜찮겠죠!",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012425.1.15",
          "form": "ㅋㅋㅋㅋㅋ이제 해야죠!",
          "original_form": "ㅋㅋㅋㅋㅋ이제 해야죠!",
          "speaker_id": "2",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100012425.1.16",
          "form": "제가 예약했을때 2차가 10월 중순쯤여여서",
          "original_form": "제가 예약했을때 2차가 10월 중순쯤여여서",
          "speaker_id": "1",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100012425.1.17",
          "form": "name2 2차는 10월 말쯤이 아닐까싶어요",
          "original_form": "&name2& 2차는 10월 말쯤이 아닐까싶어요",
          "speaker_id": "1",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100012425.1.18",
          "form": "또륵.....",
          "original_form": "또륵.....",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012425.1.19",
          "form": "그 전에 풀리면.. 운 좋기...",
          "original_form": "그 전에 풀리면.. 운 좋기...",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012425.1.20",
          "form": "잔여백신을 노려보세요!",
          "original_form": "잔여백신을 노려보세요!",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012425.1.21",
          "form": "요즘 은근 잘보인대요",
          "original_form": "요즘 은근 잘보인대요",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012425.1.22",
          "form": "그래야죠 ㅎ 늦지않게",
          "original_form": "그래야죠 ㅎ 늦지않게",
