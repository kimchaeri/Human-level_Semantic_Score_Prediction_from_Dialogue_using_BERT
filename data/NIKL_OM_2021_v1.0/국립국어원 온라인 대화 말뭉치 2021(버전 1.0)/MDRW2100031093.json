      "utterance": [
        {
          "id": "MDRW2100031093.1.1",
          "form": "좋아하는 음식 있어요?",
          "original_form": "좋아하는 음식 있어요?",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031093.1.2",
          "form": "저는 다 잘먹긴하는데",
          "original_form": "저는 다 잘먹긴하는데",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031093.1.3",
          "form": "일식이나 한식좋아해요!",
          "original_form": "일식이나 한식좋아해요!",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031093.1.4",
          "form": "밥을 좋아해서.. ㅋㅋㅋ",
          "original_form": "밥을 좋아해서.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031093.1.5",
          "form": "그중에 특별이 좋아하는 메뉴가 있다면 뭐에요?",
          "original_form": "그중에 특별이 좋아하는 메뉴가 있다면 뭐에요?",
          "speaker_id": "1",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100031093.1.6",
          "form": "음 초밥이나 회요!",
          "original_form": "음 초밥이나 회요!",
          "speaker_id": "2",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100031093.1.7",
          "form": "초밥이나 회는 싫어할 수가없죠!!",
          "original_form": "초밥이나 회는 싫어할 수가없죠!!",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031093.1.8",
          "form": "좋아하시는 음식있으세요?",
          "original_form": "좋아하시는 음식있으세요?",
          "speaker_id": "2",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100031093.1.9",
          "form": "저도 회 좋아해요",
          "original_form": "저도 회 좋아해요",
          "speaker_id": "1",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100031093.1.10",
          "form": "오옹 혹시 그럼 오늘 저녁은 뭘 드실 계획이신가요?",
          "original_form": "오옹 혹시 그럼 오늘 저녁은 뭘 드실 계획이신가요?",
          "speaker_id": "2",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100031093.1.11",
          "form": "음 지금 고민중이에요ㅋㅋㅋㅋ",
          "original_form": "음 지금 고민중이에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100031093.1.12",
          "form": "님은 뭐 드실 계획이세용?",
          "original_form": "님은 뭐 드실 계획이세용?",
          "speaker_id": "1",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100031093.1.13",
          "form": "저는 소고기굽고있어용ㅋㅋㅋㅋ",
          "original_form": "저는 소고기굽고있어용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031093.1.14",
          "form": "오!!!  맛있겠어요!!",
          "original_form": "오!!!  맛있겠어요!!",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031093.1.15",
          "form": "무슨 부위 드세요?ㅋㅋㅋ",
          "original_form": "무슨 부위 드세요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100031093.1.16",
          "form": "채끝살이요!",
          "original_form": "채끝살이요!",
          "speaker_id": "2",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031093.1.17",
          "form": "님도 아직 날 더운데 단백질보충 충분히하세용!",
          "original_form": "님도 아직 날 더운데 단백질보충 충분히하세용!",
          "speaker_id": "2",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100031093.1.18",
          "form": "와 저도 단백질 보충해야겠네요!",
          "original_form": "와 저도 단백질 보충해야겠네요!",
          "speaker_id": "1",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100031093.1.19",
          "form": "아직까진 종종 날이 더워서 그런지 입맛이 없더라고요ㅠ",
          "original_form": "아직까진 종종 날이 더워서 그런지 입맛이 없더라고요ㅠ",
