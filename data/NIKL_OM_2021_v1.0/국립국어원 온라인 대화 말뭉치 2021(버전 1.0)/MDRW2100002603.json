      "utterance": [
        {
          "id": "MDRW2100002603.1.1",
          "form": "동물키워요?",
          "original_form": "동물{emoji:●}키워요?",
          "speaker_id": "1",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002603.1.2",
          "form": "네! 고양이 키웁니다!",
          "original_form": "네! 고양이 키웁니다!",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002603.1.3",
          "form": "어렵지 않나요?",
          "original_form": "어렵지 않나요?",
          "speaker_id": "1",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002603.1.4",
          "form": "아기 하나 키우는 느낌이라고들 하던데",
          "original_form": "아기 하나 키우는 느낌이라고들 하던데",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002603.1.5",
          "form": "그런 느낌이 좀 드는거 같긴해요",
          "original_form": "그런 느낌이 좀 드는거 같긴해요",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002603.1.6",
          "form": "하지만 귀여운걸 보면 그 힘든게 다 녹더라고요 ㅎㅎ",
          "original_form": "하지만 귀여운걸 보면 그 힘든게 다 녹더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002603.1.7",
          "form": "이래서 동물들 다들 키우는거같아요",
          "original_form": "이래서 동물들 다들 키우는거같아요",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002603.1.8",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002603.1.9",
          "form": "개인적으로 고양이가 조금 더",
          "original_form": "개인적으로 고양이가 조금 더",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002603.1.10",
          "form": "키우기 수월해서",
          "original_form": "키우기 수월해서",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002603.1.11",
          "form": "나중에 기회되신다면",
          "original_form": "나중에 기회되신다면",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002603.1.12",
          "form": "고양이 키워보시는거 추천드려요!",
          "original_form": "고양이 키워보시는거 추천드려요!",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002603.1.13",
          "form": "털이 장난아니라거 하던데요",
          "original_form": "털이 장난아니라거 하던데요",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002603.1.14",
          "form": "아 맞아요! 털갈이..",
          "original_form": "아 맞아요! 털갈이..",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002603.1.15",
          "form": "털이 좀 많이 날리는게 단점이긴하죠",
          "original_form": "털이 좀 많이 날리는게 단점이긴하죠",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002603.1.16",
          "form": "친구집에 고양이 털이 우어",
          "original_form": "친구집에 고양이 털이 우어",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002603.1.17",
          "form": "그래서 털알레르기 있는 사람들은 절대로 키우면 안되죠 ㅠㅠ",
          "original_form": "그래서 털알레르기 있는 사람들은 절대로 키우면 안되죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002603.1.18",
          "form": "털이 음청 날려요!",
          "original_form": "털이 음청 날려요!",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002603.1.19",
          "form": "돌돌이 필수",
          "original_form": "돌돌이 필수",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002603.1.20",
          "form": "돌돌이 진짜 필수죠..",
          "original_form": "돌돌이 진짜 필수죠..",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002603.1.21",
          "form": "집사 필수템인지라 ㅠㅠ",
          "original_form": "집사 필수템인지라 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002603.1.22",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210820 14:04"
        }
