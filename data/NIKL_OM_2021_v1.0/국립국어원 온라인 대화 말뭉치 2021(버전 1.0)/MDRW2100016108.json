          "time": "20210826 09:48"
        },
        {
          "id": "MDRW2100016108.1.4",
          "form": "운전말 할수있구먼",
          "original_form": "운전말 할수있구먼",
          "speaker_id": "2",
          "time": "20210826 09:48"
        },
        {
          "id": "MDRW2100016108.1.5",
          "form": "우리 김기사 ㅋㅋㅋㅋ",
          "original_form": "우리 김기사 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100016108.1.6",
          "form": "김기사 잘하네",
          "original_form": "김기사 잘하네",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100016108.1.7",
          "form": "오늘 뭐 맛있는 거 먹노",
          "original_form": "오늘 뭐 맛있는 거 먹노",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100016108.1.8",
          "form": "나 오늘 점심은 햄버거",
          "original_form": "나 오늘 점심은 햄버거",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100016108.1.9",
          "form": "아직 결정 못했슈~",
          "original_form": "아직 결정 못했슈~",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016108.1.10",
          "form": "햄버거 맛나겠구만요 ㅋㅋㅋㅋ",
          "original_form": "햄버거 맛나겠구만요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016108.1.11",
          "form": "룸메이트랑 맛있는 거 먹어라",
          "original_form": "룸메이트랑 맛있는 거 먹어라",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100016108.1.12",
          "form": "난 신랑이랑 둘이 밥 먹은 지가 언젠지도 모르겠네",
          "original_form": "난 신랑이랑 둘이 밥 먹은 지가 언젠지도 모르겠네",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100016108.1.13",
          "form": "매일 육아로 치여 삶",
          "original_form": "매일 육아로 치여 삶",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100016108.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ룸메는 뭐 집에서만 봐도됨 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ룸메는 뭐 집에서만 봐도됨 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016108.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016108.1.16",
          "form": "집에서도 딱 육아시간 까지만",
          "original_form": "집에서도 딱 육아시간 까지만",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016108.1.17",
          "form": "육퇴후는 각자 자유",
          "original_form": "육퇴후는 각자 자유",
          "speaker_id": "2",
          "time": "20210826 11:09"
        },
        {
          "id": "MDRW2100016108.1.18",
          "form": "그게 싸움도 없고 좋아요",
          "original_form": "그게 싸움도 없고 좋아요",
          "speaker_id": "1",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100016108.1.19",
          "form": "오래 함께 있으면 꼭 싸웁니다",
          "original_form": "오래 함께 있으면 꼭 싸웁니다",
          "speaker_id": "1",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100016108.1.20",
          "form": "ㅋㄱㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ 그래 편하게 살아야지",
          "original_form": "ㅋㄱㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ 그래 편하게 살아야지",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016108.1.21",
          "form": "밖으로 나갔나??",
          "original_form": "밖으로 나갔나??",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016108.1.22",
          "form": "맞아요 언니 이제 뿌 염 다했어 용",
          "original_form": "맞아요 언니 이제 뿌 염 다했어 용",
          "speaker_id": "1",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016108.1.23",
          "form": "첫째 오기 전에 해야 할 일이 너무 많네요",
          "original_form": "첫째 오기 전에 해야 할 일이 너무 많네요",
          "speaker_id": "1",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016108.1.24",
          "form": "주어진 시간이 얼마 없어요",
          "original_form": "주어진 시간이 얼마 없어요",
          "speaker_id": "1",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016108.1.25",
          "form": "잘했네 뿌염",
          "original_form": "잘했네 뿌염",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016108.1.26",
          "form": "미용실 간지가 언젠지 모르겠네",
          "original_form": "미용실 간지가 언젠지 모르겠네",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016108.1.27",
          "form": "어제 원 파마 했는데",
          "original_form": "어제 원 파마 했는데",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016108.1.28",
          "form": "신랑이 데려갔는데 너무 뽀글하게 해 온 거야",
          "original_form": "신랑이 데려갔는데 너무 뽀글하게 해 온 거야",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016108.1.29",
          "form": "백종원 인줄",
          "original_form": "백종원 인줄",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016108.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아욱겨~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아욱겨~",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016108.1.31",
          "form": "일부러 절대 안풀리게~",
          "original_form": "일부러 절대 안풀리게~",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016108.1.32",
          "form": "어 ㅋㅋㅋㅋㅋㅋ 원 계속 거울보면서",
          "original_form": "어 ㅋㅋㅋㅋㅋㅋ 원 계속 거울보면서",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016108.1.33",
          "form": "나름 아빠의 큰그림 ㅋㅋㅋㅋ",
          "original_form": "나름 아빠의 큰그림 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016108.1.34",
          "form": "싫다고 ㅋㅋㅋㅋㅋ",
          "original_form": "싫다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016108.1.35",
          "form": "ㅋㅋㅋㅋㅋ이제 그런거 알 나인데~",
          "original_form": "ㅋㅋㅋㅋㅋ이제 그런거 알 나인데~",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016108.1.36",
          "form": "며칠 지남 좀 풀릴껀데~",
          "original_form": "며칠 지남 좀 풀릴껀데~",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016108.1.37",
          "form": "근데 더 웃긴거 먼줄 아나",
          "original_form": "근데 더 웃긴거 먼줄 아나",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016108.1.38",
          "form": "하필 오늘 개학이야 ㅋㅋㅋㅋ",
          "original_form": "하필 오늘 개학이야 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016108.1.39",
          "form": "어제 name3이랑 name4랑 지나가면서",
          "original_form": "어제 &name3&이랑 &name4&랑 지나가면서",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016108.1.40",
          "form": "name5 보고 머리 왜 저러냐고 비웃음",
          "original_form": "&name5& 보고 머리 왜 저러냐고 비웃음",
