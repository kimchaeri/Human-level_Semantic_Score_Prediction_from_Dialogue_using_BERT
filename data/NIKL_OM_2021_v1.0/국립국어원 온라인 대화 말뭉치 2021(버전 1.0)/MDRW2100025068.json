      "utterance": [
        {
          "id": "MDRW2100025068.1.1",
          "form": "요즘 여행이 너무 가고싶다",
          "original_form": "요즘 여행이 너무 가고싶다",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025068.1.2",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025068.1.3",
          "form": "코로나 끝나면 어디 가고싶어?",
          "original_form": "코로나 끝나면 어디 가고싶어?",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025068.1.4",
          "form": "나는 일본. 일본어 공부하고있으니까",
          "original_form": "나는 일본. 일본어 공부하고있으니까",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025068.1.5",
          "form": "아 2년 전에 도쿄 가려다 취소한거 너무 아깝네",
          "original_form": "아 2년 전에 도쿄 가려다 취소한거 너무 아깝네",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025068.1.6",
          "form": "진짜 그때 갔어야했는데. 그땐 금방 다시 갈 수 있을 줄 알았지",
          "original_form": "진짜 그때 갔어야했는데. 그땐 금방 다시 갈 수 있을 줄 알았지",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025068.1.7",
          "form": "진짜 갈 수 있을 때 가는게 좋은 것 같아",
          "original_form": "진짜 갈 수 있을 때 가는게 좋은 것 같아",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025068.1.8",
          "form": "맞아 나중에는 어떻게 될지 모르잖아",
          "original_form": "맞아 나중에는 어떻게 될지 모르잖아",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100025068.1.9",
          "form": "젊었을 때 여행 많이 다닐걸",
          "original_form": "젊었을 때 여행 많이 다닐걸",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025068.1.10",
          "form": "백신 맞았으니까 곧 갈 수 있지 않을까?",
          "original_form": "백신 맞았으니까 곧 갈 수 있지 않을까?",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025068.1.11",
          "form": "그럼 좋겠는데 변이 때문에..",
          "original_form": "그럼 좋겠는데 변이 때문에..",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025068.1.12",
          "form": "아... 델타변이 무섭더라",
          "original_form": "아... 델타변이 무섭더라",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025068.1.13",
          "form": "네 친구 일본 살잖아. 거기는 지금 어떻대?",
          "original_form": "네 친구 일본 살잖아. 거기는 지금 어떻대?",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025068.1.14",
          "form": "우리나라보다 심각한 것 같아",
          "original_form": "우리나라보다 심각한 것 같아",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100025068.1.15",
          "form": "그런데 그 친구가 나중에 오면 관광시켜준다고 했어",
          "original_form": "그런데 그 친구가 나중에 오면 관광시켜준다고 했어",
          "speaker_id": "1",
          "time": "20210831 22:57"
        }
      ]
