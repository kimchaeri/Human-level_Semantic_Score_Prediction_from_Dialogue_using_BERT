      "utterance": [
        {
          "id": "MDRW2100014572.1.1",
          "form": "오늘 저녁 뭐드셨어요??",
          "original_form": "오늘 저녁 뭐드셨어요??",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014572.1.2",
          "form": "오늘 치킨 먹었습니다!",
          "original_form": "오늘 치킨 먹었습니다!",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100014572.1.3",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100014572.1.4",
          "form": "헐 저도 치킨이요 ㅋㅋ",
          "original_form": "헐 저도 치킨이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100014572.1.5",
          "form": "무슨브랜드요?",
          "original_form": "무슨브랜드요?",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100014572.1.6",
          "form": "bhc뿌링클 먹었어요!",
          "original_form": "bhc뿌링클 먹었어요!",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100014572.1.7",
          "form": "어디 치킨드셨나요?",
          "original_form": "어디 치킨드셨나요?",
          "speaker_id": "2",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100014572.1.8",
          "form": "저는 치킨매니아요 ㅋㅋ 맛있더라고요",
          "original_form": "저는 치킨매니아요 ㅋㅋ 맛있더라고요",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100014572.1.9",
          "form": "오 치킨매니아 무슨 치킨 드셨어요?",
          "original_form": "오 치킨매니아 무슨 치킨 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100014572.1.10",
          "form": "저는 그냥 기본 후라이드양념반반이요",
          "original_form": "저는 그냥 기본 후라이드양념반반이요",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100014572.1.11",
          "form": "오 나중에 한번 먹어봐야겠어요",
          "original_form": "오 나중에 한번 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100014572.1.12",
          "form": "뿌링클은 드셔보신적 있으신가요?",
          "original_form": "뿌링클은 드셔보신적 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100014572.1.13",
          "form": "네 진짜맛있고",
          "original_form": "네 진짜맛있고",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100014572.1.14",
          "form": "달아요",
          "original_form": "달아요",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100014572.1.15",
          "form": "최애에요",
          "original_form": "최애에요",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100014572.1.16",
          "form": "그쵸ㅠㅠㅠㅠ",
          "original_form": "그쵸ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100014572.1.17",
          "form": "짭짤하니 그 가루가 너무 맛있어요ㅠㅠ",
          "original_form": "짭짤하니 그 가루가 너무 맛있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 00:11"
        },
        {
          "id": "MDRW2100014572.1.18",
          "form": "치즈볼이랑 같이 먹으면 천국이더라구요...",
          "original_form": "치즈볼이랑 같이 먹으면 천국이더라구요...",
          "speaker_id": "2",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100014572.1.19",
          "form": "맞아요 카페는 어디 좋아하세요?",
          "original_form": "맞아요 카페는 어디 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100014572.1.20",
          "form": "저는 스벅이나 동네 개인카페 자주 가요!",
          "original_form": "저는 스벅이나 동네 개인카페 자주 가요!",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100014572.1.21",
          "form": "어느 카페 좋아하시나요?",
          "original_form": "어느 카페 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100014572.1.22",
          "form": "저도 스벅 자주가요",
          "original_form": "저도 스벅 자주가요",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100014572.1.23",
          "form": "디카페인있어서 좋더라구요",
          "original_form": "디카페인있어서 좋더라구요",
