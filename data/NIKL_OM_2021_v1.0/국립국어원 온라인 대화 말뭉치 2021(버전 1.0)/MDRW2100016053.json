        {
          "id": "MDRW2100016053.1.1",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016053.1.2",
          "form": "아직 안 먹었어요",
          "original_form": "아직 안 먹었어요",
          "speaker_id": "2",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016053.1.3",
          "form": "아침 잘 안드시나",
          "original_form": "아침 잘 안드시나",
          "speaker_id": "1",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016053.1.4",
          "form": "아닌가..",
          "original_form": "아닌가..",
          "speaker_id": "1",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100016053.1.5",
          "form": "잘 안 먹긴 해요",
          "original_form": "잘 안 먹긴 해요",
          "speaker_id": "2",
          "time": "20210826 08:52"
        },
        {
          "id": "MDRW2100016053.1.6",
          "form": "아무래도 귀찮져",
          "original_form": "아무래도 귀찮져",
          "speaker_id": "1",
          "time": "20210826 08:52"
        },
        {
          "id": "MDRW2100016053.1.7",
          "form": "내 맞아요 식사 하셨나요?",
          "original_form": "내 맞아요 식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016053.1.8",
          "form": "저도 아직 전이에요",
          "original_form": "저도 아직 전이에요",
          "speaker_id": "1",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016053.1.9",
          "form": "왜 안 드셨어요?",
          "original_form": "왜 안 드셨어요?",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016053.1.10",
          "form": "저도 아침을 잘 안 먹어요",
          "original_form": "저도 아침을 잘 안 먹어요",
          "speaker_id": "1",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016053.1.11",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100016053.1.12",
          "form": "점심 뭐 드실 건가요",
          "original_form": "점심 뭐 드실 건가요",
          "speaker_id": "1",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100016053.1.13",
          "form": "아직 생각 안 했어요",
          "original_form": "아직 생각 안 했어요",
          "speaker_id": "2",
          "time": "20210826 08:56"
        },
        {
          "id": "MDRW2100016053.1.14",
          "form": "음 뭐가 좋지",
          "original_form": "음 뭐가 좋지",
          "speaker_id": "2",
          "time": "20210826 08:56"
