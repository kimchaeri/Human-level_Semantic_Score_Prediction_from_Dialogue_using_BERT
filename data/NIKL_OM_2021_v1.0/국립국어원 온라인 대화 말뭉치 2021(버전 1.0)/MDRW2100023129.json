      "utterance": [
        {
          "id": "MDRW2100023129.1.1",
          "form": "편의점알바 저희동생도했는데",
          "original_form": "편의점알바 저희동생도했는데",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023129.1.2",
          "form": "네넹 ~",
          "original_form": "네넹 ~",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023129.1.3",
          "form": "그때 유통기한 얼마안남은거 자주 가져오더라구요ㅎㅎ",
          "original_form": "그때 유통기한 얼마안남은거 자주 가져오더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023129.1.4",
          "form": "맛있게 먹었었는데ㅎ",
          "original_form": "맛있게 먹었었는데ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023129.1.5",
          "form": "저두요 ㅋ",
          "original_form": "저두요 ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023129.1.6",
          "form": "근데 생각해보니 저 알바 진짜 많이 하긴 햇네요 ㅋ",
          "original_form": "근데 생각해보니 저 알바 진짜 많이 하긴 햇네요 ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023129.1.7",
          "form": "ㅎㅎㅎ 그게 행복이죠ㅎ",
          "original_form": "ㅎㅎㅎ 그게 행복이죠ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023129.1.8",
          "form": "많이하셨을것같애요ㅎ",
          "original_form": "많이하셨을것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023129.1.9",
          "form": "이게 카톡이긴한데ㅎ 왠지모르게 전문가의 포쓰가ㅎ",
          "original_form": "이게 카톡이긴한데ㅎ 왠지모르게 전문가의 포쓰가ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023129.1.10",
          "form": "고객응대 스킬인가요?ㅎㅎ",
          "original_form": "고객응대 스킬인가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023129.1.11",
          "form": "편의점. 피아노학원.  하객 알바.  결혼식 피아노 반주.  주유소 오픈알바. 백화점 알바 등등 ㅋ",
          "original_form": "편의점. 피아노학원.  하객 알바.  결혼식 피아노 반주.  주유소 오픈알바. 백화점 알바 등등 ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023129.1.12",
          "form": "그런듯여",
          "original_form": "그런듯여",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023129.1.13",
          "form": "이야이야ㅎㅎ",
          "original_form": "이야이야ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023129.1.14",
          "form": "주유소 문여는 알바도 재믹엇는데 ㅋ",
          "original_form": "주유소 문여는 알바도 재믹엇는데 ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023129.1.15",
          "form": "엄청하셨네요ㅎ",
          "original_form": "엄청하셨네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023129.1.16",
          "form": "고생많이하셨겟다ㅠ",
          "original_form": "고생많이하셨겟다ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023129.1.17",
          "form": "여자로써 너무 힘들었겟내요ㅠ",
          "original_form": "여자로써 너무 힘들었겟내요ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023129.1.18",
          "form": "새벽 6시에 문여는거에요 딸랑 2시간하는데 60 ㅋ",
          "original_form": "새벽 6시에 문여는거에요 딸랑 2시간하는데 60 ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023129.1.19",
          "form": "그대센 절대 늦잠 노노",
          "original_form": "그대센 절대 늦잠 노노",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023129.1.20",
          "form": "오건 예전에 투잡으로 햇엇네요 ㅎㅎ",
          "original_form": "오건 예전에 투잡으로 햇엇네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023129.1.21",
          "form": "본업 하기전에 새벽에 2시간하고 ㅎㅎ 출근",
          "original_form": "본업 하기전에 새벽에 2시간하고 ㅎㅎ 출근",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023129.1.22",
          "form": "가끔은 5시 반에 오픈 하기도 하궁",
          "original_form": "가끔은 5시 반에 오픈 하기도 하궁",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023129.1.23",
          "form": "우와ㅎ",
          "original_form": "우와ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023129.1.24",
          "form": "관광차 아침 출발전에  주유하는건데",
          "original_form": "관광차 아침 출발전에  주유하는건데",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023129.1.25",
          "form": "이야 정말 꿀이네요ㅎ",
          "original_form": "이야 정말 꿀이네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023129.1.26",
          "form": "나도해야겟다ㅎ",
          "original_form": "나도해야겟다ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023129.1.27",
          "form": "기사님들이 시간안맞으먼 4시고 5시에도 전화와서",
          "original_form": "기사님들이 시간안맞으먼 4시고 5시에도 전화와서",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023129.1.28",
          "form": "할수있을것같은데ㅎㅎ",
          "original_form": "할수있을것같은데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023129.1.29",
          "form": "그러면 잠을 못주무시잖아요",
          "original_form": "그러면 잠을 못주무시잖아요",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023129.1.30",
          "form": "열어주고 그랬어여 사장님이 그래서 완전 좋아하셧죠",
          "original_form": "열어주고 그랬어여 사장님이 그래서 완전 좋아하셧죠",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023129.1.31",
          "form": "우와ㅎ 정말 많이 노력하셨내요",
          "original_form": "우와ㅎ 정말 많이 노력하셨내요",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023129.1.32",
          "form": "그땐 어렷으니 ㅋ",
          "original_form": "그땐 어렷으니 ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023129.1.33",
          "form": "그러니깐 name1이 이렇게 잘 컸죠ㅎ",
          "original_form": "그러니깐 &name1&이 이렇게 잘 컸죠ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023129.1.34",
          "form": "정말 존경스럽네요ㅠ",
          "original_form": "정말 존경스럽네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023129.1.35",
          "form": "전 아에 알바를 안했어요ㅠ",
          "original_form": "전 아에 알바를 안했어요ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023129.1.36",
          "form": "그대신 기사님들도 아침에 올때  먹을거 잔뜩 ㅋ",
          "original_form": "그대신 기사님들도 아침에 올때  먹을거 잔뜩 ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023129.1.37",
          "form": "우와ㅜㅜ",
          "original_form": "우와ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023129.1.38",
          "form": "진짜 엣날에는 그런 정들이 많이있었어요ㅎ",
          "original_form": "진짜 엣날에는 그런 정들이 많이있었어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023129.1.39",
          "form": "관광차 타면 놀러가거나 결혼식이라 음식 많이 실어서 ㅎㅎ",
          "original_form": "관광차 타면 놀러가거나 결혼식이라 음식 많이 실어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023129.1.40",
          "form": "맞아요 정 많앗죠 ㅋ",
          "original_form": "맞아요 정 많앗죠 ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023129.1.41",
          "form": "맞네요ㅎㅎ",
          "original_form": "맞네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023129.1.42",
          "form": "사람은 정이 많아야해요ㅎ",
          "original_form": "사람은 정이 많아야해요ㅎ",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023129.1.43",
          "form": "ㅎㅎㅎ 근데 이상한분도 잇엇어요",
          "original_form": "ㅎㅎㅎ 근데 이상한분도 잇엇어요",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023129.1.44",
          "form": "근데 안힘드샸어요?",
          "original_form": "근데 안힘드샸어요?",
