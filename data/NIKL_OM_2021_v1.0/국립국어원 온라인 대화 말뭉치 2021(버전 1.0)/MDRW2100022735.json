        {
          "id": "MDRW2100022735.1.2",
          "form": "제습기요",
          "original_form": "제습기요",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022735.1.3",
          "form": "어디꺼 사셨어요?",
          "original_form": "어디꺼 사셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022735.1.4",
          "form": "lg요",
          "original_form": "lg요",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022735.1.5",
          "form": "오 저희집두요 ㅎ",
          "original_form": "오 저희집두요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022735.1.6",
          "form": "얼마정도 했나요?",
          "original_form": "얼마정도 했나요?",
          "speaker_id": "1",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022735.1.7",
          "form": "60이요",
          "original_form": "60이요",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022735.1.8",
          "form": "오 용량 큰거 사셨어요??",
          "original_form": "오 용량 큰거 사셨어요??",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022735.1.9",
          "form": "네 너무 습해서",
          "original_form": "네 너무 습해서",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022735.1.10",
          "form": "있으면 좋죠 ㅎ",
          "original_form": "있으면 좋죠 ㅎ",
          "speaker_id": "1",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100022735.1.11",
          "form": "원래는 제습기 없으셨나요?",
          "original_form": "원래는 제습기 없으셨나요?",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022735.1.12",
          "form": "네 없음요",
          "original_form": "네 없음요",
          "speaker_id": "2",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022735.1.13",
          "form": "그러면 에어컨 제습으로 트셨었나요?",
          "original_form": "그러면 에어컨 제습으로 트셨었나요?",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022735.1.14",
