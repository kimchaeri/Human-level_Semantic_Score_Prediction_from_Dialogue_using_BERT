      "utterance": [
        {
          "id": "MDRW2100034603.1.1",
          "form": "의상이 너무 그렇다",
          "original_form": "의상이 너무 그렇다",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034603.1.2",
          "form": "여름이라 저래 입혔나보다",
          "original_form": "여름이라 저래 입혔나보다",
          "speaker_id": "1",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034603.1.3",
          "form": "유치원복 같다",
          "original_form": "유치원복 같다",
          "speaker_id": "2",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034603.1.4",
          "form": "ㅋㅋㅋ 폭죽도 터틀리고 학예회네",
          "original_form": "ㅋㅋㅋ 폭죽도 터틀리고 학예회네",
          "speaker_id": "1",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034603.1.5",
          "form": "그러게 저런 비용들은 소속사부담이라던데",
          "original_form": "그러게 저런 비용들은 소속사부담이라던데",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100034603.1.6",
          "form": "문세윤이 왜 나옴? ㅋㅋ",
          "original_form": "문세윤이 왜 나옴? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034603.1.7",
          "form": "부끄뚱은 뭔데 ㅋㅋ",
          "original_form": "부끄뚱은 뭔데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034603.1.8",
          "form": "저것도 노래가?",
          "original_form": "저것도 노래가?",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100034603.1.9",
          "form": "그러게 노래하고는 싶었나보다",
          "original_form": "그러게 노래하고는 싶었나보다",
          "speaker_id": "2",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100034603.1.10",
          "form": "이거 때문에 그만뒸나? ㅋ",
          "original_form": "이거 때문에 그만뒸나? ㅋ",
          "speaker_id": "1",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100034603.1.11",
          "form": "김준현이 그만 둔게 아니고",
          "original_form": "김준현이 그만 둔게 아니고",
          "speaker_id": "2",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100034603.1.12",
          "form": "아 맞나ㅋㅋ 착각했다",
          "original_form": "아 맞나ㅋㅋ 착각했다",
          "speaker_id": "1",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100034603.1.13",
          "form": "ㅋ가수라는 가수는 다 나오네",
