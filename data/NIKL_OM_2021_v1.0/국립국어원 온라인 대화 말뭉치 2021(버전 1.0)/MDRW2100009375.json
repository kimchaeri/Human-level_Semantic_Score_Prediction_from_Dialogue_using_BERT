      "utterance": [
        {
          "id": "MDRW2100009375.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009375.1.2",
          "form": "아니요 키우지 않아요",
          "original_form": "아니요 키우지 않아요",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009375.1.3",
          "form": "키우고싶지 않아요?",
          "original_form": "키우고싶지 않아요?",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009375.1.4",
          "form": "알 러지가 있어서 못 기우답니다",
          "original_form": "알 러지가 있어서 못 기우답니다",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009375.1.5",
          "form": "전 강아지가너무좋아요",
          "original_form": "전 강아지가너무좋아요",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009375.1.6",
          "form": "저도 냥이보단 강아지가 좋아요",
          "original_form": "저도 냥이보단 강아지가 좋아요",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009375.1.7",
          "form": "털이 고양이는",
          "original_form": "털이 고양이는",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009375.1.8",
          "form": "털보다 고양이 눈이 무서워요",
          "original_form": "털보다 고양이 눈이 무서워요",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009375.1.9",
          "form": "울음소리가",
          "original_form": "울음소리가",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009375.1.10",
          "form": "소리도 밤에 들으면 무섭죠",
          "original_form": "소리도 밤에 들으면 무섭죠",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009375.1.11",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009375.1.12",
          "form": "가끔 반려동물 키워보고 싶단 생각도 한답니다",
          "original_form": "가끔 반려동물 키워보고 싶단 생각도 한답니다",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009375.1.13",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009375.1.14",
          "form": "생각만 하고 만답니다",
          "original_form": "생각만 하고 만답니다",
