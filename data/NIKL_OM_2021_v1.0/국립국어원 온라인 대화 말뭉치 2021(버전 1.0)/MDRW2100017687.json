      "utterance": [
        {
          "id": "MDRW2100017687.1.1",
          "form": "재택근무는 어떻게 생각하세요?",
          "original_form": "재택근무는 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017687.1.2",
          "form": "완전 로망이에요 ㅋㅋㅋ 친구가 하는데 너무 부러워요",
          "original_form": "완전 로망이에요 ㅋㅋㅋ 친구가 하는데 너무 부러워요",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017687.1.3",
          "form": "확실히 통근시간이 없는게 최고인거 같아요",
          "original_form": "확실히 통근시간이 없는게 최고인거 같아요",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017687.1.4",
          "form": "특히 아침잠많은 사람은....",
          "original_form": "특히 아침잠많은 사람은....",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017687.1.5",
          "form": "맞아요 두세시간은 아낄 수 있을 거 같아요 ㅋㅋㅋ",
          "original_form": "맞아요 두세시간은 아낄 수 있을 거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017687.1.6",
          "form": "화장도 안 해도 되고 옷도..",
          "original_form": "화장도 안 해도 되고 옷도..",
          "speaker_id": "2",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017687.1.7",
          "form": "코로나가 잠잠해지더라도",
          "original_form": "코로나가 잠잠해지더라도",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017687.1.8",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017687.1.9",
          "form": "재택은 유지되면 좋겠어요",
          "original_form": "재택은 유지되면 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017687.1.10",
          "form": "맞아요 인간관계에서도 덜 스트레스받을거같아요",
          "original_form": "맞아요 인간관계에서도 덜 스트레스받을거같아요",
          "speaker_id": "2",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100017687.1.11",
          "form": "재택에 필요한 공간비용과 물품을 회사에서 지원하는게 앞으로 사회에 맞지않나 싶거든요",
          "original_form": "재택에 필요한 공간비용과 물품을 회사에서 지원하는게 앞으로 사회에 맞지않나 싶거든요",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017687.1.12",
          "form": "그렇게 될까요?",
          "original_form": "그렇게 될까요?",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017687.1.13",
          "form": "그렇게되면 너무 좋을거같아요 ㅠㅠ",
          "original_form": "그렇게되면 너무 좋을거같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017687.1.14",
          "form": "저두요ㅠㅠㅠ 해줘라줘",
          "original_form": "저두요ㅠㅠㅠ 해줘라줘",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017687.1.15",
          "form": "재택근무중이세요??",
          "original_form": "재택근무중이세요??",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017687.1.16",
          "form": "반재택하고있어요....",
          "original_form": "반재택하고있어요....",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017687.1.17",
          "form": "일주일에 삼일은 가고 이틀은 재택이요",
          "original_form": "일주일에 삼일은 가고 이틀은 재택이요",
