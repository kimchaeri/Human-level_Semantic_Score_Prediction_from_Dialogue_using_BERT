      "utterance": [
        {
          "id": "MDRW2100019235.1.1",
          "form": "유학생....넘나 멋져여,,,",
          "original_form": "유학생....넘나 멋져여,,,",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019235.1.2",
          "form": "미용과건강...워우 음..!",
          "original_form": "미용과건강...워우 음..!",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019235.1.3",
          "form": "사실 대면인줄알아서 잔여백신도 맞은상탠데 결국 비대면이지뭐에요",
          "original_form": "사실 대면인줄알아서 잔여백신도 맞은상탠데 결국 비대면이지뭐에요",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019235.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 백신이나 코로나얘기해도되죠뭐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 백신이나 코로나얘기해도되죠뭐",
          "speaker_id": "1",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019235.1.5",
          "form": "우앙 저눈 11일에백신맞아염",
          "original_form": "우앙 저눈 11일에백신맞아염",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019235.1.6",
          "form": "아직 한참,,,",
          "original_form": "아직 한참,,,",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019235.1.7",
          "form": "아하 그러시구나! 화이자맞으시나요? 아님 모더나?",
          "original_form": "아하 그러시구나! 화이자맞으시나요? 아님 모더나?",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019235.1.8",
          "form": "담주에",
          "original_form": "담주에",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019235.1.9",
          "form": "결과가 나와여",
          "original_form": "결과가 나와여",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019235.1.10",
          "form": "둘중하나라그",
          "original_form": "둘중하나라그",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019235.1.11",
          "form": "아 결과가 아직 안나왓구나~ 전 화이자맞았어요 ㅎㅎ",
          "original_form": "아 결과가 아직 안나왓구나~ 전 화이자맞았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019235.1.12",
          "form": "우아앙",
          "original_form": "우아앙",
          "speaker_id": "2",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100019235.1.13",
          "form": "저두 아마 화이자일거같아여!",
          "original_form": "저두 아마 화이자일거같아여!",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019235.1.14",
          "form": "역시! 부작용없길 바랄게요 ㅋㅋㅋ",
          "original_form": "역시! 부작용없길 바랄게요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019235.1.15",
          "form": "부작용없었어여???!",
          "original_form": "부작용없었어여???!",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019235.1.16",
          "form": "괜찮았나염",
          "original_form": "괜찮았나염",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019235.1.17",
          "form": "저는 운좋게도 1차2차 둘다 없었어요~럭키",
          "original_form": "저는 운좋게도 1차2차 둘다 없었어요~럭키",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019235.1.18",
          "form": "오오옹",
          "original_form": "오오옹",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019235.1.19",
          "form": "럭키걸~",
          "original_form": "럭키걸~",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019235.1.20",
          "form": "ㅋㅋㅋ근데 부작용 진짜 사바사인거같더라구요 ㅋㅋ",
