      "utterance": [
        {
          "id": "MDRW2100016168.1.1",
          "form": "영화 좋아하시나요 ㅎㅎ",
          "original_form": "영화 좋아하시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016168.1.2",
          "form": "네 영화좋아합니다",
          "original_form": "네 영화좋아합니다",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100016168.1.3",
          "form": "영화 좋아하시나요",
          "original_form": "영화 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100016168.1.4",
          "form": "네 최근에 뭐보셨어요?",
          "original_form": "네 최근에 뭐보셨어요?",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100016168.1.5",
          "form": "최근에 극장가서 본 건 없고 집에서 그..",
          "original_form": "최근에 극장가서 본 건 없고 집에서 그..",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100016168.1.6",
          "form": "대만영화인데 왕대륙이랑 니니나오는거 그..",
          "original_form": "대만영화인데 왕대륙이랑 니니나오는거 그..",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016168.1.7",
          "form": "아 28세 미성년봤어요",
          "original_form": "아 28세 미성년봤어요",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016168.1.8",
          "form": "최근에 보신영화 있으신가요",
          "original_form": "최근에 보신영화 있으신가요",
          "speaker_id": "1",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100016168.1.9",
          "form": "오 그건 뭐죠",
          "original_form": "오 그건 뭐죠",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100016168.1.10",
          "form": "이거 여주인공이 28세인데 초콜렛을 먹으면 정신이 18세로 돌아가는 내용이에요",
          "original_form": "이거 여주인공이 28세인데 초콜렛을 먹으면 정신이 18세로 돌아가는 내용이에요",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100016168.1.11",
          "form": "어 그러니까.. 과거와 현재의 자아가 왔다갔다하는.. 뭐 그런 내용",
          "original_form": "어 그러니까.. 과거와 현재의 자아가 왔다갔다하는.. 뭐 그런 내용",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100016168.1.12",
          "form": "오 재밌겠네요!",
          "original_form": "오 재밌겠네요!",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100016168.1.13",
          "form": "최근 보신 영화있으신가요",
          "original_form": "최근 보신 영화있으신가요",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100016168.1.14",
          "form": "저는 모가디슈요!",
          "original_form": "저는 모가디슈요!",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100016168.1.15",
          "form": "모가디슈 어떠셨나요",
          "original_form": "모가디슈 어떠셨나요",
          "speaker_id": "1",
          "time": "20210827 07:28"
        },
        {
          "id": "MDRW2100016168.1.16",
          "form": "재밌다고들하던데",
          "original_form": "재밌다고들하던데",
          "speaker_id": "1",
          "time": "20210827 07:28"
        },
        {
          "id": "MDRW2100016168.1.17",
          "form": "연기들 잘해요 캬",
          "original_form": "연기들 잘해요 캬",
          "speaker_id": "2",
          "time": "20210827 09:37"
        },
        {
          "id": "MDRW2100016168.1.18",
          "form": "어떤점이 가장 기억에 남나요",
          "original_form": "어떤점이 가장 기억에 남나요",
          "speaker_id": "1",
          "time": "20210827 09:38"
        },
        {
          "id": "MDRW2100016168.1.19",
          "form": "그 반군 피해서 도망치는거요 ㅎㅎ",
          "original_form": "그 반군 피해서 도망치는거요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:39"
        },
        {
          "id": "MDRW2100016168.1.20",
          "form": "지금 아프가니스탄 느낌일거 같아요",
          "original_form": "지금 아프가니스탄 느낌일거 같아요",
          "speaker_id": "2",
          "time": "20210827 09:39"
        },
        {
          "id": "MDRW2100016168.1.21",
          "form": "음 맞아요",
          "original_form": "음 맞아요",
          "speaker_id": "1",
          "time": "20210827 09:39"
        },
        {
          "id": "MDRW2100016168.1.22",
          "form": "지금 사태때문에 좀 더 공감된다는 분들이 많으시더라구요",
          "original_form": "지금 사태때문에 좀 더 공감된다는 분들이 많으시더라구요",
          "speaker_id": "1",
          "time": "20210827 09:40"
        }
      ]
