      "utterance": [
        {
          "id": "MDRW2100034271.1.1",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100034271.1.2",
          "form": "네 저는 라면먹었어요",
          "original_form": "네 저는 라면먹었어요",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034271.1.3",
          "form": "name2님뭐드셨어요",
          "original_form": "&name2&님뭐드셨어요",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034271.1.4",
          "form": "저 삼겹살 먹었어요!",
          "original_form": "저 삼겹살 먹었어요!",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034271.1.5",
          "form": "무슨 라면 드셨어요?",
          "original_form": "무슨 라면 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034271.1.6",
          "form": "오 고기 넘맛있었겠어요",
          "original_form": "오 고기 넘맛있었겠어요",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034271.1.7",
          "form": "전 진라면이요",
          "original_form": "전 진라면이요",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034271.1.8",
          "form": "진라면 좋아하세요?",
          "original_form": "진라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100034271.1.9",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034271.1.10",
          "form": "진라면 순한맛? 매운맛? 어던 거 드세요",
          "original_form": "진라면 순한맛? 매운맛? 어던 거 드세요",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034271.1.11",
          "form": "전 둘다좋아하긴하는데 요즘은 순한맛이여",
          "original_form": "전 둘다좋아하긴하는데 요즘은 순한맛이여",
          "speaker_id": "1",
          "time": "20210828 08:25"
        },
        {
          "id": "MDRW2100034271.1.12",
          "form": "name2님은 어떤맛좋아하세요",
          "original_form": "&name2&님은 어떤맛좋아하세요",
          "speaker_id": "1",
          "time": "20210828 08:25"
        },
        {
          "id": "MDRW2100034271.1.13",
          "form": "옛날엔 순한맛 좋아했는데",
          "original_form": "옛날엔 순한맛 좋아했는데",
          "speaker_id": "2",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100034271.1.14",
          "form": "요즘은 매운맛이요",
          "original_form": "요즘은 매운맛이요",
          "speaker_id": "2",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100034271.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100034271.1.16",
          "form": "전 요새 매운거 먹으면 바로 내려버려서ㅜ",
          "original_form": "전 요새 매운거 먹으면 바로 내려버려서ㅜ",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034271.1.17",
          "form": "카페가시는거 좋아하세요?",
          "original_form": "카페가시는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034271.1.18",
          "form": "네 좋아하는데 최근엔 못 갔네요ㅜ",
          "original_form": "네 좋아하는데 최근엔 못 갔네요ㅜ",
          "speaker_id": "2",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100034271.1.19",
          "form": "저도요ㅜ",
          "original_form": "저도요ㅜ",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100034271.1.20",
          "form": "어떤카페자주가셨어요",
          "original_form": "어떤카페자주가셨어요",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100034271.1.21",
          "form": "저 디저트 맛있는 데 좋아해요",
          "original_form": "저 디저트 맛있는 데 좋아해요",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100034271.1.22",
          "form": "저도 그래요 어떤 디저트 좋아하세요",
          "original_form": "저도 그래요 어떤 디저트 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100034271.1.23",
          "form": "거의 안 가리고 좋아하는데",
          "original_form": "거의 안 가리고 좋아하는데",
          "speaker_id": "2",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100034271.1.24",
          "form": "마카롱이랑 케이크 특히 좋으해요",
          "original_form": "마카롱이랑 케이크 특히 좋으해요",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100034271.1.25",
          "form": "저도 그 두개 넘좋아해요",
          "original_form": "저도 그 두개 넘좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100034271.1.26",
          "form": "케이크 어떤맛 선호하시는지요",
          "original_form": "케이크 어떤맛 선호하시는지요",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100034271.1.27",
          "form": "치즈나 초코? 다 좋아해요 사실",
          "original_form": "치즈나 초코? 다 좋아해요 사실",
          "speaker_id": "2",
          "time": "20210828 22:23"
        }
      ]
