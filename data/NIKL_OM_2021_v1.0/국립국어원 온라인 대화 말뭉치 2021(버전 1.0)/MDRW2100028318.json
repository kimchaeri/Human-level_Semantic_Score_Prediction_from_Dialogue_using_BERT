      "utterance": [
        {
          "id": "MDRW2100028318.1.1",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100028318.1.2",
          "form": "낯설다 우리 팀(다른 의미로)",
          "original_form": "낯설다 우리 팀(다른 의미로)",
          "speaker_id": "2",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100028318.1.3",
          "form": "웃겨",
          "original_form": "웃겨",
          "speaker_id": "1",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100028318.1.4",
          "form": "서교수님은 아세요?",
          "original_form": "서교수님은 아세요?",
          "speaker_id": "1",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100028318.1.5",
          "form": "약간 이제 정신이 드세요 바이브",
          "original_form": "약간 이제 정신이 드세요 바이브",
          "speaker_id": "1",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100028318.1.6",
          "form": "아니 웃기다",
          "original_form": "아니 웃기다",
          "speaker_id": "2",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100028318.1.7",
          "form": "교수님 오신 건 알죠",
          "original_form": "교수님 오신 건 알죠",
          "speaker_id": "2",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100028318.1.8",
          "form": "재원이는 모르시겠네...",
          "original_form": "재원이는 모르시겠네...",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028318.1.9",
          "form": "잠실빅보이",
          "original_form": "잠실빅보이",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028318.1.10",
          "form": "재원...",
          "original_form": "재원...",
          "speaker_id": "2",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028318.1.11",
          "form": "오재원인가요",
          "original_form": "오재원인가요",
          "speaker_id": "2",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028318.1.12",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028318.1.13",
          "form": "무슨 말을 그렇게 하세요",
          "original_form": "무슨 말을 그렇게 하세요",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028318.1.14",
          "form": "아 아닌가요 아",
          "original_form": "아 아닌가요 아",
          "speaker_id": "2",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028318.1.15",
          "form": "오재원은 두산에서 늙어죽지",
          "original_form": "오재원은 두산에서 늙어죽지",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028318.1.16",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028318.1.17",
          "form": "아는 재원이 그 재원밖에 없어서",
          "original_form": "아는 재원이 그 재원밖에 없어서",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028318.1.18",
          "form": "있어 우리팀에 젊은 거포... 이재원이라고",
          "original_form": "있어 우리팀에 젊은 거포... 이재원이라고",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028318.1.19",
          "form": "2군에서 올라온 분인가요",
          "original_form": "2군에서 올라온 분인가요",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028318.1.20",
          "form": "2군 홈런왕이에요",
          "original_form": "2군 홈런왕이에요",
          "speaker_id": "1",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028318.1.21",
          "form": "이열",
          "original_form": "이열",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028318.1.22",
          "form": "보면 약간... 홈런 잘 치게 생긴 애 있음",
          "original_form": "보면 약간... 홈런 잘 치게 생긴 애 있음",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028318.1.23",
          "form": "걔임",
          "original_form": "걔임",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028318.1.24",
          "form": "관상이 홈런 관상인가요",
          "original_form": "관상이 홈런 관상인가요",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028318.1.25",
          "form": "이형종이랑 비슷한데 더 크고 길쭉하게 생겼음",
          "original_form": "이형종이랑 비슷한데 더 크고 길쭉하게 생겼음",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028318.1.26",
          "form": "말을 그렇게 하네",
          "original_form": "말을 그렇게 하네",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028318.1.27",
          "form": "고등학교도 같은 데 나왔을걸",
          "original_form": "고등학교도 같은 데 나왔을걸",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028318.1.28",
          "form": "사이언스",
          "original_form": "사이언스",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028318.1.29",
          "form": "미니 이형종 근데 좀 홈런 특화인",
          "original_form": "미니 이형종 근데 좀 홈런 특화인",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100028318.1.30",
          "form": "미니 이형종",
          "original_form": "미니 이형종",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028318.1.31",
          "form": "도련님 볼 좋네",
          "original_form": "도련님 볼 좋네",
