        },
        {
          "id": "MDRW2100046329.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20211019 14:13"
        },
        {
          "id": "MDRW2100046329.1.3",
          "form": "백신 다 맞으셨나요 ?",
          "original_form": "백신 다 맞으셨나요 ?",
          "speaker_id": "2",
          "time": "20211019 14:13"
        },
        {
          "id": "MDRW2100046329.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211019 14:13"
        },
        {
          "id": "MDRW2100046329.1.5",
          "form": "얼마나 아프셨어요?",
          "original_form": "얼마나 아프셨어요?",
          "speaker_id": "2",
          "time": "20211019 14:13"
        },
        {
          "id": "MDRW2100046329.1.6",
          "form": "살짝 몸살기만 있고 별로안아팠어요",
          "original_form": "살짝 몸살기만 있고 별로안아팠어요",
          "speaker_id": "1",
          "time": "20211019 14:13"
        },
        {
          "id": "MDRW2100046329.1.7",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20211019 14:13"
        },
        {
          "id": "MDRW2100046329.1.8",
          "form": "2차에 많이들 아파하시던데",
          "original_form": "2차에 많이들 아파하시던데",
          "speaker_id": "2",
          "time": "20211019 14:14"
        },
        {
          "id": "MDRW2100046329.1.9",
          "form": "네 전 별로 안아프더라고요",
          "original_form": "네 전 별로 안아프더라고요",
          "speaker_id": "1",
          "time": "20211019 14:14"
        },
        {
          "id": "MDRW2100046329.1.10",
          "form": "튼튼하신가봐요",
          "original_form": "튼튼하신가봐요",
          "speaker_id": "2",
          "time": "20211019 14:15"
        },
        {
          "id": "MDRW2100046329.1.11",
          "form": "네 그런가봐요",
          "original_form": "네 그런가봐요",
          "speaker_id": "1",
          "time": "20211019 14:15"
        },
        {
          "id": "MDRW2100046329.1.12",
          "form": "운동선수들 아파도 바로 시합뛰고 그러는 거 보면",
          "original_form": "운동선수들 아파도 바로 시합뛰고 그러는 거 보면",
          "speaker_id": "2",
          "time": "20211019 14:15"
        },
        {
          "id": "MDRW2100046329.1.13",
          "form": "평소 강한 분들은",
          "original_form": "평소 강한 분들은",
          "speaker_id": "2",
          "time": "20211019 14:15"
        },
        {
          "id": "MDRW2100046329.1.14",
          "form": "백신도 팍팍 이겨내시는 듯",
          "original_form": "백신도 팍팍 이겨내시는 듯",
          "speaker_id": "2",
          "time": "20211019 14:15"
        },
        {
          "id": "MDRW2100046329.1.15",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20211019 14:15"
        },
        {
          "id": "MDRW2100046329.1.16",
          "form": "얼른 모두 마스크 벗는 날 돌아왔으면 좋겠네요 ㅠ ",
          "original_form": "얼른 모두 마스크 벗는 날 돌아왔으면 좋겠네요 ㅠ ",
          "speaker_id": "2",
