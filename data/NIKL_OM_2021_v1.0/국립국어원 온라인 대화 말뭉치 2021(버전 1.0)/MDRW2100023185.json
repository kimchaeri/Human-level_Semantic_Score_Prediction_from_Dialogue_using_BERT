      "utterance": [
        {
          "id": "MDRW2100023185.1.1",
          "form": "ㅇㅏ",
          "original_form": "ㅇㅏ",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023185.1.2",
          "form": "잊어버리고잇엇다",
          "original_form": "잊어버리고잇엇다",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023185.1.3",
          "form": "솔직히 게임자체가 재밋는건 아니라서",
          "original_form": "솔직히 게임자체가 재밋는건 아니라서",
          "speaker_id": "2",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023185.1.4",
          "form": "걍 가끔 한번씩 봐",
          "original_form": "걍 가끔 한번씩 봐",
          "speaker_id": "2",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023185.1.5",
          "form": "그치 할 것도 별로 없어서",
          "original_form": "그치 할 것도 별로 없어서",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023185.1.6",
          "form": "ㅇㅇ....뭔거 좀",
          "original_form": "ㅇㅇ....뭔거 좀",
          "speaker_id": "2",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023185.1.7",
          "form": "나도 걍 가끔 들어가서 광고 좀 보고",
          "original_form": "나도 걍 가끔 들어가서 광고 좀 보고",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023185.1.8",
          "form": "기계적으로 하트 소모시키는 중이긴 함..",
          "original_form": "기계적으로 하트 소모시키는 중이긴 함..",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023185.1.9",
          "form": "그림맞추기라도 있으면",
          "original_form": "그림맞추기라도 있으면",
          "speaker_id": "2",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023185.1.10",
          "form": "그런거라도 할텐데",
          "original_form": "그런거라도 할텐데",
          "speaker_id": "2",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023185.1.11",
          "form": "굳이 30분 기다려서 하트 찰때마다",
          "original_form": "굳이 30분 기다려서 하트 찰때마다",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023185.1.12",
          "form": "들어가고 그렇게는",
          "original_form": "들어가고 그렇게는",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023185.1.13",
          "form": "잘 안되더라",
          "original_form": "잘 안되더라",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023185.1.14",
          "form": "ㅇㅇ 나도 처음엔 30분마다 들어갔는데 이제는 약간 아슬아슬하게 들어감",
          "original_form": "ㅇㅇ 나도 처음엔 30분마다 들어갔는데 이제는 약간 아슬아슬하게 들어감",
          "speaker_id": "1",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023185.1.15",
          "form": "그리고 뭔가",
          "original_form": "그리고 뭔가",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023185.1.16",
          "form": "콘솔겜 안할거면",
          "original_form": "콘솔겜 안할거면",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023185.1.17",
          "form": "뭔가 이벤트같은거라도",
          "original_form": "뭔가 이벤트같은거라도",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023185.1.18",
          "form": "해야할텐데",
          "original_form": "해야할텐데",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023185.1.19",
          "form": "걍 아무것도 없으니까",
          "original_form": "걍 아무것도 없으니까",
          "speaker_id": "2",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100023185.1.20",
          "form": "언젠가는 다 보겠지....",
          "original_form": "언젠가는 다 보겠지....",
          "speaker_id": "2",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100023185.1.21",
          "form": "하고 잇게되네",
          "original_form": "하고 잇게되네",
          "speaker_id": "2",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100023185.1.22",
          "form": "ㅇㅇ 난 좀 이해가 안되는게",
          "original_form": "ㅇㅇ 난 좀 이해가 안되는게",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023185.1.23",
          "form": "보통 게임들이 오픈하고나면",
          "original_form": "보통 게임들이 오픈하고나면",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023185.1.24",
          "form": "한 일주일간은 정신없게 뭐 하지 않나...?",
          "original_form": "한 일주일간은 정신없게 뭐 하지 않나...?",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023185.1.25",
          "form": "ㅇㅇ솔직히",
          "original_form": "ㅇㅇ솔직히",
          "speaker_id": "2",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023185.1.26",
          "form": "아이템을 주거나 출석이벤트를 하거나",
          "original_form": "아이템을 주거나 출석이벤트를 하거나",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023185.1.27",
          "form": "이렇게까지 뭐가 없으면서",
          "original_form": "이렇게까지 뭐가 없으면서",
          "speaker_id": "2",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023185.1.28",
          "form": "무슨 자신감으로",
          "original_form": "무슨 자신감으로",
          "speaker_id": "2",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023185.1.29",
          "form": "내놓은건지 모르겠어",
          "original_form": "내놓은건지 모르겠어",
          "speaker_id": "2",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023185.1.30",
          "form": "성우는ㄹㅇ",
          "original_form": "성우는ㄹㅇ",
          "speaker_id": "2",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023185.1.31",
          "form": "왜쓴거임 진짜",
          "original_form": "왜쓴거임 진짜",
          "speaker_id": "2",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100023185.1.32",
          "form": "솔직히 돼지목에 진주목걸이야",
          "original_form": "솔직히 돼지목에 진주목걸이야",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023185.1.33",
          "form": "스토리로 밀고가고싶은거면",
          "original_form": "스토리로 밀고가고싶은거면",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023185.1.34",
          "form": "ㅇㅇ 성우 ㄹㅇ 무의미",
          "original_form": "ㅇㅇ 성우 ㄹㅇ 무의미",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023185.1.35",
          "form": "멘스는 완결을 내고",
          "original_form": "멘스는 완결을 내고",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023185.1.36",
          "form": "외전 디엘씨를 내던가",
          "original_form": "외전 디엘씨를 내던가",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023185.1.37",
          "form": "심지어",
          "original_form": "심지어",
          "speaker_id": "2",
          "time": "20210829 22:46"
        }
      ]
    }
