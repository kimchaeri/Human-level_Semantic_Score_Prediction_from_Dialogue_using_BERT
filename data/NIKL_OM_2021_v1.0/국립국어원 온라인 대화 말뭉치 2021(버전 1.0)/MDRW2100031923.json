      "utterance": [
        {
          "id": "MDRW2100031923.1.1",
          "form": "아이가 잘먹는게 복인데ㅎㅎ",
          "original_form": "아이가 잘먹는게 복인데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031923.1.2",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031923.1.3",
          "form": "안먹고 못먹으면 걱정되고",
          "original_form": "안먹고 못먹으면 걱정되고",
          "speaker_id": "2",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031923.1.4",
          "form": "부보마음은 똑같을 것같아요 ㅎ",
          "original_form": "부보마음은 똑같을 것같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031923.1.5",
          "form": "안먹는데 살도 안찌니 걱정 ㅋㅋ",
          "original_form": "안먹는데 살도 안찌니 걱정 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031923.1.6",
          "form": "저도 작은 편은 아닌데 흑흑",
          "original_form": "저도 작은 편은 아닌데 흑흑",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031923.1.7",
          "form": "병원가서  영유아검사하면",
          "original_form": "병원가서  영유아검사하면",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031923.1.8",
          "form": "ㅠㅠ 어째요ㅠ",
          "original_form": "ㅠㅠ 어째요ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031923.1.9",
          "form": "에고에고ㅠ",
          "original_form": "에고에고ㅠ",
          "speaker_id": "2",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031923.1.10",
          "form": "혼나는 기분임 ㅋㅋ",
          "original_form": "혼나는 기분임 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:46"
        },
        {
          "id": "MDRW2100031923.1.11",
          "form": "그렇겠네요ㅍ",
          "original_form": "그렇겠네요ㅍ",
          "speaker_id": "2",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100031923.1.12",
          "form": "나중에 크겟죠 ㅎㄹ",
          "original_form": "나중에 크겟죠 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100031923.1.13",
          "form": "저희도 혼나요ㅎ 비만될슈있다고ㅎ",
          "original_form": "저희도 혼나요ㅎ 비만될슈있다고ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100031923.1.14",
          "form": "남자애들은 확실이 초중딩때 많이 크니",
          "original_form": "남자애들은 확실이 초중딩때 많이 크니",
          "speaker_id": "1",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100031923.1.15",
          "form": "맞아요ㅎ 언젠가 클꺼에요ㅎ",
          "original_form": "맞아요ㅎ 언젠가 클꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100031923.1.16",
          "form": "여자애들이 초등학교6학년때까지 폭풍성장하잖아요",
          "original_form": "여자애들이 초등학교6학년때까지 폭풍성장하잖아요",
          "speaker_id": "2",
          "time": "20210905 08:47"
        },
        {
          "id": "MDRW2100031923.1.17",
          "form": "저도 여자치곤 큰편이라  은 170은 안되지만여 ㅋ",
          "original_form": "저도 여자치곤 큰편이라  은 170은 안되지만여 ㅋ",
          "speaker_id": "1",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100031923.1.18",
