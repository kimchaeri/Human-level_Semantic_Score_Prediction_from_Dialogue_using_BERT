      "utterance": [
        {
          "id": "MDRW2100040552.1.1",
          "form": "민초 가끔 땡겨요..",
          "original_form": "민초 가끔 땡겨요..",
          "speaker_id": "1",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100040552.1.2",
          "form": "아하 즐겨 드시는 편은 아니군요!",
          "original_form": "아하 즐겨 드시는 편은 아니군요!",
          "speaker_id": "2",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100040552.1.3",
          "form": "견과 종류를 좋아하시나요?",
          "original_form": "견과 종류를 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 22:34"
        },
        {
          "id": "MDRW2100040552.1.4",
          "form": "그렇다고 다른 견과아이스크림을 즐기는건 아니에요!",
          "original_form": "그렇다고 다른 견과아이스크림을 즐기는건 아니에요!",
          "speaker_id": "1",
          "time": "20210904 22:34"
        },
        {
          "id": "MDRW2100040552.1.5",
          "form": "생각해보니까 이상하군요",
          "original_form": "생각해보니까 이상하군요",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040552.1.6",
          "form": "ㅋㅋㅋㅋ 그맛만 먹는거같은데요",
          "original_form": "ㅋㅋㅋㅋ 그맛만 먹는거같은데요",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040552.1.7",
          "form": "아이스크림콘 윗부분 싫어합니다 ㅋㅋㅋ",
          "original_form": "아이스크림콘 윗부분 싫어합니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100040552.1.8",
          "form": "앜ㅋㅋㅋ 저도 윗부분 안좋아해요 ㅋㅋ",
          "original_form": "앜ㅋㅋㅋ 저도 윗부분 안좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040552.1.9",
          "form": "이상하게 그 윗부분은 별로 더군요",
          "original_form": "이상하게 그 윗부분은 별로 더군요",
          "speaker_id": "2",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040552.1.10",
          "form": "노땅콩 버전이 필요하다고 생각해요..",
          "original_form": "노땅콩 버전이 필요하다고 생각해요..",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040552.1.11",
          "form": "대체 무엇을위한 땅콩인가요",
          "original_form": "대체 무엇을위한 땅콩인가요",
          "speaker_id": "1",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100040552.1.12",
          "form": "누굴 위한 땅콩인지 모르겠어요...",
          "original_form": "누굴 위한 땅콩인지 모르겠어요...",
          "speaker_id": "2",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040552.1.13",
          "form": "배라말고는 무슨 아이스크림좋아하세요.?",
          "original_form": "배라말고는 무슨 아이스크림좋아하세요.?",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100040552.1.14",
          "form": "베라말고는 브라보콘 바닐라맛이요!",
          "original_form": "베라말고는 브라보콘 바닐라맛이요!",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040552.1.15",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040552.1.16",
          "form": "저는 구구요",
          "original_form": "저는 구구요",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040552.1.17",
          "form": "구구",
          "original_form": "구구",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040552.1.18",
          "form": "구구 입니다 구구",
          "original_form": "구구 입니다 구구",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040552.1.19",
          "form": "아하 ㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040552.1.20",
          "form": "역시 콘을 좋아하시는군요",
          "original_form": "역시 콘을 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040552.1.21",
          "form": "사실 구구 크러스트를 더 좋아합니다..",
          "original_form": "사실 구구 크러스트를 더 좋아합니다..",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040552.1.22",
          "form": "콘은 그 과자부분이 별로인거같아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "콘은 그 과자부분이 별로인거같아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040552.1.23",
          "form": "사실 잘 이해 안되실 수도 있는데",
          "original_form": "사실 잘 이해 안되실 수도 있는데",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040552.1.24",
          "form": "콘 끝부분 초콜릿 가득한 부분을 좋아하지 않아요",
          "original_form": "콘 끝부분 초콜릿 가득한 부분을 좋아하지 않아요",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100040552.1.25",
          "form": "뭔가 다 먹고 입이 다시 텁텁해지는 느낌이라서",
          "original_form": "뭔가 다 먹고 입이 다시 텁텁해지는 느낌이라서",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100040552.1.26",
          "form": "아 완전 이해합니다",
          "original_form": "아 완전 이해합니다",
          "speaker_id": "1",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100040552.1.27",
          "form": "뭘좀아시는분이군요",
          "original_form": "뭘좀아시는분이군요",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040552.1.28",
          "form": "굳이 필요없는부분이죠",
          "original_form": "굳이 필요없는부분이죠",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040552.1.29",
          "form": "아 배우신 분이네요",
          "original_form": "아 배우신 분이네요",
          "speaker_id": "2",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040552.1.30",
          "form": "이걸 공감해주시다니 너무 반가워요",
          "original_form": "이걸 공감해주시다니 너무 반가워요",
