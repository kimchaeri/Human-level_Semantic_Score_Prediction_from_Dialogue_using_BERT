      "utterance": [
        {
          "id": "MDRW2100035265.1.1",
          "form": "복면한채로 그냥 먹네 ㅋㅋ 다 묻는다",
          "original_form": "복면한채로 그냥 먹네 ㅋㅋ 다 묻는다",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100035265.1.2",
          "form": "세리머니 클럽 보고 있었다",
          "original_form": "세리머니 클럽 보고 있었다",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035265.1.3",
          "form": "한나라는 영화 보심ㅋ",
          "original_form": "한나라는 영화 보심ㅋ",
          "speaker_id": "1",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035265.1.4",
          "form": "ㅋㅋ제목부터 재미없어보인다",
          "original_form": "ㅋㅋ제목부터 재미없어보인다",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035265.1.5",
          "form": "여자 첩보영화 였던 걸로 기억남",
          "original_form": "여자 첩보영화 였던 걸로 기억남",
          "speaker_id": "1",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100035265.1.6",
          "form": "세리머니 게스트는 장동민이랑 엄지원",
          "original_form": "세리머니 게스트는 장동민이랑 엄지원",
          "speaker_id": "2",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100035265.1.7",
          "form": "골프 예능이지?",
          "original_form": "골프 예능이지?",
          "speaker_id": "1",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100035265.1.8",
          "form": "엉 박세리 나오는거",
          "original_form": "엉 박세리 나오는거",
          "speaker_id": "2",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100035265.1.9",
          "form": "골프 가르쳐 주는건가",
          "original_form": "골프 가르쳐 주는건가",
          "speaker_id": "1",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100035265.1.10",
          "form": "골프는 잘 안가르쳐주고 같이 골프 친다",
          "original_form": "골프는 잘 안가르쳐주고 같이 골프 친다",
          "speaker_id": "2",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035265.1.11",
          "form": "상대가 되나",
          "original_form": "상대가 되나",
          "speaker_id": "1",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035265.1.12",
          "form": "경쟁하는게 아니라서 잘치면 상금 누적해서 어디 기부하는가봐",
          "original_form": "경쟁하는게 아니라서 잘치면 상금 누적해서 어디 기부하는가봐",
          "speaker_id": "2",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100035265.1.13",
          "form": "취지는 좋네",
          "original_form": "취지는 좋네",
          "speaker_id": "1",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100035265.1.14",
          "form": "엄청 더운지 장동민은 얼음보따리를 머리에 이고 잇다",
          "original_form": "엄청 더운지 장동민은 얼음보따리를 머리에 이고 잇다",
          "speaker_id": "2",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100035265.1.15",
          "form": "ㅋㅋ 장동민 직설적이던데",
          "original_form": "ㅋㅋ 장동민 직설적이던데",
          "speaker_id": "1",
          "time": "20210829 14:34"
        }
