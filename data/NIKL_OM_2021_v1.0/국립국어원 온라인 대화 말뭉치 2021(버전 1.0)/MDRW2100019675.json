      "utterance": [
        {
          "id": "MDRW2100019675.1.1",
          "form": "어디로 가고 싶으세요?",
          "original_form": "어디로 가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100019675.1.2",
          "form": "제주?",
          "original_form": "제주?",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019675.1.3",
          "form": "동해?",
          "original_form": "동해?",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019675.1.4",
          "form": "바다 좋아하시나 봐요",
          "original_form": "바다 좋아하시나 봐요",
          "speaker_id": "1",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019675.1.5",
          "form": "파란 바다가 보고 싶어여ㅋㅋㅋ",
          "original_form": "파란 바다가 보고 싶어여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019675.1.6",
          "form": "남해 시커먼거 말고ㅋㅋ",
          "original_form": "남해 시커먼거 말고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019675.1.7",
          "form": "ㅋㅋㅋㅋㅋ 동해가 파랗긴 해요",
          "original_form": "ㅋㅋㅋㅋㅋ 동해가 파랗긴 해요",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019675.1.8",
          "form": "그래서 더 보고싶은거 같아요ㅋㅋㅋㅋ",
          "original_form": "그래서 더 보고싶은거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019675.1.9",
          "form": "바다가 푸르지 않으면 의미가 없죠ㅋㅋ",
          "original_form": "바다가 푸르지 않으면 의미가 없죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019675.1.10",
          "form": "수영은 못하지만ㅋㅋㅋㅋㅋㅋ",
          "original_form": "수영은 못하지만ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019675.1.11",
          "form": "보는 것만으로도 맘이 뻥 뚤리는거 같아요ㅎ",
          "original_form": "보는 것만으로도 맘이 뻥 뚤리는거 같아요ㅎ",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019675.1.12",
          "form": "저도 수영 못해요ㅋㅋ",
          "original_form": "저도 수영 못해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100019675.1.13",
          "form": "이시국이라서 더 여행이 가고 싶은거같아요ㅋㅋㅋㅋ",
          "original_form": "이시국이라서 더 여행이 가고 싶은거같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019675.1.14",
          "form": "원래 여행 별로 안좋아힜는데ㅋㅋ",
          "original_form": "원래 여행 별로 안좋아힜는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019675.1.15",
          "form": "저도 여행을 좋아하진 않아요",
          "original_form": "저도 여행을 좋아하진 않아요",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019675.1.16",
          "form": "막상 갔다오면 힘들고 지쳐서ㅋㅋ",
          "original_form": "막상 갔다오면 힘들고 지쳐서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019675.1.17",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019675.1.18",
          "form": "힐링인지는 정말 알다가도 모를ㅋㅋ",
          "original_form": "힐링인지는 정말 알다가도 모를ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019675.1.19",
          "form": "한참 후에 사진을 봐야지 좋았다는 생각이 들더라구요",
          "original_form": "한참 후에 사진을 봐야지 좋았다는 생각이 들더라구요",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019675.1.20",
          "form": "여행가면 기념품 같은건 사는 편이신가여?",
          "original_form": "여행가면 기념품 같은건 사는 편이신가여?",
          "speaker_id": "2",
          "time": "20210828 15:31"
        },
        {
          "id": "MDRW2100019675.1.21",
          "form": "잘 안사요... 너무 비싸기도 하구요...",
          "original_form": "잘 안사요... 너무 비싸기도 하구요...",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100019675.1.22",
          "form": "친구들이랑 우정으로 남기는 거 아니면 아까워요",
          "original_form": "친구들이랑 우정으로 남기는 거 아니면 아까워요",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100019675.1.23",
          "form": "기념품 많이 비싸져ㅋㅋㅋ",
          "original_form": "기념품 많이 비싸져ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100019675.1.24",
          "form": "저도 잘 안사게 되는거 같아요ㅎㅎ",
          "original_form": "저도 잘 안사게 되는거 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100019675.1.25",
          "form": "저도 기념품은 잘 안사는거 같아요ㅋㅋ",
          "original_form": "저도 기념품은 잘 안사는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100019675.1.26",
          "form": "실용적이지 못해서ㅎ",
          "original_form": "실용적이지 못해서ㅎ",
          "speaker_id": "2",
          "time": "20210829 13:04"
        }
      ]
    }
  ]
