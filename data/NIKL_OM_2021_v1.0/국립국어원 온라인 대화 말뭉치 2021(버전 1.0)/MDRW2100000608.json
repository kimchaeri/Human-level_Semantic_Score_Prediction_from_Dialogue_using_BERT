      "utterance": [
        {
          "id": "MDRW2100000608.1.1",
          "form": "여행 좋아하시나요??",
          "original_form": "여행 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000608.1.2",
          "form": "여행 좋죠!  종종 친구들이랑 돈 모아서 가곤했는데",
          "original_form": "여행 좋죠!  종종 친구들이랑 돈 모아서 가곤했는데",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000608.1.3",
          "form": "요새는 코로나땜에 못가네요ㅠ",
          "original_form": "요새는 코로나땜에 못가네요ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000608.1.4",
          "form": "그죠ㅜㅠ",
          "original_form": "그죠ㅜㅠ",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000608.1.5",
          "form": "일상을 살다가 한번씩 환기거 필요한게",
          "original_form": "일상을 살다가 한번씩 환기거 필요한게",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000608.1.6",
          "form": "필요한데",
          "original_form": "필요한데",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000608.1.7",
          "form": "코로나 끝나면 어디로 놀러가고싶으세요?",
          "original_form": "코로나 끝나면 어디로 놀러가고싶으세요?",
          "speaker_id": "1",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000608.1.8",
          "form": "일단 저희 원래 경주가려다가 못가서 경주 가려구요",
          "original_form": "일단 저희 원래 경주가려다가 못가서 경주 가려구요",
          "speaker_id": "2",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000608.1.9",
          "form": "아니면 외국 나갈수있으면 바다 이쁜곳으로 여행가고싶네요ㅠ",
          "original_form": "아니면 외국 나갈수있으면 바다 이쁜곳으로 여행가고싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000608.1.10",
          "form": "아 경주 좋죠",
          "original_form": "아 경주 좋죠",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000608.1.11",
          "form": "저는 경주월드 가는거 좋아해요!",
          "original_form": "저는 경주월드 가는거 좋아해요!",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000608.1.12",
          "form": "혹시 가보신적 있으세요?",
          "original_form": "혹시 가보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000608.1.13",
          "form": "아니요 저는 놀이기구못타서ㅠ",
          "original_form": "아니요 저는 놀이기구못타서ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000608.1.14",
          "form": "친구는 경주가면 거기 가고싶다그러더라구요",
          "original_form": "친구는 경주가면 거기 가고싶다그러더라구요",
          "speaker_id": "2",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000608.1.15",
          "form": "드라켄? 그거 타고싶다고",
          "original_form": "드라켄? 그거 타고싶다고",
          "speaker_id": "2",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000608.1.16",
          "form": "아 드라켄 재밌어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 드라켄 재밌어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000608.1.17",
          "form": "경주가면 가기좋은 관광명소인 것 같아요",
          "original_form": "경주가면 가기좋은 관광명소인 것 같아요",
          "speaker_id": "1",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000608.1.18",
          "form": "그리고 경주에 황리단길도 요즘 엄청 잘되어있어요",
          "original_form": "그리고 경주에 황리단길도 요즘 엄청 잘되어있어요",
          "speaker_id": "1",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000608.1.19",
          "form": "오 타보셨구나",
          "original_form": "오 타보셨구나",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000608.1.20",
          "form": "놀이기구 잘타시나봐요",
          "original_form": "놀이기구 잘타시나봐요",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000608.1.21",
          "form": "마자요 황리단길 잘 되있다그러더라구요",
          "original_form": "마자요 황리단길 잘 되있다그러더라구요",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000608.1.22",
          "form": "잘 못타는데 좋아는해요 ㅎㅎ",
          "original_form": "잘 못타는데 좋아는해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000608.1.23",
          "form": "황리단길에 카페들도 많아요",
          "original_form": "황리단길에 카페들도 많아요",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000608.1.24",
          "form": "다음에 가시면 카페투어 하셔도 재밌으실거에요",
          "original_form": "다음에 가시면 카페투어 하셔도 재밌으실거에요",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000608.1.25",
          "form": "좋아하면 잘타시는거에요ㅋㅋ",
          "original_form": "좋아하면 잘타시는거에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000608.1.26",
          "form": "오호! 카페도 찾아봐야겠네요",
          "original_form": "오호! 카페도 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000608.1.27",
          "form": "여행가면 그 지역 카페 찾아가는거도 너무 재밌는거같아요",
          "original_form": "여행가면 그 지역 카페 찾아가는거도 너무 재밌는거같아요",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000608.1.28",
          "form": "물론 뚜벅이들은 가기 힘들지만 ㅠㅠ",
          "original_form": "물론 뚜벅이들은 가기 힘들지만 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000608.1.29",
          "form": "마자요 지역마다 카페도 진짜 다양하게 있더라구요",
          "original_form": "마자요 지역마다 카페도 진짜 다양하게 있더라구요",
          "speaker_id": "2",
          "time": "20210819 18:13"
        },
        {
          "id": "MDRW2100000608.1.30",
          "form": "뚜벅이는 힘들죠ㅠ",
          "original_form": "뚜벅이는 힘들죠ㅠ",
          "speaker_id": "2",
          "time": "20210819 18:13"
        },
        {
          "id": "MDRW2100000608.1.31",
          "form": "빨리 차를 사서 로드트립도 가보고싶어요!",
          "original_form": "빨리 차를 사서 로드트립도 가보고싶어요!",
          "speaker_id": "1",
          "time": "20210819 18:14"
        }
      ]
    }
