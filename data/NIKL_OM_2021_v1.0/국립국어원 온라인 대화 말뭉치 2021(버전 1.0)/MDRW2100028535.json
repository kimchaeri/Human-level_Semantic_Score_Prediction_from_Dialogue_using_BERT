      "utterance": [
        {
          "id": "MDRW2100028535.1.1",
          "form": "건강하시죠",
          "original_form": "건강하시죠",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028535.1.2",
          "form": "스트레스 극복법을",
          "original_form": "스트레스 극복법을",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028535.1.3",
          "form": "어릴때 체득해서",
          "original_form": "어릴때 체득해서",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028535.1.4",
          "form": "아 근데 혈압이 좀 있어서",
          "original_form": "아 근데 혈압이 좀 있어서",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028535.1.5",
          "form": "살 빼긴 해야 되는데;;",
          "original_form": "살 빼긴 해야 되는데;;",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028535.1.6",
          "form": "혈압은 약 먹으면 돼요",
          "original_form": "혈압은 약 먹으면 돼요",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028535.1.7",
          "form": "근데 젊을때 약먹으면",
          "original_form": "근데 젊을때 약먹으면",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028535.1.8",
          "form": "평생 먹어야 된다...",
          "original_form": "평생 먹어야 된다...",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028535.1.9",
          "form": "라는 그런",
          "original_form": "라는 그런",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028535.1.10",
          "form": "이미지가 있지 않나요",
          "original_form": "이미지가 있지 않나요",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028535.1.11",
          "form": "그래도 죽지는 않잖아요",
          "original_form": "그래도 죽지는 않잖아요",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028535.1.12",
          "form": "그런가...?",
          "original_form": "그런가...?",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028535.1.13",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028535.1.14",
          "form": "식단 조절 아직 하시나요",
          "original_form": "식단 조절 아직 하시나요",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028535.1.15",
          "form": "전 다이어트에 성공했답니다",
          "original_form": "전 다이어트에 성공했답니다",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028535.1.16",
          "form": "아 치사해...",
          "original_form": "아 치사해...",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028535.1.17",
          "form": "성형하셨나요?",
          "original_form": "성형하셨나요?",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028535.1.18",
          "form": "음??????",
          "original_form": "음??????",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028535.1.19",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028535.1.20",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028535.1.21",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028535.1.22",
          "form": "라식이랑",
          "original_form": "라식이랑",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028535.1.23",
          "form": "라식 친구",
          "original_form": "라식 친구",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028535.1.24",
          "form": "관심 있긴한데",
          "original_form": "관심 있긴한데",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028535.1.25",
          "form": "뭐지?",
          "original_form": "뭐지?",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028535.1.26",
          "form": "라식말고 다른 그거 했는데 엄청 편하답니다",
          "original_form": "라식말고 다른 그거 했는데 엄청 편하답니다",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028535.1.27",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028535.1.28",
          "form": "콘텍트 렌즈도",
          "original_form": "콘텍트 렌즈도",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028535.1.29",
          "form": "안 껴봐서;;",
          "original_form": "안 껴봐서;;",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028535.1.30",
          "form": "그렇다면 미용을 위해서 이번에 도전해 보세요",
          "original_form": "그렇다면 미용을 위해서 이번에 도전해 보세요",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028535.1.31",
          "form": "미용 생각하면",
          "original_form": "미용 생각하면",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028535.1.32",
          "form": "다이어트가 선행되어야 할 거 같긴 한디..",
          "original_form": "다이어트가 선행되어야 할 거 같긴 한디..",
          "speaker_id": "1",
          "time": "20210902 20:59"
        }
      ]
