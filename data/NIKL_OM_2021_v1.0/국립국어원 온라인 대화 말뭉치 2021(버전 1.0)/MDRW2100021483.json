      "utterance": [
        {
          "id": "MDRW2100021483.1.1",
          "form": "최근에 여행 가신 적 있으세요?",
          "original_form": "최근에 여행 가신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021483.1.2",
          "form": "아뇨 못가고  있어요",
          "original_form": "아뇨 못가고  있어요",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021483.1.3",
          "form": "요즘은 어렵죠ㅜ",
          "original_form": "요즘은 어렵죠ㅜ",
          "speaker_id": "1",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021483.1.4",
          "form": "코로나 끝나면 그럼 여행 가고 싶은 데 있으세요?",
          "original_form": "코로나 끝나면 그럼 여행 가고 싶은 데 있으세요?",
          "speaker_id": "1",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021483.1.5",
          "form": "경기도 쪽 생각중이에요",
          "original_form": "경기도 쪽 생각중이에요",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100021483.1.6",
          "form": "name1은 어디 가고 싶으세요?",
          "original_form": "&name1&은 어디 가고 싶으세요?",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021483.1.7",
          "form": "전 해외요 ㅋㅋ",
          "original_form": "전 해외요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021483.1.8",
          "form": "하와이 가구 싶어요",
          "original_form": "하와이 가구 싶어요",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021483.1.9",
          "form": "하와이 관광지로 유명하죠",
          "original_form": "하와이 관광지로 유명하죠",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100021483.1.10",
          "form": "전 이집트 한 번 가보고 싶어요",
          "original_form": "전 이집트 한 번 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100021483.1.11",
          "form": "이집트도 좋죠",
          "original_form": "이집트도 좋죠",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100021483.1.12",
          "form": "이집트에서도 보고 싶은 거 있으세요?",
          "original_form": "이집트에서도 보고 싶은 거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100021483.1.13",
          "form": "전 피라미드 진짜 얼마나 큰지 가서 실제로 보고싶어요",
          "original_form": "전 피라미드 진짜 얼마나 큰지 가서 실제로 보고싶어요",
          "speaker_id": "1",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021483.1.14",
          "form": "저두요 피라미드 그리고 신전도 보고 싶어요",
          "original_form": "저두요 피라미드 그리고 신전도 보고 싶어요",
          "speaker_id": "2",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021483.1.15",
          "form": "저 최근에 이집트 관련 다큐 봤는데 흥미진진하더라구요 ㅋㅋㅋ",
          "original_form": "저 최근에 이집트 관련 다큐 봤는데 흥미진진하더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100021483.1.16",
          "form": "화면ㅇ로만 봐도 멋져서 실제로 보면 더 멋있을 것 같아요",
          "original_form": "화면ㅇ로만 봐도 멋져서 실제로 보면 더 멋있을 것 같아요",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100021483.1.17",
          "form": "이집트 말고 다른 곳도 가고싶은 데 있으세요??",
          "original_form": "이집트 말고 다른 곳도 가고싶은 데 있으세요??",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100021483.1.18",
          "form": "음... 대만이요 문구류가 좋더라고요",
          "original_form": "음... 대만이요 문구류가 좋더라고요",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100021483.1.19",
          "form": "헝 저두 대만이요 ㅋㅋ 근데 저는 먹으러 ㅋㅋ",
          "original_form": "헝 저두 대만이요 ㅋㅋ 근데 저는 먹으러 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100021483.1.20",
          "form": "대만 음식 맛있다고 하더라고요",
          "original_form": "대만 음식 맛있다고 하더라고요",
          "speaker_id": "2",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021483.1.21",
          "form": "국내에도 대만식 유행 마니 하잖아요",
          "original_form": "국내에도 대만식 유행 마니 하잖아요",
          "speaker_id": "2",
          "time": "20210828 20:08"
        },
        {
          "id": "MDRW2100021483.1.22",
          "form": "네 ㅋㅋㅋㅋ 너무 맛있는 거 많아보여서 국내에선 먹으러 가는데 그래서 현지 음식으로 먹어보고 싶어졌어요 ㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ 너무 맛있는 거 많아보여서 국내에선 먹으러 가는데 그래서 현지 음식으로 먹어보고 싶어졌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100021483.1.23",
          "form": "현지 음식하니까 베트남 쌀국수 생각나네요",
          "original_form": "현지 음식하니까 베트남 쌀국수 생각나네요",
          "speaker_id": "2",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100021483.1.24",
          "form": "전 고수 못 먹어서요 고수 잘 드시나요?",
          "original_form": "전 고수 못 먹어서요 고수 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100021483.1.25",
          "form": "넹 저는 먹어요 ㅋㅋㅋ 꼭 고수 추가!",
          "original_form": "넹 저는 먹어요 ㅋㅋㅋ 꼭 고수 추가!",
          "speaker_id": "1",
          "time": "20210828 20:16"
        }
      ]
