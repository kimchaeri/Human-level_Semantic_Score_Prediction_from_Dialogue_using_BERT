      "utterance": [
        {
          "id": "MDRW2100014639.1.1",
          "form": "우연이네요. 저도 마찬가지예요",
          "original_form": "우연이네요. 저도 마찬가지예요",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014639.1.2",
          "form": "비혼주의자가 된 배경이 있나요",
          "original_form": "비혼주의자가 된 배경이 있나요",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014639.1.3",
          "form": "일단 남자를 안좋아합니다",
          "original_form": "일단 남자를 안좋아합니다",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014639.1.4",
          "form": "아 명료한 이유네요",
          "original_form": "아 명료한 이유네요",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014639.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014639.1.6",
          "form": "성애적 감정을 느낀적이 없어서요",
          "original_form": "성애적 감정을 느낀적이 없어서요",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014639.1.7",
          "form": "첫사랑이 뭔가요 이러죠",
          "original_form": "첫사랑이 뭔가요 이러죠",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014639.1.8",
          "form": "그렇군요 이상형은 있나요?",
          "original_form": "그렇군요 이상형은 있나요?",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014639.1.9",
          "form": "딱히 사귈 생각을 안해봤으니 있을 턱이요",
          "original_form": "딱히 사귈 생각을 안해봤으니 있을 턱이요",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014639.1.10",
          "form": "아 연예인 옹성우를 좋아하는데 이걸 이상형으로 쳐야할까요",
          "original_form": "아 연예인 옹성우를 좋아하는데 이걸 이상형으로 쳐야할까요",
          "speaker_id": "2",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014639.1.11",
          "form": "name1님은 이상형이 뭔가요",
          "original_form": "&name1&님은 이상형이 뭔가요",
          "speaker_id": "2",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014639.1.12",
          "form": "음 글쎄요",
          "original_form": "음 글쎄요",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014639.1.13",
          "form": "혼자서도 잘지내는 사람이요",
          "original_form": "혼자서도 잘지내는 사람이요",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014639.1.14",
          "form": "아ㅋㅋ 저 그 생각해본 적은 있어요",
          "original_form": "아ㅋㅋ 저 그 생각해본 적은 있어요",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014639.1.15",
          "form": "애인이 군인이면 좋겠다고",
          "original_form": "애인이 군인이면 좋겠다고",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014639.1.16",
          "form": "미팅 해본적있으세요?",
          "original_form": "미팅 해본적있으세요?",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014639.1.17",
          "form": "미팅도 소개팅도 해봤죠",
          "original_form": "미팅도 소개팅도 해봤죠",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014639.1.18",
          "form": "근데 낯선 이성과 만나는게 너무 스트레스더라고요",
          "original_form": "근데 낯선 이성과 만나는게 너무 스트레스더라고요",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014639.1.19",
          "form": "그런데 낯선 이성과 만나는게 너무 불편하더라고요",
          "original_form": "그런데 낯선 이성과 만나는게 너무 불편하더라고요",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014639.1.20",
          "form": "name1님은 어떠셨나요",
          "original_form": "&name1&님은 어떠셨나요",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014639.1.21",
          "form": "해본적없어요",
          "original_form": "해본적없어요",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014639.1.22",
          "form": "같은 이유로요",
          "original_form": "같은 이유로요",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014639.1.23",
          "form": "모르는 사람 만나는게 스트레스예요",
          "original_form": "모르는 사람 만나는게 스트레스예요",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014639.1.24",
