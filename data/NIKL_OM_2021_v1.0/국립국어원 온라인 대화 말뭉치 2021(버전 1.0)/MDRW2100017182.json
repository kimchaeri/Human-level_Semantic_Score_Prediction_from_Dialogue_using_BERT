      "utterance": [
        {
          "id": "MDRW2100017182.1.1",
          "form": "주제를 좀 바꿔볼까요",
          "original_form": "주제를 좀 바꿔볼까요",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017182.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017182.1.3",
          "form": "최근 시청하신 드라마 있으신가요",
          "original_form": "최근 시청하신 드라마 있으신가요",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017182.1.4",
          "form": "전 드라마를 거의 안봐요",
          "original_form": "전 드라마를 거의 안봐요",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.5",
          "form": "아 딱 하나",
          "original_form": "아 딱 하나",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.6",
          "form": "중드 봤어요",
          "original_form": "중드 봤어요",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.7",
          "form": "어떤거보셨나요",
          "original_form": "어떤거보셨나요",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.8",
          "form": "겨우서른",
          "original_form": "겨우서른",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.9",
          "form": "아 그거도 봤다",
          "original_form": "아 그거도 봤다",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.10",
          "form": "검블유?",
          "original_form": "검블유?",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.11",
          "form": "근데 보다 말았어요",
          "original_form": "근데 보다 말았어요",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.12",
          "form": "그거 재밌어요",
          "original_form": "그거 재밌어요",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.13",
          "form": "name1님은 뭐 보셨어요?",
          "original_form": "&name1&님은 뭐 보셨어요?",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.14",
          "form": "배타미..앗..",
          "original_form": "배타미..앗..",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.15",
          "form": "검블유요?",
          "original_form": "검블유요?",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.16",
          "form": "오 ㅎㅎㅎㅎ 드라마 보세요?",
          "original_form": "오 ㅎㅎㅎㅎ 드라마 보세요?",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.17",
          "form": "아 전 검블유가...",
          "original_form": "아 전 검블유가...",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017182.1.18",
          "form": "네 드라마 자주보는 편이에요",
          "original_form": "네 드라마 자주보는 편이에요",
          "speaker_id": "1",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017182.1.19",
          "form": "남자분이 넘 부담스러워요",
          "original_form": "남자분이 넘 부담스러워요",
          "speaker_id": "2",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017182.1.20",
          "form": "그 대사는 좋은데",
          "original_form": "그 대사는 좋은데",
          "speaker_id": "2",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017182.1.21",
          "form": "그건그래요",
          "original_form": "그건그래요",
          "speaker_id": "1",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017182.1.22",
          "form": "제가봐도 좀 그래요",
          "original_form": "제가봐도 좀 그래요",
          "speaker_id": "1",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017182.1.23",
          "form": "남자배우님이 소화를 못하는 건지",
          "original_form": "남자배우님이 소화를 못하는 건지",
          "speaker_id": "2",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017182.1.24",
          "form": "연기가.. 씁..",
          "original_form": "연기가.. 씁..",
          "speaker_id": "1",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017182.1.25",
          "form": "아님 너무 문학체 느낌인건지",
          "original_form": "아님 너무 문학체 느낌인건지",
          "speaker_id": "2",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100017182.1.26",
          "form": "연기가..문제같더라는..",
          "original_form": "연기가..문제같더라는..",
