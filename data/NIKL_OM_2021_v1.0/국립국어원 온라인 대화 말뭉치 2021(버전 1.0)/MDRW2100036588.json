        },
        {
          "id": "MDRW2100036588.1.2",
          "form": "내가 보는 고양이 유튜브가 있는데",
          "original_form": "내가 보는 고양이 유튜브가 있는데",
          "speaker_id": "1",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100036588.1.3",
          "form": "누구 보는데",
          "original_form": "누구 보는데",
          "speaker_id": "2",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100036588.1.4",
          "form": "전에 말한 적 있긴 한데 하하하",
          "original_form": "전에 말한 적 있긴 한데 하하하",
          "speaker_id": "1",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100036588.1.5",
          "form": "아들은 적이 있지",
          "original_form": "아들은 적이 있지",
          "speaker_id": "2",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100036588.1.6",
          "form": "오늘도 영상이 올라왔어",
          "original_form": "오늘도 영상이 올라왔어",
          "speaker_id": "1",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100036588.1.7",
          "form": "오늘 거 재밌어?",
          "original_form": "오늘 거 재밌어?",
          "speaker_id": "2",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100036588.1.8",
          "form": "내가 가장 좋아하는 고양이가 나와",
          "original_form": "내가 가장 좋아하는 고양이가 나와",
          "speaker_id": "1",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100036588.1.9",
          "form": "대장 고양이말하는구나",
          "original_form": "대장 고양이말하는구나",
          "speaker_id": "2",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100036588.1.10",
          "form": "그런데 친구 고양이랑 싸우더라",
          "original_form": "그런데 친구 고양이랑 싸우더라",
          "speaker_id": "1",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100036588.1.11",
          "form": "재미있겠다 영상",
          "original_form": "재미있겠다 영상",
          "speaker_id": "2",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100036588.1.12",
          "form": "고양이가 화내는 거 처음 들었어",
          "original_form": "고양이가 화내는 거 처음 들었어",
          "speaker_id": "1",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100036588.1.13",
          "form": "아기 울음소리 같이 하지 않나",
          "original_form": "아기 울음소리 같이 하지 않나",
          "speaker_id": "2",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100036588.1.14",
          "form": "맞아 목소리도 귀엽더라",
          "original_form": "맞아 목소리도 귀엽더라",
          "speaker_id": "1",
