        },
        {
          "id": "MDRW2100026181.1.2",
          "form": "최근에 뭐산거있어?",
          "original_form": "최근에 뭐산거있어?",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026181.1.3",
          "form": "나는 최근 휴지삼",
          "original_form": "나는 최근 휴지삼",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026181.1.4",
          "form": "휴지 어디께 좋아?",
          "original_form": "휴지 어디께 좋아?",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026181.1.5",
          "form": "크리넥스최고",
          "original_form": "크리넥스최고",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026181.1.6",
          "form": "역시크리넥스인가?.",
          "original_form": "역시크리넥스인가?.",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026181.1.7",
          "form": "지마켓에서세일해",
          "original_form": "지마켓에서세일해",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026181.1.8",
          "form": "크리넥스가 저렴하니더좋네",
          "original_form": "크리넥스가 저렴하니더좋네",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026181.1.9",
          "form": "응ㅋㅋㅋ난무조건3겹으로삼",
          "original_form": "응ㅋㅋㅋ난무조건3겹으로삼",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026181.1.10",
          "form": "맞아ㅋㅋ부들부들좋음",
          "original_form": "맞아ㅋㅋ부들부들좋음",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026181.1.11",
          "form": "휴지는쟁겨놔야제맛ㅋ",
          "original_form": "휴지는쟁겨놔야제맛ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026181.1.12",
          "form": "ㅋㅋ난물건쟁기기좋아함ㅋ",
          "original_form": "ㅋㅋ난물건쟁기기좋아함ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026181.1.13",
          "form": "나도야ㅋㅋ유통기한지나서 버린것도많ㅇㄱㆍ",
