        },
        {
          "id": "MDRW2100040777.1.2",
          "form": "음음",
          "original_form": "음음",
          "speaker_id": "1",
          "time": "20210905 04:34"
        },
        {
          "id": "MDRW2100040777.1.3",
          "form": "항암제같은 약을",
          "original_form": "항암제같은 약을",
          "speaker_id": "1",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.4",
          "form": "타겟까지 안전하게",
          "original_form": "타겟까지 안전하게",
          "speaker_id": "1",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.5",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.6",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.7",
          "form": "모셔다드릴 수도 있고",
          "original_form": "모셔다드릴 수도 있고",
          "speaker_id": "1",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.9",
          "form": "네네 사실 그런거죠 약물을 잘 전달하기",
          "original_form": "네네 사실 그런거죠 약물을 잘 전달하기",
          "speaker_id": "1",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.10",
          "form": "워",
          "original_form": "워",
          "speaker_id": "2",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.11",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "2",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.12",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.13",
          "form": "근데 대학원에서 하는 연구가",
          "original_form": "근데 대학원에서 하는 연구가",
          "speaker_id": "1",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.14",
          "form": "대부분",
          "original_form": "대부분",
          "speaker_id": "1",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.15",
          "form": "뭔가 이런 느낌이긴 해욬ㅋㅋㅋㅋ",
          "original_form": "뭔가 이런 느낌이긴 해욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.16",
          "form": "말만 들으면",
          "original_form": "말만 들으면",
          "speaker_id": "1",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.17",
          "form": "헐 너무",
          "original_form": "헐 너무",
          "speaker_id": "2",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.18",
          "form": "멋져요",
          "original_form": "멋져요",
          "speaker_id": "2",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.19",
          "form": "진짜 다 대단해보이는...",
          "original_form": "진짜 다 대단해보이는...",
          "speaker_id": "1",
          "time": "20210905 04:35"
        },
        {
          "id": "MDRW2100040777.1.20",
          "form": "근데 이게 실제로",
          "original_form": "근데 이게 실제로",
          "speaker_id": "1",
          "time": "20210905 04:36"
        },
        {
          "id": "MDRW2100040777.1.21",
          "form": "ㅋ큐큐큐큐큐ㅠ",
          "original_form": "ㅋ큐큐큐큐큐ㅠ",
          "speaker_id": "2",
          "time": "20210905 04:36"
        },
        {
          "id": "MDRW2100040777.1.22",
          "form": "상용화 되려면",
          "original_form": "상용화 되려면",
          "speaker_id": "1",
          "time": "20210905 04:36"
        },
        {
          "id": "MDRW2100040777.1.23",
          "form": "아니그리고 그걸 말로 일반인인 저한테 쉽게설명해쥬시는거 자체가 너무 멋지신거같은데요",
          "original_form": "아니그리고 그걸 말로 일반인인 저한테 쉽게설명해쥬시는거 자체가 너무 멋지신거같은데요",
          "speaker_id": "2",
          "time": "20210905 04:36"
        },
        {
          "id": "MDRW2100040777.1.24",
          "form": "정말 많은 연구가 더 필요하고... 인력이 필요하고",
          "original_form": "정말 많은 연구가 더 필요하고... 인력이 필요하고",
