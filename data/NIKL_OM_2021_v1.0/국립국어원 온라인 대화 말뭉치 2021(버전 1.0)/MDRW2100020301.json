      "utterance": [
        {
          "id": "MDRW2100020301.1.1",
          "form": "저는 이미 그저께해서",
          "original_form": "저는 이미 그저께해서",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020301.1.2",
          "form": "없습니다ㅎㅎ",
          "original_form": "없습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020301.1.3",
          "form": "부지런하시네요",
          "original_form": "부지런하시네요",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020301.1.4",
          "form": "식구 많아서 빨래많겠어요",
          "original_form": "식구 많아서 빨래많겠어요",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020301.1.5",
          "form": "5인인가요?",
          "original_form": "5인인가요?",
          "speaker_id": "2",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020301.1.6",
          "form": "4인인데용",
          "original_form": "4인인데용",
          "speaker_id": "1",
          "time": "20210828 01:11"
        },
        {
          "id": "MDRW2100020301.1.7",
          "form": "글쿤요",
          "original_form": "글쿤요",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020301.1.8",
          "form": "일주일에 얼마나해요",
          "original_form": "일주일에 얼마나해요",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020301.1.9",
          "form": "빨래",
          "original_form": "빨래",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100020301.1.10",
          "form": "주말에 두번돌려요",
          "original_form": "주말에 두번돌려요",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020301.1.11",
          "form": "토요일 흰 일요일 검정",
          "original_form": "토요일 흰 일요일 검정",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020301.1.12",
          "form": "근데 저번주 일욜날 비와버림!",
          "original_form": "근데 저번주 일욜날 비와버림!",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100020301.1.13",
          "form": "ㅋㅋㅋㅋ비와도해야죠 뭐",
          "original_form": "ㅋㅋㅋㅋ비와도해야죠 뭐",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020301.1.14",
          "form": "일욜날 놀러간다면서요",
          "original_form": "일욜날 놀러간다면서요",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020301.1.15",
          "form": "놀러가면 검정 빨래는 누가하죠",
          "original_form": "놀러가면 검정 빨래는 누가하죠",
          "speaker_id": "2",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100020301.1.16",
          "form": "오늘흰빨래돌려놔서",
          "original_form": "오늘흰빨래돌려놔서",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020301.1.17",
          "form": "낼 검정돌리면됩니다",
          "original_form": "낼 검정돌리면됩니다",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020301.1.18",
          "form": "휴가를 괜히 낸게아님",
          "original_form": "휴가를 괜히 낸게아님",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020301.1.19",
          "form": "세탁기돌릴려고 휴가냄",
          "original_form": "세탁기돌릴려고 휴가냄",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100020301.1.20",
          "form": "ㅋㅋㅋㅋ거창한 이유네",
          "original_form": "ㅋㅋㅋㅋ거창한 이유네",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020301.1.21",
          "form": "빨래 수건 따로 돌리죠?",
          "original_form": "빨래 수건 따로 돌리죠?",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020301.1.22",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020301.1.23",
          "form": "아뇨 귀찮아서 한꺼번에 돌리는데요",
          "original_form": "아뇨 귀찮아서 한꺼번에 돌리는데요",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020301.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020301.1.25",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020301.1.26",
          "form": "그래서 저희집 수건 까슬까슬함",
          "original_form": "그래서 저희집 수건 까슬까슬함",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020301.1.27",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020301.1.28",
          "form": "약간 사포같아요",
          "original_form": "약간 사포같아요",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020301.1.29",
          "form": "빨.래는 섬유유연제",
          "original_form": "빨.래는 섬유유연제",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020301.1.30",
          "form": "쓰면안대여",
          "original_form": "쓰면안대여",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020301.1.31",
          "form": "ㅋㅋㅋㅋ ㅋ ㅋ ㅋ  ㅋㅋ ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ ㅋ ㅋ ㅋ  ㅋㅋ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020301.1.32",
          "form": "물 흡수 못 한다고 그랬음",
          "original_form": "물 흡수 못 한다고 그랬음",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020301.1.33",
          "form": "근데 섬유유연제 냄새없으면",
          "original_form": "근데 섬유유연제 냄새없으면",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020301.1.34",
          "form": "섭섭하잖아요",
          "original_form": "섭섭하잖아요",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020301.1.35",
          "form": "ㅋㅋㅋ안된다니까요",
          "original_form": "ㅋㅋㅋ안된다니까요",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020301.1.36",
          "form": "향만좋고",
          "original_form": "향만좋고",
          "speaker_id": "2",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020301.1.37",
          "form": "흡수안되면 그게 뭔 수건임",
          "original_form": "흡수안되면 그게 뭔 수건임",
          "speaker_id": "2",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020301.1.38",
          "form": "향나는 사포 되는거죠뭐",
          "original_form": "향나는 사포 되는거죠뭐",
          "speaker_id": "1",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020301.1.39",
          "form": "다음에 수건사면",
          "original_form": "다음에 수건사면",
          "speaker_id": "1",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020301.1.40",
          "form": "그땐 섬유유연제안넣어볼게요",
          "original_form": "그땐 섬유유연제안넣어볼게요",
          "speaker_id": "1",
          "time": "20210828 01:21"
        }
      ]
