        {
          "id": "MDRW2100032937.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 09:29"
        },
        {
          "id": "MDRW2100032937.1.2",
          "form": "아침먹었나요",
          "original_form": "아침먹었나요",
          "speaker_id": "1",
          "time": "20210906 09:29"
        },
        {
          "id": "MDRW2100032937.1.3",
          "form": "네 카레 먹었어요",
          "original_form": "네 카레 먹었어요",
          "speaker_id": "2",
          "time": "20210906 10:08"
        },
        {
          "id": "MDRW2100032937.1.4",
          "form": "당근 들어있었나요?",
          "original_form": "당근 들어있었나요?",
          "speaker_id": "1",
          "time": "20210906 10:09"
        },
        {
          "id": "MDRW2100032937.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210906 10:16"
        },
        {
          "id": "MDRW2100032937.1.6",
          "form": "ㅋㅋ전 당근안좋아해요ㅠㅠ",
          "original_form": "ㅋㅋ전 당근안좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 10:20"
        },
        {
          "id": "MDRW2100032937.1.7",
          "form": "저도 딱히 좋아하는 건 아닌데",
          "original_form": "저도 딱히 좋아하는 건 아닌데",
          "speaker_id": "2",
          "time": "20210906 10:22"
        },
        {
          "id": "MDRW2100032937.1.8",
          "form": "카레에 있는건 그래도 괜찮더라고요",
          "original_form": "카레에 있는건 그래도 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210906 10:22"
        },
        {
          "id": "MDRW2100032937.1.9",
          "form": "맞아요 그나마 향이 좀 약해지는 기분이에요",
          "original_form": "맞아요 그나마 향이 좀 약해지는 기분이에요",
          "speaker_id": "1",
          "time": "20210906 10:25"
        },
        {
          "id": "MDRW2100032937.1.10",
          "form": "저는 아침에 떡먹었어요",
          "original_form": "저는 아침에 떡먹었어요",
          "speaker_id": "1",
          "time": "20210906 10:26"
        },
        {
          "id": "MDRW2100032937.1.11",
          "form": "무슨 떡이요??",
          "original_form": "무슨 떡이요??",
          "speaker_id": "2",
          "time": "20210906 10:27"
        },
        {
          "id": "MDRW2100032937.1.12",
          "form": "백설기요 ㅋㅋ",
          "original_form": "백설기요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 10:29"
        },
        {
          "id": "MDRW2100032937.1.13",
          "form": "맛있었겠네요...",
          "original_form": "맛있었겠네요...",
          "speaker_id": "2",
          "time": "20210906 11:17"
        },
        {
          "id": "MDRW2100032937.1.14",
          "form": "백설기에 콩 좋아하시나요??",
          "original_form": "백설기에 콩 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210906 11:18"
        },
        {
          "id": "MDRW2100032937.1.15",
          "form": "ㅋㅋ아니요 저 콩을안좋아하는데 다행히 오늘먹은건 콩없이 하얀거였어요!!",
          "original_form": "ㅋㅋ아니요 저 콩을안좋아하는데 다행히 오늘먹은건 콩없이 하얀거였어요!!",
          "speaker_id": "1",
          "time": "20210906 11:54"
        },
        {
          "id": "MDRW2100032937.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100032937.1.17",
          "form": "이제 곧 점심도 드시겠네요",
          "original_form": "이제 곧 점심도 드시겠네요",
          "speaker_id": "2",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100032937.1.18",
          "form": "네 점심은 뭐 드실거에요?",
          "original_form": "네 점심은 뭐 드실거에요?",
          "speaker_id": "1",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100032937.1.19",
          "form": "점심은 닭계장이네요",
          "original_form": "점심은 닭계장이네요",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100032937.1.20",
          "form": "저는 모둠 돈까스요 ㅋㅋ 새우튀김이랑 돈가스랑 생선가스요",
          "original_form": "저는 모둠 돈까스요 ㅋㅋ 새우튀김이랑 돈가스랑 생선가스요",
