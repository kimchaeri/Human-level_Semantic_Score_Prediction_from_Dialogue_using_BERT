      "utterance": [
        {
          "id": "MDRW2100027985.1.1",
          "form": "그랬나요? 어이가 없네요",
          "original_form": "그랬나요? 어이가 없네요",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027985.1.2",
          "form": "울던데요",
          "original_form": "울던데요",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027985.1.3",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027985.1.4",
          "form": "아이돌 춤이라고",
          "original_form": "아이돌 춤이라고",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027985.1.5",
          "form": "자기들은",
          "original_form": "자기들은",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027985.1.6",
          "form": "어렸을때 춰봤다며",
          "original_form": "어렸을때 춰봤다며",
          "speaker_id": "1",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027985.1.7",
          "form": "ㅋㅋ 그냥 하차해야겠네요",
          "original_form": "ㅋㅋ 그냥 하차해야겠네요",
          "speaker_id": "2",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027985.1.8",
          "form": "그러게요 왜 거기 나가서",
          "original_form": "그러게요 왜 거기 나가서",
          "speaker_id": "1",
          "time": "20210902 16:40"
        },
        {
          "id": "MDRW2100027985.1.9",
          "form": "무시당하는지",
          "original_form": "무시당하는지",
          "speaker_id": "1",
          "time": "20210902 16:40"
        },
        {
          "id": "MDRW2100027985.1.10",
          "form": "저도 재방송할때 꼭 봐야겠네요 왜그러는지",
          "original_form": "저도 재방송할때 꼭 봐야겠네요 왜그러는지",
          "speaker_id": "2",
          "time": "20210902 16:43"
        },
        {
          "id": "MDRW2100027985.1.11",
          "form": "오늘은 뭐 재밌는거 안하나요",
          "original_form": "오늘은 뭐 재밌는거 안하나요",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027985.1.12",
          "form": "그러게요 평일에는 잘모르겠네요 예능은 주말예능이 재밌다보니",
          "original_form": "그러게요 평일에는 잘모르겠네요 예능은 주말예능이 재밌다보니",
          "speaker_id": "2",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100027985.1.13",
          "form": "오늘 축구하던가요",
          "original_form": "오늘 축구하던가요",
          "speaker_id": "1",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100027985.1.14",
          "form": "그런가봐요 8시에 이라크와 경기있다네요",
          "original_form": "그런가봐요 8시에 이라크와 경기있다네요",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100027985.1.15",
          "form": "오 치킨 먹고파요",
          "original_form": "오 치킨 먹고파요",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100027985.1.16",
          "form": "저는 피자요 축구경기보면서 먹고싶은데 주문을 못받나봐요ㅜ",
          "original_form": "저는 피자요 축구경기보면서 먹고싶은데 주문을 못받나봐요ㅜ",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027985.1.17",
          "form": "피자 할인하나요??",
          "original_form": "피자 할인하나요??",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027985.1.18",
          "form": "부디 잘먹기를 바라며",
          "original_form": "부디 잘먹기를 바라며",
          "speaker_id": "1",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100027985.1.19",
          "form": "티비에 축구해요",
          "original_form": "티비에 축구해요",
          "speaker_id": "1",
          "time": "20210902 19:57"
        }
      ]
