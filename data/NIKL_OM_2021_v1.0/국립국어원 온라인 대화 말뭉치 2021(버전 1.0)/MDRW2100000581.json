      "utterance": [
        {
          "id": "MDRW2100000581.1.1",
          "form": "오늘저녁 카레드시나요",
          "original_form": "오늘저녁 카레드시나요",
          "speaker_id": "1",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000581.1.2",
          "form": "네 ㅎ 카레 얼른 해서 먹고 집앞 카페 갈까봐요",
          "original_form": "네 ㅎ 카레 얼른 해서 먹고 집앞 카페 갈까봐요",
          "speaker_id": "2",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000581.1.3",
          "form": "빙수 먹으러",
          "original_form": "빙수 먹으러",
          "speaker_id": "2",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000581.1.4",
          "form": "저녁엔 좀 선선해졌지요?",
          "original_form": "저녁엔 좀 선선해졌지요?",
          "speaker_id": "2",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000581.1.5",
          "form": "ㅋㅋㅋㅋ오 카레 무슨카레하세요 전 무조건 돼지고기엄청넣어요",
          "original_form": "ㅋㅋㅋㅋ오 카레 무슨카레하세요 전 무조건 돼지고기엄청넣어요",
          "speaker_id": "1",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000581.1.6",
          "form": "맞아요 날씨가 좀 시원해졌어요",
          "original_form": "맞아요 날씨가 좀 시원해졌어요",
          "speaker_id": "1",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000581.1.7",
          "form": "남편이 고기넣은거 좋아하고 전 스팸이 좋은데 ㅠㅠ",
          "original_form": "남편이 고기넣은거 좋아하고 전 스팸이 좋은데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000581.1.8",
          "form": "전 양파 많이 들어가면 달고 맛있더라고요 ㅎ",
          "original_form": "전 양파 많이 들어가면 달고 맛있더라고요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000581.1.9",
          "form": "카페상품권 쓸겸 가서 빙수랑 초코음료랑 ㅎ",
          "original_form": "카페상품권 쓸겸 가서 빙수랑 초코음료랑 ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000581.1.10",
          "form": "애들위주로다가 ㅋㅋㅋ",
          "original_form": "애들위주로다가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000581.1.11",
          "form": "맞아요 양파는 무조건!",
          "original_form": "맞아요 양파는 무조건!",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000581.1.12",
          "form": "마음은 예쁜 카페 가고 싶은데",
          "original_form": "마음은 예쁜 카페 가고 싶은데",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000581.1.13",
          "form": "ㅋㅋㅋㅋ오 프랜차이즈카페어디루가시나여",
          "original_form": "ㅋㅋㅋㅋ오 프랜차이즈카페어디루가시나여",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000581.1.14",
          "form": "이놈의 코로나 부들부들!!!!",
          "original_form": "이놈의 코로나 부들부들!!!!",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000581.1.15",
          "form": "집앞에 이디야가 있고 개인이 하는 카페가 두개 더 있어요",
          "original_form": "집앞에 이디야가 있고 개인이 하는 카페가 두개 더 있어요",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000581.1.16",
          "form": "오늘은 카페쿠폰쓸거라 이디야 가려고요",
          "original_form": "오늘은 카페쿠폰쓸거라 이디야 가려고요",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000581.1.17",
          "form": "어디 주로 가세요?",
          "original_form": "어디 주로 가세요?",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000581.1.18",
          "form": "오 이디야 맛있져",
          "original_form": "오 이디야 맛있져",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000581.1.19",
          "form": "전 집앞에 스벅이있어서 상품권쓸때는 스벅가고요",
          "original_form": "전 집앞에 스벅이있어서 상품권쓸때는 스벅가고요",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000581.1.20",
          "form": "평소에는 더 가까운 개인카페가요 ㅋㅋㅋㅋ",
          "original_form": "평소에는 더 가까운 개인카페가요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000581.1.21",
          "form": "부러워요 스벅 시골살아서 스벅가려면 차타고 한시간 달려야해요 ㅠ",
          "original_form": "부러워요 스벅 시골살아서 스벅가려면 차타고 한시간 달려야해요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000581.1.22",
          "form": "ㅜㅜ저희지역도 크진않고 소도시라",
          "original_form": "ㅜㅜ저희지역도 크진않고 소도시라",
          "speaker_id": "1",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000581.1.23",
          "form": "개인카페가 은근 매력있어요",
          "original_form": "개인카페가 은근 매력있어요",
          "speaker_id": "2",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000581.1.24",
          "form": "오늘 저녁은 뭐해드실거에요?",
          "original_form": "오늘 저녁은 뭐해드실거에요?",
          "speaker_id": "2",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000581.1.25",
          "form": "맞아요 일단 그집만의 매력이있는듯요",
          "original_form": "맞아요 일단 그집만의 매력이있는듯요",
          "speaker_id": "1",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000581.1.26",
          "form": "전 오늘 순두부찌개할려구여",
          "original_form": "전 오늘 순두부찌개할려구여",
          "speaker_id": "1",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000581.1.27",
          "form": "가까우면 카페 한사발 떠드릴텐데 ㅎ",
          "original_form": "가까우면 카페 한사발 떠드릴텐데 ㅎ",
          "speaker_id": "2",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000581.1.28",
          "form": "오! 우리 남편이 좋아하는거!",
          "original_form": "오! 우리 남편이 좋아하는거!",
          "speaker_id": "2",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000581.1.29",
          "form": "제가 끓임 밍밍해요 ㅡㅡ",
          "original_form": "제가 끓임 밍밍해요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000581.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ저랑 남편분취향이 비슷하네여",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저랑 남편분취향이 비슷하네여",
          "speaker_id": "1",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000581.1.31",
          "form": "저도 좋아는 하죠 잘못만드니까 그런거는 밖에서 사먹자주의 ㅋㅋ",
          "original_form": "저도 좋아는 하죠 잘못만드니까 그런거는 밖에서 사먹자주의 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000581.1.32",
          "form": "저는 그래서 새우젓을 살짝넣습니당",
          "original_form": "저는 그래서 새우젓을 살짝넣습니당",
          "speaker_id": "1",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000581.1.33",
          "form": "예전에 육개장도 끓이고 감자탕도 끓이고 감자치즈고로케도 집에서 만들었는데",
          "original_form": "예전에 육개장도 끓이고 감자탕도 끓이고 감자치즈고로케도 집에서 만들었는데",
          "speaker_id": "2",
          "time": "20210819 17:03"
        },
        {
          "id": "MDRW2100000581.1.34",
          "form": "오 잘하시는데여",
          "original_form": "오 잘하시는데여",
          "speaker_id": "1",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000581.1.35",
          "form": "그땐 자기취향아니라고 반응 없더니 요즘은 나이들어 국물땡긴다고 해달래요",
          "original_form": "그땐 자기취향아니라고 반응 없더니 요즘은 나이들어 국물땡긴다고 해달래요",
          "speaker_id": "2",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000581.1.36",
          "form": "근데 제가 파업선언\r\n부엌에 가는건 애들때문이지, \r\n남편 넌....얻어먹....",
          "original_form": "근데 제가 파업선언\r\n부엌에 가는건 애들때문이지, \r\n남편 넌....얻어먹....",
          "speaker_id": "2",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000581.1.37",
          "form": "손이 많이 가는것들은 잘해서가 아니라 주부흉내낸다고 도전한거였어요 ㅎ 밖에서 먹는게 제일 맛나요ㅠㅎ",
          "original_form": "손이 많이 가는것들은 잘해서가 아니라 주부흉내낸다고 도전한거였어요 ㅎ 밖에서 먹는게 제일 맛나요ㅠㅎ",
          "speaker_id": "2",
          "time": "20210819 17:05"
        },
        {
          "id": "MDRW2100000581.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ남편은 좋아하면 알아서 먹든지말든지 ㅎ \r\n맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ남편은 좋아하면 알아서 먹든지말든지 ㅎ \r\n맞아요",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000581.1.39",
          "form": "사먹는게 최고입니다",
          "original_form": "사먹는게 최고입니다",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000581.1.40",
          "form": "괜히 파는게아니여요",
          "original_form": "괜히 파는게아니여요",
          "speaker_id": "1",
          "time": "20210819 17:07"
        },
        {
          "id": "MDRW2100000581.1.41",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000581.1.42",
          "form": "저는 잘하지는못하고 좋아하능편이에요 ㅋㅋㅋ",
          "original_form": "저는 잘하지는못하고 좋아하능편이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000581.1.43",
          "form": "백선생님 없었으면....",
          "original_form": "백선생님 없었으면....",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000581.1.44",
          "form": "ㅋㅋㅋㅋㅋ모시고 싶은",
          "original_form": "ㅋㅋㅋㅋㅋ모시고 싶은",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000581.1.45",
          "form": "소스비법이 어마어머하더러구요",
          "original_form": "소스비법이 어마어머하더러구요",
          "speaker_id": "2",
          "time": "20210819 17:12"
        },
        {
          "id": "MDRW2100000581.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그니까요 그리고 진짜 쉽게쉽게 잘가르쳐주시구여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그니까요 그리고 진짜 쉽게쉽게 잘가르쳐주시구여",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000581.1.47",
          "form": "막옛날에는 이것도넣어라 저것도넣어라하는게많고 어 우리집에없는데 빼서만들면 맛이이상해지고그랬는데",
          "original_form": "막옛날에는 이것도넣어라 저것도넣어라하는게많고 어 우리집에없는데 빼서만들면 맛이이상해지고그랬는데",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000581.1.48",
          "form": "백쌤은 집에없으면없는대러 만드는법을가르쳐주니까 좋더라구요 ㅋㅋㅋ",
          "original_form": "백쌤은 집에없으면없는대러 만드는법을가르쳐주니까 좋더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000581.1.49",
          "form": "설탕 많이 넣으시더만요 ㅋㅋㅋ",
          "original_form": "설탕 많이 넣으시더만요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000581.1.50",
          "form": "설탕 투하",
          "original_form": "설탕 투하",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
