      "utterance": [
        {
          "id": "MDRW2100005098.1.1",
          "form": "여행 조아하세요?",
          "original_form": "여행 조아하세요?",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005098.1.2",
          "form": "네 코로나전에 여행많이다녔어요",
          "original_form": "네 코로나전에 여행많이다녔어요",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005098.1.3",
          "form": "name2님은여",
          "original_form": "&name2&님은여",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005098.1.4",
          "form": "저도 세계여행 다니는 거 좋아했는데 코로나 때문에 못 다니고 있어요ㅠㅠ",
          "original_form": "저도 세계여행 다니는 거 좋아했는데 코로나 때문에 못 다니고 있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005098.1.5",
          "form": "맞아요ㅜㅜㅜ어디가 가장좋으셨어요",
          "original_form": "맞아요ㅜㅜㅜ어디가 가장좋으셨어요",
          "speaker_id": "1",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005098.1.6",
          "form": "저는 볼리비아 제일 좋았어요",
          "original_form": "저는 볼리비아 제일 좋았어요",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005098.1.7",
          "form": "우유니사막 가고 싶었는데",
          "original_form": "우유니사막 가고 싶었는데",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005098.1.8",
          "form": "딱 거기 가려고 남미 여행 갔었거든요",
          "original_form": "딱 거기 가려고 남미 여행 갔었거든요",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005098.1.9",
          "form": "진짜 거기 볼거 그거밖에없잖아여 가는길힘들고ㅜ",
          "original_form": "진짜 거기 볼거 그거밖에없잖아여 가는길힘들고ㅜ",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005098.1.10",
          "form": "근데 물찬 우유니보는순간 마음이 차란",
          "original_form": "근데 물찬 우유니보는순간 마음이 차란",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005098.1.11",
          "form": "저도 남미에서 우유니가 가장 생각나요ㅜㅜ",
          "original_form": "저도 남미에서 우유니가 가장 생각나요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005098.1.12",
          "form": "맞아요ㅠㅠㅠㅋㅋㅋㅋ",
          "original_form": "맞아요ㅠㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005098.1.13",
          "form": "특히 저는 고산병 심해서",
          "original_form": "특히 저는 고산병 심해서",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005098.1.14",
          "form": "계속 약 먹고 숨 못 쉬고 그랬는데도",
          "original_form": "계속 약 먹고 숨 못 쉬고 그랬는데도",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005098.1.15",
          "form": "남미가 기억 남더라고요...",
          "original_form": "남미가 기억 남더라고요...",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005098.1.16",
          "form": "헉 저도요 ㅜㅜ제가갔을때 지카바이러스 유행했을때라",
          "original_form": "헉 저도요 ㅜㅜ제가갔을때 지카바이러스 유행했을때라",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005098.1.17",
          "form": "진짜 식겁했었는데 고산병이더라구여",
          "original_form": "진짜 식겁했었는데 고산병이더라구여",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005098.1.18",
          "form": "막심장조이고ㅜㅜ",
          "original_form": "막심장조이고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005098.1.19",
          "form": "와ㅜㅜ 너무 무섭네요",
          "original_form": "와ㅜㅜ 너무 무섭네요",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005098.1.20",
          "form": "그전까진 고산병이 잘 안 와닿았었는데",
          "original_form": "그전까진 고산병이 잘 안 와닿았었는데",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005098.1.21",
          "form": "그냥 해발 높은 나라만 가도 그렇구나... 하고",
          "original_form": "그냥 해발 높은 나라만 가도 그렇구나... 하고",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005098.1.22",
          "form": "신기했었던 거 같아요",
          "original_form": "신기했었던 거 같아요",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005098.1.23",
          "form": "그니까여 한국에산갔었을때 좀어지럽고 속안좋아서 이게고산병인가했는데 완전새발의피 ㅋㅋㅋㅋ",
          "original_form": "그니까여 한국에산갔었을때 좀어지럽고 속안좋아서 이게고산병인가했는데 완전새발의피 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005098.1.24",
          "form": "남미가실때 혼자가셨나요?",
          "original_form": "남미가실때 혼자가셨나요?",
          "speaker_id": "1",
          "time": "20210821 20:46"
        },
        {
          "id": "MDRW2100005098.1.25",
          "form": "새발의 피ㅋㅋㅋㅋ 인정합니닷",
          "original_form": "새발의 피ㅋㅋㅋㅋ 인정합니닷",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005098.1.26",
          "form": "아뇨 어머니랑 둘이 갔었어요!",
          "original_form": "아뇨 어머니랑 둘이 갔었어요!",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005098.1.27",
          "form": "name1님은 혼자 가셨나요??",
          "original_form": "&name1&님은 혼자 가셨나요??",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005098.1.28",
          "form": "오 어머니랑같이 멀리여행가기 진짜 쉽지않잖아여",
          "original_form": "오 어머니랑같이 멀리여행가기 진짜 쉽지않잖아여",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005098.1.29",
          "form": "네 전 혼자갔었어요 ㅋㅋㅋㅋ",
          "original_form": "네 전 혼자갔었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005098.1.30",
          "form": "무대뽀",
          "original_form": "무대뽀",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005098.1.31",
          "form": "어머니 체력괜찮으셨어요? 코스가ㅜㅜ쉽지않은데",
          "original_form": "어머니 체력괜찮으셨어요? 코스가ㅜㅜ쉽지않은데",
          "speaker_id": "1",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005098.1.32",
          "form": "혼자 여행... 그것도 남미 멋있네요ㅠㅠ",
          "original_form": "혼자 여행... 그것도 남미 멋있네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005098.1.33",
          "form": "ㅋㅋㅋ그땐 그래도 어머니 젊으셨던 때라(?) 괜찮았던 것 같아요",
          "original_form": "ㅋㅋㅋ그땐 그래도 어머니 젊으셨던 때라(?) 괜찮았던 것 같아요",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005098.1.34",
          "form": "제가 더 힘들어 했었던...",
          "original_form": "제가 더 힘들어 했었던...",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005098.1.35",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005098.1.36",
          "form": "근데 혼자갔었어도 여행지에서 한국인들이랑같이 껴서다녔어요 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 혼자갔었어도 여행지에서 한국인들이랑같이 껴서다녔어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005098.1.37",
          "form": "맞아요.. 엄마모시고다녀야해여",
          "original_form": "맞아요.. 엄마모시고다녀야해여",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005098.1.38",
          "form": "체력도 내가 더안좋고",
          "original_form": "체력도 내가 더안좋고",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005098.1.39",
          "form": "저도 엄마모시고 여행다닌적있었는데 제기를 다쪽쪽 빨아갔었져..",
          "original_form": "저도 엄마모시고 여행다닌적있었는데 제기를 다쪽쪽 빨아갔었져..",
          "speaker_id": "1",
          "time": "20210821 20:51"
        }
      ]
