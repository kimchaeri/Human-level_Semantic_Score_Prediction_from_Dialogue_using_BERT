      "utterance": [
        {
          "id": "MDRW2100042808.1.1",
          "form": "어제 아가방 지나갈 때 보니까",
          "original_form": "어제 아가방 지나갈 때 보니까",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042808.1.2",
          "form": "가을 옷 다바꼈대",
          "original_form": "가을 옷 다바꼈대",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042808.1.3",
          "form": "일 열심히 했네",
          "original_form": "일 열심히 했네",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042808.1.4",
          "form": "어제 아가방 지나갔어요",
          "original_form": "어제 아가방 지나갔어요",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042808.1.5",
          "form": "박스 봤죠 언니",
          "original_form": "박스 봤죠 언니",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042808.1.6",
          "form": "오늘 또 그만큼",
          "original_form": "오늘 또 그만큼",
          "speaker_id": "1",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042808.1.7",
          "form": "싸야 돼요",
          "original_form": "싸야 돼요",
          "speaker_id": "1",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042808.1.8",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042808.1.9",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042808.1.10",
          "form": "나 30분 되면",
          "original_form": "나 30분 되면",
          "speaker_id": "1",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042808.1.11",
          "form": "떠나요",
          "original_form": "떠나요",
          "speaker_id": "1",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042808.1.12",
          "form": "끝이없네",
          "original_form": "끝이없네",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042808.1.13",
          "form": "그래 ㅋㅋㅋㅋㄱ",
          "original_form": "그래 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
