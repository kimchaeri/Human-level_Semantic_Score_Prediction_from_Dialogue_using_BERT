      "utterance": [
        {
          "id": "MDRW2100008309.1.1",
          "form": "좋아하는 프로 있으신가용?",
          "original_form": "좋아하는 프로 있으신가용?",
          "speaker_id": "1",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100008309.1.2",
          "form": "요즘 영드에 빠졌어요",
          "original_form": "요즘 영드에 빠졌어요",
          "speaker_id": "2",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008309.1.3",
          "form": "오,,? 영드요?",
          "original_form": "오,,? 영드요?",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008309.1.4",
          "form": "한번도 안본것 같아요",
          "original_form": "한번도 안본것 같아요",
          "speaker_id": "1",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008309.1.5",
          "form": "뭐 보세요?",
          "original_form": "뭐 보세요?",
          "speaker_id": "1",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008309.1.6",
          "form": "저 수사물 덕후에요",
          "original_form": "저 수사물 덕후에요",
          "speaker_id": "2",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008309.1.7",
          "form": "웨이브에서",
          "original_form": "웨이브에서",
          "speaker_id": "2",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008309.1.8",
          "form": "브라운신부 강추여",
          "original_form": "브라운신부 강추여",
          "speaker_id": "2",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008309.1.9",
          "form": "앜ㅋㅋㅋ 저는 한드를 보기에도 시간이 벅차섴ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ 저는 한드를 보기에도 시간이 벅차섴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008309.1.10",
          "form": "외국 드라마는 잘 못봤어요",
          "original_form": "외국 드라마는 잘 못봤어요",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008309.1.11",
          "form": "나중에 찾아봐야겠네요",
          "original_form": "나중에 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008309.1.12",
          "form": "요새 한드는 머가 인기인가여",
          "original_form": "요새 한드는 머가 인기인가여",
          "speaker_id": "2",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100008309.1.13",
          "form": "저는 슬기로운 의사생활 보고 있어요!",
          "original_form": "저는 슬기로운 의사생활 보고 있어요!",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008309.1.14",
          "form": "펜트하우스도 보고있긴한데ㅋㅋㅋ 재밌어서 본다기보단 뒷내용 궁금해서 봐요",
          "original_form": "펜트하우스도 보고있긴한데ㅋㅋㅋ 재밌어서 본다기보단 뒷내용 궁금해서 봐요",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008309.1.15",
          "form": "악마판사인가 그거도 잼난거같더라고요",
          "original_form": "악마판사인가 그거도 잼난거같더라고요",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008309.1.16",
          "form": "동생이 또 추천해준게 무슨 결혼작사? 작곡? 그거도 장난아니라던데",
          "original_form": "동생이 또 추천해준게 무슨 결혼작사? 작곡? 그거도 장난아니라던데",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008309.1.17",
          "form": "오 맞아옄ㅋㅋㅋ부모님 보시더라고요 막장..",
          "original_form": "오 맞아옄ㅋㅋㅋ부모님 보시더라고요 막장..{emoji:★}",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008309.1.18",
          "form": "악마판사는 시도안해봤어요ㅋㅋㅋ근데 지성님 나오셔서 재밌을 삘이긴 해요",
          "original_form": "악마판사는 시도안해봤어요ㅋㅋㅋ근데 지성님 나오셔서 재밌을 삘이긴 해요",
          "speaker_id": "1",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100008309.1.19",
          "form": "그 막장 작가드라마던데",
          "original_form": "그 막장 작가드라마던데",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008309.1.20",
          "form": "임성한? 맞나요? 그사람 ㅋㅋ",
          "original_form": "임성한? 맞나요? 그사람 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008309.1.21",
          "form": "맞아요ㅋㅋㅋ 막 불륜이 엄청 얽혔나 그런걸로 알아요",
          "original_form": "맞아요ㅋㅋㅋ 막 불륜이 엄청 얽혔나 그런걸로 알아요",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008309.1.22",
          "form": "대충봐서 잘은 모르네여..ㅎ",
          "original_form": "대충봐서 잘은 모르네여..ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008309.1.23",
          "form": "흔한 욕하면서 보게되는 한드 ㅋㅋ",
          "original_form": "흔한 욕하면서 보게되는 한드 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008309.1.24",
          "form": "ㅋㅋㅋ막장드 시청률은 정말 높죠",
          "original_form": "ㅋㅋㅋ막장드 시청률은 정말 높죠",
