      "utterance": [
        {
          "id": "MDRW2100033833.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 11:45"
        },
        {
          "id": "MDRW2100033833.1.2",
          "form": "네~ 점심 드셨나요?",
          "original_form": "네~ 점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100033833.1.3",
          "form": "네네 저이미",
          "original_form": "네네 저이미",
          "speaker_id": "2",
          "time": "20210825 13:01"
        },
        {
          "id": "MDRW2100033833.1.4",
          "form": "먹었답니다",
          "original_form": "먹었답니다",
          "speaker_id": "2",
          "time": "20210825 13:01"
        },
        {
          "id": "MDRW2100033833.1.5",
          "form": "오 뭐드셨어요?ㅎㅎ",
          "original_form": "오 뭐드셨어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:02"
        },
        {
          "id": "MDRW2100033833.1.6",
          "form": "저 치킨이요ㅋㅋㅋ",
          "original_form": "저 치킨이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100033833.1.7",
          "form": "오 어디 치킨 드셨나요?",
          "original_form": "오 어디 치킨 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100033833.1.8",
          "form": "처갓집치킨이요ㅋㅋㅋ",
          "original_form": "처갓집치킨이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100033833.1.9",
          "form": "슈프림양념 드셨나요!!",
          "original_form": "슈프림양념 드셨나요!!",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100033833.1.10",
          "form": "ㅋㅋㅋㅋ 그쵸잉",
          "original_form": "ㅋㅋㅋㅋ 그쵸잉",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100033833.1.11",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100033833.1.12",
          "form": "저는 북촌손만두에서 떡만둣국 배달시켜 먹었어요ㅎㅎ",
          "original_form": "저는 북촌손만두에서 떡만둣국 배달시켜 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100033833.1.13",
          "form": "튀김만두까지...!",
          "original_form": "튀김만두까지...!",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100033833.1.14",
          "form": "와우 떡만둣국배달이라니",
          "original_form": "와우 떡만둣국배달이라니",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100033833.1.15",
          "form": "전한번도 안시켜본메뉴네요ㅋㅋㅋ",
          "original_form": "전한번도 안시켜본메뉴네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100033833.1.16",
          "form": "물론 가서먹는게 제일 맛있지만 시켜먹으니까 편하구 좋더라구요ㅎㅎㅎ",
          "original_form": "물론 가서먹는게 제일 맛있지만 시켜먹으니까 편하구 좋더라구요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100033833.1.17",
          "form": "북촌손만두 만두들 다 맛있으니까 한번 드셔보세요!!",
          "original_form": "북촌손만두 만두들 다 맛있으니까 한번 드셔보세요!!",
          "speaker_id": "1",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100033833.1.18",
          "form": "만두 안불고",
          "original_form": "만두 안불고",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100033833.1.19",
          "form": "배달잘오나요?!!!!!",
          "original_form": "배달잘오나요?!!!!!",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100033833.1.20",
          "form": "네네! 저희집이 가까운편이라서 그런지 몰라도 괜찮게 와요!!",
          "original_form": "네네! 저희집이 가까운편이라서 그런지 몰라도 괜찮게 와요!!",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100033833.1.21",
          "form": "요즘은 진짜 배달안되는게 없는것 같아요",
          "original_form": "요즘은 진짜 배달안되는게 없는것 같아요",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100033833.1.22",
          "form": "그니까요ㅋㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100033833.1.23",
          "form": "호떡도 배달되더라고요?",
          "original_form": "호떡도 배달되더라고요?",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100033833.1.24",
          "form": "과일도요",
          "original_form": "과일도요",
          "speaker_id": "2",
          "time": "20210825 16:38"
        }
      ]
    }
