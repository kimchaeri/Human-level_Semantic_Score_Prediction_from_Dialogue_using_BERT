      "utterance": [
        {
          "id": "MDRW2100035244.1.1",
          "form": "최근 갖고싶은거 있으세요?",
          "original_form": "최근 갖고싶은거 있으세요?",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100035244.1.2",
          "form": "요즘 스마트워치에 관심이 가더라고요",
          "original_form": "요즘 스마트워치에 관심이 가더라고요",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100035244.1.3",
          "form": "오.. 그거 편해보이긴하더라구요",
          "original_form": "오.. 그거 편해보이긴하더라구요",
          "speaker_id": "1",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100035244.1.4",
          "form": "네 건강체크도 해주고",
          "original_form": "네 건강체크도 해주고",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100035244.1.5",
          "form": "주변에 갖고 계신 분들이 많아서",
          "original_form": "주변에 갖고 계신 분들이 많아서",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100035244.1.6",
          "form": "옆에서 보다보니",
          "original_form": "옆에서 보다보니",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100035244.1.7",
          "form": "맞아요 아빠가 갖고 있으신데",
          "original_form": "맞아요 아빠가 갖고 있으신데",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100035244.1.8",
          "form": "탐나네요",
          "original_form": "탐나네요",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100035244.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100035244.1.10",
          "form": "ㅎㅎㅎ스마트워치 잘 쓰고계신가요 아버님은?",
          "original_form": "ㅎㅎㅎ스마트워치 잘 쓰고계신가요 아버님은?",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100035244.1.11",
          "form": "넵ㅋㅋ 전화받으실때 멋지시더라구요",
          "original_form": "넵ㅋㅋ 전화받으실때 멋지시더라구요",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100035244.1.12",
          "form": "저보다 신세대이십니다ㅎㅎ",
          "original_form": "저보다 신세대이십니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100035244.1.13",
          "form": "오오 전화받으실때ㅎㅎ",
          "original_form": "오오 전화받으실때ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100035244.1.14",
          "form": "네ㅋㅋ 버튼 슥 누르시는게 크으ㅋㅋ",
          "original_form": "네ㅋㅋ 버튼 슥 누르시는게 크으ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035244.1.15",
          "form": "구매희망 욕구가 조금 더 생겨나네요",
          "original_form": "구매희망 욕구가 조금 더 생겨나네요",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035244.1.16",
          "form": "저도 슬슬 생기네요",
          "original_form": "저도 슬슬 생기네요",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100035244.1.17",
          "form": "최근에 갤럭시워치도 새로 나왔다는데",
          "original_form": "최근에 갤럭시워치도 새로 나왔다는데",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035244.1.18",
          "form": "제 친구가 예약해서 구매했다고 자랑하더라고요",
          "original_form": "제 친구가 예약해서 구매했다고 자랑하더라고요",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100035244.1.19",
          "form": "우와..",
          "original_form": "우와..",
