      "utterance": [
        {
          "id": "MDRW2100019467.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100019467.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019467.1.3",
          "form": "코로나 끝나고 가고싶은곳 있으세요?",
          "original_form": "코로나 끝나고 가고싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019467.1.4",
          "form": "많죠",
          "original_form": "많죠",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019467.1.5",
          "form": "국내도 좋지만 역시 해외로 가고싶네요",
          "original_form": "국내도 좋지만 역시 해외로 가고싶네요",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019467.1.6",
          "form": "휴양지 쪽으로 한 번 가고 싶어요",
          "original_form": "휴양지 쪽으로 한 번 가고 싶어요",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019467.1.7",
          "form": "저두요... 생각은 자주 하지만 코로나가 끝날 생각을 안하네요",
          "original_form": "저두요... 생각은 자주 하지만 코로나가 끝날 생각을 안하네요",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019467.1.8",
          "form": "그러니까요..",
          "original_form": "그러니까요..",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019467.1.9",
          "form": "이렇게 오래 갈거라곤 생각 못했는데..",
          "original_form": "이렇게 오래 갈거라곤 생각 못했는데..",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019467.1.10",
          "form": "내년에는 가능하겠죠?",
          "original_form": "내년에는 가능하겠죠?",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019467.1.11",
          "form": "그러기를 제발 바라고 있어요",
          "original_form": "그러기를 제발 바라고 있어요",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019467.1.12",
          "form": "내년엔 마스크도 벗을 수 있었으면 좋겠네요",
          "original_form": "내년엔 마스크도 벗을 수 있었으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019467.1.13",
          "form": "저두요... 방역의 끝을 보고 싶네요",
          "original_form": "저두요... 방역의 끝을 보고 싶네요",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019467.1.14",
          "form": "근데 뭔가 한참 더 남은 느낌이긴해요",
          "original_form": "근데 뭔가 한참 더 남은 느낌이긴해요",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019467.1.15",
          "form": "아직도 확진자는 계속 늘어나니...",
          "original_form": "아직도 확진자는 계속 늘어나니...",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019467.1.16",
          "form": "이번에 2천명 찍는거 보고 놀랐어요",
          "original_form": "이번에 2천명 찍는거 보고 놀랐어요",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019467.1.17",
          "form": "변이가 전염력이 강한건지 사람이 많이 돌아다니는건지",
          "original_form": "변이가 전염력이 강한건지 사람이 많이 돌아다니는건지",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019467.1.18",
          "form": "우와....",
          "original_form": "우와....",
          "speaker_id": "2",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019467.1.19",
          "form": "아마 전부 다인것 같아요",
          "original_form": "아마 전부 다인것 같아요",
          "speaker_id": "2",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019467.1.20",
          "form": "왜 주변을 보면 다들 집에만 있는거 같은데",
          "original_form": "왜 주변을 보면 다들 집에만 있는거 같은데",
          "speaker_id": "2",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019467.1.21",
          "form": "넓게 보면 왜 계속 확진자가 생기죠..?",
          "original_form": "넓게 보면 왜 계속 확진자가 생기죠..?",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019467.1.22",
          "form": "수도권 번화가는 사람이 넘쳐나더라구요",
          "original_form": "수도권 번화가는 사람이 넘쳐나더라구요",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019467.1.23",
          "form": "어후.. 전 무서워서라도 못나가겠던데",
          "original_form": "어후.. 전 무서워서라도 못나가겠던데",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019467.1.24",
          "form": "다들 괜찮은가봐요",
          "original_form": "다들 괜찮은가봐요",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019467.1.25",
          "form": "그런가봐요ㅠ 저도 집콕하고 살고 있는데",
          "original_form": "그런가봐요ㅠ 저도 집콕하고 살고 있는데",
          "speaker_id": "1",
          "time": "20210827 19:55"
        },
        {
          "id": "MDRW2100019467.1.26",
          "form": "저두요..",
          "original_form": "저두요..",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019467.1.27",
          "form": "외출다운 외출이 언젠지 기억도 안나요..",
          "original_form": "외출다운 외출이 언젠지 기억도 안나요..",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019467.1.28",
          "form": "진짜 외출을 장보러 나가기만 했네요",
          "original_form": "진짜 외출을 장보러 나가기만 했네요",
          "speaker_id": "1",
          "time": "20210827 20:04"
        }
      ]
    }
