      "utterance": [
        {
          "id": "MDRW2100024904.1.1",
          "form": "난 요즘 초밥 시켜먹는거 좋아해ㅋㅋ",
          "original_form": "난 요즘 초밥 시켜먹는거 좋아해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100024904.1.2",
          "form": "초밥 좋지ㅋㅋㅋ",
          "original_form": "초밥 좋지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100024904.1.3",
          "form": "나는 새우초밥 좋아함ㅋ",
          "original_form": "나는 새우초밥 좋아함ㅋ",
          "speaker_id": "1",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100024904.1.4",
          "form": "계란이랑ㅋ",
          "original_form": "계란이랑ㅋ",
          "speaker_id": "1",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100024904.1.5",
          "form": "계란초밥 나도 좋아ㅋㅋㅋㅋ",
          "original_form": "계란초밥 나도 좋아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:31"
        },
        {
          "id": "MDRW2100024904.1.6",
          "form": "우리동네 진짜 맛있음ㅋㅋ",
          "original_form": "우리동네 진짜 맛있음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:31"
        },
        {
          "id": "MDRW2100024904.1.7",
          "form": "오ㅋㅋㅋ좋겠다ㅋㅋㅋ",
          "original_form": "오ㅋㅋㅋ좋겠다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:31"
        },
        {
          "id": "MDRW2100024904.1.8",
          "form": "응ㅋㅋㅋ나는 떡볶이 좋아함",
          "original_form": "응ㅋㅋㅋ나는 떡볶이 좋아함",
          "speaker_id": "2",
          "time": "20210831 20:31"
        },
        {
          "id": "MDRW2100024904.1.9",
          "form": "떡볶이는 없어서 못먹지ㅋㅋ",
          "original_form": "떡볶이는 없어서 못먹지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:31"
        },
        {
          "id": "MDRW2100024904.1.10",
          "form": "ㅋㅋㅋ떡볶이 살찌는거 알면서 사먹음ㅡㅡㅋ",
          "original_form": "ㅋㅋㅋ떡볶이 살찌는거 알면서 사먹음ㅡㅡㅋ",
          "speaker_id": "2",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100024904.1.11",
          "form": "떡볶이는 소울푸드ㅋㅋ",
          "original_form": "떡볶이는 소울푸드ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100024904.1.12",
          "form": "떡볶이는 사랑입니다ㅋ",
          "original_form": "떡볶이는 사랑입니다ㅋ",
          "speaker_id": "2",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100024904.1.13",
          "form": "어디꺼 좋아함?",
          "original_form": "어디꺼 좋아함?",
          "speaker_id": "2",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100024904.1.14",
          "form": "나는 엽떡ㅋㅋ",
          "original_form": "나는 엽떡ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100024904.1.15",
          "form": "근데 맵찔이라서ㅋㅋ",
          "original_form": "근데 맵찔이라서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100024904.1.16",
          "form": "나도맵찔임ㅋㅋㅇ",
          "original_form": "나도맵찔임ㅋㅋㅇ",
          "speaker_id": "2",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100024904.1.17",
          "form": "무조건순한맛으로먹어",
          "original_form": "무조건순한맛으로먹어",
          "speaker_id": "2",
          "time": "20210831 20:33"
        },
        {
          "id": "MDRW2100024904.1.18",
          "form": "순한맛먹어도배탈남ㅋㅋ",
          "original_form": "순한맛먹어도배탈남ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:33"
        },
        {
          "id": "MDRW2100024904.1.19",
          "form": "악ㅋㅋㅋㅋ나도야ㅋㅋ매운거못먹음ㅋㅋ",
          "original_form": "악ㅋㅋㅋㅋ나도야ㅋㅋ매운거못먹음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:33"
        },
        {
          "id": "MDRW2100024904.1.20",
          "form": "아쉬워ㅋㅋ",
          "original_form": "아쉬워ㅋㅋ",
