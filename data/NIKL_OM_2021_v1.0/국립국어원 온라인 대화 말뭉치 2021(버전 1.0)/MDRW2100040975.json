      "utterance": [
        {
          "id": "MDRW2100040975.1.1",
          "form": "공부안하나요?",
          "original_form": "공부안하나요?",
          "speaker_id": "1",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100040975.1.2",
          "form": "아 제말이요",
          "original_form": "아 제말이요",
          "speaker_id": "2",
          "time": "20210907 16:39"
        },
        {
          "id": "MDRW2100040975.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ왜 안하죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ왜 안하죠",
          "speaker_id": "2",
          "time": "20210907 16:39"
        },
        {
          "id": "MDRW2100040975.1.4",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "1",
          "time": "20210907 16:41"
        },
        {
          "id": "MDRW2100040975.1.5",
          "form": "안하셧다구요?",
          "original_form": "안하셧다구요?",
          "speaker_id": "1",
          "time": "20210907 16:41"
        },
        {
          "id": "MDRW2100040975.1.6",
          "form": "아직 컴터두 한 번 안켜봤답니다",
          "original_form": "아직 컴터두 한 번 안켜봤답니다",
          "speaker_id": "2",
          "time": "20210907 16:41"
        },
        {
          "id": "MDRW2100040975.1.7",
          "form": "아 근데 진짜 국영한도문제지만",
          "original_form": "아 근데 진짜 국영한도문제지만",
          "speaker_id": "1",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100040975.1.8",
          "form": "직렬과목이 끝이야",
          "original_form": "직렬과목이 끝이야",
          "speaker_id": "1",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100040975.1.9",
          "form": "노우 답인데",
          "original_form": "노우 답인데",
          "speaker_id": "1",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100040975.1.10",
          "form": "ㅋㅋㅋ노우답",
          "original_form": "ㅋㅋㅋ노우답",
          "speaker_id": "2",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100040975.1.11",
          "form": "우빼면 안되요",
          "original_form": "우빼면 안되요",
          "speaker_id": "1",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100040975.1.12",
          "form": "나쁜말이에요",
          "original_form": "나쁜말이에요",
          "speaker_id": "1",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100040975.1.13",
          "form": "하기.싫다",
          "original_form": "하기.싫다",
          "speaker_id": "2",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100040975.1.14",
          "form": "난 한국사도 좀 시들해졋어 ㅋㅋㅋㅋ",
          "original_form": "난 한국사도 좀 시들해졋어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100040975.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ적.이.....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ적.이.....",
          "speaker_id": "2",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100040975.1.16",
          "form": "앞에 다 휘발되엇는데",
          "original_form": "앞에 다 휘발되엇는데",
          "speaker_id": "1",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100040975.1.17",
          "form": "시들해지지마세요",
          "original_form": "시들해지지마세요",
          "speaker_id": "2",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100040975.1.18",
          "form": "근대는 계속",
          "original_form": "근대는 계속",
          "speaker_id": "1",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100040975.1.19",
          "form": "근현대 ㅋㅋㅋㅋㅋ",
          "original_form": "근현대 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100040975.1.20",
          "form": "동학운동에서 계속 자서",
          "original_form": "동학운동에서 계속 자서",
          "speaker_id": "1",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100040975.1.21",
          "form": "계속 그 동영상만 몇번을 틀엇는지",
          "original_form": "계속 그 동영상만 몇번을 틀엇는지",
          "speaker_id": "1",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100040975.1.22",
          "form": "명성황후만 계속 시해당해",
          "original_form": "명성황후만 계속 시해당해",
          "speaker_id": "1",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100040975.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100040975.1.24",
          "form": "꿈에서 동학농민운동나갈듯ㅋㅋㅋ",
          "original_form": "꿈에서 동학농민운동나갈듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100040975.1.25",
          "form": "ㅋㅋㅋㅋㅋ 내용이 하나도 남은게 옶네ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ 내용이 하나도 남은게 옶네ㅠ",
          "speaker_id": "1",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100040975.1.26",
          "form": "에효",
          "original_form": "에효",
          "speaker_id": "1",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100040975.1.27",
          "form": "ㅋㅋㅋ아 진짜 공부말고 다른 길 없을까",
          "original_form": "ㅋㅋㅋ아 진짜 공부말고 다른 길 없을까",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100040975.1.28",
          "form": "난 그냥 진짜 그냥잇고싶다",
          "original_form": "난 그냥 진짜 그냥잇고싶다",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100040975.1.29",
          "form": "아 누가 집이랑 한달에 100만원씩만",
          "original_form": "아 누가 집이랑 한달에 100만원씩만",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100040975.1.30",
          "form": "나죽을때까지주면",
          "original_form": "나죽을때까지주면",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100040975.1.31",
          "form": "좋겟다",
          "original_form": "좋겟다",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100040975.1.32",
          "form": "ㅋㅋㅋ진짜 아무것도 안하고 잘 살 자신 있는데..",
          "original_form": "ㅋㅋㅋ진짜 아무것도 안하고 잘 살 자신 있는데..",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100040975.1.33",
          "form": "나 욕심안많ㅈㅣ?",
          "original_form": "나 욕심안많ㅈㅣ?",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100040975.1.34",
          "form": "누가 일안하면 지겹대 ㅠㅠ",
          "original_form": "누가 일안하면 지겹대 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100040975.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100040975.1.36",
          "form": "뉴가요",
          "original_form": "뉴가요",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100040975.1.37",
          "form": "ㅋㅋㅋ100은 좀 많은듯",
          "original_form": "ㅋㅋㅋ100은 좀 많은듯",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100040975.1.38",
          "form": "100이 많다고!?!?!?!?",
          "original_form": "100이 많다고!?!?!?!?",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100040975.1.39",
          "form": "아니 물론 마음으론 적은뎈ㅋㅋㅋㅋ",
          "original_form": "아니 물론 마음으론 적은뎈ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100040975.1.40",
          "form": "아무것도안하고 100은 많다 이거죠 ㅋㅋㅋㅋ",
          "original_form": "아무것도안하고 100은 많다 이거죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100040975.1.41",
          "form": "ㅋㅋㅋㅋ무튼 얼마든...",
          "original_form": "ㅋㅋㅋㅋ무튼 얼마든...",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100040975.1.42",
          "form": "먹어야할꺼아냐ㅠ",
          "original_form": "먹어야할꺼아냐ㅠ",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100040975.1.43",
          "form": "세금도내야하고ㅠ",
          "original_form": "세금도내야하고ㅠ",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100040975.1.44",
          "form": "공과금도내야하고ㅠㅠ엉엉",
          "original_form": "공과금도내야하고ㅠㅠ엉엉",
          "speaker_id": "1",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100040975.1.45",
          "form": "ㅋㅋ연금복권을 사야돼",
          "original_form": "ㅋㅋ연금복권을 사야돼",
          "speaker_id": "2",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100040975.1.46",
          "form": "그것말곤 답이 음슴",
          "original_form": "그것말곤 답이 음슴",
          "speaker_id": "2",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100040975.1.47",
          "form": "ㅠㅡㅠ",
          "original_form": "ㅠㅡㅠ",
          "speaker_id": "1",
          "time": "20210907 16:51"
        },
        {
          "id": "MDRW2100040975.1.48",
          "form": "진짜되면 어떤인생이될까",
          "original_form": "진짜되면 어떤인생이될까",
