      "utterance": [
        {
          "id": "MDRW2100002851.1.1",
          "form": "스포츠 좋아하시나요?",
          "original_form": "스포츠 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002851.1.2",
          "form": "저는 스포츠는 크게 관심이 없어요 ㅎㅎ",
          "original_form": "저는 스포츠는 크게 관심이 없어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 18:28"
        },
        {
          "id": "MDRW2100002851.1.3",
          "form": "걍 올림픽하면 보는 정도?",
          "original_form": "걍 올림픽하면 보는 정도?",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100002851.1.4",
          "form": "저도요 ㅋㅋ 근데 이번에 배구에 빠졌어요ㅠㅠ 넘 멋있어요",
          "original_form": "저도요 ㅋㅋ 근데 이번에 배구에 빠졌어요ㅠㅠ 넘 멋있어요",
          "speaker_id": "1",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100002851.1.5",
          "form": "앗 저도요 ㅋㅋ",
          "original_form": "앗 저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100002851.1.6",
          "form": "연경신 진짜 대단해요",
          "original_form": "연경신 진짜 대단해요",
          "speaker_id": "2",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100002851.1.7",
          "form": "너무 멋있어요..  운동선수한테 이렇게 반하다니",
          "original_form": "너무 멋있어요..  운동선수한테 이렇게 반하다니",
          "speaker_id": "1",
          "time": "20210820 19:35"
        },
        {
          "id": "MDRW2100002851.1.8",
          "form": "저도요 ㅎㅎ",
          "original_form": "저도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100002851.1.9",
          "form": "외쳐 갓연경",
          "original_form": "외쳐 갓연경",
          "speaker_id": "2",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100002851.1.10",
          "form": "인성이 지대루죠",
          "original_form": "인성이 지대루죠",
          "speaker_id": "2",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100002851.1.11",
          "form": "실력도 최고인데다 성격도 쿨해서 ,, 넘좋아요",
          "original_form": "실력도 최고인데다 성격도 쿨해서 ,, 넘좋아요",
          "speaker_id": "1",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100002851.1.12",
          "form": "덕분에 저도 스포츠에 더 관심 생겼어요 ㅎㅎ",
          "original_form": "덕분에 저도 스포츠에 더 관심 생겼어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100002851.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100002851.1.14",
          "form": "ㅋㅋㅋ 저도요",
          "original_form": "ㅋㅋㅋ 저도요",
          "speaker_id": "2",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100002851.1.15",
          "form": "여자배구 선수들 다 사랑스러워요",
          "original_form": "여자배구 선수들 다 사랑스러워요",
          "speaker_id": "2",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100002851.1.16",
          "form": "그니까요ㅠㅠ 팀워크도 너무좋고",
          "original_form": "그니까요ㅠㅠ 팀워크도 너무좋고",
          "speaker_id": "1",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100002851.1.17",
          "form": "자랑스러워요 선수들..",
          "original_form": "자랑스러워요 선수들..",
          "speaker_id": "1",
          "time": "20210820 20:08"
        },
        {
          "id": "MDRW2100002851.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 20:09"
        },
        {
          "id": "MDRW2100002851.1.19",
          "form": "진짜 드라마 썼어요 ㅎㅎ",
          "original_form": "진짜 드라마 썼어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:09"
        },
        {
          "id": "MDRW2100002851.1.20",
          "form": "영화로 맹글어도 될 판",
          "original_form": "영화로 맹글어도 될 판",
          "speaker_id": "2",
          "time": "20210820 20:10"
        },
        {
          "id": "MDRW2100002851.1.21",
          "form": "진짜.. 영화로 만들어졌어도 설정과다였을 각 ㅠㅠ 오늘 연경선수는 트위터 스페이스하고",
          "original_form": "진짜.. 영화로 만들어졌어도 설정과다였을 각 ㅠㅠ 오늘 연경선수는 트위터 스페이스하고",
          "speaker_id": "1",
          "time": "20210820 23:16"
        },
        {
          "id": "MDRW2100002851.1.22",
          "form": "희진선수 수지선수는 소속팀에서 제작 메달 받았대요",
          "original_form": "희진선수 수지선수는 소속팀에서 제작 메달 받았대요",
          "speaker_id": "1",
          "time": "20210820 23:17"
        },
        {
          "id": "MDRW2100002851.1.23",
          "form": "브라질 배구팀 전부 도핑 테스트했다는 얘기가있던데.. 어떻게 될지..",
          "original_form": "브라질 배구팀 전부 도핑 테스트했다는 얘기가있던데.. 어떻게 될지..",
          "speaker_id": "1",
          "time": "20210820 23:17"
        },
        {
          "id": "MDRW2100002851.1.24",
          "form": "아 그런 얘기가 있군요",
          "original_form": "아 그런 얘기가 있군요",
          "speaker_id": "2",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100002851.1.25",
          "form": "진짜 대단해요 ㅎㅎ",
          "original_form": "진짜 대단해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100002851.1.26",
          "form": "노메달이어도 넘 잘싸웠어요",
          "original_form": "노메달이어도 넘 잘싸웠어요",
          "speaker_id": "2",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100002851.1.27",
          "form": "정말 결과보다 과정이 더 멋진 경기였어요..ㅎㅎㅎㅎ",
          "original_form": "정말 결과보다 과정이 더 멋진 경기였어요..ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 23:28"
        },
        {
          "id": "MDRW2100002851.1.28",
          "form": "한국 배구연맹이 그렇게 나쁘다던데",
          "original_form": "한국 배구연맹이 그렇게 나쁘다던데",
          "speaker_id": "1",
          "time": "20210820 23:28"
        },
        {
          "id": "MDRW2100002851.1.29",
          "form": "부디 선수들 복지에 신경써주길..ㅠ",
          "original_form": "부디 선수들 복지에 신경써주길..ㅠ",
          "speaker_id": "1",
          "time": "20210820 23:28"
        },
        {
          "id": "MDRW2100002851.1.30",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100002851.1.31",
