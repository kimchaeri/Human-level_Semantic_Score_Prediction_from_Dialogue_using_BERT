      "utterance": [
        {
          "id": "MDRW2100024236.1.1",
          "form": "그래서 어 장발도 괜찮아보인다는 생각을",
          "original_form": "그래서 어 장발도 괜찮아보인다는 생각을",
          "speaker_id": "2",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024236.1.2",
          "form": "저는 장발이 뭐 그런건 줄 알았어요",
          "original_form": "저는 장발이 뭐 그런건 줄 알았어요",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024236.1.3",
          "form": "아까 들국화 충격에 헤어나오지 못해서 그런지",
          "original_form": "아까 들국화 충격에 헤어나오지 못해서 그런지",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024236.1.4",
          "form": "전인권 가수 같은 장발....ㅋㅋㅋ",
          "original_form": "전인권 가수 같은 장발....ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024236.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024236.1.6",
          "form": "김경호 가수님 같은 장발.. 그런거..",
          "original_form": "김경호 가수님 같은 장발.. 그런거..",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024236.1.7",
          "form": "그건 너무...",
          "original_form": "그건 너무...",
          "speaker_id": "2",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024236.1.8",
          "form": "ㅋㅋㅋㅋ너무 장발이고...",
          "original_form": "ㅋㅋㅋㅋ너무 장발이고...",
          "speaker_id": "2",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024236.1.9",
          "form": "근데 김경호님도 장발 되게 잘 어울리지 않아요?",
          "original_form": "근데 김경호님도 장발 되게 잘 어울리지 않아요?",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024236.1.10",
          "form": "오히려 짧은게 어색하더라구요",
          "original_form": "오히려 짧은게 어색하더라구요",
          "speaker_id": "2",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024236.1.11",
          "form": "짧은 머리도 하셨어요?",
          "original_form": "짧은 머리도 하셨어요?",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024236.1.12",
          "form": "전에 잘랏을때 뭔가 힘을 다 뺏긴 것 같앗어옷",
          "original_form": "전에 잘랏을때 뭔가 힘을 다 뺏긴 것 같앗어옷",
          "speaker_id": "2",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024236.1.13",
          "form": "헐 그랬구나ㅜㅜ 짧은머리 본 적 없어서",
          "original_form": "헐 그랬구나ㅜㅜ 짧은머리 본 적 없어서",
          "speaker_id": "1",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024236.1.14",
          "form": "되게 예쁜 누나 같아요 김경호 가수님 머리 되게 찰랑거리고",
          "original_form": "되게 예쁜 누나 같아요 김경호 가수님 머리 되게 찰랑거리고",
          "speaker_id": "1",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024236.1.15",
          "form": "진짜 잠깐 짧게햇다가",
          "original_form": "진짜 잠깐 짧게햇다가",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024236.1.16",
          "form": "긴머리 어울리는 것을 인정하고",
          "original_form": "긴머리 어울리는 것을 인정하고",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024236.1.17",
          "form": "다시 돌아갔어요",
          "original_form": "다시 돌아갔어요",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024236.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024236.1.19",
          "form": "뭔가 자신의 정체성 찾은 느낌이네요",
          "original_form": "뭔가 자신의 정체성 찾은 느낌이네요",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024236.1.20",
          "form": "저 그 애니매이션 생각났어요",
          "original_form": "저 그 애니매이션 생각났어요",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024236.1.21",
          "form": "라푼젤이요 ㅋㅋ..",
          "original_form": "라푼젤이요 ㅋㅋ..",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024236.1.22",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024236.1.23",
          "form": "머리를 자르면 힘을 잃게 되는 우리 김경호씨 ㅜㅜ",
          "original_form": "머리를 자르면 힘을 잃게 되는 우리 김경호씨 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024236.1.24",
          "form": "맞아요....김경호님 라푼젤이야...",
          "original_form": "맞아요....김경호님 라푼젤이야...",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024236.1.25",
          "form": "라푼젤 공주님",
          "original_form": "라푼젤 공주님",
          "speaker_id": "2",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100024236.1.26",
          "form": "요새 무슨 활동하는 지 모르겠지만 잘 사셨으면 좋겠다",
          "original_form": "요새 무슨 활동하는 지 모르겠지만 잘 사셨으면 좋겠다",
          "speaker_id": "1",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100024236.1.27",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:36"
        }
      ]
    }
