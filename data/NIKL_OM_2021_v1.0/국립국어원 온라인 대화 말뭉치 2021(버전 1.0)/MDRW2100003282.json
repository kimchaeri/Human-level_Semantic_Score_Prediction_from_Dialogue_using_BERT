      "utterance": [
        {
          "id": "MDRW2100003282.1.1",
          "form": "혹시 종교 있으신가요?",
          "original_form": "혹시 종교 있으신가요?",
          "speaker_id": "2",
          "time": "20210820 21:22"
        },
        {
          "id": "MDRW2100003282.1.2",
          "form": "저는 없지만",
          "original_form": "저는 없지만",
          "speaker_id": "1",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003282.1.3",
          "form": "부모님이 불교를 좋아하세요!",
          "original_form": "부모님이 불교를 좋아하세요!",
          "speaker_id": "1",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003282.1.4",
          "form": "그래서 유치원을 불교유치원으로 다녔습니다..",
          "original_form": "그래서 유치원을 불교유치원으로 다녔습니다..",
          "speaker_id": "1",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003282.1.5",
          "form": "우와 불교유치원은 뭐 배우나요????",
          "original_form": "우와 불교유치원은 뭐 배우나요????",
          "speaker_id": "2",
          "time": "20210820 21:34"
        },
        {
          "id": "MDRW2100003282.1.6",
          "form": "너무 어릴 때라 기억이 잘 안나지만 ..",
          "original_form": "너무 어릴 때라 기억이 잘 안나지만 ..",
          "speaker_id": "1",
          "time": "20210820 21:35"
        },
        {
          "id": "MDRW2100003282.1.7",
          "form": "불상 앞에서 기도하고",
          "original_form": "불상 앞에서 기도하고",
          "speaker_id": "1",
          "time": "20210820 21:35"
        },
        {
          "id": "MDRW2100003282.1.8",
          "form": "스님도 계셨어요 ㅎㅎㅎ",
          "original_form": "스님도 계셨어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 21:35"
        },
        {
          "id": "MDRW2100003282.1.9",
          "form": "우와 나름 재밌응 거 같아요",
          "original_form": "우와 나름 재밌응 거 같아요",
          "speaker_id": "2",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003282.1.10",
          "form": "어릴 때 유치원이 추억이네요",
          "original_form": "어릴 때 유치원이 추억이네요",
          "speaker_id": "2",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003282.1.11",
          "form": "네넹 맞아요!",
          "original_form": "네넹 맞아요!",
          "speaker_id": "1",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003282.1.12",
          "form": "그래서 유일하게 기억나요~~~",
          "original_form": "그래서 유일하게 기억나요~~~",
          "speaker_id": "1",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003282.1.13",
          "form": "전 그냥 어릴 때 막 교회에서 김치전주고 그러잖아요",
          "original_form": "전 그냥 어릴 때 막 교회에서 김치전주고 그러잖아요",
          "speaker_id": "2",
          "time": "20210820 21:47"
        },
        {
          "id": "MDRW2100003282.1.14",
          "form": "아니면 성경학교 간다고 간거믄 기억나요",
          "original_form": "아니면 성경학교 간다고 간거믄 기억나요",
          "speaker_id": "2",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003282.1.15",
          "form": "지금도 교회나가세요!?",
          "original_form": "지금도 교회나가세요!?",
          "speaker_id": "1",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003282.1.16",
          "form": "아니요 ㅎ 어릴 때 잠깐 나간거라",
          "original_form": "아니요 ㅎ 어릴 때 잠깐 나간거라",
          "speaker_id": "2",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003282.1.17",
          "form": "이젠 교회 쳐다보지도 않아요",
          "original_form": "이젠 교회 쳐다보지도 않아요",
          "speaker_id": "2",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003282.1.18",
          "form": "아아~~ 그렇군요!",
          "original_form": "아아~~ 그렇군요!",
          "speaker_id": "1",
          "time": "20210820 21:55"
        },
        {
          "id": "MDRW2100003282.1.19",
          "form": "저는 중딩 때 성당 학교 나오고",
          "original_form": "저는 중딩 때 성당 학교 나오고",
          "speaker_id": "1",
          "time": "20210820 21:55"
        },
        {
          "id": "MDRW2100003282.1.20",
          "form": "대학교 때 교회 학교",
          "original_form": "대학교 때 교회 학교",
          "speaker_id": "1",
          "time": "20210820 21:55"
        },
        {
          "id": "MDRW2100003282.1.21",
          "form": "삼대종교 다 체험했어요 ㅠㅠㅠ",
          "original_form": "삼대종교 다 체험했어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:55"
        },
        {
          "id": "MDRW2100003282.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그 중 어디가 그나마 괜찮았나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그 중 어디가 그나마 괜찮았나요",
          "speaker_id": "2",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003282.1.23",
          "form": "나름 좋은 경험이네요",
          "original_form": "나름 좋은 경험이네요",
          "speaker_id": "2",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003282.1.24",
          "form": "저는 불교가 가장 괜찮았어요~",
          "original_form": "저는 불교가 가장 괜찮았어요~",
          "speaker_id": "1",
          "time": "20210820 22:12"
        },
        {
          "id": "MDRW2100003282.1.25",
          "form": "강요도 없고!",
          "original_form": "강요도 없고!",
          "speaker_id": "1",
          "time": "20210820 22:13"
        },
        {
          "id": "MDRW2100003282.1.26",
          "form": "성당이랑 교회는 너무 시키는게 많았어여",
          "original_form": "성당이랑 교회는 너무 시키는게 많았어여",
          "speaker_id": "1",
          "time": "20210820 22:13"
        },
        {
          "id": "MDRW2100003282.1.27",
          "form": "아 맞아요.. 막 뭐 시키니까요..",
          "original_form": "아 맞아요.. 막 뭐 시키니까요..",
