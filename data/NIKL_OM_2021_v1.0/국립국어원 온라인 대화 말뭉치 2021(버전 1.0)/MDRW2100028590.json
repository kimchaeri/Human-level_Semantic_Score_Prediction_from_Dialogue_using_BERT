      "utterance": [
        {
          "id": "MDRW2100028590.1.1",
          "form": "놀러 가기!!!! 일본 가고 싶어요",
          "original_form": "놀러 가기!!!! 일본 가고 싶어요",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028590.1.2",
          "form": "저도 일본 안가봐서 함 가보고싶어요",
          "original_form": "저도 일본 안가봐서 함 가보고싶어요",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028590.1.3",
          "form": "초밥한번 먹어보고싶은데 ㅎㅎ",
          "original_form": "초밥한번 먹어보고싶은데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028590.1.4",
          "form": "해외 어디 가 본 곳 있으세요?",
          "original_form": "해외 어디 가 본 곳 있으세요?",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028590.1.5",
          "form": "헉 저두요!!",
          "original_form": "헉 저두요!!",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028590.1.6",
          "form": "전 없어요 ㅠㅠ 계속 미루다가",
          "original_form": "전 없어요 ㅠㅠ 계속 미루다가",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028590.1.7",
          "form": "결국 코로나...ㅎ",
          "original_form": "결국 코로나...ㅎ",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028590.1.8",
          "form": "일본하면 초밥이나",
          "original_form": "일본하면 초밥이나",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028590.1.9",
          "form": "타코야끼같은거",
          "original_form": "타코야끼같은거",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028590.1.10",
          "form": "함 먹어보고싶네요 ㅎㅎ",
          "original_form": "함 먹어보고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028590.1.11",
          "form": "진짜요! 본토의 맛은 다를까 궁금하네요",
          "original_form": "진짜요! 본토의 맛은 다를까 궁금하네요",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028590.1.12",
          "form": "그쵸 ㅎㅎㅎ",
          "original_form": "그쵸 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028590.1.13",
          "form": "참치가맛있을거같아요",
          "original_form": "참치가맛있을거같아요",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028590.1.14",
          "form": "와 군침 돌아요.. 일본 가본 적 있긴 한데 정작 그런 걸 안 먹어봤네요",
          "original_form": "와 군침 돌아요.. 일본 가본 적 있긴 한데 정작 그런 걸 안 먹어봤네요",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028590.1.15",
          "form": "그럼 그때는 뭐드셔보셨어요??ㅎㅎ",
          "original_form": "그럼 그때는 뭐드셔보셨어요??ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028590.1.16",
          "form": "라멘 먹어봤는데 그 라멘집이 딱히 취향이 아니더라구요ㅠ",
          "original_form": "라멘 먹어봤는데 그 라멘집이 딱히 취향이 아니더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028590.1.17",
          "form": "아 맞다 라멘도 있었죵",
          "original_form": "아 맞다 라멘도 있었죵",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028590.1.18",
          "form": "한국에있는 라멘하고 많이다르던가요??",
          "original_form": "한국에있는 라멘하고 많이다르던가요??",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028590.1.19",
          "form": "제가 한국에 있는 라멘을 성공해본 적이 없어서...흑... 잘 모르겠아요ㅠㅠ",
          "original_form": "제가 한국에 있는 라멘을 성공해본 적이 없어서...흑... 잘 모르겠아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028590.1.20",
          "form": "아 ㅠㅠ",
          "original_form": "아 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028590.1.21",
          "form": "라멘이...",
          "original_form": "라멘이...",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028590.1.22",
          "form": "정말 유명한곳아니면 ㅠ",
          "original_form": "정말 유명한곳아니면 ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028590.1.23",
          "form": "정말 느끼하기만 하자나요ㅠㅠ",
          "original_form": "정말 느끼하기만 하자나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028590.1.24",
          "form": "맞아요ㅠㅠ 맛없는 곳만 두 번 가서 이제 가기가 무서워지더라구오",
          "original_form": "맞아요ㅠㅠ 맛없는 곳만 두 번 가서 이제 가기가 무서워지더라구오",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100028590.1.25",
          "form": "참 ...맛집찾기가ㅜ어렵긴해요 ㅠㅠ",
          "original_form": "참 ...맛집찾기가ㅜ어렵긴해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028590.1.26",
          "form": "진짜요ㅜㅜ 맛집이라서 갔는데 맛없는 곳도 있어서... 취향 차이도 있으니가요...",
          "original_form": "진짜요ㅜㅜ 맛집이라서 갔는데 맛없는 곳도 있어서... 취향 차이도 있으니가요...",
          "speaker_id": "1",
          "time": "20210902 22:34"
        }
      ]
