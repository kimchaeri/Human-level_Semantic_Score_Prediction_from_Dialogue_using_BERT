      "utterance": [
        {
          "id": "MDRW2100029881.1.1",
          "form": "천개의 파랑 보눈데",
          "original_form": "천개의 파랑 보눈데",
          "speaker_id": "2",
          "time": "20210903 16:02"
        },
        {
          "id": "MDRW2100029881.1.2",
          "form": "등장인물 이름 넘 헷갈려",
          "original_form": "등장인물 이름 넘 헷갈려",
          "speaker_id": "2",
          "time": "20210903 16:02"
        },
        {
          "id": "MDRW2100029881.1.3",
          "form": "외국이름이야?",
          "original_form": "외국이름이야?",
          "speaker_id": "1",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029881.1.4",
          "form": "한국인디 자꾸 시점이 바뀌니까...",
          "original_form": "한국인디 자꾸 시점이 바뀌니까...",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029881.1.5",
          "form": "네이버 웹소설 같은게 필요하니?",
          "original_form": "네이버 웹소설 같은게 필요하니?",
          "speaker_id": "1",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029881.1.6",
          "form": "그건 싫어...",
          "original_form": "그건 싫어...",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100029881.1.7",
          "form": "그치만 이름 못 외우는 건 내 고질병이라...",
          "original_form": "그치만 이름 못 외우는 건 내 고질병이라...",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100029881.1.8",
          "form": "반은 읽어야 알아보겠군",
          "original_form": "반은 읽어야 알아보겠군",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100029881.1.9",
          "form": "그정도까진... 아니야",
          "original_form": "그정도까진... 아니야",
          "speaker_id": "2",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029881.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029881.1.11",
          "form": "그래도 누가 말하고 있눈진 안다구요",
          "original_form": "그래도 누가 말하고 있눈진 안다구요",
          "speaker_id": "2",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029881.1.12",
          "form": "다시읽으면 색다른 느낌이 드는거 아녀?",
          "original_form": "다시읽으면 색다른 느낌이 드는거 아녀?",
          "speaker_id": "1",
          "time": "20210903 16:18"
        },
        {
          "id": "MDRW2100029881.1.13",
          "form": "그정돈 아냐ㅠㅠ",
          "original_form": "그정돈 아냐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 16:18"
        },
        {
          "id": "MDRW2100029881.1.14",
          "form": "갸갸 누군지 로딩이 오래 걸리거나 뒤을 뒤적일 수 있지만",
          "original_form": "갸갸 누군지 로딩이 오래 걸리거나 뒤을 뒤적일 수 있지만",
          "speaker_id": "2",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100029881.1.15",
          "form": "등장인물과 매번 초면인 상황...ㅋㅋㅋㅋ",
          "original_form": "등장인물과 매번 초면인 상황...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:20"
        },
        {
          "id": "MDRW2100029881.1.16",
          "form": "우리 어디서 만난 적 있던가요?",
          "original_form": "우리 어디서 만난 적 있던가요?",
          "speaker_id": "2",
          "time": "20210903 16:20"
        },
        {
          "id": "MDRW2100029881.1.17",
          "form": "옛날 책이 좋았눈데",
          "original_form": "옛날 책이 좋았눈데",
          "speaker_id": "2",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029881.1.18",
          "form": "맨 앞에 등장인물 소개 있고",
          "original_form": "맨 앞에 등장인물 소개 있고",
          "speaker_id": "2",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029881.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029881.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:21"
        },
        {
          "id": "MDRW2100029881.1.21",
          "form": "그런거 잘 안봤는데",
          "original_form": "그런거 잘 안봤는데",
          "speaker_id": "1",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029881.1.22",
          "form": "뒤에 나올거아냐 휙휙 넘김",
          "original_form": "뒤에 나올거아냐 휙휙 넘김",
          "speaker_id": "1",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100029881.1.23",
          "form": "나도 시작할 땐 안 봐",
          "original_form": "나도 시작할 땐 안 봐",
          "speaker_id": "2",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029881.1.24",
          "form": "두세번 째 등장에 필요하지",
          "original_form": "두세번 째 등장에 필요하지",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029881.1.25",
          "form": "ㅋㅋㅋㅋ정말 필요할때 보는군",
          "original_form": "ㅋㅋㅋㅋ정말 필요할때 보는군",
          "speaker_id": "1",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100029881.1.26",
          "form": "그러라거 있는 거 아냐?",
          "original_form": "그러라거 있는 거 아냐?",
