      "utterance": [
        {
          "id": "MDRW2100013950.1.1",
          "form": "안녕하세요! 직장 다니고 계신가요?",
          "original_form": "안녕하세요! 직장 다니고 계신가요?",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100013950.1.2",
          "form": "안녕하세요!name1",
          "original_form": "안녕하세요!&name1&",
          "speaker_id": "2",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100013950.1.3",
          "form": "31일자로 계약만료해서",
          "original_form": "31일자로 계약만료해서",
          "speaker_id": "2",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100013950.1.4",
          "form": "집에있습니다!ㅋㅋㅋ",
          "original_form": "집에있습니다!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100013950.1.5",
          "form": "오! 그럼 쉬고 계시는 중이시겠네요!!",
          "original_form": "오! 그럼 쉬고 계시는 중이시겠네요!!",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100013950.1.6",
          "form": "열심히 일하시다가 쉬셔서 좋으시겠어요!!",
          "original_form": "열심히 일하시다가 쉬셔서 좋으시겠어요!!",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100013950.1.7",
          "form": "네 좋은건 3일가고",
          "original_form": "네 좋은건 3일가고",
          "speaker_id": "2",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100013950.1.8",
          "form": "급우울해지고있어요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "급우울해지고있어요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100013950.1.9",
          "form": "먹이를 찾아 산기슭을 찾아 헤메이는 하이에나 마냥",
          "original_form": "먹이를 찾아 산기슭을 찾아 헤메이는 하이에나 마냥",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100013950.1.10",
          "form": "일자리를 찾아보고있습니다만..",
          "original_form": "일자리를 찾아보고있습니다만..",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100013950.1.11",
          "form": "안보이네요",
          "original_form": "안보이네요",
          "speaker_id": "2",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100013950.1.12",
          "form": "요즘 일구하기가 어렵죠ㅠㅠ",
          "original_form": "요즘 일구하기가 어렵죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100013950.1.13",
          "form": "직장구하기도 구하기인데 알바조차도 구하기가 너무 힘들더라구여ㅠㅠ",
          "original_form": "직장구하기도 구하기인데 알바조차도 구하기가 너무 힘들더라구여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100013950.1.14",
          "form": "맞아요ㅠㅠ 슬퍼요ㅜㅜ",
          "original_form": "맞아요ㅠㅠ 슬퍼요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100013950.1.15",
          "form": "쉬는게 쉬는게 아니시겠아요ㅠㅠㅠ",
          "original_form": "쉬는게 쉬는게 아니시겠아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100013950.1.16",
          "form": "알바라도 하고싶은데.ㅠㅠ",
          "original_form": "알바라도 하고싶은데.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100013950.1.17",
          "form": "ㅎㅎㅎ 쉬니까 좋기는해요^^;",
          "original_form": "ㅎㅎㅎ 쉬니까 좋기는해요^^;",
          "speaker_id": "2",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100013950.1.18",
          "form": "알바 구하는게 더 어려운거 같아요ㅠㅠ",
          "original_form": "알바 구하는게 더 어려운거 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100013950.1.19",
          "form": "애들 유치원보내고",
          "original_form": "애들 유치원보내고",
          "speaker_id": "2",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100013950.1.20",
          "form": "제 자유시간도 생기고",
          "original_form": "제 자유시간도 생기고",
          "speaker_id": "2",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100013950.1.21",
          "form": "맞아요 그냥 일자체가 그닥 없네요.ㅠ",
          "original_form": "맞아요 그냥 일자체가 그닥 없네요.ㅠ",
          "speaker_id": "2",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100013950.1.22",
          "form": "알바천국 알바몬을 아무리 뒤져도 안나오더라구요ㅠㅠ",
          "original_form": "알바천국 알바몬을 아무리 뒤져도 안나오더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100013950.1.23",
          "form": "그나마 나오는데는 쿠팡이던데",
          "original_form": "그나마 나오는데는 쿠팡이던데",
          "speaker_id": "1",
          "time": "20210906 19:23"
        },
        {
          "id": "MDRW2100013950.1.24",
          "form": "시간대가 밤이더라구요ㅠㅠ",
          "original_form": "시간대가 밤이더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100013950.1.25",
          "form": "ㅠㅠ 저희도 지역특성상 일자리가 너무없어요",
          "original_form": "ㅠㅠ 저희도 지역특성상 일자리가 너무없어요",
          "speaker_id": "2",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100013950.1.26",
          "form": "일자리가 지역에 상관없이 많았으면 좋겠어요ㅠㅠ",
          "original_form": "일자리가 지역에 상관없이 많았으면 좋겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:28"
        },
        {
          "id": "MDRW2100013950.1.27",
          "form": "있는데는 많은데 없는데는 또 너무 없으니까 뭔가 현타오는 기분이 들때도 있더라구요ㅠㅠ",
          "original_form": "있는데는 많은데 없는데는 또 너무 없으니까 뭔가 현타오는 기분이 들때도 있더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:29"
        },
        {
          "id": "MDRW2100013950.1.28",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100013950.1.29",
          "form": "저흰..",
          "original_form": "저흰..",
          "speaker_id": "2",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100013950.1.30",
          "form": "고용위기지역이에요.ㅋ",
          "original_form": "고용위기지역이에요.ㅋ",
          "speaker_id": "2",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100013950.1.31",
          "form": "ㅠㅠ 안습..",
          "original_form": "ㅠㅠ 안습..",
          "speaker_id": "2",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100013950.1.32",
          "form": "헐 진짜요??",
          "original_form": "헐 진짜요??",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100013950.1.33",
          "form": "그럼 진짜 일자리 구하는게 하늘의 별따기겠어요ㅠㅠ",
          "original_form": "그럼 진짜 일자리 구하는게 하늘의 별따기겠어요ㅠㅠ",
