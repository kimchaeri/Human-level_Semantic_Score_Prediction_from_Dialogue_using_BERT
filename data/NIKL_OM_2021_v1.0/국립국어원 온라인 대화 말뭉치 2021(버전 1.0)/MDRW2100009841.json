      "utterance": [
        {
          "id": "MDRW2100009841.1.1",
          "form": "대전도 장난아니지",
          "original_form": "대전도 장난아니지",
          "speaker_id": "2",
          "time": "20210823 11:18"
        },
        {
          "id": "MDRW2100009841.1.2",
          "form": "집값이랑 또이또이해",
          "original_form": "집값이랑 또이또이해",
          "speaker_id": "2",
          "time": "20210823 11:19"
        },
        {
          "id": "MDRW2100009841.1.3",
          "form": "나 지금 월세인데 미치갔어",
          "original_form": "나 지금 월세인데 미치갔어",
          "speaker_id": "1",
          "time": "20210823 11:23"
        },
        {
          "id": "MDRW2100009841.1.4",
          "form": "캥거루처럼 옥천에 얹혀사는게 돈모으는 길이야..",
          "original_form": "캥거루처럼 옥천에 얹혀사는게 돈모으는 길이야..",
          "speaker_id": "2",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009841.1.5",
          "form": "자취하고싶어도 할 수 없어",
          "original_form": "자취하고싶어도 할 수 없어",
          "speaker_id": "2",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009841.1.6",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009841.1.7",
          "form": "돈 모으려면 아무래도 그게 낫지",
          "original_form": "돈 모으려면 아무래도 그게 낫지",
          "speaker_id": "1",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009841.1.8",
          "form": "너무비싸서 힘들어 진짜",
          "original_form": "너무비싸서 힘들어 진짜",
          "speaker_id": "2",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009841.1.9",
          "form": "나라가 어찌되려고 이러는건지",
          "original_form": "나라가 어찌되려고 이러는건지",
          "speaker_id": "2",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009841.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210823 12:14"
        },
        {
          "id": "MDRW2100009841.1.11",
          "form": "월세땜시룽 나도 힘들구려",
          "original_form": "월세땜시룽 나도 힘들구려",
          "speaker_id": "1",
          "time": "20210823 12:14"
        },
        {
          "id": "MDRW2100009841.1.12",
          "form": "승호루 식사 맛있게해",
          "original_form": "승호루 식사 맛있게해",
          "speaker_id": "1",
          "time": "20210823 12:22"
        },
        {
          "id": "MDRW2100009841.1.13",
          "form": "이걸루 대홧내",
          "original_form": "이걸루 대홧내",
          "speaker_id": "1",
          "time": "20210823 12:37"
        },
        {
          "id": "MDRW2100009841.1.14",
          "form": "이걸루 하자규",
          "original_form": "이걸루 하자규",
          "speaker_id": "1",
          "time": "20210823 12:37"
        },
        {
          "id": "MDRW2100009841.1.15",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "2",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009841.1.16",
          "form": "점심반찬머나왔어?",
          "original_form": "점심반찬머나왔어?",
          "speaker_id": "2",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009841.1.17",
          "form": "구내식당서먹어??",
          "original_form": "구내식당서먹어??",
          "speaker_id": "2",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009841.1.18",
          "form": "다음달 결혼하는 직원이",
          "original_form": "다음달 결혼하는 직원이",
          "speaker_id": "1",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100009841.1.19",
          "form": "육회도시락 돌렸어",
          "original_form": "육회도시락 돌렸어",
          "speaker_id": "1",
          "time": "20210823 13:14"
        },
        {
          "id": "MDRW2100009841.1.20",
          "form": "오아",
          "original_form": "오아",
          "speaker_id": "2",
          "time": "20210823 13:34"
        },
        {
          "id": "MDRW2100009841.1.21",
          "form": "스케일이 다르네",
          "original_form": "스케일이 다르네",
          "speaker_id": "2",
          "time": "20210823 13:34"
        },
        {
          "id": "MDRW2100009841.1.22",
          "form": "아니 이정도 안해ㅋㅋㅋ",
          "original_form": "아니 이정도 안해ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100009841.1.23",
          "form": "아 그분이 부르부주아인거야?ㅋㅋ",
          "original_form": "아 그분이 부르부주아인거야?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100009841.1.24",
          "form": "나도 다담주에 시보해제라",
          "original_form": "나도 다담주에 시보해제라",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100009841.1.25",
          "form": "머 돌릴지 생각중",
          "original_form": "머 돌릴지 생각중",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100009841.1.26",
          "form": "떡",
          "original_form": "떡",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100009841.1.27",
          "form": "무난하지 않아?",
          "original_form": "무난하지 않아?",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100009841.1.28",
          "form": "그렇긴한데 시보떡 하지말라고 하니까  ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇긴한데 시보떡 하지말라고 하니까  ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100009841.1.29",
          "form": "아무것도 안해도 상관은 없는데",
          "original_form": "아무것도 안해도 상관은 없는데",
          "speaker_id": "2",
          "time": "20210823 15:33"
        },
        {
          "id": "MDRW2100009841.1.30",
          "form": "떡말고 다른걸하라는거야",
          "original_form": "떡말고 다른걸하라는거야",
          "speaker_id": "1",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100009841.1.31",
          "form": "아니면 아무것도하지말라는거야?",
          "original_form": "아니면 아무것도하지말라는거야?",
          "speaker_id": "1",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100009841.1.32",
          "form": "아무것도 하지마   인데",
          "original_form": "아무것도 하지마   인데",
          "speaker_id": "2",
          "time": "20210823 15:41"
        },
        {
          "id": "MDRW2100009841.1.33",
          "form": "이게 또 사람사는조직사회에서 입 싹닦는것도 현실적으로 쉽지가 않자아 ㅌㅌ",
          "original_form": "이게 또 사람사는조직사회에서 입 싹닦는것도 현실적으로 쉽지가 않자아 ㅌㅌ",
          "speaker_id": "2",
          "time": "20210823 15:41"
        },
        {
          "id": "MDRW2100009841.1.34",
          "form": "흠 분위기를 모르겠고만",
          "original_form": "흠 분위기를 모르겠고만",
          "speaker_id": "1",
          "time": "20210823 15:50"
        },
        {
          "id": "MDRW2100009841.1.35",
          "form": "안한다고 누구도 머라고하진않지만 그냥 내 맘에서 우러러너 하는거지",
          "original_form": "안한다고 누구도 머라고하진않지만 그냥 내 맘에서 우러러너 하는거지",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100009841.1.36",
          "form": "덕분에 6개월 시보 잘 버텼다..",
          "original_form": "덕분에 6개월 시보 잘 버텼다..",
          "speaker_id": "2",
          "time": "20210823 15:51"
        }
      ]
