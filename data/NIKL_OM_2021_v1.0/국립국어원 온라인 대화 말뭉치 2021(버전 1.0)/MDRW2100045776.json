        {
          "id": "MDRW2100045776.1.3",
          "form": "아이스크림 뭐 좋아해",
          "original_form": "아이스크림 뭐 좋아해",
          "speaker_id": "2",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045776.1.4",
          "form": "나는 초코맛 아이스크림",
          "original_form": "나는 초코맛 아이스크림",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045776.1.5",
          "form": "얼마전에 피스타치오 먹었는데 맛있더라",
          "original_form": "얼마전에 피스타치오 먹었는데 맛있더라",
          "speaker_id": "2",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045776.1.6",
          "form": "그 맛 브라보콘도 있잖아",
          "original_form": "그 맛 브라보콘도 있잖아",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045776.1.7",
          "form": "맞아맞아 젤라또는 좋아해?",
          "original_form": "맞아맞아 젤라또는 좋아해?",
          "speaker_id": "2",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045776.1.8",
          "form": "ㅜㅜ 이태리가면 꼭먹잖아",
          "original_form": "ㅜㅜ 이태리가면 꼭먹잖아",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045776.1.9",
          "form": "강남구청 근처에도 젤라또 맛집있대",
          "original_form": "강남구청 근처에도 젤라또 맛집있대",
          "speaker_id": "2",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045776.1.10",
          "form": "오 가까운 데에 있네",
          "original_form": "오 가까운 데에 있네",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045776.1.11",
          "form": "여기는 포장도 예쁘더라",
          "original_form": "여기는 포장도 예쁘더라",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045776.1.12",
          "form": "쌀맛 아이스크림 도 팔아?",
          "original_form": "쌀맛 아이스크림 도 팔아?",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045776.1.13",
          "form": "응 있던것 같아",
          "original_form": "응 있던것 같아",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045776.1.14",
          "form": "나 강릉서 젤라또 사먹었거든",
          "original_form": "나 강릉서 젤라또 사먹었거든",
