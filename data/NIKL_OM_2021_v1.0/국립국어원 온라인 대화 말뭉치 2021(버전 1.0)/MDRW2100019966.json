      "utterance": [
        {
          "id": "MDRW2100019966.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019966.1.2",
          "form": "영화도 좋아하는데 드라마를 요즘 더 많이봐요",
          "original_form": "영화도 좋아하는데 드라마를 요즘 더 많이봐요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019966.1.3",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019966.1.4",
          "form": "한국드라마 많이 봐요 ㅋㅋ",
          "original_form": "한국드라마 많이 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019966.1.5",
          "form": "펜트요?",
          "original_form": "펜트요?",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019966.1.6",
          "form": "그건안봐요 ㅋㅋㅋ 재미없더라구요 지금 오월의 청춘봐요",
          "original_form": "그건안봐요 ㅋㅋㅋ 재미없더라구요 지금 오월의 청춘봐요",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019966.1.7",
          "form": "누구 나오죠?",
          "original_form": "누구 나오죠?",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100019966.1.8",
          "form": "고민시 금새록이요",
          "original_form": "고민시 금새록이요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019966.1.9",
          "form": "뮤지컬은 좋아하세요?",
          "original_form": "뮤지컬은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100019966.1.10",
          "form": "한번더 못봤네요",
          "original_form": "한번더 못봤네요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019966.1.11",
          "form": "아 정말요? 전 연극을 한번도 못봤어요 ㅋㅋ",
          "original_form": "아 정말요? 전 연극을 한번도 못봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019966.1.12",
          "form": "연극은 와이프랑",
          "original_form": "연극은 와이프랑",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019966.1.13",
          "form": "연극 데이트 좋으셨겠어요! 기회되면 뮤지컬도 꼭 보세요! 혹시 자녀분 있으신가요?",
          "original_form": "연극 데이트 좋으셨겠어요! 기회되면 뮤지컬도 꼭 보세요! 혹시 자녀분 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019966.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
