        {
          "id": "MDRW2100024084.1.1",
          "form": "저녁뭐먹었어",
          "original_form": "저녁뭐먹었어",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024084.1.2",
          "form": "저녁엔 남은 된장찌개...",
          "original_form": "저녁엔 남은 된장찌개...",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024084.1.3",
          "form": "다른거먹지 왜먹었어",
          "original_form": "다른거먹지 왜먹었어",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024084.1.4",
          "form": "남은거 좀 아깝길래 먹었어요",
          "original_form": "남은거 좀 아깝길래 먹었어요",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024084.1.5",
          "form": "뭐 드셨어요",
          "original_form": "뭐 드셨어요",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024084.1.6",
          "form": "황태해장국 김치 먹었어",
          "original_form": "황태해장국 김치 먹었어",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024084.1.7",
          "form": "다른 것도 드셨을거 아니야",
          "original_form": "다른 것도 드셨을거 아니야",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024084.1.8",
          "form": "니가 자주먹던거 계란에 먹는거도 나왔어",
          "original_form": "니가 자주먹던거 계란에 먹는거도 나왔어",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024084.1.9",
          "form": "계란에 먹는거가 뭐야",
          "original_form": "계란에 먹는거가 뭐야",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024084.1.10",
          "form": "계란에 부쳐 먹는거",
          "original_form": "계란에 부쳐 먹는거",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024084.1.11",
          "form": "분홍소세지??",
          "original_form": "분홍소세지??",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100024084.1.12",
          "form": "응 그거랑 호박나왔어",
          "original_form": "응 그거랑 호박나왔어",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100024084.1.13",
          "form": "그래도 분홍소세지 맛있어",
          "original_form": "그래도 분홍소세지 맛있어",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100024084.1.14",
          "form": "난 그거 맛없어",
          "original_form": "난 그거 맛없어",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100024084.1.15",
          "form": "그래서 아빠 없을때만 먹고 있어",
          "original_form": "그래서 아빠 없을때만 먹고 있어",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
