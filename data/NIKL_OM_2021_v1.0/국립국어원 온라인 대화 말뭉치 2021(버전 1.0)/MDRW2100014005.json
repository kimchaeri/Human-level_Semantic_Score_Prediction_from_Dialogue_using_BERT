      "utterance": [
        {
          "id": "MDRW2100014005.1.1",
          "form": "저녁은 뭐해먹을까요ㅠ",
          "original_form": "저녁은 뭐해먹을까요ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100014005.1.2",
          "form": "하... 뭐드셨어요?? 저는 오늘 귀가가 너무 늦어서 피자 시켜먹어었어요.",
          "original_form": "하... 뭐드셨어요?? 저는 오늘 귀가가 너무 늦어서 피자 시켜먹어었어요.",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014005.1.3",
          "form": "오 피자 좋지요.",
          "original_form": "오 피자 좋지요.",
          "speaker_id": "1",
          "time": "20210826 08:36"
        },
        {
          "id": "MDRW2100014005.1.4",
          "form": "어디 피자 드셨어요?",
          "original_form": "어디 피자 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 08:36"
        },
        {
          "id": "MDRW2100014005.1.5",
          "form": "알볼로요.ㅋㅋㅋ 자극적이지 않아서 아이들이 잘먹더라고요",
          "original_form": "알볼로요.ㅋㅋㅋ 자극적이지 않아서 아이들이 잘먹더라고요",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100014005.1.6",
          "form": "알볼로 진짜 괜찮죠",
          "original_form": "알볼로 진짜 괜찮죠",
          "speaker_id": "1",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100014005.1.7",
          "form": "거기 매운것도 맛있어요",
          "original_form": "거기 매운것도 맛있어요",
          "speaker_id": "1",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100014005.1.8",
          "form": "흑... 매운거 먹어본게언제더라..",
          "original_form": "흑... 매운거 먹어본게언제더라..",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100014005.1.9",
          "form": "매운거 어떤거 드세요??",
          "original_form": "매운거 어떤거 드세요??",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100014005.1.10",
          "form": "기억이 안나는데",
          "original_form": "기억이 안나는데",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100014005.1.11",
          "form": "알볼로에 하나 있을거예요",
          "original_form": "알볼로에 하나 있을거예요",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100014005.1.12",
          "form": "그거 유명하진 않은데 맛나서 깜놀",
          "original_form": "그거 유명하진 않은데 맛나서 깜놀",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100014005.1.13",
          "form": "뭐지.. 디아블로일까요??ㅋㅋㅋ",
          "original_form": "뭐지.. 디아블로일까요??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100014005.1.14",
          "form": "뭔가 매워보이는 이름이였는뎈ㅋㅋㅋ",
          "original_form": "뭔가 매워보이는 이름이였는뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100014005.1.15",
          "form": "어 그런듯?ㅋㅋㅋㅋ",
          "original_form": "어 그런듯?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100014005.1.16",
          "form": "그런 느낌이에요ㅋ",
          "original_form": "그런 느낌이에요ㅋ",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100014005.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 한번도 못먹어 봤습니다..껄껄껄..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 한번도 못먹어 봤습니다..껄껄껄..",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100014005.1.18",
          "form": "아근데 아이랑 먹으면ㅠ",
          "original_form": "아근데 아이랑 먹으면ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100014005.1.19",
          "form": "오로지 포테이토..",
          "original_form": "오로지 포테이토..",
          "speaker_id": "2",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100014005.1.20",
          "form": "싸서 좋긴한데... 맛있는것도 먹고 싶네요.ㅜㅠㅠ",
          "original_form": "싸서 좋긴한데... 맛있는것도 먹고 싶네요.ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100014005.1.21",
          "form": "그 피자헛에 치즈피자 드셔보셨나요.. 정말 도우 토마토소스 치즈만 올려져있는 피잔데..",
          "original_form": "그 피자헛에 치즈피자 드셔보셨나요.. 정말 도우 토마토소스 치즈만 올려져있는 피잔데..",
          "speaker_id": "2",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100014005.1.22",
          "form": "피자헛은 안 먹은지 오래인데",
          "original_form": "피자헛은 안 먹은지 오래인데",
          "speaker_id": "1",
          "time": "20210826 12:08"
        },
        {
          "id": "MDRW2100014005.1.23",
          "form": "맛있나요ㅋㅋ",
          "original_form": "맛있나요ㅋㅋ",
