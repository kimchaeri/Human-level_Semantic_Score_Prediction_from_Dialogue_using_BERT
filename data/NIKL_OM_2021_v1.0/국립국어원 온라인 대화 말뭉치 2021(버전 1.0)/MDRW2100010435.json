      "utterance": [
        {
          "id": "MDRW2100010435.1.1",
          "form": "ㅋㅋㅋ오늘 저녁메뉴는 뭔가요?",
          "original_form": "ㅋㅋㅋ오늘 저녁메뉴는 뭔가요?",
          "speaker_id": "2",
          "time": "20210823 18:10"
        },
        {
          "id": "MDRW2100010435.1.2",
          "form": "저는 까르보나라불닭입니당",
          "original_form": "저는 까르보나라불닭입니당",
          "speaker_id": "2",
          "time": "20210823 18:10"
        },
        {
          "id": "MDRW2100010435.1.3",
          "form": "헙 맛있는거드시네요",
          "original_form": "헙 맛있는거드시네요",
          "speaker_id": "1",
          "time": "20210823 18:11"
        },
        {
          "id": "MDRW2100010435.1.4",
          "form": "저는 동거가족이 주는거 먹어요 선택권×",
          "original_form": "저는 동거가족이 주는거 먹어요 선택권×",
          "speaker_id": "1",
          "time": "20210823 18:11"
        },
        {
          "id": "MDRW2100010435.1.5",
          "form": "그것도 좋은듯해요",
          "original_form": "그것도 좋은듯해요",
          "speaker_id": "2",
          "time": "20210823 18:34"
        },
        {
          "id": "MDRW2100010435.1.6",
          "form": "식사메뉴 고민하지 않는 삶",
          "original_form": "식사메뉴 고민하지 않는 삶",
          "speaker_id": "2",
          "time": "20210823 18:34"
        },
        {
          "id": "MDRW2100010435.1.7",
          "form": "네 감사하게 살고있죠ㅎㅎ!",
          "original_form": "네 감사하게 살고있죠ㅎㅎ!",
          "speaker_id": "1",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100010435.1.8",
          "form": "불닭 하나로는 조금 모자라지않나여",
          "original_form": "불닭 하나로는 조금 모자라지않나여",
          "speaker_id": "1",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010435.1.9",
          "form": "역시 잘 아시는군요",
          "original_form": "역시 잘 아시는군요",
          "speaker_id": "2",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010435.1.10",
          "form": "알새우칩도 먹어서 한껏 불량함을 높여봤습니다",
          "original_form": "알새우칩도 먹어서 한껏 불량함을 높여봤습니다",
          "speaker_id": "2",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010435.1.11",
          "form": "완벽하네요",
          "original_form": "완벽하네요",
          "speaker_id": "1",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100010435.1.12",
          "form": "알새우칩이라니 생각치못한 조합,,,!",
          "original_form": "알새우칩이라니 생각치못한 조합,,,!",
          "speaker_id": "1",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100010435.1.13",
          "form": "ㅋㅋ그냥 먹고싶은걸 때려먹는 거지만요 ㅋㅋ",
          "original_form": "ㅋㅋ그냥 먹고싶은걸 때려먹는 거지만요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100010435.1.14",
          "form": "ㅋㅋㅋ 맛만 있으면되는거아닐까요",
          "original_form": "ㅋㅋㅋ 맛만 있으면되는거아닐까요",
          "speaker_id": "1",
          "time": "20210823 18:51"
        },
        {
          "id": "MDRW2100010435.1.15",
          "form": "옳습니다",
          "original_form": "옳습니다",
          "speaker_id": "2",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010435.1.16",
          "form": "그리고 이런과정에서 새 조합도 찾게되지 않을까 생각도 해봅니다",
          "original_form": "그리고 이런과정에서 새 조합도 찾게되지 않을까 생각도 해봅니다",
          "speaker_id": "2",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010435.1.17",
          "form": "가래떡+김 식빵+김을 그렇게 발견했어요 ㅌㅋ",
          "original_form": "가래떡+김 식빵+김을 그렇게 발견했어요 ㅌㅋ",
          "speaker_id": "2",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010435.1.18",
          "form": "그져 꿀조합은 시행착오후에 나오는건데",
          "original_form": "그져 꿀조합은 시행착오후에 나오는건데",
          "speaker_id": "1",
          "time": "20210823 18:53"
        },
        {
          "id": "MDRW2100010435.1.19",
          "form": "김? 조미김인가요?",
          "original_form": "김? 조미김인가요?",
          "speaker_id": "1",
          "time": "20210823 18:53"
        },
        {
          "id": "MDRW2100010435.1.20",
          "form": "조미김은 더 짝짝붙는데 그냥 전장김도 맛있더라고요",
          "original_form": "조미김은 더 짝짝붙는데 그냥 전장김도 맛있더라고요",
          "speaker_id": "2",
          "time": "20210823 18:58"
        },
        {
          "id": "MDRW2100010435.1.21",
          "form": "원래 해조류 사랑해요",
          "original_form": "원래 해조류 사랑해요",
          "speaker_id": "2",
          "time": "20210823 18:58"
        },
        {
          "id": "MDRW2100010435.1.22",
          "form": "오 그럼 혹시 매생이국도 좋아하시나요",
          "original_form": "오 그럼 혹시 매생이국도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010435.1.23",
          "form": "너무좋아하죠",
          "original_form": "너무좋아하죠",
          "speaker_id": "2",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010435.1.24",
          "form": "매생이 조랭이떡국 최고 존엄입니다",
          "original_form": "매생이 조랭이떡국 최고 존엄입니다",
          "speaker_id": "2",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010435.1.25",
          "form": "처음에 매생이굴국에 홀랑 데었던 쓰라린 기억이 있지만.. 그래도 맛있어요",
          "original_form": "처음에 매생이굴국에 홀랑 데었던 쓰라린 기억이 있지만.. 그래도 맛있어요",
          "speaker_id": "2",
          "time": "20210823 19:04"
        },
        {
          "id": "MDRW2100010435.1.26",
          "form": "그죠 크 맛잘알",
          "original_form": "그죠 크 맛잘알",
          "speaker_id": "1",
          "time": "20210823 19:04"
        },
        {
          "id": "MDRW2100010435.1.27",
          "form": "굴은 진짜 조심히 드셔야해요 혀도 같이 익음",
          "original_form": "굴은 진짜 조심히 드셔야해요 혀도 같이 익음",
