      "utterance": [
        {
          "id": "MDRW2100045671.1.1",
          "form": "참치요?? 오 진짜 새롭네요 저도 아침에 해볼까요",
          "original_form": "참치요?? 오 진짜 새롭네요 저도 아침에 해볼까요",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045671.1.2",
          "form": "물 넣고 참치 넣고 연두 좀 뿌려서 전자렌지 행!",
          "original_form": "물 넣고 참치 넣고 연두 좀 뿌려서 전자렌지 행!",
          "speaker_id": "2",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045671.1.3",
          "form": "맛있어요ㅋㅋㅋㅋㅋ 속도 든든하고",
          "original_form": "맛있어요ㅋㅋㅋㅋㅋ 속도 든든하고",
          "speaker_id": "2",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045671.1.4",
          "form": "연두! 없어요 ㅠㅠ 대체할게 뭐가 있을까요",
          "original_form": "연두! 없어요 ㅠㅠ 대체할게 뭐가 있을까요",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045671.1.5",
          "form": "치킨스톡이나 다시다가 가장 좋긴 한데",
          "original_form": "치킨스톡이나 다시다가 가장 좋긴 한데",
          "speaker_id": "2",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045671.1.6",
          "form": "다 없으면 맛소금같은 것도 괜찮을 것 같아요!",
          "original_form": "다 없으면 맛소금같은 것도 괜찮을 것 같아요!",
          "speaker_id": "2",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045671.1.7",
          "form": "그럼 참치죽 맛 나요?",
          "original_form": "그럼 참치죽 맛 나요?",
          "speaker_id": "1",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045671.1.8",
          "form": "참치죽에서 조금 더 서쪽으로 간 맛이나요 ㅋㅋㅋㅋ",
          "original_form": "참치죽에서 조금 더 서쪽으로 간 맛이나요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045671.1.9",
          "form": "오트밀 효과",
          "original_form": "오트밀 효과",
          "speaker_id": "2",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045671.1.10",
          "form": "간장넣으면 좀 더 동쪽으로 간 맛이 날 것 같아요ㅋㅋㅋ",
          "original_form": "간장넣으면 좀 더 동쪽으로 간 맛이 날 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045671.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 무슨 느낌일지 굼금허네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 무슨 느낌일지 굼금허네요",
          "speaker_id": "1",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045671.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ한 번 해봐요 츄라이츄라이!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ한 번 해봐요 츄라이츄라이!!",
          "speaker_id": "2",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045671.1.13",
          "form": "유튜브에 레시피 영상도 있었던 것 같아요",
          "original_form": "유튜브에 레시피 영상도 있었던 것 같아요",
          "speaker_id": "2",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045671.1.14",
          "form": "name2님 믿고 해볼게요",
          "original_form": "&name2&님 믿고 해볼게요",
          "speaker_id": "1",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045671.1.15",
          "form": "저 저녁 안먹엌ㅅ어요",
          "original_form": "저 저녁 안먹엌ㅅ어요",
          "speaker_id": "1",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045671.1.16",
          "form": "name2님 믿어도 대져?",
          "original_form": "&name2&님 믿어도 대져?",
          "speaker_id": "1",
          "time": "20210908 05:13"
        },
        {
          "id": "MDRW2100045671.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ갑자기 부담되는걸욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ갑자기 부담되는걸욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045671.1.18",
          "form": "근데 실패하기는 힘든 맛이에요!",
          "original_form": "근데 실패하기는 힘든 맛이에요!",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045671.1.19",
          "form": "츄라이츄라이!",
          "original_form": "츄라이츄라이!",
          "speaker_id": "2",
          "time": "20210908 05:14"
        },
        {
          "id": "MDRW2100045671.1.20",
          "form": "ㅋㅋㅋㅋ 알겠어요 해볼게요 아진짜 너무 배고프다",
          "original_form": "ㅋㅋㅋㅋ 알겠어요 해볼게요 아진짜 너무 배고프다",
          "speaker_id": "1",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045671.1.21",
          "form": "지금 뭐 먹는건 오바죠?",
          "original_form": "지금 뭐 먹는건 오바죠?",
          "speaker_id": "1",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045671.1.22",
          "form": "배고프면 드셔야죠",
          "original_form": "배고프면 드셔야죠",
          "speaker_id": "2",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045671.1.23",
          "form": "몸이 음식을 필요로 하기 때문에 허기가 있는 법",
          "original_form": "몸이 음식을 필요로 하기 때문에 허기가 있는 법",
          "speaker_id": "2",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045671.1.24",
          "form": ".ㅠㅠ하지만 지금은 족금 그래요",
          "original_form": ".ㅠㅠ하지만 지금은 족금 그래요",
          "speaker_id": "1",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045671.1.25",
          "form": "좀만 더 버텨볼게요",
          "original_form": "좀만 더 버텨볼게요",
          "speaker_id": "1",
          "time": "20210908 05:16"
        },
        {
          "id": "MDRW2100045671.1.26",
          "form": "ㅠㅠㅠ화이팅이에요",
          "original_form": "ㅠㅠㅠ화이팅이에요",
          "speaker_id": "2",
          "time": "20210908 05:17"
        },
        {
          "id": "MDRW2100045671.1.27",
          "form": "제 기준 6시부터는 야식이 아니라 아침식사에요!!",
          "original_form": "제 기준 6시부터는 야식이 아니라 아침식사에요!!",
          "speaker_id": "2",
          "time": "20210908 05:17"
        },
        {
