      "utterance": [
        {
          "id": "MDRW2100005383.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005383.1.2",
          "form": "안 키워요",
          "original_form": "안 키워요",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005383.1.3",
          "form": "키우고 싶은데 코로나 끝나면",
          "original_form": "키우고 싶은데 코로나 끝나면",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005383.1.4",
          "form": "집에 비는 시간 많아지니까 못 키우겠어요",
          "original_form": "집에 비는 시간 많아지니까 못 키우겠어요",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005383.1.5",
          "form": "저도 일때문에",
          "original_form": "저도 일때문에",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005383.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005383.1.7",
          "form": "같이 있고 싶은데 여건이",
          "original_form": "같이 있고 싶은데 여건이",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005383.1.8",
          "form": "맞아요ㅠㅜ",
          "original_form": "맞아요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005383.1.9",
          "form": "애정쏟을 시간이 없어요",
          "original_form": "애정쏟을 시간이 없어요",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005383.1.10",
          "form": "많이 바쁘신가봐요",
          "original_form": "많이 바쁘신가봐요",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005383.1.11",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005383.1.12",
          "form": "아마도 일을 하니",
          "original_form": "아마도 일을 하니",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005383.1.13",
          "form": "보통 몇 시에 퇴근하세요?",
          "original_form": "보통 몇 시에 퇴근하세요?",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005383.1.14",
          "form": "9시요",
          "original_form": "9시요",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005383.1.15",
          "form": "늦게 퇴근하시네요",
          "original_form": "늦게 퇴근하시네요",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005383.1.16",
          "form": "대부분 드렇죠",
          "original_form": "대부분 드렇죠",
          "speaker_id": "1",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005383.1.17",
          "form": "9시 퇴근이면 애정 쏟을 시간이 없을 만 하네요",
          "original_form": "9시 퇴근이면 애정 쏟을 시간이 없을 만 하네요",
          "speaker_id": "2",
          "time": "20210821 22:28"
        }
