      "utterance": [
        {
          "id": "MDRW2100024259.1.1",
          "form": "단발로?",
          "original_form": "단발로?",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024259.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024259.1.3",
          "form": "난 머리 기르고 싶은데",
          "original_form": "난 머리 기르고 싶은데",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024259.1.4",
          "form": "얼마나?",
          "original_form": "얼마나?",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024259.1.5",
          "form": "머리 기르다 답답해서 짜르게 됨",
          "original_form": "머리 기르다 답답해서 짜르게 됨",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024259.1.6",
          "form": "맞아 기르는 거 쉽지 않아",
          "original_form": "맞아 기르는 거 쉽지 않아",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024259.1.7",
          "form": "그냥 계속 기르는거지",
          "original_form": "그냥 계속 기르는거지",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024259.1.8",
          "form": "답답해서",
          "original_form": "답답해서",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100024259.1.9",
          "form": "잊고 살아야 길어지더라",
          "original_form": "잊고 살아야 길어지더라",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100024259.1.10",
          "form": "ㅋㅋㅋㅋ 그니까",
          "original_form": "ㅋㅋㅋㅋ 그니까",
          "speaker_id": "2",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024259.1.11",
          "form": "대강 묶고 다니면서 잊고 살다보면 길어져ㅋㅋㅋ",
          "original_form": "대강 묶고 다니면서 잊고 살다보면 길어져ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024259.1.12",
          "form": "ㅋㅋㅋ 여름에 기르려고 한게 실수였어",
          "original_form": "ㅋㅋㅋ 여름에 기르려고 한게 실수였어",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024259.1.13",
          "form": "여름엔 아예 묶을 수 있는 수준 아니면 못길러ㅋㅋㅋ",
          "original_form": "여름엔 아예 묶을 수 있는 수준 아니면 못길러ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100024259.1.14",
          "form": "뒤에서 땀이나",
          "original_form": "뒤에서 땀이나",
