      "utterance": [
        {
          "id": "MDRW2100005833.1.1",
          "form": "도서관에서 책 빌리고 이제 지하철 기다리는중",
          "original_form": "도서관에서 책 빌리고 이제 지하철 기다리는중",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100005833.1.2",
          "form": "9월엔 연체 도서가 있어도 반납하면 바로 대출규제 풀어준대ㅋㅋ",
          "original_form": "9월엔 연체 도서가 있어도 반납하면 바로 대출규제 풀어준대ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100005833.1.3",
          "form": "도서관 홍보물에 써있더라",
          "original_form": "도서관 홍보물에 써있더라",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100005833.1.4",
          "form": "아 그럼 낼 안반납하구",
          "original_form": "아 그럼 낼 안반납하구",
          "speaker_id": "1",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100005833.1.5",
          "form": "하루이틀 연체하게? ㅋㅋ",
          "original_form": "하루이틀 연체하게? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100005833.1.6",
          "form": "내일 꼭 반납해야한다는 마음의 짐을 덜었다",
          "original_form": "내일 꼭 반납해야한다는 마음의 짐을 덜었다",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100005833.1.7",
          "form": "이거지",
          "original_form": "이거지",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100005833.1.8",
          "form": "오키오키",
          "original_form": "오키오키",
          "speaker_id": "1",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100005833.1.9",
          "form": "밖에 비옵니까?",
          "original_form": "밖에 비옵니까?",
          "speaker_id": "1",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100005833.1.10",
          "form": "응 비옵니다",
          "original_form": "응 비옵니다",
          "speaker_id": "2",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100005833.1.11",
          "form": "도서관에 반납하러갈까 했는데",
          "original_form": "도서관에 반납하러갈까 했는데",
          "speaker_id": "1",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100005833.1.12",
          "form": "미뤄야겠군",
          "original_form": "미뤄야겠군",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005833.1.13",
          "form": "아아 많이는 안와",
          "original_form": "아아 많이는 안와",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005833.1.14",
          "form": "그치만 나오긴 별로인 날씨",
          "original_form": "그치만 나오긴 별로인 날씨",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005833.1.15",
          "form": "책 젖을까 신경쓰기 뭣해서",
          "original_form": "책 젖을까 신경쓰기 뭣해서",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005833.1.16",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005833.1.17",
          "form": "걍 내일 오전에 반납해야겠당",
          "original_form": "걍 내일 오전에 반납해야겠당",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005833.1.18",
          "form": "내일 하는 게 좋겠어",
          "original_form": "내일 하는 게 좋겠어",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005833.1.19",
          "form": "책도 언니도 뽀송뽀송",
          "original_form": "책도 언니도 뽀송뽀송",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005833.1.20",
          "form": "이제 실시간수업 없어서 오전중에도 반납 가능하지",
          "original_form": "이제 실시간수업 없어서 오전중에도 반납 가능하지",
          "speaker_id": "1",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100005833.1.21",
