      "utterance": [
        {
          "id": "MDRW2100011556.1.1",
          "form": "싸이월드 부활했나봐요",
          "original_form": "싸이월드 부활했나봐요",
          "speaker_id": "1",
          "time": "20210825 12:44"
        },
        {
          "id": "MDRW2100011556.1.2",
          "form": "벌써요? 싸이월드 부활 예정인 거 아닌가요?",
          "original_form": "벌써요? 싸이월드 부활 예정인 거 아닌가요?",
          "speaker_id": "2",
          "time": "20210825 12:45"
        },
        {
          "id": "MDRW2100011556.1.3",
          "form": "아 아직 아니네요 ㅎㅎ",
          "original_form": "아 아직 아니네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:45"
        },
        {
          "id": "MDRW2100011556.1.4",
          "form": "싸이월드에는 흑역사도 많지만 추억의 저장소라서 부활되는 거 찬성이에요ㅋㅋ",
          "original_form": "싸이월드에는 흑역사도 많지만 추억의 저장소라서 부활되는 거 찬성이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100011556.1.5",
          "form": "부활하면 싸이하실거에요?",
          "original_form": "부활하면 싸이하실거에요?",
          "speaker_id": "1",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100011556.1.6",
          "form": "ㅋㅋ네",
          "original_form": "ㅋㅋ네",
          "speaker_id": "1",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100011556.1.7",
          "form": "그럼요. 저는 싸이월드 다시 이용할 겁니다.",
          "original_form": "그럼요. 저는 싸이월드 다시 이용할 겁니다.",
          "speaker_id": "2",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100011556.1.8",
          "form": "저는 사진백업을 못해서",
          "original_form": "저는 사진백업을 못해서",
          "speaker_id": "1",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100011556.1.9",
          "form": "부활한다니 좋네요",
          "original_form": "부활한다니 좋네요",
          "speaker_id": "1",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100011556.1.10",
          "form": "저도요. 예전 사진들이랑 제가 포도알 모아가며 열심히 썼던 다이어리를 다시 볼 수 있다니, 기대되네요.",
          "original_form": "저도요. 예전 사진들이랑 제가 포도알 모아가며 열심히 썼던 다이어리를 다시 볼 수 있다니, 기대되네요.",
          "speaker_id": "2",
          "time": "20210825 12:47"
        },
        {
          "id": "MDRW2100011556.1.11",
          "form": "포도알 저도 기억나요 ㅎㅎ",
          "original_form": "포도알 저도 기억나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100011556.1.12",
          "form": "모으면 스티커아이템 줬던거같아요",
          "original_form": "모으면 스티커아이템 줬던거같아요",
          "speaker_id": "1",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100011556.1.13",
          "form": "포도알, 도토리... 일촌... 싸이월드 용어들은 하나같이 좀 귀여운데요",
          "original_form": "포도알, 도토리... 일촌... 싸이월드 용어들은 하나같이 좀 귀여운데요",
          "speaker_id": "2",
          "time": "20210825 12:49"
        },
        {
          "id": "MDRW2100011556.1.14",
          "form": "ㅎㅎㅎ그러네요 ㅎㅎ",
          "original_form": "ㅎㅎㅎ그러네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100011556.1.15",
          "form": "친근해요",
          "original_form": "친근해요",
          "speaker_id": "1",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100011556.1.16",
          "form": "그쵸? 사이 좋은 사람들, 싸이월드ㅋㅋㅋㅋ",
          "original_form": "그쵸? 사이 좋은 사람들, 싸이월드ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:50"
        },
        {
          "id": "MDRW2100011556.1.17",
          "form": "억 그거도",
          "original_form": "억 그거도",
          "speaker_id": "1",
          "time": "20210825 12:51"
        },
        {
          "id": "MDRW2100011556.1.18",
          "form": "사이랑 싸이네요 ㅎㅎㅎ",
          "original_form": "사이랑 싸이네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:51"
        },
        {
          "id": "MDRW2100011556.1.19",
          "form": "일촌들이랑 비밀 방명록 주고받고, 비밀 커플다이어리 쓰던 것도 좋았어요ㅋㅋ",
          "original_form": "일촌들이랑 비밀 방명록 주고받고, 비밀 커플다이어리 쓰던 것도 좋았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:51"
        }
      ]
