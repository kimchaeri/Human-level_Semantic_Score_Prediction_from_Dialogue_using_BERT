      "utterance": [
        {
          "id": "MDRW2100017831.1.1",
          "form": "가족이랑 같이 사세요?",
          "original_form": "가족이랑 같이 사세요?",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017831.1.2",
          "form": "저 혼자 살아요~",
          "original_form": "저 혼자 살아요~",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017831.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017831.1.4",
          "form": "저는 동생이랑 둘이요~",
          "original_form": "저는 동생이랑 둘이요~",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017831.1.5",
          "form": "얼마전에 독립했어요",
          "original_form": "얼마전에 독립했어요",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017831.1.6",
          "form": "오~ 그럼 집안일은 어떻게 분담했어요?",
          "original_form": "오~ 그럼 집안일은 어떻게 분담했어요?",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017831.1.7",
          "form": "ㅎㅎㅎ제가 밥해주거나 사주면",
          "original_form": "ㅎㅎㅎ제가 밥해주거나 사주면",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017831.1.8",
          "form": "설거지는 동생이 하는걸로ㅋㅋㅋㅋ",
          "original_form": "설거지는 동생이 하는걸로ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017831.1.9",
          "form": "방청소는 그냥 보이면 조금씩 돌리구요",
          "original_form": "방청소는 그냥 보이면 조금씩 돌리구요",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017831.1.10",
          "form": "오~ 갈등 없이 정해졌나요?",
          "original_form": "오~ 갈등 없이 정해졌나요?",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017831.1.11",
          "form": "당연히....불만 많겠죠ㅋㅋㅋㅋ",
          "original_form": "당연히....불만 많겠죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017831.1.12",
          "form": "그렇지만 월세도 음식값도 제가 내는걸요~",
          "original_form": "그렇지만 월세도 음식값도 제가 내는걸요~",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017831.1.13",
          "form": "돈 안 내는 자.. 집안일을 하라",
          "original_form": "돈 안 내는 자.. 집안일을 하라",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017831.1.14",
          "form": "헐.. 님이 더 불만 많아야 되는 것 아닌가요 ㅠㅠ",
          "original_form": "헐.. 님이 더 불만 많아야 되는 것 아닌가요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017831.1.15",
          "form": "월세는 당연 반반씩 내야죠!",
          "original_form": "월세는 당연 반반씩 내야죠!",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017831.1.16",
          "form": "동생이 학생이라ㅎㅎ",
          "original_form": "동생이 학생이라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017831.1.17",
          "form": "혼자지내면 할일 진짜 많지않아요?",
          "original_form": "혼자지내면 할일 진짜 많지않아요?",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017831.1.18",
          "form": "빨래 청소 설거지 요리 공과금 등등",
          "original_form": "빨래 청소 설거지 요리 공과금 등등",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017831.1.19",
          "form": "맞아요 ㅜㅜ 돈만 많으면 기계가 다 해줄텐데..",
          "original_form": "맞아요 ㅜㅜ 돈만 많으면 기계가 다 해줄텐데..",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017831.1.20",
          "form": "빨래는 세탁기가.. 청소는 청소기가. 설거지는 식기세척기가.. 요리는 에어프라이기가..",
          "original_form": "빨래는 세탁기가.. 청소는 청소기가. 설거지는 식기세척기가.. 요리는 에어프라이기가..",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017831.1.21",
          "form": "그렇지만.. 세탁기에서 꺼내서 널고 청소기 필터비우고.. 에어프라이어 닦고........ㅋㅋㅋㅋ",
          "original_form": "그렇지만.. 세탁기에서 꺼내서 널고 청소기 필터비우고.. 에어프라이어 닦고........ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017831.1.22",
          "form": "집안일 끝도없다##",
          "original_form": "집안일 끝도없다##",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017831.1.23",
          "form": "앗.. 바로 우울해졌어요 ㅋㅋ",
          "original_form": "앗.. 바로 우울해졌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017831.1.24",
          "form": "그냥 가정부를 들여놔야되겠네요.",
          "original_form": "그냥 가정부를 들여놔야되겠네요.",
          "speaker_id": "1",
          "time": "20210826 21:36"
        }
      ]
