      "utterance": [
        {
          "id": "MDRW2100016741.1.1",
          "form": "스포츠 보심 거 있으세요?",
          "original_form": "스포츠 보심 거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016741.1.2",
          "form": "최근에 올림픽 열심히 봤네요",
          "original_form": "최근에 올림픽 열심히 봤네요",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016741.1.3",
          "form": "뭐 보셨나요",
          "original_form": "뭐 보셨나요",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016741.1.4",
          "form": "거의 본 거 같아요",
          "original_form": "거의 본 거 같아요",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016741.1.5",
          "form": "양궁이랑 배구도 보고요",
          "original_form": "양궁이랑 배구도 보고요",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016741.1.6",
          "form": "ㅋㅋㅋㅋㅋ 저랑 비슷하네요!@@!",
          "original_form": "ㅋㅋㅋㅋㅋ 저랑 비슷하네요!@@!",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016741.1.7",
          "form": "너무 긴장하면서 봤어요ㅋㅋㅋ",
          "original_form": "너무 긴장하면서 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016741.1.8",
          "form": "전 거의 못보다시피..",
          "original_form": "전 거의 못보다시피..",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016741.1.9",
          "form": "제가 보면 질까봐ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "제가 보면 질까봐ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016741.1.10",
          "form": "아 그런거 있죠ㅋㅋㅋ",
          "original_form": "아 그런거 있죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016741.1.11",
          "form": "넘 궁금해서 슬쩍 보고요",
          "original_form": "넘 궁금해서 슬쩍 보고요",
          "speaker_id": "1",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016741.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 네.. 이기면 제가봐서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 네.. 이기면 제가봐서",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016741.1.13",
          "form": "지면 제가 안봐서 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "지면 제가 안봐서 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016741.1.14",
          "form": "알죠 ㅎ",
          "original_form": "알죠 ㅎ",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016741.1.15",
          "form": "ㅋㅋ맞아요 요번에 다들 그랬다고 하더라구요",
          "original_form": "ㅋㅋ맞아요 요번에 다들 그랬다고 하더라구요",
          "speaker_id": "1",
          "time": "20210826 16:50"
        },
        {
          "id": "MDRW2100016741.1.16",
          "form": "ㅋㅋㅋ떨려서 죽을 뻔 했습니다",
          "original_form": "ㅋㅋㅋ떨려서 죽을 뻔 했습니다",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100016741.1.17",
          "form": "축구랑 야구도 봤는데",
          "original_form": "축구랑 야구도 봤는데",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
