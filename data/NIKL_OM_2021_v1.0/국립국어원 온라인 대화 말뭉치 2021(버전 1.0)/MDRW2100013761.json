      "utterance": [
        {
          "id": "MDRW2100013761.1.1",
          "form": "공연 어떤거좋아하세요",
          "original_form": "공연 어떤거좋아하세요",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013761.1.2",
          "form": "흑흑 답이업따",
          "original_form": "흑흑 답이업따",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013761.1.3",
          "form": "콘서트가고싶네요ㅋ",
          "original_form": "콘서트가고싶네요ㅋ",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013761.1.4",
          "form": "저도!!!코로나때문에 ㅠㅠ",
          "original_form": "저도!!!코로나때문에 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013761.1.5",
          "form": "많이다니셨어요?",
          "original_form": "많이다니셨어요?",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013761.1.6",
          "form": "공연은 많이 다니진 않았어요~ 많이다니셨어요?",
          "original_form": "공연은 많이 다니진 않았어요~ 많이다니셨어요?",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013761.1.7",
          "form": "그냥 콘서트꾸준히 ? ㅠㅋㅋ 다 막혀서 재밌는게없네요",
          "original_form": "그냥 콘서트꾸준히 ? ㅠㅋㅋ 다 막혀서 재밌는게없네요",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100013761.1.8",
          "form": "콘서트  추천해주세요~",
          "original_form": "콘서트  추천해주세요~",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100013761.1.9",
          "form": "앗...전 케이팝 콘서트 다다니는데 ㅋㅋㅋ ㅋ",
          "original_form": "앗...전 케이팝 콘서트 다다니는데 ㅋㅋㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100013761.1.10",
          "form": "아이유콘서트 좋다라구요 근데 티켓팅이 빡세요ㅠㅠ",
          "original_form": "아이유콘서트 좋다라구요 근데 티켓팅이 빡세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100013761.1.11",
          "form": "와 아이유콘서트 꼭가보고싶어요! 아이유 킬링보이스 혹시보셨어요?",
          "original_form": "와 아이유콘서트 꼭가보고싶어요! 아이유 킬링보이스 혹시보셨어요?",
          "speaker_id": "2",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100013761.1.12",
          "form": "아 네!!진짴ㅋㅋ 노래넘잘부름..",
          "original_form": "아 네!!진짴ㅋㅋ 노래넘잘부름..",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100013761.1.13",
          "form": "진짜 잘하던데요!? 좋아서 자주들어요",
          "original_form": "진짜 잘하던데요!? 좋아서 자주들어요",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100013761.1.14",
          "form": "코로나끝나 면 꼭.. 콘서트함가보세용",
          "original_form": "코로나끝나 면 꼭.. 콘서트함가보세용",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100013761.1.15",
          "form": "또 가고싶은공연잇으신가요 ㅋㅋ",
          "original_form": "또 가고싶은공연잇으신가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100013761.1.16",
          "form": "성시경콘서트요!! 그것도 킬링보이스보고 너무가고싶어졌어요ㅋ",
          "original_form": "성시경콘서트요!! 그것도 킬링보이스보고 너무가고싶어졌어요ㅋ",
          "speaker_id": "2",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100013761.1.17",
          "form": "오 ㅋㅋㅋㅋ저도저도 성시경 ㅋㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋ저도저도 성시경 ㅋㅋㅋㅋㅋ",
