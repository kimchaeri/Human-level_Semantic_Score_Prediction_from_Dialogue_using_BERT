      "utterance": [
        {
          "id": "MDRW2100018973.1.1",
          "form": "종교? ㅡㅡ",
          "original_form": "종교? ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018973.1.2",
          "form": "종교 없어",
          "original_form": "종교 없어",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018973.1.3",
          "form": "예수 좋아하세요?",
          "original_form": "예수 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018973.1.4",
          "form": "아..네",
          "original_form": "아..네",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018973.1.5",
          "form": "예수 믿는 작자들",
          "original_form": "예수 믿는 작자들",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100018973.1.6",
          "form": "감튀교",
          "original_form": "감튀교",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.7",
          "form": "이상한사람들 많더라.",
          "original_form": "이상한사람들 많더라.",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.8",
          "form": "날아다니는 스파게티교",
          "original_form": "날아다니는 스파게티교",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.9",
          "form": "아웃겨",
          "original_form": "아웃겨",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.10",
          "form": "가입하고싶다",
          "original_form": "가입하고싶다",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.11",
          "form": "우리도 종교만들어요",
          "original_form": "우리도 종교만들어요",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.12",
          "form": "나도 종교 내가 만들고싶어",
          "original_form": "나도 종교 내가 만들고싶어",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.13",
          "form": "천재교",
          "original_form": "천재교",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.15",
          "form": "천재교 진짜 있을거같아 이름이",
          "original_form": "천재교 진짜 있을거같아 이름이",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.16",
          "form": "교리는 어떻게 할까",
          "original_form": "교리는 어떻게 할까",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.17",
          "form": "천재만재만 들어오기가능",
          "original_form": "천재만재만 들어오기가능",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.19",
          "form": "교단원 모두가 천재임을 증명함",
          "original_form": "교단원 모두가 천재임을 증명함",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100018973.1.20",
          "form": "그 이외에는 교단을 우러러볼것",
          "original_form": "그 이외에는 교단을 우러러볼것",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018973.1.21",
          "form": "우리테스트에 통과해야",
          "original_form": "우리테스트에 통과해야",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018973.1.22",
          "form": "가입가능",
          "original_form": "가입가능",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018973.1.23",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018973.1.24",
          "form": "일단 예수믿으면",
          "original_form": "일단 예수믿으면",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018973.1.25",
          "form": "들ㅇ어올수없음",
          "original_form": "들ㅇ어올수없음",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018973.1.26",
          "form": "자기 자신만을 믿는자만이",
          "original_form": "자기 자신만을 믿는자만이",
          "speaker_id": "1",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018973.1.27",
          "form": "예수믿으면 천재아님",
          "original_form": "예수믿으면 천재아님",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100018973.1.28",
          "form": "우리 교에 드렁올수있다.",
          "original_form": "우리 교에 드렁올수있다.",
          "speaker_id": "1",
          "time": "20210827 14:12"
        }
      ]
    }
