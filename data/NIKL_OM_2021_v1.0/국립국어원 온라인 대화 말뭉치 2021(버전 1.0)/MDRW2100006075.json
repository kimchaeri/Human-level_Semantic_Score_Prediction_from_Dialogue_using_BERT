      "utterance": [
        {
          "id": "MDRW2100006075.1.1",
          "form": "사서 그날 바로 해먹으면 되겠네요",
          "original_form": "사서 그날 바로 해먹으면 되겠네요",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100006075.1.2",
          "form": "네 ㅎㅎ 2일정도면 뭐",
          "original_form": "네 ㅎㅎ 2일정도면 뭐",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100006075.1.3",
          "form": "컨디션 괜찮쥬",
          "original_form": "컨디션 괜찮쥬",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100006075.1.4",
          "form": "드셔보신 것 중에는 어떤 게 맛있으셨어요?",
          "original_form": "드셔보신 것 중에는 어떤 게 맛있으셨어요?",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100006075.1.5",
          "form": "저는 짬뽕순두부요",
          "original_form": "저는 짬뽕순두부요",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100006075.1.6",
          "form": "진짜 체인점 수준 나옵니다",
          "original_form": "진짜 체인점 수준 나옵니다",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100006075.1.7",
          "form": "하나에 2인분인가요?",
          "original_form": "하나에 2인분인가요?",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006075.1.8",
          "form": "아 그쵸?ㅠ",
          "original_form": "아 그쵸?ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100006075.1.9",
          "form": "1인분 찾으시나요",
          "original_form": "1인분 찾으시나요",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006075.1.10",
          "form": "아뇨 그냥 궁금해서요ㅋㅋ",
          "original_form": "아뇨 그냥 궁금해서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006075.1.11",
          "form": "2일거에요 ㅎㅎ",
          "original_form": "2일거에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100006075.1.12",
          "form": "제 기억에는",
          "original_form": "제 기억에는",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100006075.1.13",
          "form": "생각해보니 제가 샀던 곳은 2인분이라고 했는데 양이 좀 적기도 했었어요",
          "original_form": "생각해보니 제가 샀던 곳은 2인분이라고 했는데 양이 좀 적기도 했었어요",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100006075.1.14",
          "form": "저는 적당했던거 같습니당 ㅋㅋ",
          "original_form": "저는 적당했던거 같습니당 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006075.1.15",
          "form": "이마트 믿으세요!!",
          "original_form": "이마트 믿으세요!!",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006075.1.16",
          "form": "이마트 구경 좀 가야겠네요 ㅋㅋ",
          "original_form": "이마트 구경 좀 가야겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100006075.1.17",
          "form": "네 갓마트!",
          "original_form": "네 갓마트!",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100006075.1.18",
          "form": "믿어보세요",
          "original_form": "믿어보세요",
