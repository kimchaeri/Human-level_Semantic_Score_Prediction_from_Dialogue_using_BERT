      "utterance": [
        {
          "id": "MDRW2100042343.1.1",
          "form": "국물은 아주 조금 있는 요리야",
          "original_form": "국물은 아주 조금 있는 요리야",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042343.1.2",
          "form": "고기도 들어가니",
          "original_form": "고기도 들어가니",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042343.1.3",
          "form": "육지짐승은 안 들어가",
          "original_form": "육지짐승은 안 들어가",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042343.1.4",
          "form": "해산물들어가니",
          "original_form": "해산물들어가니",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042343.1.5",
          "form": "응 해산물 요리야",
          "original_form": "응 해산물 요리야",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042343.1.6",
          "form": "바다친구",
          "original_form": "바다친구",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042343.1.7",
          "form": "찜요리 먹었니",
          "original_form": "찜요리 먹었니",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042343.1.8",
          "form": "찜이라고 해야 되나 비슷한 조리법인데",
          "original_form": "찜이라고 해야 되나 비슷한 조리법인데",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042343.1.9",
          "form": "정확히는 찜 요리가 아냐",
          "original_form": "정확히는 찜 요리가 아냐",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042343.1.10",
          "form": "조개를 쪘니",
          "original_form": "조개를 쪘니",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042343.1.11",
          "form": "아니! 조개 친구 안 들어가",
          "original_form": "아니! 조개 친구 안 들어가",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042343.1.12",
          "form": "오징어 볶음 먹었니",
          "original_form": "오징어 볶음 먹었니",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042343.1.13",
          "form": "아니! 오징어도 안 들어가",
          "original_form": "아니! 오징어도 안 들어가",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042343.1.14",
          "form": "생선이 들어가니",
          "original_form": "생선이 들어가니",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042343.1.15",
          "form": "눼 생선이 들어갑니다",
          "original_form": "눼 생선이 들어갑니다",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042343.1.16",
          "form": "생선찜같은 요리구나",
          "original_form": "생선찜같은 요리구나",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042343.1.17",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042343.1.18",
          "form": "저어엉확히는 찜이 아냐",
          "original_form": "저어엉확히는 찜이 아냐",
