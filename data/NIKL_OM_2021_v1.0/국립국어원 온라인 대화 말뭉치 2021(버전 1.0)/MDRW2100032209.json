      "utterance": [
        {
          "id": "MDRW2100032209.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100032209.1.2",
          "form": "네 좋아하죠ㅎㅎ",
          "original_form": "네 좋아하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032209.1.3",
          "form": "자주 다니시나요?",
          "original_form": "자주 다니시나요?",
          "speaker_id": "1",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100032209.1.4",
          "form": "코로나터지고선 자주 못가요ㅜㅜ",
          "original_form": "코로나터지고선 자주 못가요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100032209.1.5",
          "form": "그렇죠 코로나 진정되면 가장 먼저 가고싶은 곳은요?",
          "original_form": "그렇죠 코로나 진정되면 가장 먼저 가고싶은 곳은요?",
          "speaker_id": "1",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100032209.1.6",
          "form": "전 해외쪽 가고싶어요",
          "original_form": "전 해외쪽 가고싶어요",
          "speaker_id": "2",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100032209.1.7",
          "form": "유럽으로",
          "original_form": "유럽으로",
          "speaker_id": "2",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100032209.1.8",
          "form": "유럽 가보셨나요",
          "original_form": "유럽 가보셨나요",
          "speaker_id": "1",
          "time": "20210905 16:05"
        },
        {
          "id": "MDRW2100032209.1.9",
          "form": "프랑스 영국만요",
          "original_form": "프랑스 영국만요",
          "speaker_id": "2",
          "time": "20210905 16:15"
        },
        {
          "id": "MDRW2100032209.1.10",
          "form": "다른데 더 가보고싶어요",
          "original_form": "다른데 더 가보고싶어요",
          "speaker_id": "2",
          "time": "20210905 16:15"
        },
        {
          "id": "MDRW2100032209.1.11",
          "form": "어디를 더 가보시고 싶나요?",
          "original_form": "어디를 더 가보시고 싶나요?",
          "speaker_id": "1",
          "time": "20210905 16:44"
        },
        {
          "id": "MDRW2100032209.1.12",
          "form": "이탈리아?",
          "original_form": "이탈리아?",
          "speaker_id": "2",
          "time": "20210905 17:56"
        },
        {
          "id": "MDRW2100032209.1.13",
          "form": "이탈리아 좋겠네요 저는 스페인에 가보고싶네요",
          "original_form": "이탈리아 좋겠네요 저는 스페인에 가보고싶네요",
          "speaker_id": "1",
          "time": "20210905 18:50"
        },
        {
          "id": "MDRW2100032209.1.14",
          "form": "가고싶은곳은 넘쳐나는데",
          "original_form": "가고싶은곳은 넘쳐나는데",
          "speaker_id": "2",
          "time": "20210905 18:51"
        },
        {
          "id": "MDRW2100032209.1.15",
          "form": "갈수가 없네요ㅜㅜ",
          "original_form": "갈수가 없네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 18:51"
        },
        {
          "id": "MDRW2100032209.1.16",
          "form": "안타깝죠 기억에 남는 국내여행지는요,",
          "original_form": "안타깝죠 기억에 남는 국내여행지는요,",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100032209.1.17",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100032209.1.18",
          "form": "국내는 음.... 장소보다는 그냥 가서 숯불에 고기구워먹고 얘기하고 그런거 생각나네요",
