      "utterance": [
        {
          "id": "MDRW2100027109.1.1",
          "form": "스포츠좋아하심?",
          "original_form": "스포츠좋아하심?",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100027109.1.2",
          "form": "name1님 운동봐여?  전 올림픽 열심히 봄",
          "original_form": "&name1&님 운동봐여?  전 올림픽 열심히 봄",
          "speaker_id": "2",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100027109.1.3",
          "form": "전 안봤어요ㅜㅜ",
          "original_form": "전 안봤어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027109.1.4",
          "form": "뭐가 재밌었나요?",
          "original_form": "뭐가 재밌었나요?",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027109.1.5",
          "form": "여자배구 감동 감동ㅜㅜㅜ",
          "original_form": "여자배구 감동 감동ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027109.1.6",
          "form": "김연경선수혼자 다 하던데요?",
          "original_form": "김연경선수혼자 다 하던데요?",
          "speaker_id": "1",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027109.1.7",
          "form": "올림픽이 저희 일하는시간에 열려서 보기어렵긴 했쥬",
          "original_form": "올림픽이 저희 일하는시간에 열려서 보기어렵긴 했쥬",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100027109.1.8",
          "form": "오 다른분들도 열심히 뛰었쥬 김연경선수가 세계랭킹1위라",
          "original_form": "오 다른분들도 열심히 뛰었쥬 김연경선수가 세계랭킹1위라",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027109.1.9",
          "form": "일하는 시간에 해도 안봤을듯..",
          "original_form": "일하는 시간에 해도 안봤을듯..",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027109.1.10",
          "form": "잘했음",
          "original_form": "잘했음",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027109.1.11",
          "form": "티비를 안봐서ㅋ",
          "original_form": "티비를 안봐서ㅋ",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027109.1.12",
          "form": "감동의 도가니",
          "original_form": "감동의 도가니",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027109.1.13",
          "form": "근ㄷㅔ 뉴스는 봤어요",
          "original_form": "근ㄷㅔ 뉴스는 봤어요",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027109.1.14",
          "form": "나도 티비없음메",
          "original_form": "나도 티비없음메",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027109.1.15",
          "form": "인터넷서봄",
          "original_form": "인터넷서봄",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027109.1.16",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027109.1.17",
          "form": "양궁도 잘함",
          "original_form": "양궁도 잘함",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027109.1.18",
          "form": "양궁미쳤죠",
          "original_form": "양궁미쳤죠",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100027109.1.19",
          "form": "응? 올림픽 내용은 다 아네요?ㅋㅋㅋㅋ",
          "original_form": "응? 올림픽 내용은 다 아네요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027109.1.20",
          "form": "마조마조",
          "original_form": "마조마조",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027109.1.21",
          "form": "어 어케다아시네요",
          "original_form": "어 어케다아시네요",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027109.1.22",
          "form": "그러게요?",
          "original_form": "그러게요?",
          "speaker_id": "1",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100027109.1.23",
          "form": "저 평창때 못봐서 인도에서 아이피차단하더라구요 ㅜ 이번에 재미나게봤어요",
          "original_form": "저 평창때 못봐서 인도에서 아이피차단하더라구요 ㅜ 이번에 재미나게봤어요",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100027109.1.24",
          "form": "아..평창 재밌었는데ㅋㅋ",
          "original_form": "아..평창 재밌었는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027109.1.25",
          "form": "영미 모름ㅜㅜ",
          "original_form": "영미 모름ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027109.1.26",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027109.1.27",
          "form": "영미영미!",
          "original_form": "영미영미!",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027109.1.28",
          "form": "영미 대체 뭐여ㅜㅜㅜㅜㅜㅜ왜난 못봄ㅜㅜ",
          "original_form": "영미 대체 뭐여ㅜㅜㅜㅜㅜㅜ왜난 못봄ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027109.1.29",
          "form": "뭐 또 동계올림픽 열리면 볼 수 있겠져!",
          "original_form": "뭐 또 동계올림픽 열리면 볼 수 있겠져!",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027109.1.30",
          "form": "다섯달남았음",
          "original_form": "다섯달남았음",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027109.1.31",
          "form": "곧 이네요!",
