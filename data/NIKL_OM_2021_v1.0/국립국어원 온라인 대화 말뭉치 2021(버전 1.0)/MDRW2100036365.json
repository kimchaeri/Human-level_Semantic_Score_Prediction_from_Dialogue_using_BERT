      "utterance": [
        {
          "id": "MDRW2100036365.1.1",
          "form": "저 너무 즐거워요",
          "original_form": "저 너무 즐거워요",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036365.1.2",
          "form": "케이팝 짝꿍 만난 기분....",
          "original_form": "케이팝 짝꿍 만난 기분....",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036365.1.3",
          "form": "저두요 !!!",
          "original_form": "저두요 !!!",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036365.1.4",
          "form": "name1님 또 어떤 그룹 좋아하시나요!!",
          "original_form": "&name1&님 또 어떤 그룹 좋아하시나요!!",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036365.1.5",
          "form": "이렇게 시간이 잘가다니 ㅜㅜ",
          "original_form": "이렇게 시간이 잘가다니 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036365.1.6",
          "form": "저 몬엑두 좋아하는데 ㅎㅎ",
          "original_form": "저 몬엑두 좋아하는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036365.1.7",
          "form": "아까 좋아하신다면서요 !",
          "original_form": "아까 좋아하신다면서요 !",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036365.1.8",
          "form": "네네!! 저 완전 환장해요",
          "original_form": "네네!! 저 완전 환장해요",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036365.1.9",
          "form": "아이엠 사랑하고..",
          "original_form": "아이엠 사랑하고..",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100036365.1.10",
          "form": "채형원 사랑해요",
          "original_form": "채형원 사랑해요",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100036365.1.11",
          "form": "아 제가 본 인물중 ...",
          "original_form": "아 제가 본 인물중 ...",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100036365.1.12",
          "form": "채형원씨가 가장 카메라를 부셔야합니다 ..",
          "original_form": "채형원씨가 가장 카메라를 부셔야합니다 ..",
          "speaker_id": "1",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100036365.1.13",
          "form": "오마이갓 저 실물 본 적 없어요",
          "original_form": "오마이갓 저 실물 본 적 없어요",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100036365.1.14",
          "form": "아 카메라 진짜 유죄다 유죄",
          "original_form": "아 카메라 진짜 유죄다 유죄",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100036365.1.15",
          "form": "근데 화면보다 더 잘생겼더고요?!?!?!?!",
          "original_form": "근데 화면보다 더 잘생겼더고요?!?!?!?!",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100036365.1.16",
          "form": "네 !!!!",
          "original_form": "네 !!!!",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100036365.1.17",
          "form": "저 그 예전에 심지어 신인일때 ! 봤는데요 !",
          "original_form": "저 그 예전에 심지어 신인일때 ! 봤는데요 !",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100036365.1.18",
          "form": "무슨 조각상인줄 알았어요",
          "original_form": "무슨 조각상인줄 알았어요",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036365.1.19",
          "form": "얼굴 겁~~~~나 작구",
          "original_form": "얼굴 겁~~~~나 작구",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036365.1.20",
          "form": "우왁 심지어!!! 카메라 마사지가 없던 신인때...",
          "original_form": "우왁 심지어!!! 카메라 마사지가 없던 신인때...",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036365.1.21",
          "form": "저 특히 그 두터운 입술이",
          "original_form": "저 특히 그 두터운 입술이",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036365.1.22",
          "form": "너무 좋아요 ㅠ.^",
          "original_form": "너무 좋아요 ㅠ.^",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100036365.1.23",
          "form": "ㅡ눈코입이 다 들어가있어요 심지어",
          "original_form": "ㅡ눈코입이 다 들어가있어요 심지어",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036365.1.24",
          "form": "같은 인간이 아닙니다.",
          "original_form": "같은 인간이 아닙니다.",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036365.1.25",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036365.1.26",
          "form": "그리고 전 그 손도 좋아요.. 섬섬옥수..",
          "original_form": "그리고 전 그 손도 좋아요.. 섬섬옥수..",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100036365.1.27",
          "form": "으앗 진짜 맞아요",
          "original_form": "으앗 진짜 맞아요",
          "speaker_id": "2",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036365.1.28",
          "form": "저 그렇게 길쭉하고 멋진 손",
          "original_form": "저 그렇게 길쭉하고 멋진 손",
          "speaker_id": "2",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036365.1.29",
          "form": "처음 봤잖아요!!!!!!@",
          "original_form": "처음 봤잖아요!!!!!!@",
          "speaker_id": "2",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036365.1.30",
          "form": "진짜요 ㅜㅜㅜ",
          "original_form": "진짜요 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036365.1.31",
          "form": "힘없이 웃는 것도 좋고.....",
          "original_form": "힘없이 웃는 것도 좋고.....",
          "speaker_id": "2",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036365.1.32",
          "form": "전 사실 춤 잘추는것도 좋아요 ..",
          "original_form": "전 사실 춤 잘추는것도 좋아요 ..",
          "speaker_id": "1",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036365.1.33",
          "form": "그렇게 안생겨가지곤.. 춤까지 잘추다니 ㅎㅎ",
          "original_form": "그렇게 안생겨가지곤.. 춤까지 잘추다니 ㅎㅎ",
