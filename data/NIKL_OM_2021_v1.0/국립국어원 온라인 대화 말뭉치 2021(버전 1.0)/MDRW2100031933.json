      "utterance": [
        {
          "id": "MDRW2100031933.1.1",
          "form": "배달 음식 자주 드신다면서요",
          "original_form": "배달 음식 자주 드신다면서요",
          "speaker_id": "1",
          "time": "20210905 09:11"
        },
        {
          "id": "MDRW2100031933.1.2",
          "form": "예 배민 좋아해요",
          "original_form": "예 배민 좋아해요",
          "speaker_id": "2",
          "time": "20210905 09:11"
        },
        {
          "id": "MDRW2100031933.1.3",
          "form": "배민이 짱입니다",
          "original_form": "배민이 짱입니다",
          "speaker_id": "2",
          "time": "20210905 09:11"
        },
        {
          "id": "MDRW2100031933.1.4",
          "form": "그리고 아파트 단지 안에 소자 토스트가 있는데",
          "original_form": "그리고 아파트 단지 안에 소자 토스트가 있는데",
          "speaker_id": "2",
          "time": "20210905 09:11"
        },
        {
          "id": "MDRW2100031933.1.5",
          "form": "그 집 토스트 맛있어요",
          "original_form": "그 집 토스트 맛있어요",
          "speaker_id": "2",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100031933.1.6",
          "form": "약간 이삭토스트 짝퉁",
          "original_form": "약간 이삭토스트 짝퉁",
          "speaker_id": "2",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100031933.1.7",
          "form": "토스트 맛있죠",
          "original_form": "토스트 맛있죠",
          "speaker_id": "1",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100031933.1.8",
          "form": "비슷비슷합니다",
          "original_form": "비슷비슷합니다",
          "speaker_id": "2",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100031933.1.9",
          "form": "그렇군요 소자 토스트 처음 들어요",
          "original_form": "그렇군요 소자 토스트 처음 들어요",
          "speaker_id": "1",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100031933.1.10",
          "form": "오 지방에만 있나 봐요",
          "original_form": "오 지방에만 있나 봐요",
          "speaker_id": "2",
          "time": "20210905 09:12"
        },
        {
          "id": "MDRW2100031933.1.11",
          "form": "찾아보니 서울 지방 다 있네요",
          "original_form": "찾아보니 서울 지방 다 있네요",
          "speaker_id": "1",
          "time": "20210905 09:13"
        },
        {
          "id": "MDRW2100031933.1.12",
          "form": "아 있어요?",
          "original_form": "아 있어요?",
          "speaker_id": "2",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100031933.1.13",
          "form": "근데 네이버에 쳐 보니까 나오네요",
          "original_form": "근데 네이버에 쳐 보니까 나오네요",
          "speaker_id": "1",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100031933.1.14",
          "form": "한번 드셔보시면 그냥 그저 그런 토스트 일 겁니다",
          "original_form": "한번 드셔보시면 그냥 그저 그런 토스트 일 겁니다",
          "speaker_id": "2",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100031933.1.15",
          "form": "제 입맛엔",
          "original_form": "제 입맛엔",
          "speaker_id": "2",
          "time": "20210905 09:15"
        },
        {
          "id": "MDRW2100031933.1.16",
          "form": "맞아여 ㅋㅋㅋㅋ",
          "original_form": "맞아여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100031933.1.17",
          "form": "그렇게 만들어서 팔고 단가 맞추는게 참 대단하다 그 생각해요",
          "original_form": "그렇게 만들어서 팔고 단가 맞추는게 참 대단하다 그 생각해요",
          "speaker_id": "1",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100031933.1.18",
          "form": "에그드랍 드셔보셨어요?",
          "original_form": "에그드랍 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100031933.1.19",
          "form": "와이프가 안 먹어 봤다고 하네요",
          "original_form": "와이프가 안 먹어 봤다고 하네요",
          "speaker_id": "2",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100031933.1.20",
          "form": "맛이 있나요",
          "original_form": "맛이 있나요",
          "speaker_id": "2",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100031933.1.21",
          "form": "그거 대학생들한테 인기래요",
          "original_form": "그거 대학생들한테 인기래요",
          "speaker_id": "1",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031933.1.22",
          "form": "한번 먹어 봤는데 부들부들한게 먹을만했어요",
          "original_form": "한번 먹어 봤는데 부들부들한게 먹을만했어요",
          "speaker_id": "1",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031933.1.23",
          "form": "근데 좀 뜨거워요",
          "original_form": "근데 좀 뜨거워요",
          "speaker_id": "1",
          "time": "20210905 09:20"
        },
        {
          "id": "MDRW2100031933.1.24",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031933.1.25",
          "form": "애 그  드 랍 한번 보고 보겠습니다 나중에",
          "original_form": "애 그  드 랍 한번 보고 보겠습니다 나중에",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031933.1.26",
          "form": "먹어",
          "original_form": "먹어",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031933.1.27",
          "form": "볼개용",
          "original_form": "볼개용",
          "speaker_id": "2",
          "time": "20210905 09:21"
        }
      ]
    }
  ]
