      "utterance": [
        {
          "id": "MDRW2100022452.1.1",
          "form": "매운탕에 들어있는 건 왜 살이 없을까..",
          "original_form": "매운탕에 들어있는 건 왜 살이 없을까..",
          "speaker_id": "2",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022452.1.2",
          "form": "그건.. 육수용이라",
          "original_form": "그건.. 육수용이라",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022452.1.3",
          "form": "그럴걸요?",
          "original_form": "그럴걸요?",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022452.1.4",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "2",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022452.1.5",
          "form": "회 같은 건 좋아해요?",
          "original_form": "회 같은 건 좋아해요?",
          "speaker_id": "2",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022452.1.6",
          "form": "어두육미라고 원래 생선은 머리가 제일 맛있대요",
          "original_form": "어두육미라고 원래 생선은 머리가 제일 맛있대요",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022452.1.7",
          "form": "저 해산물 알러지 때문에",
          "original_form": "저 해산물 알러지 때문에",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022452.1.8",
          "form": "우리 엄마도 대가리랑 눈알 좋아해요",
          "original_form": "우리 엄마도 대가리랑 눈알 좋아해요",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022452.1.9",
          "form": "회 이런거 못먹..",
          "original_form": "회 이런거 못먹..",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022452.1.10",
          "form": "눈알이요?",
          "original_form": "눈알이요?",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022452.1.11",
          "form": "허걱 해산물 먹으면 어떻게 돼요?",
          "original_form": "허걱 해산물 먹으면 어떻게 돼요?",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022452.1.12",
          "form": "알러지가 올라오겠죠...?",
          "original_form": "알러지가 올라오겠죠...?",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022452.1.13",
          "form": "피부 두드러기 나요?",
          "original_form": "피부 두드러기 나요?",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022452.1.14",
          "form": "아니면 호흡곤란?",
          "original_form": "아니면 호흡곤란?",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022452.1.15",
          "form": "심하면 호흡곤란도 와요",
          "original_form": "심하면 호흡곤란도 와요",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022452.1.16",
          "form": "허걱 진짜 조심하셔야겠다",
          "original_form": "허걱 진짜 조심하셔야겠다",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022452.1.17",
          "form": "보통 가려움으로 시작하죠",
          "original_form": "보통 가려움으로 시작하죠",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022452.1.18",
          "form": "그런 적 있어요?",
          "original_form": "그런 적 있어요?",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022452.1.19",
          "form": "가렵기 시작하면 얼마나 가요?",
          "original_form": "가렵기 시작하면 얼마나 가요?",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022452.1.20",
          "form": "꽤 오래...?",
          "original_form": "꽤 오래...?",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022452.1.21",
          "form": "하루종일 가려운적도 있고",
          "original_form": "하루종일 가려운적도 있고",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022452.1.22",
          "form": "ㅠㅠ 주로 어디가 가려워요?",
          "original_form": "ㅠㅠ 주로 어디가 가려워요?",
          "speaker_id": "2",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022452.1.23",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "1",
          "time": "20210829 14:19"
        }
      ]
    }
