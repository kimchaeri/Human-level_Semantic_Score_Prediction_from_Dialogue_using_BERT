      "utterance": [
        {
          "id": "MDRW2100027951.1.1",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100027951.1.2",
          "form": "지금당장하고싶지만",
          "original_form": "지금당장하고싶지만",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027951.1.3",
          "form": "이제 11분남았어",
          "original_form": "이제 11분남았어",
          "speaker_id": "1",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027951.1.4",
          "form": "굳 즐퇴하자",
          "original_form": "굳 즐퇴하자",
          "speaker_id": "2",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100027951.1.5",
          "form": "오늘 아침부터 회사 정신어뵤닥",
          "original_form": "오늘 아침부터 회사 정신어뵤닥",
          "speaker_id": "1",
          "time": "20210903 08:34"
        },
        {
          "id": "MDRW2100027951.1.6",
          "form": "나도 출퇴근해야대 ㅎㅎ...",
          "original_form": "나도 출퇴근해야대 ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210903 08:40"
        },
        {
          "id": "MDRW2100027951.1.7",
          "form": "금요일이니까 참자",
          "original_form": "금요일이니까 참자",
          "speaker_id": "2",
          "time": "20210903 08:40"
        },
        {
          "id": "MDRW2100027951.1.8",
          "form": "다음주에 3일만 일하면 쉰다",
          "original_form": "다음주에 3일만 일하면 쉰다",
          "speaker_id": "1",
          "time": "20210903 08:42"
        },
        {
          "id": "MDRW2100027951.1.9",
          "form": "진짜..?",
          "original_form": "진짜..?",
          "speaker_id": "2",
          "time": "20210903 08:42"
        },
        {
          "id": "MDRW2100027951.1.10",
          "form": "부럽닥",
          "original_form": "부럽닥",
          "speaker_id": "2",
          "time": "20210903 08:42"
        },
        {
          "id": "MDRW2100027951.1.11",
          "form": "목 금 연차냈어ㅋㅋㅋ",
          "original_form": "목 금 연차냈어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:42"
        },
        {
          "id": "MDRW2100027951.1.12",
          "form": "히히 시그니엘갈거야",
          "original_form": "히히 시그니엘갈거야",
          "speaker_id": "1",
          "time": "20210903 08:42"
        },
        {
          "id": "MDRW2100027951.1.13",
          "form": "올...나는 휴간뎅 일있어서 밀릴듯",
          "original_form": "올...나는 휴간뎅 일있어서 밀릴듯",
          "speaker_id": "2",
          "time": "20210903 08:43"
        },
        {
          "id": "MDRW2100027951.1.14",
          "form": "또륵 추석이후로 봐야겠다",
          "original_form": "또륵 추석이후로 봐야겠다",
          "speaker_id": "2",
          "time": "20210903 08:43"
        },
        {
          "id": "MDRW2100027951.1.15",
          "form": "맞아 너 10월인가로본다그러지않았니",
          "original_form": "맞아 너 10월인가로본다그러지않았니",
          "speaker_id": "1",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100027951.1.16",
          "form": "웅 근데 10월에 정검있어서 모르겠네 하핫",
          "original_form": "웅 근데 10월에 정검있어서 모르겠네 하핫",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100027951.1.17",
          "form": "못가는거아니냐",
          "original_form": "못가는거아니냐",
          "speaker_id": "1",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100027951.1.18",
          "form": "쉿..그런말하지마",
          "original_form": "쉿..그런말하지마",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100027951.1.19",
          "form": "퇴사말린다...",
          "original_form": "퇴사말린다...",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
