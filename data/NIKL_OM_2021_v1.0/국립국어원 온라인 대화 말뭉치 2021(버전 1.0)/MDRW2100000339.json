        },
        {
          "id": "MDRW2100000339.1.2",
          "form": "애기들 잘 크고있어?",
          "original_form": "애기들 잘 크고있어?",
          "speaker_id": "1",
          "time": "20210820 09:12"
        },
        {
          "id": "MDRW2100000339.1.3",
          "form": "공룡이다 공룡",
          "original_form": "공룡이다 공룡",
          "speaker_id": "2",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100000339.1.4",
          "form": "둘째가 너무큼",
          "original_form": "둘째가 너무큼",
          "speaker_id": "2",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100000339.1.5",
          "form": "첫째보다 더 나감",
          "original_form": "첫째보다 더 나감",
          "speaker_id": "2",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100000339.1.6",
          "form": "28kg 6살",
          "original_form": "28kg 6살",
          "speaker_id": "2",
          "time": "20210820 09:13"
        },
        {
          "id": "MDRW2100000339.1.7",
          "form": "키도 커?",
          "original_form": "키도 커?",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100000339.1.8",
          "form": "키도 나름?",
          "original_form": "키도 나름?",
          "speaker_id": "2",
          "time": "20210820 09:19"
        },
        {
          "id": "MDRW2100000339.1.9",
          "form": "역시 부계 유전자가 좋구만",
          "original_form": "역시 부계 유전자가 좋구만",
          "speaker_id": "1",
          "time": "20210820 09:27"
        },
        {
          "id": "MDRW2100000339.1.10",
          "form": "너도 애 낳아",
          "original_form": "너도 애 낳아",
          "speaker_id": "2",
          "time": "20210820 09:27"
        },
        {
          "id": "MDRW2100000339.1.11",
          "form": "돈 만 모아서 뭐하누",
          "original_form": "돈 만 모아서 뭐하누",
          "speaker_id": "2",
          "time": "20210820 09:27"
        },
        {
          "id": "MDRW2100000339.1.12",
          "form": "나 어제",
          "original_form": "나 어제",
          "speaker_id": "1",
          "time": "20210820 09:29"
        },
        {
          "id": "MDRW2100000339.1.13",
          "form": "일이 좀 있었어",
          "original_form": "일이 좀 있었어",
          "speaker_id": "1",
          "time": "20210820 09:29"
        },
        {
          "id": "MDRW2100000339.1.14",
          "form": "응?",
          "original_form": "응?",
          "speaker_id": "2",
          "time": "20210820 09:29"
        },
        {
          "id": "MDRW2100000339.1.15",
          "form": "아냥",
          "original_form": "아냥",
          "speaker_id": "1",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100000339.1.16",
          "form": "나 지금",
          "original_form": "나 지금",
          "speaker_id": "1",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100000339.1.17",
          "form": "신용대출이랑 회사대출이랑 으마으마하다",
          "original_form": "신용대출이랑 회사대출이랑 으마으마하다",
          "speaker_id": "1",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100000339.1.18",
          "form": "집산다고?",
          "original_form": "집산다고?",
          "speaker_id": "2",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100000339.1.19",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100000339.1.20",
          "form": "울산 집값 비싼갑네",
          "original_form": "울산 집값 비싼갑네",
          "speaker_id": "2",
          "time": "20210820 09:30"
        },
        {
          "id": "MDRW2100000339.1.21",
          "form": "10억넘는 아파트도 간간히 나오더라",
          "original_form": "10억넘는 아파트도 간간히 나오더라",
          "speaker_id": "1",
          "time": "20210820 09:31"
        },
        {
          "id": "MDRW2100000339.1.22",
          "form": "아파트 팔아서 갚으면 되겠네",
          "original_form": "아파트 팔아서 갚으면 되겠네",
          "speaker_id": "2",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100000339.1.23",
          "form": "나도 전세 때문에 처음으로 대출 받음",
          "original_form": "나도 전세 때문에 처음으로 대출 받음",
          "speaker_id": "2",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100000339.1.24",
          "form": "아니 사야해",
          "original_form": "아니 사야해",
          "speaker_id": "1",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100000339.1.25",
          "form": "학자금도 있었군",
          "original_form": "학자금도 있었군",
          "speaker_id": "2",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100000339.1.26",
          "form": "청약이 된게 있는데",
          "original_form": "청약이 된게 있는데",
          "speaker_id": "1",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100000339.1.27",
          "form": "지금 집 전세고",
          "original_form": "지금 집 전세고",
          "speaker_id": "2",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100000339.1.28",
          "form": "그거 지금 중도금 대출받아서 내고있는데 입주전에",
          "original_form": "그거 지금 중도금 대출받아서 내고있는데 입주전에",
