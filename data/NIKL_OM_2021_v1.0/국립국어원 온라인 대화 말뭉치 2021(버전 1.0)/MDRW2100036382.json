      "utterance": [
        {
          "id": "MDRW2100036382.1.1",
          "form": "소프트콘이 완전 화이트색인데",
          "original_form": "소프트콘이 완전 화이트색인데",
          "speaker_id": "1",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100036382.1.2",
          "form": "동네에 맥도날드가없네요ㅠㅠ",
          "original_form": "동네에 맥도날드가없네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100036382.1.3",
          "form": "빽다방 아이스크림은 약간 아이보리색이에요",
          "original_form": "빽다방 아이스크림은 약간 아이보리색이에요",
          "speaker_id": "1",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100036382.1.4",
          "form": "더 진한맛!",
          "original_form": "더 진한맛!",
          "speaker_id": "1",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100036382.1.5",
          "form": "오~",
          "original_form": "오~",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100036382.1.6",
          "form": "비법인가",
          "original_form": "비법인가",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100036382.1.7",
          "form": "연유가들어가나!?",
          "original_form": "연유가들어가나!?",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.8",
          "form": "먹어보고싶네요!",
          "original_form": "먹어보고싶네요!",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.9",
          "form": "네네 아 그렇겠구나",
          "original_form": "네네 아 그렇겠구나",
          "speaker_id": "1",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.10",
          "form": "연유인것같아요",
          "original_form": "연유인것같아요",
          "speaker_id": "1",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.11",
          "form": "맞네",
          "original_form": "맞네",
          "speaker_id": "1",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.12",
          "form": "빽다방은 기본 사이즈가 스벅 벤티라서 좋아요ㅋㅋㅋㅋ",
          "original_form": "빽다방은 기본 사이즈가 스벅 벤티라서 좋아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.13",
          "form": "전 맥도날드 아스쿠림좋아햇어요 맞아요 ㅋㅋㅋ",
          "original_form": "전 맥도날드 아스쿠림좋아햇어요 맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.14",
          "form": "저 가끔 1리터로 사먹엇어요 ㅋㅋㅋ올해",
          "original_form": "저 가끔 1리터로 사먹엇어요 ㅋㅋㅋ올해",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.15",
          "form": "아이스크림 가격이 많이 올랐더라구요ㅠㅠㅠ",
          "original_form": "아이스크림 가격이 많이 올랐더라구요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.16",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.17",
          "form": "여기 1리터도 팔아요??",
          "original_form": "여기 1리터도 팔아요??",
          "speaker_id": "1",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.18",
          "form": "저 마지막으로 700원이엿나",
          "original_form": "저 마지막으로 700원이엿나",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100036382.1.19",
          "form": "넵 ㅋㅋㅋ 겁나큰거",
          "original_form": "넵 ㅋㅋㅋ 겁나큰거",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100036382.1.20",
          "form": "오 가격 웬지",
          "original_form": "오 가격 웬지",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100036382.1.21",
          "form": "삼천원 안넘을것같은...",
          "original_form": "삼천원 안넘을것같은...",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100036382.1.22",
          "form": "빽사이즈 ㅋㅋㅋ3000",
          "original_form": "빽사이즈 ㅋㅋㅋ3000",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100036382.1.23",
          "form": "원이에요 ㅋㅋㅋ",
          "original_form": "원이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100036382.1.24",
          "form": "이얔ㅋㅋㅋㅋㅋ",
          "original_form": "이얔ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100036382.1.25",
          "form": "맞췄네요",
          "original_form": "맞췄네요",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100036382.1.26",
          "form": "우와아",
          "original_form": "우와아",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100036382.1.27",
          "form": "백종원 방식의 단가책정은",
          "original_form": "백종원 방식의 단가책정은",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100036382.1.28",
          "form": "전 더울때 잘얼음으로 요청해서 먹엇어요",
          "original_form": "전 더울때 잘얼음으로 요청해서 먹엇어요",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100036382.1.29",
          "form": "그정도 할것같았어요ㅋㅋㅋ",
          "original_form": "그정도 할것같았어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:13"
        }
      ]
    }
