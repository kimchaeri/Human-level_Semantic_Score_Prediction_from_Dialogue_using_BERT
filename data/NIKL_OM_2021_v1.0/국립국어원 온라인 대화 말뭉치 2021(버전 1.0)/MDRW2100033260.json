      "utterance": [
        {
          "id": "MDRW2100033260.1.1",
          "form": "미역국 좋아하나",
          "original_form": "미역국 좋아하나",
          "speaker_id": "1",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100033260.1.2",
          "form": "오 미역국 좋지.",
          "original_form": "오 미역국 좋지.",
          "speaker_id": "2",
          "time": "20210906 20:22"
        },
        {
          "id": "MDRW2100033260.1.3",
          "form": "갑자기 웬 미역국?",
          "original_form": "갑자기 웬 미역국?",
          "speaker_id": "2",
          "time": "20210906 20:22"
        },
        {
          "id": "MDRW2100033260.1.4",
          "form": "나는 소고기 미역국이 좋음",
          "original_form": "나는 소고기 미역국이 좋음",
          "speaker_id": "1",
          "time": "20210906 20:22"
        },
        {
          "id": "MDRW2100033260.1.5",
          "form": "소고기 미역국 맛있지. 근데 가끔 할머니표 가자미 미역국도 생각난다.",
          "original_form": "소고기 미역국 맛있지. 근데 가끔 할머니표 가자미 미역국도 생각난다.",
          "speaker_id": "2",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100033260.1.6",
          "form": "그게 뭐야 나는 먹어본 기억이 없는데",
          "original_form": "그게 뭐야 나는 먹어본 기억이 없는데",
          "speaker_id": "1",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100033260.1.7",
          "form": "가자미 살 넣고 끓인 건데 맛있다.",
          "original_form": "가자미 살 넣고 끓인 건데 맛있다.",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100033260.1.8",
          "form": "조개 넣고 끓인거랑 비슷하려나",
          "original_form": "조개 넣고 끓인거랑 비슷하려나",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033260.1.9",
          "form": "응 그런 맛. 바다맛.",
          "original_form": "응 그런 맛. 바다맛.",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033260.1.10",
          "form": "미역국은 그래도 소고기지",
          "original_form": "미역국은 그래도 소고기지",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033260.1.11",
          "form": "소고기가 맛있긴 하지.",
          "original_form": "소고기가 맛있긴 하지.",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100033260.1.12",
          "form": "그건 그렇다 올해 생일엔 미역국 못먹음",
          "original_form": "그건 그렇다 올해 생일엔 미역국 못먹음",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100033260.1.13",
          "form": "엄마가 안 끓여주드나?",
          "original_form": "엄마가 안 끓여주드나?",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100033260.1.14",
          "form": "응 올해는 안 끓여줌",
          "original_form": "응 올해는 안 끓여줌",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100033260.1.15",
          "form": "엄마가 생일 까먹었나보네",
          "original_form": "엄마가 생일 까먹었나보네",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100033260.1.16",
          "form": "아니 그냥 안끓여줬다던데",
          "original_form": "아니 그냥 안끓여줬다던데",
          "speaker_id": "1",
          "time": "20210906 20:30"
        }
      ]
