      "utterance": [
        {
          "id": "MDRW2100043955.1.1",
          "form": "기억에 남는 여행지 있으세요?",
          "original_form": "기억에 남는 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043955.1.2",
          "form": "음 최근에 제주도가서 제주가 젤 기억남아요",
          "original_form": "음 최근에 제주도가서 제주가 젤 기억남아요",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043955.1.3",
          "form": "오 제주도 저 어릴때 이후로 안가봐서 너무 가고 싶은데",
          "original_form": "오 제주도 저 어릴때 이후로 안가봐서 너무 가고 싶은데",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043955.1.4",
          "form": "맛있는거 많이 드셨나요 ㅋㅋ",
          "original_form": "맛있는거 많이 드셨나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043955.1.5",
          "form": "고등어회 너무 맛있었어요",
          "original_form": "고등어회 너무 맛있었어요",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043955.1.6",
          "form": "애플망고도 ㅎㅎ",
          "original_form": "애플망고도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043955.1.7",
          "form": "아 제주도에 애플망고도 유명한가요?",
          "original_form": "아 제주도에 애플망고도 유명한가요?",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043955.1.8",
          "form": "유명보다 파는곳이 있어요 ㅋㅋ",
          "original_form": "유명보다 파는곳이 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043955.1.9",
          "form": "요즘 날씨가 이상기온이라",
          "original_form": "요즘 날씨가 이상기온이라",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043955.1.10",
          "form": "재배하시더라구요",
          "original_form": "재배하시더라구요",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043955.1.11",
          "form": "아 농장이 있는거군요",
          "original_form": "아 농장이 있는거군요",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043955.1.12",
          "form": "귤처럼",
          "original_form": "귤처럼",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043955.1.13",
          "form": "그런거같아요 ㅎㅎ",
          "original_form": "그런거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043955.1.14",
          "form": "하지만 가격은 정말 비싸요",
          "original_form": "하지만 가격은 정말 비싸요",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043955.1.15",
          "form": "아 그렇겠어요 제주도꺼면 국산이니까 뭔가 수요도 많을거 같은",
          "original_form": "아 그렇겠어요 제주도꺼면 국산이니까 뭔가 수요도 많을거 같은",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043955.1.16",
          "form": "ㅋㅋ제주도내에서나 유명하죠...물건너오면 그렇지도않은거같아요",
          "original_form": "ㅋㅋ제주도내에서나 유명하죠...물건너오면 그렇지도않은거같아요",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043955.1.17",
          "form": "그러니까요 ㅋㅋㅋ 날씨는 괜찮았나요 엄청 변덕이라던데",
          "original_form": "그러니까요 ㅋㅋㅋ 날씨는 괜찮았나요 엄청 변덕이라던데",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043955.1.18",
          "form": "네 ㅎㅎ 비만 안오면 다 좋은거라 ㅎㅎㅎ 좋았어요 한여름에가서 무진장 더웠지만 ㅎㅎ",
          "original_form": "네 ㅎㅎ 비만 안오면 다 좋은거라 ㅎㅎㅎ 좋았어요 한여름에가서 무진장 더웠지만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043955.1.19",
          "form": "좋았겠어요 부럽네요 제주바다 가보고싶은..",
          "original_form": "좋았겠어요 부럽네요 제주바다 가보고싶은..",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043955.1.20",
          "form": "저도 다시 가고싶어요 ㅎㅎ 제주바다 ㅎㅎ",
          "original_form": "저도 다시 가고싶어요 ㅎㅎ 제주바다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:19"
        }
      ]
