      "utterance": [
        {
          "id": "MDRW2100022126.1.1",
          "form": "어제 빨래 걷었니?",
          "original_form": "어제 빨래 걷었니?",
          "speaker_id": "2",
          "time": "20210829 08:43"
        },
        {
          "id": "MDRW2100022126.1.2",
          "form": "해가 나길래 얼른 걷었죠",
          "original_form": "해가 나길래 얼른 걷었죠",
          "speaker_id": "1",
          "time": "20210829 08:43"
        },
        {
          "id": "MDRW2100022126.1.3",
          "form": "잘했다",
          "original_form": "잘했다",
          "speaker_id": "2",
          "time": "20210829 08:43"
        },
        {
          "id": "MDRW2100022126.1.4",
          "form": "근데 또 비와요",
          "original_form": "근데 또 비와요",
          "speaker_id": "1",
          "time": "20210829 08:44"
        },
        {
          "id": "MDRW2100022126.1.5",
          "form": "당분간 비온대",
          "original_form": "당분간 비온대",
          "speaker_id": "2",
          "time": "20210829 08:44"
        },
        {
          "id": "MDRW2100022126.1.6",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210829 08:44"
        },
        {
          "id": "MDRW2100022126.1.7",
          "form": "일단 새로운 빨래는 접어두고 습기 관리 잘해야 한다",
          "original_form": "일단 새로운 빨래는 접어두고 습기 관리 잘해야 한다",
          "speaker_id": "2",
          "time": "20210829 08:44"
        },
        {
          "id": "MDRW2100022126.1.8",
          "form": "건조기를 사는 건 어때요?",
          "original_form": "건조기를 사는 건 어때요?",
          "speaker_id": "1",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100022126.1.9",
          "form": "건조기까지  사면 집에 놓을데가 없어",
          "original_form": "건조기까지  사면 집에 놓을데가 없어",
          "speaker_id": "2",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100022126.1.10",
          "form": "남들 다 사는데",
          "original_form": "남들 다 사는데",
          "speaker_id": "1",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100022126.1.11",
          "form": "사서 몇번이나 쓰겠니",
          "original_form": "사서 몇번이나 쓰겠니",
          "speaker_id": "2",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100022126.1.12",
          "form": "한국이 갈수록 동남아 기후가 되니까 앞으로 더 필요해질 거예요",
          "original_form": "한국이 갈수록 동남아 기후가 되니까 앞으로 더 필요해질 거예요",
          "speaker_id": "1",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100022126.1.13",
          "form": "글쎄.. 아빠 눈에는 낭비같구나",
          "original_form": "글쎄.. 아빠 눈에는 낭비같구나",
          "speaker_id": "2",
          "time": "20210829 08:45"
        },
        {
          "id": "MDRW2100022126.1.14",
          "form": "우리 에어커도 겨우 얼마전에 샀잖아요",
          "original_form": "우리 에어커도 겨우 얼마전에 샀잖아요",
          "speaker_id": "1",
          "time": "20210829 08:46"
        },
        {
          "id": "MDRW2100022126.1.15",
          "form": "그동안은 버틸만 했으니까",
          "original_form": "그동안은 버틸만 했으니까",
          "speaker_id": "2",
          "time": "20210829 08:46"
        },
        {
          "id": "MDRW2100022126.1.16",
          "form": "근데 사니까 좋잖아요",
          "original_form": "근데 사니까 좋잖아요",
          "speaker_id": "1",
          "time": "20210829 08:46"
        },
        {
          "id": "MDRW2100022126.1.17",
          "form": "확실히 여름이 상쾌해졌어",
          "original_form": "확실히 여름이 상쾌해졌어",
          "speaker_id": "2",
          "time": "20210829 08:46"
        },
        {
          "id": "MDRW2100022126.1.18",
          "form": "건조기도 그래요",
          "original_form": "건조기도 그래요",
          "speaker_id": "1",
          "time": "20210829 08:46"
        },
        {
          "id": "MDRW2100022126.1.19",
          "form": "흠.. 사야하나..",
          "original_form": "흠.. 사야하나..",
          "speaker_id": "2",
          "time": "20210829 08:46"
        },
        {
          "id": "MDRW2100022126.1.20",
          "form": "잘 생각해보세요 사면 좋을거예요",
          "original_form": "잘 생각해보세요 사면 좋을거예요",
          "speaker_id": "1",
          "time": "20210829 08:47"
        },
        {
          "id": "MDRW2100022126.1.21",
          "form": "생각해보마",
          "original_form": "생각해보마",
          "speaker_id": "2",
          "time": "20210829 08:47"
        }
      ]
