      "utterance": [
        {
          "id": "MDRW2100039498.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100039498.1.2",
          "form": "여행 정말 좋아해요 코로나때문에 못가서..",
          "original_form": "여행 정말 좋아해요 코로나때문에 못가서..",
          "speaker_id": "2",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100039498.1.3",
          "form": "저두요 ㅠㅠㅠㅠ",
          "original_form": "저두요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100039498.1.4",
          "form": "코로나끝나면 어디가고 싶으세요?",
          "original_form": "코로나끝나면 어디가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100039498.1.5",
          "form": "전 오키나와랃 베트남이요!",
          "original_form": "전 오키나와랃 베트남이요!",
          "speaker_id": "2",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100039498.1.6",
          "form": "근데 이제 가기 무섭긴하네용 ㅎㅎ 어디 좋아하세용?",
          "original_form": "근데 이제 가기 무섭긴하네용 ㅎㅎ 어디 좋아하세용?",
          "speaker_id": "2",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100039498.1.7",
          "form": "그쵸 코로나끝나도... 그냥 나갈 마음이 잘 안드네요 ㅠㅠ",
          "original_form": "그쵸 코로나끝나도... 그냥 나갈 마음이 잘 안드네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100039498.1.8",
          "form": "저는 미국 ....ㅠㅠ 미국가고싶어요",
          "original_form": "저는 미국 ....ㅠㅠ 미국가고싶어요",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100039498.1.9",
          "form": "맞아요 ㅜㅜㅜ미국도 아직 못가봤는데",
          "original_form": "맞아요 ㅜㅜㅜ미국도 아직 못가봤는데",
          "speaker_id": "2",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100039498.1.10",
          "form": "갑자기 격리시킬까 무섭네요",
          "original_form": "갑자기 격리시킬까 무섭네요",
          "speaker_id": "2",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100039498.1.11",
          "form": "맞아요 ㅠ 그리구 뭔가 중국이 코로나 시작한거라 동양인혐오 무서워서 ㅠㅠ끝나도 가기 무서워요",
          "original_form": "맞아요 ㅠ 그리구 뭔가 중국이 코로나 시작한거라 동양인혐오 무서워서 ㅠㅠ끝나도 가기 무서워요",
          "speaker_id": "1",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100039498.1.12",
          "form": "하긴 그러네용 ㅠㅠㅠ 2년후에는 갈수잇겠죠...?",
          "original_form": "하긴 그러네용 ㅠㅠㅠ 2년후에는 갈수잇겠죠...?",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100039498.1.13",
          "form": "그렇겠죠? ㅠㅠㅠㅠㅠ 2년.....휴 정말 계속 변이가 나와서 너무 무섭네요",
          "original_form": "그렇겠죠? ㅠㅠㅠㅠㅠ 2년.....휴 정말 계속 변이가 나와서 너무 무섭네요",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100039498.1.14",
          "form": "이제 2주격리 안해도 될 것 같긴한데",
          "original_form": "이제 2주격리 안해도 될 것 같긴한데",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100039498.1.15",
          "form": "일본이라도 다녀오고싶네용",
          "original_form": "일본이라도 다녀오고싶네용",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100039498.1.16",
          "form": "백신 접종 완료하면 자가격리 면제되는 나라가 있었던것같아요! 어디였더라...(까먹)",
          "original_form": "백신 접종 완료하면 자가격리 면제되는 나라가 있었던것같아요! 어디였더라...(까먹)",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100039498.1.17",
          "form": "맞아요 10개넘게 있얶는데",
          "original_form": "맞아요 10개넘게 있얶는데",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100039498.1.18",
          "form": "베트남이랑....태국이었나 ㅠㅠ",
          "original_form": "베트남이랑....태국이었나 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100039498.1.19",
          "form": "오오 그래듀 나름 10개넘게있었군요?!?!?!?",
          "original_form": "오오 그래듀 나름 10개넘게있었군요?!?!?!?",
          "speaker_id": "1",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100039498.1.20",
          "form": "근데 격리 면제해준다해도 왠지",
          "original_form": "근데 격리 면제해준다해도 왠지",
          "speaker_id": "1",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100039498.1.21",
          "form": "베트남이랑 태국에도 확진자 많이나오고있죠? ㅠ 무서워서 가기 좀 두렵네요 ㅠ",
          "original_form": "베트남이랑 태국에도 확진자 많이나오고있죠? ㅠ 무서워서 가기 좀 두렵네요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 09:42"
        }
      ]
