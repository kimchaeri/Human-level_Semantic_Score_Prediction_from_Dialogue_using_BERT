      "utterance": [
        {
          "id": "MDRW2100032494.1.1",
          "form": "그래서 다들 집에 있으면 좋겠는데 말 지지리도 안듣지",
          "original_form": "그래서 다들 집에 있으면 좋겠는데 말 지지리도 안듣지",
          "speaker_id": "1",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100032494.1.2",
          "form": "예",
          "original_form": "예",
          "speaker_id": "2",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100032494.1.3",
          "form": "집에서 지냈음",
          "original_form": "집에서 지냈음",
          "speaker_id": "2",
          "time": "20210905 19:33"
        },
        {
          "id": "MDRW2100032494.1.4",
          "form": "남들은...",
          "original_form": "남들은...",
          "speaker_id": "2",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032494.1.5",
          "form": "안지키고    ㅜ",
          "original_form": "안지키고    ㅜ",
          "speaker_id": "2",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032494.1.6",
          "form": "지키는 사람만 고통이지",
          "original_form": "지키는 사람만 고통이지",
          "speaker_id": "2",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032494.1.7",
          "form": "사실 머무르는 사람들 바보료",
          "original_form": "사실 머무르는 사람들 바보료",
          "speaker_id": "1",
          "time": "20210905 19:37"
        },
        {
          "id": "MDRW2100032494.1.8",
          "form": "만든다고 생각해",
          "original_form": "만든다고 생각해",
          "speaker_id": "1",
          "time": "20210905 19:37"
        },
        {
          "id": "MDRW2100032494.1.9",
          "form": "의료진 안불쌍하냐고  물으면 불쌍하고",
          "original_form": "의료진 안불쌍하냐고  물으면 불쌍하고",
          "speaker_id": "1",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100032494.1.10",
          "form": "고생 많이 헌다고 하는데",
          "original_form": "고생 많이 헌다고 하는데",
          "speaker_id": "1",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100032494.1.11",
          "form": "자기 나가는 건 별개더라",
          "original_form": "자기 나가는 건 별개더라",
          "speaker_id": "1",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100032494.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100032494.1.13",
          "form": "하....",
          "original_form": "하....",
          "speaker_id": "2",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100032494.1.14",
          "form": "걸리고 하늘로 가세용",
          "original_form": "걸리고 하늘로 가세용",
          "speaker_id": "2",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100032494.1.15",
          "form": "사실 작년 광복절때도",
          "original_form": "사실 작년 광복절때도",
          "speaker_id": "1",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100032494.1.16",
          "form": "전 광 훈 그",
          "original_form": "전 광 훈 그",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100032494.1.17",
          "form": "하나님 나한테 죽어 갸때문에",
          "original_form": "하나님 나한테 죽어 갸때문에",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100032494.1.18",
          "form": "확산되고",
          "original_form": "확산되고",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100032494.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100032494.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100032494.1.21",
          "form": "그동네 깜빡 죽었다던데",
          "original_form": "그동네 깜빡 죽었다던데",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100032494.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100032494.1.23",
          "form": "여하간 종교인들이 가만리 있질 못하는듯",
          "original_form": "여하간 종교인들이 가만리 있질 못하는듯",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100032494.1.24",
          "form": "집에서 기도하라고",
          "original_form": "집에서 기도하라고",
          "speaker_id": "2",
          "time": "20210905 19:39"
