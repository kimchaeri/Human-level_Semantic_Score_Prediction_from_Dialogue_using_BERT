        {
          "id": "MDRW2100010495.1.1",
          "form": "82년생김지영 읽어봤어요?",
          "original_form": "82년생김지영 읽어봤어요?",
          "speaker_id": "1",
          "time": "20210823 18:51"
        },
        {
          "id": "MDRW2100010495.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210823 18:51"
        },
        {
          "id": "MDRW2100010495.1.3",
          "form": "해외에서 베스트셀러래",
          "original_form": "해외에서 베스트셀러래",
          "speaker_id": "2",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010495.1.4",
          "form": "전 안읽어봤는데",
          "original_form": "전 안읽어봤는데",
          "speaker_id": "1",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010495.1.5",
          "form": "영화로만 봤어요",
          "original_form": "영화로만 봤어요",
          "speaker_id": "1",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100010495.1.6",
          "form": "책으로도 읽어봐",
          "original_form": "책으로도 읽어봐",
          "speaker_id": "2",
          "time": "20210823 18:53"
        },
        {
          "id": "MDRW2100010495.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 18:54"
        },
        {
          "id": "MDRW2100010495.1.8",
          "form": "사피엔스는 진짜 내용이 좋대",
          "original_form": "사피엔스는 진짜 내용이 좋대",
          "speaker_id": "2",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010495.1.9",
          "form": "아 들어봤어요",
          "original_form": "아 들어봤어요",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010495.1.10",
          "form": "극찬을하던데",
          "original_form": "극찬을하던데",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010495.1.11",
          "form": "괜히 스테디셀러가 아닌가봐",
          "original_form": "괜히 스테디셀러가 아닌가봐",
          "speaker_id": "2",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010495.1.12",
          "form": "저 이북으로 사놨긴했는데",
          "original_form": "저 이북으로 사놨긴했는데",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010495.1.13",
          "form": "아직 안 읽었어요ㅎ",
          "original_form": "아직 안 읽었어요ㅎ",
          "speaker_id": "1",
          "time": "20210823 18:57"
        },
        {
          "id": "MDRW2100010495.1.14",
          "form": "나 읽어도돼?",
          "original_form": "나 읽어도돼?",
          "speaker_id": "2",
          "time": "20210823 18:57"
        },
        {
          "id": "MDRW2100010495.1.15",
