      "utterance": [
        {
          "id": "MDRW2100015326.1.1",
          "form": "아 그럴 수 있지",
          "original_form": "아 그럴 수 있지",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015326.1.2",
          "form": "그 막 돼지바 마카롱 있었잖아",
          "original_form": "그 막 돼지바 마카롱 있었잖아",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015326.1.3",
          "form": "아 그거",
          "original_form": "아 그거",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015326.1.4",
          "form": "그거 맛있었지?",
          "original_form": "그거 맛있었지?",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015326.1.5",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015326.1.6",
          "form": "근데 어느 날부터 그거 없어졌더라고",
          "original_form": "근데 어느 날부터 그거 없어졌더라고",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015326.1.7",
          "form": "나 몰랐어",
          "original_form": "나 몰랐어",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015326.1.8",
          "form": "관심이 없구만",
          "original_form": "관심이 없구만",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015326.1.9",
          "form": "응 ㅋㅋ",
          "original_form": "응 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015326.1.10",
          "form": "무화과 마카롱도 맛있는데  잘 안 팔더라",
          "original_form": "무화과 마카롱도 맛있는데  잘 안 팔더라",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015326.1.11",
          "form": "그건 못 들어봤어",
          "original_form": "그건 못 들어봤어",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015326.1.12",
          "form": "마카롱 안에 무화과를 넣은 거야",
          "original_form": "마카롱 안에 무화과를 넣은 거야",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015326.1.13",
          "form": "그건 아는데 잘 없던데",
          "original_form": "그건 아는데 잘 없던데",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015326.1.14",
          "form": "그니까 무화과가 엄청 맛있는데 사람들이 생각보다 잘 몰라",
          "original_form": "그니까 무화과가 엄청 맛있는데 사람들이 생각보다 잘 몰라",
          "speaker_id": "1",
          "time": "20210825 22:45"
