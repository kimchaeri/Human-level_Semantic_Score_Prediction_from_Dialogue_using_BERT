      "utterance": [
        {
          "id": "MDRW2100013868.1.1",
          "form": "안녕하세요. 현재 하시는 운동 있나요?",
          "original_form": "안녕하세요. 현재 하시는 운동 있나요?",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013868.1.2",
          "form": "저는 만보걷기 이런거 밖에 안해요 ㅋㅋㅋ",
          "original_form": "저는 만보걷기 이런거 밖에 안해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013868.1.3",
          "form": "대단한데요. 만보 매일 채우시나요",
          "original_form": "대단한데요. 만보 매일 채우시나요",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013868.1.4",
          "form": "매일은 아닌데 보통 걸으면 만보 채우려고 해요 ㅋㅋㅋ",
          "original_form": "매일은 아닌데 보통 걸으면 만보 채우려고 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013868.1.5",
          "form": "근데 은근 걷기가 어렵더라구요 ㅠㅠ",
          "original_form": "근데 은근 걷기가 어렵더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013868.1.6",
          "form": "캐시워크 이런것도 하시나요??",
          "original_form": "캐시워크 이런것도 하시나요??",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013868.1.7",
          "form": "오래걸으면 돈 준다던데요",
          "original_form": "오래걸으면 돈 준다던데요",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013868.1.8",
          "form": "아녀 그런거 하면 폰 느려지는 느낌이라",
          "original_form": "아녀 그런거 하면 폰 느려지는 느낌이라",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013868.1.9",
          "form": "그리고 광고 너무 많은게 싫어서",
          "original_form": "그리고 광고 너무 많은게 싫어서",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013868.1.10",
          "form": "안해욥",
          "original_form": "안해욥",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013868.1.11",
          "form": "그렇군요. 패션에 관심있으신가요",
          "original_form": "그렇군요. 패션에 관심있으신가요",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100013868.1.12",
          "form": "아녀 패션은 별루....",
          "original_form": "아녀 패션은 별루....",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100013868.1.13",
          "form": "영양제 뭐 챙겨드세요?",
          "original_form": "영양제 뭐 챙겨드세요?",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100013868.1.14",
          "form": "전 비타민이여",
          "original_form": "전 비타민이여",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013868.1.15",
          "form": "저도요 비타민 c 챙겨먹어요",
          "original_form": "저도요 비타민 c 챙겨먹어요",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100013868.1.16",
          "form": "아직 막 영양제 챙겨먹어야겠단 마늠이 안들어서",
          "original_form": "아직 막 영양제 챙겨먹어야겠단 마늠이 안들어서",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013868.1.17",
          "form": "그냥 비타민만 먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 비타민만 먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:26"
        },
        {
          "id": "MDRW2100013868.1.18",
          "form": "젊으실것 같아요 ㅋㅋ",
          "original_form": "젊으실것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013868.1.19",
