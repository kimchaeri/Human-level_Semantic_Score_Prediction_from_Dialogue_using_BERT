      "utterance": [
        {
          "id": "MDRW2100031621.1.1",
          "form": "요새 어떤 운동을 하고 계시나요?",
          "original_form": "요새 어떤 운동을 하고 계시나요?",
          "speaker_id": "2",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031621.1.2",
          "form": "원래 헬스장 다녔는데",
          "original_form": "원래 헬스장 다녔는데",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031621.1.3",
          "form": "코로나가 너무 심해지고 확진자도 나와서",
          "original_form": "코로나가 너무 심해지고 확진자도 나와서",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031621.1.4",
          "form": "요즘은 홈트랑 만보걷고 있어요 ㅎㅎ",
          "original_form": "요즘은 홈트랑 만보걷고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031621.1.5",
          "form": "헬스장 다니시는군요!",
          "original_form": "헬스장 다니시는군요!",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031621.1.6",
          "form": "와 만보를 매일요?",
          "original_form": "와 만보를 매일요?",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031621.1.7",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031621.1.8",
          "form": "만보 토스 만보기 아세요?",
          "original_form": "만보 토스 만보기 아세요?",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031621.1.9",
          "form": "그거 친구들이랑 하고 있어요 ㅋㅋㅋ",
          "original_form": "그거 친구들이랑 하고 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031621.1.10",
          "form": "네네 들어봤어요!",
          "original_form": "네네 들어봤어요!",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031621.1.11",
          "form": "재택할때 일평균 300보도 안걸어서",
          "original_form": "재택할때 일평균 300보도 안걸어서",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031621.1.12",
          "form": "신생아라는 소리 들었었는데",
          "original_form": "신생아라는 소리 들었었는데",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031621.1.13",
          "form": "대단해요!",
          "original_form": "대단해요!",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031621.1.14",
          "form": "앜ㅋㅋㅌ 신생아",
          "original_form": "앜ㅋㅋㅌ 신생아",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031621.1.15",
          "form": "저도 친구들이랑 해서 만보 걸을 수 있는거 같아요!",
          "original_form": "저도 친구들이랑 해서 만보 걸을 수 있는거 같아요!",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031621.1.16",
          "form": "name2님도 충분히 가능해요!!",
          "original_form": "&name2&님도 충분히 가능해요!!",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031621.1.17",
          "form": "진짜로 너무 안걷는다고 하지만... 일이 책상에서끝나는걸요 ㅠㅠ",
          "original_form": "진짜로 너무 안걷는다고 하지만... 일이 책상에서끝나는걸요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031621.1.18",
          "form": "만보... 그거 가능한 걸음 수.. 맞죠?",
          "original_form": "만보... 그거 가능한 걸음 수.. 맞죠?",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031621.1.19",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031621.1.20",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031621.1.21",
          "form": "할 수 있어요",
          "original_form": "할 수 있어요",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031621.1.22",
          "form": "재택근무하면 진짜 안나가게되요 ㅋㅋㅋ",
          "original_form": "재택근무하면 진짜 안나가게되요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031621.1.23",
          "form": "그쵸 ㅋㅋㅋ 그냥 책상에서조차 잘 일어나지 않게되는것 같아요",
          "original_form": "그쵸 ㅋㅋㅋ 그냥 책상에서조차 잘 일어나지 않게되는것 같아요",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031621.1.24",
          "form": "원래부터 운동을 열심히 하시던 분인가봐요!",
          "original_form": "원래부터 운동을 열심히 하시던 분인가봐요!",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031621.1.25",
          "form": "어 아니예요 ㅠㅠ",
          "original_form": "어 아니예요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031621.1.26",
          "form": "저 의사가 운동해야한다고 해서 ㅋ큐ㅠㅠ",
          "original_form": "저 의사가 운동해야한다고 해서 ㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031621.1.27",
          "form": "올해부터 열심히 하고있어요ㅠㅠ!!",
          "original_form": "올해부터 열심히 하고있어요ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031621.1.28",
          "form": "앗... 그.. 그런가요? ㅠㅜ",
          "original_form": "앗... 그.. 그런가요? ㅠㅜ",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031621.1.29",
          "form": "name2님도 무조건 하실수 있어요",
          "original_form": "&name2&님도 무조건 하실수 있어요",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031621.1.30",
          "form": "그치만 실천하고 계신게 정말 대단해요!!",
          "original_form": "그치만 실천하고 계신게 정말 대단해요!!",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031621.1.31",
          "form": "네,, 근육이 너무 없다고 하더라구요 ㅋㅋ큐ㅠ",
          "original_form": "네,, 근육이 너무 없다고 하더라구요 ㅋㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031621.1.32",
          "form": "근육량 매번 비엠아이 찍으면 부족하다고 나와요 ㅋㅋ",
