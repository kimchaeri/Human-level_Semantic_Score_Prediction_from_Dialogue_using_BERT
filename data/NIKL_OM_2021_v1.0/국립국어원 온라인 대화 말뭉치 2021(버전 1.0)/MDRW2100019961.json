      "utterance": [
        {
          "id": "MDRW2100019961.1.1",
          "form": "진로 두꺼비 드셔보셨나요",
          "original_form": "진로 두꺼비 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019961.1.2",
          "form": "그.. 클래식한 병이죠?  파랑색 두꺼비 캐릭터 광고하는?",
          "original_form": "그.. 클래식한 병이죠?  파랑색 두꺼비 캐릭터 광고하는?",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019961.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019961.1.4",
          "form": "엄청 순하던데요...",
          "original_form": "엄청 순하던데요...",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019961.1.5",
          "form": "취향에 맞으시던가여?",
          "original_form": "취향에 맞으시던가여?",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019961.1.6",
          "form": "앗 전 술을 못해서",
          "original_form": "앗 전 술을 못해서",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019961.1.7",
          "form": "헉... 근데 어케 아시는거에요?",
          "original_form": "헉... 근데 어케 아시는거에요?",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019961.1.8",
          "form": "저희 아부지가 드시더라구여ㅎ",
          "original_form": "저희 아부지가 드시더라구여ㅎ",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019961.1.9",
          "form": "아... ㅋㅋㅋ 대박 났을거에요..",
          "original_form": "아... ㅋㅋㅋ 대박 났을거에요..",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019961.1.10",
          "form": "가격도 제일 저렴하고..",
          "original_form": "가격도 제일 저렴하고..",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019961.1.11",
          "form": "전 병이 너무 이뻐서 일부러 고르게.되더란..",
          "original_form": "전 병이 너무 이뻐서 일부러 고르게.되더란..",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019961.1.12",
          "form": "저도 디자인에 끌리더라구요",
          "original_form": "저도 디자인에 끌리더라구요",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019961.1.13",
          "form": "밀가루 맥주도 엄청 장팔려요",
          "original_form": "밀가루 맥주도 엄청 장팔려요",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100019961.1.14",
          "form": "희귀템 수준..",
          "original_form": "희귀템 수준..",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100019961.1.15",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019961.1.16",
          "form": "마케팅이 참 중요한듯요",
          "original_form": "마케팅이 참 중요한듯요",
          "speaker_id": "1",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019961.1.17",
          "form": "네.. 그게 아마 1빠?로 성공해서",
          "original_form": "네.. 그게 아마 1빠?로 성공해서",
          "speaker_id": "2",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019961.1.18",
          "form": "그뒤로 우후죽순 콜라조ㅠ제품들이..",
          "original_form": "그뒤로 우후죽순 콜라조ㅠ제품들이..",
          "speaker_id": "2",
          "time": "20210827 23:56"
        },
        {
          "id": "MDRW2100019961.1.19",
          "form": "아 그런거구나",
          "original_form": "아 그런거구나",
          "speaker_id": "1",
