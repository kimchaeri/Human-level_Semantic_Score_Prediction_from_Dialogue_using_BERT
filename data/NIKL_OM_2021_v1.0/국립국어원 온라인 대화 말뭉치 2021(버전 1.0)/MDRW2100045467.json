      "utterance": [
        {
          "id": "MDRW2100045467.1.1",
          "form": "촉촉좋아",
          "original_form": "촉촉좋아",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045467.1.2",
          "form": "나는 몇시까지 가야됨?",
          "original_form": "나는 몇시까지 가야됨?",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045467.1.3",
          "form": "거기 리뷰보니까 엄청오래걸리는것같던데",
          "original_form": "거기 리뷰보니까 엄청오래걸리는것같던데",
          "speaker_id": "2",
          "time": "20210908 02:42"
        },
        {
          "id": "MDRW2100045467.1.4",
          "form": "맞어 오래걸려",
          "original_form": "맞어 오래걸려",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045467.1.5",
          "form": "보통 이십분..?",
          "original_form": "보통 이십분..?",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045467.1.6",
          "form": "나혼자주문했을경우",
          "original_form": "나혼자주문했을경우",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045467.1.7",
          "form": "이십분넘게걸렸어 ㅋㅋㅋㅋ",
          "original_form": "이십분넘게걸렸어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045467.1.8",
          "form": "헉 그럼 좀 일찍가야겠네",
          "original_form": "헉 그럼 좀 일찍가야겠네",
          "speaker_id": "2",
          "time": "20210908 02:44"
        },
        {
          "id": "MDRW2100045467.1.9",
          "form": "한십분십오분먼저가 ㅋㅋ너무 일찍가도좀그래",
          "original_form": "한십분십오분먼저가 ㅋㅋ너무 일찍가도좀그래",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045467.1.10",
          "form": "빨리내일오면좋겠당 배가고픔",
          "original_form": "빨리내일오면좋겠당 배가고픔",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045467.1.11",
          "form": "난 야식먹어서 더부룩해",
          "original_form": "난 야식먹어서 더부룩해",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045467.1.12",
          "form": "빠새한봉지다먹었어",
          "original_form": "빠새한봉지다먹었어",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045467.1.13",
          "form": "짜요짜요도 먹음 ....",
          "original_form": "짜요짜요도 먹음 ....",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045467.1.14",
          "form": "빠새가뭐야?",
          "original_form": "빠새가뭐야?",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045467.1.15",
          "form": "짜요짜요는뭐",
          "original_form": "짜요짜요는뭐",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045467.1.16",
          "form": "배가차는게아니지",
          "original_form": "배가차는게아니지",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045467.1.17",
          "form": "과자 ㅋㅋㅋ빠새라는과자",
          "original_form": "과자 ㅋㅋㅋ빠새라는과자",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045467.1.18",
          "form": "아너무많이먹었어",
          "original_form": "아너무많이먹었어",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045467.1.19",
          "form": "사실 파운드케이크도 하나 남아있길래먹음...",
          "original_form": "사실 파운드케이크도 하나 남아있길래먹음...",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045467.1.20",
          "form": "코코넛은 절래절래...",
          "original_form": "코코넛은 절래절래...",
          "speaker_id": "1",
          "time": "20210908 02:48"
        },
        {
          "id": "MDRW2100045467.1.21",
          "form": "파운드케익먹고싶다",
          "original_form": "파운드케익먹고싶다",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045467.1.22",
          "form": "먹어라 아직 있지않나",
          "original_form": "먹어라 아직 있지않나",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045467.1.23",
          "form": "다른방에있어서슬프다",
          "original_form": "다른방에있어서슬프다",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045467.1.24",
          "form": "뭐가제일낫던데",
          "original_form": "뭐가제일낫던데",
          "speaker_id": "1",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045467.1.25",
          "form": "내방에없어서ㅜㅜ",
          "original_form": "내방에없어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:49"
        },
        {
          "id": "MDRW2100045467.1.26",
          "form": "나는 아몬드!",
          "original_form": "나는 아몬드!",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045467.1.27",
          "form": "기본맛이져",
          "original_form": "기본맛이져",
          "speaker_id": "2",
          "time": "20210908 02:50"
        },
        {
          "id": "MDRW2100045467.1.28",
          "form": "나는 그거4위",
          "original_form": "나는 그거4위",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045467.1.29",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045467.1.30",
          "form": "블베가짱",
          "original_form": "블베가짱",
          "speaker_id": "1",
          "time": "20210908 02:51"
        },
        {
          "id": "MDRW2100045467.1.31",
          "form": "블베맛잇긴하더라ㅋㅋ2위는요?",
          "original_form": "블베맛잇긴하더라ㅋㅋ2위는요?",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045467.1.32",
          "form": "멜론!",
          "original_form": "멜론!",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045467.1.33",
          "form": "박빙이었어",
          "original_form": "박빙이었어",
          "speaker_id": "1",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100045467.1.34",
          "form": "명예의 2위는 멜론",
          "original_form": "명예의 2위는 멜론",
          "speaker_id": "1",
          "time": "20210908 02:54"
        }
      ]
    }
