      "utterance": [
        {
          "id": "MDRW2100022163.1.1",
          "form": "전 요즘 몸이 말랑해지는 걸 느꼈어요",
          "original_form": "전 요즘 몸이 말랑해지는 걸 느꼈어요",
          "speaker_id": "2",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100022163.1.2",
          "form": "운동 안한지 3개월이 되니",
          "original_form": "운동 안한지 3개월이 되니",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100022163.1.3",
          "form": "몸이 말랑하네요",
          "original_form": "몸이 말랑하네요",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100022163.1.4",
          "form": "인간마시멜로우가 따로 없어요",
          "original_form": "인간마시멜로우가 따로 없어요",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100022163.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:43"
        },
        {
          "id": "MDRW2100022163.1.6",
          "form": "필라테스는어디갓어용",
          "original_form": "필라테스는어디갓어용",
          "speaker_id": "1",
          "time": "20210901 16:43"
        },
        {
          "id": "MDRW2100022163.1.7",
          "form": "다시해야하는데",
          "original_form": "다시해야하는데",
          "speaker_id": "2",
          "time": "20210901 16:45"
        },
        {
          "id": "MDRW2100022163.1.8",
          "form": "어디서할지 찾아보는 중이에요",
          "original_form": "어디서할지 찾아보는 중이에요",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100022163.1.9",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100022163.1.10",
          "form": "그전까진",
          "original_form": "그전까진",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100022163.1.11",
          "form": "홈트를합시드아아아아ㅏㅅ",
          "original_form": "홈트를합시드아아아아ㅏㅅ",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100022163.1.12",
          "form": "앗 방금 수업 예약했습니다~~",
          "original_form": "앗 방금 수업 예약했습니다~~",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100022163.1.13",
          "form": "운동은 역시 필라테스가 제일 좋은거 같아요",
          "original_form": "운동은 역시 필라테스가 제일 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100022163.1.14",
          "form": "다른 수업은 너무 지루해서 할수없어요",
          "original_form": "다른 수업은 너무 지루해서 할수없어요",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100022163.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100022163.1.16",
          "form": "플라잉요가는어떤가욤??",
          "original_form": "플라잉요가는어떤가욤??",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100022163.1.17",
          "form": "아앗...",
          "original_form": "아앗...",
          "speaker_id": "2",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100022163.1.18",
          "form": "그건 아직까진..",
          "original_form": "그건 아직까진..",
          "speaker_id": "2",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100022163.1.19",
          "form": "그리고 청주에는 플라잉요가 하는곳이 별로 없어요",
          "original_form": "그리고 청주에는 플라잉요가 하는곳이 별로 없어요",
          "speaker_id": "2",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100022163.1.20",
          "form": "제 자취방에서 너무 멀어요",
          "original_form": "제 자취방에서 너무 멀어요",
          "speaker_id": "2",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100022163.1.21",
          "form": "아앗...",
          "original_form": "아앗...",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100022163.1.22",
          "form": "안타깝군요...ㅠㅠ",
          "original_form": "안타깝군요...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100022163.1.23",
          "form": "마자요~",
          "original_form": "마자요~",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100022163.1.24",
          "form": "나중에 기회가 된다면 해보고 싶어요",
          "original_form": "나중에 기회가 된다면 해보고 싶어요",
          "speaker_id": "2",
          "time": "20210901 17:04"
