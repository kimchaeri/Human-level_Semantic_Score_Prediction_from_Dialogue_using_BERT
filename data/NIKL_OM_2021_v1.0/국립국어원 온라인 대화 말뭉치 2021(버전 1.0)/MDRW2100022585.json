      "utterance": [
        {
          "id": "MDRW2100022585.1.1",
          "form": "좋아하는편이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "좋아하는편이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:01"
        },
        {
          "id": "MDRW2100022585.1.2",
          "form": "근데 은근 귀찮아서 자주안해욬ㅋㅋㅋ",
          "original_form": "근데 은근 귀찮아서 자주안해욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022585.1.3",
          "form": "아 ㅋㅋ 그렇구나 혹시 알고 싶은 요리 있어요?",
          "original_form": "아 ㅋㅋ 그렇구나 혹시 알고 싶은 요리 있어요?",
          "speaker_id": "1",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022585.1.4",
          "form": "제가 레시피 공유해드릴게요",
          "original_form": "제가 레시피 공유해드릴게요",
          "speaker_id": "1",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022585.1.5",
          "form": "오 요리잘하세요?!",
          "original_form": "오 요리잘하세요?!",
          "speaker_id": "2",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022585.1.6",
          "form": "살기 위해서 할 줄 알아요",
          "original_form": "살기 위해서 할 줄 알아요",
          "speaker_id": "1",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022585.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ아 자취하신뎃죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 자취하신뎃죠",
          "speaker_id": "2",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022585.1.8",
          "form": "자주해먹는 요리가 뭐에요?",
          "original_form": "자주해먹는 요리가 뭐에요?",
          "speaker_id": "2",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022585.1.9",
          "form": "네네 간단히 먹고 싶을 때는 파스타 먹어요!",
          "original_form": "네네 간단히 먹고 싶을 때는 파스타 먹어요!",
          "speaker_id": "1",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022585.1.10",
          "form": "무슨 파스타 좋아해요?",
          "original_form": "무슨 파스타 좋아해요?",
          "speaker_id": "1",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022585.1.11",
          "form": "크 저도 파스타 자주해요 ㅋㅋㅋㅋ",
          "original_form": "크 저도 파스타 자주해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022585.1.12",
          "form": "오~파스타 잘해요?",
          "original_form": "오~파스타 잘해요?",
          "speaker_id": "1",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022585.1.13",
          "form": "저는 크림 좋아해요! 알리오올리오나요",
          "original_form": "저는 크림 좋아해요! 알리오올리오나요",
          "speaker_id": "2",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022585.1.14",
          "form": "잘하는건진모르겟어요ㅋㅋㅋㅋ",
          "original_form": "잘하는건진모르겟어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022585.1.15",
          "form": "ㅎㅎㅎ 크림파스타에 브로콜리랑 베이컨 넣으면 진짜 맛있는데 ㅜㅜ",
          "original_form": "ㅎㅎㅎ 크림파스타에 브로콜리랑 베이컨 넣으면 진짜 맛있는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022585.1.16",
          "form": "넣어봤어요?",
          "original_form": "넣어봤어요?",
          "speaker_id": "1",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022585.1.17",
          "form": "거의 제품에 의지합니당 ㅋㅋㅋㅋㅋ",
          "original_form": "거의 제품에 의지합니당 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022585.1.18",
          "form": "저도 소스는 시판소스 애용 ㅎㅎ",
          "original_form": "저도 소스는 시판소스 애용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022585.1.19",
          "form": "크 당연하죠 베이컨 버섯 필수!",
          "original_form": "크 당연하죠 베이컨 버섯 필수!",
          "speaker_id": "2",
          "time": "20210829 16:03"
        },
        {
          "id": "MDRW2100022585.1.20",
          "form": "ㅋㅋ 보통 베이컨 버섯은 마지막에 넣어요 맨 처음에 넣어요?",
          "original_form": "ㅋㅋ 보통 베이컨 버섯은 마지막에 넣어요 맨 처음에 넣어요?",
          "speaker_id": "1",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100022585.1.21",
          "form": "크림은 은근 소스만들기쉽다하더라구요",
          "original_form": "크림은 은근 소스만들기쉽다하더라구요",
          "speaker_id": "2",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100022585.1.22",
          "form": "저는 처음에 양파같은재료랑 같이 볶아요!",
          "original_form": "저는 처음에 양파같은재료랑 같이 볶아요!",
          "speaker_id": "2",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100022585.1.23",
          "form": "오 ㅋㅋㅋ 잘하시네요",
          "original_form": "오 ㅋㅋㅋ 잘하시네요",
          "speaker_id": "1",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100022585.1.24",
          "form": "저도 그 베이컨 기름에 양파 볶아서",
          "original_form": "저도 그 베이컨 기름에 양파 볶아서",
          "speaker_id": "1",
          "time": "20210829 16:04"
        },
        {
          "id": "MDRW2100022585.1.25",
          "form": "소스랑 섞어서 만들어요!",
          "original_form": "소스랑 섞어서 만들어요!",
          "speaker_id": "1",
          "time": "20210829 16:04"
        }
      ]
    }
  ]
