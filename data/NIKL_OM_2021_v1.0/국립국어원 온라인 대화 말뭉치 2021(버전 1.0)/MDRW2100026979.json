      "utterance": [
        {
          "id": "MDRW2100026979.1.1",
          "form": "오 1차 맞으러가세요?",
          "original_form": "오 1차 맞으러가세요?",
          "speaker_id": "2",
          "time": "20210902 02:26"
        },
        {
          "id": "MDRW2100026979.1.2",
          "form": "네네 화이자맞아요",
          "original_form": "네네 화이자맞아요",
          "speaker_id": "1",
          "time": "20210902 02:28"
        },
        {
          "id": "MDRW2100026979.1.3",
          "form": "님은 언제 예약하셨어요?",
          "original_form": "님은 언제 예약하셨어요?",
          "speaker_id": "1",
          "time": "20210902 02:28"
        },
        {
          "id": "MDRW2100026979.1.4",
          "form": "저는 아직요ㅜ",
          "original_form": "저는 아직요ㅜ",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100026979.1.5",
          "form": "맞기전에 꼭 타이레놀 챙겨놓으세요!",
          "original_form": "맞기전에 꼭 타이레놀 챙겨놓으세요!",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100026979.1.6",
          "form": "네네 가족중 제가 마지막이라 집에 진통제는 두둑해요!",
          "original_form": "네네 가족중 제가 마지막이라 집에 진통제는 두둑해요!",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100026979.1.7",
          "form": "지금 맞았는데 엄청 순식간이네요",
          "original_form": "지금 맞았는데 엄청 순식간이네요",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100026979.1.8",
          "form": "오 지금은 몸 어떠세요?",
          "original_form": "오 지금은 몸 어떠세요?",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100026979.1.9",
          "form": "맞고 집에 와서 돈가스 해치우고 한숨 잤죠",
          "original_form": "맞고 집에 와서 돈가스 해치우고 한숨 잤죠",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100026979.1.10",
          "form": "개운해요ㅎㅎ",
          "original_form": "개운해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100026979.1.11",
          "form": "다행이네요!! 백신부작용이 이것저것 있더라고요",
          "original_form": "다행이네요!! 백신부작용이 이것저것 있더라고요",
          "speaker_id": "2",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100026979.1.12",
          "form": "백신 맞고는 잘먹고 잘자야되는거 같아요",
          "original_form": "백신 맞고는 잘먹고 잘자야되는거 같아요",
          "speaker_id": "2",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100026979.1.13",
          "form": "ㅎㅎ 맞아요! 그래서 오늘내일은 배터지게 먹으려구요ㅎㅎ",
          "original_form": "ㅎㅎ 맞아요! 그래서 오늘내일은 배터지게 먹으려구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100026979.1.14",
          "form": "한 2주간은 먹고 싶은거 다먹어도 괜찮아요ㅋㅋㅋ 다 몸을 위한거니까요!!",
          "original_form": "한 2주간은 먹고 싶은거 다먹어도 괜찮아요ㅋㅋㅋ 다 몸을 위한거니까요!!",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100026979.1.15",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100026979.1.16",
          "form": "저는 배고파서가 아니라 건강을 위해 먹는거임ㅎ",
          "original_form": "저는 배고파서가 아니라 건강을 위해 먹는거임ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100026979.1.17",
          "form": "이게 다 백신맞느라 수고한 건강을 위해 먹는거니까요ㅋㅋ",
          "original_form": "이게 다 백신맞느라 수고한 건강을 위해 먹는거니까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100026979.1.18",
          "form": "아 그리고 오히려 백신 맞고 일정시간은 운동하면 안된다던데 맞나요?",
          "original_form": "아 그리고 오히려 백신 맞고 일정시간은 운동하면 안된다던데 맞나요?",
          "speaker_id": "2",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100026979.1.19",
          "form": "네 하지 말래요!",
          "original_form": "네 하지 말래요!",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100026979.1.20",
          "form": "몸 격하게 움직이지도 말고 음주도 말고",
          "original_form": "몸 격하게 움직이지도 말고 음주도 말고",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100026979.1.21",
          "form": "전자는 되는데 후자가 문제네요ㅜ",
          "original_form": "전자는 되는데 후자가 문제네요ㅜ",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100026979.1.22",
          "form": "아이고 금주에요??",
          "original_form": "아이고 금주에요??",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100026979.1.23",
          "form": "내일까진 몸 사리려구요",
          "original_form": "내일까진 몸 사리려구요",
          "speaker_id": "1",
          "time": "20210902 22:10"
        }
      ]
    }
  ]
