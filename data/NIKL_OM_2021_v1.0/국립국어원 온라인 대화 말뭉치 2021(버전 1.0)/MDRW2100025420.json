      "utterance": [
        {
          "id": "MDRW2100025420.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025420.1.2",
          "form": "아니요. 키우세요?",
          "original_form": "아니요. 키우세요?",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025420.1.3",
          "form": "네 저는 고양이 두마리 키워요",
          "original_form": "네 저는 고양이 두마리 키워요",
          "speaker_id": "1",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025420.1.4",
          "form": "몇 살이에요?",
          "original_form": "몇 살이에요?",
          "speaker_id": "2",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025420.1.5",
          "form": "이제 4살 생일 지났어요",
          "original_form": "이제 4살 생일 지났어요",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025420.1.6",
          "form": "사람 나이로 32살이래요",
          "original_form": "사람 나이로 32살이래요",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025420.1.7",
          "form": "ㅋㅋㅋ그래두 귀엽잖아요",
          "original_form": "ㅋㅋㅋ그래두 귀엽잖아요",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025420.1.8",
          "form": "맞아요 고양이는 영원히 3살이래요 ㅋㅋㅋ",
          "original_form": "맞아요 고양이는 영원히 3살이래요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025420.1.9",
          "form": "둘다 수컷이고 형제인데 전혀 다르게 생겼어요",
          "original_form": "둘다 수컷이고 형제인데 전혀 다르게 생겼어요",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025420.1.10",
          "form": "털색이 다른가요?",
          "original_form": "털색이 다른가요?",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025420.1.11",
          "form": "네 하나는 엄마를 닮아서 흰색털인데 하나는 고등어에요",
          "original_form": "네 하나는 엄마를 닮아서 흰색털인데 하나는 고등어에요",
          "speaker_id": "1",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025420.1.12",
          "form": "부모가 길냥이들이라 아빠냥이가 고등어였나봐요",
          "original_form": "부모가 길냥이들이라 아빠냥이가 고등어였나봐요",
          "speaker_id": "1",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025420.1.13",
          "form": "너무 귀엽겠네요",
          "original_form": "너무 귀엽겠네요",
          "speaker_id": "2",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025420.1.14",
          "form": "둘이 잘 안싸우고 지내나요?",
          "original_form": "둘이 잘 안싸우고 지내나요?",
          "speaker_id": "2",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025420.1.15",
          "form": "네 고양이라 싸우는지 노는건지 가끔 헷갈릴때도 있지만",
          "original_form": "네 고양이라 싸우는지 노는건지 가끔 헷갈릴때도 있지만",
          "speaker_id": "1",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025420.1.16",
          "form": "아ㅋㅋ 전 친구들 고양이 볼 때마다 노는 줄 모르고 놀라게 되요",
          "original_form": "아ㅋㅋ 전 친구들 고양이 볼 때마다 노는 줄 모르고 놀라게 되요",
          "speaker_id": "2",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025420.1.17",
          "form": "그게 알고보면 노는거라고 유튜브에서 그러더라구요 ㅋㅋ",
          "original_form": "그게 알고보면 노는거라고 유튜브에서 그러더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025420.1.18",
          "form": "고양이는 재밌는",
          "original_form": "고양이는 재밌는",
          "speaker_id": "1",
          "time": "20210901 11:18"
        }
      ]
    }
