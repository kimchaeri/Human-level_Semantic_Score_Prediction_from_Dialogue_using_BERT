      "utterance": [
        {
          "id": "MDRW2100018500.1.1",
          "form": "신제품 말씀하시는거죠?",
          "original_form": "신제품 말씀하시는거죠?",
          "speaker_id": "2",
          "time": "20210827 06:43"
        },
        {
          "id": "MDRW2100018500.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.3",
          "form": "창녕 마늘이 들어갔대요",
          "original_form": "창녕 마늘이 들어갔대요",
          "speaker_id": "1",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.4",
          "form": "아직이예요 드셔보셨어요?",
          "original_form": "아직이예요 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.5",
          "form": "이게 후기를 보니까",
          "original_form": "이게 후기를 보니까",
          "speaker_id": "1",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.6",
          "form": "호불호가 극과극이더라구요ㅋㅋㅋ",
          "original_form": "호불호가 극과극이더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.7",
          "form": "아직 안먹어봤어용",
          "original_form": "아직 안먹어봤어용",
          "speaker_id": "1",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.8",
          "form": "저 마늘 좋아해서 먹어보면 제입맛이랑 맞을꺼라 생각하는데",
          "original_form": "저 마늘 좋아해서 먹어보면 제입맛이랑 맞을꺼라 생각하는데",
          "speaker_id": "2",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.9",
          "form": "갑자기 두렵네요",
          "original_form": "갑자기 두렵네요",
          "speaker_id": "2",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.10",
          "form": "마늘향이 엄청 강하대요",
          "original_form": "마늘향이 엄청 강하대요",
          "speaker_id": "1",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.11",
          "form": "약간 소스에 버무린 생마늘 느낌이라고",
          "original_form": "약간 소스에 버무린 생마늘 느낌이라고",
          "speaker_id": "1",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.12",
          "form": "누가 그래서 ㅠㅋㅋ",
          "original_form": "누가 그래서 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.13",
          "form": "ㅋㅋㅋ앜ㅋㅋㅋ...무서운데요?",
          "original_form": "ㅋㅋㅋ앜ㅋㅋㅋ...무서운데요?",
          "speaker_id": "2",
          "time": "20210827 06:44"
        },
        {
          "id": "MDRW2100018500.1.14",
          "form": "어디 안나갈때 먹어야겠어요",
          "original_form": "어디 안나갈때 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210827 06:45"
        },
        {
          "id": "MDRW2100018500.1.15",
          "form": "속에서 마늘냄새 올라올꺼같아섴ㅋㅋㅋㅋ",
          "original_form": "속에서 마늘냄새 올라올꺼같아섴ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 06:45"
        },
        {
          "id": "MDRW2100018500.1.16",
          "form": "그쵸ㅋㅋㅋㅋ 토핑을 너무 푸짐하게 줄까봐 걱정이에요",
          "original_form": "그쵸ㅋㅋㅋㅋ 토핑을 너무 푸짐하게 줄까봐 걱정이에요",
          "speaker_id": "1",
          "time": "20210827 06:45"
        },
        {
          "id": "MDRW2100018500.1.17",
          "form": "양치 몇번해야할거에용ㅋㅋ",
          "original_form": "양치 몇번해야할거에용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:45"
        },
        {
          "id": "MDRW2100018500.1.18",
          "form": "공감이욬ㅋㅋㅋㅋㅋㅋ맥모닝은 좋아하세요?",
          "original_form": "공감이욬ㅋㅋㅋㅋㅋㅋ맥모닝은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 06:45"
        },
        {
          "id": "MDRW2100018500.1.19",
          "form": "넘 좋아해요",
          "original_form": "넘 좋아해요",
          "speaker_id": "1",
          "time": "20210827 06:45"
        },
        {
          "id": "MDRW2100018500.1.20",
          "form": "그게 왜이렇게 맛있는지!",
          "original_form": "그게 왜이렇게 맛있는지!",
          "speaker_id": "1",
          "time": "20210827 06:45"
        },
        {
          "id": "MDRW2100018500.1.21",
          "form": "저두요! 그거 넘 맛있어요",
          "original_form": "저두요! 그거 넘 맛있어요",
          "speaker_id": "2",
          "time": "20210827 06:46"
        },
        {
          "id": "MDRW2100018500.1.22",
          "form": "10:30분까지밖에 안파니까",
          "original_form": "10:30분까지밖에 안파니까",
          "speaker_id": "2",
          "time": "20210827 06:46"
        },
        {
          "id": "MDRW2100018500.1.23",
          "form": "맞아요ㅠㅠ 베이컨 들어간거 안들어간거 다 맛있어요",
          "original_form": "맞아요ㅠㅠ 베이컨 들어간거 안들어간거 다 맛있어요",
          "speaker_id": "1",
          "time": "20210827 06:46"
        },
        {
          "id": "MDRW2100018500.1.24",
          "form": "희소성때문에 더 맛있게 느껴지는건지!",
          "original_form": "희소성때문에 더 맛있게 느껴지는건지!",
          "speaker_id": "2",
          "time": "20210827 06:46"
        },
        {
          "id": "MDRW2100018500.1.25",
          "form": "제일 좋아하시는 음식이 뭔가용?",
          "original_form": "제일 좋아하시는 음식이 뭔가용?",
          "speaker_id": "1",
          "time": "20210827 06:47"
        }
      ]
    }
  ]
