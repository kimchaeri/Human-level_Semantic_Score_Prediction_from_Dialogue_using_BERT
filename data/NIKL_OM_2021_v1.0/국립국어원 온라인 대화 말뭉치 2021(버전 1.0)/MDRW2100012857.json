        {
          "id": "MDRW2100012857.1.1",
          "form": "요리좋아하시나요!",
          "original_form": "요리좋아하시나요!",
          "speaker_id": "1",
          "time": "20210825 03:07"
        },
        {
          "id": "MDRW2100012857.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210825 03:07"
        },
        {
          "id": "MDRW2100012857.1.3",
          "form": "요리 정말 못해요 ㅋㅋ 라면이 최대치입니다",
          "original_form": "요리 정말 못해요 ㅋㅋ 라면이 최대치입니다",
          "speaker_id": "2",
          "time": "20210825 03:19"
        },
        {
          "id": "MDRW2100012857.1.4",
          "form": "배달음식 시켜드시나요?",
          "original_form": "배달음식 시켜드시나요?",
          "speaker_id": "1",
          "time": "20210825 03:20"
        },
        {
          "id": "MDRW2100012857.1.5",
          "form": "가끔 시켜먹어요",
          "original_form": "가끔 시켜먹어요",
          "speaker_id": "2",
          "time": "20210825 03:23"
        },
        {
          "id": "MDRW2100012857.1.6",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210825 03:23"
        },
        {
          "id": "MDRW2100012857.1.7",
          "form": "매일해요",
          "original_form": "매일해요",
          "speaker_id": "1",
          "time": "20210825 03:23"
        },
        {
          "id": "MDRW2100012857.1.8",
          "form": "잘은 모르겠고요ㅋㅋ",
          "original_form": "잘은 모르겠고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 03:23"
        },
        {
          "id": "MDRW2100012857.1.9",
          "form": "보통 어떤 요리 많이 하시나요?",
          "original_form": "보통 어떤 요리 많이 하시나요?",
          "speaker_id": "2",
          "time": "20210825 03:27"
        },
        {
          "id": "MDRW2100012857.1.10",
          "form": "야채 볶아 먹어요",
          "original_form": "야채 볶아 먹어요",
          "speaker_id": "1",
          "time": "20210825 03:28"
        },
        {
          "id": "MDRW2100012857.1.11",
          "form": "고기는 안드세요?",
          "original_form": "고기는 안드세요?",
          "speaker_id": "2",
          "time": "20210825 03:35"
        },
        {
          "id": "MDRW2100012857.1.12",
          "form": "먹어요",
          "original_form": "먹어요",
          "speaker_id": "1",
          "time": "20210825 03:36"
        },
        {
          "id": "MDRW2100012857.1.13",
          "form": "도겸님은 소 돼지 닭 중에 어떤 고기가 좋으세요?",
          "original_form": "도겸님은 소 돼지 닭 중에 어떤 고기가 좋으세요?",
          "speaker_id": "1",
          "time": "20210825 03:36"
        },
        {
          "id": "MDRW2100012857.1.14",
          "form": "다 좋아해서 고르기 어렵네요",
          "original_form": "다 좋아해서 고르기 어렵네요",
          "speaker_id": "2",
          "time": "20210825 03:38"
        },
        {
          "id": "MDRW2100012857.1.15",
          "form": "저는 돼지 닭 좋아해요",
          "original_form": "저는 돼지 닭 좋아해요",
          "speaker_id": "1",
          "time": "20210825 03:39"
        },
        {
          "id": "MDRW2100012857.1.16",
          "form": "치킨도 좋아하세요?",
          "original_form": "치킨도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 03:46"
        },
        {
          "id": "MDRW2100012857.1.17",
          "form": "있으면 먹어요",
          "original_form": "있으면 먹어요",
          "speaker_id": "1",
          "time": "20210825 03:46"
        }
      ]
