      "utterance": [
        {
          "id": "MDRW2100046105.1.1",
          "form": "추석 되니 날이 흐리네",
          "original_form": "추석 되니 날이 흐리네",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046105.1.2",
          "form": "어제도 흐렸어",
          "original_form": "어제도 흐렸어",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046105.1.3",
          "form": "비 좀 그만 왔으면",
          "original_form": "비 좀 그만 왔으면",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046105.1.4",
          "form": "여름에도 안오더니",
          "original_form": "여름에도 안오더니",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046105.1.5",
          "form": "가을비가 많이오네",
          "original_form": "가을비가 많이오네",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046105.1.6",
          "form": "짜증나 진짜 사과 당도 떨어졌어",
          "original_form": "짜증나 진짜 사과 당도 떨어졌어",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046105.1.7",
          "form": "휴...",
          "original_form": "휴...",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046105.1.8",
          "form": "딱 추석 끝나고 여행가는데",
          "original_form": "딱 추석 끝나고 여행가는데",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046105.1.9",
          "form": "비 그만 왔으면",
          "original_form": "비 그만 왔으면",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046105.1.10",
          "form": "지금부터 햇빛 났으면 좋겠다",
          "original_form": "지금부터 햇빛 났으면 좋겠다",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046105.1.11",
          "form": "지금 장마니?",
          "original_form": "지금 장마니?",
          "speaker_id": "2",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046105.1.12",
          "form": "아닐텐데...",
          "original_form": "아닐텐데...",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046105.1.13",
          "form": "이게 그 이상 기후니...",
          "original_form": "이게 그 이상 기후니...",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046105.1.14",
          "form": "이상하게 오래 비가온다는거면 맞는말같다...",
          "original_form": "이상하게 오래 비가온다는거면 맞는말같다...",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046105.1.15",
          "form": "정말 쓰잘떼기 없는 비인디",
          "original_form": "정말 쓰잘떼기 없는 비인디",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046105.1.16",
          "form": "오늘도 우산 들고 다니느라 귀찮다",
          "original_form": "오늘도 우산 들고 다니느라 귀찮다",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046105.1.17",
          "form": "애매하게 오니까...",
          "original_form": "애매하게 오니까...",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046105.1.18",
          "form": "나는 그냥 모자쓰고다녀",
          "original_form": "나는 그냥 모자쓰고다녀",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046105.1.19",
          "form": "와아 영국 사람이다",
          "original_form": "와아 영국 사람이다",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046105.1.20",
          "form": "우산들고다니기 귀찮아",
          "original_form": "우산들고다니기 귀찮아",
          "speaker_id": "1",
          "time": "20210908 10:39"
        }
