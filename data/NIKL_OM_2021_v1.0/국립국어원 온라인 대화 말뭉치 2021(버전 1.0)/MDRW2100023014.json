      "utterance": [
        {
          "id": "MDRW2100023014.1.1",
          "form": "내일 월요일인데",
          "original_form": "내일 월요일인데",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023014.1.2",
          "form": "왜 나가기 싫죠?",
          "original_form": "왜 나가기 싫죠?",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023014.1.3",
          "form": "지금 사무실이라 오늘도 출근한듯요ㅎㅎ",
          "original_form": "지금 사무실이라 오늘도 출근한듯요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100023014.1.4",
          "form": "헐 사무실이였어요?",
          "original_form": "헐 사무실이였어요?",
          "speaker_id": "1",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100023014.1.5",
          "form": "밥은 먹엇어요?",
          "original_form": "밥은 먹엇어요?",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023014.1.6",
          "form": "ㅎㅎ 사무실사 놀고있어요ㅎ",
          "original_form": "ㅎㅎ 사무실사 놀고있어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023014.1.7",
          "form": "아무래도 집에있으면 못할것같애서요ㅎ",
          "original_form": "아무래도 집에있으면 못할것같애서요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023014.1.8",
          "form": "밥은욤?",
          "original_form": "밥은욤?",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023014.1.9",
          "form": "밥은 패쓰요ㅎㅎㅎ",
          "original_form": "밥은 패쓰요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023014.1.10",
          "form": "커피먹어서 괜찮아요ㅎㅎ",
          "original_form": "커피먹어서 괜찮아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023014.1.11",
          "form": "원래는 서류만 정리하고 들어가려고했는데",
          "original_form": "원래는 서류만 정리하고 들어가려고했는데",
          "speaker_id": "2",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023014.1.12",
          "form": "에궁 뭐라도 챙겨 드시지",
          "original_form": "에궁 뭐라도 챙겨 드시지",
          "speaker_id": "1",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023014.1.13",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "1",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023014.1.14",
          "form": "아내가 또.... 성질내서 가면 싸울것같애서ㅎㅎ",
          "original_form": "아내가 또.... 성질내서 가면 싸울것같애서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023014.1.15",
          "form": "그래서 피하는중ㅎ",
          "original_form": "그래서 피하는중ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023014.1.16",
          "form": "배가 별로 안고파요ㅎ 아까 김치찌개, 라면 먹었어요ㅎㅎ",
          "original_form": "배가 별로 안고파요ㅎ 아까 김치찌개, 라면 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023014.1.17",
          "form": "아 그러셨구나",
          "original_form": "아 그러셨구나",
          "speaker_id": "1",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023014.1.18",
          "form": "김치찌개라 그나마다행요",
          "original_form": "김치찌개라 그나마다행요",
          "speaker_id": "1",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023014.1.19",
          "form": "ㅎㅎ 걱정해줘서 고마워요ㅎ",
          "original_form": "ㅎㅎ 걱정해줘서 고마워요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023014.1.20",
          "form": "아아님 밖에없네ㅎ",
          "original_form": "아아님 밖에없네ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023014.1.21",
          "form": "우리회사직원 욕좀해볼까요?ㅎ",
          "original_form": "우리회사직원 욕좀해볼까요?ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023014.1.22",
          "form": "근데 화상 오티 음",
          "original_form": "근데 화상 오티 음",
          "speaker_id": "1",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023014.1.23",
          "form": "ㅋㅋ 그래요",
          "original_form": "ㅋㅋ 그래요",
          "speaker_id": "1",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023014.1.24",
          "form": "ㅎㅎㅎㅎㅇ",
          "original_form": "ㅎㅎㅎㅎㅇ",
          "speaker_id": "2",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023014.1.25",
          "form": "계속 심심이해야징ㅎ",
          "original_form": "계속 심심이해야징ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023014.1.26",
          "form": "우리회사에 20년정도 근무한 여자 직원분이있었는데",
          "original_form": "우리회사에 20년정도 근무한 여자 직원분이있었는데",
          "speaker_id": "2",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023014.1.27",
          "form": "이번에 육아휴직을 1년썼어요ㅎ",
          "original_form": "이번에 육아휴직을 1년썼어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023014.1.28",
          "form": "아이들이 초등학교 다니고 그런데 썼더라구요ㅎ",
          "original_form": "아이들이 초등학교 다니고 그런데 썼더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023014.1.29",
          "form": "아항.네넹",
          "original_form": "아항.네넹",
          "speaker_id": "1",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023014.1.30",
          "form": "그런데ㅎ 갑자기 쓴거보니깐ㅎ",
          "original_form": "그런데ㅎ 갑자기 쓴거보니깐ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023014.1.31",
          "form": "아무래도 요즘 회사일이 바쁘니깐 쓴것같애요",
          "original_form": "아무래도 요즘 회사일이 바쁘니깐 쓴것같애요",
          "speaker_id": "2",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100023014.1.32",
          "form": "상무님이 한분 바뀌셨는데 엄청 똑똑하신가봐요",
          "original_form": "상무님이 한분 바뀌셨는데 엄청 똑똑하신가봐요",
          "speaker_id": "2",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100023014.1.33",
          "form": "뭐 회계쪽으론 거의 회계사급? 이런대요ㅎ",
          "original_form": "뭐 회계쪽으론 거의 회계사급? 이런대요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100023014.1.34",
          "form": "그래서 항상 직원들에게 어느정도 기준을 요구하는거에요ㅎ",
          "original_form": "그래서 항상 직원들에게 어느정도 기준을 요구하는거에요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100023014.1.35",
          "form": "아하  머리 쓰신건가요~",
          "original_form": "아하  머리 쓰신건가요~",
          "speaker_id": "1",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100023014.1.36",
          "form": "문자로 보셔도 뻔히보아죠?",
          "original_form": "문자로 보셔도 뻔히보아죠?",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100023014.1.37",
          "form": "네네 ㅎㅎㅎ",
          "original_form": "네네 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100023014.1.38",
          "form": "자기 남편이 1년 육아휴직하고 공무원이됬거든요",
          "original_form": "자기 남편이 1년 육아휴직하고 공무원이됬거든요",
          "speaker_id": "2",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100023014.1.39",
          "form": "네넵 ~~",
          "original_form": "네넵 ~~",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100023014.1.40",
          "form": "그러니깐 자기도 그렇게 한건가봐요ㅎ",
          "original_form": "그러니깐 자기도 그렇게 한건가봐요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100023014.1.41",
          "form": "근데 너무 무책임한게 남아있는사람은 어떡해요?",
          "original_form": "근데 너무 무책임한게 남아있는사람은 어떡해요?",
          "speaker_id": "2",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100023014.1.42",
          "form": "이기적이긴 하네요 후웁",
          "original_form": "이기적이긴 하네요 후웁",
          "speaker_id": "1",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100023014.1.43",
          "form": "맞아요ㅍ",
          "original_form": "맞아요ㅍ",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100023014.1.44",
          "form": "엄청 힘들어서 맨날 야근하고 같이 고생하는데",
          "original_form": "엄청 힘들어서 맨날 야근하고 같이 고생하는데",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100023014.1.45",
          "form": "조금이라도 보탬이 되진 않더라도",
          "original_form": "조금이라도 보탬이 되진 않더라도",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100023014.1.46",
          "form": "피해는 주지말아야죠ㅎ 나중에 그만두더라도",
          "original_form": "피해는 주지말아야죠ㅎ 나중에 그만두더라도",
          "speaker_id": "2",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100023014.1.47",
          "form": "제일  싫어하는 스탈이네요 ㅜㅜ",
          "original_form": "제일  싫어하는 스탈이네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:41"
        }
      ]
    }
