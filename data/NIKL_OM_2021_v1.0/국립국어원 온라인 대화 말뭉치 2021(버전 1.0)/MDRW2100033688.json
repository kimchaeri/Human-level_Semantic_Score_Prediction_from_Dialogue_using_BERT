      "utterance": [
        {
          "id": "MDRW2100033688.1.1",
          "form": "오늘 태풍이 온대서 걱정했는데, 생각보단 잠잠이 지나갔네요",
          "original_form": "오늘 태풍이 온대서 걱정했는데, 생각보단 잠잠이 지나갔네요",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100033688.1.2",
          "form": "그러게요 제주도 400미리온댔는데",
          "original_form": "그러게요 제주도 400미리온댔는데",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100033688.1.3",
          "form": "수도권은 그냥 비오는 정도",
          "original_form": "수도권은 그냥 비오는 정도",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100033688.1.4",
          "form": "네 저도 경기도에 사는데",
          "original_form": "네 저도 경기도에 사는데",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100033688.1.5",
          "form": "낮에 비 오고 큰 소란 없이 지나갔어요",
          "original_form": "낮에 비 오고 큰 소란 없이 지나갔어요",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100033688.1.6",
          "form": "근데 가을장마인지 계속 비오네요",
          "original_form": "근데 가을장마인지 계속 비오네요",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100033688.1.7",
          "form": "네 요즘 비 자주 오죠",
          "original_form": "네 요즘 비 자주 오죠",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033688.1.8",
          "form": "갑자기 비 올 때도 많고",
          "original_form": "갑자기 비 올 때도 많고",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033688.1.9",
          "form": "빨리 가을하늘 보고싶어요",
          "original_form": "빨리 가을하늘 보고싶어요",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033688.1.10",
          "form": "비좀 언능그치길",
          "original_form": "비좀 언능그치길",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033688.1.11",
          "form": "요즘 아침 저녁은 선선한 바람이 부는 게",
          "original_form": "요즘 아침 저녁은 선선한 바람이 부는 게",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100033688.1.12",
          "form": "가을이 성큼 다가온 것 같아요",
          "original_form": "가을이 성큼 다가온 것 같아요",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100033688.1.13",
          "form": "비 얼른 그치고 가을하늘 보고싶네요",
          "original_form": "비 얼른 그치고 가을하늘 보고싶네요",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100033688.1.14",
          "form": "비그치면 추워질거같아요",
          "original_form": "비그치면 추워질거같아요",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100033688.1.15",
          "form": "ㅠㅠ가을이 좀 더 길면 좋겠는데",
          "original_form": "ㅠㅠ가을이 좀 더 길면 좋겠는데",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033688.1.16",
          "form": "금방 겨울이 오려나요?",
          "original_form": "금방 겨울이 오려나요?",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033688.1.17",
          "form": "아마도 가을이네 하는순간 겨울올듯요ㅎ",
          "original_form": "아마도 가을이네 하는순간 겨울올듯요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033688.1.18",
          "form": "그럴 듯해요",
          "original_form": "그럴 듯해요",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033688.1.19",
          "form": "요즘 봄과 가을은 너무 짧게 지나가요",
          "original_form": "요즘 봄과 가을은 너무 짧게 지나가요",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033688.1.20",
          "form": "요즘은 간절기옷사기도 애매해요",
          "original_form": "요즘은 간절기옷사기도 애매해요",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033688.1.21",
          "form": "몇번 입지도  못하더라구요",
          "original_form": "몇번 입지도  못하더라구요",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033688.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033688.1.23",
          "form": "그래도 간절기 옷은 필요하니까 사게되고",
          "original_form": "그래도 간절기 옷은 필요하니까 사게되고",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033688.1.24",
          "form": "비가 계속 오네요",
          "original_form": "비가 계속 오네요",
          "speaker_id": "1",
