      "utterance": [
        {
          "id": "MDRW2100018744.1.1",
          "form": "안녕하세요^^ 점심은 드셨나요?",
          "original_form": "안녕하세요^^ 점심은 드셨나요?",
          "speaker_id": "1",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018744.1.2",
          "form": "넹 저는 오늘 떡볶이 먹었어요",
          "original_form": "넹 저는 오늘 떡볶이 먹었어요",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018744.1.3",
          "form": "떡볶이에 맥주...ㅋㅋㅋ",
          "original_form": "떡볶이에 맥주...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018744.1.4",
          "form": "아 낮부터 맥주를 ㅋㅋㅋ",
          "original_form": "아 낮부터 맥주를 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018744.1.5",
          "form": "어떤 맥주 드셨어요?",
          "original_form": "어떤 맥주 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018744.1.6",
          "form": "곰표 밀맥주요!",
          "original_form": "곰표 밀맥주요!",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018744.1.7",
          "form": "요즘 콜라보레이션 해서 이것저것 새로운 제품들이 많더라고요",
          "original_form": "요즘 콜라보레이션 해서 이것저것 새로운 제품들이 많더라고요",
          "speaker_id": "2",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018744.1.8",
          "form": "아~ 곰표가 밀가루 회사인데",
          "original_form": "아~ 곰표가 밀가루 회사인데",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018744.1.9",
          "form": "맥주라고 하셔서 놀랐어요",
          "original_form": "맥주라고 하셔서 놀랐어요",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018744.1.10",
          "form": "ㅋㅋㅋㅋ 맥주에 밀가루 타먹는 거 생각하신건 아니죠?",
          "original_form": "ㅋㅋㅋㅋ 맥주에 밀가루 타먹는 거 생각하신건 아니죠?",
          "speaker_id": "2",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018744.1.11",
          "form": "사실 그렇게 생각했네요 ㅎㅎ",
          "original_form": "사실 그렇게 생각했네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018744.1.12",
          "form": "아~ 요즘 그런식으로 콜라보레이션하는 게 트렌드군요",
          "original_form": "아~ 요즘 그런식으로 콜라보레이션하는 게 트렌드군요",
          "speaker_id": "1",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018744.1.13",
          "form": "네 맞아요 곰표 밀맥주 말고도",
          "original_form": "네 맞아요 곰표 밀맥주 말고도",
          "speaker_id": "2",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018744.1.14",
          "form": "모나미 아시죠?",
          "original_form": "모나미 아시죠?",
          "speaker_id": "2",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018744.1.15",
          "form": "네 우리나라 볼펜회사잖아요",
          "original_form": "네 우리나라 볼펜회사잖아요",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018744.1.16",
          "form": "맞아요 모나미랑도 협력해서 상품 내놓고 하더라구요",
          "original_form": "맞아요 모나미랑도 협력해서 상품 내놓고 하더라구요",
          "speaker_id": "2",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018744.1.17",
          "form": "우와 신기하네요",
          "original_form": "우와 신기하네요",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018744.1.18",
          "form": "보통 음식이랑 많이 콜라보하나보다",
          "original_form": "보통 음식이랑 많이 콜라보하나보다",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018744.1.19",
          "form": "넵 그런 편이에요",
          "original_form": "넵 그런 편이에요",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018744.1.20",
          "form": "그나저나 곰표 밀맥주 맛있나요?",
          "original_form": "그나저나 곰표 밀맥주 맛있나요?",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018744.1.21",
          "form": "신기해서 저도 먹고싶어지네요 ㅋㅋ",
          "original_form": "신기해서 저도 먹고싶어지네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018744.1.22",
          "form": "저도 오늘 처음 먹어봤는데 생각보다 맛있어요",
          "original_form": "저도 오늘 처음 먹어봤는데 생각보다 맛있어요",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018744.1.23",
          "form": "곰표 팝콘도 있길래 그거도 먹고요 ㅎㅎ",
          "original_form": "곰표 팝콘도 있길래 그거도 먹고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018744.1.24",
          "form": "앗 곰표 팝콘도 있어요? ㅋㅋ 귀엽네요",
          "original_form": "앗 곰표 팝콘도 있어요? ㅋㅋ 귀엽네요",
          "speaker_id": "1",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018744.1.25",
          "form": "다음에 한번 콜라보레이션 음식 먹어봐야겠어요",
          "original_form": "다음에 한번 콜라보레이션 음식 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 11:47"
        }
      ]
