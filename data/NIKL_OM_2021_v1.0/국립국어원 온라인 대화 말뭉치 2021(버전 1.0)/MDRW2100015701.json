      "utterance": [
        {
          "id": "MDRW2100015701.1.1",
          "form": "어떤 게임 좋아하세요?",
          "original_form": "어떤 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100015701.1.2",
          "form": "ㅋㅋ딱히없어요 넘 중독돼서 안하려고 하거든요 ㅠㅠ최근에 메이플햇엇어요!",
          "original_form": "ㅋㅋ딱히없어요 넘 중독돼서 안하려고 하거든요 ㅠㅠ최근에 메이플햇엇어요!",
          "speaker_id": "2",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015701.1.3",
          "form": "슈퍼스타리듬게임이낰ㅋㅋㅋ",
          "original_form": "슈퍼스타리듬게임이낰ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015701.1.4",
          "form": "오 메이플",
          "original_form": "오 메이플",
          "speaker_id": "1",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100015701.1.5",
          "form": "저는 pc게임은 데스크탑이 없은 지 꽤 돼서 모바일로만 하는 것 같아요",
          "original_form": "저는 pc게임은 데스크탑이 없은 지 꽤 돼서 모바일로만 하는 것 같아요",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015701.1.6",
          "form": "쿠킹덤 하는데",
          "original_form": "쿠킹덤 하는데",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015701.1.7",
          "form": "시간을 많이 쏟아야 해서 힘들어요 ㅋㅋㅋㅋ",
          "original_form": "시간을 많이 쏟아야 해서 힘들어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015701.1.8",
          "form": "저도 그래서 아예 시작을 안했어요 ㅋㅋ",
          "original_form": "저도 그래서 아예 시작을 안했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015701.1.9",
          "form": "ㅋㅋㅋㅋㅋ현명하세요",
          "original_form": "ㅋㅋㅋㅋㅋ현명하세요",
          "speaker_id": "1",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015701.1.10",
          "form": "쿠키들 귀엽던데..ㅠㅠ",
          "original_form": "쿠키들 귀엽던데..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 01:02"
        },
        {
          "id": "MDRW2100015701.1.11",
          "form": "그건 맞아요 ㅋㅋㅋㅋ",
          "original_form": "그건 맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015701.1.12",
          "form": "하지만 키워줄려면 노동이죠",
          "original_form": "하지만 키워줄려면 노동이죠",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015701.1.13",
          "form": "ㅋㅋㅋㅋ 막 레벨업하는거도 복잡해보이더라구욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 막 레벨업하는거도 복잡해보이더라구욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100015701.1.14",
          "form": "만렙 쿠키는 단 두개입니다...",
          "original_form": "만렙 쿠키는 단 두개입니다...",
          "speaker_id": "1",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015701.1.15",
          "form": "별사탕을 어찌나 먹는지",
          "original_form": "별사탕을 어찌나 먹는지",
          "speaker_id": "1",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015701.1.16",
          "form": "힘드네요 쿠키 키우기",
          "original_form": "힘드네요 쿠키 키우기",
          "speaker_id": "1",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015701.1.17",
          "form": "ㅋㅋㅋ귀여워요",
          "original_form": "ㅋㅋㅋ귀여워요",
          "speaker_id": "2",
          "time": "20210826 01:11"
        },
        {
          "id": "MDRW2100015701.1.18",
          "form": "귀여운 맛에 열심히 키웁니다...!",
          "original_form": "귀여운 맛에 열심히 키웁니다...!",
          "speaker_id": "1",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015701.1.19",
          "form": "리듬 게임 어떤 거 좋아하세요?",
          "original_form": "리듬 게임 어떤 거 좋아하세요?",
          "speaker_id": "1",
