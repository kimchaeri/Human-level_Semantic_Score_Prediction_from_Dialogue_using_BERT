      "utterance": [
        {
          "id": "MDRW2100043604.1.1",
          "form": "토끼정 가봣슈?",
          "original_form": "토끼정 가봣슈?",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.2",
          "form": "노 아직",
          "original_form": "노 아직",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.3",
          "form": "거기에 크림카레우동 먹어봣어??",
          "original_form": "거기에 크림카레우동 먹어봣어??",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.4",
          "form": "아니ㅜㅠ",
          "original_form": "아니ㅜㅠ",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.5",
          "form": "와 그거진짜",
          "original_form": "와 그거진짜",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.6",
          "form": "혁명적인 맛이래",
          "original_form": "혁명적인 맛이래",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.7",
          "form": "오그래??",
          "original_form": "오그래??",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.8",
          "form": "어어 진짜..",
          "original_form": "어어 진짜..",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.9",
          "form": "상상 안가는데",
          "original_form": "상상 안가는데",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.10",
          "form": "나도 그생각햇는데",
          "original_form": "나도 그생각햇는데",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.11",
          "form": "의외의 맛이라네",
          "original_form": "의외의 맛이라네",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.12",
          "form": "크림에",
          "original_form": "크림에",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.13",
          "form": "카레가 잇어?",
          "original_form": "카레가 잇어?",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.14",
          "form": "아래는",
          "original_form": "아래는",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043604.1.15",
          "form": "카레고 위는 크림!",
          "original_form": "카레고 위는 크림!",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043604.1.16",
          "form": "사진보고싶다",
          "original_form": "사진보고싶다",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043604.1.17",
          "form": "그 검색하면 나올거야",
          "original_form": "그 검색하면 나올거야",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043604.1.18",
          "form": "토끼정 대구도잇어?",
          "original_form": "토끼정 대구도잇어?",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043604.1.19",
          "form": "응 있어",
          "original_form": "응 있어",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043604.1.20",
          "form": "어디?",
          "original_form": "어디?",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043604.1.21",
          "form": "신세계!!",
          "original_form": "신세계!!",
          "speaker_id": "2",
