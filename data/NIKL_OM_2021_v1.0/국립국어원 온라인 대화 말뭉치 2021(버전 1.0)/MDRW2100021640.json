      "utterance": [
        {
          "id": "MDRW2100021640.1.1",
          "form": "아기용품은 다 사놨어?",
          "original_form": "아기용품은 다 사놨어?",
          "speaker_id": "1",
          "time": "20210829 09:14"
        },
        {
          "id": "MDRW2100021640.1.2",
          "form": "아녀ㅋㅋ",
          "original_form": "아녀ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:51"
        },
        {
          "id": "MDRW2100021640.1.3",
          "form": "애기 나올날이 먼사람처럼 1도 준비안함",
          "original_form": "애기 나올날이 먼사람처럼 1도 준비안함",
          "speaker_id": "2",
          "time": "20210829 09:51"
        },
        {
          "id": "MDRW2100021640.1.4",
          "form": "ㅋㅋㅋㅋㅋ 짱",
          "original_form": "ㅋㅋㅋㅋㅋ 짱",
          "speaker_id": "1",
          "time": "20210829 09:52"
        },
        {
          "id": "MDRW2100021640.1.5",
          "form": "ㅋㅋㅋ이제해아하는데",
          "original_form": "ㅋㅋㅋ이제해아하는데",
          "speaker_id": "2",
          "time": "20210829 09:52"
        },
        {
          "id": "MDRW2100021640.1.6",
          "form": "그나마 둘째라 다행",
          "original_form": "그나마 둘째라 다행",
          "speaker_id": "1",
          "time": "20210829 09:52"
        },
        {
          "id": "MDRW2100021640.1.7",
          "form": "ㅋㅋ그래도 머라도 사야하는데 넋이라도 잇고없고",
          "original_form": "ㅋㅋ그래도 머라도 사야하는데 넋이라도 잇고없고",
          "speaker_id": "2",
          "time": "20210829 10:20"
        },
        {
          "id": "MDRW2100021640.1.8",
          "form": "ㅎㅎㅎㅎ 뭐사야하지?",
          "original_form": "ㅎㅎㅎㅎ 뭐사야하지?",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100021640.1.9",
          "form": "ㅋㅋㅋㅋ 쪽쪽이도 사야하나",
          "original_form": "ㅋㅋㅋㅋ 쪽쪽이도 사야하나",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100021640.1.10",
          "form": "ㅋㅋ그것도 필수죠",
          "original_form": "ㅋㅋ그것도 필수죠",
          "speaker_id": "2",
          "time": "20210829 15:15"
        },
        {
          "id": "MDRW2100021640.1.11",
          "form": "ㅎㅎ 살것이 한가득",
          "original_form": "ㅎㅎ 살것이 한가득",
          "speaker_id": "1",
          "time": "20210829 15:16"
        },
        {
          "id": "MDRW2100021640.1.12",
          "form": "ㅋㅋ근데 귀차니즘",
          "original_form": "ㅋㅋ근데 귀차니즘",
          "speaker_id": "2",
          "time": "20210829 15:16"
        },
        {
          "id": "MDRW2100021640.1.13",
          "form": "침대도 사야하고",
          "original_form": "침대도 사야하고",
          "speaker_id": "2",
          "time": "20210829 15:16"
        },
        {
          "id": "MDRW2100021640.1.14",
          "form": "살것도 많은만큼 집안은 채워지고",
          "original_form": "살것도 많은만큼 집안은 채워지고",
          "speaker_id": "1",
          "time": "20210829 15:21"
        },
        {
          "id": "MDRW2100021640.1.15",
          "form": "내발은 어디에 놓냐며",
          "original_form": "내발은 어디에 놓냐며",
          "speaker_id": "1",
          "time": "20210829 15:21"
        },
        {
          "id": "MDRW2100021640.1.16",
          "form": "ㅋㅋ발디딜곳도 없다며",
          "original_form": "ㅋㅋ발디딜곳도 없다며",
          "speaker_id": "2",
          "time": "20210829 15:22"
        },
        {
          "id": "MDRW2100021640.1.17",
          "form": "그래도 옷 이런건 있으니 다행인 부분",
          "original_form": "그래도 옷 이런건 있으니 다행인 부분",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100021640.1.18",
          "form": "ㅋㅋ맞아여 같은여아라",
          "original_form": "ㅋㅋ맞아여 같은여아라",
          "speaker_id": "2",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100021640.1.19",
          "form": "그나마 다행",
          "original_form": "그나마 다행",
          "speaker_id": "1",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100021640.1.20",
          "form": "ㅋ남자엿어도 그냥 입혓을듯",
          "original_form": "ㅋ남자엿어도 그냥 입혓을듯",
          "speaker_id": "2",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100021640.1.21",
          "form": "그치 아기는 다 용서됨",
          "original_form": "그치 아기는 다 용서됨",
          "speaker_id": "1",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100021640.1.22",
          "form": "아기용품쇼핑이 만만치않지",
          "original_form": "아기용품쇼핑이 만만치않지",
          "speaker_id": "1",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100021640.1.23",
          "form": "ㅋㅋ요즘 내꺼는 항상 못사지",
          "original_form": "ㅋㅋ요즘 내꺼는 항상 못사지",
          "speaker_id": "2",
