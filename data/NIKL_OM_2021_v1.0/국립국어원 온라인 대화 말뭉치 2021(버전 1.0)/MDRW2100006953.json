      "utterance": [
        {
          "id": "MDRW2100006953.1.1",
          "form": "오늘 아침에",
          "original_form": "오늘 아침에",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006953.1.2",
          "form": "비가 엄청 왔다",
          "original_form": "비가 엄청 왔다",
          "speaker_id": "1",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006953.1.3",
          "form": "그치만",
          "original_form": "그치만",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006953.1.4",
          "form": "나는 문을 열고잤다",
          "original_form": "나는 문을 열고잤다",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006953.1.5",
          "form": "엄마한테 혼났다",
          "original_form": "엄마한테 혼났다",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006953.1.6",
          "form": "사실 알고있었지만 귀찮아서 창문을 닫지 않았다",
          "original_form": "사실 알고있었지만 귀찮아서 창문을 닫지 않았다",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006953.1.7",
          "form": "혼날 만 한걸",
          "original_form": "혼날 만 한걸",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006953.1.8",
          "form": "그치만 귀찮은데 어떡하겠어",
          "original_form": "그치만 귀찮은데 어떡하겠어",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006953.1.9",
          "form": "비 오면 자동으로 창문 닫히는 집!",
          "original_form": "비 오면 자동으로 창문 닫히는 집!",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006953.1.10",
          "form": "구해줘",
          "original_form": "구해줘",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006953.1.11",
          "form": "우리집도 없는데",
          "original_form": "우리집도 없는데",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006953.1.12",
          "form": "돈으로 산 집은 강아지집뿐이야",
          "original_form": "돈으로 산 집은 강아지집뿐이야",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006953.1.13",
          "form": "강아지도 집이 있어야징",
          "original_form": "강아지도 집이 있어야징",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006953.1.14",
          "form": "요새 저녁에는 바람이 선선해서",
          "original_form": "요새 저녁에는 바람이 선선해서",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006953.1.15",
          "form": "완전가을이야",
          "original_form": "완전가을이야",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006953.1.16",
          "form": "name3 산책하기 좋겠어",
          "original_form": "&name3& 산책하기 좋겠어",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006953.1.17",
          "form": "name3는 요즘 유모차타고 산책해",
          "original_form": "&name3&는 요즘 유모차타고 산책해",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006953.1.18",
          "form": "비가 와서 안올때 잘 맞춰나가야해",
          "original_form": "비가 와서 안올때 잘 맞춰나가야해",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006953.1.19",
          "form": "개강은 언제하니",
          "original_form": "개강은 언제하니",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006953.1.20",
          "form": "어제 저녁에 비 무지 왔지",
          "original_form": "어제 저녁에 비 무지 왔지",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006953.1.21",
          "form": "요새 완전 동남아날씨야",
          "original_form": "요새 완전 동남아날씨야",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006953.1.22",
          "form": "갑자기와",
          "original_form": "갑자기와",
          "speaker_id": "1",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006953.1.23",
          "form": "태풍온다는 일기예보 들은 것 같아",
          "original_form": "태풍온다는 일기예보 들은 것 같아",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006953.1.24",
          "form": "헐~~~~",
          "original_form": "헐~~~~",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006953.1.25",
          "form": "여름 다끝났는데 무슨 태풍이야",
          "original_form": "여름 다끝났는데 무슨 태풍이야",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006953.1.26",
          "form": "난 태풍 좋아",
          "original_form": "난 태풍 좋아",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006953.1.27",
          "form": "태풍핑계로 학원 안가도될까",
          "original_form": "태풍핑계로 학원 안가도될까",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006953.1.28",
          "form": "쿨하다",
          "original_form": "쿨하다",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006953.1.29",
          "form": "고등학교때는 지구가 날라가도 학원에 갔었는데",
          "original_form": "고등학교때는 지구가 날라가도 학원에 갔었는데",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006953.1.30",
          "form": "비오는날은 학원 가기싫었지",
          "original_form": "비오는날은 학원 가기싫었지",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100006953.1.31",
          "form": "항상이였나",
          "original_form": "항상이였나",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100006953.1.32",
          "form": "응 날이 좋아도 가기싫지",
          "original_form": "응 날이 좋아도 가기싫지",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100006953.1.33",
          "form": "그래도 비오면 쌤도 같이 하기싫어해서",
          "original_form": "그래도 비오면 쌤도 같이 하기싫어해서",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100006953.1.34",
          "form": "놀았엉ᆢ리는",
          "original_form": "놀았엉ᆢ리는",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100006953.1.35",
          "form": "놀았었어 우리는",
          "original_form": "놀았었어 우리는",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100006953.1.36",
          "form": "어우 좋네",
          "original_form": "어우 좋네",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100006953.1.37",
          "form": "학생에게 좋은 학원이야",
          "original_form": "학생에게 좋은 학원이야",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100006953.1.38",
          "form": "이젠 밤에 춥더라",
          "original_form": "이젠 밤에 춥더라",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100006953.1.39",
          "form": "맞아.... 완전 가을이야 가을",
          "original_form": "맞아.... 완전 가을이야 가을",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100006953.1.40",
          "form": "낮엔 이런 얘기 안나오지먀",
          "original_form": "낮엔 이런 얘기 안나오지먀",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100006953.1.41",
          "form": "만",
          "original_form": "만",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100006953.1.42",
          "form": "낮은 여름이고 밤은 가을이야",
          "original_form": "낮은 여름이고 밤은 가을이야",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100006953.1.43",
          "form": "예전에는 겨울이 좋았는데 요즘은 가을이 좋더라",
          "original_form": "예전에는 겨울이 좋았는데 요즘은 가을이 좋더라",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100006953.1.44",
          "form": "나이를 먹은걸까",
          "original_form": "나이를 먹은걸까",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100006953.1.45",
          "form": "나도 가을좋아해",
          "original_form": "나도 가을좋아해",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100006953.1.46",
          "form": "가을은 내 생일",
          "original_form": "가을은 내 생일",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100006953.1.47",
          "form": "겨울은 그래도 벌레가 없잖아",
          "original_form": "겨울은 그래도 벌레가 없잖아",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100006953.1.48",
          "form": "난 겨울 좋아해",
          "original_form": "난 겨울 좋아해",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100006953.1.49",
          "form": "겨울 내 생일",
          "original_form": "겨울 내 생일",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100006953.1.50",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100006953.1.51",
          "form": "겨울의 추억",
          "original_form": "겨울의 추억",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100006953.1.52",
          "form": "겨울에 너희 가족 놀러갔을 때",
          "original_form": "겨울에 너희 가족 놀러갔을 때",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100006953.1.53",
          "form": "name3 밥챙겨줬던 추억",
          "original_form": "&name3& 밥챙겨줬던 추억",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100006953.1.54",
          "form": "그치 name3 요즘 친구들와도 안 짖어",
          "original_form": "그치 &name3& 요즘 친구들와도 안 짖어",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100006953.1.55",
          "form": "늦여름 바람 맞고 name3는 코막혀하고있어",
          "original_form": "늦여름 바람 맞고 &name3&는 코막혀하고있어",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100006953.1.56",
          "form": "가을name3는 산책할때",
          "original_form": "가을&name3&는 산책할때",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100006953.1.57",
          "form": "양쪽볼에 낙엽을 붙이고와",
          "original_form": "양쪽볼에 낙엽을 붙이고와",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100006953.1.58",
          "form": "귀엽다 가을name3",
          "original_form": "귀엽다 가을&name3&",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100006953.1.59",
          "form": "여름감기는 개도 걸리는 거였구나",
          "original_form": "여름감기는 개도 걸리는 거였구나",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100006953.1.60",
          "form": "그런가봐",
          "original_form": "그런가봐",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100006953.1.61",
          "form": "겨울name3는 눈속에서 제일 잘보여",
          "original_form": "겨울&name3&는 눈속에서 제일 잘보여",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100006953.1.62",
          "form": "계절별 name3",
          "original_form": "계절별 &name3&",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100006953.1.63",
          "form": "눈에서 잘보일 거 생각하니까 귀엽다",
          "original_form": "눈에서 잘보일 거 생각하니까 귀엽다",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100006953.1.64",
          "form": "오늘도 날씨가 흐려",
          "original_form": "오늘도 날씨가 흐려",
          "speaker_id": "2",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100006953.1.65",
          "form": "오늘도 비오겠지?",
          "original_form": "오늘도 비오겠지?",
          "speaker_id": "2",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100006953.1.66",
          "form": "난 집에 있을거니까 비와도 좋아",
          "original_form": "난 집에 있을거니까 비와도 좋아",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100006953.1.67",
          "form": "비올때 나가있으면 눈물나지만",
          "original_form": "비올때 나가있으면 눈물나지만",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100006953.1.68",
          "form": "저녁에 name3랑 나가면 추워",
          "original_form": "저녁에 &name3&랑 나가면 추워",
          "speaker_id": "2",
          "time": "20210822 19:18"
        },
        {
