      "utterance": [
        {
          "id": "MDRW2100043169.1.1",
          "form": "네네 삶이 빡셉니다",
          "original_form": "네네 삶이 빡셉니다",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043169.1.2",
          "form": "워라밸좋으신가요",
          "original_form": "워라밸좋으신가요",
          "speaker_id": "1",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043169.1.3",
          "form": "그래도 대학병원 만큼은 아니겠지요",
          "original_form": "그래도 대학병원 만큼은 아니겠지요",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043169.1.4",
          "form": "워라밸 안좋아요 ㅋㅋㅋㅋㅋ",
          "original_form": "워라밸 안좋아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043169.1.5",
          "form": "한의사 너무 부러워요ㅠㅠ 전 수학 못해서 꿈 접었잖아요",
          "original_form": "한의사 너무 부러워요ㅠㅠ 전 수학 못해서 꿈 접었잖아요",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043169.1.6",
          "form": "헛ㅜㅜ그렇군요ㅠㅜ",
          "original_form": "헛ㅜㅜ그렇군요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043169.1.7",
          "form": "저도수학ㅋㅋㅋ꾸역꾸역했어요",
          "original_form": "저도수학ㅋㅋㅋ꾸역꾸역했어요",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043169.1.8",
          "form": "역시 멋져요 뀨역꾸역 해여죠",
          "original_form": "역시 멋져요 뀨역꾸역 해여죠",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043169.1.9",
          "form": "대학병원 다니시면서 석박사통합까지 너무 힌드시겠다",
          "original_form": "대학병원 다니시면서 석박사통합까지 너무 힌드시겠다",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043169.1.10",
          "form": "ㅎㅎㅎ근데하기싫어서계속안하다가안그래도ㅎㅎ한번불려갔어요ㅜㅜ",
          "original_form": "ㅎㅎㅎ근데하기싫어서계속안하다가안그래도ㅎㅎ한번불려갔어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100043169.1.11",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100043169.1.12",
          "form": "엌덕해요ㅠㅠㅠ",
          "original_form": "엌덕해요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100043169.1.13",
          "form": "근데 한의대도 위계질서 심해요?",
          "original_form": "근데 한의대도 위계질서 심해요?",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100043169.1.14",
          "form": "ㅎㅎㅎ까라면까죵..",
          "original_form": "ㅎㅎㅎ까라면까죵..",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043169.1.15",
          "form": "그래도교수님이합리적인분이세요",
          "original_form": "그래도교수님이합리적인분이세요",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043169.1.16",
          "form": "아휴 그렇군요",
          "original_form": "아휴 그렇군요",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100043169.1.17",
          "form": "오 그나마 합리적이라 다행이에요",
          "original_form": "오 그나마 합리적이라 다행이에요",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043169.1.18",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043169.1.19",
          "form": "존버해야죠 몽ㅎ..",
          "original_form": "존버해야죠 몽ㅎ..",
