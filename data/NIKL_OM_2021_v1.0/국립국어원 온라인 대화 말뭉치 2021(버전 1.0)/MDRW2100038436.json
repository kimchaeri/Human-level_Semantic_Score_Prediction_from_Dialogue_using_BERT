      "utterance": [
        {
          "id": "MDRW2100038436.1.1",
          "form": "오늘 출근룩은 뭔가요",
          "original_form": "오늘 출근룩은 뭔가요",
          "speaker_id": "1",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100038436.1.2",
          "form": "저는 아직 반팔반바지임다ㅋ",
          "original_form": "저는 아직 반팔반바지임다ㅋ",
          "speaker_id": "2",
          "time": "20210902 13:11"
        },
        {
          "id": "MDRW2100038436.1.3",
          "form": "저도요 오늘 재택근무라 반팔 반바지에요 ㅋㅋ",
          "original_form": "저도요 오늘 재택근무라 반팔 반바지에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100038436.1.4",
          "form": "재택근무 조으네요ㅜㅜㅜㅜㅜ",
          "original_form": "재택근무 조으네요ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100038436.1.5",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100038436.1.6",
          "form": "사실 일안하고 놀고이썽여 급한게많은데",
          "original_form": "사실 일안하고 놀고이썽여 급한게많은데",
          "speaker_id": "1",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100038436.1.7",
          "form": "빨리퇴근하고 침대 직행하고싶어요ㅜㅜ",
          "original_form": "빨리퇴근하고 침대 직행하고싶어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100038436.1.8",
          "form": "다섯시 존버하고잇어욤",
          "original_form": "다섯시 존버하고잇어욤",
          "speaker_id": "2",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100038436.1.9",
          "form": "오오 침대ㅠㅠㅠㅠ",
          "original_form": "오오 침대ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100038436.1.10",
          "form": "저도 침대눕고싶으나 죄챡감ㅋㅋㅋ",
          "original_form": "저도 침대눕고싶으나 죄챡감ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100038436.1.11",
          "form": "ㅜㅜ부럽네욤 흑",
          "original_form": "ㅜㅜ부럽네욤 흑",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100038436.1.12",
          "form": "저는 파워집수니라서",
          "original_form": "저는 파워집수니라서",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100038436.1.13",
          "form": "출근과동시에 방전됩니당",
          "original_form": "출근과동시에 방전됩니당",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100038436.1.14",
          "form": "방전 ㅋㅋㅋㅋ저도 그렇습니다",
          "original_form": "방전 ㅋㅋㅋㅋ저도 그렇습니다",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038436.1.15",
          "form": "원랜 파워 밖순이었는데",
          "original_form": "원랜 파워 밖순이었는데",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038436.1.16",
          "form": "저는 밖순이들이 부러워요",
          "original_form": "저는 밖순이들이 부러워요",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038436.1.17",
          "form": "완전 저질체력이라서ㅜㅜㅜㅜ",
          "original_form": "완전 저질체력이라서ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038436.1.18",
          "form": "이젠 집순이어요ㅠㅠ",
          "original_form": "이젠 집순이어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038436.1.19",
          "form": "ㅋㅋ이제 가을에 날좋아지면",
          "original_form": "ㅋㅋ이제 가을에 날좋아지면",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
