      "utterance": [
        {
          "id": "MDRW2100026202.1.1",
          "form": "저 요즘 엄청 많아요 ㅋㅋㅋ",
          "original_form": "저 요즘 엄청 많아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026202.1.2",
          "form": "옷도 사고 싶고 ㅋㅋㅋ 신발도요",
          "original_form": "옷도 사고 싶고 ㅋㅋㅋ 신발도요",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026202.1.3",
          "form": "ㅋㅋ 저는 안마의자요",
          "original_form": "ㅋㅋ 저는 안마의자요",
          "speaker_id": "1",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100026202.1.4",
          "form": "와 ㅋㅋㅋ 안마의자 넘 좋죠",
          "original_form": "와 ㅋㅋㅋ 안마의자 넘 좋죠",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026202.1.5",
          "form": "저는 옷은. . 요즘 외출을안해서ㅋㅋ욕심이 사라졌어요",
          "original_form": "저는 옷은. . 요즘 외출을안해서ㅋㅋ욕심이 사라졌어요",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026202.1.6",
          "form": "친구네에 있어서 봤는데 좋아보이더라고요",
          "original_form": "친구네에 있어서 봤는데 좋아보이더라고요",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026202.1.7",
          "form": "재택근무중이거든요",
          "original_form": "재택근무중이거든요",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026202.1.8",
          "form": "저도 ㅋㅋ 어제도 자켓 샀는데 이걸 언제 입나 싶긴 해요",
          "original_form": "저도 ㅋㅋ 어제도 자켓 샀는데 이걸 언제 입나 싶긴 해요",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026202.1.9",
          "form": "근데 너무 비싸더라구요",
          "original_form": "근데 너무 비싸더라구요",
          "speaker_id": "1",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026202.1.10",
          "form": "재택이면 진짜 옷 잘 안사게 되죠ㅠㅠ",
          "original_form": "재택이면 진짜 옷 잘 안사게 되죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026202.1.11",
          "form": "안마의자 뭐 알아보셨어요?",
          "original_form": "안마의자 뭐 알아보셨어요?",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100026202.1.12",
          "form": "ㅋㅋ계절이바뀌면 또 옷을 사고싶죠",
          "original_form": "ㅋㅋ계절이바뀌면 또 옷을 사고싶죠",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026202.1.13",
          "form": "리쏘랑 세라젬",
          "original_form": "리쏘랑 세라젬",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026202.1.14",
          "form": "두개 테스트했어요",
          "original_form": "두개 테스트했어요",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026202.1.15",
          "form": "2백 아래로 예산잡았는데, 테스트해보니깐",
          "original_form": "2백 아래로 예산잡았는데, 테스트해보니깐",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026202.1.16",
          "form": "3d. 4d라고 몸 사이즈 스캔해서 해주는게 좋긴하더라구요",
          "original_form": "3d. 4d라고 몸 사이즈 스캔해서 해주는게 좋긴하더라구요",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026202.1.17",
          "form": "오 그거 완전 비싸보이네요",
          "original_form": "오 그거 완전 비싸보이네요",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026202.1.18",
          "form": "이런건 3백 넘어가서ㅋㅋ",
          "original_form": "이런건 3백 넘어가서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026202.1.19",
          "form": "근데 사이즈 맞춤이 더 좋을 것 같기는 해요",
          "original_form": "근데 사이즈 맞춤이 더 좋을 것 같기는 해요",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026202.1.20",
          "form": "그래야 좀 더 편하기도 하고 제대로겠어요",
          "original_form": "그래야 좀 더 편하기도 하고 제대로겠어요",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026202.1.21",
          "form": "그쵸ㅋㅋㅋ 계속 알아보면 눈이 높아져요",
          "original_form": "그쵸ㅋㅋㅋ 계속 알아보면 눈이 높아져요",
          "speaker_id": "1",
          "time": "20210901 20:52"
        }
      ]
