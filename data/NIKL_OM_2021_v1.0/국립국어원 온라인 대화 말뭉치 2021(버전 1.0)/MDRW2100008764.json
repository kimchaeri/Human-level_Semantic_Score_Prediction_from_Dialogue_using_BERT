      "utterance": [
        {
          "id": "MDRW2100008764.1.1",
          "form": "요새 날씨 이상함",
          "original_form": "요새 날씨 이상함",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008764.1.2",
          "form": "비가 한번에 너무 많이오지를 않나",
          "original_form": "비가 한번에 너무 많이오지를 않나",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008764.1.3",
          "form": "오가을되서야 찾아온 장마ㅜㅜ",
          "original_form": "오가을되서야 찾아온 장마ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008764.1.4",
          "form": "파란하늘에 빗방울떨어지지를 않나",
          "original_form": "파란하늘에 빗방울떨어지지를 않나",
          "speaker_id": "2",
          "time": "20210822 18:09"
        },
        {
          "id": "MDRW2100008764.1.5",
          "form": "내 방에 비샘ㅜㅜㅜㅜ",
          "original_form": "내 방에 비샘ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008764.1.6",
          "form": "슬퍼용",
          "original_form": "슬퍼용",
          "speaker_id": "1",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008764.1.7",
          "form": "사실 8/31까지는 여름이라 굳게 믿지만 ㅋㅋ",
          "original_form": "사실 8/31까지는 여름이라 굳게 믿지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008764.1.8",
          "form": "헐 어떻게함?",
          "original_form": "헐 어떻게함?",
          "speaker_id": "2",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008764.1.9",
          "form": "주인이 고쳐준대요?",
          "original_form": "주인이 고쳐준대요?",
          "speaker_id": "2",
          "time": "20210822 18:10"
        },
        {
          "id": "MDRW2100008764.1.10",
          "form": "오늘 온다던데 소식이 없으시네 따흐흑ㅜㅜ",
          "original_form": "오늘 온다던데 소식이 없으시네 따흐흑ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008764.1.11",
          "form": "심지어 침대위라서 ㅋㅋ 축축하게 잘뻔했엉",
          "original_form": "심지어 침대위라서 ㅋㅋ 축축하게 잘뻔했엉",
          "speaker_id": "1",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008764.1.12",
          "form": "으아니 빨리고쳐줘야지",
          "original_form": "으아니 빨리고쳐줘야지",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008764.1.13",
          "form": "담주도 비온다는데",
          "original_form": "담주도 비온다는데",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008764.1.14",
          "form": "지금 우째놓음?",
          "original_form": "지금 우째놓음?",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008764.1.15",
          "form": "태풍오고있오",
          "original_form": "태풍오고있오",
          "speaker_id": "1",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008764.1.16",
          "form": "양동이?",
          "original_form": "양동이?",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008764.1.17",
          "form": "비닐?",
          "original_form": "비닐?",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008764.1.18",
          "form": "바가지 둠ㅋㅋ",
          "original_form": "바가지 둠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008764.1.19",
          "form": "얼마나 새는거임ㅜㅜ",
          "original_form": "얼마나 새는거임ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008764.1.20",
          "form": "열악하다",
          "original_form": "열악하다",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008764.1.21",
          "form": "나쁘다 월세도 비싸면서",
          "original_form": "나쁘다 월세도 비싸면서",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008764.1.22",
          "form": "글게 옥탑의 슬픔ㅜㅜㅜㅜㅜㅜ",
          "original_form": "글게 옥탑의 슬픔ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008764.1.23",
          "form": "아 옥탑이에요?",
          "original_form": "아 옥탑이에요?",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008764.1.24",
          "form": "방 옮길수는 없나",
          "original_form": "방 옮길수는 없나",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008764.1.25",
          "form": "어차피 그건물자체가 게스트하우스인데",
          "original_form": "어차피 그건물자체가 게스트하우스인데",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008764.1.26",
          "form": "이사가 귀찮당ㅎ..",
          "original_form": "이사가 귀찮당ㅎ..",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008764.1.27",
          "form": "빈방있음 되지만 지금은 엄네",
          "original_form": "빈방있음 되지만 지금은 엄네",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008764.1.28",
          "form": "저런 ㅠ",
          "original_form": "저런 ㅠ",
          "speaker_id": "2",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008764.1.29",
          "form": "비가 촉촉하게오네 서울은",
          "original_form": "비가 촉촉하게오네 서울은",
          "speaker_id": "1",
          "time": "20210823 21:28"
        }
      ]
    }
  ]
