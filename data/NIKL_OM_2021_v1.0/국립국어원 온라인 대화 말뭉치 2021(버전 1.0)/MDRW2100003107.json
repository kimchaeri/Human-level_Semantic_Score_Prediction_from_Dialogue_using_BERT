      "utterance": [
        {
          "id": "MDRW2100003107.1.1",
          "form": "난 라떼!",
          "original_form": "난 라떼!",
          "speaker_id": "2",
          "time": "20210820 18:47"
        },
        {
          "id": "MDRW2100003107.1.2",
          "form": "잊을 수 없다 그 라떼ㅋㅋ",
          "original_form": "잊을 수 없다 그 라떼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:47"
        },
        {
          "id": "MDRW2100003107.1.3",
          "form": "해수염커피 안 먹어봤지? 것도 꼭 먹어보길 바람",
          "original_form": "해수염커피 안 먹어봤지? 것도 꼭 먹어보길 바람",
          "speaker_id": "1",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100003107.1.4",
          "form": "짭짤한가ㅋㅋ",
          "original_form": "짭짤한가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100003107.1.5",
          "form": "예압 근데 많이는 아니고",
          "original_form": "예압 근데 많이는 아니고",
          "speaker_id": "1",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003107.1.6",
          "form": "한여름에 땀흘리고 먹음 좋을듯ㅎㅎ",
          "original_form": "한여름에 땀흘리고 먹음 좋을듯ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100003107.1.7",
          "form": "부산에 모모스 커피 맛있다고 유명하던데 가보고싶다",
          "original_form": "부산에 모모스 커피 맛있다고 유명하던데 가보고싶다",
          "speaker_id": "1",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100003107.1.8",
          "form": "개인 카페야?",
          "original_form": "개인 카페야?",
          "speaker_id": "2",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100003107.1.9",
          "form": "해운대도 지점 있다던데",
          "original_form": "해운대도 지점 있다던데",
          "speaker_id": "1",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100003107.1.10",
          "form": "음.. 개인이라고 봐야겠지?",
          "original_form": "음.. 개인이라고 봐야겠지?",
          "speaker_id": "1",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003107.1.11",
          "form": "아아 그렇구나ㅎㅎ",
          "original_form": "아아 그렇구나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003107.1.12",
          "form": "거기서 원두만 사도 맛있대",
          "original_form": "거기서 원두만 사도 맛있대",
          "speaker_id": "1",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003107.1.13",
          "form": "오오 드리퍼에 내려 먹고싶네",
          "original_form": "오오 드리퍼에 내려 먹고싶네",
          "speaker_id": "2",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003107.1.14",
          "form": "와 본점 사진 보니까 분위기 장난아님...갈거면 본점으로\r\n..",
          "original_form": "와 본점 사진 보니까 분위기 장난아님...갈거면 본점으로\r\n..",
          "speaker_id": "1",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003107.1.15",
          "form": "본점이 부산 어디에 있는데?",
          "original_form": "본점이 부산 어디에 있는데?",
          "speaker_id": "2",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100003107.1.16",
          "form": "온천장이라고 하면 알래나",
          "original_form": "온천장이라고 하면 알래나",
          "speaker_id": "1",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100003107.1.17",
          "form": "담에 부산가면 꼭 가봐야지",
          "original_form": "담에 부산가면 꼭 가봐야지",
          "speaker_id": "1",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100003107.1.18",
          "form": "나도 데려가줘용ㅋㅋㅋ",
          "original_form": "나도 데려가줘용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:54"
        },
        {
          "id": "MDRW2100003107.1.19",
          "form": "가보자 빵도 맛있다함 ㅋㅋ",
          "original_form": "가보자 빵도 맛있다함 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:54"
        },
        {
          "id": "MDRW2100003107.1.20",
          "form": "아 베이커리 카페구나!",
          "original_form": "아 베이커리 카페구나!",
          "speaker_id": "2",
          "time": "20210820 18:54"
        },
        {
          "id": "MDRW2100003107.1.21",
          "form": "요즘은 여행하면 사람들이 카페투어 많이 하는 듯 하루에 두세개씩 가고 ㅋㅋㅋ",
          "original_form": "요즘은 여행하면 사람들이 카페투어 많이 하는 듯 하루에 두세개씩 가고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100003107.1.22",
          "form": "와 좋다 진짜 코로나 끝나면 카페투어 가쟈!",
          "original_form": "와 좋다 진짜 코로나 끝나면 카페투어 가쟈!",
          "speaker_id": "2",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100003107.1.23",
          "form": "돈 매니 모아 놔 ㅋㅋㅋ",
          "original_form": "돈 매니 모아 놔 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100003107.1.24",
          "form": "우리 그 급하게 먹었던 센텀 국밥 맛있었는데",
          "original_form": "우리 그 급하게 먹었던 센텀 국밥 맛있었는데",
          "speaker_id": "1",
          "time": "20210820 18:56"
        }
      ]
    }
  ]
