      "utterance": [
        {
          "id": "MDRW2100018223.1.1",
          "form": "여름 다 끝났지만 하나장만하세유... ㅋㅋㅋ",
          "original_form": "여름 다 끝났지만 하나장만하세유... ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100018223.1.2",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100018223.1.3",
          "form": "조만간 사야겠어요",
          "original_form": "조만간 사야겠어요",
          "speaker_id": "2",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100018223.1.4",
          "form": "중고거래도 하시나요?",
          "original_form": "중고거래도 하시나요?",
          "speaker_id": "1",
          "time": "20210827 10:23"
        },
        {
          "id": "MDRW2100018223.1.5",
          "form": "네 당근마켓 자주 합니다",
          "original_form": "네 당근마켓 자주 합니다",
          "speaker_id": "2",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018223.1.6",
          "form": "사본건 고데기 하나네요 팔기만 해요 주로",
          "original_form": "사본건 고데기 하나네요 팔기만 해요 주로",
          "speaker_id": "2",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018223.1.7",
          "form": "뭐뭐 팔아보셧어요?",
          "original_form": "뭐뭐 팔아보셧어요?",
          "speaker_id": "1",
          "time": "20210827 11:01"
        },
        {
          "id": "MDRW2100018223.1.8",
          "form": "가구랑 청소기요",
          "original_form": "가구랑 청소기요",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018223.1.9",
          "form": "가..구...요? 어케 팔아요?",
          "original_form": "가..구...요? 어케 팔아요?",
          "speaker_id": "1",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018223.1.10",
          "form": "트럭같은거 불러서요?",
          "original_form": "트럭같은거 불러서요?",
          "speaker_id": "1",
          "time": "20210827 11:29"
        },
        {
          "id": "MDRW2100018223.1.11",
          "form": "차에실려서 집에와서 가져가셨어요",
          "original_form": "차에실려서 집에와서 가져가셨어요",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018223.1.12",
          "form": "가구는 버리면 다 돈인데..",
          "original_form": "가구는 버리면 다 돈인데..",
          "speaker_id": "1",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018223.1.13",
          "form": "맞아요 잘 팔아야해요 ㅋㅋ",
          "original_form": "맞아요 잘 팔아야해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018223.1.14",
          "form": "확실히 당근땜에 생활많이 변한거 같아요",
          "original_form": "확실히 당근땜에 생활많이 변한거 같아요",
          "speaker_id": "1",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018223.1.15",
          "form": "정말 그래요",
          "original_form": "정말 그래요",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018223.1.16",
          "form": "저도 뭐 살때 맘에안들면 팔지.. 이런생각으로 \n충동구매 많이하게 되더라구요",
          "original_form": "저도 뭐 살때 맘에안들면 팔지.. 이런생각으로 \n충동구매 많이하게 되더라구요",
