      "utterance": [
        {
          "id": "MDRW2100045068.1.1",
          "form": "멜론도 좋아하세요??",
          "original_form": "멜론도 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045068.1.2",
          "form": "아 좋아해용 !",
          "original_form": "아 좋아해용 !",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045068.1.3",
          "form": "비싼그",
          "original_form": "비싼그",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045068.1.4",
          "form": "저 멜론은 비싼거 사본적 없어요!!",
          "original_form": "저 멜론은 비싼거 사본적 없어요!!",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045068.1.5",
          "form": "맛있는 멜론 아시면 추천좀 해주세요",
          "original_form": "맛있는 멜론 아시면 추천좀 해주세요",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045068.1.6",
          "form": "허니듀??",
          "original_form": "허니듀??",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045068.1.7",
          "form": "핫딜방에서 자주보이던데",
          "original_form": "핫딜방에서 자주보이던데",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045068.1.8",
          "form": "오 허니듀 맛있나요?",
          "original_form": "오 허니듀 맛있나요?",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045068.1.9",
          "form": "올해는 끝났을거 같고 내년엔 한번 사볼까봐요",
          "original_form": "올해는 끝났을거 같고 내년엔 한번 사볼까봐요",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045068.1.10",
          "form": "네네 ! 이제는 귤의 시대가옵니다",
          "original_form": "네네 ! 이제는 귤의 시대가옵니다",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045068.1.11",
          "form": "하우스 귤이 맛있는데 한번 살까봐요!!",
          "original_form": "하우스 귤이 맛있는데 한번 살까봐요!!",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045068.1.12",
          "form": "귤로장생 많이 사더라구요",
          "original_form": "귤로장생 많이 사더라구요",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045068.1.13",
          "form": "오 귤로장생 참고하겠습니다!!",
          "original_form": "오 귤로장생 참고하겠습니다!!",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045068.1.14",
          "form": "먹는거 계속 추천 ㅋㅋㅋㅋㅋ",
          "original_form": "먹는거 계속 추천 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045068.1.15",
          "form": "아침은요? 드시나요",
          "original_form": "아침은요? 드시나요",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045068.1.16",
          "form": "ㅋㅋㅋ 아침은 잘 안먹게 돼요",
          "original_form": "ㅋㅋㅋ 아침은 잘 안먹게 돼요",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045068.1.17",
          "form": "먹어도 사과정도",
          "original_form": "먹어도 사과정도",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045068.1.18",
          "form": "어제 아침도 사과?",
          "original_form": "어제 아침도 사과?",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045068.1.19",
          "form": "어제는 두유먹었어요!!",
          "original_form": "어제는 두유먹었어요!!",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045068.1.20",
          "form": "오늘은 모르겠어요ㅋㅋㅋ name1님은??",
          "original_form": "오늘은 모르겠어요ㅋㅋㅋ &name1&님은??",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045068.1.21",
          "form": "전 모카빵 먹을려구요 아메랑",
          "original_form": "전 모카빵 먹을려구요 아메랑",
