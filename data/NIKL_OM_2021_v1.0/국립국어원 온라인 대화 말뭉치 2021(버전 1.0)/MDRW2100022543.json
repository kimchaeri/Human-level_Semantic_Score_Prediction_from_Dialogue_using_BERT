      "utterance": [
        {
          "id": "MDRW2100022543.1.1",
          "form": "우와 국도 휘리릭~  요리잘하실거같네용!ㅎㅎ",
          "original_form": "우와 국도 휘리릭~  요리잘하실거같네용!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022543.1.2",
          "form": "아녀유ㅠ~~",
          "original_form": "아녀유ㅠ~~",
          "speaker_id": "2",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022543.1.3",
          "form": "그냥 진짜 대강 ㅋㅋ하는 국찌개",
          "original_form": "그냥 진짜 대강 ㅋㅋ하는 국찌개",
          "speaker_id": "2",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022543.1.4",
          "form": "전 다시다 미원은 없는데 연두가 잇어요 ㅋㅋ",
          "original_form": "전 다시다 미원은 없는데 연두가 잇어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022543.1.5",
          "form": "저도 대강잘했으면 좋겠어요 ㅋㅋ",
          "original_form": "저도 대강잘했으면 좋겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022543.1.6",
          "form": "오 저도 다시다 미원 안쓰는데",
          "original_form": "오 저도 다시다 미원 안쓰는데",
          "speaker_id": "1",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022543.1.7",
          "form": "연두는 괜찮나요?ㅎ",
          "original_form": "연두는 괜찮나요?ㅎ",
          "speaker_id": "1",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100022543.1.8",
          "form": "몸괜찮는지? 모르겟지만 ㅋㅋ 끓일때 살짝씩 넣어요 ㅋㅋ",
          "original_form": "몸괜찮는지? 모르겟지만 ㅋㅋ 끓일때 살짝씩 넣어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022543.1.9",
          "form": "뭔가 먓이 부족하다싶으면 ㅋㅋ",
          "original_form": "뭔가 먓이 부족하다싶으면 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:41"
        },
        {
          "id": "MDRW2100022543.1.10",
          "form": "ㅋㅋ얼마전에 유퀴즈에 연두 나왔다고하던데요 ㅎㅎ\r\n관심가네요^^",
          "original_form": "ㅋㅋ얼마전에 유퀴즈에 연두 나왔다고하던데요 ㅎㅎ\r\n관심가네요^^",
          "speaker_id": "1",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022543.1.11",
          "form": "ㅋㅋㅋ맞아여 ㅎㅎ 연두하고 참치 햇반 회사사람들 전잼있게 봤어요",
          "original_form": "ㅋㅋㅋ맞아여 ㅎㅎ 연두하고 참치 햇반 회사사람들 전잼있게 봤어요",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022543.1.12",
          "form": "전 락스회사도 봤어요 ㅋㅋㅋ",
          "original_form": "전 락스회사도 봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022543.1.13",
          "form": "락스회사도 나왓나요 ㅋㅋㅋ",
          "original_form": "락스회사도 나왓나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022543.1.14",
          "form": "넼ㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022543.1.15",
          "form": "누가해준밥이 제일 맛있는데 ㅎㅎㅎㅎ",
          "original_form": "누가해준밥이 제일 맛있는데 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022543.1.16",
          "form": "저녁도 누가좀 해줬으면 ㅠㅠ",
          "original_form": "저녁도 누가좀 해줬으면 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022543.1.17",
          "form": "저도 저녁뭐먹을지 ㅠ",
          "original_form": "저도 저녁뭐먹을지 ㅠ",
          "speaker_id": "2",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022543.1.18",
          "form": "저희동네엔",
          "original_form": "저희동네엔",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022543.1.19",
          "form": "고민이네요ㅠㅠ",
          "original_form": "고민이네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022543.1.20",
          "form": "혼밥이신가요??",
          "original_form": "혼밥이신가요??",
          "speaker_id": "2",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022543.1.21",
          "form": "동네가아니라 제가 사는 지역이",
          "original_form": "동네가아니라 제가 사는 지역이",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022543.1.22",
          "form": "여행지라",
          "original_form": "여행지라",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022543.1.23",
          "form": "카페가 엄청 많아용!",
          "original_form": "카페가 엄청 많아용!",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022543.1.24",
          "form": "혼밥아니고 애기들과 함께에요 ㅋㅋㅋ",
          "original_form": "혼밥아니고 애기들과 함께에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022543.1.25",
          "form": "핫한곳에 사시는군요!",
          "original_form": "핫한곳에 사시는군요!",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022543.1.26",
          "form": "근데 정작 사는저는 못가봤네요ㅠㅠ",
          "original_form": "근데 정작 사는저는 못가봤네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022543.1.27",
          "form": "아 그럼 ㅠㅠ 직접하셔야하는군여ㅠㅠ 애기한테 미룰순없으니ㅠ",
          "original_form": "아 그럼 ㅠㅠ 직접하셔야하는군여ㅠㅠ 애기한테 미룰순없으니ㅠ",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022543.1.28",
          "form": "요샌 어디든 가기 힘들죠ㅠㅠ",
          "original_form": "요샌 어디든 가기 힘들죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022543.1.29",
          "form": "유명카페들 엄청 비싸요 ㅠㅠ",
          "original_form": "유명카페들 엄청 비싸요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022543.1.30",
          "form": "그렇기도하구요ㅠ 노키즈존도있고",
          "original_form": "그렇기도하구요ㅠ 노키즈존도있고",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022543.1.31",
          "form": "음료도 비싸요",
          "original_form": "음료도 비싸요",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022543.1.32",
          "form": "애들을 두고갈순없으니까욬ㅋㅋ",
          "original_form": "애들을 두고갈순없으니까욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022543.1.33",
          "form": "맞아요 애기도 1인1음료 포함이구요",
          "original_form": "맞아요 애기도 1인1음료 포함이구요",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022543.1.34",
          "form": "관광지면 더 심할듯하네여",
          "original_form": "관광지면 더 심할듯하네여",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022543.1.35",
          "form": "아기도요?",
          "original_form": "아기도요?",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022543.1.36",
          "form": "안그래도 먐카페에서",
          "original_form": "안그래도 먐카페에서",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022543.1.37",
          "form": "1인1음료네 나이 기준에대한 글이있었는데",
          "original_form": "1인1음료네 나이 기준에대한 글이있었는데",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022543.1.38",
          "form": "오 그런글이 있었나요",
          "original_form": "오 그런글이 있었나요",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022543.1.39",
          "form": "전 10살이후라고 생각햇어요 근데 갓덩카페에서 5세아이도 1인으로",
          "original_form": "전 10살이후라고 생각햇어요 근데 갓덩카페에서 5세아이도 1인으로",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022543.1.40",
          "form": "그쵸 그게 문제같아요.ㅠㅠ",
          "original_form": "그쵸 그게 문제같아요.ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022543.1.41",
          "form": "주문해야한다했데요 또 댓글에 1세부터라는 곳도있었데요..",
          "original_form": "주문해야한다했데요 또 댓글에 1세부터라는 곳도있었데요..",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022543.1.42",
          "form": "미취학아이에게 1인이라고.ㅠㅠ",
          "original_form": "미취학아이에게 1인이라고.ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022543.1.43",
          "form": "헐 1세.ㅠㅠ",
          "original_form": "헐 1세.ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022543.1.44",
          "form": "거기서에서 파는 사과주스엿나 뭐가 3500원 그거라도 시켜야한다고..",
          "original_form": "거기서에서 파는 사과주스엿나 뭐가 3500원 그거라도 시켜야한다고..",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022543.1.45",
          "form": "너무하네요",
          "original_form": "너무하네요",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022543.1.46",
          "form": "구니깐요",
          "original_form": "구니깐요",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022543.1.47",
          "form": "참 팍팍한세상이다싶엇어요;;;",
          "original_form": "참 팍팍한세상이다싶엇어요;;;",
