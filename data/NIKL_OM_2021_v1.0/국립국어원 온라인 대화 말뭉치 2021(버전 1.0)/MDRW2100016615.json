      "utterance": [
        {
          "id": "MDRW2100016615.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016615.1.2",
          "form": "혹시 혼자 사시나요?",
          "original_form": "혹시 혼자 사시나요?",
          "speaker_id": "1",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016615.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016615.1.4",
          "form": "부모님하고 같이 살아요",
          "original_form": "부모님하고 같이 살아요",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016615.1.5",
          "form": "저는 얼마전에 동생이랑 독립했어요~",
          "original_form": "저는 얼마전에 동생이랑 독립했어요~",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016615.1.6",
          "form": "부모님집에 살때가 좋았는데ㅜ",
          "original_form": "부모님집에 살때가 좋았는데ㅜ",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016615.1.7",
          "form": "와 그래도 독립하면 색다르고 재밌을 것 같은데요 ㅋㅋㅋ",
          "original_form": "와 그래도 독립하면 색다르고 재밌을 것 같은데요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016615.1.8",
          "form": "동생 원격수업때문에 반강제로 독립한거라ㅜㅜ",
          "original_form": "동생 원격수업때문에 반강제로 독립한거라ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016615.1.9",
          "form": "가족들이랑 살면 뭔가 집에 온기가느껴지는데",
          "original_form": "가족들이랑 살면 뭔가 집에 온기가느껴지는데",
          "speaker_id": "1",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016615.1.10",
          "form": "원룸으로 오니까.....휑..",
          "original_form": "원룸으로 오니까.....휑..",
          "speaker_id": "1",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016615.1.11",
          "form": "아 ㅠㅠ 빈자리가 느껴지겠네요",
          "original_form": "아 ㅠㅠ 빈자리가 느껴지겠네요",
          "speaker_id": "2",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016615.1.12",
          "form": "동생이랑 살고있긴하지만 막상 몇달 나와살다보니까",
          "original_form": "동생이랑 살고있긴하지만 막상 몇달 나와살다보니까",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016615.1.13",
          "form": "다시 가족들이랑 살려니까 막막하기도해요...ㅋㅋㅋ그리우면서도 자유가 끝난다는 생각에..",
          "original_form": "다시 가족들이랑 살려니까 막막하기도해요...ㅋㅋㅋ그리우면서도 자유가 끝난다는 생각에..",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016615.1.14",
          "form": "이모저모 장단점이 다 있네요 ㅋㅋㅋㅋ",
          "original_form": "이모저모 장단점이 다 있네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016615.1.15",
          "form": "독립하면 진짜 할일이 많아지더라고요ㅋㅋ",
          "original_form": "독립하면 진짜 할일이 많아지더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016615.1.16",
          "form": "그럴거같아요 공과금에 집안일에 ㅠㅠ",
          "original_form": "그럴거같아요 공과금에 집안일에 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016615.1.17",
          "form": "공과금, 청소, 요리 등등..",
          "original_form": "공과금, 청소, 요리 등등..",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016615.1.18",
          "form": "손이 많이 가겠어요",
          "original_form": "손이 많이 가겠어요",
          "speaker_id": "2",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016615.1.19",
          "form": "집안일을 많이 참여하세요?",
          "original_form": "집안일을 많이 참여하세요?",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016615.1.20",
          "form": "많이는 아니고 설거지나 빨래개기 정도만요 ㅋㅋㅋ",
          "original_form": "많이는 아니고 설거지나 빨래개기 정도만요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:52"
        }
      ]
