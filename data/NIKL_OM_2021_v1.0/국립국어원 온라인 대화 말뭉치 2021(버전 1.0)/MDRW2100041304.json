      "utterance": [
        {
          "id": "MDRW2100041304.1.1",
          "form": "어떤일을 하시나요??",
          "original_form": "어떤일을 하시나요??",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100041304.1.2",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100041304.1.3",
          "form": "하....좡애in 복지쪽이에요",
          "original_form": "하....좡애in 복지쪽이에요",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041304.1.4",
          "form": "자꾸 나쁜말이라고 나오네요ㅋㅋㅋㅋㅋ",
          "original_form": "자꾸 나쁜말이라고 나오네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041304.1.5",
          "form": "아 ㅎㅎㅎ 얘가 말을 좀 그렇게 알아듣죠?? ㅎㅎ",
          "original_form": "아 ㅎㅎㅎ 얘가 말을 좀 그렇게 알아듣죠?? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100041304.1.6",
          "form": "어이쿠 힘드시겠어요",
          "original_form": "어이쿠 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041304.1.7",
          "form": "복지관련은 힘드시지않나요??",
          "original_form": "복지관련은 힘드시지않나요??",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041304.1.8",
          "form": "네 굉장히 힘들어요",
          "original_form": "네 굉장히 힘들어요",
          "speaker_id": "2",
          "time": "20210906 08:32"
        },
        {
          "id": "MDRW2100041304.1.9",
          "form": "몸이 골병나요",
          "original_form": "몸이 골병나요",
          "speaker_id": "2",
          "time": "20210906 08:33"
        },
        {
          "id": "MDRW2100041304.1.10",
          "form": "정신적으로 스트레스도 있지만 직원들땜에 스트레스가 많음",
          "original_form": "정신적으로 스트레스도 있지만 직원들땜에 스트레스가 많음",
          "speaker_id": "2",
          "time": "20210906 08:33"
        },
        {
          "id": "MDRW2100041304.1.11",
          "form": "어후 ㅠ 말만 들어도 힘드시겠어요",
          "original_form": "어후 ㅠ 말만 들어도 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210906 10:07"
        },
        {
          "id": "MDRW2100041304.1.12",
          "form": "직원들도 별로 이신가봐요 ㅠ",
          "original_form": "직원들도 별로 이신가봐요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 10:07"
        },
        {
          "id": "MDRW2100041304.1.13",
          "form": "넹....",
          "original_form": "넹....",
          "speaker_id": "2",
          "time": "20210906 11:07"
        },
        {
          "id": "MDRW2100041304.1.14",
          "form": "상사..,도 도라이고",
          "original_form": "상사..,도 도라이고",
          "speaker_id": "2",
          "time": "20210906 11:07"
        },
        {
          "id": "MDRW2100041304.1.15",
          "form": "회사마다 도라이는 한두명 있다는데 여긴 집단으로 있어서 ㅎ 저도 돌아버린",
          "original_form": "회사마다 도라이는 한두명 있다는데 여긴 집단으로 있어서 ㅎ 저도 돌아버린",
          "speaker_id": "2",
          "time": "20210906 11:07"
        },
        {
          "id": "MDRW2100041304.1.16",
          "form": "아하.....",
          "original_form": "아하.....",
          "speaker_id": "1",
          "time": "20210906 11:08"
        },
        {
          "id": "MDRW2100041304.1.17",
          "form": "안그래도 복지면 진짜 힘드시겠는데",
          "original_form": "안그래도 복지면 진짜 힘드시겠는데",
          "speaker_id": "1",
          "time": "20210906 11:08"
        },
        {
          "id": "MDRW2100041304.1.18",
          "form": "상사까지 도.라.이면 진짜...",
          "original_form": "상사까지 도.라.이면 진짜...",
          "speaker_id": "1",
          "time": "20210906 11:08"
        },
        {
          "id": "MDRW2100041304.1.19",
          "form": "집단.도.라.이 라니.... ㅠ",
          "original_form": "집단.도.라.이 라니.... ㅠ",
          "speaker_id": "1",
          "time": "20210906 11:08"
        },
        {
          "id": "MDRW2100041304.1.20",
          "form": "ㅠ... 일하는거는 이제 어느정도 익숙해졌는데",
          "original_form": "ㅠ... 일하는거는 이제 어느정도 익숙해졌는데",
          "speaker_id": "2",
          "time": "20210906 11:09"
        },
        {
          "id": "MDRW2100041304.1.21",
          "form": "하..... 진짜 직원들... ",
          "original_form": "하..... 진짜 직원들... ",
          "speaker_id": "2",
          "time": "20210906 11:09"
        },
        {
          "id": "MDRW2100041304.1.22",
          "form": "직원들이 더 힘들죠",
          "original_form": "직원들이 더 힘들죠",
          "speaker_id": "1",
          "time": "20210906 11:22"
        },
        {
          "id": "MDRW2100041304.1.23",
          "form": "전 서비스업인데",
          "original_form": "전 서비스업인데",
          "speaker_id": "1",
          "time": "20210906 11:22"
        },
        {
          "id": "MDRW2100041304.1.24",
          "form": "진.상.사람들 정말 많지만",
          "original_form": "진.상.사람들 정말 많지만",
          "speaker_id": "1",
          "time": "20210906 11:22"
        },
        {
          "id": "MDRW2100041304.1.25",
          "form": "그래도 직원들이 좋아서 할만하거든요",
          "original_form": "그래도 직원들이 좋아서 할만하거든요",
          "speaker_id": "1",
          "time": "20210906 11:22"
        },
        {
          "id": "MDRW2100041304.1.26",
          "form": "아ㅜ 서비스업",
          "original_form": "아ㅜ 서비스업",
          "speaker_id": "2",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100041304.1.27",
          "form": "진상...들 많죠",
          "original_form": "진상...들 많죠",
          "speaker_id": "2",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100041304.1.28",
          "form": "증말 진상들",
          "original_form": "증말 진상들",
          "speaker_id": "2",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100041304.1.29",
          "form": "직원이라도 좋아서 다행이네요",
          "original_form": "직원이라도 좋아서 다행이네요",
          "speaker_id": "2",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100041304.1.30",
          "form": "네\r\n그나마 다행이예요",
          "original_form": "네\r\n그나마 다행이예요",
          "speaker_id": "1",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041304.1.31",
          "form": "같이 욕. 할수도 있고",
          "original_form": "같이 욕. 할수도 있고",
          "speaker_id": "1",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041304.1.32",
          "form": "맞아요 그게 은근 버팀목이더라고요",
          "original_form": "맞아요 그게 은근 버팀목이더라고요",
          "speaker_id": "2",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100041304.1.33",
          "form": "맘맞는 직원이 있으면 좋은거같아여",
          "original_form": "맘맞는 직원이 있으면 좋은거같아여",
          "speaker_id": "2",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100041304.1.34",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100041304.1.35",
          "form": "저도 진.상 사람들 전화받고 마주할때만 진짜",
          "original_form": "저도 진.상 사람들 전화받고 마주할때만 진짜",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100041304.1.36",
          "form": "기가 다 빨리는거 같거든요",
          "original_form": "기가 다 빨리는거 같거든요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100041304.1.37",
          "form": "말도 안통하고",
          "original_form": "말도 안통하고",
          "speaker_id": "1",
          "time": "20210906 21:29"
        }
      ]
    }
