      "utterance": [
        {
          "id": "MDRW2100015382.1.1",
          "form": "운동하는거 좋아하시나여?",
          "original_form": "운동하는거 좋아하시나여?",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015382.1.2",
          "form": "싫어하지는 않는데 ... 좋아하는 운동도 있는데요 음",
          "original_form": "싫어하지는 않는데 ... 좋아하는 운동도 있는데요 음",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015382.1.3",
          "form": "의지가 약해서 꾸준히는 못하는거 같아요 ㅜ.ㅜ",
          "original_form": "의지가 약해서 꾸준히는 못하는거 같아요 ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015382.1.4",
          "form": "뭐 해본거 있어요?",
          "original_form": "뭐 해본거 있어요?",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015382.1.5",
          "form": "주로 집에서 홈트하는데",
          "original_form": "주로 집에서 홈트하는데",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015382.1.6",
          "form": "작년에는 꾸준히 해서 다이어트도 성공했었는데",
          "original_form": "작년에는 꾸준히 해서 다이어트도 성공했었는데",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015382.1.7",
          "form": "올해는 영 꾸준히 못하네요  ( o̴̶̷᷄o̴̶̷̥᷅ )...",
          "original_form": "올해는 영 꾸준히 못하네요  ( o̴̶̷᷄{emoji:⌓}o̴̶̷̥᷅ )...",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015382.1.8",
          "form": "홈트는 강제성이 없어서 진짜 힘들죠..",
          "original_form": "홈트는 강제성이 없어서 진짜 힘들죠..",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015382.1.9",
          "form": "작년에 빼고 유지가 안됫나바요?",
          "original_form": "작년에 빼고 유지가 안됫나바요?",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015382.1.10",
          "form": "14키로 정도 빼고 2키로 정도 다시 찐듯요. . .ㅎ",
          "original_form": "14키로 정도 빼고 2키로 정도 다시 찐듯요. . .ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015382.1.11",
          "form": "헬스장 나가고 싶은데 코로나땜에 한번두 못갔어요 ㅜㅜ",
          "original_form": "헬스장 나가고 싶은데 코로나땜에 한번두 못갔어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015382.1.12",
          "form": "14키로요? 와... 어케 빼신거에요? \r\n식단?기간은요?",
          "original_form": "14키로요? 와... 어케 빼신거에요? \r\n식단?기간은요?",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015382.1.13",
          "form": "12월부터 3월까지 4달정도 했구용!!",
          "original_form": "12월부터 3월까지 4달정도 했구용!!",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015382.1.14",
          "form": "식단은 아침 저녁은 가볍게(샐러드?) 먹구 점심은 일반식..?",
          "original_form": "식단은 아침 저녁은 가볍게(샐러드?) 먹구 점심은 일반식..?",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100015382.1.15",
          "form": "근데 외식두 거의 안했구 (치팅데이 빼구요!!)",
          "original_form": "근데 외식두 거의 안했구 (치팅데이 빼구요!!)",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015382.1.16",
          "form": "와.. 전 3키로 빼는거도 힘든데..",
          "original_form": "와.. 전 3키로 빼는거도 힘든데..",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015382.1.17",
          "form": "과자두 끊었구...",
          "original_form": "과자두 끊었구...",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015382.1.18",
          "form": "근력운동 위주로요?",
          "original_form": "근력운동 위주로요?",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015382.1.19",
          "form": "그땐 독하게 했어요 ㅎㅎ",
          "original_form": "그땐 독하게 했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015382.1.20",
          "form": "유산소 운동 위주로 한거같아요",
          "original_form": "유산소 운동 위주로 한거같아요",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015382.1.21",
          "form": "근데 그때는 학업스트레스로 살이 많이 쪘었던지라",
          "original_form": "근데 그때는 학업스트레스로 살이 많이 쪘었던지라",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015382.1.22",
          "form": "비교적 쉽게 빠지더라구요...",
          "original_form": "비교적 쉽게 빠지더라구요...",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015382.1.23",
          "form": "원래 날씬하다가 그때만 쪘었던 거에요?",
          "original_form": "원래 날씬하다가 그때만 쪘었던 거에요?",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015382.1.24",
          "form": "원래는 정상이었어요!!  한번도 날씬한적 없었어요 젠장ㅜㅜ",
          "original_form": "원래는 정상이었어요!!  한번도 날씬한적 없었어요 젠장ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015382.1.25",
          "form": "전 보이는데만 날씬한데.. \r\n속살이 무지 많아서..",
          "original_form": "전 보이는데만 날씬한데.. \r\n속살이 무지 많아서..",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015382.1.26",
          "form": "뱃살이 안빠져요",
          "original_form": "뱃살이 안빠져요",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015382.1.27",
          "form": "저도 뱃살 때문에 고민많은데",
          "original_form": "저도 뱃살 때문에 고민많은데",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015382.1.28",
          "form": "땅끄 11자복근 핵매운맛 영상이 효과 좋다구해서",
          "original_form": "땅끄 11자복근 핵매운맛 영상이 효과 좋다구해서",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015382.1.29",
          "form": "앞으로 하려구용!!",
          "original_form": "앞으로 하려구용!!",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015382.1.30",
          "form": "전 애기때 비만이었다가 빠진거라... \r\n뱃살은 그냥 포기..ㅋㅋㅋ",
          "original_form": "전 애기때 비만이었다가 빠진거라... \r\n뱃살은 그냥 포기..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015382.1.31",
          "form": "땅크 핵힘들어보이던데",
          "original_form": "땅크 핵힘들어보이던데",
          "speaker_id": "1",
          "time": "20210825 23:14"
        }
