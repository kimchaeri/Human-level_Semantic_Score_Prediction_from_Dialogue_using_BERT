      "utterance": [
        {
          "id": "MDRW2100024771.1.1",
          "form": "오늘 내가 저녁밥으로 뭐 먹게?",
          "original_form": "오늘 내가 저녁밥으로 뭐 먹게?",
          "speaker_id": "1",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100024771.1.2",
          "form": "나 오늘 저녁은 외식하려고",
          "original_form": "나 오늘 저녁은 외식하려고",
          "speaker_id": "2",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100024771.1.3",
          "form": "글쎄 마님이 밥사달래",
          "original_form": "글쎄 마님이 밥사달래",
          "speaker_id": "2",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100024771.1.4",
          "form": "난 집에서 만들어 먹을 것이야",
          "original_form": "난 집에서 만들어 먹을 것이야",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100024771.1.5",
          "form": "맛나게 먹고 왔지롱 너도 외식 다녀왔니?",
          "original_form": "맛나게 먹고 왔지롱 너도 외식 다녀왔니?",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100024771.1.6",
          "form": "오늘 맘스터치 시켰어",
          "original_form": "오늘 맘스터치 시켰어",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100024771.1.7",
          "form": "동생것도ㅈ겸사",
          "original_form": "동생것도ㅈ겸사",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100024771.1.8",
          "form": "맘터 맛나겠다 햄버거 뭐 시켰네",
          "original_form": "맘터 맛나겠다 햄버거 뭐 시켰네",
          "speaker_id": "1",
          "time": "20210901 19:35"
        },
        {
          "id": "MDRW2100024771.1.9",
          "form": "니",
          "original_form": "니",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100024771.1.10",
          "form": "난무조건 싸이버거세트",
          "original_form": "난무조건 싸이버거세트",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100024771.1.11",
          "form": "엄마는 새우불고기버거",
          "original_form": "엄마는 새우불고기버거",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100024771.1.12",
          "form": "동생은 양념치킨버거",
          "original_form": "동생은 양념치킨버거",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100024771.1.13",
          "form": "나도 싸이버거파인데 통했구만 기래",
          "original_form": "나도 싸이버거파인데 통했구만 기래",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100024771.1.14",
          "form": "새우불고기같은 것도 있구나 닭종류만 있는 줄",
          "original_form": "새우불고기같은 것도 있구나 닭종류만 있는 줄",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100024771.1.15",
          "form": "짱좋아하더라",
          "original_form": "짱좋아하더라",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100024771.1.16",
          "form": "불고기랑 새우들어간다고",
          "original_form": "불고기랑 새우들어간다고",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100024771.1.17",
          "form": "ㅋㅋㅌㅋ맛있는 게 모였으니 두 배로 맛있나봐",
          "original_form": "ㅋㅋㅌㅋ맛있는 게 모였으니 두 배로 맛있나봐",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100024771.1.18",
          "form": "거기서 파는 화이트갈릭버거도 먹어봤니",
          "original_form": "거기서 파는 화이트갈릭버거도 먹어봤니",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100024771.1.19",
          "form": "아니 퍽퍽살버거는",
          "original_form": "아니 퍽퍽살버거는",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100024771.1.20",
          "form": "일부러 피해다녀서",
          "original_form": "일부러 피해다녀서",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100024771.1.21",
          "form": "이럴 수가 퍽퍽살의 멋짐을 몰라주다니 슬프구나",
          "original_form": "이럴 수가 퍽퍽살의 멋짐을 몰라주다니 슬프구나",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100024771.1.22",
          "form": "근데 화이트갈릭버거에는 양상추가 안 들어가서",
          "original_form": "근데 화이트갈릭버거에는 양상추가 안 들어가서",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100024771.1.23",
          "form": "퍽퍽살은",
          "original_form": "퍽퍽살은",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100024771.1.24",
          "form": "마음이 퍽퍽해",
          "original_form": "마음이 퍽퍽해",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100024771.1.25",
          "form": "아쉽더라",
          "original_form": "아쉽더라",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100024771.1.26",
          "form": "아 그럴 수 있지",
          "original_form": "아 그럴 수 있지",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100024771.1.27",
          "form": "촉촉한 마음을 유지하도록 해",
          "original_form": "촉촉한 마음을 유지하도록 해",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100024771.1.28",
          "form": "나는 건조한 사람이 될게",
          "original_form": "나는 건조한 사람이 될게",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100024771.1.29",
          "form": "난 밥먹고올게",
          "original_form": "난 밥먹고올게",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100024771.1.30",
          "form": "아직 안 먹었니",
          "original_form": "아직 안 먹었니",
          "speaker_id": "1",
          "time": "20210901 19:47"
        },
        {
          "id": "MDRW2100024771.1.31",
          "form": "밥먹고왔다",
          "original_form": "밥먹고왔다",
          "speaker_id": "2",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100024771.1.32",
          "form": "응 배달이 늦더라",
          "original_form": "응 배달이 늦더라",
          "speaker_id": "2",
          "time": "20210901 20:10"
        }
      ]
    }
