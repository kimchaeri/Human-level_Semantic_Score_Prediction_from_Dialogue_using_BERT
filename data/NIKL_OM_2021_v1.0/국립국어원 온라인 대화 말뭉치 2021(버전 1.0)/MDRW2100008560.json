        {
          "id": "MDRW2100008560.1.1",
          "form": "디저트는 마카롱 제일 좋아하시나요?",
          "original_form": "디저트는 마카롱 제일 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008560.1.2",
          "form": "name2은 그냥 쓴거라ㅎㅎ",
          "original_form": "&name2&은 그냥 쓴거라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008560.1.3",
          "form": "커피좋아해요",
          "original_form": "커피좋아해요",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008560.1.4",
          "form": "뭐좋아해요?",
          "original_form": "뭐좋아해요?",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100008560.1.5",
          "form": "그렇군요ㅎㅎ",
          "original_form": "그렇군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008560.1.6",
          "form": "저는 마카롱도 그렇고 케이크나 다쿠아즈 등등",
          "original_form": "저는 마카롱도 그렇고 케이크나 다쿠아즈 등등",
          "speaker_id": "1",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008560.1.7",
          "form": "달달한 디저트 다 좋아햐요",
          "original_form": "달달한 디저트 다 좋아햐요",
          "speaker_id": "1",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100008560.1.8",
          "form": "밀크크레이프는 좋아해요",
          "original_form": "밀크크레이프는 좋아해요",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008560.1.9",
          "form": "치즈케익도",
          "original_form": "치즈케익도",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008560.1.10",
          "form": "우오ㅠ 맛있겠어요",
          "original_form": "우오ㅠ 맛있겠어요",
          "speaker_id": "1",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008560.1.11",
          "form": "커피빈 크레이프가 맛있어요",
          "original_form": "커피빈 크레이프가 맛있어요",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008560.1.12",
          "form": "커피는 안좋아하세요?",
          "original_form": "커피는 안좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008560.1.13",
          "form": "달달한 커피는 조금 먹긴 하는데",
          "original_form": "달달한 커피는 조금 먹긴 하는데",
          "speaker_id": "1",
          "time": "20210822 16:46"
        },
        {
          "id": "MDRW2100008560.1.14",
          "form": "아직 아메리카노는 너무 써요ㅜ",
          "original_form": "아직 아메리카노는 너무 써요ㅜ",
          "speaker_id": "1",
          "time": "20210822 16:46"
        },
        {
          "id": "MDRW2100008560.1.15",
          "form": "저도 아메리카노는 안먹어요ㅎㅎ",
          "original_form": "저도 아메리카노는 안먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008560.1.16",
          "form": "달달커피로만",
          "original_form": "달달커피로만",
          "speaker_id": "2",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008560.1.17",
          "form": "커피빈 크레이프 메모...",
          "original_form": "커피빈 크레이프 메모...",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008560.1.18",
          "form": "전 헤이즐넛 제일 좋아해요",
          "original_form": "전 헤이즐넛 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008560.1.19",
          "form": "어떤 거 제일 좋아흐세용??",
          "original_form": "어떤 거 제일 좋아흐세용??",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008560.1.20",
          "form": "전 요즘 스벅 블론드 바닐라더블샷 마끼아또요ㅎ",
          "original_form": "전 요즘 스벅 블론드 바닐라더블샷 마끼아또요ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008560.1.21",
          "form": "이름이 넘길죠ㅋ",
          "original_form": "이름이 넘길죠ㅋ",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008560.1.22",
          "form": "우왘ㅋㅋㅋ 길긴 하네요",
          "original_form": "우왘ㅋㅋㅋ 길긴 하네요",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008560.1.23",
          "form": "그니까요 외우기도 힘들어요",
          "original_form": "그니까요 외우기도 힘들어요",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008560.1.24",
          "form": "밥먹었더니 커피마시고 싶네요",
          "original_form": "밥먹었더니 커피마시고 싶네요",
          "speaker_id": "2",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008560.1.25",
          "form": "저도요ㅋㅋㅋㅋㅋ",
