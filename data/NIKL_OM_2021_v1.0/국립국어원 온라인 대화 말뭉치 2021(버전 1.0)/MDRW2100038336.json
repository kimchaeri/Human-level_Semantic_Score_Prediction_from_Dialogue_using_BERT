      "utterance": [
        {
          "id": "MDRW2100038336.1.1",
          "form": "얼른가을이오면 좋겠어요",
          "original_form": "얼른가을이오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100038336.1.2",
          "form": "오전은 그래도 가을같아요!",
          "original_form": "오전은 그래도 가을같아요!",
          "speaker_id": "1",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100038336.1.3",
          "form": "하늘도 이쁘네요",
          "original_form": "하늘도 이쁘네요",
          "speaker_id": "1",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100038336.1.4",
          "form": "저희는비와서 ㅠㅠ",
          "original_form": "저희는비와서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100038336.1.5",
          "form": "헉 거기는 오늘도 비가오나요ㅠㅠ",
          "original_form": "헉 거기는 오늘도 비가오나요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100038336.1.6",
          "form": "지금은 또 안오더라구여 ㅠ",
          "original_form": "지금은 또 안오더라구여 ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100038336.1.7",
          "form": "기상청믿을게못되요",
          "original_form": "기상청믿을게못되요",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100038336.1.8",
          "form": "우리나라도 스콜성 비가 너무자주 와서 예측자체가 불가능한거 같아요",
          "original_form": "우리나라도 스콜성 비가 너무자주 와서 예측자체가 불가능한거 같아요",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100038336.1.9",
          "form": "진짜 갑자기 비가 너무 심해요 ㅠㅠ",
          "original_form": "진짜 갑자기 비가 너무 심해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100038336.1.10",
          "form": "마른하늘에 비도 많이보지 않나요?",
          "original_form": "마른하늘에 비도 많이보지 않나요?",
          "speaker_id": "1",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100038336.1.11",
          "form": "이번여름에 자주봤어요",
          "original_form": "이번여름에 자주봤어요",
          "speaker_id": "1",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100038336.1.12",
          "form": "아진짜요??",
          "original_form": "아진짜요??",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100038336.1.13",
          "form": "하늘을 잘안쳐다보니..",
          "original_form": "하늘을 잘안쳐다보니..",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100038336.1.14",
          "form": "햇빛 쨍쨍한데 한 십분잠깐 비오고 하더라구요",
          "original_form": "햇빛 쨍쨍한데 한 십분잠깐 비오고 하더라구요",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100038336.1.15",
          "form": "ㅋㅋㅋㅋ 비그망 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 비그망 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038336.1.16",
          "form": "날씨는 그래도 안더운거 같은데",
          "original_form": "날씨는 그래도 안더운거 같은데",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100038336.1.17",
          "form": "거긴어때요?",
          "original_form": "거긴어때요?",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100038336.1.18",
          "form": "덥고 습해요 ㅜㅠ",
          "original_form": "덥고 습해요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100038336.1.19",
          "form": "고생이시네요ㅠㅠ",
          "original_form": "고생이시네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 12:24"
        }
