      "utterance": [
        {
          "id": "MDRW2100019403.1.1",
          "form": "네 ㅋㅋㅋ 더 앨리라는 카페 브랜드인데",
          "original_form": "네 ㅋㅋㅋ 더 앨리라는 카페 브랜드인데",
          "speaker_id": "2",
          "time": "20210827 18:50"
        },
        {
          "id": "MDRW2100019403.1.2",
          "form": "공차 비슷한.... 아세요? 거기 있더라구요",
          "original_form": "공차 비슷한.... 아세요? 거기 있더라구요",
          "speaker_id": "2",
          "time": "20210827 18:50"
        },
        {
          "id": "MDRW2100019403.1.3",
          "form": "처음 들어봐요",
          "original_form": "처음 들어봐요",
          "speaker_id": "1",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019403.1.4",
          "form": "거기도 버블티로 유명한가요??",
          "original_form": "거기도 버블티로 유명한가요??",
          "speaker_id": "1",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019403.1.5",
          "form": "네!! 버블티가 주력 메뉴 같아요 ㅋㅋ",
          "original_form": "네!! 버블티가 주력 메뉴 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019403.1.6",
          "form": "크림브륄레 마카롱 정말 맛있으셨겠어요....",
          "original_form": "크림브륄레 마카롱 정말 맛있으셨겠어요....",
          "speaker_id": "2",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019403.1.7",
          "form": "버블티 처음 먹었을 때가 생각나네요 ㅋㅋㅋ",
          "original_form": "버블티 처음 먹었을 때가 생각나네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019403.1.8",
          "form": "네 ㅠㅠ 마카롱 그저그렇다 생각했는데 신세계였어요 쫜득쫜득...",
          "original_form": "네 ㅠㅠ 마카롱 그저그렇다 생각했는데 신세계였어요 쫜득쫜득...",
          "speaker_id": "1",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019403.1.9",
          "form": "버블티 언제 처음 드셔보셨나요?",
          "original_form": "버블티 언제 처음 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210827 18:51"
        },
        {
          "id": "MDRW2100019403.1.10",
          "form": "전 그제 먹은 마카롱이 실패였어요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 그제 먹은 마카롱이 실패였어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:52"
        },
        {
          "id": "MDRW2100019403.1.11",
          "form": "음 저는 5년..? 넘은거같아요",
          "original_form": "음 저는 5년..? 넘은거같아요",
          "speaker_id": "1",
          "time": "20210827 18:52"
        },
        {
          "id": "MDRW2100019403.1.12",
          "form": "무슨맛 드셨는데요??",
          "original_form": "무슨맛 드셨는데요??",
          "speaker_id": "1",
          "time": "20210827 18:52"
        },
        {
          "id": "MDRW2100019403.1.13",
          "form": "그냥 평범한 마카롱이었는데 쫀득한게 아니라 질겨서 놀랬어요ㅠㅠㅋㅋ",
          "original_form": "그냥 평범한 마카롱이었는데 쫀득한게 아니라 질겨서 놀랬어요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:53"
        },
        {
          "id": "MDRW2100019403.1.14",
          "form": "버블티 유행 끝나서 갈줄 알았더니 꾸준한게 신기하네요!",
          "original_form": "버블티 유행 끝나서 갈줄 알았더니 꾸준한게 신기하네요!",
          "speaker_id": "2",
          "time": "20210827 18:53"
        },
        {
          "id": "MDRW2100019403.1.15",
          "form": "버블티도 좋아하시나요?",
          "original_form": "버블티도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 18:53"
        },
        {
          "id": "MDRW2100019403.1.16",
          "form": "질기다니..ㅠㅠ",
          "original_form": "질기다니..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 18:53"
        },
        {
          "id": "MDRW2100019403.1.17",
          "form": "저는 버블티보다는 공차에서 알로에가 맛있더라구요",
          "original_form": "저는 버블티보다는 공차에서 알로에가 맛있더라구요",
          "speaker_id": "1",
          "time": "20210827 18:53"
        },
        {
          "id": "MDRW2100019403.1.18",
          "form": "공차 알로에요? 헉 저도 그거 맛있다고 생각했어욬ㅋㅋㅋ",
          "original_form": "공차 알로에요? 헉 저도 그거 맛있다고 생각했어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:53"
        },
        {
          "id": "MDRW2100019403.1.19",
          "form": "오호 ㅋㅋㅋ",
          "original_form": "오호 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:54"
        },
        {
          "id": "MDRW2100019403.1.20",
          "form": "근데 그거 잘못 먹으면 목에 걸리겠더라구요 빨리 흡입하면ㅋㅋㅋㅋ",
          "original_form": "근데 그거 잘못 먹으면 목에 걸리겠더라구요 빨리 흡입하면ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:54"
        },
        {
          "id": "MDRW2100019403.1.21",
          "form": "어떤 음료로 드세요?",
          "original_form": "어떤 음료로 드세요?",
          "speaker_id": "1",
          "time": "20210827 18:54"
        },
        {
          "id": "MDRW2100019403.1.22",
          "form": "악 그러겠네요 진짜 ㅋㅋㅋㅋ",
          "original_form": "악 그러겠네요 진짜 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:54"
        },
        {
          "id": "MDRW2100019403.1.23",
          "form": "최근에 먹은 조합이...무슨 인터넷에서 맛있는 조합 이거였는데 까먹었네요ㅠㅠㅋㅋㅋ",
          "original_form": "최근에 먹은 조합이...무슨 인터넷에서 맛있는 조합 이거였는데 까먹었네요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:54"
        },
        {
          "id": "MDRW2100019403.1.24",
          "form": "아 초당옥수수 맛도 마셔봤어요",
          "original_form": "아 초당옥수수 맛도 마셔봤어요",
          "speaker_id": "2",
          "time": "20210827 18:54"
        },
        {
          "id": "MDRW2100019403.1.25",
          "form": "드셔보셨나요 ㅋㅋ",
          "original_form": "드셔보셨나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:54"
        },
        {
          "id": "MDRW2100019403.1.26",
          "form": "공차에서요???",
          "original_form": "공차에서요???",
          "speaker_id": "1",
          "time": "20210827 18:55"
        },
        {
          "id": "MDRW2100019403.1.27",
          "form": "와 별게다이쎄요",
          "original_form": "와 별게다이쎄요",
          "speaker_id": "1",
          "time": "20210827 18:55"
        },
        {
          "id": "MDRW2100019403.1.28",
          "form": "있네요 ㅋㅋㅋㅋ",
          "original_form": "있네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:55"
        },
        {
          "id": "MDRW2100019403.1.29",
          "form": "넼ㅋㅋ 새로 나왔어요 한번 드셔보세요",
          "original_form": "넼ㅋㅋ 새로 나왔어요 한번 드셔보세요",
          "speaker_id": "2",
          "time": "20210827 18:55"
        },
        {
          "id": "MDRW2100019403.1.30",
          "form": "전 초당옥수수 좋아해서 시도해봤는데",
          "original_form": "전 초당옥수수 좋아해서 시도해봤는데",
          "speaker_id": "2",
          "time": "20210827 18:55"
        },
        {
          "id": "MDRW2100019403.1.31",
          "form": "차갑고 달달한 콘스프 맛이 났어요 ㅋㅋㅋ",
          "original_form": "차갑고 달달한 콘스프 맛이 났어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:55"
        },
        {
          "id": "MDRW2100019403.1.32",
          "form": "신기하네요 ㅋㅋㅋㅋ",
          "original_form": "신기하네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:55"
        },
        {
