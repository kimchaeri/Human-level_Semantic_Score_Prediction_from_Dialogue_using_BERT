      "utterance": [
        {
          "id": "MDRW2100009060.1.1",
          "form": "우리는 죄 없어요ㅜ",
          "original_form": "우리는 죄 없어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:54"
        },
        {
          "id": "MDRW2100009060.1.2",
          "form": "내가 살찐건 다 코로나 때문ㅜ",
          "original_form": "내가 살찐건 다 코로나 때문ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:54"
        },
        {
          "id": "MDRW2100009060.1.3",
          "form": "그래도 맛집도 배달오니 좋네요",
          "original_form": "그래도 맛집도 배달오니 좋네요",
          "speaker_id": "2",
          "time": "20210822 20:54"
        },
        {
          "id": "MDRW2100009060.1.4",
          "form": "주로 뭐 시켜드세요?",
          "original_form": "주로 뭐 시켜드세요?",
          "speaker_id": "2",
          "time": "20210822 20:55"
        },
        {
          "id": "MDRW2100009060.1.5",
          "form": "저는 그래도 치킨을 제일 많이 먹는거 같고 그다음이 피자나 짜장면이요!",
          "original_form": "저는 그래도 치킨을 제일 많이 먹는거 같고 그다음이 피자나 짜장면이요!",
          "speaker_id": "1",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009060.1.6",
          "form": "그거말거는 음식은 초밥?",
          "original_form": "그거말거는 음식은 초밥?",
          "speaker_id": "1",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009060.1.7",
          "form": "저는 치킨 회 피자 순이요",
          "original_form": "저는 치킨 회 피자 순이요",
          "speaker_id": "2",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100009060.1.8",
          "form": "혼자서 탕수육만 시켜먹는 것도 좋아해요",
          "original_form": "혼자서 탕수육만 시켜먹는 것도 좋아해요",
          "speaker_id": "2",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100009060.1.9",
          "form": "중국집에서는 자장 짬뽕보다 탕수육을 더 시켜먹은듯ㅎ",
          "original_form": "중국집에서는 자장 짬뽕보다 탕수육을 더 시켜먹은듯ㅎ",
          "speaker_id": "2",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100009060.1.10",
          "form": "ㅋㅋㅋㅋㅋ 저는 배달앱 생기고 나서 중국집에 1인메뉴 다양해져서 좋아요@",
          "original_form": "ㅋㅋㅋㅋㅋ 저는 배달앱 생기고 나서 중국집에 1인메뉴 다양해져서 좋아요@",
          "speaker_id": "1",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100009060.1.11",
          "form": "짜장짬뽕탕수육 1인세트 같은거도 있구~",
          "original_form": "짜장짬뽕탕수육 1인세트 같은거도 있구~",
          "speaker_id": "1",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100009060.1.12",
          "form": "탕짜면에 짬뽕국물만 추가해도 다 먹을수있구!",
          "original_form": "탕짜면에 짬뽕국물만 추가해도 다 먹을수있구!",
          "speaker_id": "1",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100009060.1.13",
          "form": "종류별로 먹고싶어도 중국음식은 늘 남기는경우가 많았거든요~",
          "original_form": "종류별로 먹고싶어도 중국음식은 늘 남기는경우가 많았거든요~",
          "speaker_id": "1",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100009060.1.14",
          "form": "오오. 탕수육만 주로 시켜서 몰랐어요",
          "original_form": "오오. 탕수육만 주로 시켜서 몰랐어요",
          "speaker_id": "2",
          "time": "20210822 21:03"
        },
        {
          "id": "MDRW2100009060.1.15",
          "form": "우리 동네 중국집 메뉴도 한번 들여다봐야겠어요",
          "original_form": "우리 동네 중국집 메뉴도 한번 들여다봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 21:04"
        },
        {
          "id": "MDRW2100009060.1.16",
          "form": "탕짜면에 짬뽕국물이라니 완전 드림팀",
          "original_form": "탕짜면에 짬뽕국물이라니 완전 드림팀",
          "speaker_id": "2",
          "time": "20210822 21:04"
        },
        {
          "id": "MDRW2100009060.1.17",
          "form": "그리고 중국집은 무료배달인 곳이 꽤 있더라구요!",
          "original_form": "그리고 중국집은 무료배달인 곳이 꽤 있더라구요!",
          "speaker_id": "1",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009060.1.18",
          "form": "원래 배달하던 음식점이라서 그런지 ㅎㅎ 배달앱 생기고나서 중국집은 오히려 안좋을것 같은...",
          "original_form": "원래 배달하던 음식점이라서 그런지 ㅎㅎ 배달앱 생기고나서 중국집은 오히려 안좋을것 같은...",
          "speaker_id": "1",
          "time": "20210822 21:11"
        },
        {
          "id": "MDRW2100009060.1.19",
          "form": "배달도 그냥해주고 주문이벤트도 하면..",
          "original_form": "배달도 그냥해주고 주문이벤트도 하면..",
          "speaker_id": "1",
          "time": "20210822 21:11"
        },
        {
          "id": "MDRW2100009060.1.20",
          "form": "우리 동네 중국집은 원래 배달비 안받더니 차츰ㅜ",
          "original_form": "우리 동네 중국집은 원래 배달비 안받더니 차츰ㅜ",
          "speaker_id": "2",
          "time": "20210822 21:15"
        },
        {
          "id": "MDRW2100009060.1.21",
          "form": "저도 님 집 근처로 이사가고 싶어요ㅜ",
          "original_form": "저도 님 집 근처로 이사가고 싶어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 21:15"
        },
        {
          "id": "MDRW2100009060.1.22",
          "form": "배달비 안받는 중국집 많은 데로",
          "original_form": "배달비 안받는 중국집 많은 데로",
          "speaker_id": "2",
          "time": "20210822 21:15"
        },
        {
          "id": "MDRW2100009060.1.23",
          "form": "한군데는 있을거에요 ㅎㅎㅎ 잘 찾아보세요",
          "original_form": "한군데는 있을거에요 ㅎㅎㅎ 잘 찾아보세요",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009060.1.24",
          "form": "배달비있어도 천원정도로 저렴하더라구요!",
          "original_form": "배달비있어도 천원정도로 저렴하더라구요!",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009060.1.25",
          "form": "넹 계속 입점가게가 느니까요",
          "original_form": "넹 계속 입점가게가 느니까요",
          "speaker_id": "2",
          "time": "20210822 21:22"
        },
        {
          "id": "MDRW2100009060.1.26",
          "form": "배달비가 중국집이 착하긴 해요",
          "original_form": "배달비가 중국집이 착하긴 해요",
          "speaker_id": "2",
          "time": "20210822 21:22"
        },
        {
          "id": "MDRW2100009060.1.27",
          "form": "울 동네도 천원만 받는곳은 몇 있어요!",
          "original_form": "울 동네도 천원만 받는곳은 몇 있어요!",
          "speaker_id": "2",
          "time": "20210822 21:23"
        },
        {
          "id": "MDRW2100009060.1.28",
          "form": "그쵸~~ 음식가격도 저렴하고! ㅎㅎ 그래서 오늘의 야식은 뭔가요!",
          "original_form": "그쵸~~ 음식가격도 저렴하고! ㅎㅎ 그래서 오늘의 야식은 뭔가요!",
          "speaker_id": "1",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009060.1.29",
          "form": "지금 라면 먹어요",
          "original_form": "지금 라면 먹어요",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009060.1.30",
          "form": "헛ㅋㅋㅋㅋ",
          "original_form": "헛ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009060.1.31",
          "form": "진라면인가요!",
          "original_form": "진라면인가요!",
          "speaker_id": "1",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009060.1.32",
          "form": "진매에 파송송 청양고추 송송 고춧가루 팍팍",
          "original_form": "진매에 파송송 청양고추 송송 고춧가루 팍팍",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009060.1.33",
          "form": "사진 보내기 기능 없나요ㅎ",
          "original_form": "사진 보내기 기능 없나요ㅎ",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009060.1.34",
          "form": "크으 계란은요? ㅋㅋㅋㅋ",
          "original_form": "크으 계란은요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009060.1.35",
          "form": "이거 보여줘야 하는데ㅎ",
          "original_form": "이거 보여줘야 하는데ㅎ",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009060.1.36",
          "form": "머릿속으로 그려볼게옄ㅋㅋ",
          "original_form": "머릿속으로 그려볼게옄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009060.1.37",
          "form": "맵게 먹고 싶어서 계란은 생략이요",
          "original_form": "맵게 먹고 싶어서 계란은 생략이요",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009060.1.38",
          "form": "라면에 계란파시인가요",
          "original_form": "라면에 계란파시인가요",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009060.1.39",
          "form": "가끔씩 땡기면요 ㅋㅋㅋ",
          "original_form": "가끔씩 땡기면요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009060.1.40",
          "form": "ㅎㅎ 저도요",
          "original_form": "ㅎㅎ 저도요",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009060.1.41",
          "form": "저는 아까 순수 라면만 먹었어요! ㅋㅋㅋ",
          "original_form": "저는 아까 순수 라면만 먹었어요! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:29"
        }
