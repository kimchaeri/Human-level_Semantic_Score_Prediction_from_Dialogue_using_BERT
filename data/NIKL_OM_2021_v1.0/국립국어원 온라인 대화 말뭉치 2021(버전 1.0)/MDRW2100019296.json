      "utterance": [
        {
          "id": "MDRW2100019296.1.1",
          "form": "보통 유튜브로는 뭐 즐겨보세요?",
          "original_form": "보통 유튜브로는 뭐 즐겨보세요?",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019296.1.2",
          "form": "저는 타로 많이 봐요! 신기하더라구요",
          "original_form": "저는 타로 많이 봐요! 신기하더라구요",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019296.1.3",
          "form": "선생님은 뭐 즐겨보세요?",
          "original_form": "선생님은 뭐 즐겨보세요?",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019296.1.4",
          "form": "오 타로 ! 전 주로 예능 봐요 ㅋㅋ",
          "original_form": "오 타로 ! 전 주로 예능 봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019296.1.5",
          "form": "아하 어떤 예능 보세요?",
          "original_form": "아하 어떤 예능 보세요?",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019296.1.6",
          "form": "저 무도 좋아해요 ㅎㅎㅎ",
          "original_form": "저 무도 좋아해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019296.1.7",
          "form": "5분순삭 같은거 봐요!",
          "original_form": "5분순삭 같은거 봐요!",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019296.1.8",
          "form": "앜ㅋㅋㅋ무도 재밌죠ㅠㅠ 저도 요새 티비에서 재방송 하는 거 자주 봐요",
          "original_form": "앜ㅋㅋㅋ무도 재밌죠ㅠㅠ 저도 요새 티비에서 재방송 하는 거 자주 봐요",
          "speaker_id": "2",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019296.1.9",
          "form": "맞죠 ㅋㅋ 재방 엄청 해요 !",
          "original_form": "맞죠 ㅋㅋ 재방 엄청 해요 !",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019296.1.10",
          "form": "근데 항상 했던거만 하더라구요 ㅋㅋ",
          "original_form": "근데 항상 했던거만 하더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019296.1.11",
          "form": "ㅋㅋㅋ이번에 놀면 뭐하니 보니까 거의 무한도전 2 시작인 것 같더라구요",
          "original_form": "ㅋㅋㅋ이번에 놀면 뭐하니 보니까 거의 무한도전 2 시작인 것 같더라구요",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019296.1.12",
          "form": "맞아요 ! 근데 멤버들이 달라서",
          "original_form": "맞아요 ! 근데 멤버들이 달라서",
          "speaker_id": "1",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100019296.1.13",
          "form": "전 예전멤버들 좋아해요 ㅎㅎ",
          "original_form": "전 예전멤버들 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100019296.1.14",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019296.1.15",
          "form": "이번에 조세호, 광희님 들어오셨죠?",
          "original_form": "이번에 조세호, 광희님 들어오셨죠?",
          "speaker_id": "2",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019296.1.16",
          "form": "네네 ! 양세형도 나오는거같아요",
          "original_form": "네네 ! 양세형도 나오는거같아요",
          "speaker_id": "1",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019296.1.17",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019296.1.18",
          "form": "놀면 뭐하니는 보시나요?",
          "original_form": "놀면 뭐하니는 보시나요?",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019296.1.19",
          "form": "전 막 챙겨보지는 않아요 !",
          "original_form": "전 막 챙겨보지는 않아요 !",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019296.1.20",
          "form": "이번에 msg 워너비 잠깐 봤다가 ㅋㅋ 너무 질질 끌어서",
          "original_form": "이번에 msg 워너비 잠깐 봤다가 ㅋㅋ 너무 질질 끌어서",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019296.1.21",
