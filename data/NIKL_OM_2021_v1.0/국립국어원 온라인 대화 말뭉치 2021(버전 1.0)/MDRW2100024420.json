      "utterance": [
        {
          "id": "MDRW2100024420.1.1",
          "form": "네!! 근데 국내여행만 많이 다녀봤어요ㅜ",
          "original_form": "네!! 근데 국내여행만 많이 다녀봤어요ㅜ",
          "speaker_id": "2",
          "time": "20210831 07:50"
        },
        {
          "id": "MDRW2100024420.1.2",
          "form": "해외는 1번밖에 못갔는데 name1님은 여행 좋아하시나봐요!",
          "original_form": "해외는 1번밖에 못갔는데 &name1&님은 여행 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210831 07:50"
        },
        {
          "id": "MDRW2100024420.1.3",
          "form": "앞으로 기회 많으실거에요",
          "original_form": "앞으로 기회 많으실거에요",
          "speaker_id": "1",
          "time": "20210831 07:51"
        },
        {
          "id": "MDRW2100024420.1.4",
          "form": "여행은 사람들 다 좋아하지 않을까요",
          "original_form": "여행은 사람들 다 좋아하지 않을까요",
          "speaker_id": "1",
          "time": "20210831 07:51"
        },
        {
          "id": "MDRW2100024420.1.5",
          "form": "ㅎㅎ 어디 가보셨어요?",
          "original_form": "ㅎㅎ 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210831 07:51"
        },
        {
          "id": "MDRW2100024420.1.6",
          "form": "특별히 기억에 남는 곳",
          "original_form": "특별히 기억에 남는 곳",
          "speaker_id": "2",
          "time": "20210831 07:51"
        },
        {
          "id": "MDRW2100024420.1.7",
          "form": "추천해주세요!",
          "original_form": "추천해주세요!",
          "speaker_id": "2",
          "time": "20210831 07:51"
        },
        {
          "id": "MDRW2100024420.1.8",
          "form": "지금 부산 와있고",
          "original_form": "지금 부산 와있고",
          "speaker_id": "1",
          "time": "20210831 07:51"
        },
        {
          "id": "MDRW2100024420.1.9",
          "form": "작년에 신혼여행으로 제주도 다녀왔고",
          "original_form": "작년에 신혼여행으로 제주도 다녀왔고",
          "speaker_id": "1",
          "time": "20210831 07:52"
        },
        {
          "id": "MDRW2100024420.1.10",
          "form": "제천 리솜포레스트 다녀왔고",
          "original_form": "제천 리솜포레스트 다녀왔고",
          "speaker_id": "1",
          "time": "20210831 07:52"
        },
        {
          "id": "MDRW2100024420.1.11",
          "form": "오 부산으로 여행가신거예요?",
          "original_form": "오 부산으로 여행가신거예요?",
          "speaker_id": "2",
          "time": "20210831 07:52"
        },
        {
          "id": "MDRW2100024420.1.12",
          "form": "아내가 몸이 안좋은상황이라",
          "original_form": "아내가 몸이 안좋은상황이라",
          "speaker_id": "1",
          "time": "20210831 07:53"
        },
        {
          "id": "MDRW2100024420.1.13",
          "form": "집에서 멀리 못가고 부산 왔어요",
          "original_form": "집에서 멀리 못가고 부산 왔어요",
          "speaker_id": "1",
          "time": "20210831 07:53"
        },
        {
          "id": "MDRW2100024420.1.14",
          "form": "이번 여름에 휴가 다녀오셨어요?",
          "original_form": "이번 여름에 휴가 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210831 07:53"
        },
        {
          "id": "MDRW2100024420.1.15",
          "form": "ㅜㅠ 아내분 괜찮으시길 바래요!",
          "original_form": "ㅜㅠ 아내분 괜찮으시길 바래요!",
          "speaker_id": "2",
          "time": "20210831 07:54"
        },
        {
          "id": "MDRW2100024420.1.16",
          "form": "앗 여름엔 속초 다녀왔어요",
          "original_form": "앗 여름엔 속초 다녀왔어요",
          "speaker_id": "2",
          "time": "20210831 07:54"
        },
        {
          "id": "MDRW2100024420.1.17",
          "form": "바다보니 좋더라구요ㅎㅎ",
          "original_form": "바다보니 좋더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 07:54"
        },
        {
          "id": "MDRW2100024420.1.18",
          "form": "속초 좋았겠다",
          "original_form": "속초 좋았겠다",
          "speaker_id": "1",
          "time": "20210831 07:54"
        },
        {
          "id": "MDRW2100024420.1.19",
          "form": "아바이순대가 속초 맞죠?",
          "original_form": "아바이순대가 속초 맞죠?",
          "speaker_id": "1",
          "time": "20210831 07:55"
        },
        {
          "id": "MDRW2100024420.1.20",
          "form": "넵!!",
          "original_form": "넵!!",
          "speaker_id": "2",
          "time": "20210831 07:55"
        },
        {
          "id": "MDRW2100024420.1.21",
          "form": "드셔보셨어요!",
          "original_form": "드셔보셨어요!",
          "speaker_id": "1",
          "time": "20210831 07:55"
        },
        {
          "id": "MDRW2100024420.1.22",
          "form": "아바이 순대도 먹고 회도 많이 먹고ㅎㅎ",
          "original_form": "아바이 순대도 먹고 회도 많이 먹고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 07:55"
        },
        {
          "id": "MDRW2100024420.1.23",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210831 07:55"
        },
        {
          "id": "MDRW2100024420.1.24",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210831 07:55"
        },
        {
          "id": "MDRW2100024420.1.25",
          "form": "전 항상 식도락 여행이예요ㅋㅋ",
          "original_form": "전 항상 식도락 여행이예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 07:55"
        },
        {
          "id": "MDRW2100024420.1.26",
          "form": "저도 식도락여행 좋아해요!!",
          "original_form": "저도 식도락여행 좋아해요!!",
          "speaker_id": "1",
          "time": "20210831 07:56"
        }
      ]
