      "utterance": [
        {
          "id": "MDRW2100002897.1.1",
          "form": "집이지?",
          "original_form": "집이지?",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002897.1.2",
          "form": "뭐하고 있어?",
          "original_form": "뭐하고 있어?",
          "speaker_id": "1",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002897.1.3",
          "form": "바닥 청소 중이야",
          "original_form": "바닥 청소 중이야",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002897.1.4",
          "form": "밀대로 한 번 밀었어",
          "original_form": "밀대로 한 번 밀었어",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002897.1.5",
          "form": "고생하네",
          "original_form": "고생하네",
          "speaker_id": "1",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002897.1.6",
          "form": "내일 집에 올 거니?",
          "original_form": "내일 집에 올 거니?",
          "speaker_id": "1",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002897.1.7",
          "form": "응 내일 5시쯤 갈게",
          "original_form": "응 내일 5시쯤 갈게",
          "speaker_id": "2",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002897.1.8",
          "form": "엄만 퇴근했어?",
          "original_form": "엄만 퇴근했어?",
          "speaker_id": "2",
          "time": "20210820 16:45"
        },
        {
          "id": "MDRW2100002897.1.9",
          "form": "응 방금 집 도착했어",
          "original_form": "응 방금 집 도착했어",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002897.1.10",
          "form": "좀 자고 이따 저녁 준비해야지",
          "original_form": "좀 자고 이따 저녁 준비해야지",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002897.1.11",
          "form": "name3이는 자고?",
          "original_form": "&name3&이는 자고?",
          "speaker_id": "2",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002897.1.12",
          "form": "응 피곤하대, 잠들었어.",
          "original_form": "응 피곤하대, 잠들었어.",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002897.1.13",
          "form": "아빠도 잘 거 아니야ㅋㅋㅋ",
          "original_form": "아빠도 잘 거 아니야ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002897.1.14",
          "form": "대낮에 다들 자고 있네",
          "original_form": "대낮에 다들 자고 있네",
          "speaker_id": "2",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002897.1.15",
          "form": "할머니는 안 주무셔.",
          "original_form": "할머니는 안 주무셔.",
          "speaker_id": "1",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002897.1.16",
          "form": "할머니는 저녁 드시고 8시면 주무시잖아",
          "original_form": "할머니는 저녁 드시고 8시면 주무시잖아",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002897.1.17",
          "form": "그래서 오늘 저녁 뭐할 건데?",
          "original_form": "그래서 오늘 저녁 뭐할 건데?",
          "speaker_id": "2",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002897.1.18",
          "form": "글쎄. 현진이는 감자탕 먹고 싶대.",
          "original_form": "글쎄. 현진이는 감자탕 먹고 싶대.",
          "speaker_id": "1",
          "time": "20210820 16:48"
        },
        {
          "id": "MDRW2100002897.1.19",
          "form": "티비에서 봤다고 오늘 그거 해달라네.",
          "original_form": "티비에서 봤다고 오늘 그거 해달라네.",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002897.1.20",
          "form": "김자탕 하려면 미리 준비해놔야 되잖아",
          "original_form": "김자탕 하려면 미리 준비해놔야 되잖아",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002897.1.21",
          "form": "내일 맛있는 거 먹을 거니까 오늘은 그냥 대충 때우자고 해",
          "original_form": "내일 맛있는 거 먹을 거니까 오늘은 그냥 대충 때우자고 해",
          "speaker_id": "2",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002897.1.22",
          "form": "name3이는 막내라 먹고 싶은 게 많아.",
          "original_form": "&name3&이는 막내라 먹고 싶은 게 많아.",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002897.1.23",
          "form": "애기야 애기.",
          "original_form": "애기야 애기.",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002897.1.24",
          "form": "걔도 다 컸어~",
          "original_form": "걔도 다 컸어~",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002897.1.25",
          "form": "오늘 이불 빨래한다며~ 빨래 돌렸어?",
          "original_form": "오늘 이불 빨래한다며~ 빨래 돌렸어?",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002897.1.26",
          "form": "응 아빠가 넌다고 빨래 다 되면 깨우래.",
          "original_form": "응 아빠가 넌다고 빨래 다 되면 깨우래.",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002897.1.27",
          "form": "반찬투정은 현진이보다 아빠가 더 심해.",
          "original_form": "반찬투정은 현진이보다 아빠가 더 심해.",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002897.1.28",
          "form": "맨날 고기 달라고 하지?ㅋㅋㅋㅋㅋ",
          "original_form": "맨날 고기 달라고 하지?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002897.1.29",
          "form": "고기 구워주고 설거지 시켜",
          "original_form": "고기 구워주고 설거지 시켜",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002897.1.30",
          "form": "요즘엔 설거지 다 아빠가 해.",
          "original_form": "요즘엔 설거지 다 아빠가 해.",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002897.1.31",
          "form": "요리는 엄마가 다 하니까~",
          "original_form": "요리는 엄마가 다 하니까~",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002897.1.32",
          "form": "답답하다고 엄마가 다 하지 말고 집안일은 각자 나눠서 해",
          "original_form": "답답하다고 엄마가 다 하지 말고 집안일은 각자 나눠서 해",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002897.1.33",
          "form": "그래야지. 엄마도 나이 먹어서 이제 힘들어.",
          "original_form": "그래야지. 엄마도 나이 먹어서 이제 힘들어.",
          "speaker_id": "1",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002897.1.34",
          "form": "오늘 저녁 몇 시에 먹을 거야?",
          "original_form": "오늘 저녁 몇 시에 먹을 거야?",
          "speaker_id": "2",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002897.1.35",
          "form": "좀 늦게 먹을 것 같은데. 7시 반쯤.",
          "original_form": "좀 늦게 먹을 것 같은데. 7시 반쯤.",
          "speaker_id": "1",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002897.1.36",
          "form": "다들 피곤하다고 자고 있어서.",
          "original_form": "다들 피곤하다고 자고 있어서.",
          "speaker_id": "1",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002897.1.37",
          "form": "잠 많은 건 유전인가 봐",
          "original_form": "잠 많은 건 유전인가 봐",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002897.1.38",
          "form": "응 다들 똑같아.",
          "original_form": "응 다들 똑같아.",
          "speaker_id": "1",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002897.1.39",
          "form": "어제는 아빠가 욕실 청소도 했다.",
          "original_form": "어제는 아빠가 욕실 청소도 했다.",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002897.1.40",
          "form": "ㅋㅋㅋㅋ큰 일 했네",
          "original_form": "ㅋㅋㅋㅋ큰 일 했네",
          "speaker_id": "2",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002897.1.41",
          "form": "깨끗하게 잘 했어?",
          "original_form": "깨끗하게 잘 했어?",
          "speaker_id": "2",
          "time": "20210820 17:06"
        },
        {
          "id": "MDRW2100002897.1.42",
          "form": "너네 아빠가 안 해서 그렇지 하면 또 잘해",
          "original_form": "너네 아빠가 안 해서 그렇지 하면 또 잘해",
          "speaker_id": "1",
          "time": "20210820 17:06"
        },
        {
          "id": "MDRW2100002897.1.43",
          "form": "칭찬 많이 해주고 또 시켜",
          "original_form": "칭찬 많이 해주고 또 시켜",
          "speaker_id": "2",
          "time": "20210820 17:07"
        },
        {
          "id": "MDRW2100002897.1.44",
          "form": "요리도 해보라고 해",
          "original_form": "요리도 해보라고 해",
          "speaker_id": "2",
          "time": "20210820 17:07"
        },
        {
          "id": "MDRW2100002897.1.45",
          "form": "아빠 라면은 잘 끓이잖아",
          "original_form": "아빠 라면은 잘 끓이잖아",
          "speaker_id": "2",
          "time": "20210820 17:07"
        },
        {
          "id": "MDRW2100002897.1.46",
          "form": "알겠어 이것저것 많이 시켜볼게.",
          "original_form": "알겠어 이것저것 많이 시켜볼게.",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002897.1.47",
          "form": "네 집으로 출장청소 보낼까",
          "original_form": "네 집으로 출장청소 보낼까",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002897.1.48",
          "form": "ㅋㅋㅋㅋ아빠가 출장 오는 거야?",
          "original_form": "ㅋㅋㅋㅋ아빠가 출장 오는 거야?",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002897.1.49",
          "form": "일은 아빠가 하고 돈은 엄마한테 줄게",
          "original_form": "일은 아빠가 하고 돈은 엄마한테 줄게",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002897.1.50",
          "form": "아빠랑 반 주고 반은 나 가져야지.",
          "original_form": "아빠랑 반 주고 반은 나 가져야지.",
          "speaker_id": "1",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002897.1.51",
          "form": "그래 아빠한테 용돈이라고 해ㅋㅋㅋㅋ",
          "original_form": "그래 아빠한테 용돈이라고 해ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:11"
        }
      ]
