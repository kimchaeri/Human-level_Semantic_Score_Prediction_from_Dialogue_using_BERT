        },
        {
          "id": "MDRW2100014496.1.2",
          "form": "네 김동률 콘서트요^^",
          "original_form": "네 김동률 콘서트요^^",
          "speaker_id": "2",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014496.1.3",
          "form": "좋았나요?",
          "original_form": "좋았나요?",
          "speaker_id": "1",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014496.1.4",
          "form": "저도 올해엔 가볼까해요  코로나 주사 다맞았거든요",
          "original_form": "저도 올해엔 가볼까해요  코로나 주사 다맞았거든요",
          "speaker_id": "1",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014496.1.5",
          "form": "내일 티켓오픈해서 할려고요",
          "original_form": "내일 티켓오픈해서 할려고요",
          "speaker_id": "1",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014496.1.6",
          "form": "무슨 티켓 오픈인가요?",
          "original_form": "무슨 티켓 오픈인가요?",
          "speaker_id": "2",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100014496.1.7",
          "form": "10cm요",
          "original_form": "10cm요",
          "speaker_id": "1",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014496.1.8",
          "form": "아 10cm요~ 멤버는 둘 그대로인가요?",
          "original_form": "아 10cm요~ 멤버는 둘 그대로인가요?",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014496.1.9",
          "form": "혼자된지 오래전일걸요 .",
          "original_form": "혼자된지 오래전일걸요 .",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014496.1.10",
          "form": "아 그렇군요 예매사이트는 어디인가요",
          "original_form": "아 그렇군요 예매사이트는 어디인가요",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014496.1.11",
          "form": "네이버에서요",
          "original_form": "네이버에서요",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014496.1.12",
          "form": "친구가 갈지고민중이라서요",
          "original_form": "친구가 갈지고민중이라서요",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014496.1.13",
          "form": "그렇군요 조성진이 투어하던데 전석매진이더라구요",
          "original_form": "그렇군요 조성진이 투어하던데 전석매진이더라구요",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014496.1.14",
          "form": "코시국에도 매진이라니 대단하네요",
          "original_form": "코시국에도 매진이라니 대단하네요",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014496.1.15",
          "form": "그래서 저도 궁금해지더라구요~ 엄청 감동적이라도도 하고..",
          "original_form": "그래서 저도 궁금해지더라구요~ 엄청 감동적이라도도 하고..",
