      "utterance": [
        {
          "id": "MDRW2100022215.1.1",
          "form": "사고싶은데",
          "original_form": "사고싶은데",
          "speaker_id": "1",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100022215.1.2",
          "form": "아직 구매는 안하거나 못하는거",
          "original_form": "아직 구매는 안하거나 못하는거",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022215.1.3",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022215.1.4",
          "form": "요즘 스마트워치 좀 관심있어요",
          "original_form": "요즘 스마트워치 좀 관심있어요",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022215.1.5",
          "form": "오~ 어디꺼요?",
          "original_form": "오~ 어디꺼요?",
          "speaker_id": "1",
          "time": "20210829 15:00"
        },
        {
          "id": "MDRW2100022215.1.6",
          "form": "최근에 친구가 갤럭시워치 신제품 예약해서 받았다고 자랑하더라고요",
          "original_form": "최근에 친구가 갤럭시워치 신제품 예약해서 받았다고 자랑하더라고요",
          "speaker_id": "2",
          "time": "20210829 15:10"
        },
        {
          "id": "MDRW2100022215.1.7",
          "form": "아 그거 엄청 좋아보이더라고요",
          "original_form": "아 그거 엄청 좋아보이더라고요",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022215.1.8",
          "form": "거치대도 귀엽고!!!",
          "original_form": "거치대도 귀엽고!!!",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022215.1.9",
          "form": "그래서 저도 구매욕구가 스멀스멀",
          "original_form": "그래서 저도 구매욕구가 스멀스멀",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022215.1.10",
          "form": "전 스마트워치 이해 못하는 편이었는데",
          "original_form": "전 스마트워치 이해 못하는 편이었는데",
          "speaker_id": "1",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022215.1.11",
          "form": "어쩌다 갤럭시핏 생겨서 착용해보니까 좋더라고요ㅎㅎㅎ",
          "original_form": "어쩌다 갤럭시핏 생겨서 착용해보니까 좋더라고요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022215.1.12",
          "form": "물론 워치에 비해선 기능1도 없는 편이지만",
          "original_form": "물론 워치에 비해선 기능1도 없는 편이지만",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022215.1.13",
          "form": "오 확실히 좋긴 하군요",
          "original_form": "오 확실히 좋긴 하군요",
          "speaker_id": "2",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022215.1.14",
          "form": "연락오는거 바로 손목에서 확인가능하고",
          "original_form": "연락오는거 바로 손목에서 확인가능하고",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022215.1.15",
          "form": "휴대전화 손에 들고다닐 필요가 없겠어요",
          "original_form": "휴대전화 손에 들고다닐 필요가 없겠어요",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022215.1.16",
          "form": "전화 건강체크 등 엄청 좋던데요 워치는?",
          "original_form": "전화 건강체크 등 엄청 좋던데요 워치는?",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022215.1.17",
          "form": "맞아요 답장도 가능하던데",
          "original_form": "맞아요 답장도 가능하던데",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022215.1.18",
          "form": "기능이 확실히 좋긴 하죠",
          "original_form": "기능이 확실히 좋긴 하죠",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022215.1.19",
          "form": "이번에 나온 갤럭시워치는 인바디도 된대요",
          "original_form": "이번에 나온 갤럭시워치는 인바디도 된대요",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022215.1.20",
          "form": "좋긴한데ㅜㅜ 가격이....",
          "original_form": "좋긴한데ㅜㅜ 가격이....",
          "speaker_id": "1",
