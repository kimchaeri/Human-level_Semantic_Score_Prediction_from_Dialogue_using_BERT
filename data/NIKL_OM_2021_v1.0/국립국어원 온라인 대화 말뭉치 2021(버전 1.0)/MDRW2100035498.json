      "utterance": [
        {
          "id": "MDRW2100035498.1.1",
          "form": "그쵸 나가기만 해도 땀나서 힘들어요",
          "original_form": "그쵸 나가기만 해도 땀나서 힘들어요",
          "speaker_id": "2",
          "time": "20210829 18:35"
        },
        {
          "id": "MDRW2100035498.1.2",
          "form": "비오는게 나은ㄱㅏ요 더운게 나으세요?",
          "original_form": "비오는게 나은ㄱㅏ요 더운게 나으세요?",
          "speaker_id": "1",
          "time": "20210829 18:36"
        },
        {
          "id": "MDRW2100035498.1.3",
          "form": "흠 어렵네요 전 비오는거요",
          "original_form": "흠 어렵네요 전 비오는거요",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100035498.1.4",
          "form": "비 오는 것도 나름 괜찮을 때가 있어요ㅋㅋ",
          "original_form": "비 오는 것도 나름 괜찮을 때가 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100035498.1.5",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100035498.1.6",
          "form": "차라리 비오는게.. 감성은 있죠 ㅋㅋ",
          "original_form": "차라리 비오는게.. 감성은 있죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100035498.1.7",
          "form": "ㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100035498.1.8",
          "form": "더우면 감성은 무슨 ㅠㅠ",
          "original_form": "더우면 감성은 무슨 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100035498.1.9",
          "form": "더운건 짜증만 나요",
          "original_form": "더운건 짜증만 나요",
          "speaker_id": "2",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100035498.1.10",
          "form": "저희집 에어컨이 고장나서",
          "original_form": "저희집 에어컨이 고장나서",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100035498.1.11",
          "form": "너무 짜증났던 기억이 ㅋㅋ 있네요",
          "original_form": "너무 짜증났던 기억이 ㅋㅋ 있네요",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100035498.1.12",
          "form": "헐 여름에 에어컨 고장나면 어떻게 사나요",
          "original_form": "헐 여름에 에어컨 고장나면 어떻게 사나요",
          "speaker_id": "2",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100035498.1.13",
          "form": "못살아요 ㅋㅋㅜㅜ 할머니집 갔었어요 ㅋㅋㅋ",
          "original_form": "못살아요 ㅋㅋㅜㅜ 할머니집 갔었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100035498.1.14",
          "form": "ㅋㅋㅋㅋ잘하셨어요 대피해야해요",
          "original_form": "ㅋㅋㅋㅋ잘하셨어요 대피해야해요",
          "speaker_id": "2",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100035498.1.15",
          "form": "전 겨울엔 히터 잘 안트는데 ㅋㅋ 히터 트시나요?",
          "original_form": "전 겨울엔 히터 잘 안트는데 ㅋㅋ 히터 트시나요?",
