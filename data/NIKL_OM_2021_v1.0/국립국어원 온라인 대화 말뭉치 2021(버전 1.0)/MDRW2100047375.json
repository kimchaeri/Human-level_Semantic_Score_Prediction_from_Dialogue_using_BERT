      "utterance": [
        {
          "id": "MDRW2100047375.1.1",
          "form": "아 올초에",
          "original_form": "아 올초에",
          "speaker_id": "2",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100047375.1.2",
          "form": "갑자기 인생의 회전목마 듣다",
          "original_form": "갑자기 인생의 회전목마 듣다",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100047375.1.3",
          "form": "그 드라마 생각나서 봤어요",
          "original_form": "그 드라마 생각나서 봤어요",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100047375.1.4",
          "form": "김현주 지진희",
          "original_form": "김현주 지진희",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100047375.1.5",
          "form": "애인 있어요",
          "original_form": "애인 있어요",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100047375.1.6",
          "form": "처음듣는 드라마인데..  아마 저는 배우 가려서 보느라",
          "original_form": "처음듣는 드라마인데..  아마 저는 배우 가려서 보느라",
          "speaker_id": "1",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100047375.1.7",
          "form": "못봤을수도 있을것같네요ㅋㅋ\r\n 김현주를 안좋아해서요ㅜㅡㅜ",
          "original_form": "못봤을수도 있을것같네요ㅋㅋ\r\n 김현주를 안좋아해서요ㅜㅡㅜ",
          "speaker_id": "1",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100047375.1.8",
          "form": "오 김현주 전 좋아해요",
          "original_form": "오 김현주 전 좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100047375.1.9",
          "form": "지진희 후회남요",
          "original_form": "지진희 후회남요",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100047375.1.10",
          "form": "노래들으며 우는장면이 ㅜㅜ생각나서 다시 봤네요",
          "original_form": "노래들으며 우는장면이 ㅜㅜ생각나서 다시 봤네요",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100047375.1.11",
          "form": "넘 슬픈 얘기예요",
          "original_form": "넘 슬픈 얘기예요",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100047375.1.12",
          "form": "왜 싫어 하세요?",
          "original_form": "왜 싫어 하세요?",
          "speaker_id": "2",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100047375.1.13",
          "form": "모르겠어요..  그냥 특별한 이유는 없는데ㅋㅋ 보면 괜히 안좋은 남자배우나 여자배우가 있어요.. ㅋㅋ",
          "original_form": "모르겠어요..  그냥 특별한 이유는 없는데ㅋㅋ 보면 괜히 안좋은 남자배우나 여자배우가 있어요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100047375.1.14",
          "form": "아 그런거 있어요  ㅋㅋ",
          "original_form": "아 그런거 있어요  ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100047375.1.15",
          "form": "전 원래 좋아했어요",
          "original_form": "전 원래 좋아했어요",
          "speaker_id": "2",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100047375.1.16",
          "form": "여기서 이뻐요 ㅋㅋ",
          "original_form": "여기서 이뻐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100047375.1.17",
          "form": "아하!ㅋㅋㅋ  그것도 슬쩍 내용보고  재밌으면 도전해봐야겠네요",
          "original_form": "아하!ㅋㅋㅋ  그것도 슬쩍 내용보고  재밌으면 도전해봐야겠네요",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100047375.1.18",
          "form": "ㅋㅋ고구마 가",
          "original_form": "ㅋㅋ고구마 가",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100047375.1.19",
          "form": "근데 넘 아련하고 잼나요",
          "original_form": "근데 넘 아련하고 잼나요",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100047375.1.20",
          "form": "길어서 중간에 넘기며 보긴했어요",
          "original_form": "길어서 중간에 넘기며 보긴했어요",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100047375.1.21",
          "form": "이건 그냥 추리 이런거 아니고 드라마 장르인거죠?!",
          "original_form": "이건 그냥 추리 이런거 아니고 드라마 장르인거죠?!",
          "speaker_id": "1",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100047375.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100047375.1.23",
          "form": "정통멜로요 ㅋㅋ",
          "original_form": "정통멜로요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100047375.1.24",
          "form": "안맞으실것 같아요 ㅋㅋ",
          "original_form": "안맞으실것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100047375.1.25",
          "form": "그냥 꽂혀서 봤어요 ㅎㅎ",
          "original_form": "그냥 꽂혀서 봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100047375.1.26",
          "form": "김현주 지진희가 좋아서요 ㅎㅎ",
          "original_form": "김현주 지진희가 좋아서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100047375.1.27",
          "form": "그럴꺼 같네요ㅎ",
          "original_form": "그럴꺼 같네요ㅎ",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100047375.1.28",
          "form": "저 어제 시크릿호.텔 다봤어요!",
          "original_form": "저 어제 시크릿호.텔 다봤어요!",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100047375.1.29",
          "form": "대박이세요",
          "original_form": "대박이세요",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100047375.1.30",
          "form": "오 범인 아셨겠네요 ㅋㅋ",
          "original_form": "오 범인 아셨겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100047375.1.31",
          "form": "그걸 다보시다니",
          "original_form": "그걸 다보시다니",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100047375.1.32",
          "form": "추카추카",
          "original_form": "추카추카",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100047375.1.33",
          "form": "정말 의지의 여인이십니다",
          "original_form": "정말 의지의 여인이십니다",
          "speaker_id": "2",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100047375.1.34",
          "form": "네ㅋㅋㅋ 캠핑갔다와서 한동안 또 쉬었네요ㅠㅋ\r\n다른거 시작했어요",
          "original_form": "네ㅋㅋㅋ 캠핑갔다와서 한동안 또 쉬었네요ㅠㅋ\r\n다른거 시작했어요",
          "speaker_id": "1",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100047375.1.35",
          "form": "뭐요?",
          "original_form": "뭐요?",
          "speaker_id": "2",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100047375.1.36",
          "form": "7일의 왕비\r\n드라마요ㅋ",
          "original_form": "7일의 왕비\r\n드라마요ㅋ",
          "speaker_id": "1",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100047375.1.37",
          "form": "오 요새 안봐서 첨들어봐요",
          "original_form": "오 요새 안봐서 첨들어봐요",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100047375.1.38",
          "form": "잼나요?",
          "original_form": "잼나요?",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100047375.1.39",
          "form": "17년도 드라마던데 검색하다가 재미있어보여서 지금 3화째보고 있어요~~\r\n아직까진 재밌네요ㅋ",
          "original_form": "17년도 드라마던데 검색하다가 재미있어보여서 지금 3화째보고 있어요~~\r\n아직까진 재밌네요ㅋ",
          "speaker_id": "1",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100047375.1.40",
          "form": "오 그런가요",
          "original_form": "오 그런가요",
          "speaker_id": "2",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100047375.1.41",
          "form": "아 뭔지 알겠어요",
          "original_form": "아 뭔지 알겠어요",
          "speaker_id": "2",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100047375.1.42",
          "form": "잠깐 본것 같아요",
          "original_form": "잠깐 본것 같아요",
          "speaker_id": "2",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100047375.1.43",
          "form": "근데 잠깐 본거라 잊었네요 ㅎㅎ",
          "original_form": "근데 잠깐 본거라 잊었네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:16"
        }
      ]
