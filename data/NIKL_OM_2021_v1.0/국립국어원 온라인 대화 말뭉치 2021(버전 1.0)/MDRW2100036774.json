      "utterance": [
        {
          "id": "MDRW2100036774.1.1",
          "form": "전 수제비여",
          "original_form": "전 수제비여",
          "speaker_id": "2",
          "time": "20210831 12:31"
        },
        {
          "id": "MDRW2100036774.1.2",
          "form": "님은여?",
          "original_form": "님은여?",
          "speaker_id": "2",
          "time": "20210831 12:31"
        },
        {
          "id": "MDRW2100036774.1.3",
          "form": "저 소불고기 해먹으려고요..ㅎㅎ",
          "original_form": "저 소불고기 해먹으려고요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:33"
        },
        {
          "id": "MDRW2100036774.1.4",
          "form": "와 맛날듯요",
          "original_form": "와 맛날듯요",
          "speaker_id": "2",
          "time": "20210831 12:34"
        },
        {
          "id": "MDRW2100036774.1.5",
          "form": "레시피 있으세요?",
          "original_form": "레시피 있으세요?",
          "speaker_id": "2",
          "time": "20210831 12:34"
        },
        {
          "id": "MDRW2100036774.1.6",
          "form": "불고기 양념이 있어요..ㅋㅋㅋㅋㅋ",
          "original_form": "불고기 양념이 있어요..ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:36"
        },
        {
          "id": "MDRW2100036774.1.7",
          "form": "요새는 양념도 잘나오더라구오ㅎㅎㅎ",
          "original_form": "요새는 양념도 잘나오더라구오ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:36"
        },
        {
          "id": "MDRW2100036774.1.8",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:38"
        },
        {
          "id": "MDRW2100036774.1.9",
          "form": "정말 다행",
          "original_form": "정말 다행",
          "speaker_id": "2",
          "time": "20210831 12:38"
        },
        {
          "id": "MDRW2100036774.1.10",
          "form": "한우로 하시나요?",
          "original_form": "한우로 하시나요?",
          "speaker_id": "2",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100036774.1.11",
          "form": "네~ 한우가 확실히 부드러워서요..",
          "original_form": "네~ 한우가 확실히 부드러워서요..",
          "speaker_id": "1",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100036774.1.12",
          "form": "아 먹고싶네요",
          "original_form": "아 먹고싶네요",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100036774.1.13",
          "form": "동생은 한우 구워먹는 거 좋아하는데 전 굽는것보다 양념을 좋아해요",
          "original_form": "동생은 한우 구워먹는 거 좋아하는데 전 굽는것보다 양념을 좋아해요",
          "speaker_id": "1",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100036774.1.14",
          "form": "전 둘다요",
          "original_form": "전 둘다요",
          "speaker_id": "2",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100036774.1.15",
          "form": "어떻게해도 맛이 좋아요",
          "original_form": "어떻게해도 맛이 좋아요",
          "speaker_id": "2",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100036774.1.16",
          "form": "뭐 고기는 사랑이죠..ㅎㅎ",
          "original_form": "뭐 고기는 사랑이죠..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100036774.1.17",
          "form": "수제비 잘 드셨어요??",
          "original_form": "수제비 잘 드셨어요??",
          "speaker_id": "1",
          "time": "20210831 14:31"
        },
        {
          "id": "MDRW2100036774.1.18",
          "form": "넘나 맛있어여ㅋ",
          "original_form": "넘나 맛있어여ㅋ",
          "speaker_id": "2",
          "time": "20210831 20:49"
        },
        {
          "id": "MDRW2100036774.1.19",
          "form": "낼은 또 모먹죠?",
          "original_form": "낼은 또 모먹죠?",
          "speaker_id": "2",
          "time": "20210831 20:49"
        },
        {
          "id": "MDRW2100036774.1.20",
          "form": "매 끼니 정하는게 일이죠?ㅎㅎㅎㅎ",
          "original_form": "매 끼니 정하는게 일이죠?ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 06:25"
        },
        {
          "id": "MDRW2100036774.1.21",
          "form": "요새 비오니까 부침개도 한끼 먹으면 괜찮을 거 같아요",
          "original_form": "요새 비오니까 부침개도 한끼 먹으면 괜찮을 거 같아요",
          "speaker_id": "1",
          "time": "20210901 06:26"
        },
        {
          "id": "MDRW2100036774.1.22",
          "form": "전 비빔밥먹었어요",
          "original_form": "전 비빔밥먹었어요",
          "speaker_id": "2",
          "time": "20210901 14:03"
        }
      ]
