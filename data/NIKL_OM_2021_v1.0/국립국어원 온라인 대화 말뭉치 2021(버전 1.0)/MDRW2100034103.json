      "utterance": [
        {
          "id": "MDRW2100034103.1.1",
          "form": "늦게 입문하셨네요 !",
          "original_form": "늦게 입문하셨네요 !",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100034103.1.2",
          "form": "저는 마라샹궈가 더 맛있어요 ㅎㅎ",
          "original_form": "저는 마라샹궈가 더 맛있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100034103.1.3",
          "form": "마라샹궈 먹어본 친구들은 다 마라샹궈가 더 맛있다고 하더라고요!",
          "original_form": "마라샹궈 먹어본 친구들은 다 마라샹궈가 더 맛있다고 하더라고요!",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100034103.1.4",
          "form": "마라탕이랑 다른 맛인가요?",
          "original_form": "마라탕이랑 다른 맛인가요?",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100034103.1.5",
          "form": "약간 간이 더 쎄요!",
          "original_form": "약간 간이 더 쎄요!",
          "speaker_id": "2",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100034103.1.6",
          "form": "볶음면 같은거 좋아하세요?",
          "original_form": "볶음면 같은거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100034103.1.7",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100034103.1.8",
          "form": "그러면 마라샹궈도 좋아하실꺼에요!",
          "original_form": "그러면 마라샹궈도 좋아하실꺼에요!",
          "speaker_id": "2",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100034103.1.9",
          "form": "좀 자극적인 맛이에요 ㅋㅋ",
          "original_form": "좀 자극적인 맛이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100034103.1.10",
          "form": "꼭 먹어봐야겠어요ㅎㅎ",
          "original_form": "꼭 먹어봐야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100034103.1.11",
          "form": "꿔바로우도 좋아하시나요?",
          "original_form": "꿔바로우도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100034103.1.12",
          "form": "네 좋아해요 ㅎㅎㅎ",
          "original_form": "네 좋아해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100034103.1.13",
          "form": "약간 필수로 ㅋㅋ 시켜요 !",
          "original_form": "약간 필수로 ㅋㅋ 시켜요 !",
          "speaker_id": "2",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100034103.1.14",
          "form": "자도 친구들이랑 먹을 때는 꼭 시켜요!",
          "original_form": "자도 친구들이랑 먹을 때는 꼭 시켜요!",
          "speaker_id": "1",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100034103.1.15",
          "form": "찹쌀 반죽이 너무 맛있어요ㅠㅜ",
          "original_form": "찹쌀 반죽이 너무 맛있어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100034103.1.16",
          "form": "맞죠 ㅠㅠ 약간 찹쌀 탕수육이랑은 다른 느낌이에요",
          "original_form": "맞죠 ㅠㅠ 약간 찹쌀 탕수육이랑은 다른 느낌이에요",
          "speaker_id": "2",
          "time": "20210827 16:10"
        },
        {
          "id": "MDRW2100034103.1.17",
          "form": "근데 꿔바로우는 좀 비싼거같기도 하고 ㅋㅋ",
          "original_form": "근데 꿔바로우는 좀 비싼거같기도 하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100034103.1.18",
          "form": "저도 가격 때문에 혼자서 먹을 때는 부담스럽더라고요ㅠ",
          "original_form": "저도 가격 때문에 혼자서 먹을 때는 부담스럽더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100034103.1.19",
          "form": "약간 1인분씩 해서 싸게 팔았으면 좋겠어요",
          "original_form": "약간 1인분씩 해서 싸게 팔았으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100034103.1.20",
          "form": "한 5천원 정도 ㅋㅋ",
          "original_form": "한 5천원 정도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100034103.1.21",
          "form": "아 그럼 너무 좋을 거 같아요ㅠㅜ",
          "original_form": "아 그럼 너무 좋을 거 같아요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100034103.1.22",
          "form": "그럼 더 잘 팔릴 텐데",
          "original_form": "그럼 더 잘 팔릴 텐데",
          "speaker_id": "1",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100034103.1.23",
          "form": "가게 하나 차려야겠어요 ㅋㅋㅋ",
          "original_form": "가게 하나 차려야겠어요 ㅋㅋㅋ",
