      "utterance": [
        {
          "id": "MDRW2100037965.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100037965.1.2",
          "form": "기억에 남는 기념품 있나요?",
          "original_form": "기억에 남는 기념품 있나요?",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100037965.1.3",
          "form": "여행 가볍게 다녀오는건 좋아해요",
          "original_form": "여행 가볍게 다녀오는건 좋아해요",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100037965.1.4",
          "form": "그리고 기념품을 따로 사지는 않아요",
          "original_form": "그리고 기념품을 따로 사지는 않아요",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100037965.1.5",
          "form": "오.. 기념품 안모으는 타입이군요",
          "original_form": "오.. 기념품 안모으는 타입이군요",
          "speaker_id": "1",
          "time": "20210902 06:16"
        },
        {
          "id": "MDRW2100037965.1.6",
          "form": "사실 기념품이란 예쁜 쓰레기죠..ㅎㅎ",
          "original_form": "사실 기념품이란 예쁜 쓰레기죠..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 06:17"
        },
        {
          "id": "MDRW2100037965.1.7",
          "form": "맞아요... 괜히 비싸기만 하고",
          "original_form": "맞아요... 괜히 비싸기만 하고",
          "speaker_id": "2",
          "time": "20210902 07:38"
        },
        {
          "id": "MDRW2100037965.1.8",
          "form": "쓸데도 없죠... 관상용이랄까",
          "original_form": "쓸데도 없죠... 관상용이랄까",
          "speaker_id": "2",
          "time": "20210902 07:39"
        },
        {
          "id": "MDRW2100037965.1.9",
          "form": "저도 한때 기념품 욕심이 있었는데.. 점점 사는 일이 줄고 있어요..ㅎㅎ",
          "original_form": "저도 한때 기념품 욕심이 있었는데.. 점점 사는 일이 줄고 있어요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 07:39"
        },
        {
          "id": "MDRW2100037965.1.10",
          "form": "예전에 어떤거 사셨어요?",
          "original_form": "예전에 어떤거 사셨어요?",
          "speaker_id": "2",
          "time": "20210902 07:40"
        },
        {
          "id": "MDRW2100037965.1.11",
          "form": "그 자석들 엄청 많아요",
          "original_form": "그 자석들 엄청 많아요",
          "speaker_id": "1",
          "time": "20210902 07:41"
        },
        {
          "id": "MDRW2100037965.1.12",
          "form": "여행지마다 파는 자석들이요..ㅎㅎ",
          "original_form": "여행지마다 파는 자석들이요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 07:41"
        },
        {
          "id": "MDRW2100037965.1.13",
          "form": "아하ㅋㅋ 그건 저도 몇개 있네요",
          "original_form": "아하ㅋㅋ 그건 저도 몇개 있네요",
          "speaker_id": "2",
          "time": "20210902 07:42"
        },
        {
          "id": "MDRW2100037965.1.14",
          "form": "물론 제가 산건 아니지만",
          "original_form": "물론 제가 산건 아니지만",
          "speaker_id": "2",
          "time": "20210902 07:42"
        },
        {
          "id": "MDRW2100037965.1.15",
          "form": "그게 뭐라고 그동안 샀을까요? 참내..",
          "original_form": "그게 뭐라고 그동안 샀을까요? 참내..",
          "speaker_id": "1",
          "time": "20210902 07:43"
        },
        {
          "id": "MDRW2100037965.1.16",
          "form": "냉장고에 많이들 붙어있죠",
          "original_form": "냉장고에 많이들 붙어있죠",
          "speaker_id": "2",
          "time": "20210902 07:43"
        },
        {
          "id": "MDRW2100037965.1.17",
          "form": "거기 아니면 붙일데도 없지만...",
          "original_form": "거기 아니면 붙일데도 없지만...",
          "speaker_id": "2",
          "time": "20210902 07:44"
        },
        {
          "id": "MDRW2100037965.1.18",
          "form": "ㅎㅎ 그니까요.. 냉장고 자석인데.. 전기만 잡아먹고",
          "original_form": "ㅎㅎ 그니까요.. 냉장고 자석인데.. 전기만 잡아먹고",
          "speaker_id": "1",
          "time": "20210902 07:46"
        },
        {
          "id": "MDRW2100037965.1.19",
          "form": "그거 붙이면 전기가 나가는군요",
          "original_form": "그거 붙이면 전기가 나가는군요",
          "speaker_id": "2",
          "time": "20210902 07:48"
        },
        {
          "id": "MDRW2100037965.1.20",
          "form": "설마 전자석인가",
          "original_form": "설마 전자석인가",
          "speaker_id": "2",
          "time": "20210902 07:48"
        },
        {
          "id": "MDRW2100037965.1.21",
          "form": "전기를 많이 먹는대요.. 안붙일때보다",
          "original_form": "전기를 많이 먹는대요.. 안붙일때보다",
          "speaker_id": "1",
          "time": "20210902 07:49"
        },
        {
          "id": "MDRW2100037965.1.22",
          "form": "아하 처음 알았어요",
          "original_form": "아하 처음 알았어요",
          "speaker_id": "2",
          "time": "20210902 07:49"
        },
        {
          "id": "MDRW2100037965.1.23",
          "form": "그래서 결국 냉장고에도 못붙이고  서랍행이네요",
          "original_form": "그래서 결국 냉장고에도 못붙이고  서랍행이네요",
          "speaker_id": "1",
          "time": "20210902 07:51"
        }
      ]
