      "utterance": [
        {
          "id": "MDRW2100009005.1.1",
          "form": "저녁은 드셨어요?",
          "original_form": "저녁은 드셨어요?",
          "speaker_id": "2",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100009005.1.2",
          "form": "헉 늦게 봤네요ㅠㅠ",
          "original_form": "헉 늦게 봤네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 02:18"
        },
        {
          "id": "MDRW2100009005.1.3",
          "form": "저는 점심 늦게 먹어서 배가 딱히 안 고파서 그냥 군것질로 배 채웠어요!",
          "original_form": "저는 점심 늦게 먹어서 배가 딱히 안 고파서 그냥 군것질로 배 채웠어요!",
          "speaker_id": "1",
          "time": "20210823 02:19"
        },
        {
          "id": "MDRW2100009005.1.4",
          "form": "앗 그랬군요.. 저는 군것질로 끼니 잘 못 때우고 무조건 밥 먹어요ㅋㅋㅋ",
          "original_form": "앗 그랬군요.. 저는 군것질로 끼니 잘 못 때우고 무조건 밥 먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:37"
        },
        {
          "id": "MDRW2100009005.1.5",
          "form": "오늘 저녁메뉴는 정하셨어요?",
          "original_form": "오늘 저녁메뉴는 정하셨어요?",
          "speaker_id": "2",
          "time": "20210823 16:37"
        },
        {
          "id": "MDRW2100009005.1.6",
          "form": "저 어제도... 안 먹었어요ㅋㅋㅋ 야식으로 컵라면 먹고ㅠㅠ",
          "original_form": "저 어제도... 안 먹었어요ㅋㅋㅋ 야식으로 컵라면 먹고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100009005.1.7",
          "form": "식습관 고쳐야 하는데",
          "original_form": "식습관 고쳐야 하는데",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100009005.1.8",
          "form": "앗 체질마다 다르긴 하지만ㅠㅠ 몸에 안 좋을 수 있을 거  같아요ㅠㅠ",
          "original_form": "앗 체질마다 다르긴 하지만ㅠㅠ 몸에 안 좋을 수 있을 거  같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100009005.1.9",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100009005.1.10",
          "form": "젊을 때 먹어야죠ㅋㅋㅋㅋ",
          "original_form": "젊을 때 먹어야죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100009005.1.11",
          "form": "그렇긴 하죠ㅋㅋㅋ",
          "original_form": "그렇긴 하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100009005.1.12",
          "form": "어떤 음식 제일 좋아하시나요?",
          "original_form": "어떤 음식 제일 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100009005.1.13",
          "form": "저는 너무 많은데",
          "original_form": "저는 너무 많은데",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100009005.1.14",
          "form": "역시 한식 제일 좋아해요",
          "original_form": "역시 한식 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100009005.1.15",
          "form": "밥 고기 김치 찌개",
          "original_form": "밥 고기 김치 찌개",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100009005.1.16",
          "form": "맞아요 한식 너무 맛있죠!",
          "original_form": "맞아요 한식 너무 맛있죠!",
          "speaker_id": "1",
          "time": "20210828 01:44"
        },
        {
          "id": "MDRW2100009005.1.17",
          "form": "건강하기도 하고",
          "original_form": "건강하기도 하고",
          "speaker_id": "1",
          "time": "20210828 01:44"
        },
        {
          "id": "MDRW2100009005.1.18",
          "form": "네 진짜 한국사람이어서 다행이에요",
          "original_form": "네 진짜 한국사람이어서 다행이에요",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100009005.1.19",
          "form": "음식만은.. ㅋㅋㅋ우리나라 음식 너무 맛있어요ㅜㅠ",
          "original_form": "음식만은.. ㅋㅋㅋ우리나라 음식 너무 맛있어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100009005.1.20",
          "form": "맞아요 해외여행 가면 입에 안 맞는 경우도 많아서ㅠㅠ",
          "original_form": "맞아요 해외여행 가면 입에 안 맞는 경우도 많아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100009005.1.21",
          "form": "맞아요ㅠㅠ 저도 해외여행 가면 엄청 맛있게 먹지는 못하거든요",
          "original_form": "맞아요ㅠㅠ 저도 해외여행 가면 엄청 맛있게 먹지는 못하거든요",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100009005.1.22",
          "form": "매일 피자 먹고 그러면..",
          "original_form": "매일 피자 먹고 그러면..",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100009005.1.23",
          "form": "맞아요 그래서 여행가면 컵라면 같은 거라도 꼭 챙겨가요ㅠㅠ",
          "original_form": "맞아요 그래서 여행가면 컵라면 같은 거라도 꼭 챙겨가요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:07"
        }
      ]
    }
  ]
