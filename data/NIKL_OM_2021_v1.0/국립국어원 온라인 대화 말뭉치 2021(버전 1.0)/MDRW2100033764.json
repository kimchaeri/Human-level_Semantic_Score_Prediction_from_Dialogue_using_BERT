      "utterance": [
        {
          "id": "MDRW2100033764.1.1",
          "form": "윙잇은 뺏어갔나요?",
          "original_form": "윙잇은 뺏어갔나요?",
          "speaker_id": "1",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100033764.1.2",
          "form": "아뇨 다행히 살아있어요ㅎ",
          "original_form": "아뇨 다행히 살아있어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100033764.1.3",
          "form": "근데 적용되는거지는 낼 해봐야알듯요",
          "original_form": "근데 적용되는거지는 낼 해봐야알듯요",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100033764.1.4",
          "form": "아 근데 아까 언뜻 글보니 만료어쩌고 본것 같은데 다른거였나봐요",
          "original_form": "아 근데 아까 언뜻 글보니 만료어쩌고 본것 같은데 다른거였나봐요",
          "speaker_id": "1",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100033764.1.5",
          "form": "저도 그거보고 들어간거였어요",
          "original_form": "저도 그거보고 들어간거였어요",
          "speaker_id": "2",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100033764.1.6",
          "form": "낚였네요 ㅋㅋㅋㅋㅋ",
          "original_form": "낚였네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100033764.1.7",
          "form": "만료가아니고 막혔단얘기아니였을까요?",
          "original_form": "만료가아니고 막혔단얘기아니였을까요?",
          "speaker_id": "2",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100033764.1.8",
          "form": "근데 사진에 쿠폰이 줄이 쫙 있었는데 다른쿠폰이었나봐요",
          "original_form": "근데 사진에 쿠폰이 줄이 쫙 있었는데 다른쿠폰이었나봐요",
          "speaker_id": "1",
          "time": "20210825 01:10"
        },
        {
          "id": "MDRW2100033764.1.9",
          "form": "지금 암거나 담고 해보니 아직 적용되네요",
          "original_form": "지금 암거나 담고 해보니 아직 적용되네요",
          "speaker_id": "2",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100033764.1.10",
          "form": "낼후딱 사는걸로ㅎ",
          "original_form": "낼후딱 사는걸로ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100033764.1.11",
          "form": "생각해놓으신거 있으세요?",
          "original_form": "생각해놓으신거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100033764.1.12",
          "form": "백순대 밀키트가 무배거든요ㅎ",
          "original_form": "백순대 밀키트가 무배거든요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100033764.1.13",
          "form": "업체배송",
          "original_form": "업체배송",
          "speaker_id": "2",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100033764.1.14",
          "form": "다른건 더사야되서 패스할려구요",
          "original_form": "다른건 더사야되서 패스할려구요",
          "speaker_id": "2",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100033764.1.15",
          "form": "와아아아 얼마에요???",
          "original_form": "와아아아 얼마에요???",
          "speaker_id": "1",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100033764.1.16",
          "form": "현명한 소비입니다",
          "original_form": "현명한 소비입니다",
          "speaker_id": "1",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100033764.1.17",
          "form": "9800원인가ㅎ",
          "original_form": "9800원인가ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100033764.1.18",
          "form": "두개사고 사리추가함될듯해욪ㅎ",
          "original_form": "두개사고 사리추가함될듯해욪ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:14"
        },
        {
          "id": "MDRW2100033764.1.19",
          "form": "2만원쪼끔 넘기고",
          "original_form": "2만원쪼끔 넘기고",
          "speaker_id": "2",
          "time": "20210825 01:14"
        },
        {
          "id": "MDRW2100033764.1.20",
          "form": "좋지용 맛있겠어요ㅠㅠ 듣다보니 더 아쉽네요ㅠㅠ",
