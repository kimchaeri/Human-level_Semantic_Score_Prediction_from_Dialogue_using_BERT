      "utterance": [
        {
          "id": "MDRW2100015284.1.1",
          "form": "종교가 있으세요?",
          "original_form": "종교가 있으세요?",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015284.1.2",
          "form": "전 있다가 안다닌 지 꽤 됐어요ㅎㅎ",
          "original_form": "전 있다가 안다닌 지 꽤 됐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015284.1.3",
          "form": "어떤 종교를 믿으셨나요?",
          "original_form": "어떤 종교를 믿으셨나요?",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015284.1.4",
          "form": "저 교회 다니다가 말았어요! name2 님은 종교 있으신가요?",
          "original_form": "저 교회 다니다가 말았어요! &name2& 님은 종교 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015284.1.5",
          "form": "저는 지금 교회 다녀요!",
          "original_form": "저는 지금 교회 다녀요!",
          "speaker_id": "2",
          "time": "20210826 01:11"
        },
        {
          "id": "MDRW2100015284.1.6",
          "form": "오 모태신앙이신가요?",
          "original_form": "오 모태신앙이신가요?",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015284.1.7",
          "form": "네네 3대째 모태신앙이에요",
          "original_form": "네네 3대째 모태신앙이에요",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100015284.1.8",
          "form": "오오 대박이에요! 전 미션스쿨도 다니고 교회도 다녔는데 신앙심이 막 생기진 않더라구요...",
          "original_form": "오오 대박이에요! 전 미션스쿨도 다니고 교회도 다녔는데 신앙심이 막 생기진 않더라구요...",
          "speaker_id": "1",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100015284.1.9",
          "form": "모태신앙도 그닥 신앙이 있지는 않아요...ㅎㅎ",
          "original_form": "모태신앙도 그닥 신앙이 있지는 않아요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:14"
        },
        {
          "id": "MDRW2100015284.1.10",
          "form": "앗...ㅋㅋㅋㅋ",
          "original_form": "앗...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:25"
        },
        {
          "id": "MDRW2100015284.1.11",
          "form": "일요일마다 교회가는 것 힘들지 않으세요?",
          "original_form": "일요일마다 교회가는 것 힘들지 않으세요?",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100015284.1.12",
          "form": "요즘엔 온라인 실시간 중계라 편해졌어요ㅌㅌ",
          "original_form": "요즘엔 온라인 실시간 중계라 편해졌어요ㅌㅌ",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100015284.1.13",
          "form": "아! 그렇군요 종교도 비대면으로 하는게 확실히 편하죠ㅋㅋㅋ",
          "original_form": "아! 그렇군요 종교도 비대면으로 하는게 확실히 편하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100015284.1.14",
          "form": "일단 준비시간과 이동시간이 절약되니까요ㅎㅎ",
          "original_form": "일단 준비시간과 이동시간이 절약되니까요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:31"
        }
      ]
