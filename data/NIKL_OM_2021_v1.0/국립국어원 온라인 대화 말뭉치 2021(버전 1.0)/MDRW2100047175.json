      "utterance": [
        {
          "id": "MDRW2100047175.1.1",
          "form": "거기도 힘들더라구요",
          "original_form": "거기도 힘들더라구요",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100047175.1.2",
          "form": "임용준비해야하니까요",
          "original_form": "임용준비해야하니까요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100047175.1.3",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100047175.1.4",
          "form": "그리고 티오가 잘안나요 ㅠㅠㅋㅋㅋㅋ",
          "original_form": "그리고 티오가 잘안나요 ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100047175.1.5",
          "form": "임용포기하면 전공살려 일하기 힘들기도 하고요...",
          "original_form": "임용포기하면 전공살려 일하기 힘들기도 하고요...",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100047175.1.6",
          "form": "ㅜㅜㅜ맞아요......",
          "original_form": "ㅜㅜㅜ맞아요......",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100047175.1.7",
          "form": "교육공무원인가",
          "original_form": "교육공무원인가",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100047175.1.8",
          "form": "그런쪽으로도 빠지던데",
          "original_form": "그런쪽으로도 빠지던데",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100047175.1.9",
          "form": "아 공무직요??",
          "original_form": "아 공무직요??",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100047175.1.10",
          "form": "그것도 좋죠",
          "original_form": "그것도 좋죠",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100047175.1.11",
          "form": "네 ㄱ그쪽도 셤쳐야지요ㅜ 할게많음 ㅜ",
          "original_form": "네 ㄱ그쪽도 셤쳐야지요ㅜ 할게많음 ㅜ",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100047175.1.12",
          "form": "제친구는 과외하기도 하더래요",
          "original_form": "제친구는 과외하기도 하더래요",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100047175.1.13",
          "form": "과외도 좋죸ㅋㅋㅋ큐ㅠ",
          "original_form": "과외도 좋죸ㅋㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100047175.1.14",
          "form": "대학생활하시면서 동아리활동은 하셨어요?",
          "original_form": "대학생활하시면서 동아리활동은 하셨어요?",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100047175.1.15",
          "form": "만화동아리요",
          "original_form": "만화동아리요",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100047175.1.16",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100047175.1.17",
          "form": "오 저는 언론사요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 저는 언론사요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100047175.1.18",
          "form": "햐",
          "original_form": "햐",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100047175.1.19",
          "form": "멋잇네요",
          "original_form": "멋잇네요",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100047175.1.20",
          "form": "만화동아리 재밌을거같아요",
          "original_form": "만화동아리 재밌을거같아요",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100047175.1.21",
          "form": "만화 그리는 동아리였나요?? 아니면 읽는 거??",
          "original_form": "만화 그리는 동아리였나요?? 아니면 읽는 거??",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100047175.1.22",
          "form": "그리는쪽인데 열심히 안했어요",
          "original_form": "그리는쪽인데 열심히 안했어요",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100047175.1.23",
          "form": "오... 그리는 동아리면",
          "original_form": "오... 그리는 동아리면",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100047175.1.24",
          "form": "전 절대 못드네요...",
          "original_form": "전 절대 못드네요...",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100047175.1.25",
          "form": "ㅎㅎ...ㅠ",
          "original_form": "ㅎㅎ...ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100047175.1.26",
          "form": "원래 동아리는 동아리사람들이랑 친해져서 맛있는거 먹으러다니는 낙이죠...",
          "original_form": "원래 동아리는 동아리사람들이랑 친해져서 맛있는거 먹으러다니는 낙이죠...",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100047175.1.27",
          "form": "저야말로 신문에 글쓰는 활동절대못해요",
          "original_form": "저야말로 신문에 글쓰는 활동절대못해요",
