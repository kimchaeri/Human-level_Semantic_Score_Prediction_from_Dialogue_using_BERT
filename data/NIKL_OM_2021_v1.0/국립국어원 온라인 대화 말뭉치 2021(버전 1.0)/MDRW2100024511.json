      "utterance": [
        {
          "id": "MDRW2100024511.1.1",
          "form": "어제 주문한거 언제옴?  후기궁금",
          "original_form": "어제 주문한거 언제옴?  후기궁금",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024511.1.2",
          "form": "맞다 옥션쿠폰 오늘까지네",
          "original_form": "맞다 옥션쿠폰 오늘까지네",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024511.1.3",
          "form": "어제 내가 뭐샀더라 요즘 뭐사고 산게 기억이  안남 ㅋㅋ",
          "original_form": "어제 내가 뭐샀더라 요즘 뭐사고 산게 기억이  안남 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024511.1.4",
          "form": "ㅎㅎㅎ폭립이 그리 궁금하다ㅋ",
          "original_form": "ㅎㅎㅎ폭립이 그리 궁금하다ㅋ",
          "speaker_id": "2",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100024511.1.5",
          "form": "아 그거 수욜 올거같더라",
          "original_form": "아 그거 수욜 올거같더라",
          "speaker_id": "1",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100024511.1.6",
          "form": "와 내일이네ㅋㅋㅋ",
          "original_form": "와 내일이네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:43"
        },
        {
          "id": "MDRW2100024511.1.7",
          "form": "나한테오느것도아닌데 내가 다 신나네ㅋ",
          "original_form": "나한테오느것도아닌데 내가 다 신나네ㅋ",
          "speaker_id": "2",
          "time": "20210831 12:43"
        },
        {
          "id": "MDRW2100024511.1.8",
          "form": "어차피 수욜은 못 먹고 주말이나되야 먹을듯",
          "original_form": "어차피 수욜은 못 먹고 주말이나되야 먹을듯",
          "speaker_id": "1",
          "time": "20210831 13:05"
        },
        {
          "id": "MDRW2100024511.1.9",
          "form": "나 오늘의집에서 머그컵이랑 유리컵샀당",
          "original_form": "나 오늘의집에서 머그컵이랑 유리컵샀당",
          "speaker_id": "1",
          "time": "20210831 13:05"
        },
        {
          "id": "MDRW2100024511.1.10",
          "form": "유리컵 깨먹어서 ㅋㅋ",
          "original_form": "유리컵 깨먹어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:05"
        },
        {
          "id": "MDRW2100024511.1.11",
          "form": "오늘의집\r\n들어본거 같은데ㅋ\r\n유리컵 강화로?",
          "original_form": "오늘의집\r\n들어본거 같은데ㅋ\r\n유리컵 강화로?",
          "speaker_id": "2",
          "time": "20210831 14:35"
        },
        {
          "id": "MDRW2100024511.1.12",
          "form": "아니 걍 유리컵 그 뚜껑있는거 유리컵",
          "original_form": "아니 걍 유리컵 그 뚜껑있는거 유리컵",
          "speaker_id": "1",
          "time": "20210831 14:37"
        },
        {
          "id": "MDRW2100024511.1.13",
          "form": "사기?",
          "original_form": "사기?",
          "speaker_id": "2",
          "time": "20210831 14:37"
        },
        {
          "id": "MDRW2100024511.1.14",
          "form": "아니 유리컵!",
          "original_form": "아니 유리컵!",
          "speaker_id": "1",
          "time": "20210831 14:37"
        },
        {
          "id": "MDRW2100024511.1.15",
          "form": "ㅎㅎㅎ모양 상상안돼너",
          "original_form": "ㅎㅎㅎ모양 상상안돼너",
          "speaker_id": "2",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100024511.1.16",
          "form": "유리컵이랑 포인트 좀 남아서 머그컵도 하나사고",
          "original_form": "유리컵이랑 포인트 좀 남아서 머그컵도 하나사고",
          "speaker_id": "1",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100024511.1.17",
          "form": "포인트 많았나보네ㅎ",
          "original_form": "포인트 많았나보네ㅎ",
          "speaker_id": "2",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100024511.1.18",
          "form": "유리컵이 4900원 머그가 2900원",
          "original_form": "유리컵이 4900원 머그가 2900원",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100024511.1.19",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:39"
        },
        {
          "id": "MDRW2100024511.1.20",
          "form": "800원주고 샀엉 칠천포인트 받아서",
          "original_form": "800원주고 샀엉 칠천포인트 받아서",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100024511.1.21",
          "form": "오ㅎㅎㅎ",
          "original_form": "오ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100024511.1.22",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100024511.1.23",
          "form": "잘했다",
          "original_form": "잘했다",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100024511.1.24",
          "form": "ㅇㅇ 보냉백도 하나삼 6900원이라",
          "original_form": "ㅇㅇ 보냉백도 하나삼 6900원이라",
          "speaker_id": "1",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100024511.1.25",
          "form": "요즘 보냉백 이쁜거 많더라",
          "original_form": "요즘 보냉백 이쁜거 많더라",
          "speaker_id": "2",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100024511.1.26",
          "form": "난 귀찮아서 잘 안쓰지만ㅎ\r\n회사언니 테아 보냉백에 넣고다니더라고ㅎ",
          "original_form": "난 귀찮아서 잘 안쓰지만ㅎ\r\n회사언니 테아 보냉백에 넣고다니더라고ㅎ",
          "speaker_id": "2",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100024511.1.27",
          "form": "아 그것도 괜찮네! 샐러드같은거 신선도 유지되겠다",
          "original_form": "아 그것도 괜찮네! 샐러드같은거 신선도 유지되겠다",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100024511.1.28",
          "form": "그런가봐ㅎ 다이소에도 보냉백 종류 많더라",
          "original_form": "그런가봐ㅎ 다이소에도 보냉백 종류 많더라",
          "speaker_id": "2",
          "time": "20210831 14:58"
        },
        {
          "id": "MDRW2100024511.1.29",
          "form": "나 토욜 다이소 치실 치간칫솔 구매함",
          "original_form": "나 토욜 다이소 치실 치간칫솔 구매함",
          "speaker_id": "2",
          "time": "20210831 14:58"
        },
        {
          "id": "MDRW2100024511.1.30",
          "form": "ㅋㅋㅋ종류많아서 고민하다 여러개사버림ㅋ",
          "original_form": "ㅋㅋㅋ종류많아서 고민하다 여러개사버림ㅋ",
          "speaker_id": "2",
          "time": "20210831 14:58"
        }
      ]
