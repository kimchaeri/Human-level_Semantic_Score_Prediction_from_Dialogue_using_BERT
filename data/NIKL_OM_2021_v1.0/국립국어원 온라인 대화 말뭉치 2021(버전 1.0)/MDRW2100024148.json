      "utterance": [
        {
          "id": "MDRW2100024148.1.1",
          "form": "군대?",
          "original_form": "군대?",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024148.1.2",
          "form": "나는 여자인데..",
          "original_form": "나는 여자인데..",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024148.1.3",
          "form": "군대? 저도 여자인데요",
          "original_form": "군대? 저도 여자인데요",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024148.1.4",
          "form": "아는거 있으신가요",
          "original_form": "아는거 있으신가요",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100024148.1.5",
          "form": "없어여 ㅋㅋ",
          "original_form": "없어여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024148.1.6",
          "form": "ㅋㅋㅋ 저도 있을리가 없는데여 밥이 부실하다고 뉴스에 나왔었어요",
          "original_form": "ㅋㅋㅋ 저도 있을리가 없는데여 밥이 부실하다고 뉴스에 나왔었어요",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024148.1.7",
          "form": "군대에서 밥은 제대로 주지 나쁜놈들이에요",
          "original_form": "군대에서 밥은 제대로 주지 나쁜놈들이에요",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024148.1.8",
          "form": "맞아여 그게 다 군대비리 때문이죠",
          "original_form": "맞아여 그게 다 군대비리 때문이죠",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024148.1.9",
          "form": "그러니까요 밥도 제대로 안주고 돈도 쥐꼬리만큼 주니까 진짜 가기 싫을거 같아요",
          "original_form": "그러니까요 밥도 제대로 안주고 돈도 쥐꼬리만큼 주니까 진짜 가기 싫을거 같아요",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024148.1.10",
          "form": "솔직히 인건비안들고, 이익 낼 이유도 없는데, 군대비리 없어져야해요",
          "original_form": "솔직히 인건비안들고, 이익 낼 이유도 없는데, 군대비리 없어져야해요",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024148.1.11",
          "form": "그러니까요 여잔데도 솔직히 좀 군대 그래요",
          "original_form": "그러니까요 여잔데도 솔직히 좀 군대 그래요",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024148.1.12",
          "form": "여군 폭행 폭력 문제도 그렇고...",
          "original_form": "여군 폭행 폭력 문제도 그렇고...",
          "speaker_id": "2",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024148.1.13",
          "form": "할말 없을줄 알았는데 그래도 군대에 대해 말할게 있긴 하네요 ㅋㅋㅋㅋ",
          "original_form": "할말 없을줄 알았는데 그래도 군대에 대해 말할게 있긴 하네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024148.1.14",
          "form": "여군 폭행 특히 성적인 부분은 너무 충격이였어요",
          "original_form": "여군 폭행 특히 성적인 부분은 너무 충격이였어요",
          "speaker_id": "1",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024148.1.15",
          "form": "공군,해군,육군 다 그런일이 있으니까요",
          "original_form": "공군,해군,육군 다 그런일이 있으니까요",
          "speaker_id": "1",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024148.1.16",
          "form": "네 그게 만연하다는게 더 충격이긴 했어요",
          "original_form": "네 그게 만연하다는게 더 충격이긴 했어요",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024148.1.17",
          "form": "그냥 그런 놈들 잘라버리고 싶다",
          "original_form": "그냥 그런 놈들 잘라버리고 싶다",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024148.1.18",
          "form": "라고 말하면 안되겠죠",
          "original_form": "라고 말하면 안되겠죠",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024148.1.19",
          "form": "계급도 자르고, 그곳(!)도 잘랐으면...좋겠어요",
          "original_form": "계급도 자르고, 그곳(!)도 잘랐으면...좋겠어요",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024148.1.20",
          "form": "네 둘다 잘라버렸으면 좋겠어요!!",
          "original_form": "네 둘다 잘라버렸으면 좋겠어요!!",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024148.1.21",
          "form": "주변에 직업군인이랑 결혼한 지인들 있는데, 고생이 많더라구요ㅎㅎ",
          "original_form": "주변에 직업군인이랑 결혼한 지인들 있는데, 고생이 많더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024148.1.22",
          "form": "남편 계급이 부인 계급이라는거 기사로만 접했는데, 실제로 일어나는 일이라해서 충격이였어요",
          "original_form": "남편 계급이 부인 계급이라는거 기사로만 접했는데, 실제로 일어나는 일이라해서 충격이였어요",
          "speaker_id": "1",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024148.1.23",
          "form": "직업군인이랑 결혼하신 분이 있으시구나 계급이라니 중세시대같네요",
          "original_form": "직업군인이랑 결혼하신 분이 있으시구나 계급이라니 중세시대같네요",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024148.1.24",
          "form": "네...심지어 관사에 살고 있어요ㅎㅎ",
          "original_form": "네...심지어 관사에 살고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024148.1.25",
          "form": "관사가 낡아서 힘들다고 하더라구요",
          "original_form": "관사가 낡아서 힘들다고 하더라구요",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024148.1.26",
          "form": "고향떠나서 타지생활도 힘들고요",
          "original_form": "고향떠나서 타지생활도 힘들고요",
          "speaker_id": "1",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100024148.1.27",
          "form": "진짜 고생이시겠다 듣기만 해도 그러네요",
          "original_form": "진짜 고생이시겠다 듣기만 해도 그러네요",
          "speaker_id": "2",
          "time": "20210830 22:54"
        }
      ]
