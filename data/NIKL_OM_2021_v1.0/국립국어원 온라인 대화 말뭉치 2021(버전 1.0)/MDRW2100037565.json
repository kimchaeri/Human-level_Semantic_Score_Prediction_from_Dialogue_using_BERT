      "utterance": [
        {
          "id": "MDRW2100037565.1.1",
          "form": "맥주까며 보면 딱일거 같아요 ㅋㅋ",
          "original_form": "맥주까며 보면 딱일거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100037565.1.2",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100037565.1.3",
          "form": "육성재랑 나온",
          "original_form": "육성재랑 나온",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100037565.1.4",
          "form": "아뭐더라 포차?",
          "original_form": "아뭐더라 포차?",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100037565.1.5",
          "form": "포장마차 나오고 그랬는데",
          "original_form": "포장마차 나오고 그랬는데",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100037565.1.6",
          "form": "쌍갑포차?",
          "original_form": "쌍갑포차?",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100037565.1.7",
          "form": "아! 네네",
          "original_form": "아! 네네",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100037565.1.8",
          "form": "그거 봤어요 ㅋㅋㅋ",
          "original_form": "그거 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100037565.1.9",
          "form": "보셨어요? ㅋㅋ",
          "original_form": "보셨어요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100037565.1.10",
          "form": "혼술남녀하니까 갑자기 떠오름",
          "original_form": "혼술남녀하니까 갑자기 떠오름",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100037565.1.11",
          "form": "아 저보다 많이보셨는데요? ㅋㅋㅋㅋ이것도 주변에 보는사람없었는데",
          "original_form": "아 저보다 많이보셨는데요? ㅋㅋㅋㅋ이것도 주변에 보는사람없었는데",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100037565.1.12",
          "form": "그건어때요?",
          "original_form": "그건어때요?",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100037565.1.13",
          "form": "재밌게보긴했는데 강추 까진 아니에요 ㅋㅋㅋ",
          "original_form": "재밌게보긴했는데 강추 까진 아니에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100037565.1.14",
          "form": "제가 원작을 안봐서 황정음이 그 역에 최선이었을까? 라는 생각이 많이들더라구요...",
          "original_form": "제가 원작을 안봐서 황정음이 그 역에 최선이었을까? 라는 생각이 많이들더라구요...",
          "speaker_id": "2",
          "time": "20210901 11:54"
        },
        {
          "id": "MDRW2100037565.1.15",
          "form": "연기력논란이 있던거 같기도 해요",
          "original_form": "연기력논란이 있던거 같기도 해요",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100037565.1.16",
          "form": "와 하석진 이거찍을때 진짜 좋았겠어요",
          "original_form": "와 하석진 이거찍을때 진짜 좋았겠어요",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100037565.1.17",
          "form": "술 좋아하는데 첫씬부터 맥주를 캬..",
          "original_form": "술 좋아하는데 첫씬부터 맥주를 캬..",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100037565.1.18",
          "form": "아...진짜 술이 아니겠구나........",
          "original_form": "아...진짜 술이 아니겠구나........",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100037565.1.19",
          "form": "하석진 술마시는 장면 거의 매 회 나온거같아요 ㅋㅋㅋㅋㅋ 약간 차도남 느낌으로다가 ㅋㅋㅋ",
          "original_form": "하석진 술마시는 장면 거의 매 회 나온거같아요 ㅋㅋㅋㅋㅋ 약간 차도남 느낌으로다가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:10"
        },
        {
          "id": "MDRW2100037565.1.20",
          "form": "지금 1화 봤는데 벌써 흥미진진!!",
          "original_form": "지금 1화 봤는데 벌써 흥미진진!!",
          "speaker_id": "1",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100037565.1.21",
          "form": "거의 식사를합시다 급인데요?",
          "original_form": "거의 식사를합시다 급인데요?",
          "speaker_id": "1",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100037565.1.22",
          "form": "맞아요맞아요 약간 그런류로 나왔어요 ㅋㅋㅋ  노량진얘기도 재밌고 ㅋㅋ",
          "original_form": "맞아요맞아요 약간 그런류로 나왔어요 ㅋㅋㅋ  노량진얘기도 재밌고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:34"
        },
        {
          "id": "MDRW2100037565.1.23",
          "form": "왘ㅋㅋㅋ이거 실시간으로 봤던분들",
          "original_form": "왘ㅋㅋㅋ이거 실시간으로 봤던분들",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100037565.1.24",
          "form": "완전 고문이었을거같아요",
          "original_form": "완전 고문이었을거같아요",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100037565.1.25",
          "form": "안주랑 술이 끊임없넼ㅋㅋㅋ",
          "original_form": "안주랑 술이 끊임없넼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:02"
        }
      ]
