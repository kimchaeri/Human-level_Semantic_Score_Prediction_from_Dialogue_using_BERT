      "utterance": [
        {
          "id": "MDRW2100040855.1.1",
          "form": "음 식당마다 다른거 같은데",
          "original_form": "음 식당마다 다른거 같은데",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100040855.1.2",
          "form": "기본은 안 넣고 추가요청해야 더 넣어주는 곳도 잇고",
          "original_form": "기본은 안 넣고 추가요청해야 더 넣어주는 곳도 잇고",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100040855.1.3",
          "form": "그냥 기본으로 치즈 들어가는 곳도 있고 그렇더라구요",
          "original_form": "그냥 기본으로 치즈 들어가는 곳도 있고 그렇더라구요",
          "speaker_id": "2",
          "time": "20210905 10:50"
        },
        {
          "id": "MDRW2100040855.1.4",
          "form": "치즈사랑해요ㅋㅋㅋ",
          "original_form": "치즈사랑해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:59"
        },
        {
          "id": "MDRW2100040855.1.5",
          "form": "마자요 치즈는 들어갈수록 맛있어지죠 ㅋㅋㅋ",
          "original_form": "마자요 치즈는 들어갈수록 맛있어지죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:14"
        },
        {
          "id": "MDRW2100040855.1.6",
          "form": "그니깐요 치즈들어가 있으면 좋아요!!",
          "original_form": "그니깐요 치즈들어가 있으면 좋아요!!",
          "speaker_id": "1",
          "time": "20210906 06:07"
        },
        {
          "id": "MDRW2100040855.1.7",
          "form": "어제 치킨 기프티콘 썼어요",
          "original_form": "어제 치킨 기프티콘 썼어요",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100040855.1.8",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100040855.1.9",
          "form": "어땠어요???먹을만했어요?",
          "original_form": "어땠어요???먹을만했어요?",
          "speaker_id": "1",
          "time": "20210906 16:01"
        },
        {
          "id": "MDRW2100040855.1.10",
          "form": "넹넹 자극적인 맛",
          "original_form": "넹넹 자극적인 맛",
          "speaker_id": "2",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100040855.1.11",
          "form": "자주 시켜먹던 건 아니라서 오랜만에 먹어봤어용",
          "original_form": "자주 시켜먹던 건 아니라서 오랜만에 먹어봤어용",
          "speaker_id": "2",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100040855.1.12",
          "form": "맛있었겠당",
          "original_form": "맛있었겠당",
          "speaker_id": "1",
          "time": "20210906 16:24"
        },
        {
          "id": "MDRW2100040855.1.13",
          "form": "저도 먹고싶어요ㅠㅠ",
          "original_form": "저도 먹고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 16:24"
        },
        {
          "id": "MDRW2100040855.1.14",
          "form": "치킨....",
          "original_form": "치킨....",
          "speaker_id": "1",
          "time": "20210906 16:29"
        },
        {
          "id": "MDRW2100040855.1.15",
          "form": "오늘저녁 치킨먹고싶네요",
          "original_form": "오늘저녁 치킨먹고싶네요",
          "speaker_id": "1",
          "time": "20210906 16:29"
        },
        {
          "id": "MDRW2100040855.1.16",
          "form": "ㅋㅋㅋㅋ 드세용!!!",
          "original_form": "ㅋㅋㅋㅋ 드세용!!!",
          "speaker_id": "2",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100040855.1.17",
          "form": "어제도 드시지 않았어요 근데",
          "original_form": "어제도 드시지 않았어요 근데",
