      "utterance": [
        {
          "id": "MDRW2100023773.1.1",
          "form": "나도 머리결이 약한 편인가..?",
          "original_form": "나도 머리결이 약한 편인가..?",
          "speaker_id": "2",
          "time": "20210830 17:08"
        },
        {
          "id": "MDRW2100023773.1.2",
          "form": "쫌 얇기는 한데 나보다는 안 약할 걸",
          "original_form": "쫌 얇기는 한데 나보다는 안 약할 걸",
          "speaker_id": "1",
          "time": "20210830 17:08"
        },
        {
          "id": "MDRW2100023773.1.3",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "2",
          "time": "20210830 17:08"
        },
        {
          "id": "MDRW2100023773.1.4",
          "form": "나보다는 좀 두꺼운 거 같던데",
          "original_form": "나보다는 좀 두꺼운 거 같던데",
          "speaker_id": "1",
          "time": "20210830 17:08"
        },
        {
          "id": "MDRW2100023773.1.5",
          "form": "그럼 다행이네",
          "original_form": "그럼 다행이네",
          "speaker_id": "2",
          "time": "20210830 17:09"
        },
        {
          "id": "MDRW2100023773.1.6",
          "form": "나는 탈색하면",
          "original_form": "나는 탈색하면",
          "speaker_id": "1",
          "time": "20210830 17:09"
        },
        {
          "id": "MDRW2100023773.1.7",
          "form": "로즈 골드 해보고 싶었어",
          "original_form": "로즈 골드 해보고 싶었어",
          "speaker_id": "1",
          "time": "20210830 17:10"
        },
        {
          "id": "MDRW2100023773.1.8",
          "form": "내가 아는 로즈골드?",
          "original_form": "내가 아는 로즈골드?",
          "speaker_id": "2",
          "time": "20210830 17:10"
        },
        {
          "id": "MDRW2100023773.1.9",
          "form": "약간 붉은 끼도는 금색",
          "original_form": "약간 붉은 끼도는 금색",
          "speaker_id": "1",
          "time": "20210830 17:10"
        },
        {
          "id": "MDRW2100023773.1.10",
          "form": "예쁘겠다 나도 어울리려나?",
          "original_form": "예쁘겠다 나도 어울리려나?",
          "speaker_id": "2",
          "time": "20210830 17:10"
        },
        {
          "id": "MDRW2100023773.1.11",
          "form": "해봐 대리만족이라도 하고 싶다",
          "original_form": "해봐 대리만족이라도 하고 싶다",
          "speaker_id": "1",
          "time": "20210830 17:11"
        },
        {
          "id": "MDRW2100023773.1.12",
          "form": "ㅋㅋㅋ 민트초코는 어때?",
          "original_form": "ㅋㅋㅋ 민트초코는 어때?",
          "speaker_id": "2",
          "time": "20210830 17:11"
        },
        {
          "id": "MDRW2100023773.1.13",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "2",
          "time": "20210830 17:11"
        },
        {
          "id": "MDRW2100023773.1.14",
          "form": "주변에 민트 초코 염색한 사람 그러니까 민트색 염색한 사람 두 명 있었는데 진짜 별로야",
          "original_form": "주변에 민트 초코 염색한 사람 그러니까 민트색 염색한 사람 두 명 있었는데 진짜 별로야",
          "speaker_id": "1",
          "time": "20210830 17:11"
        },
        {
          "id": "MDRW2100023773.1.15",
          "form": "그럼 머하지?",
          "original_form": "그럼 머하지?",
          "speaker_id": "2",
          "time": "20210830 17:11"
        },
        {
          "id": "MDRW2100023773.1.16",
          "form": "나는 염색은 좀 붉은 계열을 좋아하는 거 같애 분홍색도 예쁘더라고",
          "original_form": "나는 염색은 좀 붉은 계열을 좋아하는 거 같애 분홍색도 예쁘더라고",
          "speaker_id": "1",
          "time": "20210830 17:12"
        },
        {
          "id": "MDRW2100023773.1.17",
          "form": "근데 분홍색은 넘 튀지 않나..?",
          "original_form": "근데 분홍색은 넘 튀지 않나..?",
          "speaker_id": "2",
          "time": "20210830 17:12"
        },
        {
          "id": "MDRW2100023773.1.18",
          "form": "그니까 약간 은은하게 톤 더운 덴 분홍색 계열",
          "original_form": "그니까 약간 은은하게 톤 더운 덴 분홍색 계열",
          "speaker_id": "1",
          "time": "20210830 17:12"
        },
        {
          "id": "MDRW2100023773.1.19",
          "form": "아 그럼 괜찮지",
          "original_form": "아 그럼 괜찮지",
          "speaker_id": "2",
          "time": "20210830 17:13"
        },
        {
          "id": "MDRW2100023773.1.20",
          "form": "은근 분위기 있어 보이고 멋있어 아니면 아예 불 빨간색",
          "original_form": "은근 분위기 있어 보이고 멋있어 아니면 아예 불 빨간색",
          "speaker_id": "1",
          "time": "20210830 17:13"
        }
      ]
