      "utterance": [
        {
          "id": "MDRW2100045143.1.1",
          "form": "name2님은 여행 좋아하세요?",
          "original_form": "&name2&님은 여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045143.1.2",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045143.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045143.1.4",
          "form": "엄청 좋아하죠...ㅠㅠ 근데 코로나때문에 못간지 오래돼서 넘 슬퍼요ㅠ",
          "original_form": "엄청 좋아하죠...ㅠㅠ 근데 코로나때문에 못간지 오래돼서 넘 슬퍼요ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045143.1.5",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045143.1.6",
          "form": "여행 갔던 곳 중에 제일 좋았던 곳이 어디예요?",
          "original_form": "여행 갔던 곳 중에 제일 좋았던 곳이 어디예요?",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045143.1.7",
          "form": "저는 양양 여행 좋았어요",
          "original_form": "저는 양양 여행 좋았어요",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045143.1.8",
          "form": "혹시 다녀오셨나요?",
          "original_form": "혹시 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045143.1.9",
          "form": "양양 좋죠ㅋㅋ 저희 본가랑 가까워요 서핑 하러 자주 갔었어요!",
          "original_form": "양양 좋죠ㅋㅋ 저희 본가랑 가까워요 서핑 하러 자주 갔었어요!",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045143.1.10",
          "form": "속초는 가보셨나요?",
          "original_form": "속초는 가보셨나요?",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045143.1.11",
          "form": "오 속초를 안가봤네요",
          "original_form": "오 속초를 안가봤네요",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045143.1.12",
          "form": "속초 어떤가요?",
          "original_form": "속초 어떤가요?",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045143.1.13",
          "form": "속초 좋아요ㅋㅋ 자주 갔었는데 물회랑 닭강정 꼭 드셔보세요!",
          "original_form": "속초 좋아요ㅋㅋ 자주 갔었는데 물회랑 닭강정 꼭 드셔보세요!",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045143.1.14",
          "form": "해외여행은 어디가 제일 좋으셨어요?",
          "original_form": "해외여행은 어디가 제일 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045143.1.15",
          "form": "대만이요!!",
          "original_form": "대만이요!!",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045143.1.16",
          "form": "가본곳이 많이 없는데 대만이 기억에 남네요",
          "original_form": "가본곳이 많이 없는데 대만이 기억에 남네요",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045143.1.17",
          "form": "아 저는 대만 안가봤어요! 대만 많이 덥다면서요?",
          "original_form": "아 저는 대만 안가봤어요! 대만 많이 덥다면서요?",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045143.1.18",
          "form": "네 좀 습한편인거 같아욤ㅁ",
          "original_form": "네 좀 습한편인거 같아욤ㅁ",
          "speaker_id": "2",
          "time": "20210908 01:11"
        },
        {
          "id": "MDRW2100045143.1.19",
          "form": "해외여행 기억남는곳 있으신가요?",
          "original_form": "해외여행 기억남는곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100045143.1.20",
          "form": "저는 가본 곳 중에 진짜 좋았던 건 방콕이었어요! 또 가고싶어요ㅋㅋ",
          "original_form": "저는 가본 곳 중에 진짜 좋았던 건 방콕이었어요! 또 가고싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:12"
        }
      ]
