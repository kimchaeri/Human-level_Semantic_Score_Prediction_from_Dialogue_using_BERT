      "utterance": [
        {
          "id": "MDRW2100006722.1.1",
          "form": "최근 놀면 뭐하니 재밌어요ㅎㅎ",
          "original_form": "최근 놀면 뭐하니 재밌어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100006722.1.2",
          "form": "저는 집에 티비가 없어서",
          "original_form": "저는 집에 티비가 없어서",
          "speaker_id": "1",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100006722.1.3",
          "form": "놀뭐는 그 노래하는거 까지 봣어요",
          "original_form": "놀뭐는 그 노래하는거 까지 봣어요",
          "speaker_id": "1",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100006722.1.4",
          "form": "와 저도 유튜브로 봐요ㅎㅎ",
          "original_form": "와 저도 유튜브로 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:39"
        },
        {
          "id": "MDRW2100006722.1.5",
          "form": "그럼 티비 프로그램 안 보세요?",
          "original_form": "그럼 티비 프로그램 안 보세요?",
          "speaker_id": "2",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100006722.1.6",
          "form": "티빙하고 넷플있어서 그걸로 나오는건만 봐요 ㅎㅎ",
          "original_form": "티빙하고 넷플있어서 그걸로 나오는건만 봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:41"
        },
        {
          "id": "MDRW2100006722.1.7",
          "form": "공중파는 그래서 안보게되네요",
          "original_form": "공중파는 그래서 안보게되네요",
          "speaker_id": "1",
          "time": "20210908 07:41"
        },
        {
          "id": "MDRW2100006722.1.8",
          "form": "오 두개나ㅎㅎ",
          "original_form": "오 두개나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100006722.1.9",
          "form": "그럼 거기선 뭐보세요",
          "original_form": "그럼 거기선 뭐보세요",
          "speaker_id": "2",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100006722.1.10",
          "form": "최근엔 dp봤습니다",
          "original_form": "최근엔 dp봤습니다",
          "speaker_id": "1",
          "time": "20210908 07:42"
        },
        {
          "id": "MDRW2100006722.1.11",
          "form": "와 핫하더라고요",
          "original_form": "와 핫하더라고요",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100006722.1.12",
          "form": "완결되었나요?",
          "original_form": "완결되었나요?",
          "speaker_id": "2",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100006722.1.13",
          "form": "네. 6편밖에 안돼요",
          "original_form": "네. 6편밖에 안돼요",
          "speaker_id": "1",
          "time": "20210908 07:43"
        },
        {
          "id": "MDRW2100006722.1.14",
          "form": "아 짧구나",
          "original_form": "아 짧구나",
          "speaker_id": "2",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100006722.1.15",
          "form": "재밌던가요?",
          "original_form": "재밌던가요?",
          "speaker_id": "2",
          "time": "20210908 07:44"
        },
        {
          "id": "MDRW2100006722.1.16",
          "form": "네 재밌게 봤어요. 국방부가 우려스럽다고 그러더라구요 ㅋㅋ",
          "original_form": "네 재밌게 봤어요. 국방부가 우려스럽다고 그러더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:45"
        },
        {
          "id": "MDRW2100006722.1.17",
          "form": "근데 불편해도 사실적이래요ㅠ",
          "original_form": "근데 불편해도 사실적이래요ㅠ",
