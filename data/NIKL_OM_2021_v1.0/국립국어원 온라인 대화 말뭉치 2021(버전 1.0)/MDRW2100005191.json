      "utterance": [
        {
          "id": "MDRW2100005191.1.1",
          "form": "집안일 많이하세요?",
          "original_form": "집안일 많이하세요?",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005191.1.2",
          "form": "혼자니 제가 다해야죠ㅎ",
          "original_form": "혼자니 제가 다해야죠ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005191.1.3",
          "form": "가사일 많이 도와주시나요?",
          "original_form": "가사일 많이 도와주시나요?",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005191.1.4",
          "form": "저는 많이 도와준다곤 하는데 아내는 만족을 못하죠ㅎㅎ",
          "original_form": "저는 많이 도와준다곤 하는데 아내는 만족을 못하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005191.1.5",
          "form": "아무래도 엄마와 아빠는 차이가 있잖아요ㅎ",
          "original_form": "아무래도 엄마와 아빠는 차이가 있잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005191.1.6",
          "form": "나중에 혼자한거 미래의 남편에게 다시키세요 ㅎ",
          "original_form": "나중에 혼자한거 미래의 남편에게 다시키세요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005191.1.7",
          "form": "ㅋㅋ 그런 남편이 있을까요?",
          "original_form": "ㅋㅋ 그런 남편이 있을까요?",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005191.1.8",
          "form": "화장실청소 음식물 버려주고",
          "original_form": "화장실청소 음식물 버려주고",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005191.1.9",
          "form": "요즘 그런건 많이들 해주나보던데",
          "original_form": "요즘 그런건 많이들 해주나보던데",
          "speaker_id": "1",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005191.1.10",
          "form": "에이 저도 하잖아요ㅎ",
          "original_form": "에이 저도 하잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005191.1.11",
          "form": "전 빨래 빼고 다하는것같은데요",
          "original_form": "전 빨래 빼고 다하는것같은데요",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005191.1.12",
          "form": "빨래는 자기가 한대요ㅎ 못믿겟다고",
          "original_form": "빨래는 자기가 한대요ㅎ 못믿겟다고",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005191.1.13",
          "form": "오 그럼 많이 하시네요",
          "original_form": "오 그럼 많이 하시네요",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005191.1.14",
          "form": "빨래는 세탁기가ㅎ",
          "original_form": "빨래는 세탁기가ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005191.1.15",
          "form": "세탁기힘을 빌리는 주제에... 내가빨면 다른가봐요ㅎㅎ",
          "original_form": "세탁기힘을 빌리는 주제에... 내가빨면 다른가봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005191.1.16",
          "form": "근데 그럴수있는게 저는 기능을 잘못쓰고 한거번에 빨아벌이니ㄹㅎ",
          "original_form": "근데 그럴수있는게 저는 기능을 잘못쓰고 한거번에 빨아벌이니ㄹㅎ",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005191.1.17",
          "form": "아마 세탁할 옷을 분류해야되니 그게 걱정되셨나봐요ㅎ",
          "original_form": "아마 세탁할 옷을 분류해야되니 그게 걱정되셨나봐요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005191.1.18",
          "form": "그런가봐요ㅎ 전 세상에 수건에 섬유유연제 안넣는거 처음알았어요ㅎ",
          "original_form": "그런가봐요ㅎ 전 세상에 수건에 섬유유연제 안넣는거 처음알았어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005191.1.19",
          "form": "엇 전 그냥 다넣는데ㅋ 넣응안된대요?",
          "original_form": "엇 전 그냥 다넣는데ㅋ 넣응안된대요?",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005191.1.20",
          "form": "아셨어요? 섬유유연재 넣으면 수명이 짧아진대요ㅎ",
          "original_form": "아셨어요? 섬유유연재 넣으면 수명이 짧아진대요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005191.1.21",
          "form": "근데 왜 우리집수건은 그모냥인지....",
          "original_form": "근데 왜 우리집수건은 그모냥인지....",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005191.1.22",
          "form": "아니요 첨들어요ㅎ",
          "original_form": "아니요 첨들어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005191.1.23",
          "form": "유연제라 그런걸까요?",
          "original_form": "유연제라 그런걸까요?",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005191.1.24",
          "form": "저도 빨래를 대충한거군요",
          "original_form": "저도 빨래를 대충한거군요",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005191.1.25",
          "form": "우린대충한거니깐ㅎㅎ",
          "original_form": "우린대충한거니깐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005191.1.26",
          "form": "아니요ㅎ 저희집은 유연제안넣었어요ㅎ",
          "original_form": "아니요ㅎ 저희집은 유연제안넣었어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005191.1.27",
          "form": "내가봤을땐 다똑같애요ㅎ",
          "original_form": "내가봤을땐 다똑같애요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005191.1.28",
          "form": "그냥 자주빨아서 수명이 닳지않을까요?",
          "original_form": "그냥 자주빨아서 수명이 닳지않을까요?",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005191.1.29",
          "form": "자녀는 딸 한명이예요?",
          "original_form": "자녀는 딸 한명이예요?",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005191.1.30",
          "form": "그랬을수도있어요ㅎㅎ",
          "original_form": "그랬을수도있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005191.1.31",
          "form": "딸한명이요ㅎ 4살ㅎㅎ",
          "original_form": "딸한명이요ㅎ 4살ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005191.1.32",
          "form": "아 4살이면 말잘하겠네요",
          "original_form": "아 4살이면 말잘하겠네요",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005191.1.33",
          "form": "얼마나 귀엽게요ㅎㅎ 하루는 아빠! 저 핸드폰으로 유튜브 보면 안될까요? 이러는데 엄청 귀여워서ㅎㅎ",
          "original_form": "얼마나 귀엽게요ㅎㅎ 하루는 아빠! 저 핸드폰으로 유튜브 보면 안될까요? 이러는데 엄청 귀여워서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005191.1.34",
          "form": "말잘해요ㅎ",
          "original_form": "말잘해요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005191.1.35",
          "form": "올해 1월까지  말못하다가 갑자기 트였어요 ㅣㅎ",
          "original_form": "올해 1월까지  말못하다가 갑자기 트였어요 ㅣㅎ",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005191.1.36",
          "form": "존댓말 쓰는군요 엄청 이쁘겠어요ㅎㅎ",
          "original_form": "존댓말 쓰는군요 엄청 이쁘겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005191.1.37",
          "form": "애교도 많겠어요",
          "original_form": "애교도 많겠어요",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005191.1.38",
          "form": "글죠ㅎ 너무 애교가 많죠ㄹㅎ",
          "original_form": "글죠ㅎ 너무 애교가 많죠ㄹㅎ",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005191.1.39",
          "form": "근데 말하는게 너무좋아요ㅎ",
          "original_form": "근데 말하는게 너무좋아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005191.1.40",
          "form": "예전에는,말못했는데 말하고나서는 대화가 되니깐 아이랑 유대감도 잘생기는것같고ㅎ 너무행복하죠ㅎ",
          "original_form": "예전에는,말못했는데 말하고나서는 대화가 되니깐 아이랑 유대감도 잘생기는것같고ㅎ 너무행복하죠ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005191.1.41",
          "form": "name1님도 빨리 좋은짝 만나서 가정을 이루세요 ㅎ",
          "original_form": "&name1&님도 빨리 좋은짝 만나서 가정을 이루세요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005191.1.42",
          "form": "뭘해도 이쁠거같아요ㅎ 나닮아 나오면 더더욱",
          "original_form": "뭘해도 이쁠거같아요ㅎ 나닮아 나오면 더더욱",
          "speaker_id": "1",
