          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100032791.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100032791.1.13",
          "form": "욕심쟁이 우후훗",
          "original_form": "욕심쟁이 우후훗",
          "speaker_id": "2",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100032791.1.14",
          "form": "아주 정답이에요",
          "original_form": "아주 정답이에요",
          "speaker_id": "1",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100032791.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
