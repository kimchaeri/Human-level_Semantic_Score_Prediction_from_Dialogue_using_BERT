      "utterance": [
        {
          "id": "MDRW2100046069.1.1",
          "form": "네ㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046069.1.2",
          "form": "학교지역이",
          "original_form": "학교지역이",
          "speaker_id": "2",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100046069.1.3",
          "form": "확진자가 많아서ㅜ",
          "original_form": "확진자가 많아서ㅜ",
          "speaker_id": "2",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100046069.1.4",
          "form": "진짜 랜선동기네요....",
          "original_form": "진짜 랜선동기네요....",
          "speaker_id": "1",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100046069.1.5",
          "form": "휴학한 동기 한명있는데",
          "original_form": "휴학한 동기 한명있는데",
          "speaker_id": "2",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100046069.1.6",
          "form": "2학기시작했는데도",
          "original_form": "2학기시작했는데도",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046069.1.7",
          "form": "아무도 몰랐어요 ㅡㅋㅋㅋㅋㅋㅋ",
          "original_form": "아무도 몰랐어요 ㅡㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046069.1.8",
          "form": "ㅋㅋㅋㅋㅋ아 너무해",
          "original_form": "ㅋㅋㅋㅋㅋ아 너무해",
          "speaker_id": "1",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046069.1.9",
          "form": "학교를 안가니까",
          "original_form": "학교를 안가니까",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046069.1.10",
          "form": "서로관심이없는...",
          "original_form": "서로관심이없는...",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046069.1.11",
          "form": "미술 수업듣는다고했죠?",
          "original_form": "미술 수업듣는다고했죠?",
          "speaker_id": "1",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046069.1.12",
          "form": "네 전공으로",
          "original_form": "네 전공으로",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046069.1.13",
          "form": "어제도 했어요",
          "original_form": "어제도 했어요",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046069.1.14",
          "form": "실습인데",
          "original_form": "실습인데",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046069.1.15",
          "form": "어떻게 수업해요??",
          "original_form": "어떻게 수업해요??",
          "speaker_id": "1",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046069.1.16",
          "form": "교수님이 수업한거 따라하고",
          "original_form": "교수님이 수업한거 따라하고",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046069.1.17",
          "form": "핸드폰으로 수업시간에",
          "original_form": "핸드폰으로 수업시간에",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046069.1.18",
          "form": "사진찍어보내요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사진찍어보내요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046069.1.19",
          "form": "아?ㅋㅋㅋㅋㅋ스마트하시네",
          "original_form": "아?ㅋㅋㅋㅋㅋ스마트하시네",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046069.1.20",
          "form": "그럼 피드백해주시는거예요?",
          "original_form": "그럼 피드백해주시는거예요?",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046069.1.21",
          "form": "교수님이보고 카메라통해서",
          "original_form": "교수님이보고 카메라통해서",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046069.1.22",
          "form": "잘한거 고칠거 알려주세요",
          "original_form": "잘한거 고칠거 알려주세요",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046069.1.23",
          "form": "좀 답답할거갘아요",
          "original_form": "좀 답답할거갘아요",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046069.1.24",
          "form": "바로 옆에서 지도해줘야하는데",
          "original_form": "바로 옆에서 지도해줘야하는데",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046069.1.25",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210908 10:08"
        }
      ]
    }
