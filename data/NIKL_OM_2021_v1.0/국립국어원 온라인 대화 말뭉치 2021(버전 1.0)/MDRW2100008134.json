        {
          "id": "MDRW2100008134.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008134.1.2",
          "form": "가끔요",
          "original_form": "가끔요",
          "speaker_id": "2",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008134.1.3",
          "form": "얼마전 당근 해봤어요",
          "original_form": "얼마전 당근 해봤어요",
          "speaker_id": "2",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008134.1.4",
          "form": "저는 당근",
          "original_form": "저는 당근",
          "speaker_id": "1",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008134.1.5",
          "form": "하세요?",
          "original_form": "하세요?",
          "speaker_id": "2",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008134.1.6",
          "form": "아당근 전 첨해봤어요",
          "original_form": "아당근 전 첨해봤어요",
          "speaker_id": "2",
          "time": "20210822 13:30"
        },
        {
          "id": "MDRW2100008134.1.7",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008134.1.8",
          "form": "좋은 물건도 있구",
          "original_form": "좋은 물건도 있구",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008134.1.9",
          "form": "그렇군요 전 중나가ㅜ편했는데",
          "original_form": "그렇군요 전 중나가ㅜ편했는데",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008134.1.10",
          "form": "예전에는 그랬는데 이제는 동네라서",
          "original_form": "예전에는 그랬는데 이제는 동네라서",
          "speaker_id": "1",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008134.1.11",
          "form": "택배 귀찮아서 당근했더니 편하긴했어요",
          "original_form": "택배 귀찮아서 당근했더니 편하긴했어요",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008134.1.12",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008134.1.13",
          "form": "바로 거래되니 좋죠",
          "original_form": "바로 거래되니 좋죠",
          "speaker_id": "1",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008134.1.14",
          "form": "기다릴다 취소할까ㅠ그건 걱정돼요",
          "original_form": "기다릴다 취소할까ㅠ그건 걱정돼요",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008134.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008134.1.16",
          "form": "아마더 그랗뎌",
          "original_form": "아마더 그랗뎌",
          "speaker_id": "1",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008134.1.17",
          "form": "전 팔기만하지 사보진 않았어요",
          "original_form": "전 팔기만하지 사보진 않았어요",
          "speaker_id": "2",
