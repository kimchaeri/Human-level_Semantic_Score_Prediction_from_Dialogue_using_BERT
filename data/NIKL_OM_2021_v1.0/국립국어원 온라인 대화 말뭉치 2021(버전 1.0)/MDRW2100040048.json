      "utterance": [
        {
          "id": "MDRW2100040048.1.1",
          "form": "왜냐면 케이아로 할때는",
          "original_form": "왜냐면 케이아로 할때는",
          "speaker_id": "2",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100040048.1.2",
          "form": "네이버에",
          "original_form": "네이버에",
          "speaker_id": "1",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100040048.1.3",
          "form": "걍 10초 딱 되고",
          "original_form": "걍 10초 딱 되고",
          "speaker_id": "2",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100040048.1.4",
          "form": "반 라이덴 특훈 작전 검색하면",
          "original_form": "반 라이덴 특훈 작전 검색하면",
          "speaker_id": "1",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100040048.1.5",
          "form": "토템 터져서 끝났거든",
          "original_form": "토템 터져서 끝났거든",
          "speaker_id": "2",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100040048.1.6",
          "form": "분노에 찬 사람들이 나타남...",
          "original_form": "분노에 찬 사람들이 나타남...",
          "speaker_id": "1",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100040048.1.7",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100040048.1.8",
          "form": "바닥 불 피격 외엔",
          "original_form": "바닥 불 피격 외엔",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100040048.1.9",
          "form": "차이가 없어",
          "original_form": "차이가 없어",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100040048.1.10",
          "form": "이러면 하",
          "original_form": "이러면 하",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100040048.1.11",
          "form": "케이아 궁 채워와서",
          "original_form": "케이아 궁 채워와서",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100040048.1.12",
          "form": "20초 되기전에",
          "original_form": "20초 되기전에",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100040048.1.13",
          "form": "근데 난 다이루크라 바닥 불은 디폴트엿어",
          "original_form": "근데 난 다이루크라 바닥 불은 디폴트엿어",
          "speaker_id": "1",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100040048.1.14",
          "form": "미리 쓰고 시작하고",
          "original_form": "미리 쓰고 시작하고",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100040048.1.15",
          "form": "그래야되나",
          "original_form": "그래야되나",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100040048.1.16",
          "form": "그 토템 등장이",
          "original_form": "그 토템 등장이",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100040048.1.17",
          "form": "20초 남았을때지?",
          "original_form": "20초 남았을때지?",
          "speaker_id": "2",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100040048.1.18",
          "form": "20~18초 사이",
          "original_form": "20~18초 사이",
          "speaker_id": "1",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100040048.1.19",
          "form": "그럼 20초쯤에",
          "original_form": "그럼 20초쯤에",
          "speaker_id": "2",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040048.1.20",
          "form": "미리 켜고 때려봐야겠다",
          "original_form": "미리 켜고 때려봐야겠다",
          "speaker_id": "2",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040048.1.21",
          "form": "보통 토템 근처까지 이동하는데도 초단위 시간 걸리니까",
          "original_form": "보통 토템 근처까지 이동하는데도 초단위 시간 걸리니까",
          "speaker_id": "1",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040048.1.22",
          "form": "궁부터....채워서.........",
          "original_form": "궁부터....채워서.........",
          "speaker_id": "2",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040048.1.23",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "2",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040048.1.24",
          "form": "일단 나 유튜브에",
          "original_form": "일단 나 유튜브에",
          "speaker_id": "2",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040048.1.25",
          "form": "공략좀 쳐볼게",
          "original_form": "공략좀 쳐볼게",
          "speaker_id": "2",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040048.1.26",
          "form": "어제 내가 분노에 차잇던 이유를 알겟지...",
          "original_form": "어제 내가 분노에 차잇던 이유를 알겟지...",
          "speaker_id": "1",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040048.1.27",
          "form": "남이 하는거 보면서",
          "original_form": "남이 하는거 보면서",
          "speaker_id": "2",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040048.1.28",
          "form": "감좀 잡아봐야겠어",
          "original_form": "감좀 잡아봐야겠어",
          "speaker_id": "2",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040048.1.29",
          "form": "ㅇㅇ 함 찾아봐 나도 궁금하다",
          "original_form": "ㅇㅇ 함 찾아봐 나도 궁금하다",
          "speaker_id": "1",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040048.1.30",
          "form": "종려가 잇잔아",
          "original_form": "종려가 잇잔아",
          "speaker_id": "2",
          "time": "20210904 01:40"
        },
        {
          "id": "MDRW2100040048.1.31",
          "form": "사기꾼같으니",
          "original_form": "사기꾼같으니",
          "speaker_id": "2",
          "time": "20210904 01:40"
        },
        {
          "id": "MDRW2100040048.1.32",
          "form": "안봐",
          "original_form": "안봐",
          "speaker_id": "2",
          "time": "20210904 01:40"
        },
        {
          "id": "MDRW2100040048.1.33",
          "form": "아 ㅋㅋㅋㅋ 진짜 그런 사람들이 있구나 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ 진짜 그런 사람들이 있구나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:40"
        }
      ]
    }
  ]
