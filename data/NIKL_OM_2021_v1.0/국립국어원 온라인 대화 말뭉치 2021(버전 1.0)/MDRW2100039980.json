      "utterance": [
        {
          "id": "MDRW2100039980.1.1",
          "form": "즐겨보는거 있으세요?",
          "original_form": "즐겨보는거 있으세요?",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039980.1.2",
          "form": "예능 좋아하고요",
          "original_form": "예능 좋아하고요",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100039980.1.3",
          "form": "드라마는 주로 짤로... ㅋㅋ",
          "original_form": "드라마는 주로 짤로... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039980.1.4",
          "form": "유투브 공연같은거 봐요",
          "original_form": "유투브 공연같은거 봐요",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039980.1.5",
          "form": "공연 좋아하시나봐요",
          "original_form": "공연 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039980.1.6",
          "form": "저는 요즘 보는 예능은 골때리는 그녀들",
          "original_form": "저는 요즘 보는 예능은 골때리는 그녀들",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039980.1.7",
          "form": "재미있어요 ㅋㅋㅋ",
          "original_form": "재미있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039980.1.8",
          "form": "아 그거 봤었어요!!!",
          "original_form": "아 그거 봤었어요!!!",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039980.1.9",
          "form": "못하는 팀 우리팀 하면서 봐요",
          "original_form": "못하는 팀 우리팀 하면서 봐요",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100039980.1.10",
          "form": "되게 열심히들 하셔서",
          "original_form": "되게 열심히들 하셔서",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100039980.1.11",
          "form": "ㅋㅋ맞아요 되게 응원하게 되고ㅜㅠ",
          "original_form": "ㅋㅋ맞아요 되게 응원하게 되고ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039980.1.12",
          "form": "축구 안좋아했는데 좋아졌어요",
          "original_form": "축구 안좋아했는데 좋아졌어요",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039980.1.13",
          "form": "맞아요 저도요!! 진짜 그 구척장신",
          "original_form": "맞아요 저도요!! 진짜 그 구척장신",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039980.1.14",
          "form": "골키퍼분 너무 잘하심ㅋㅋㅋ",
          "original_form": "골키퍼분 너무 잘하심ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039980.1.15",
          "form": "맞아요! 그 단발!!!",
          "original_form": "맞아요! 그 단발!!!",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039980.1.16",
          "form": "검은리본!!!",
          "original_form": "검은리본!!!",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039980.1.17",
          "form": "마르고 이쁜데 진짜 선수같더라고요",
          "original_form": "마르고 이쁜데 진짜 선수같더라고요",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039980.1.18",
          "form": "배구하셨다고 하더라고요",
          "original_form": "배구하셨다고 하더라고요",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039980.1.19",
          "form": "역시 반사신경이",
          "original_form": "역시 반사신경이",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039980.1.20",
          "form": "아 역시...선출!!!",
          "original_form": "아 역시...선출!!!",
