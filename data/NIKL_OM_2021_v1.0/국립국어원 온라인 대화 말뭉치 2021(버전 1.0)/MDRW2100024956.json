      "utterance": [
        {
          "id": "MDRW2100024956.1.1",
          "form": "해쉬브라운 맛있죠",
          "original_form": "해쉬브라운 맛있죠",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024956.1.2",
          "form": "맞아요 감튀처럼 눅눅해지지도 않아요",
          "original_form": "맞아요 감튀처럼 눅눅해지지도 않아요",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024956.1.3",
          "form": "원래 눅눅하고 부드러운 맛으로 먹는 거죠!",
          "original_form": "원래 눅눅하고 부드러운 맛으로 먹는 거죠!",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024956.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024956.1.5",
          "form": "배고프네요ㅠㅠ",
          "original_form": "배고프네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024956.1.6",
          "form": "전 좀있으면 엽떡먹어요",
          "original_form": "전 좀있으면 엽떡먹어요",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024956.1.7",
          "form": "소시지추가는 하셨나요?",
          "original_form": "소시지추가는 하셨나요?",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024956.1.8",
          "form": "중당추가했어요!",
          "original_form": "중당추가했어요!",
          "speaker_id": "1",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024956.1.9",
          "form": "와 배운 사람이시군요",
          "original_form": "와 배운 사람이시군요",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024956.1.10",
          "form": "그럼요 쩝쩝박사랍니다",
          "original_form": "그럼요 쩝쩝박사랍니다",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024956.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024956.1.12",
          "form": "전 오늘 이미 콩국수랑 칼국수로",
          "original_form": "전 오늘 이미 콩국수랑 칼국수로",
          "speaker_id": "2",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024956.1.13",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024956.1.14",
          "form": "위장을 데웠다가 얼렸다가 해먹었기때문에",
          "original_form": "위장을 데웠다가 얼렸다가 해먹었기때문에",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024956.1.15",
          "form": "올해의 마지막 콩국수를 먹었읍니다",
          "original_form": "올해의 마지막 콩국수를 먹었읍니다",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024956.1.16",
          "form": "먹는거좋아하세요??",
          "original_form": "먹는거좋아하세요??",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024956.1.17",
          "form": "평소에 닭찌만 먹으면 개밥도 맛있게 먹을 수 있답니다...",
          "original_form": "평소에 닭찌만 먹으면 개밥도 맛있게 먹을 수 있답니다...",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024956.1.18",
          "form": "아 근육만드시는중이시구나ㅋㅋ",
          "original_form": "아 근육만드시는중이시구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024956.1.19",
          "form": "밥도 운동을 해야 맛있게 먹을 수 있는 거더라고요 허허",
          "original_form": "밥도 운동을 해야 맛있게 먹을 수 있는 거더라고요 허허",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024956.1.20",
          "form": "저는 운동안해도 밥이 맛있던데....ㅋㅋㅋㅋ",
          "original_form": "저는 운동안해도 밥이 맛있던데....ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024956.1.21",
          "form": "최근에 입맛을 잃어서 아니었지만ㅋㅋ",
          "original_form": "최근에 입맛을 잃어서 아니었지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024956.1.22",
          "form": "헐 입맛이 왜요",
          "original_form": "헐 입맛이 왜요",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024956.1.23",
          "form": "원래 여름에 입맛을 잃을 수가 없는데",
          "original_form": "원래 여름에 입맛을 잃을 수가 없는데",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024956.1.24",
          "form": "스트레스받는일이 있어서요",
          "original_form": "스트레스받는일이 있어서요",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024956.1.25",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024956.1.26",
          "form": "두달동안....",
          "original_form": "두달동안....",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024956.1.27",
          "form": "아이구야 고생하셨구나",
          "original_form": "아이구야 고생하셨구나",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024956.1.28",
          "form": "엽떡 맛있게 조지시고 잊어버려요!!",
          "original_form": "엽떡 맛있게 조지시고 잊어버려요!!",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024956.1.29",
          "form": "감사해요",
          "original_form": "감사해요",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024956.1.30",
          "form": "다행히 이제끝나서",
          "original_form": "다행히 이제끝나서",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024956.1.31",
          "form": "입맛이 돌아올것같긴 해요ㅋㅋ",
          "original_form": "입맛이 돌아올것같긴 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024956.1.32",
          "form": "그쵸 스트레스 받을 땐 먹어야죠",
          "original_form": "그쵸 스트레스 받을 땐 먹어야죠",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024956.1.33",
          "form": "매운거 좋아하세요???",
          "original_form": "매운거 좋아하세요???",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024956.1.34",
          "form": "네 완전요",
          "original_form": "네 완전요",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024956.1.35",
          "form": "완전완전",
          "original_form": "완전완전",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100024956.1.36",
          "form": "그래서 엽떡도기대중이에요",
          "original_form": "그래서 엽떡도기대중이에요",
          "speaker_id": "1",
          "time": "20210831 21:38"
        }
      ]
