      "utterance": [
        {
          "id": "MDRW2100035916.1.1",
          "form": "난 요기요 3천원노예라",
          "original_form": "난 요기요 3천원노예라",
          "speaker_id": "2",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100035916.1.2",
          "form": "보고있는데 키토김밥? ㅋㅋㅋ",
          "original_form": "보고있는데 키토김밥? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100035916.1.3",
          "form": "ㅋㅋㅋ오 건걍한 김밥",
          "original_form": "ㅋㅋㅋ오 건걍한 김밥",
          "speaker_id": "1",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100035916.1.4",
          "form": "근데 요즘 식중독 핫해서 김밥 먹기 꺼려져 ㅠ",
          "original_form": "근데 요즘 식중독 핫해서 김밥 먹기 꺼려져 ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:26"
        },
        {
          "id": "MDRW2100035916.1.5",
          "form": "아그렇네ㅜㅠ바보",
          "original_form": "아그렇네ㅜㅠ바보",
          "speaker_id": "2",
          "time": "20210830 11:29"
        },
        {
          "id": "MDRW2100035916.1.6",
          "form": "다른거 찾아보자!! ㅠㅠ 김밥말고 밥 뭐 없남?",
          "original_form": "다른거 찾아보자!! ㅠㅠ 김밥말고 밥 뭐 없남?",
          "speaker_id": "1",
          "time": "20210830 11:30"
        },
        {
          "id": "MDRW2100035916.1.7",
          "form": "ㅋㅋㅋ샐러디 샐러드사볼까 ㅋㅋㅋ급연어샐러드먹고싶은데",
          "original_form": "ㅋㅋㅋ샐러디 샐러드사볼까 ㅋㅋㅋ급연어샐러드먹고싶은데",
          "speaker_id": "2",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035916.1.8",
          "form": "이건또괜찮나싶고..",
          "original_form": "이건또괜찮나싶고..",
          "speaker_id": "2",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035916.1.9",
          "form": "연어말고는 없나요!! ㅋㅋㅋ날 것도 피해!!",
          "original_form": "연어말고는 없나요!! ㅋㅋㅋ날 것도 피해!!",
          "speaker_id": "1",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035916.1.10",
          "form": "그니깐 엉엉엉",
          "original_form": "그니깐 엉엉엉",
          "speaker_id": "2",
          "time": "20210830 11:31"
        },
        {
          "id": "MDRW2100035916.1.11",
          "form": "ㅋㅋ닭가슴살 이런거..",
          "original_form": "ㅋㅋ닭가슴살 이런거..",
          "speaker_id": "1",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100035916.1.12",
          "form": "걘 익혀주잖아...",
          "original_form": "걘 익혀주잖아...",
          "speaker_id": "1",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100035916.1.13",
          "form": "아 서브웨이 먹고싶다 ㅋㅋ 아삭아삭",
          "original_form": "아 서브웨이 먹고싶다 ㅋㅋ 아삭아삭",
          "speaker_id": "1",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100035916.1.14",
          "form": "ㅋㅋㅋㅋ언제먹엇던지 기억두안나네ㅠ",
          "original_form": "ㅋㅋㅋㅋ언제먹엇던지 기억두안나네ㅠ",
          "speaker_id": "2",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100035916.1.15",
          "form": "그니까!! 구미에도 좀 생겨라 ㅠㅠ",
          "original_form": "그니까!! 구미에도 좀 생겨라 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100035916.1.16",
          "form": "그나마 근처에 퀴즈노스 있음ㅋㅋ",
          "original_form": "그나마 근처에 퀴즈노스 있음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100035916.1.17",
          "form": "ㅋㅋㅋ과연",
          "original_form": "ㅋㅋㅋ과연",
          "speaker_id": "2",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100035916.1.18",
          "form": "안생길걸..",
          "original_form": "안생길걸..",
          "speaker_id": "1",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100035916.1.19",
          "form": "그건첨들어보네",
          "original_form": "그건첨들어보네",
          "speaker_id": "2",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100035916.1.20",
          "form": "여기도 샌드위치 파는 곳이야!! 난 트레디셔널 밖에 안먹어봤는데 맛있어!",
          "original_form": "여기도 샌드위치 파는 곳이야!! 난 트레디셔널 밖에 안먹어봤는데 맛있어!",
          "speaker_id": "1",
          "time": "20210830 11:34"
        },
        {
          "id": "MDRW2100035916.1.21",
          "form": "어려운곳이구만 ㅋㅋㅋ",
          "original_form": "어려운곳이구만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100035916.1.22",
          "form": "난 닭탕저녁포장으로삼 ㅋㅋㅋ오늘저녁은 닭탕이야",
          "original_form": "난 닭탕저녁포장으로삼 ㅋㅋㅋ오늘저녁은 닭탕이야",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100035916.1.23",
          "form": "닭탕은 뭐에유?!+",
          "original_form": "닭탕은 뭐에유?!+",
          "speaker_id": "1",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100035916.1.24",
          "form": "모퉁이식당이라고",
          "original_form": "모퉁이식당이라고",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100035916.1.25",
          "form": "사람들이 많이알더라구",
          "original_form": "사람들이 많이알더라구",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100035916.1.26",
          "form": "닭볶음탕인데 국물이 많다고",
          "original_form": "닭볶음탕인데 국물이 많다고",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100035916.1.27",
          "form": "일할때 애들이 많이얘기하더라고",
          "original_form": "일할때 애들이 많이얘기하더라고",
