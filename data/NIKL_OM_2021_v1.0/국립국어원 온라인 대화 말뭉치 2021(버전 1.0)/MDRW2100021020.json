      "utterance": [
        {
          "id": "MDRW2100021020.1.1",
          "form": "기억에 남는 기념품 있으세요",
          "original_form": "기억에 남는 기념품 있으세요",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100021020.1.2",
          "form": "캄보디아에서 사온 것들이요 ㅋㅋㅋ",
          "original_form": "캄보디아에서 사온 것들이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100021020.1.3",
          "form": "오 뭐 사오셨는데여",
          "original_form": "오 뭐 사오셨는데여",
          "speaker_id": "2",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100021020.1.4",
          "form": "그 이상한 금장으로 된 기념품이랑",
          "original_form": "그 이상한 금장으로 된 기념품이랑",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100021020.1.5",
          "form": "과일 먹을 때 쓰는 포크랑 손톱깎이 세트요 ㅋㅋㅋ",
          "original_form": "과일 먹을 때 쓰는 포크랑 손톱깎이 세트요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100021020.1.6",
          "form": "실용적인 것들 사오시는군요ㅋㅋㅋ",
          "original_form": "실용적인 것들 사오시는군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100021020.1.7",
          "form": "다 도금이여가지고 뭔가 신기했는데...",
          "original_form": "다 도금이여가지고 뭔가 신기했는데...",
          "speaker_id": "1",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100021020.1.8",
          "form": "공항에서 걸려서 다 압수당했어요 ^.^",
          "original_form": "공항에서 걸려서 다 압수당했어요 ^.^",
          "speaker_id": "1",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100021020.1.9",
          "form": "앜ㅋㅋ 안돌려주나요 그럼?",
          "original_form": "앜ㅋㅋ 안돌려주나요 그럼?",
          "speaker_id": "2",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100021020.1.10",
          "form": "네.. 그들이 가지겠죠...",
          "original_form": "네.. 그들이 가지겠죠...",
          "speaker_id": "1",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100021020.1.11",
          "form": "애초에 압수대상 아닌데 억지부린 것 같기도 해요",
          "original_form": "애초에 압수대상 아닌데 억지부린 것 같기도 해요",
          "speaker_id": "1",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100021020.1.12",
          "form": "입국할 땐 스팸 통조림도 뺏어가더라니까요?",
          "original_form": "입국할 땐 스팸 통조림도 뺏어가더라니까요?",
          "speaker_id": "1",
          "time": "20210828 15:18"
        },
        {
          "id": "MDRW2100021020.1.13",
          "form": "헐 그렇구나..아무거나 사오면 안되네요 ㅋㅋㅋ",
          "original_form": "헐 그렇구나..아무거나 사오면 안되네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021020.1.14",
          "form": "그냥 자기네끼리 쟁여두려고 뺏는거 같아요",
          "original_form": "그냥 자기네끼리 쟁여두려고 뺏는거 같아요",
          "speaker_id": "1",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021020.1.15",
          "form": "그런 일이 비일비재하다고 들었어요 ㅋㅋ",
          "original_form": "그런 일이 비일비재하다고 들었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021020.1.16",
          "form": "그래서 돈 쥐어주면 몰래 봐주고 그런다고..",
          "original_form": "그래서 돈 쥐어주면 몰래 봐주고 그런다고..",
          "speaker_id": "1",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021020.1.17",
          "form": "헐 그런 비리가!!",
          "original_form": "헐 그런 비리가!!",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021020.1.18",
          "form": "진짜 별거아닌건데 빼앗아가다니 아깝네요 ㅋㅋ",
          "original_form": "진짜 별거아닌건데 빼앗아가다니 아깝네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021020.1.19",
          "form": "진짜 열받았어요 ㅜ",
          "original_form": "진짜 열받았어요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021020.1.20",
          "form": "지들 불리할 때만 영어 못하는 척 하더라구요",
          "original_form": "지들 불리할 때만 영어 못하는 척 하더라구요",
          "speaker_id": "1",
          "time": "20210828 15:23"
        }
