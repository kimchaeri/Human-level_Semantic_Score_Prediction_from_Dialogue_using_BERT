      "utterance": [
        {
          "id": "MDRW2100045451.1.1",
          "form": "왜 인기있엇는데",
          "original_form": "왜 인기있엇는데",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045451.1.2",
          "form": "이승기의 정바름역할이 좀",
          "original_form": "이승기의 정바름역할이 좀",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045451.1.3",
          "form": "이번엔 연기 그나마 좋았다",
          "original_form": "이번엔 연기 그나마 좋았다",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045451.1.4",
          "form": "이드라마는 뭐가 복잡더라",
          "original_form": "이드라마는 뭐가 복잡더라",
          "speaker_id": "1",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045451.1.5",
          "form": "어 좀 그런편이었다 ㅋㅋ",
          "original_form": "어 좀 그런편이었다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:38"
        },
        {
          "id": "MDRW2100045451.1.6",
          "form": "이희준은 왜 늙어보이는데",
          "original_form": "이희준은 왜 늙어보이는데",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045451.1.7",
          "form": "ㅋㅋ약간 노안스타일",
          "original_form": "ㅋㅋ약간 노안스타일",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045451.1.8",
          "form": "나중에는 동안일려나",
          "original_form": "나중에는 동안일려나",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045451.1.9",
          "form": "어  한참 세월이 흘러서",
          "original_form": "어  한참 세월이 흘러서",
          "speaker_id": "2",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045451.1.10",
          "form": "경수진도 연기 잘하고",
          "original_form": "경수진도 연기 잘하고",
          "speaker_id": "1",
          "time": "20210908 02:39"
        },
        {
          "id": "MDRW2100045451.1.11",
          "form": "오랜만에 나온 안재욱도 그래",
          "original_form": "오랜만에 나온 안재욱도 그래",
          "speaker_id": "2",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045451.1.12",
          "form": "맞다 안재욱 닥터 역은 정말 신의 한수",
          "original_form": "맞다 안재욱 닥터 역은 정말 신의 한수",
          "speaker_id": "1",
          "time": "20210908 02:40"
        },
        {
          "id": "MDRW2100045451.1.13",
          "form": "어 계속 보게 되더라",
          "original_form": "어 계속 보게 되더라",
          "speaker_id": "2",
          "time": "20210908 02:40"
