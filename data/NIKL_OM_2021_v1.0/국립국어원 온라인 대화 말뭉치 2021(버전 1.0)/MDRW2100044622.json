      "utterance": [
        {
          "id": "MDRW2100044622.1.1",
          "form": "초밥 좋아하세요?",
          "original_form": "초밥 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044622.1.2",
          "form": "네 초밥 너무 좋아해요",
          "original_form": "네 초밥 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044622.1.3",
          "form": "오 어떤 초밥 젤 조아하세요",
          "original_form": "오 어떤 초밥 젤 조아하세요",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044622.1.4",
          "form": "황새치 초밥 제일 좋아합니다",
          "original_form": "황새치 초밥 제일 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044622.1.5",
          "form": "쿠크다스님은요?",
          "original_form": "쿠크다스님은요?",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044622.1.6",
          "form": "그래요..? 처음들어욬ㅋㅋㅋ",
          "original_form": "그래요..? 처음들어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044622.1.7",
          "form": "저는 광어 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 광어 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044622.1.8",
          "form": "저도 광어 초밥도 진짜 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 광어 초밥도 진짜 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044622.1.9",
          "form": "사실 초밥이면 다 좋아해욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사실 초밥이면 다 좋아해욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044622.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저두요 근데 장어랑 참치는 못먹어여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저두요 근데 장어랑 참치는 못먹어여",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044622.1.11",
          "form": "아 정말요? 전 장어랑 참치도 좋아해욬ㅋㅋㅋㅋㅋ",
          "original_form": "아 정말요? 전 장어랑 참치도 좋아해욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044622.1.12",
          "form": "ㅋㅋㅋㅋㅋ저는 초밥좋아하는데 비린거를못먹어서욬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ저는 초밥좋아하는데 비린거를못먹어서욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044622.1.13",
          "form": "묘하게 비리더라구요",
          "original_form": "묘하게 비리더라구요",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044622.1.14",
          "form": "광어가 덜비리죠",
          "original_form": "광어가 덜비리죠",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044622.1.15",
          "form": "그럼 연어도 못드세요?",
          "original_form": "그럼 연어도 못드세요?",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044622.1.16",
          "form": "연어는 먹어요! 맛있는데는 안비리더라구요!",
          "original_form": "연어는 먹어요! 맛있는데는 안비리더라구요!",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044622.1.17",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044622.1.18",
          "form": "연어 너무 맛잇잖아욬ㅋㅋㅋㅋ",
          "original_form": "연어 너무 맛잇잖아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044622.1.19",
          "form": "ㅋㅋㅋㅋㅋ맛아욬ㅋㅋㅋㅋ근데 연어초밥만 열개는 못먹겟더라구요",
          "original_form": "ㅋㅋㅋㅋㅋ맛아욬ㅋㅋㅋㅋ근데 연어초밥만 열개는 못먹겟더라구요",
          "speaker_id": "1",
