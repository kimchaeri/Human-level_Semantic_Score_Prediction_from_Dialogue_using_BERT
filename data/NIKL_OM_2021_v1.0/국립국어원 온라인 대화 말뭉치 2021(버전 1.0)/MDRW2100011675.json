      "utterance": [
        {
          "id": "MDRW2100011675.1.1",
          "form": "공연 보는거 좋아하세요?",
          "original_form": "공연 보는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011675.1.2",
          "form": "네 자주 다녔어요 님은요?",
          "original_form": "네 자주 다녔어요 님은요?",
          "speaker_id": "1",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011675.1.3",
          "form": "네 저는 클래식 공연보는거 좋아해서 자주다녔어요",
          "original_form": "네 저는 클래식 공연보는거 좋아해서 자주다녔어요",
          "speaker_id": "2",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011675.1.4",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011675.1.5",
          "form": "무슨 공연 좋아하세요?",
          "original_form": "무슨 공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011675.1.6",
          "form": "해외 연예인 내한 공연 갔었어요",
          "original_form": "해외 연예인 내한 공연 갔었어요",
          "speaker_id": "1",
          "time": "20210824 15:13"
        },
        {
          "id": "MDRW2100011675.1.7",
          "form": "오 가수에요?",
          "original_form": "오 가수에요?",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011675.1.8",
          "form": "저는 가수 공연은 한번도 못가봤어요",
          "original_form": "저는 가수 공연은 한번도 못가봤어요",
          "speaker_id": "2",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011675.1.9",
          "form": "네 가수예요~",
          "original_form": "네 가수예요~",
          "speaker_id": "1",
          "time": "20210824 15:14"
        },
        {
          "id": "MDRW2100011675.1.10",
          "form": "클래식 공연은 어떻게 가게되셨나요?",
          "original_form": "클래식 공연은 어떻게 가게되셨나요?",
          "speaker_id": "1",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011675.1.11",
          "form": "학교에서 교양수업 듣는데",
          "original_form": "학교에서 교양수업 듣는데",
          "speaker_id": "2",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011675.1.12",
          "form": "그런거 숙제로 내주잖아요 공연감상문 쓰라고요",
          "original_form": "그런거 숙제로 내주잖아요 공연감상문 쓰라고요",
          "speaker_id": "2",
          "time": "20210824 15:15"
        },
        {
          "id": "MDRW2100011675.1.13",
          "form": "숙제하려고 갔다가 잘하는 사람 알게되고 자꾸 관심이 가다보니까 그리되었어요",
          "original_form": "숙제하려고 갔다가 잘하는 사람 알게되고 자꾸 관심이 가다보니까 그리되었어요",
          "speaker_id": "2",
          "time": "20210824 15:16"
        },
        {
          "id": "MDRW2100011675.1.14",
          "form": "클래식 추천해주세요",
          "original_form": "클래식 추천해주세요",
          "speaker_id": "1",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011675.1.15",
          "form": "작곡가요 연주자요?",
          "original_form": "작곡가요 연주자요?",
          "speaker_id": "2",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011675.1.16",
          "form": "상관없이 name2님이 좋았던 분이요",
          "original_form": "상관없이 &name2&님이 좋았던 분이요",
          "speaker_id": "1",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011675.1.17",
          "form": "저는 클래식 잘모르거든요",
          "original_form": "저는 클래식 잘모르거든요",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011675.1.18",
          "form": "요즘 좋아하는사람은 문지영이요",
          "original_form": "요즘 좋아하는사람은 문지영이요",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011675.1.19",
          "form": "이사람 진짜 잘쳐요 유튜브에서 들어보세요",
          "original_form": "이사람 진짜 잘쳐요 유튜브에서 들어보세요",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011675.1.20",
          "form": "오 한번 볼게요",
          "original_form": "오 한번 볼게요",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011675.1.21",
          "form": "혹시 피아니스트 지용 아시나요",
          "original_form": "혹시 피아니스트 지용 아시나요",
