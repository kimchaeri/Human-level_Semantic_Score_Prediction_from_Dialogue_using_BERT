      "utterance": [
        {
          "id": "MDRW2100014336.1.1",
          "form": "저녁 메뉴 고르셨나요?",
          "original_form": "저녁 메뉴 고르셨나요?",
          "speaker_id": "1",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014336.1.2",
          "form": "넹 저는 도미노피자먹엇어요 ㅎㄹ",
          "original_form": "넹 저는 도미노피자먹엇어요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014336.1.3",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014336.1.4",
          "form": "블랙앵거스 맛잇네요",
          "original_form": "블랙앵거스 맛잇네요",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014336.1.5",
          "form": "오 피자 좋아요",
          "original_form": "오 피자 좋아요",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014336.1.6",
          "form": "저는 떡볶이요 ㅋㅋㅋ",
          "original_form": "저는 떡볶이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014336.1.7",
          "form": "모두 배달음식이네요",
          "original_form": "모두 배달음식이네요",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014336.1.8",
          "form": "오늘 티데이에서 방문포장50프로할인하길래",
          "original_form": "오늘 티데이에서 방문포장50프로할인하길래",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014336.1.9",
          "form": "바로다녀왓어요 ㅋㅋㅋㅋ",
          "original_form": "바로다녀왓어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100014336.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014336.1.11",
          "form": "도미노가 할인 많이 하는 것 같아요",
          "original_form": "도미노가 할인 많이 하는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014336.1.12",
          "form": "마자요 비싸긴한데 젤맛나요",
          "original_form": "마자요 비싸긴한데 젤맛나요",
          "speaker_id": "2",
          "time": "20210826 08:15"
        },
        {
          "id": "MDRW2100014336.1.13",
          "form": "도미노에서 이번에 4만원짜리 나왔대요",
          "original_form": "도미노에서 이번에 4만원짜리 나왔대요",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100014336.1.14",
          "form": "헥 진짜요? 4만원짜리 피자라니",
          "original_form": "헥 진짜요? 4만원짜리 피자라니",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100014336.1.15",
          "form": "아까워서어캐먹죠 ㅋㅋㅋ",
          "original_form": "아까워서어캐먹죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100014336.1.16",
          "form": "도미노가 최초로 단품 4만원 깼다는 기사 봣어요 ㅋㅋ",
          "original_form": "도미노가 최초로 단품 4만원 깼다는 기사 봣어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 08:09"
        },
        {
          "id": "MDRW2100014336.1.17",
          "form": "피자는 근데 요일별이나 이렇게 할인해서 뭐가 정가인지 모르겠어요 ㅋㅋ",
          "original_form": "피자는 근데 요일별이나 이렇게 할인해서 뭐가 정가인지 모르겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 08:09"
        },
        {
          "id": "MDRW2100014336.1.18",
          "form": "할인안받고먹으면 뭔가억울하죠 ㅋㅋㅋㅋㅋ",
          "original_form": "할인안받고먹으면 뭔가억울하죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100014336.1.19",
          "form": "저녁뭐드세요? 전족발먹을거에요 ㅎ",
          "original_form": "저녁뭐드세요? 전족발먹을거에요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100014336.1.20",
          "form": "오 저는 비빔국수 해먹었어요",
          "original_form": "오 저는 비빔국수 해먹었어요",
          "speaker_id": "1",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100014336.1.21",
          "form": "여름이 가는 걸 추억하며랄까요 ㅋㅋㅋ",
          "original_form": "여름이 가는 걸 추억하며랄까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100014336.1.22",
          "form": "헐 ㅠㅠ저 비빔국수 킬러,, 비빔막국수,,",
          "original_form": "헐 ㅠㅠ저 비빔국수 킬러,, 비빔막국수,,",
          "speaker_id": "2",
          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100014336.1.23",
          "form": "맛있었겠어요 저진짜 오이 많이넣어먹는데 ㅋㅋㅋ",
          "original_form": "맛있었겠어요 저진짜 오이 많이넣어먹는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:16"
        },
        {
          "id": "MDRW2100014336.1.24",
