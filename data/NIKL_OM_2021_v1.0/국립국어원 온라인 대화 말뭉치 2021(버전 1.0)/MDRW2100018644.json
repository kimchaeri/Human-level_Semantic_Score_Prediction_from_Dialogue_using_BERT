      "utterance": [
        {
          "id": "MDRW2100018644.1.1",
          "form": "싸이월드 부활 한다는데",
          "original_form": "싸이월드 부활 한다는데",
          "speaker_id": "2",
          "time": "20210827 12:06"
        },
        {
          "id": "MDRW2100018644.1.2",
          "form": "아셨나요",
          "original_form": "아셨나요",
          "speaker_id": "2",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100018644.1.3",
          "form": "실제로 부활한대여??",
          "original_form": "실제로 부활한대여??",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018644.1.4",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "2",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100018644.1.5",
          "form": "어이없어요",
          "original_form": "어이없어요",
          "speaker_id": "2",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100018644.1.6",
          "form": "오...서비스 종료한다고 다들 난리일 땐 가만히 있더니ㅠㅜ",
          "original_form": "오...서비스 종료한다고 다들 난리일 땐 가만히 있더니ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100018644.1.7",
          "form": "아니 웃긴게",
          "original_form": "아니 웃긴게",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018644.1.8",
          "form": "정보를 다 보관했더라구요",
          "original_form": "정보를 다 보관했더라구요",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018644.1.9",
          "form": "그거 그대로 살린다고 해요 ㅎ",
          "original_form": "그거 그대로 살린다고 해요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018644.1.10",
          "form": "네?? 아니, 개인정보를요?",
          "original_form": "네?? 아니, 개인정보를요?",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100018644.1.11",
          "form": "네.... 허락도 안맞고요 !!!!",
          "original_form": "네.... 허락도 안맞고요 !!!!",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018644.1.12",
          "form": "개인정보 이슈 요즘 민감할텐데ㅠ",
          "original_form": "개인정보 이슈 요즘 민감할텐데ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018644.1.13",
          "form": "그니가요",
          "original_form": "그니가요",
          "speaker_id": "2",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100018644.1.14",
          "form": "너무 별로입니다 ㅠ",
          "original_form": "너무 별로입니다 ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100018644.1.15",
          "form": "싸이월드 해보셨나요?",
          "original_form": "싸이월드 해보셨나요?",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018644.1.16",
          "form": "네 해봤죠 ㅎ",
          "original_form": "네 해봤죠 ㅎ",
          "speaker_id": "2",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100018644.1.17",
          "form": "해보셨나요",
          "original_form": "해보셨나요",
          "speaker_id": "2",
          "time": "20210827 13:08"
        },
        {
          "id": "MDRW2100018644.1.18",
          "form": "저는 안 해봤어요",
          "original_form": "저는 안 해봤어요",
          "speaker_id": "1",
          "time": "20210827 13:10"
        },
        {
          "id": "MDRW2100018644.1.19",
          "form": "아 정말요?????",
          "original_form": "아 정말요?????",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018644.1.20",
          "form": "저때는 핫해가지구요ㅋㅋㅋ",
          "original_form": "저때는 핫해가지구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018644.1.21",
          "form": "주변에서도 몇 몇 하긴 했는데 그때는 관심이 없었어서요ㅌㅋ",
          "original_form": "주변에서도 몇 몇 하긴 했는데 그때는 관심이 없었어서요ㅌㅋ",
          "speaker_id": "1",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018644.1.22",
          "form": "ㅋㅋㅋㅋㅋ아 근데 잘하신 거 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ아 근데 잘하신 거 같아요",
          "speaker_id": "2",
          "time": "20210827 13:14"
        },
        {
          "id": "MDRW2100018644.1.23",
          "form": "지금 보면 안한게 나아요 ㅠ",
          "original_form": "지금 보면 안한게 나아요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 13:14"
        }
      ]
