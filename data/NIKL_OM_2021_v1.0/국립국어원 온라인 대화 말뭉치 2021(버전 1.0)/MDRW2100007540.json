      "utterance": [
        {
          "id": "MDRW2100007540.1.1",
          "form": "요즘 머해",
          "original_form": "요즘 머해",
          "speaker_id": "1",
          "time": "20210822 03:04"
        },
        {
          "id": "MDRW2100007540.1.2",
          "form": "곧 개학이라서 학교 갈 준비하고있어",
          "original_form": "곧 개학이라서 학교 갈 준비하고있어",
          "speaker_id": "2",
          "time": "20210822 03:05"
        },
        {
          "id": "MDRW2100007540.1.3",
          "form": "헐~중고딩은 이미 개학하지 않았어?",
          "original_form": "헐~중고딩은 이미 개학하지 않았어?",
          "speaker_id": "1",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007540.1.4",
          "form": "개학 언제야? 난 8.30",
          "original_form": "개학 언제야? 난 8.30",
          "speaker_id": "1",
          "time": "20210822 03:06"
        },
        {
          "id": "MDRW2100007540.1.5",
          "form": "나는 다음주 화요일!",
          "original_form": "나는 다음주 화요일!",
          "speaker_id": "2",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007540.1.6",
          "form": "헉 코앞이네",
          "original_form": "헉 코앞이네",
          "speaker_id": "1",
          "time": "20210822 03:08"
        },
        {
          "id": "MDRW2100007540.1.7",
          "form": "등교는 해?? 동생은 하던대",
          "original_form": "등교는 해?? 동생은 하던대",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007540.1.8",
          "form": "데",
          "original_form": "데",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007540.1.9",
          "form": "등교랑 온라인이랑 번갈아가면서 한다더라ㅠ",
          "original_form": "등교랑 온라인이랑 번갈아가면서 한다더라ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007540.1.10",
          "form": "아항.....",
          "original_form": "아항.....",
          "speaker_id": "1",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007540.1.11",
          "form": "등교해야 재밌는데ㅠㅡㅠ",
          "original_form": "등교해야 재밌는데ㅠㅡㅠ",
          "speaker_id": "1",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007540.1.12",
          "form": "등교... 친구들 만나는건 재미있는데 수업은 너무 지루해ㅠ",
          "original_form": "등교... 친구들 만나는건 재미있는데 수업은 너무 지루해ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007540.1.13",
          "form": "설득해볼게",
          "original_form": "설득해볼게",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007540.1.14",
          "form": "등교하면 온라인 안 듣고 자도 돼",
          "original_form": "등교하면 온라인 안 듣고 자도 돼",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007540.1.15",
          "form": "친구도 만나도 급식도 줘",
          "original_form": "친구도 만나도 급식도 줘",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007540.1.16",
          "form": "ㅋㅋㅋㅋ ㅠㅠ 온라인은 캠 끄고 딴짓할 수 있는걸...",
          "original_form": "ㅋㅋㅋㅋ ㅠㅠ 온라인은 캠 끄고 딴짓할 수 있는걸...",
          "speaker_id": "2",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007540.1.17",
          "form": "급식...중요하긴하지 등교는 급식먹으려고 하는게 맞긴해",
          "original_form": "급식...중요하긴하지 등교는 급식먹으려고 하는게 맞긴해",
          "speaker_id": "2",
          "time": "20210822 03:17"
        },
        {
          "id": "MDRW2100007540.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ글킨하지....",
          "original_form": "ㅋㅋㅋㅋㅋㅋ글킨하지....",
          "speaker_id": "1",
          "time": "20210822 03:21"
        },
        {
          "id": "MDRW2100007540.1.19",
          "form": "헐 캠 안 켜두 돼?",
          "original_form": "헐 캠 안 켜두 돼?",
          "speaker_id": "1",
          "time": "20210822 03:21"
        },
        {
          "id": "MDRW2100007540.1.20",
          "form": "그럼 닥 온라인이지",
          "original_form": "그럼 닥 온라인이지",
          "speaker_id": "1",
          "time": "20210822 03:21"
        },
        {
          "id": "MDRW2100007540.1.21",
          "form": "캠 키라고 하지만... 수업 중에 몰래 끄는거지ㅋㅋㅋ ㅠ",
          "original_form": "캠 키라고 하지만... 수업 중에 몰래 끄는거지ㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007540.1.22",
          "form": "들키면 후다닥 키고",
          "original_form": "들키면 후다닥 키고",
          "speaker_id": "2",
          "time": "20210822 03:25"
        },
        {
          "id": "MDRW2100007540.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007540.1.24",
          "form": "맞아혼 좀 나면 어때",
          "original_form": "맞아혼 좀 나면 어때",
          "speaker_id": "1",
          "time": "20210822 03:28"
        },
        {
          "id": "MDRW2100007540.1.25",
          "form": "맞아 맞아!",
          "original_form": "맞아 맞아!",
          "speaker_id": "2",
          "time": "20210822 03:30"
        },
        {
          "id": "MDRW2100007540.1.26",
          "form": "하지말라고 하면 더 하고 싶어",
          "original_form": "하지말라고 하면 더 하고 싶어",
          "speaker_id": "2",
          "time": "20210822 03:31"
        },
        {
          "id": "MDRW2100007540.1.27",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "1",
          "time": "20210822 03:31"
        },
        {
          "id": "MDRW2100007540.1.28",
          "form": "아이 그리구 창피하단말야",
          "original_form": "아이 그리구 창피하단말야",
          "speaker_id": "1",
          "time": "20210822 03:31"
        },
        {
          "id": "MDRW2100007540.1.29",
          "form": "맞아...ㅎ 초췌한 몰골을 친구들이 본다니ㅠ",
          "original_form": "맞아...ㅎ 초췌한 몰골을 친구들이 본다니ㅠ",
