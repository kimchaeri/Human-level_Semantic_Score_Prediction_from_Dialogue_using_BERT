      "utterance": [
        {
          "id": "MDRW2100012134.1.1",
          "form": "겨울이 기다려지네요",
          "original_form": "겨울이 기다려지네요",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012134.1.2",
          "form": "당근으로 찾아서 붕어빵 구매하기..",
          "original_form": "당근으로 찾아서 붕어빵 구매하기..",
          "speaker_id": "2",
          "time": "20210824 19:36"
        },
        {
          "id": "MDRW2100012134.1.3",
          "form": "ㅋㄱㅋ귀여우세용^.^ 맛난거 사먹는 재미도 좋죠~",
          "original_form": "ㅋㄱㅋ귀여우세용^.^ 맛난거 사먹는 재미도 좋죠~",
          "speaker_id": "1",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100012134.1.4",
          "form": "겨울엔  군고구마도 짱이죠 ㅋ",
          "original_form": "겨울엔  군고구마도 짱이죠 ㅋ",
          "speaker_id": "1",
          "time": "20210824 19:38"
        },
        {
          "id": "MDRW2100012134.1.5",
          "form": "ㅋㅋㅋㅋㅋ군고구마 좋죠",
          "original_form": "ㅋㅋㅋㅋㅋ군고구마 좋죠",
          "speaker_id": "2",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100012134.1.6",
          "form": "역시 세상에서 젤 재밌는건",
          "original_form": "역시 세상에서 젤 재밌는건",
          "speaker_id": "2",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100012134.1.7",
          "form": "물건구매인가봐요..돈쓰기",
          "original_form": "물건구매인가봐요..돈쓰기",
          "speaker_id": "2",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100012134.1.8",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012134.1.9",
          "form": "근데 주변에 중고거래도 활발하게 하시는 분 보면",
          "original_form": "근데 주변에 중고거래도 활발하게 하시는 분 보면",
          "speaker_id": "1",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012134.1.10",
          "form": "중고도 잘만사면 너무 이득인거같아요",
          "original_form": "중고도 잘만사면 너무 이득인거같아요",
          "speaker_id": "1",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012134.1.11",
          "form": "ㅋㅋ맞아요 다들 어디서 그렇게 잘 사는지",
          "original_form": "ㅋㅋ맞아요 다들 어디서 그렇게 잘 사는지",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012134.1.12",
          "form": "저는 집에 선풍기가 하나 생겨서",
          "original_form": "저는 집에 선풍기가 하나 생겨서",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012134.1.13",
          "form": "그거를 당근으로 팔아볼까 생각중이예요",
          "original_form": "그거를 당근으로 팔아볼까 생각중이예요",
          "speaker_id": "2",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012134.1.14",
          "form": "ㅋㅋㅋ너무 신기한데 저는 잘안되네요ㅜㅜ 당근도 약속잡다 폭망..ㅠㅠ",
          "original_form": "ㅋㅋㅋ너무 신기한데 저는 잘안되네요ㅜㅜ 당근도 약속잡다 폭망..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012134.1.15",
          "form": "근데 여름이 다 지나가네요..",
          "original_form": "근데 여름이 다 지나가네요..",
          "speaker_id": "2",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012134.1.16",
          "form": "네 이제 쇼핑할때죠",
          "original_form": "네 이제 쇼핑할때죠",
          "speaker_id": "1",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012134.1.17",
          "form": "당일에 약속 어기시는 분도 계셨어요?",
          "original_form": "당일에 약속 어기시는 분도 계셨어요?",
          "speaker_id": "2",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012134.1.18",
          "form": "가을옷ㅍㅎㅎㅎㅎ",
          "original_form": "가을옷ㅍㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012134.1.19",
          "form": "가을옷 사야해요 금방 추워질꺼같아용",
          "original_form": "가을옷 사야해요 금방 추워질꺼같아용",
          "speaker_id": "2",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012134.1.20",
          "form": "아니요  서로 계속 일이있어서 이리옮겼다 저리옮겼다 시간옮기다보니  너무 지쳐서 그냥 취소했거든요",
          "original_form": "아니요  서로 계속 일이있어서 이리옮겼다 저리옮겼다 시간옮기다보니  너무 지쳐서 그냥 취소했거든요",
          "speaker_id": "1",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012134.1.21",
          "form": "계속 이렇게 신경쓰느니  분리수거해버리는게 낫겠다싶어서요ㅠ",
          "original_form": "계속 이렇게 신경쓰느니  분리수거해버리는게 낫겠다싶어서요ㅠ",
          "speaker_id": "1",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100012134.1.22",
          "form": "전 차라리 중고나라가 편했어요",
          "original_form": "전 차라리 중고나라가 편했어요",
          "speaker_id": "1",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100012134.1.23",
          "form": "다만 택배비가 슬플따름이죠",
          "original_form": "다만 택배비가 슬플따름이죠",
          "speaker_id": "1",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100012134.1.24",
          "form": "ㅠㅠㅠㅠ아 중고나라는 택배로 보내만 되니까 속편해요",
          "original_form": "ㅠㅠㅠㅠ아 중고나라는 택배로 보내만 되니까 속편해요",
          "speaker_id": "2",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100012134.1.25",
          "form": "이리저리 시간 바꾸면 다른분만나거나",
          "original_form": "이리저리 시간 바꾸면 다른분만나거나",
          "speaker_id": "2",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100012134.1.26",
          "form": "차라리 그냥 취소하는게 맘편할거같네요ㅠㅠ",
          "original_form": "차라리 그냥 취소하는게 맘편할거같네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100012134.1.27",
          "form": "그쵸 그래서 그냥 취소하고",
          "original_form": "그쵸 그래서 그냥 취소하고",
          "speaker_id": "1",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100012134.1.28",
          "form": "당근 어플삭제했어요",
          "original_form": "당근 어플삭제했어요",
          "speaker_id": "1",
