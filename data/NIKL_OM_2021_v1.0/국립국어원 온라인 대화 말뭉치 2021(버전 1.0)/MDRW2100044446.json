      "utterance": [
        {
          "id": "MDRW2100044446.1.1",
          "form": "샤인 과육이 보돌느낌?",
          "original_form": "샤인 과육이 보돌느낌?",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044446.1.2",
          "form": "보돌?",
          "original_form": "보돌?",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044446.1.3",
          "form": "기본당도가 16?",
          "original_form": "기본당도가 16?",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044446.1.4",
          "form": "당도도 높아서",
          "original_form": "당도도 높아서",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044446.1.5",
          "form": "포도 과육이",
          "original_form": "포도 과육이",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044446.1.6",
          "form": "망고 비슷",
          "original_form": "망고 비슷",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044446.1.7",
          "form": "포도인데 망고느낌?",
          "original_form": "포도인데 망고느낌?",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044446.1.8",
          "form": "일반포도보다 과육이 연해",
          "original_form": "일반포도보다 과육이 연해",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044446.1.9",
          "form": "오 그렇구나",
          "original_form": "오 그렇구나",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044446.1.10",
          "form": "탱땡한데 연한 느낌",
          "original_form": "탱땡한데 연한 느낌",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044446.1.11",
          "form": "막 상상이 안가네 ㅋㅋ",
          "original_form": "막 상상이 안가네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044446.1.12",
          "form": "담에 먹어볼일 있겠지 ㅋㅋ",
          "original_form": "담에 먹어볼일 있겠지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044446.1.13",
          "form": "난 역시 딸기가최고",
          "original_form": "난 역시 딸기가최고",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044446.1.14",
          "form": "응ㅎ",
          "original_form": "응ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044446.1.15",
          "form": "설향 딸기 좋지",
          "original_form": "설향 딸기 좋지",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044446.1.16",
          "form": "과일은 제철과일 짱짱",
          "original_form": "과일은 제철과일 짱짱",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044446.1.17",
          "form": "마쟈 가격도 저렴하고 맛있고",
          "original_form": "마쟈 가격도 저렴하고 맛있고",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044446.1.18",
          "form": "난 캠벨 포도는 싫더라",
          "original_form": "난 캠벨 포도는 싫더라",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044446.1.19",
          "form": "껍찔채 먹고싶어",
          "original_form": "껍찔채 먹고싶어",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044446.1.20",
          "form": "편하자나ㅋㅋ",
          "original_form": "편하자나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044446.1.21",
          "form": "편해서 좋아",
          "original_form": "편해서 좋아",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044446.1.22",
          "form": "그래도 맛은 머루포도ㅋㅋ",
          "original_form": "그래도 맛은 머루포도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044446.1.23",
          "form": "씨많은 머루ㅜㅠ",
          "original_form": "씨많은 머루ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044446.1.24",
          "form": "머루포도는 맛있어",
          "original_form": "머루포도는 맛있어",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044446.1.25",
          "form": "난 씨도삼켜",
          "original_form": "난 씨도삼켜",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044446.1.26",
          "form": "포도씨 안좋다는 얘기듣고 다 분리 배출함ㅋ",
          "original_form": "포도씨 안좋다는 얘기듣고 다 분리 배출함ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044446.1.27",
          "form": "헐 진짜?",
          "original_form": "헐 진짜?",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044446.1.28",
          "form": "어릴땐 꿀떡 삼켷었지",
          "original_form": "어릴땐 꿀떡 삼켷었지",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044446.1.29",
          "form": "나 여태껏 먹어왔는데",
          "original_form": "나 여태껏 먹어왔는데",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044446.1.30",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044446.1.31",
          "form": "씨 분리 귀찮긴해",
          "original_form": "씨 분리 귀찮긴해",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044446.1.32",
          "form": "근데 인자 습관돼서 씨를 못먹어",
          "original_form": "근데 인자 습관돼서 씨를 못먹어",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044446.1.33",
          "form": "ㅇㅇ 그냥 씨포도를 안먹을래 ㅜㅜ",
          "original_form": "ㅇㅇ 그냥 씨포도를 안먹을래 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044446.1.34",
          "form": "대신 과육이랑 껍질살 맛나게 먹게되었어",
          "original_form": "대신 과육이랑 껍질살 맛나게 먹게되었어",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044446.1.35",
          "form": "그 포도과육 씹음 맛있어",
          "original_form": "그 포도과육 씹음 맛있어",
          "speaker_id": "2",
          "time": "20210907 22:56"
        }
      ]
    }
