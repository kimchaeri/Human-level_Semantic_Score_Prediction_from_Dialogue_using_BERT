      "utterance": [
        {
          "id": "MDRW2100034513.1.1",
          "form": "저녁드셨어요?!",
          "original_form": "저녁드셨어요?!",
          "speaker_id": "2",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100034513.1.2",
          "form": "아직이요 ㅜㅜ",
          "original_form": "아직이요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100034513.1.3",
          "form": "드셨나요?!",
          "original_form": "드셨나요?!",
          "speaker_id": "1",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100034513.1.4",
          "form": "네네 볶음밥해먹엇어요",
          "original_form": "네네 볶음밥해먹엇어요",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100034513.1.5",
          "form": "왜 아직 못드셨나요 ㅠ",
          "original_form": "왜 아직 못드셨나요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100034513.1.6",
          "form": "점심에 바가지로 먹었더니 배불러서",
          "original_form": "점심에 바가지로 먹었더니 배불러서",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034513.1.7",
          "form": "좀이따 먹으려구요 ㅋㅋ",
          "original_form": "좀이따 먹으려구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100034513.1.8",
          "form": "아 ㅋㅋㅋㅋㅋ점심으루 뭐드셨는데요?",
          "original_form": "아 ㅋㅋㅋㅋㅋ점심으루 뭐드셨는데요?",
          "speaker_id": "2",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100034513.1.9",
          "form": "닭갈비랑 볶음밥이요 ㅋㅋㅋ",
          "original_form": "닭갈비랑 볶음밥이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034513.1.10",
          "form": "아아ㅋㅋㅋㅋㅋ",
          "original_form": "아아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100034513.1.11",
          "form": "밤에 갑자기 배고파지는거아니에요?",
          "original_form": "밤에 갑자기 배고파지는거아니에요?",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034513.1.12",
          "form": "아 그건 괜찮아요 왜냐면",
          "original_form": "아 그건 괜찮아요 왜냐면",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034513.1.13",
          "form": "좀이따 먹긴 할거거든요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "좀이따 먹긴 할거거든요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100034513.1.14",
          "form": "아아 ㅋㅋㅋㅋㅋ그렇군욬ㅋㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋㅋㅋ그렇군욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034513.1.15",
          "form": "뭐드실예정이세요 ㅋㅋㅋ",
          "original_form": "뭐드실예정이세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034513.1.16",
          "form": "이따 또 바가지로 넣을려구용",
          "original_form": "이따 또 바가지로 넣을려구용",
          "speaker_id": "1",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034513.1.17",
          "form": "흠 모르겠어요 아직 ㅋㅋㅋㅋ",
          "original_form": "흠 모르겠어요 아직 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100034513.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 피자 야식으로 먹을거같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 피자 야식으로 먹을거같아요",
          "speaker_id": "2",
          "time": "20210828 19:58"
        },
        {
          "id": "MDRW2100034513.1.19",
          "form": "피자를 야식으로 드시다니",
          "original_form": "피자를 야식으로 드시다니",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034513.1.20",
          "form": "부러워요 ㅠㅠ",
          "original_form": "부러워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100034513.1.21",
          "form": "좋긴한데 살찔각이여서 걱정되네욬ㅋㅋㅋㅋ",
          "original_form": "좋긴한데 살찔각이여서 걱정되네욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100034513.1.22",
          "form": "동생이 먹으려고 시킬려구하더라구요",
          "original_form": "동생이 먹으려고 시킬려구하더라구요",
          "speaker_id": "2",
          "time": "20210828 20:01"
        }
      ]
