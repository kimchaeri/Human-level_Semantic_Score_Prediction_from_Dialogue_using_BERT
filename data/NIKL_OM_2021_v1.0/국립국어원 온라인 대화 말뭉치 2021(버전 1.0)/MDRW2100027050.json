      "utterance": [
        {
          "id": "MDRW2100027050.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100027050.1.2",
          "form": "아니요. 지금 구피키우고있어요.",
          "original_form": "아니요. 지금 구피키우고있어요.",
          "speaker_id": "2",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100027050.1.3",
          "form": "동물키우세요?",
          "original_form": "동물키우세요?",
          "speaker_id": "2",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100027050.1.4",
          "form": "저도ㅜ구피요",
          "original_form": "저도ㅜ구피요",
          "speaker_id": "1",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100027050.1.5",
          "form": "오ㅋㅋ저도 구피요ㅋㅋ",
          "original_form": "오ㅋㅋ저도 구피요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100027050.1.6",
          "form": "키운지 얼마나 되셨어요?",
          "original_form": "키운지 얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100027050.1.7",
          "form": "6개월료",
          "original_form": "6개월료",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027050.1.8",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027050.1.9",
          "form": "저는 3~4년된것같아요.",
          "original_form": "저는 3~4년된것같아요.",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027050.1.10",
          "form": "오래됬네요",
          "original_form": "오래됬네요",
          "speaker_id": "1",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027050.1.11",
          "form": "네 중간에 누구줬다가 다시받았어요",
          "original_form": "네 중간에 누구줬다가 다시받았어요",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027050.1.12",
          "form": "물관리 힘들잖아요",
          "original_form": "물관리 힘들잖아요",
          "speaker_id": "1",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027050.1.13",
          "form": "네 힘든데 애들이 좋아해서요ㅠ",
          "original_form": "네 힘든데 애들이 좋아해서요ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027050.1.14",
          "form": "하긴 저도",
          "original_form": "하긴 저도",
          "speaker_id": "1",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027050.1.15",
          "form": "ㅋㅋ 누구주자고하니 싫대요",
          "original_form": "ㅋㅋ 누구주자고하니 싫대요",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027050.1.16",
          "form": "울더라구요",
          "original_form": "울더라구요",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027050.1.17",
          "form": "저도 매일 밥주죠",
          "original_form": "저도 매일 밥주죠",
