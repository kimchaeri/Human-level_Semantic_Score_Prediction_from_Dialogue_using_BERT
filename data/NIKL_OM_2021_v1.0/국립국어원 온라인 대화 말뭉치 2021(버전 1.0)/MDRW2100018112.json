      "utterance": [
        {
          "id": "MDRW2100018112.1.1",
          "form": "카드한도 체크하느라 제가 사려고요",
          "original_form": "카드한도 체크하느라 제가 사려고요",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100018112.1.2",
          "form": "혜택많은 카드로 몰아서 쓰시는구나",
          "original_form": "혜택많은 카드로 몰아서 쓰시는구나",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018112.1.3",
          "form": "네 담달 마지막으로 단종되는 신카라ㅜㅠ",
          "original_form": "네 담달 마지막으로 단종되는 신카라ㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100018112.1.4",
          "form": "귀금속사고싶은데 진짜 돈이..",
          "original_form": "귀금속사고싶은데 진짜 돈이..",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018112.1.5",
          "form": "귀금속은 마음에 드는걸 사야하지만 그런건 가격이 비싸죠ㅠㅜ",
          "original_form": "귀금속은 마음에 드는걸 사야하지만 그런건 가격이 비싸죠ㅠㅜ",
          "speaker_id": "2",
          "time": "20210826 22:46"
        },
        {
          "id": "MDRW2100018112.1.6",
          "form": "전 이번주말에 머리하러 갑니다 ㅎㅎ",
          "original_form": "전 이번주말에 머리하러 갑니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018112.1.7",
          "form": "5천원 할인쿠폰이 있고",
          "original_form": "5천원 할인쿠폰이 있고",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018112.1.8",
          "form": "해야할 시기라 ㅋㅋㅋㄱㄱ",
          "original_form": "해야할 시기라 ㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018112.1.9",
          "form": "아니 그거 사연이 많은데ㅋㅋ",
          "original_form": "아니 그거 사연이 많은데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018112.1.10",
          "form": "머리 이쁘게하세요",
          "original_form": "머리 이쁘게하세요",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018112.1.11",
          "form": "네 고마워요",
          "original_form": "네 고마워요",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018112.1.12",
          "form": "닭갈비사려고했는데 기존 닭갈비키트는 매진이네요",
          "original_form": "닭갈비사려고했는데 기존 닭갈비키트는 매진이네요",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018112.1.13",
          "form": "아 아쉽겠네요",
          "original_form": "아 아쉽겠네요",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018112.1.14",
          "form": "그게 맛있던가요?",
          "original_form": "그게 맛있던가요?",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018112.1.15",
          "form": "맛나더라고요 양이 코딱지만해서...",
          "original_form": "맛나더라고요 양이 코딱지만해서...",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018112.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018112.1.17",
          "form": "키트제품이 그렇죠ㅜ",
          "original_form": "키트제품이 그렇죠ㅜ",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018112.1.18",
          "form": "재도전하고싶었는데 품절ㅜ",
          "original_form": "재도전하고싶었는데 품절ㅜ",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018112.1.19",
          "form": "다른것도 구매해보아요",
          "original_form": "다른것도 구매해보아요",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018112.1.20",
          "form": "안그래도 다른 후보가 있어요",
          "original_form": "안그래도 다른 후보가 있어요",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018112.1.21",
          "form": "냉동실공간때문에 망설이고있어요",
          "original_form": "냉동실공간때문에 망설이고있어요",
          "speaker_id": "1",
          "time": "20210826 22:56"
        }
      ]
