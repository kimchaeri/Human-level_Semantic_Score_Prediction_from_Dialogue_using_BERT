      "utterance": [
        {
          "id": "MDRW2100045343.1.1",
          "form": "점장님이 자리를 자주 비우세요?",
          "original_form": "점장님이 자리를 자주 비우세요?",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045343.1.2",
          "form": "점장님이 주3회 출근하시는데",
          "original_form": "점장님이 주3회 출근하시는데",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045343.1.3",
          "form": "스케줄이 유동적이셔서 제가 일할때 있을때도 없을때도 있어요 ㅜㅜㅋㅋㅋ",
          "original_form": "스케줄이 유동적이셔서 제가 일할때 있을때도 없을때도 있어요 ㅜㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045343.1.4",
          "form": "앗ㅜㅜ",
          "original_form": "앗ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045343.1.5",
          "form": "그래도 같이 일하시는 분들이 좋다니 다행이에요",
          "original_form": "그래도 같이 일하시는 분들이 좋다니 다행이에요",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045343.1.6",
          "form": "직장에서는 사람만 잘 만나도 중간은 가는거같아요...",
          "original_form": "직장에서는 사람만 잘 만나도 중간은 가는거같아요...",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045343.1.7",
          "form": "그러니까요ㅠㅠㅋㅋㅋ 인턴 경험 있다고 하셨던건 어떠셨어요?!",
          "original_form": "그러니까요ㅠㅠㅋㅋㅋ 인턴 경험 있다고 하셨던건 어떠셨어요?!",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045343.1.8",
          "form": "회사에서 까마득한 막내여서",
          "original_form": "회사에서 까마득한 막내여서",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045343.1.9",
          "form": "얼떨결에 다녔던거같아요 ㅋㅋㅋ",
          "original_form": "얼떨결에 다녔던거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045343.1.10",
          "form": "헉 우와ㅠㅠㅋㅋㅋ 그래도 너무 멋있어요",
          "original_form": "헉 우와ㅠㅠㅋㅋㅋ 그래도 너무 멋있어요",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045343.1.11",
          "form": "전 알바경험만 있어서 그런가 엄청 어른같아요",
          "original_form": "전 알바경험만 있어서 그런가 엄청 어른같아요",
          "speaker_id": "2",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045343.1.12",
          "form": "진짜 별거 아니에요 ㅋㅋㅋㅋ ㅠ",
          "original_form": "진짜 별거 아니에요 ㅋㅋㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045343.1.13",
          "form": "그리고 결국은 고객 상대하는거라",
          "original_form": "그리고 결국은 고객 상대하는거라",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045343.1.14",
          "form": "알바 많이 해보신분들이 능숙하게 대처하세요",
          "original_form": "알바 많이 해보신분들이 능숙하게 대처하세요",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045343.1.15",
          "form": "회사생활에도 역시 진상 고객은 있는거군요..ㅠㅠ 아이고 어렵다",
          "original_form": "회사생활에도 역시 진상 고객은 있는거군요..ㅠㅠ 아이고 어렵다",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045343.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045343.1.17",
          "form": "그래도 월급 들어올때가 보람차니까 힘냅시다ㅠㅠ!!!!ㅋㅋㅋ",
          "original_form": "그래도 월급 들어올때가 보람차니까 힘냅시다ㅠㅠ!!!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045343.1.18",
          "form": "알바 한지 얼마나 되셧어요?",
          "original_form": "알바 한지 얼마나 되셧어요?",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045343.1.19",
          "form": "여기서 일한지는 이제 3달됐어요!!",
          "original_form": "여기서 일한지는 이제 3달됐어요!!",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045343.1.20",
          "form": "그 전에는 공연쪽 알바했거든요 ㅎㅎ",
          "original_form": "그 전에는 공연쪽 알바했거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045343.1.21",
          "form": "와 공연쪽이면",
          "original_form": "와 공연쪽이면",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045343.1.22",
          "form": "스태프인가요??",
          "original_form": "스태프인가요??",
          "speaker_id": "1",
