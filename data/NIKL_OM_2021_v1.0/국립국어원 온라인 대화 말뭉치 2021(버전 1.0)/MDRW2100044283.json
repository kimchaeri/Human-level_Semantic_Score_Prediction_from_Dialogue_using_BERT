      "utterance": [
        {
          "id": "MDRW2100044283.1.1",
          "form": "두부는 질겅거려",
          "original_form": "두부는 질겅거려",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044283.1.2",
          "form": "그래서 취향이 아니더라",
          "original_form": "그래서 취향이 아니더라",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044283.1.3",
          "form": "오잉 진짜? 두부가 말랑거리는 게 아니야?",
          "original_form": "오잉 진짜? 두부가 말랑거리는 게 아니야?",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044283.1.4",
          "form": "된장찌개 두부같은 건 엄청 부드럽잖아",
          "original_form": "된장찌개 두부같은 건 엄청 부드럽잖아",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044283.1.5",
          "form": "응 거긴 건두부라",
          "original_form": "응 거긴 건두부라",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044283.1.6",
          "form": "우리나라 두부랑 달라",
          "original_form": "우리나라 두부랑 달라",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044283.1.7",
          "form": "되게 궁금하다",
          "original_form": "되게 궁금하다",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044283.1.8",
          "form": "이전에 마라탕 컵라면은 먹어봤는데",
          "original_form": "이전에 마라탕 컵라면은 먹어봤는데",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044283.1.9",
          "form": "거기에 들어있는 포두부인가는",
          "original_form": "거기에 들어있는 포두부인가는",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044283.1.10",
          "form": "안 질겼던 것 같은데 정식 마라탕이랑 다르려나",
          "original_form": "안 질겼던 것 같은데 정식 마라탕이랑 다르려나",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044283.1.11",
          "form": "취향이니",
          "original_form": "취향이니",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044283.1.12",
          "form": "나쁘지 않았어",
          "original_form": "나쁘지 않았어",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044283.1.13",
          "form": "그럼 너한테 맞을수도있어",
          "original_form": "그럼 너한테 맞을수도있어",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044283.1.14",
          "form": "난 무슨 콩껍데기같아서",
          "original_form": "난 무슨 콩껍데기같아서",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044283.1.15",
          "form": "별로더라고",
          "original_form": "별로더라고",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044283.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ궁금한 재료 하나씩 다 넣어버려야지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ궁금한 재료 하나씩 다 넣어버려야지",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044283.1.17",
          "form": "콩과 관련된 걸",
          "original_form": "콩과 관련된 걸",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044283.1.18",
          "form": "모든 걸 싫어하는구나",
          "original_form": "모든 걸 싫어하는구나",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044283.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044283.1.20",
          "form": "음식으로",
          "original_form": "음식으로",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044283.1.21",
          "form": "사랑해줄수없어",
          "original_form": "사랑해줄수없어",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044283.1.22",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044283.1.23",
          "form": "위정척사파다",
          "original_form": "위정척사파다",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044283.1.24",
          "form": "궁금한 재료 진짜",
          "original_form": "궁금한 재료 진짜",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044283.1.25",
          "form": "ㅋㅋㅋㅋㄱㅋㄱㅋㄱㅋㄱㄱㄱ난 나만의 길을 간다...,.",
          "original_form": "ㅋㅋㅋㅋㄱㅋㄱㅋㄱㅋㄱㄱㄱ난 나만의 길을 간다...,.",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044283.1.26",
          "form": "면도 여러종류 먹어봐",
          "original_form": "면도 여러종류 먹어봐",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044283.1.27",
          "form": "다 먹어볼거야",
          "original_form": "다 먹어볼거야",
          "speaker_id": "1",
          "time": "20210907 22:12"
        }
      ]
