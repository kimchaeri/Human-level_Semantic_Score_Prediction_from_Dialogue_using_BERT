      "utterance": [
        {
          "id": "MDRW2100019473.1.1",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "1",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019473.1.2",
          "form": "안녕하세요. 운동하시나요?",
          "original_form": "안녕하세요. 운동하시나요?",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019473.1.3",
          "form": "가끔 홈트만 하고 있어요. name2님은요?",
          "original_form": "가끔 홈트만 하고 있어요. &name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019473.1.4",
          "form": "저는 얼마전에 실내자전거를 사서 타고있어요!",
          "original_form": "저는 얼마전에 실내자전거를 사서 타고있어요!",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019473.1.5",
          "form": "와 부러워요 ㅋㅋ 저는 놓을 공간이 애매해서 사고싶은데 못 사고 있어요.",
          "original_form": "와 부러워요 ㅋㅋ 저는 놓을 공간이 애매해서 사고싶은데 못 사고 있어요.",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019473.1.6",
          "form": "어떠신가요?",
          "original_form": "어떠신가요?",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019473.1.7",
          "form": "아직은 열심히 타고 있는데 언제 옷걸이가 될 지 모르겠어요 ㅋㅋㅋ",
          "original_form": "아직은 열심히 타고 있는데 언제 옷걸이가 될 지 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019473.1.8",
          "form": "ㅋㅋㅋ 구매를 망설이게 되는 가장 큰 이유죠",
          "original_form": "ㅋㅋㅋ 구매를 망설이게 되는 가장 큰 이유죠",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019473.1.9",
          "form": "홈트는 유튜브 보고 따라하시는건가요? ㅎㅎ",
          "original_form": "홈트는 유튜브 보고 따라하시는건가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019473.1.10",
          "form": "저는 핏데이라는 어플을 보고 따라하고 있어요",
          "original_form": "저는 핏데이라는 어플을 보고 따라하고 있어요",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019473.1.11",
          "form": "자세하게 설명되어있나요?",
          "original_form": "자세하게 설명되어있나요?",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019473.1.12",
          "form": "설명이랑 자세 보고 따라하면 돼요. 순서대로 운동 코스를 알려주는 식이에요.",
          "original_form": "설명이랑 자세 보고 따라하면 돼요. 순서대로 운동 코스를 알려주는 식이에요.",
          "speaker_id": "1",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100019473.1.13",
          "form": "좋군요! 혼자서 꾸준히만 한다면 요즘 운동하는 법은 무궁무진한 것 같아요!",
          "original_form": "좋군요! 혼자서 꾸준히만 한다면 요즘 운동하는 법은 무궁무진한 것 같아요!",
          "speaker_id": "2",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100019473.1.14",
          "form": "ㅋㅋ 근데 꾸준히가 진짜 어렵더라구요ㅠ",
          "original_form": "ㅋㅋ 근데 꾸준히가 진짜 어렵더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019473.1.15",
          "form": "맞아요 ㅠㅠ 전 근육을 갖고 싶은데 근력운동해야한다더라구요",
          "original_form": "맞아요 ㅠㅠ 전 근육을 갖고 싶은데 근력운동해야한다더라구요",
          "speaker_id": "2",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019473.1.16",
          "form": "근력운동 진짜 힘들어요ㅠ",
          "original_form": "근력운동 진짜 힘들어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:59"
        }
      ]
