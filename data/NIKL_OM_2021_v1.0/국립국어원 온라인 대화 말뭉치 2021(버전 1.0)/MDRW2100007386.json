      "utterance": [
        {
          "id": "MDRW2100007386.1.1",
          "form": "저도 이직한 케이스라서",
          "original_form": "저도 이직한 케이스라서",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007386.1.2",
          "form": "전 직장은 이직하면",
          "original_form": "전 직장은 이직하면",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007386.1.3",
          "form": "끝이에요 사실",
          "original_form": "끝이에요 사실",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007386.1.4",
          "form": "아하...! 이직이셨군요",
          "original_form": "아하...! 이직이셨군요",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007386.1.5",
          "form": "이직할때 되게 티 안내도록 조심해야겟어요",
          "original_form": "이직할때 되게 티 안내도록 조심해야겟어요",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007386.1.6",
          "form": "제친구도 이직햇는데 연 다 끊엇더라구요",
          "original_form": "제친구도 이직햇는데 연 다 끊엇더라구요",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007386.1.7",
          "form": "음 이직하고도 챙길 \u0000사람 챙겨야지 했는데",
          "original_form": "음 이직하고도 챙길 \u0000사람 챙겨야지 했는데",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007386.1.8",
          "form": "점점 멀어지더라구요",
          "original_form": "점점 멀어지더라구요",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007386.1.9",
          "form": "사회생활이란게 그런건가봐요",
          "original_form": "사회생활이란게 그런건가봐요",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007386.1.10",
          "form": "학창시절과는 달리...",
          "original_form": "학창시절과는 달리...",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007386.1.11",
          "form": "그렇게 어른이되나봐요",
          "original_form": "그렇게 어른이되나봐요",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007386.1.12",
          "form": "네 점점 나이드는게 그런거 아닐까요ㅎㅎ",
          "original_form": "네 점점 나이드는게 그런거 아닐까요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007386.1.13",
          "form": "학창시절때 알바하며 지내던 그때로 돌아가고싶네요...!",
          "original_form": "학창시절때 알바하며 지내던 그때로 돌아가고싶네요...!",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007386.1.14",
          "form": "아 그때가 행복했습니다",
          "original_form": "아 그때가 행복했습니다",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007386.1.15",
          "form": "알바 뭐하셨나용ㅎ",
          "original_form": "알바 뭐하셨나용ㅎ",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007386.1.16",
          "form": "저는 집앞에 편의점이요ㅎㅎ",
          "original_form": "저는 집앞에 편의점이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007386.1.17",
          "form": "쿼님은요?",
          "original_form": "쿼님은요?",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007386.1.18",
          "form": "저는 이것저것햇네요",
          "original_form": "저는 이것저것햇네요",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007386.1.19",
          "form": "카페 빵집 애슐리 pc...방 이게 나쁜말이라니",
          "original_form": "카페 빵집 애슐리 pc...방 이게 나쁜말이라니",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007386.1.20",
          "form": "와우 알바 경험 다수네요",
          "original_form": "와우 알바 경험 다수네요",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007386.1.21",
          "form": "드클이라구 그걸 제일 오래했어요 ㅎ 진짜 그립네요 대학생ㅠ",
          "original_form": "드클이라구 그걸 제일 오래했어요 ㅎ 진짜 그립네요 대학생ㅠ",
          "speaker_id": "1",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007386.1.22",
          "form": "드클?",
          "original_form": "드클?",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007386.1.23",
          "form": "그게 뭐죠ㅎㅎ",
          "original_form": "그게 뭐죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007386.1.24",
          "form": "드림클래스라구 수학강의하는건데",
          "original_form": "드림클래스라구 수학강의하는건데",
          "speaker_id": "1",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007386.1.25",
          "form": "시급이 4만원쯤나와요!!삼성주최라서",
          "original_form": "시급이 4만원쯤나와요!!삼성주최라서",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007386.1.26",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007386.1.27",
          "form": "대학생에게만 관대한사회 ㅠ",
          "original_form": "대학생에게만 관대한사회 ㅠ",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007386.1.28",
          "form": "삼성 드림클래스",
          "original_form": "삼성 드림클래스",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007386.1.29",
          "form": "그거 알아요",
          "original_form": "그거 알아요",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007386.1.30",
          "form": "저 그거 진짜 대학다니는 내내 했어요",
          "original_form": "저 그거 진짜 대학다니는 내내 했어요",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007386.1.31",
          "form": "9학기한 수의대생도 봤어요",
          "original_form": "9학기한 수의대생도 봤어요",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007386.1.32",
          "form": "고학력자셨군요!",
          "original_form": "고학력자셨군요!",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007386.1.33",
          "form": "아닙니다ㅋㅋ지방대에요!!!",
          "original_form": "아닙니다ㅋㅋ지방대에요!!!",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007386.1.34",
          "form": "지거국!!!!!",
          "original_form": "지거국!!!!!",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007386.1.35",
          "form": "ㅠ요즘은 미달이라구하더라구요 부산대도",
          "original_form": "ㅠ요즘은 미달이라구하더라구요 부산대도",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007386.1.36",
          "form": "공기업 다니시는 안녕하산님이 멋있을뿐이에요 전..",
          "original_form": "공기업 다니시는 안녕하산님이 멋있을뿐이에요 전..",
