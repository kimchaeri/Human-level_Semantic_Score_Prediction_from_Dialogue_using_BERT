      "utterance": [
        {
          "id": "MDRW2100041172.1.1",
          "form": "얼마전에 KFC앞에 우연히 갔는데",
          "original_form": "얼마전에 KFC앞에 우연히 갔는데",
          "speaker_id": "1",
          "time": "20210905 19:24"
        },
        {
          "id": "MDRW2100041172.1.2",
          "form": "8시 50분인거에요 9시부터 치킨 1+1이라면서요 ㅎㅎ",
          "original_form": "8시 50분인거에요 9시부터 치킨 1+1이라면서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100041172.1.3",
          "form": "아 맞아요 ㅋㅋ 나이트 어쩌구",
          "original_form": "아 맞아요 ㅋㅋ 나이트 어쩌구",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100041172.1.4",
          "form": "기다렸나요 ㅋㅋ",
          "original_form": "기다렸나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100041172.1.5",
          "form": "10분 기다릴까하다 그냥 왔어요 ㅜㅠ ㅋㅋ",
          "original_form": "10분 기다릴까하다 그냥 왔어요 ㅜㅠ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100041172.1.6",
          "form": "그거 치킨 대신에 그렇게 먹는 사람도 있더라고요",
          "original_form": "그거 치킨 대신에 그렇게 먹는 사람도 있더라고요",
          "speaker_id": "2",
          "time": "20210905 19:25"
        },
        {
          "id": "MDRW2100041172.1.7",
          "form": "그렇게 먹는게 어떤거에요?",
          "original_form": "그렇게 먹는게 어떤거에요?",
          "speaker_id": "1",
          "time": "20210905 19:26"
        },
        {
          "id": "MDRW2100041172.1.8",
          "form": "어떤 매장은 콜라빼고 맥주로 바꿀수도 있어요",
          "original_form": "어떤 매장은 콜라빼고 맥주로 바꿀수도 있어요",
          "speaker_id": "2",
          "time": "20210905 19:26"
        },
        {
          "id": "MDRW2100041172.1.9",
          "form": "아 원뿔원으로 많이 해서요",
          "original_form": "아 원뿔원으로 많이 해서요",
          "speaker_id": "2",
          "time": "20210905 19:26"
        },
        {
          "id": "MDRW2100041172.1.10",
          "form": "맥주좋아하는 사람 좋아하겎어요 ㅋㅋ",
          "original_form": "맥주좋아하는 사람 좋아하겎어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:26"
        },
        {
          "id": "MDRW2100041172.1.11",
          "form": "네 한번 바꿀수있대서 그렇게 먹어봤는데 색다르더라고요 ㅋㅋ",
          "original_form": "네 한번 바꿀수있대서 그렇게 먹어봤는데 색다르더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:27"
        },
        {
          "id": "MDRW2100041172.1.12",
          "form": "네 저도 먹어보고 싶어요",
          "original_form": "네 저도 먹어보고 싶어요",
          "speaker_id": "1",
          "time": "20210905 19:27"
        },
        {
          "id": "MDRW2100041172.1.13",
          "form": "블랙라벨인가 하는거 한번 먹어봤는데요 맛있었어요",
          "original_form": "블랙라벨인가 하는거 한번 먹어봤는데요 맛있었어요",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100041172.1.14",
          "form": "아 맞아요 저도 전에 먹어봤는데",
          "original_form": "아 맞아요 저도 전에 먹어봤는데",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100041172.1.15",
          "form": "근데 맛이 기억이 안나네요 ㅋㅋ",
          "original_form": "근데 맛이 기억이 안나네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100041172.1.16",
          "form": "부드럽더라구요",
          "original_form": "부드럽더라구요",
          "speaker_id": "1",
          "time": "20210905 19:28"
        },
        {
          "id": "MDRW2100041172.1.17",
          "form": "약간 순살느낌나구요 저도 자세히 기억이 다 안나요",
          "original_form": "약간 순살느낌나구요 저도 자세히 기억이 다 안나요",
          "speaker_id": "1",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100041172.1.18",
          "form": "오 고기를 더 좋은걸 쓰는걸까요",
          "original_form": "오 고기를 더 좋은걸 쓰는걸까요",
          "speaker_id": "2",
          "time": "20210905 19:29"
        },
        {
          "id": "MDRW2100041172.1.19",
          "form": "웬지 그럴거같아요 이름부터 블랙라벨이잖아요",
          "original_form": "웬지 그럴거같아요 이름부터 블랙라벨이잖아요",
