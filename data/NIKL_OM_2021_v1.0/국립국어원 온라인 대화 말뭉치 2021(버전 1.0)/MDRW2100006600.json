      "utterance": [
        {
          "id": "MDRW2100006600.1.1",
          "form": "나 코스트코 가야해ㅋㅋ",
          "original_form": "나 코스트코 가야해ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100006600.1.2",
          "form": "이번주에 갔다왔잖아 뭐 또 살거있어?",
          "original_form": "이번주에 갔다왔잖아 뭐 또 살거있어?",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100006600.1.3",
          "form": "머핀이랑 갈릭바게트 다 먹음",
          "original_form": "머핀이랑 갈릭바게트 다 먹음",
          "speaker_id": "1",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100006600.1.4",
          "form": "벌써? 머핀은 두박스나 샀잖아",
          "original_form": "벌써? 머핀은 두박스나 샀잖아",
          "speaker_id": "2",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100006600.1.5",
          "form": "머핀 한번 먹으면 멈출수없어",
          "original_form": "머핀 한번 먹으면 멈출수없어",
          "speaker_id": "1",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100006600.1.6",
          "form": "하긴 너 옛날에 코스트코 머핀 소분된거 엄청 사먹었잖아",
          "original_form": "하긴 너 옛날에 코스트코 머핀 소분된거 엄청 사먹었잖아",
          "speaker_id": "2",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100006600.1.7",
          "form": "1일 2머핀 그것도 시나몬으로",
          "original_form": "1일 2머핀 그것도 시나몬으로",
          "speaker_id": "1",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100006600.1.8",
          "form": "시나몬 맛있는지 잘 모르겠던데 신기해",
          "original_form": "시나몬 맛있는지 잘 모르겠던데 신기해",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100006600.1.9",
          "form": "시나몬 최고지ㅋ 무시하지말라구우!!.",
          "original_form": "시나몬 최고지ㅋ 무시하지말라구우!!.",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100006600.1.10",
          "form": "알겠어 머핀하고 또 다른것도 살거있어?",
          "original_form": "알겠어 머핀하고 또 다른것도 살거있어?",
          "speaker_id": "2",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100006600.1.11",
          "form": "뭐냐 그 우유에 말아먹는거 ...",
          "original_form": "뭐냐 그 우유에 말아먹는거 ...",
          "speaker_id": "1",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100006600.1.12",
          "form": "콘프로스트 ㅋㅋㅋㅋ 별걸 다 기억못해",
          "original_form": "콘프로스트 ㅋㅋㅋㅋ 별걸 다 기억못해",
          "speaker_id": "2",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100006600.1.13",
          "form": "그래 콘프로스트 살거야",
          "original_form": "그래 콘프로스트 살거야",
          "speaker_id": "1",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100006600.1.14",
          "form": "소분해놓을테니 픽업하슈",
          "original_form": "소분해놓을테니 픽업하슈",
          "speaker_id": "1",
          "time": "20210908 03:10"
        },
