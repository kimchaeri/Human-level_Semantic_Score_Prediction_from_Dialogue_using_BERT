      "utterance": [
        {
          "id": "MDRW2100016050.1.1",
          "form": "적금보험은 보험기는 있다캐가 생각했는데.",
          "original_form": "적금보험은 보험기는 있다캐가 생각했는데.",
          "speaker_id": "2",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016050.1.2",
          "form": "보험쪽이",
          "original_form": "보험쪽이",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016050.1.3",
          "form": "혹시나 해약하는 뭐",
          "original_form": "혹시나 해약하는 뭐",
          "speaker_id": "2",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016050.1.4",
          "form": "일단은 만기를해야되는데",
          "original_form": "일단은 만기를해야되는데",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016050.1.5",
          "form": "해약하믄 한개도 못받으니..",
          "original_form": "해약하믄 한개도 못받으니..",
          "speaker_id": "2",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016050.1.6",
          "form": "해지하면 손해라고",
          "original_form": "해지하면 손해라고",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016050.1.7",
          "form": "차라리",
          "original_form": "차라리",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016050.1.8",
          "form": "그냥 적금들어주는게 낫다더라고",
          "original_form": "그냥 적금들어주는게 낫다더라고",
          "speaker_id": "1",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016050.1.9",
          "form": "글쿠마이~",
          "original_form": "글쿠마이~",
          "speaker_id": "2",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016050.1.10",
          "form": "난도 오늘 통장만들러 고고~",
          "original_form": "난도 오늘 통장만들러 고고~",
          "speaker_id": "2",
          "time": "20210826 08:47"
        },
        {
          "id": "MDRW2100016050.1.11",
          "form": "그래서 나는 그냥 애들 청약통장에 매달 2만원씩 넣어주고있다ㅋ",
          "original_form": "그래서 나는 그냥 애들 청약통장에 매달 2만원씩 넣어주고있다ㅋ",
          "speaker_id": "1",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016050.1.12",
          "form": "난 여태 애들돈 넘 많이써서 ㅋㅋㅋㅋ",
          "original_form": "난 여태 애들돈 넘 많이써서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016050.1.13",
          "form": "애들통장에 많이 넣어주기에는 다른데도 돈 나갈때가 많아서ㅋㅋ",
          "original_form": "애들통장에 많이 넣어주기에는 다른데도 돈 나갈때가 많아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016050.1.14",
          "form": "나두 그래",
          "original_form": "나두 그래",
          "speaker_id": "1",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016050.1.15",
          "form": "이제 첫찌는 돈 개념아니깐 더이상",
          "original_form": "이제 첫찌는 돈 개념아니깐 더이상",
          "speaker_id": "2",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016050.1.16",
          "form": "못뺏아 쓰겠음 ㅋㅋㅋㅋ",
          "original_form": "못뺏아 쓰겠음 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016050.1.17",
          "form": "첫째통장에 40만원인가 있어ㅋㅋ",
          "original_form": "첫째통장에 40만원인가 있어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:48"
        },
        {
          "id": "MDRW2100016050.1.18",
          "form": "그게 어디고 ㅋㅋㅋㅋ",
          "original_form": "그게 어디고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016050.1.19",
          "form": "거의 다 쓰고 넣어준지 얼마안됨ㅋㅋ",
          "original_form": "거의 다 쓰고 넣어준지 얼마안됨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016050.1.20",
          "form": "다빼썼다 ㅋㅋㅋㅋㅋ",
          "original_form": "다빼썼다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:49"
        },
        {
          "id": "MDRW2100016050.1.21",
          "form": "텅장 만들어놨음 ㅋㅋㅋㅋ",
          "original_form": "텅장 만들어놨음 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:49"
        },
        {
