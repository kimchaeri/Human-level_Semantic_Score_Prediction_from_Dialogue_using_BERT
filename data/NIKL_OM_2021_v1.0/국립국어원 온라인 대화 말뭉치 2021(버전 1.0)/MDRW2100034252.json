      "utterance": [
        {
          "id": "MDRW2100034252.1.1",
          "form": "저는 먼저 말 못거는 스타일이라",
          "original_form": "저는 먼저 말 못거는 스타일이라",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034252.1.2",
          "form": "좌석에서도 일어나서 막 뛰면서 놀더라구요",
          "original_form": "좌석에서도 일어나서 막 뛰면서 놀더라구요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034252.1.3",
          "form": "옆분이 걸어주면 넘 좋더라구요",
          "original_form": "옆분이 걸어주면 넘 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034252.1.4",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034252.1.5",
          "form": "그런분들 너무 좋아요",
          "original_form": "그런분들 너무 좋아요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100034252.1.6",
          "form": "옆사람이랑 너무 대화없이 공연보면",
          "original_form": "옆사람이랑 너무 대화없이 공연보면",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034252.1.7",
          "form": "좀 그런데 말걸어주면 더 재밌게 놀수있더라구요",
          "original_form": "좀 그런데 말걸어주면 더 재밌게 놀수있더라구요",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034252.1.8",
          "form": "맞아요 공감도 하고",
          "original_form": "맞아요 공감도 하고",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034252.1.9",
          "form": "같이 응원하면 더 재밌어요",
          "original_form": "같이 응원하면 더 재밌어요",
          "speaker_id": "1",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034252.1.10",
          "form": "마자요 같이 막 앓고ㅋㅋ\n콘서트 스탠딩도 해보셨어요?",
          "original_form": "마자요 같이 막 앓고ㅋㅋ\n콘서트 스탠딩도 해보셨어요?",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034252.1.11",
          "form": "스탠딩이 진리죠 ㅋㅋㅋㅋ",
          "original_form": "스탠딩이 진리죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034252.1.12",
          "form": "가까이서보려면 발은 아작나도 무조건 스탠딩입니다",
          "original_form": "가까이서보려면 발은 아작나도 무조건 스탠딩입니다",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034252.1.13",
          "form": "스탠딩 안 힘드셨어요?ㅠ",
          "original_form": "스탠딩 안 힘드셨어요?ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034252.1.14",
          "form": "너~~~무 힘들어요 정말 ㅠㅠ",
          "original_form": "너~~~무 힘들어요 정말 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100034252.1.15",
          "form": "저는 한번 스탠딩 갔다가 팬스 잡았는데도 뒤에서 밀어가지구 너무 힘들었거든요ㅠ",
          "original_form": "저는 한번 스탠딩 갔다가 팬스 잡았는데도 뒤에서 밀어가지구 너무 힘들었거든요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100034252.1.16",
          "form": "그냥 가수하나 보겠단 일념으로 참아야죠 ㅠㅜ",
          "original_form": "그냥 가수하나 보겠단 일념으로 참아야죠 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100034252.1.17",
          "form": "진짜 중간에 나가고싶더라구요ㅠ",
          "original_form": "진짜 중간에 나가고싶더라구요ㅠ",
