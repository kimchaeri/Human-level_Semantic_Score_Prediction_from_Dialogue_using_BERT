      "utterance": [
        {
          "id": "MDRW2100000645.1.1",
          "form": "싸이월드 도토리 남아있었던가 궁금",
          "original_form": "싸이월드 도토리 남아있었던가 궁금",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000645.1.2",
          "form": "난 도로리를 돈 주고 산 기억이 없어서ㅋㅋㅋ",
          "original_form": "난 도로리를 돈 주고 산 기억이 없어서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000645.1.3",
          "form": "근데 싸이 다시 부활해도 딱히 인기는 없는듯?",
          "original_form": "근데 싸이 다시 부활해도 딱히 인기는 없는듯?",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000645.1.4",
          "form": "응 모바일로 하기 불편하면 전혀 승산 없을 듯",
          "original_form": "응 모바일로 하기 불편하면 전혀 승산 없을 듯",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000645.1.5",
          "form": "그리고 다시 나의 흑역사를 보고싶지도 않고ㅋㅋㅋ",
          "original_form": "그리고 다시 나의 흑역사를 보고싶지도 않고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000645.1.6",
          "form": "추억이라고 열어봤다가 아주 화들짝 ㅋㅋㅋㅋ",
          "original_form": "추억이라고 열어봤다가 아주 화들짝 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000645.1.7",
          "form": "삭제버튼 누르기 바쁨ㅋㅋㅋ",
          "original_form": "삭제버튼 누르기 바쁨ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000645.1.8",
          "form": "다들 없애려고 부활을 기다리는 중 ㅋㅋㅋ",
          "original_form": "다들 없애려고 부활을 기다리는 중 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:26"
        },
        {
          "id": "MDRW2100000645.1.9",
          "form": "싸이 회사는 그걸 바라고 부활시킨게 아닐텐데ㅋㅋㅋ",
          "original_form": "싸이 회사는 그걸 바라고 부활시킨게 아닐텐데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000645.1.10",
          "form": "내 추억은 남몰래 몇개 저장 쩜 하고 ㅋㅋㅋㅋ",
          "original_form": "내 추억은 남몰래 몇개 저장 쩜 하고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000645.1.11",
          "form": "난 추억이랄게 없어 다 그냥 흑역사여ㅋㅋ",
          "original_form": "난 추억이랄게 없어 다 그냥 흑역사여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000645.1.12",
          "form": "근데 뭐 크게 흑역사랄거 있나 ㅋㅋㅋ",
          "original_form": "근데 뭐 크게 흑역사랄거 있나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000645.1.13",
          "form": "내 과거는 그냥 다 흑역사여ㅋㅋㅋ",
          "original_form": "내 과거는 그냥 다 흑역사여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000645.1.14",
          "form": "그때 노래도 많이 사고 그랬는디",
          "original_form": "그때 노래도 많이 사고 그랬는디",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000645.1.15",
          "form": "나도 도토리 산 기억은 없는디 노래는 산 기억이...",
          "original_form": "나도 도토리 산 기억은 없는디 노래는 산 기억이...",
          "speaker_id": "2",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100000645.1.16",
          "form": "도로리가 어디서 나서 노래를 산거지?ㅋㅋ",
          "original_form": "도로리가 어디서 나서 노래를 산거지?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:02"
        },
        {
          "id": "MDRW2100000645.1.17",
          "form": "누가 선물해줬나 ㅋㅋ",
          "original_form": "누가 선물해줬나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100000645.1.18",
          "form": "아닌디ㅋㅋ 선물받은 기억도 읎어ㅋㄱㄲ",
          "original_form": "아닌디ㅋㅋ 선물받은 기억도 읎어ㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210819 18:31"
        },
        {
          "id": "MDRW2100000645.1.19",
          "form": "그나저나 너랑나랑 일촌인가?ㅋㅋ",
          "original_form": "그나저나 너랑나랑 일촌인가?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:31"
        },
        {
          "id": "MDRW2100000645.1.20",
          "form": "당연하지 ㅋㅋㅋ",
          "original_form": "당연하지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100000645.1.21",
          "form": "아 그래ㅋㅋ 그래도 타고 넘어오지 마ㅋㅋㅋㅋㅋ",
          "original_form": "아 그래ㅋㅋ 그래도 타고 넘어오지 마ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100000645.1.22",
          "form": "흑역사 부끄부끄ㅋㅋ",
          "original_form": "흑역사 부끄부끄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100000645.1.23",
          "form": "싸이 시절 그리운디 ㅋㅋㅋ",
          "original_form": "싸이 시절 그리운디 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000645.1.24",
          "form": "크흡 나의 흑역사ㅋㅋㅋ",
          "original_form": "크흡 나의 흑역사ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000645.1.25",
          "form": "그 시절 그립다 핸드폰으로 컴터 연결해서 사진올리고 ㅋㅋㅋㅋ",
          "original_form": "그 시절 그립다 핸드폰으로 컴터 연결해서 사진올리고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000645.1.26",
          "form": "아 너무 웃기다ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 너무 웃기다ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100000645.1.27",
          "form": "온갖 뽀샤시 효과 다 집어넣고ㅋㅋㅋ",
          "original_form": "온갖 뽀샤시 효과 다 집어넣고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100000645.1.28",
          "form": "그 귀찮은걸 어째했나몰라",
          "original_form": "그 귀찮은걸 어째했나몰라",
          "speaker_id": "1",
          "time": "20210819 19:20"
        },
        {
          "id": "MDRW2100000645.1.29",
          "form": "어려서 쓸떼없이 부지런했지 뭐ㅋㅋ",
          "original_form": "어려서 쓸떼없이 부지런했지 뭐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:27"
        },
        {
          "id": "MDRW2100000645.1.30",
