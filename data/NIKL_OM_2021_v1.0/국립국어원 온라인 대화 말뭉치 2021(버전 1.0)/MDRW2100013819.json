      "utterance": [
        {
          "id": "MDRW2100013819.1.1",
          "form": "요즘 날씨 더무 더웠죠ㅠ",
          "original_form": "요즘 날씨 더무 더웠죠ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013819.1.2",
          "form": "ㅜㅜ완죤요 그래도 비오고하니 한결나은듯해요",
          "original_form": "ㅜㅜ완죤요 그래도 비오고하니 한결나은듯해요",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013819.1.3",
          "form": "맞아요 좀 시원해졌는데 그래도 비오니 습하네요ㅠ",
          "original_form": "맞아요 좀 시원해졌는데 그래도 비오니 습하네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013819.1.4",
          "form": "이번 여름 비가 왔다안왔다하고 덥고 넘힘들었네요",
          "original_form": "이번 여름 비가 왔다안왔다하고 덥고 넘힘들었네요",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013819.1.5",
          "form": "그니까요 진짜 더웠죠? 밤에 열대야도 넘 심하고",
          "original_form": "그니까요 진짜 더웠죠? 밤에 열대야도 넘 심하고",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013819.1.6",
          "form": "네에 에어컨전기비 걱정될정도였어요ㅋㅋ",
          "original_form": "네에 에어컨전기비 걱정될정도였어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013819.1.7",
          "form": "아 근데 너무 더워서 안 켜긴 힘들더라구요",
          "original_form": "아 근데 너무 더워서 안 켜긴 힘들더라구요",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013819.1.8",
          "form": "2주전엔 정정도 됐었어요",
          "original_form": "2주전엔 정정도 됐었어요",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013819.1.9",
          "form": "정전",
          "original_form": "정전",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013819.1.10",
          "form": "아 진짜요? 저흰 그래도 가구수가 적어서 안되긴했는데 엄청 불편하셨겠다ㅜ",
          "original_form": "아 진짜요? 저흰 그래도 가구수가 적어서 안되긴했는데 엄청 불편하셨겠다ㅜ",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013819.1.11",
          "form": "갑자기 1시간 정도 정전됐는데 선풍기도 안 켜져서",
          "original_form": "갑자기 1시간 정도 정전됐는데 선풍기도 안 켜져서",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013819.1.12",
          "form": "더워죽는 줄 알았어요 ㅋㅋ",
          "original_form": "더워죽는 줄 알았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013819.1.13",
          "form": "헉 완죤 더웠겠다..아 진짜 앞으로 더더워질텐데 무선선풍기 구매를 해놔야겠어요ㅠㅠ",
          "original_form": "헉 완죤 더웠겠다..아 진짜 앞으로 더더워질텐데 무선선풍기 구매를 해놔야겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013819.1.14",
          "form": "그래서 그 미니 선풍기 들고있었어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래서 그 미니 선풍기 들고있었어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013819.1.15",
          "form": "그래도 땀이 비오듯 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래도 땀이 비오듯 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013819.1.16",
          "form": "미니선풍기로는 안되는 날씨죠ㅜ",
          "original_form": "미니선풍기로는 안되는 날씨죠ㅜ",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013819.1.17",
          "form": "이번주 내내 비온다는데 비오니까 또 좀 습하네요",
          "original_form": "이번주 내내 비온다는데 비오니까 또 좀 습하네요",
          "speaker_id": "1",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013819.1.18",
          "form": "ㅜㅜ 지금 휴가왔는데 비가 왛다안왔다하네요",
          "original_form": "ㅜㅜ 지금 휴가왔는데 비가 왛다안왔다하네요",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013819.1.19",
          "form": "아니 휴가인데 왜 하필 피가ㅠㅠ",
          "original_form": "아니 휴가인데 왜 하필 피가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013819.1.20",
          "form": "비가ㅠㅠ",
          "original_form": "비가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013819.1.21",
          "form": "저는 여행올때마다 비오는거같아요ㅋㅋ",
          "original_form": "저는 여행올때마다 비오는거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013819.1.22",
          "form": "엇 제가 그 유명한 아메온나라고...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "엇 제가 그 유명한 아메온나라고...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013819.1.23",
          "form": "혹시 아메온나 아세요? 가는데마다 비몰고다니는 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "혹시 아메온나 아세요? 가는데마다 비몰고다니는 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013819.1.24",
          "form": "아메온나요? 첨들어요ㅋㅋㅋ근데 저랑 같으시네요ㅋㅋ",
          "original_form": "아메온나요? 첨들어요ㅋㅋㅋ근데 저랑 같으시네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013819.1.25",
          "form": "일본어인데 가는데마다 비 몰고다니는 사람을 저렇게 부른대요 ㅋㅋㅋㅋㅋ",
          "original_form": "일본어인데 가는데마다 비 몰고다니는 사람을 저렇게 부른대요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013819.1.26",
          "form": "전 겨울에 여행가면 눈이 와요 ㅋㅋㅋ",
          "original_form": "전 겨울에 여행가면 눈이 와요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013819.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ앗 겨울이면 뭔가 이쁠거같은데 불편할거같은",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ앗 겨울이면 뭔가 이쁠거같은데 불편할거같은",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100013819.1.28",
          "form": "그쵸 눈때문에 다니기 힘들고 춥고 ㅋㅋㅋ 날씨운이 좋은 적이 없더라구요 ㅋㅋ",
          "original_form": "그쵸 눈때문에 다니기 힘들고 춥고 ㅋㅋㅋ 날씨운이 좋은 적이 없더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013819.1.29",
          "form": "그럴땐 맑음이들하고 다니면 되요ㅋㅋㅋㅋ",
          "original_form": "그럴땐 맑음이들하고 다니면 되요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100013819.1.30",
          "form": "맞아요 맑음이가 제 날씨운을 중화시켜줘야해요 ㅋㅋ",
          "original_form": "맞아요 맑음이가 제 날씨운을 중화시켜줘야해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100013819.1.31",
          "form": "그래도 비가 많이 오진 않죠?",
          "original_form": "그래도 비가 많이 오진 않죠?",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100013819.1.32",
          "form": "가족끼리 왔는데 어머니가 맑음이셔서",
          "original_form": "가족끼리 왔는데 어머니가 맑음이셔서",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100013819.1.33",
          "form": "다행히 차타면 비오고 내리면 그치고 그러네요",
          "original_form": "다행히 차타면 비오고 내리면 그치고 그러네요",
          "speaker_id": "2",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100013819.1.34",
          "form": "아 다행이네요 ㅋㅋㅋ 비가 금요일까지 올 예정이라던데 조금만 오면 좋겠어요",
          "original_form": "아 다행이네요 ㅋㅋㅋ 비가 금요일까지 올 예정이라던데 조금만 오면 좋겠어요",
          "speaker_id": "1",
          "time": "20210825 17:08"
        }
