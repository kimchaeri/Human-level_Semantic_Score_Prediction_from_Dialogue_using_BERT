        },
        {
          "id": "MDRW2100024174.1.6",
          "form": "그런가봐",
          "original_form": "그런가봐",
          "speaker_id": "1",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100024174.1.7",
          "form": "어우 어디 못 나가겠네",
