      "utterance": [
        {
          "id": "MDRW2100001747.1.1",
          "form": "요즘은 공연을 못봐서 너무 아쉬워요",
          "original_form": "요즘은 공연을 못봐서 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001747.1.2",
          "form": "공연이요?",
          "original_form": "공연이요?",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001747.1.3",
          "form": "어떤 공연 보고 싶으세요?",
          "original_form": "어떤 공연 보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100001747.1.4",
          "form": "전 가수 공연 보러다녔었어요",
          "original_form": "전 가수 공연 보러다녔었어요",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100001747.1.5",
          "form": "혹시 공연 좋아하세여?",
          "original_form": "혹시 공연 좋아하세여?",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100001747.1.6",
          "form": "전 영화 좋아해요",
          "original_form": "전 영화 좋아해요",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001747.1.7",
          "form": "최근에도 봤구",
          "original_form": "최근에도 봤구",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001747.1.8",
          "form": "방법: 재화의랑 이광수가 나오는 싱크홀",
          "original_form": "방법: 재화의랑 이광수가 나오는 싱크홀",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001747.1.9",
          "form": "오 영화 재밌죠",
          "original_form": "오 영화 재밌죠",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001747.1.10",
          "form": "싱크홀 재밌나요?",
          "original_form": "싱크홀 재밌나요?",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001747.1.11",
          "form": "와우 진짜 겁나 재밌어요",
          "original_form": "와우 진짜 겁나 재밌어요",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001747.1.12",
          "form": "이광수 런닝맨 하차하고",
          "original_form": "이광수 런닝맨 하차하고",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001747.1.13",
          "form": "개그를 여기서.. 하핳",
          "original_form": "개그를 여기서.. 하핳",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001747.1.14",
          "form": "한번 시간나면 꼭 봐보세요!!",
          "original_form": "한번 시간나면 꼭 봐보세요!!",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001747.1.15",
          "form": "꼭 한번 봐야겠어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "꼭 한번 봐야겠어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100001747.1.16",
          "form": "좋아하는 영화 뭐 있으세요?",
          "original_form": "좋아하는 영화 뭐 있으세요?",
          "speaker_id": "1",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100001747.1.17",
          "form": "저는 다 좋아해요",
          "original_form": "저는 다 좋아해요",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100001747.1.18",
          "form": "판타지",
          "original_form": "판타지",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100001747.1.19",
          "form": "로맨스",
          "original_form": "로맨스",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100001747.1.20",
          "form": "드라마",
          "original_form": "드라마",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100001747.1.21",
          "form": "공포",
          "original_form": "공포",
          "speaker_id": "2",
          "time": "20210819 18:55"
        },
        {
          "id": "MDRW2100001747.1.22",
          "form": "전부 ㅎㅎ",
          "original_form": "전부 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100001747.1.23",
          "form": "싫어하는게 없죠!!",
          "original_form": "싫어하는게 없죠!!",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100001747.1.24",
          "form": "다음주에는 인질 봐볼려고요",
          "original_form": "다음주에는 인질 봐볼려고요",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100001747.1.25",
          "form": "황정민이 나오시는건데",
          "original_form": "황정민이 나오시는건데",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100001747.1.26",
          "form": "재밌어보이더라궁ㅅ",
          "original_form": "재밌어보이더라궁ㅅ",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100001747.1.27",
          "form": "재밌어보이더라구요",
          "original_form": "재밌어보이더라구요",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100001747.1.28",
          "form": "영화 나오면 전부 보는 스타일이신가요?",
          "original_form": "영화 나오면 전부 보는 스타일이신가요?",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100001747.1.29",
          "form": "되게 보신 영화가 많을 거 같아요",
          "original_form": "되게 보신 영화가 많을 거 같아요",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100001747.1.30",
          "form": "아뇨아뇨",
          "original_form": "아뇨아뇨",
          "speaker_id": "2",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100001747.1.31",
          "form": "이번달에",
          "original_form": "이번달에",
          "speaker_id": "2",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100001747.1.32",
          "form": "방학이라",
          "original_form": "방학이라",
          "speaker_id": "2",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100001747.1.33",
          "form": "내려와서 친구들이 영화보자해서",
          "original_form": "내려와서 친구들이 영화보자해서",
          "speaker_id": "2",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100001747.1.34",
          "form": "1주일에 하나씩 보고 있네요 히히",
          "original_form": "1주일에 하나씩 보고 있네요 히히",
          "speaker_id": "2",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100001747.1.35",
          "form": "우와 일주일에 하나요?",
          "original_form": "우와 일주일에 하나요?",
          "speaker_id": "1",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100001747.1.36",
          "form": "되게 많이 보시네요",
          "original_form": "되게 많이 보시네요",
          "speaker_id": "1",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100001747.1.37",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100001747.1.38",
          "form": "짜피 이번달에 또",
          "original_form": "짜피 이번달에 또",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100001747.1.39",
          "form": "기숙가 가야해서.. ㅠㅠ",
          "original_form": "기숙가 가야해서.. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100001747.1.40",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100001747.1.41",
          "form": "아까 공연보신다 하셨잔아요",
          "original_form": "아까 공연보신다 하셨잔아요",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100001747.1.42",
          "form": "아하 ㅠ 그럼 왕창 봐놔야겠네여",
          "original_form": "아하 ㅠ 그럼 왕창 봐놔야겠네여",
