        },
        {
          "id": "MDRW2100026837.1.3",
          "form": "재택입니다",
          "original_form": "재택입니다",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026837.1.4",
          "form": "출근하시나요",
          "original_form": "출근하시나요",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026837.1.5",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026837.1.6",
          "form": "전출근입니다",
          "original_form": "전출근입니다",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026837.1.7",
          "form": "집에서 모든일을 합니다",
          "original_form": "집에서 모든일을 합니다",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026837.1.8",
          "form": "회의는 줌으로 해요",
          "original_form": "회의는 줌으로 해요",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026837.1.9",
          "form": "출근하는게 능률이 오르지요",
          "original_form": "출근하는게 능률이 오르지요",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026837.1.10",
          "form": "4단계 끝나고 3단계되면 대면으로 바꾼대요",
          "original_form": "4단계 끝나고 3단계되면 대면으로 바꾼대요",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026837.1.11",
          "form": "저는 디자인쪽이라 재택이 가능합니다",
          "original_form": "저는 디자인쪽이라 재택이 가능합니다",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026837.1.12",
          "form": "코로나라서 걱정돼요",
          "original_form": "코로나라서 걱정돼요",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026837.1.13",
          "form": "부럽네요 재택근무가 효율이 높을것같아요",
          "original_form": "부럽네요 재택근무가 효율이 높을것같아요",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026837.1.14",
          "form": "네 재택이 생활화되면 좋겠어요",
          "original_form": "네 재택이 생활화되면 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026837.1.15",
          "form": "네 누워서도 일할수있어요",
          "original_form": "네 누워서도 일할수있어요",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026837.1.16",
          "form": "제 친구는 일주일 재택 일주일 출근이라더군요",
          "original_form": "제 친구는 일주일 재택 일주일 출근이라더군요",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026837.1.17",
          "form": "그것도 좋네요",
          "original_form": "그것도 좋네요",
