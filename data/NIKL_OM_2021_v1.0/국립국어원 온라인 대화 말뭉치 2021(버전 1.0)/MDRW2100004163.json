      "utterance": [
        {
          "id": "MDRW2100004163.1.1",
          "form": "혹시 반려동물 키우시나요!",
          "original_form": "혹시 반려동물 키우시나요!",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004163.1.2",
          "form": "저는 반려동물을 안키워요!",
          "original_form": "저는 반려동물을 안키워요!",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004163.1.3",
          "form": "털 알러지가 있어서ㅠㅠ",
          "original_form": "털 알러지가 있어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004163.1.4",
          "form": "아아ㅠㅠㅠㅠ 알러지 있으면 키우기 힘들죠",
          "original_form": "아아ㅠㅠㅠㅠ 알러지 있으면 키우기 힘들죠",
          "speaker_id": "1",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004163.1.5",
          "form": "저도 지금은 안 키우는데 어릴 때 키운 적이 있어요",
          "original_form": "저도 지금은 안 키우는데 어릴 때 키운 적이 있어요",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004163.1.6",
          "form": "요즘따라 너무 귀여워보이더라구요",
          "original_form": "요즘따라 너무 귀여워보이더라구요",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004163.1.7",
          "form": "뭘 키우셨나요? 궁금해요!",
          "original_form": "뭘 키우셨나요? 궁금해요!",
          "speaker_id": "2",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004163.1.8",
          "form": "저는 강아지키웠었어요!",
          "original_form": "저는 강아지키웠었어요!",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004163.1.9",
          "form": "아직도 고양이는 무서운데 강아지는 너무 귀여워요",
          "original_form": "아직도 고양이는 무서운데 강아지는 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004163.1.10",
          "form": "막 인스타에서 반려동물 보면 귀여워서 쓰다듬고 싶어져요",
          "original_form": "막 인스타에서 반려동물 보면 귀여워서 쓰다듬고 싶어져요",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004163.1.11",
          "form": "강아지 정말 귀여웠을것 같아요!",
          "original_form": "강아지 정말 귀여웠을것 같아요!",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004163.1.12",
          "form": "너무 귀여워요",
          "original_form": "너무 귀여워요",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004163.1.13",
          "form": "저는 오히려 강아지를 무서워했어요! 어릴때 강아지가 무섭게 쫓아온적 있는데",
          "original_form": "저는 오히려 강아지를 무서워했어요! 어릴때 강아지가 무섭게 쫓아온적 있는데",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004163.1.14",
          "form": "그게 충격이어서 한동안 무서워했네요",
          "original_form": "그게 충격이어서 한동안 무서워했네요",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004163.1.15",
          "form": "헉 그런거면 너무 무서울 거 같아요",
          "original_form": "헉 그런거면 너무 무서울 거 같아요",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004163.1.16",
          "form": "하지만 지금은 고양이 강아지 둘다 좋아해요!",
          "original_form": "하지만 지금은 고양이 강아지 둘다 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004163.1.17",
          "form": "뉴스에서도 강아지 물림 사고도 많은 거 보면",
          "original_form": "뉴스에서도 강아지 물림 사고도 많은 거 보면",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004163.1.18",
          "form": "주인이 더 주의해서 관리해야 되겠더라구요",
          "original_form": "주인이 더 주의해서 관리해야 되겠더라구요",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004163.1.19",
          "form": "맞아요 절 쫓아왔던 강아지도 목줄이 없었거든요",
          "original_form": "맞아요 절 쫓아왔던 강아지도 목줄이 없었거든요",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004163.1.20",
          "form": "진짜 무서우셨을 거 같아여ㅠㅠㅠㅠ",
          "original_form": "진짜 무서우셨을 거 같아여ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004163.1.21",
          "form": "큰강아지들은 지금도 뮤섭더라구요",
          "original_form": "큰강아지들은 지금도 뮤섭더라구요",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004163.1.22",
          "form": "맞아요 큰 애들은 위압감이 엄청나죠.. 그래도 정말 어릴때라 지금은 다 마냥 귀여워요!ㅎㅎ",
          "original_form": "맞아요 큰 애들은 위압감이 엄청나죠.. 그래도 정말 어릴때라 지금은 다 마냥 귀여워요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:21"
        }
      ]
    }
