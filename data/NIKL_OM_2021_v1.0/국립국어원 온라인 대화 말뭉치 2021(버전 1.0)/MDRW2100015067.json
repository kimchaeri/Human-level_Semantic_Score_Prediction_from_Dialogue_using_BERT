      "utterance": [
        {
          "id": "MDRW2100015067.1.1",
          "form": "볶음밥에치즈계란말이!!!",
          "original_form": "볶음밥에치즈계란말이!!!",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015067.1.2",
          "form": "내일아점입니당",
          "original_form": "내일아점입니당",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015067.1.3",
          "form": "치즈계란말이 이 또한 배우신분! ㅋㅋㅋㅋ",
          "original_form": "치즈계란말이 이 또한 배우신분! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015067.1.4",
          "form": "저는 내일이야말로 기필로 쌀국수를 먹으려구요 ㅋㅋㅋ",
          "original_form": "저는 내일이야말로 기필로 쌀국수를 먹으려구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015067.1.5",
          "form": "저 에머이랑 포몬스좋아해용",
          "original_form": "저 에머이랑 포몬스좋아해용",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015067.1.6",
          "form": "쌀국수진짜 주기적으로먹어야해요",
          "original_form": "쌀국수진짜 주기적으로먹어야해요",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015067.1.7",
          "form": "진짜 이거레알입니다 ㅋㅋㅋ",
          "original_form": "진짜 이거레알입니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015067.1.8",
          "form": "쌀국수 수혈주기가 있어요",
          "original_form": "쌀국수 수혈주기가 있어요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015067.1.9",
          "form": "은은한 고기국물 이건 주기적으로 먹어줘야해요",
          "original_form": "은은한 고기국물 이건 주기적으로 먹어줘야해요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015067.1.10",
          "form": "내일 먹을거예요 ㅋㅋㅋㅋ",
          "original_form": "내일 먹을거예요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015067.1.11",
          "form": "네ㅋㅋ 해장용으로도최고예요",
          "original_form": "네ㅋㅋ 해장용으로도최고예요",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015067.1.12",
          "form": "빨간국물보단하얀국물이좋더라구요",
          "original_form": "빨간국물보단하얀국물이좋더라구요",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015067.1.13",
          "form": "저도요..... 막 먹은것도 없고 속에 얹힌것도 없는데 속이 풀리는 느낌 ㅋㅋㅋ",
          "original_form": "저도요.....{emoji:♡} 막 먹은것도 없고 속에 얹힌것도 없는데 속이 풀리는 느낌 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015067.1.14",
          "form": "네ㅋㅋ해장이아닌데해장느낌",
          "original_form": "네ㅋㅋ해장이아닌데해장느낌",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015067.1.15",
          "form": "비오면그렇게땡기더라구요",
          "original_form": "비오면그렇게땡기더라구요",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015067.1.16",
          "form": "느낌적인느낌!! 그쵸!!!",
          "original_form": "느낌적인느낌!! 그쵸!!!",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015067.1.17",
          "form": "제가 진짜 지난주말부터 계속 쌀국수 칼국수 이런게 너무 땡겨요 ㅋㅋㅋㅋ",
          "original_form": "제가 진짜 지난주말부터 계속 쌀국수 칼국수 이런게 너무 땡겨요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015067.1.18",
          "form": "계~ 속 비가오니까ㅜㅜ..",
          "original_form": "계~ 속 비가오니까ㅜㅜ..",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015067.1.19",
          "form": "맞아요 근데볶음쌀국수는 그냥그렇더라구요",
          "original_form": "맞아요 근데볶음쌀국수는 그냥그렇더라구요",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015067.1.20",
          "form": "제기국물파라그런듯..",
          "original_form": "제기국물파라그런듯..",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015067.1.21",
