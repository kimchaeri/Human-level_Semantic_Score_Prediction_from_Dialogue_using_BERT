      "utterance": [
        {
          "id": "MDRW2100045007.1.1",
          "form": "덤앤더머 아닌가요 저희",
          "original_form": "덤앤더머 아닌가요 저희",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045007.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045007.1.3",
          "form": "ㅋㅋㅋㅋㅋ 개그하는거같아요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 개그하는거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045007.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그니까요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그니까요",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045007.1.5",
          "form": "그래서 단품으로 먹나요?",
          "original_form": "그래서 단품으로 먹나요?",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045007.1.6",
          "form": "넵 ㅋㅋㅋㅋ 이것만 먹어요 보통",
          "original_form": "넵 ㅋㅋㅋㅋ 이것만 먹어요 보통",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045007.1.7",
          "form": "밀가루 안 들어간댔죠?!",
          "original_form": "밀가루 안 들어간댔죠?!",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045007.1.8",
          "form": "아뇨 밀가루 들어가요!",
          "original_form": "아뇨 밀가루 들어가요!",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045007.1.9",
          "form": "튀긴거 아니랬죠?!",
          "original_form": "튀긴거 아니랬죠?!",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045007.1.10",
          "form": "네 튀기진 않아요!",
          "original_form": "네 튀기진 않아요!",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045007.1.11",
          "form": "국물이 있나요?",
          "original_form": "국물이 있나요?",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045007.1.12",
          "form": "아뇨 없어요!",
          "original_form": "아뇨 없어요!",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045007.1.13",
          "form": "앗 뭐지 뭐지 뭐지",
          "original_form": "앗 뭐지 뭐지 뭐지",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045007.1.14",
          "form": "신라면 건면..?",
          "original_form": "신라면 건면..?",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045007.1.15",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045007.1.16",
          "form": "힌트를 드리자면",
          "original_form": "힌트를 드리자면",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045007.1.17",
          "form": "분류상 양식입니다...!",
          "original_form": "분류상 양식입니다...!",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045007.1.18",
          "form": "이이잇",
          "original_form": "이이잇",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045007.1.19",
          "form": "면 요리예요?",
          "original_form": "면 요리예요?",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045007.1.20",
          "form": "크아아아악 뭘까요",
          "original_form": "크아아아악 뭘까요",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045007.1.21",
          "form": "샌드위치?",
          "original_form": "샌드위치?",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045007.1.22",
          "form": "아뇨 야채 안들어가요!",
          "original_form": "아뇨 야채 안들어가요!",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045007.1.23",
          "form": "ㅠㅠㅠㅠㅠㅠㅜㅜㅜㅠㅠㅠㅠㅜㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅜㅜㅜㅠㅠㅠㅠㅜㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045007.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045007.1.25",
          "form": "아 혹시 함박스테이크?",
          "original_form": "아 혹시 함박스테이크?",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045007.1.26",
          "form": "고기 안들어가요 ㅋㅋㅋㅋㅋ",
          "original_form": "고기 안들어가요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045007.1.27",
          "form": "힌트 드릴까여??",
          "original_form": "힌트 드릴까여??",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045007.1.28",
          "form": "ㅋㅋㅋㅋㅋ네 저 정말 패트와매트 된 기분이예요",
          "original_form": "ㅋㅋㅋㅋㅋ네 저 정말 패트와매트 된 기분이예요",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045007.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그쳐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그쳐",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045007.1.30",
          "form": "전 그냥 먹었지맘",
          "original_form": "전 그냥 먹었지맘",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045007.1.31",
          "form": "보통 잼이나 시럽이랑 먹어요!",
          "original_form": "보통 잼이나 시럽이랑 먹어요!",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045007.1.32",
          "form": "! 혹시 와플?!",
          "original_form": "! 혹시 와플?!",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045007.1.33",
          "form": "와플 드셨나요!",
          "original_form": "와플 드셨나요!",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045007.1.34",
          "form": "아뇨 저희 집에 와플기가 없어서....",
          "original_form": "아뇨 저희 집에 와플기가 없어서....",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045007.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045007.1.36",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅌㅋㅋㅋ아아아아악",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅌㅋㅋㅋ아아아아악",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045007.1.37",
          "form": "그럼 토스트!",
          "original_form": "그럼 토스트!",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045007.1.38",
          "form": "아뇨 집에서 만들어 먹을 수 잇는 종류예요!",
          "original_form": "아뇨 집에서 만들어 먹을 수 잇는 종류예요!",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045007.1.39",
          "form": "아 설마설마설마",
          "original_form": "아 설마설마설마",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045007.1.40",
          "form": "팬 케이크?",
          "original_form": "팬 케이크?",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045007.1.41",
          "form": "넵 ㅋㅋㅋ 맞아요!",
          "original_form": "넵 ㅋㅋㅋ 맞아요!",
          "speaker_id": "1",
          "time": "20210908 00:43"
        }
      ]
