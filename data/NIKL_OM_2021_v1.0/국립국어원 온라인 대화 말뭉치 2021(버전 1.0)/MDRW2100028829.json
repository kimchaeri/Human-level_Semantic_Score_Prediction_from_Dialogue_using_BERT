      "utterance": [
        {
          "id": "MDRW2100028829.1.1",
          "form": "공공기관입니당",
          "original_form": "공공기관입니당",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028829.1.2",
          "form": "오오  신의 직장이시군요!",
          "original_form": "오오  신의 직장이시군요!",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028829.1.3",
          "form": "그래서 그렇게  부르주아처럼 잘 먹으시는ㅈ거였군요ㅜ",
          "original_form": "그래서 그렇게  부르주아처럼 잘 먹으시는ㅈ거였군요ㅜ",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028829.1.4",
          "form": "사노비는 웁니다",
          "original_form": "사노비는 웁니다",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028829.1.5",
          "form": "나쁘진 않습니다",
          "original_form": "나쁘진 않습니다",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028829.1.6",
          "form": "서울사시면",
          "original_form": "서울사시면",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028829.1.7",
          "form": "지방이랑 하늘과 땅차이 아니겟습니까",
          "original_form": "지방이랑 하늘과 땅차이 아니겟습니까",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028829.1.8",
          "form": "서울 집값 오우야",
          "original_form": "서울 집값 오우야",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028829.1.9",
          "form": "잘 못먹구요 오늘 라면 먹었어유",
          "original_form": "잘 못먹구요 오늘 라면 먹었어유",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100028829.1.10",
          "form": "천사가 직접 사골을 우려낸 수제라면 말고요?",
          "original_form": "천사가 직접 사골을 우려낸 수제라면 말고요?",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028829.1.11",
          "form": "ㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028829.1.12",
          "form": "자기 닮은 너구리",
          "original_form": "자기 닮은 너구리",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028829.1.13",
          "form": "끓여주던데요?",
          "original_form": "끓여주던데요?",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028829.1.14",
          "form": "설거지좀 하고 오겠습니당 ㅠ",
          "original_form": "설거지좀 하고 오겠습니당 ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028829.1.15",
          "form": "ㅎㅎㅎ 성실하시군요ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ 성실하시군요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028829.1.16",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028829.1.17",
          "form": "설거지하고 계란먹을려고 홈트했습니다 ㅎㅎ",
          "original_form": "설거지하고 계란먹을려고 홈트했습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100028829.1.18",
          "form": "독하신분ㄷㄷ",
          "original_form": "독하신분ㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100028829.1.19",
          "form": "몸괜찮죠?",
          "original_form": "몸괜찮죠?",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100028829.1.20",
          "form": "야식 드실시간 아닌가요?",
          "original_form": "야식 드실시간 아닌가요?",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100028829.1.21",
          "form": "강아지 산책시키고 먹으려고요ㅎ",
          "original_form": "강아지 산책시키고 먹으려고요ㅎ",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100028829.1.22",
          "form": "지금 강아지와 밖입니닿",
          "original_form": "지금 강아지와 밖입니닿",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100028829.1.23",
          "form": "오 좋네요 어디서 봤는데 강아지가 산책을 엄청 좋아한다더라구요",
          "original_form": "오 좋네요 어디서 봤는데 강아지가 산책을 엄청 좋아한다더라구요",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100028829.1.24",
          "form": "반려사람하고 같이 하면",
          "original_form": "반려사람하고 같이 하면",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100028829.1.25",
          "form": "행복감을 느낀다고..",
          "original_form": "행복감을 느낀다고..",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100028829.1.26",
          "form": "ㅎㅎ 먹는것보다 더 좋아해요",
          "original_form": "ㅎㅎ 먹는것보다 더 좋아해요",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100028829.1.27",
          "form": "저도 행복ㅎ",
          "original_form": "저도 행복ㅎ",
          "speaker_id": "1",
          "time": "20210903 00:44"
        },
        {
          "id": "MDRW2100028829.1.28",
          "form": "문제는 출근날에도 아침저녁 산책을 졸라서ㅜ",
          "original_form": "문제는 출근날에도 아침저녁 산책을 졸라서ㅜ",
          "speaker_id": "1",
          "time": "20210903 00:45"
        },
        {
          "id": "MDRW2100028829.1.29",
          "form": "해줘야겠네요 더 일찍 일어나셔야 될듯",
          "original_form": "해줘야겠네요 더 일찍 일어나셔야 될듯",
          "speaker_id": "2",
          "time": "20210903 00:55"
        },
        {
          "id": "MDRW2100028829.1.30",
          "form": "몇시 출근하나요?",
          "original_form": "몇시 출근하나요?",
          "speaker_id": "2",
          "time": "20210903 00:55"
        }
      ]
    }
