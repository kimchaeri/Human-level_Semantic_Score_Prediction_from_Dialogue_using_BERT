      "utterance": [
        {
          "id": "MDRW2100019519.1.1",
          "form": "전 고양이 2마리 키우고있어요",
          "original_form": "전 고양이 2마리 키우고있어요",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019519.1.2",
          "form": "name3이랑 name4이라고 해요",
          "original_form": "&name3&이랑 &name4&이라고 해요",
          "speaker_id": "2",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019519.1.3",
          "form": "오 저는 제 자신을 돌보고 있어요",
          "original_form": "오 저는 제 자신을 돌보고 있어요",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100019519.1.4",
          "form": "name3이랑 name4이는 몇 살이예요?",
          "original_form": "&name3&이랑 &name4&이는 몇 살이예요?",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100019519.1.5",
          "form": "name3이 name4이 둘다 4살...?",
          "original_form": "&name3&이 &name4&이 둘다 4살...?",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100019519.1.6",
          "form": "입니다",
          "original_form": "입니다",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100019519.1.7",
          "form": "사실 기억을 안하고 살아서",
          "original_form": "사실 기억을 안하고 살아서",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100019519.1.8",
          "form": "고양이가 4살이면 사람 나이로",
          "original_form": "고양이가 4살이면 사람 나이로",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100019519.1.9",
          "form": "틀릴수도 있어요",
          "original_form": "틀릴수도 있어요",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100019519.1.10",
          "form": "허허",
          "original_form": "허허",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100019519.1.11",
          "form": "어.. 계산이 안 된다",
          "original_form": "어.. 계산이 안 된다",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100019519.1.12",
          "form": "대략 30대?",
          "original_form": "대략 30대?",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100019519.1.13",
          "form": "20대후반으로",
          "original_form": "20대후반으로",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100019519.1.14",
          "form": "보시면돼요",
          "original_form": "보시면돼요",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100019519.1.15",
          "form": "입이 얼마나 고급인지 음식을 가려먹어요...",
          "original_form": "입이 얼마나 고급인지 음식을 가려먹어요...",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100019519.1.16",
          "form": "오 한창때네요 짜식들",
          "original_form": "오 한창때네요 짜식들",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100019519.1.17",
          "form": "ㅋㅋㅋㅋ무슨 간식을 제일 잘 먹는대요?",
          "original_form": "ㅋㅋㅋㅋ무슨 간식을 제일 잘 먹는대요?",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100019519.1.18",
          "form": "캣닢이라고",
          "original_form": "캣닢이라고",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100019519.1.19",
          "form": "고양이 마약있는데",
          "original_form": "고양이 마약있는데",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100019519.1.20",
          "form": "그것만 주면 좋아서",
          "original_form": "그것만 주면 좋아서",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100019519.1.21",
          "form": "콧바람에 캣닢 날아가요",
          "original_form": "콧바람에 캣닢 날아가요",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100019519.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 웃겨요 근데 캣닢을 먹으면 배가 차나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 웃겨요 근데 캣닢을 먹으면 배가 차나요",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100019519.1.23",
          "form": "배 채우는 야채가 아니라",
          "original_form": "배 채우는 야채가 아니라",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100019519.1.24",
          "form": "향을 즐기는 건가요",
          "original_form": "향을 즐기는 건가요",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100019519.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㄱ네 취해서 온방을 돌아다니는 용도로",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㄱ네 취해서 온방을 돌아다니는 용도로",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100019519.1.26",
          "form": "즐기더라구요",
          "original_form": "즐기더라구요",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100019519.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ상상돼서 진짜 웃겨요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ상상돼서 진짜 웃겨요",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100019519.1.28",
          "form": "저는 동물을 예전에는 키우고 싶었는데",
          "original_form": "저는 동물을 예전에는 키우고 싶었는데",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100019519.1.29",
          "form": "머리 크고 보니까 저 자신도 제대로 못 챙기길래",
          "original_form": "머리 크고 보니까 저 자신도 제대로 못 챙기길래",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100019519.1.30",
          "form": "동물 키우는 건 먼 훗날로 미뤄뒀어요",
          "original_form": "동물 키우는 건 먼 훗날로 미뤄뒀어요",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100019519.1.31",
          "form": "아마 키우면 식물이나 파충류 종류를 키울 것 같습니다",
          "original_form": "아마 키우면 식물이나 파충류 종류를 키울 것 같습니다",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100019519.1.32",
          "form": "냅두면 알아서 쭉쭉 자라는 녀석들로",
          "original_form": "냅두면 알아서 쭉쭉 자라는 녀석들로",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100019519.1.33",
          "form": "키우다가",
          "original_form": "키우다가",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100019519.1.34",
          "form": "전 파충류 유지비가 비싸다고 들어서",
          "original_form": "전 파충류 유지비가 비싸다고 들어서",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100019519.1.35",
          "form": "먹이나 온도조절 등",
          "original_form": "먹이나 온도조절 등",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100019519.1.36",
          "form": "신경써야할 요소가 꽤있더라구요",
          "original_form": "신경써야할 요소가 꽤있더라구요",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100019519.1.37",
          "form": "아 맞아요 초기비용이 상당히 드나보더라구요",
          "original_form": "아 맞아요 초기비용이 상당히 드나보더라구요",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100019519.1.38",
          "form": "이놈들이 그냥 냅두면 얼어죽어서",
          "original_form": "이놈들이 그냥 냅두면 얼어죽어서",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100019519.1.39",
          "form": "ㅋㅋㅋㅋㅋ온도관리가 좀 까다롭다고는 해요",
          "original_form": "ㅋㅋㅋㅋㅋ온도관리가 좀 까다롭다고는 해요",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100019519.1.40",
          "form": "겨울에 난방비도 그렇고",
          "original_form": "겨울에 난방비도 그렇고",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100019519.1.41",
          "form": "적고보니 저한테는 스투키가 제일 잘 어울리는 것 같네요",
          "original_form": "적고보니 저한테는 스투키가 제일 잘 어울리는 것 같네요",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100019519.1.42",
          "form": "동물 안 키울래요 감사합니다",
          "original_form": "동물 안 키울래요 감사합니다",
          "speaker_id": "1",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100019519.1.43",
          "form": "남의 집 동물 구경을 해야겠어요",
          "original_form": "남의 집 동물 구경을 해야겠어요",
          "speaker_id": "1",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100019519.1.44",
          "form": "ㅋㅋㅋㅋㄱㅋㄱㄱ맞아요 제일좋은건",
          "original_form": "ㅋㅋㅋㅋㄱㅋㄱㄱ맞아요 제일좋은건",
          "speaker_id": "2",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100019519.1.45",
          "form": "자기자신을 잘관리하면서 보내는거 아니겠습니까",
          "original_form": "자기자신을 잘관리하면서 보내는거 아니겠습니까",
          "speaker_id": "2",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100019519.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래야겠어요 간식도 내 간식을 사먹겠습니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래야겠어요 간식도 내 간식을 사먹겠습니다",
          "speaker_id": "1",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100019519.1.47",
          "form": "하지만 동물 이름은 한 번쯤 지어보고 싶었는데",
          "original_form": "하지만 동물 이름은 한 번쯤 지어보고 싶었는데",
          "speaker_id": "1",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100019519.1.48",
          "form": "뱀 키워서 스파게티라고 부르는 게 꿈이었어요",
          "original_form": "뱀 키워서 스파게티라고 부르는 게 꿈이었어요",
          "speaker_id": "1",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100019519.1.49",
          "form": "살다살다 뱀이름을",
          "original_form": "살다살다 뱀이름을",
          "speaker_id": "2",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100019519.1.50",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100019519.1.51",
          "form": "스파게티라고 짓는건",
          "original_form": "스파게티라고 짓는건",
          "speaker_id": "2",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100019519.1.52",
          "form": "처음이네요",
          "original_form": "처음이네요",
          "speaker_id": "2",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100019519.1.53",
          "form": "귀엽지않나요",
          "original_form": "귀엽지않나요",
          "speaker_id": "1",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100019519.1.54",
          "form": "뭐 어때요 개 이름도 초코라고 짓는데",
          "original_form": "뭐 어때요 개 이름도 초코라고 짓는데",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100019519.1.55",
          "form": "그것도 그렇네요",
          "original_form": "그것도 그렇네요",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100019519.1.56",
          "form": "같은 음식인 스파게티는 왜 안되나요",
          "original_form": "같은 음식인 스파게티는 왜 안되나요",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100019519.1.57",
          "form": "개가 초코먹으면 죽으니까",
          "original_form": "개가 초코먹으면 죽으니까",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100019519.1.58",
          "form": "인간이름을 독버섯으로 지은거랑",
          "original_form": "인간이름을 독버섯으로 지은거랑",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100019519.1.59",
          "form": "똑같네요",
          "original_form": "똑같네요",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100019519.1.60",
          "form": "어떤 도라히가 지은거지",
          "original_form": "어떤 도라히가 지은거지",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100019519.1.61",
          "form": "웃기네요",
          "original_form": "웃기네요",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
