      "utterance": [
        {
          "id": "MDRW2100044060.1.1",
          "form": "밥 먹었어요?",
          "original_form": "밥 먹었어요?",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044060.1.2",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044060.1.3",
          "form": "뭐 먹었어요?",
          "original_form": "뭐 먹었어요?",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044060.1.4",
          "form": "그냥 집밥요",
          "original_form": "그냥 집밥요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044060.1.5",
          "form": "집밥이 좋죠",
          "original_form": "집밥이 좋죠",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044060.1.6",
          "form": "건강을 위해 좋은 듯해요",
          "original_form": "건강을 위해 좋은 듯해요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044060.1.7",
          "form": "반찬은 어떤 거예요",
          "original_form": "반찬은 어떤 거예요",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044060.1.8",
          "form": "있는 거 먹어요 멸치 김치 코다리",
          "original_form": "있는 거 먹어요 멸치 김치 코다리",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044060.1.9",
          "form": "코다리 맛있는데",
          "original_form": "코다리 맛있는데",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044060.1.10",
          "form": "양념 구이로 해 먹었어요 맛있더라고요",
          "original_form": "양념 구이로 해 먹었어요 맛있더라고요",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044060.1.11",
          "form": "요리를 잘 하나 봐요",
          "original_form": "요리를 잘 하나 봐요",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044060.1.12",
          "form": "아니요 그냥 반조리 되어 있는거 익혀먹었어요",
          "original_form": "아니요 그냥 반조리 되어 있는거 익혀먹었어요",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044060.1.13",
          "form": "우리집은 반찬으로 젓갈 더덕 장아찌 좋아해요",
          "original_form": "우리집은 반찬으로 젓갈 더덕 장아찌 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044060.1.14",
          "form": "더덕 맛있죠",
          "original_form": "더덕 맛있죠",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044060.1.15",
          "form": "도라지 오징어 초무침 오늘은 먹었네요",
          "original_form": "도라지 오징어 초무침 오늘은 먹었네요",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044060.1.16",
          "form": "집에 초대해 주세요",
          "original_form": "집에 초대해 주세요",
          "speaker_id": "1",
          "time": "20210907 21:28"
        }
