      "utterance": [
        {
          "id": "MDRW2100031868.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 06:39"
        },
        {
          "id": "MDRW2100031868.1.2",
          "form": "맥도날드 자주 가시나요?",
          "original_form": "맥도날드 자주 가시나요?",
          "speaker_id": "1",
          "time": "20210905 06:39"
        },
        {
          "id": "MDRW2100031868.1.3",
          "form": "아 좋아하긴 하는데",
          "original_form": "아 좋아하긴 하는데",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031868.1.4",
          "form": "자주 가진 않네요",
          "original_form": "자주 가진 않네요",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031868.1.5",
          "form": "그러면 배달 음식은 어떻게 자주 드시나요?",
          "original_form": "그러면 배달 음식은 어떻게 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210905 08:10"
        },
        {
          "id": "MDRW2100031868.1.6",
          "form": "오 배달 음식도",
          "original_form": "오 배달 음식도",
          "speaker_id": "2",
          "time": "20210905 08:10"
        },
        {
          "id": "MDRW2100031868.1.7",
          "form": "싫어하진 않는데",
          "original_form": "싫어하진 않는데",
          "speaker_id": "2",
          "time": "20210905 08:10"
        },
        {
          "id": "MDRW2100031868.1.8",
          "form": "자주 먹진 않아요",
          "original_form": "자주 먹진 않아요",
          "speaker_id": "2",
          "time": "20210905 08:11"
        },
        {
          "id": "MDRW2100031868.1.9",
          "form": "네 먹으면 맛있긴 맛있는데",
          "original_form": "네 먹으면 맛있긴 맛있는데",
          "speaker_id": "1",
          "time": "20210905 08:11"
        },
        {
          "id": "MDRW2100031868.1.10",
          "form": "저도 자주는 안 먹어요",
          "original_form": "저도 자주는 안 먹어요",
          "speaker_id": "1",
          "time": "20210905 08:11"
        },
        {
          "id": "MDRW2100031868.1.11",
          "form": "일단 요즘",
          "original_form": "일단 요즘",
          "speaker_id": "2",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100031868.1.12",
          "form": "요리 유튜브도 잘 나오고",
          "original_form": "요리 유튜브도 잘 나오고",
          "speaker_id": "2",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100031868.1.13",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "2",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100031868.1.14",
          "form": "물론 튀긴 요리는 여전히 집에서 하기 머뭇거리게 되긴 하지만...",
          "original_form": "물론 튀긴 요리는 여전히 집에서 하기 머뭇거리게 되긴 하지만...",
          "speaker_id": "2",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100031868.1.15",
          "form": "일요일 아침이라 그런지 맥모닝 먹고 싶어요",
          "original_form": "일요일 아침이라 그런지 맥모닝 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100031868.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100031868.1.17",
          "form": "드셨나요",
          "original_form": "드셨나요",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031868.1.18",
          "form": "11시까지였나...?",
          "original_form": "11시까지였나...?",
          "speaker_id": "2",
          "time": "20210905 10:30"
        },
        {
          "id": "MDRW2100031868.1.19",
          "form": "10시 반까지요",
          "original_form": "10시 반까지요",
          "speaker_id": "1",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100031868.1.20",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "2",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100031868.1.21",
          "form": "저 질문 했을때는 이미...",
          "original_form": "저 질문 했을때는 이미...",
          "speaker_id": "2",
          "time": "20210905 14:52"
        },
        {
          "id": "MDRW2100031868.1.22",
          "form": "점심은 드셨어요?",
          "original_form": "점심은 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 15:37"
        },
        {
          "id": "MDRW2100031868.1.23",
          "form": "예 먹었어요",
          "original_form": "예 먹었어요",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100031868.1.24",
          "form": "초밥 먹었어요",
          "original_form": "초밥 먹었어요",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100031868.1.25",
          "form": "맛있는거 드셨네요",
          "original_form": "맛있는거 드셨네요",
          "speaker_id": "1",
          "time": "20210905 16:44"
        }
