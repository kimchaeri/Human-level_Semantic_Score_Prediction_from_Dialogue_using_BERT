      "utterance": [
        {
          "id": "MDRW2100020039.1.1",
          "form": "좋아하는 음식이 무엇인가요?",
          "original_form": "좋아하는 음식이 무엇인가요?",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020039.1.2",
          "form": "초밥 좋아해요",
          "original_form": "초밥 좋아해요",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020039.1.3",
          "form": "와 잊고 있던 음식이네요. 초밥 최고",
          "original_form": "와 잊고 있던 음식이네요. 초밥 최고",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020039.1.4",
          "form": "초밥 뭐를 제일 좋아하시나요? 저는 간장새우요",
          "original_form": "초밥 뭐를 제일 좋아하시나요? 저는 간장새우요",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020039.1.5",
          "form": "연어도 좋네요",
          "original_form": "연어도 좋네요",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020039.1.6",
          "form": "저도 연어초밥이요!",
          "original_form": "저도 연어초밥이요!",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020039.1.7",
          "form": "가끔 매운게 땡길때는 타코와사비도 먹어요 ㅋㅋㅋㅋ",
          "original_form": "가끔 매운게 땡길때는 타코와사비도 먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020039.1.8",
          "form": "와 연어초밥~ 아니 연어 은근 호불호 많이 갈리더라고요.",
          "original_form": "와 연어초밥~ 아니 연어 은근 호불호 많이 갈리더라고요.",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020039.1.9",
          "form": "타코와사비 좋죠",
          "original_form": "타코와사비 좋죠",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020039.1.10",
          "form": "저는 치킨 좋아해요 하하 누구나 좋아하겠지만요",
          "original_form": "저는 치킨 좋아해요 하하 누구나 좋아하겠지만요",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020039.1.11",
          "form": "치킨 맛있죠 ㅋㅋㅋㅋ",
          "original_form": "치킨 맛있죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020039.1.12",
          "form": "좋아하는 브랜드 있으신가요?",
          "original_form": "좋아하는 브랜드 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020039.1.13",
          "form": "저 다 좋긴한데 요즘 자담치킨 맛있더라고요",
          "original_form": "저 다 좋긴한데 요즘 자담치킨 맛있더라고요",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020039.1.14",
          "form": "푸라닭도 맛있는데 비싸서 잘 먹지 못하네요..",
          "original_form": "푸라닭도 맛있는데 비싸서 잘 먹지 못하네요..",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020039.1.15",
          "form": "네네치킨 순살도 좋고..하하 다 좋아요",
          "original_form": "네네치킨 순살도 좋고..하하 다 좋아요",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020039.1.16",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020039.1.17",
          "form": "저는 굽네 좋아해요",
          "original_form": "저는 굽네 좋아해요",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020039.1.18",
          "form": "한동안 맵슐랭 엄청 시켜먹었어요 ㅋㅋㅋ",
          "original_form": "한동안 맵슐랭 엄청 시켜먹었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020039.1.19",
          "form": "푸라닭도 좋더라구요",
          "original_form": "푸라닭도 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020039.1.20",
          "form": "요즘 치킨이 다 비싼거같아요",
          "original_form": "요즘 치킨이 다 비싼거같아요",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020039.1.21",
          "form": "아 맵슐랭 가족 중 한명이 좋아하는 메뉴라서 계속 먹었었네요",
          "original_form": "아 맵슐랭 가족 중 한명이 좋아하는 메뉴라서 계속 먹었었네요",
          "speaker_id": "1",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100020039.1.22",
          "form": "맞아요. 막상 마트가면 닭값은 그렇게까지 안나가는데..",
          "original_form": "맞아요. 막상 마트가면 닭값은 그렇게까지 안나가는데..",
          "speaker_id": "1",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100020039.1.23",
          "form": "어제 카페갔는데 호두바나나주스가 있어서 먹었더니 맛있더라고요",
          "original_form": "어제 카페갔는데 호두바나나주스가 있어서 먹었더니 맛있더라고요",
          "speaker_id": "1",
          "time": "20210828 09:53"
        },
        {
          "id": "MDRW2100020039.1.24",
          "form": "우유, 바나나, 호두를 갈아서 꾸덕하게 나왔는데 완전 맛있어요. 집에서 만들어 봐도 될 것 같네요",
          "original_form": "우유, 바나나, 호두를 갈아서 꾸덕하게 나왔는데 완전 맛있어요. 집에서 만들어 봐도 될 것 같네요",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100020039.1.25",
          "form": "아 그런 것도 파나요?",
          "original_form": "아 그런 것도 파나요?",
          "speaker_id": "2",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020039.1.26",
          "form": "아몬드 브리즈 같은 맛인가요?",
          "original_form": "아몬드 브리즈 같은 맛인가요?",
          "speaker_id": "2",
          "time": "20210828 09:55"
        },
        {
          "id": "MDRW2100020039.1.27",
          "form": "아몬드 브리즈를 안 먹어 봐서 모르겠어요..",
          "original_form": "아몬드 브리즈를 안 먹어 봐서 모르겠어요..",
          "speaker_id": "1",
          "time": "20210828 09:57"
        },
        {
          "id": "MDRW2100020039.1.28",
          "form": "호두 갈아서 만들었다니 비슷할거같아서요",
          "original_form": "호두 갈아서 만들었다니 비슷할거같아서요",
          "speaker_id": "2",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020039.1.29",
          "form": "그냥 바나나에 우유 갈았는데 거기에 더 고소한 맛이 추가됐다고 생각하시면 될 것 같아요",
          "original_form": "그냥 바나나에 우유 갈았는데 거기에 더 고소한 맛이 추가됐다고 생각하시면 될 것 같아요",
          "speaker_id": "1",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020039.1.30",
          "form": "오 아몬드 브리즈는 어떤 맛인가요?",
          "original_form": "오 아몬드 브리즈는 어떤 맛인가요?",
          "speaker_id": "1",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020039.1.31",
          "form": "아 바나나우유에 가까운 맛이군요",
          "original_form": "아 바나나우유에 가까운 맛이군요",
          "speaker_id": "2",
          "time": "20210828 09:58"
        },
        {
          "id": "MDRW2100020039.1.32",
          "form": "무지방우유에 가까운 맛인데 약간 향이 있어요",
          "original_form": "무지방우유에 가까운 맛인데 약간 향이 있어요",
          "speaker_id": "2",
          "time": "20210828 09:59"
        }
      ]
    }
