      },
      "utterance": [
        {
          "id": "MDRW2100002183.1.1",
          "form": "나는 산보다는 바다가 좋아.",
          "original_form": "나는 산보다는 바다가 좋아.",
          "speaker_id": "2",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002183.1.2",
          "form": "나는 부산에 또 가는 것도 좋고 강릉도 좋아",
          "original_form": "나는 부산에 또 가는 것도 좋고 강릉도 좋아",
          "speaker_id": "2",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002183.1.3",
          "form": "아니면 우리 돈을 더 모아서 가까운 해외에 나가보자.",
          "original_form": "아니면 우리 돈을 더 모아서 가까운 해외에 나가보자.",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002183.1.4",
          "form": "오 좋지 대만은 어때",
          "original_form": "오 좋지 대만은 어때",
          "speaker_id": "1",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002183.1.5",
          "form": "대만 좋지.",
          "original_form": "대만 좋지.",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002183.1.6",
          "form": "항상 가보고싶었는데 아직도 못갔어",
          "original_form": "항상 가보고싶었는데 아직도 못갔어",
          "speaker_id": "1",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002183.1.7",
          "form": "대만 가깝고 영어도 꽤 통해서 좋은 거 같아.",
          "original_form": "대만 가깝고 영어도 꽤 통해서 좋은 거 같아.",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002183.1.8",
          "form": "가서 딤섬을 너무 먹어보고싶어",
          "original_form": "가서 딤섬을 너무 먹어보고싶어",
          "speaker_id": "1",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002183.1.9",
          "form": "그 때까지 돈을 잘 모으자,",
          "original_form": "그 때까지 돈을 잘 모으자,",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002183.1.10",
          "form": "사실 나는 딤섬 먹기에는 싱가포르가 더 좋았어.",
          "original_form": "사실 나는 딤섬 먹기에는 싱가포르가 더 좋았어.",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002183.1.11",
          "form": "맞아 우리 회비도 모으니까",
          "original_form": "맞아 우리 회비도 모으니까",
          "speaker_id": "1",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002183.1.12",
          "form": "대만에는 쓸데없이 줄이 길어.",
          "original_form": "대만에는 쓸데없이 줄이 길어.",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002183.1.13",
          "form": "여행용으로 딱이다",
          "original_form": "여행용으로 딱이다",
          "speaker_id": "1",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002183.1.14",
          "form": "근데 싱가포르는 비행기가 비싸지않아?",
          "original_form": "근데 싱가포르는 비행기가 비싸지않아?",
          "speaker_id": "1",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002183.1.15",
          "form": "맞아. 열심히 모으면 숙박비는 나올 거 같아.",
          "original_form": "맞아. 열심히 모으면 숙박비는 나올 거 같아.",
          "speaker_id": "2",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002183.1.16",
          "form": "그거는 이제 미래의 우리가 알아서 할 일이야.",
          "original_form": "그거는 이제 미래의 우리가 알아서 할 일이야.",
          "speaker_id": "2",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002183.1.17",
          "form": "여행가게되면 하루는 좋은 곳에서 자자",
          "original_form": "여행가게되면 하루는 좋은 곳에서 자자",
          "speaker_id": "1",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002183.1.18",
          "form": "좋아 미래의 나 화이팅!",
          "original_form": "좋아 미래의 나 화이팅!",
          "speaker_id": "1",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002183.1.19",
          "form": "좋아. 돈을 열심히 모아야겠네.",
          "original_form": "좋아. 돈을 열심히 모아야겠네.",
          "speaker_id": "2",
          "time": "20210820 00:20"
        }
      ]
