      "utterance": [
        {
          "id": "MDRW2100037192.1.1",
          "form": "안녕하세요 저녁 드셨어요?",
          "original_form": "안녕하세요 저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210904 01:02"
        },
        {
          "id": "MDRW2100037192.1.2",
          "form": "그럼요ㅎㅎ 뭐 드셨어요?",
          "original_form": "그럼요ㅎㅎ 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100037192.1.3",
          "form": "소 불고기랑 깻잎이랑 먹었어요",
          "original_form": "소 불고기랑 깻잎이랑 먹었어요",
          "speaker_id": "1",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100037192.1.4",
          "form": "맛있는 거 드셨네요 ㅋㅋ",
          "original_form": "맛있는 거 드셨네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100037192.1.5",
          "form": "전 된장국이랑 반찬들 먹었어요",
          "original_form": "전 된장국이랑 반찬들 먹었어요",
          "speaker_id": "2",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100037192.1.6",
          "form": "된장국도 맛있잖아요 왜 그러세요",
          "original_form": "된장국도 맛있잖아요 왜 그러세요",
          "speaker_id": "1",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100037192.1.7",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100037192.1.8",
          "form": "ㅋㅋ 맛있었어요ㅋㅋ",
          "original_form": "ㅋㅋ 맛있었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100037192.1.9",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100037192.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100037192.1.11",
          "form": "주말에 식사 약속 있으세요?",
          "original_form": "주말에 식사 약속 있으세요?",
          "speaker_id": "1",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100037192.1.12",
          "form": "아뇨. 주말에 뭐 드실 예정이신가요?",
          "original_form": "아뇨. 주말에 뭐 드실 예정이신가요?",
          "speaker_id": "2",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100037192.1.13",
          "form": "아니요 저도 약속은 없어요",
          "original_form": "아니요 저도 약속은 없어요",
          "speaker_id": "1",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100037192.1.14",
          "form": "얼마 전에 배홍동 비빔면 처음 먹어봤는데 이것도 맛있더라구요",
          "original_form": "얼마 전에 배홍동 비빔면 처음 먹어봤는데 이것도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210904 01:21"
        },
        {
          "id": "MDRW2100037192.1.15",
          "form": "아 그거  눈독 들이고 있었는데 아직 못먹어 봤어요 맛있다니 저도 먹고 싶네요",
          "original_form": "아 그거  눈독 들이고 있었는데 아직 못먹어 봤어요 맛있다니 저도 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210904 01:22"
        },
        {
          "id": "MDRW2100037192.1.16",
          "form": "팔도랑 비슷한데 면이 좀 더 얇은 느낌?",
          "original_form": "팔도랑 비슷한데 면이 좀 더 얇은 느낌?",
          "speaker_id": "2",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100037192.1.17",
          "form": "오 팔도보다 면이 얇다구요",
          "original_form": "오 팔도보다 면이 얇다구요",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100037192.1.18",
          "form": "팔도 비빔면",
          "original_form": "팔도 비빔면",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100037192.1.19",
          "form": "팔도비빔면 먹은지 좀 되긴 하는데 그런 것 같아요  ㅋㅋ",
