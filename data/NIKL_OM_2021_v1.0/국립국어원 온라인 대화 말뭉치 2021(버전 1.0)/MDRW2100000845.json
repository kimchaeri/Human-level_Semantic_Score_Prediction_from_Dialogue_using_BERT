      "utterance": [
        {
          "id": "MDRW2100000845.1.1",
          "form": "전 나중에 유럽을 너무 가고싶어요",
          "original_form": "전 나중에 유럽을 너무 가고싶어요",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000845.1.2",
          "form": "저도ㅠ스페인",
          "original_form": "저도ㅠ스페인",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000845.1.3",
          "form": "가서 사진도 찍고 너무 좋을 거 같아요",
          "original_form": "가서 사진도 찍고 너무 좋을 거 같아요",
          "speaker_id": "2",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000845.1.4",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000845.1.5",
          "form": "그리고 그 나라에 음식도 너무 먹고싶어요 ㅎ",
          "original_form": "그리고 그 나라에 음식도 너무 먹고싶어요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 20:07"
        },
        {
          "id": "MDRW2100000845.1.6",
          "form": "맛있겠죠?",
          "original_form": "맛있겠죠?",
          "speaker_id": "2",
          "time": "20210819 20:07"
        },
        {
          "id": "MDRW2100000845.1.7",
          "form": "네 그럴것 같아여",
          "original_form": "네 그럴것 같아여",
          "speaker_id": "1",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100000845.1.8",
          "form": "혹시 어떤 여행가서 어떤 음식을 드시고 싶으세요?",
          "original_form": "혹시 어떤 여행가서 어떤 음식을 드시고 싶으세요?",
          "speaker_id": "2",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000845.1.9",
          "form": "전 음식보다 그나라 분위기가 좋아여",
          "original_form": "전 음식보다 그나라 분위기가 좋아여",
          "speaker_id": "1",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000845.1.10",
          "form": "오 분위기 좋죠",
          "original_form": "오 분위기 좋죠",
          "speaker_id": "2",
          "time": "20210819 20:18"
        },
        {
          "id": "MDRW2100000845.1.11",
          "form": "스폐인은 보통 어떤 분위기가 느껴지세요?",
          "original_form": "스폐인은 보통 어떤 분위기가 느껴지세요?",
          "speaker_id": "2",
          "time": "20210819 20:18"
        },
        {
          "id": "MDRW2100000845.1.12",
          "form": "정열이요",
          "original_form": "정열이요",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000845.1.13",
          "form": "스페인에 정열,, 오 전 뭔가 왜 탱고가 생각이날까요",
          "original_form": "스페인에 정열,, 오 전 뭔가 왜 탱고가 생각이날까요",
          "speaker_id": "2",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000845.1.14",
          "form": "스페인에 정열이라고 하셔서 생각났나봐여ㅋㅋㅋㅋㅋ",
          "original_form": "스페인에 정열이라고 하셔서 생각났나봐여ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000845.1.15",
          "form": "ㅎㅎ 그란가봐요",
          "original_form": "ㅎㅎ 그란가봐요",
          "speaker_id": "1",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000845.1.16",
          "form": "전 미국도 가고싶긴해요! 크리스마스 이런날에요",
          "original_form": "전 미국도 가고싶긴해요! 크리스마스 이런날에요",
          "speaker_id": "2",
          "time": "20210819 20:28"
        }
      ]
