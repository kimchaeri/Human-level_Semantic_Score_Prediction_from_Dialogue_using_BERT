      "utterance": [
        {
          "id": "MDRW2100029038.1.1",
          "form": "그래서 막 요즘 생크림 한가득 들어있는 빵 정말 싫어해요",
          "original_form": "그래서 막 요즘 생크림 한가득 들어있는 빵 정말 싫어해요",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029038.1.2",
          "form": "역시 사람들은 싫어하는 거로 통합된다는 게 진짜예요",
          "original_form": "역시 사람들은 싫어하는 거로 통합된다는 게 진짜예요",
          "speaker_id": "1",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029038.1.3",
          "form": "먹기도 너무 불편해보이고 쳐다만봐도 느끼ㅠㅠ",
          "original_form": "먹기도 너무 불편해보이고 쳐다만봐도 느끼ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029038.1.4",
          "form": "저는 그래서 크림 안들어간거만 먹어요...",
          "original_form": "저는 그래서 크림 안들어간거만 먹어요...",
          "speaker_id": "1",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100029038.1.5",
          "form": "전 슈크림은 괜찮아요!",
          "original_form": "전 슈크림은 괜찮아요!",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029038.1.6",
          "form": "헉 별개로 저도 커스타드크림은 좋아해요",
          "original_form": "헉 별개로 저도 커스타드크림은 좋아해요",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029038.1.7",
          "form": "생크림 특유의 맛이 싫었을 뿐..",
          "original_form": "생크림 특유의 맛이 싫었을 뿐..",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029038.1.8",
          "form": "핫쉬... 커스터드 쥰맛",
          "original_form": "핫쉬... 커스터드 쥰맛",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029038.1.9",
          "form": "크림치즈도요.",
          "original_form": "크림치즈도요.",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100029038.1.10",
          "form": "크림치즈는 인정이죠.",
          "original_form": "크림치즈는 인정이죠.",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029038.1.11",
          "form": "저 치즈 광인이라 너무 좋아요.",
          "original_form": "저 치즈 광인이라 너무 좋아요.",
          "speaker_id": "1",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029038.1.12",
          "form": "진짜치즈광인같아서 너무 웃겨요",
          "original_form": "진짜치즈광인같아서 너무 웃겨요",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029038.1.13",
          "form": "아 ㅋㅋㅋㅋㅋㄱㅋㄱㅋㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㅋㄱ",
          "original_form": "아 ㅋㅋㅋㅋㅋㄱㅋㄱㅋㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㄱㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210902 23:32"
        },
        {
          "id": "MDRW2100029038.1.14",
          "form": "아ㅋ\r\nㅋㅋ\r\nㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠㅠㅠ ㅋㅋ ㅠㅠㅠㅠ",
          "original_form": "아ㅋ\r\nㅋㅋ\r\nㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠㅠㅠ ㅋㅋ ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029038.1.15",
          "form": "광인이긴 하죠... 치즈 안넣어먹는 곳이 없어요",
          "original_form": "광인이긴 하죠... 치즈 안넣어먹는 곳이 없어요",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029038.1.16",
          "form": "라면 김치볶음밥 김치찌개 어쩌구...등등..",
          "original_form": "라면 김치볶음밥 김치찌개 어쩌구...등등..",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029038.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜 재밌네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜 재밌네요",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029038.1.18",
          "form": "헐찌개에두요?",
          "original_form": "헐찌개에두요?",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029038.1.19",
          "form": "몹시 민망했으나 흡! 심심이 정신차렷!",
          "original_form": "몹시 민망했으나 흡! 심심이 정신차렷!",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029038.1.20",
          "form": "저 찌개에 밥말아먹을때마다 치즈 한장씩 넣어요!",
          "original_form": "저 찌개에 밥말아먹을때마다 치즈 한장씩 넣어요!",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029038.1.21",
          "form": "오 싱기해요 고소한가요?",
          "original_form": "오 싱기해요 고소한가요?",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029038.1.22",
          "form": "치즈맛이 나서 생각보다 괜찮아요! 느끼해질수 있어서 싫어하는 분들은 싫어할거같아요",
          "original_form": "치즈맛이 나서 생각보다 괜찮아요! 느끼해질수 있어서 싫어하는 분들은 싫어할거같아요",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029038.1.23",
          "form": "저는 와구와구 먹지만요",
          "original_form": "저는 와구와구 먹지만요",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029038.1.24",
          "form": "체다치즈 넣으세요?",
          "original_form": "체다치즈 넣으세요?",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029038.1.25",
          "form": "네네! 저는 보통 체다치즈 넣어요!",
          "original_form": "네네! 저는 보통 체다치즈 넣어요!",
          "speaker_id": "1",
          "time": "20210902 23:42"
        }
      ]
    }
