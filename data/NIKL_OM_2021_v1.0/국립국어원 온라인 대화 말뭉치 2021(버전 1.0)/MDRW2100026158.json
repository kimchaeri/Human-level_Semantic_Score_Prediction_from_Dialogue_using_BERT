        },
        {
          "id": "MDRW2100026158.1.2",
          "form": "너 저녁 뭐 먹는댔지",
          "original_form": "너 저녁 뭐 먹는댔지",
          "speaker_id": "2",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100026158.1.3",
          "form": "나 배달 음식 시켰어",
          "original_form": "나 배달 음식 시켰어",
          "speaker_id": "1",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100026158.1.4",
          "form": "대패삼겹살",
          "original_form": "대패삼겹살",
          "speaker_id": "1",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100026158.1.5",
          "form": "맛있는 거 먹네",
          "original_form": "맛있는 거 먹네",
          "speaker_id": "2",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026158.1.6",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026158.1.7",
          "form": "김치 사놓은 지 너무 오래돼서",
          "original_form": "김치 사놓은 지 너무 오래돼서",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026158.1.8",
          "form": "밥 볶아 먹었어",
          "original_form": "밥 볶아 먹었어",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026158.1.9",
          "form": "곰팡이 안 핀게 다행",
          "original_form": "곰팡이 안 핀게 다행",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026158.1.10",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026158.1.11",
          "form": "김치볶음밥 먹었구나",
          "original_form": "김치볶음밥 먹었구나",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026158.1.12",
          "form": "나 어제 대패김치볶음밥에 계란 올려서 먹었는데",
          "original_form": "나 어제 대패김치볶음밥에 계란 올려서 먹었는데",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026158.1.13",
          "form": "뭔데",
          "original_form": "뭔데",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026158.1.14",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026158.1.15",
          "form": "거기가",
          "original_form": "거기가",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026158.1.16",
          "form": "김가루 올려먹어",
          "original_form": "김가루 올려먹어",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026158.1.17",
          "form": "맛있음",
          "original_form": "맛있음",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026158.1.18",
          "form": "와 배달 시킨 거 왔다",
          "original_form": "와 배달 시킨 거 왔다",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026158.1.19",
          "form": "맞아 나 삼겹살이랑",
          "original_form": "맞아 나 삼겹살이랑",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026158.1.20",
          "form": "대패새우도 같이 시켰어",
          "original_form": "대패새우도 같이 시켰어",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026158.1.21",
          "form": "맛있겠지",
          "original_form": "맛있겠지",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026158.1.22",
          "form": "내일 나 뭐 먹을까",
          "original_form": "내일 나 뭐 먹을까",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100026158.1.23",
          "form": "우동",
          "original_form": "우동",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100026158.1.24",
          "form": "우동... 맛있겠다",
          "original_form": "우동... 맛있겠다",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100026158.1.25",
          "form": "솔직히 우동 면 보다 국물이 더 좋음",
          "original_form": "솔직히 우동 면 보다 국물이 더 좋음",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100026158.1.26",
          "form": "국물만 퍼마시고 싶다",
          "original_form": "국물만 퍼마시고 싶다",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100026158.1.27",
          "form": "국물만?ㅋㅋㅋㅋㅋ",
          "original_form": "국물만?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100026158.1.28",
          "form": "아 아니면 냉모밀 어때",
          "original_form": "아 아니면 냉모밀 어때",
          "speaker_id": "1",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100026158.1.29",
          "form": "냉모밀... 전에 먹었을 때 그닥 내 취향은 아니었던 거 같어",
          "original_form": "냉모밀... 전에 먹었을 때 그닥 내 취향은 아니었던 거 같어",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100026158.1.30",
          "form": "아 맞어 나 그거 물어볼라 했는데",
          "original_form": "아 맞어 나 그거 물어볼라 했는데",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100026158.1.31",
          "form": "어떤 거?",
          "original_form": "어떤 거?",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100026158.1.32",
          "form": "컴포즈 가면 뭐 먹어 추천좀",
          "original_form": "컴포즈 가면 뭐 먹어 추천좀",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100026158.1.33",
          "form": "아이스 아메리카노",
          "original_form": "아이스 아메리카노",
