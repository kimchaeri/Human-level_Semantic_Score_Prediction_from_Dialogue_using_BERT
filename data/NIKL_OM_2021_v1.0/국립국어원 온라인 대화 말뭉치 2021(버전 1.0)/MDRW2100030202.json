      "utterance": [
        {
          "id": "MDRW2100030202.1.1",
          "form": "냥냥이 봉사활동을 하신다고요...?",
          "original_form": "냥냥이 봉사활동을 하신다고요...?",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030202.1.2",
          "form": "구체적으로 무슨 일을 하시는지 알려주시면 감사드리겠습니다",
          "original_form": "구체적으로 무슨 일을 하시는지 알려주시면 감사드리겠습니다",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030202.1.3",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030202.1.4",
          "form": "고양이 쉼터에 가서 아이들의 밥과 물을 갈아주고",
          "original_form": "고양이 쉼터에 가서 아이들의 밥과 물을 갈아주고",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030202.1.5",
          "form": "응아를 치워주며 놀아주는 활동 입니다",
          "original_form": "응아를 치워주며 놀아주는 활동 입니다",
          "speaker_id": "2",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100030202.1.6",
          "form": "와 대단하시네요 ㅇ0ㅇ",
          "original_form": "와 대단하시네요 ㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030202.1.7",
          "form": "힘들지 않으신가요ㅡ",
          "original_form": "힘들지 않으신가요ㅡ",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030202.1.8",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100030202.1.9",
          "form": "알러지 때문에 힘들어오..",
          "original_form": "알러지 때문에 힘들어오..",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030202.1.10",
          "form": "오늘 그리고 냥이한테 냥펀치 맞아서 피보고 온...",
          "original_form": "오늘 그리고 냥이한테 냥펀치 맞아서 피보고 온...",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030202.1.11",
          "form": "엄마께 1번은 뭐냐고 주에 2번가자 했지만",
          "original_form": "엄마께 1번은 뭐냐고 주에 2번가자 했지만",
          "speaker_id": "2",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030202.1.12",
          "form": "그냥 1번 갈래오....^^",
          "original_form": "그냥 1번 갈래오....^^",
          "speaker_id": "2",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030202.1.13",
          "form": "앗 님",
          "original_form": "앗 님",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030202.1.14",
          "form": "고생하시네요 정말로",
          "original_form": "고생하시네요 정말로",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030202.1.15",
          "form": "알러지 약 드시나요?",
          "original_form": "알러지 약 드시나요?",
          "speaker_id": "1",
          "time": "20210903 23:05"
        },
        {
          "id": "MDRW2100030202.1.16",
          "form": "냥펀치 맞고 안다치셨어요???",
          "original_form": "냥펀치 맞고 안다치셨어요???",
          "speaker_id": "1",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030202.1.17",
          "form": "그래도 가서 보고나면 힘은 납니댜",
          "original_form": "그래도 가서 보고나면 힘은 납니댜",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030202.1.18",
          "form": "동물이 주는 힘이 있는거 같아오..",
          "original_form": "동물이 주는 힘이 있는거 같아오..",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030202.1.19",
          "form": "가입하고 싶은 동아리 있나오?",
          "original_form": "가입하고 싶은 동아리 있나오?",
          "speaker_id": "2",
          "time": "20210903 23:06"
        },
        {
          "id": "MDRW2100030202.1.20",
          "form": "없어오",
          "original_form": "없어오",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030202.1.21",
          "form": "강아지 봉사활동 기회되면 해보고싶어오",
          "original_form": "강아지 봉사활동 기회되면 해보고싶어오",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030202.1.22",
          "form": "동물이 주는 힘 ㅇㅈ 입니다",
          "original_form": "동물이 주는 힘 ㅇㅈ 입니다",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100030202.1.23",
          "form": "강아지 좋져",
          "original_form": "강아지 좋져",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030202.1.24",
          "form": "사실 강아지가 좋아요....",
          "original_form": "사실 강아지가 좋아요....",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030202.1.25",
          "form": "고양이 이제 몇번 냥펀치 맞았더니 좀 무서워요.........",
          "original_form": "고양이 이제 몇번 냥펀치 맞았더니 좀 무서워요.........",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100030202.1.26",
          "form": "그렇져 댕댕이가 최고라개",
          "original_form": "그렇져 댕댕이가 최고라개",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030202.1.27",
          "form": "냥냥이는 냥아치라개",
          "original_form": "냥냥이는 냥아치라개",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030202.1.28",
          "form": "냥아치 킹정",
          "original_form": "냥아치 킹정",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030202.1.29",
          "form": "ㅎ ㅏ냥펀치만 맞으면 피를 보니..",
          "original_form": "ㅎ ㅏ냥펀치만 맞으면 피를 보니..",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030202.1.30",
          "form": "이젠 냥이 앞에서는 눈깔고 다녀야 할거 같....",
          "original_form": "이젠 냥이 앞에서는 눈깔고 다녀야 할거 같....",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030202.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030202.1.32",
          "form": "용서해주실거야",
          "original_form": "용서해주실거야",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030202.1.33",
          "form": "츄-르를 바치면",
          "original_form": "츄-르를 바치면",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030202.1.34",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030202.1.35",
          "form": "그렇져",
          "original_form": "그렇져",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030202.1.36",
          "form": "츄우르가 없으면 봉사활동의 의미가 없지 않나요",
          "original_form": "츄우르가 없으면 봉사활동의 의미가 없지 않나요",
          "speaker_id": "1",
          "time": "20210903 23:19"
        }
      ]
