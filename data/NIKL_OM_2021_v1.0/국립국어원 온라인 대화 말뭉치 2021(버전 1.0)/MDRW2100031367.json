      "utterance": [
        {
          "id": "MDRW2100031367.1.1",
          "form": "담주에 시간 남 가보자",
          "original_form": "담주에 시간 남 가보자",
          "speaker_id": "2",
          "time": "20210904 20:50"
        },
        {
          "id": "MDRW2100031367.1.2",
          "form": "ㅇㅇ 그보다 부산대 이흥용 가는 것보다",
          "original_form": "ㅇㅇ 그보다 부산대 이흥용 가는 것보다",
          "speaker_id": "1",
          "time": "20210904 20:58"
        },
        {
          "id": "MDRW2100031367.1.3",
          "form": "여기서 케이크 주문하는게 나을 거 같아서",
          "original_form": "여기서 케이크 주문하는게 나을 거 같아서",
          "speaker_id": "1",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100031367.1.4",
          "form": "기장이 훨씬 가깝지",
          "original_form": "기장이 훨씬 가깝지",
          "speaker_id": "2",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031367.1.5",
          "form": "거기서 해 그럼",
          "original_form": "거기서 해 그럼",
          "speaker_id": "2",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031367.1.6",
          "form": "근데 아까 전화하니까 한시간째 통화중이더라고",
          "original_form": "근데 아까 전화하니까 한시간째 통화중이더라고",
          "speaker_id": "1",
          "time": "20210904 21:02"
        },
        {
          "id": "MDRW2100031367.1.7",
          "form": "인스타로 메세지도 보냈는데 답 없고ㅡㅡ",
          "original_form": "인스타로 메세지도 보냈는데 답 없고ㅡㅡ",
          "speaker_id": "1",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100031367.1.8",
          "form": "보자 내가 전화해 볼게",
          "original_form": "보자 내가 전화해 볼게",
          "speaker_id": "2",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100031367.1.9",
          "form": "안 받을 걸..",
          "original_form": "안 받을 걸..",
          "speaker_id": "1",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100031367.1.10",
          "form": "통화중이다",
          "original_form": "통화중이다",
          "speaker_id": "2",
          "time": "20210904 21:03"
        },
        {
          "id": "MDRW2100031367.1.11",
          "form": "무슨 통화를 하루종일한다니",
          "original_form": "무슨 통화를 하루종일한다니",
          "speaker_id": "2",
          "time": "20210904 21:04"
        },
        {
          "id": "MDRW2100031367.1.12",
          "form": "배가 불렀네 불렀어",
          "original_form": "배가 불렀네 불렀어",
          "speaker_id": "2",
          "time": "20210904 21:04"
        },
        {
          "id": "MDRW2100031367.1.13",
          "form": "홀케잌은 예약을 해야 된다는데 여기 주문하기 진짜",
          "original_form": "홀케잌은 예약을 해야 된다는데 여기 주문하기 진짜",
          "speaker_id": "1",
          "time": "20210904 21:04"
        },
        {
          "id": "MDRW2100031367.1.14",
          "form": "힘들다",
          "original_form": "힘들다",
          "speaker_id": "1",
          "time": "20210904 21:04"
        },
        {
          "id": "MDRW2100031367.1.15",
          "form": "다른데 찾아봐",
          "original_form": "다른데 찾아봐",
          "speaker_id": "2",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100031367.1.16",
          "form": "좀 전에 문자 왔다",
          "original_form": "좀 전에 문자 왔다",
          "speaker_id": "1",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100031367.1.17",
          "form": "딸기 케이크만 된다는데?",
          "original_form": "딸기 케이크만 된다는데?",
          "speaker_id": "1",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100031367.1.18",
          "form": "뭐 되는 케이크도 없고만",
          "original_form": "뭐 되는 케이크도 없고만",
          "speaker_id": "2",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100031367.1.19",
          "form": "주문도 하기 힘들고",
          "original_form": "주문도 하기 힘들고",
          "speaker_id": "2",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100031367.1.20",
          "form": "부산대서 해라",
          "original_form": "부산대서 해라",
          "speaker_id": "2",
          "time": "20210904 21:05"
        },
        {
          "id": "MDRW2100031367.1.21",
          "form": "거기 케이크 종류도 많은데",
          "original_form": "거기 케이크 종류도 많은데",
          "speaker_id": "2",
          "time": "20210904 21:06"
        },
        {
          "id": "MDRW2100031367.1.22",
          "form": "먹고싶은거 고를 수 있는 데서 해",
          "original_form": "먹고싶은거 고를 수 있는 데서 해",
          "speaker_id": "2",
          "time": "20210904 21:06"
        },
        {
          "id": "MDRW2100031367.1.23",
          "form": "그래야 될까봐",
          "original_form": "그래야 될까봐",
          "speaker_id": "1",
          "time": "20210904 21:06"
        },
        {
          "id": "MDRW2100031367.1.24",
          "form": "나 먹고 싶은 걸로 해도 되?",
          "original_form": "나 먹고 싶은 걸로 해도 되?",
          "speaker_id": "1",
          "time": "20210904 21:06"
        },
        {
          "id": "MDRW2100031367.1.25",
          "form": "니생일인데 너 먹고 싶은데로해야지",
          "original_form": "니생일인데 너 먹고 싶은데로해야지",
          "speaker_id": "2",
          "time": "20210904 21:07"
        },
        {
          "id": "MDRW2100031367.1.26",
          "form": "대로",
          "original_form": "대로",
          "speaker_id": "2",
          "time": "20210904 21:07"
        },
        {
          "id": "MDRW2100031367.1.27",
          "form": "ㅇㅋㅇㅋ초코케잌한다~",
          "original_form": "ㅇㅋㅇㅋ초코케잌한다~",
          "speaker_id": "1",
          "time": "20210904 21:07"
        }
      ]
    }
  ]
