      "utterance": [
        {
          "id": "MDRW2100037581.1.1",
          "form": "1시간 반이라니.,",
          "original_form": "1시간 반이라니.,",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037581.1.2",
          "form": "쉬는시간 있나요 ㅋㅋ",
          "original_form": "쉬는시간 있나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037581.1.3",
          "form": "선생님이 남들 가르칠때 그때 쉬는거죠ㅋㅋㅋ",
          "original_form": "선생님이 남들 가르칠때 그때 쉬는거죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037581.1.4",
          "form": "아하 ㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037581.1.5",
          "form": "몇명이서 해요?",
          "original_form": "몇명이서 해요?",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037581.1.6",
          "form": "10명까지 받는다 했는데",
          "original_form": "10명까지 받는다 했는데",
          "speaker_id": "2",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037581.1.7",
          "form": "지금은 8명 해요",
          "original_form": "지금은 8명 해요",
          "speaker_id": "2",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037581.1.8",
          "form": "와 꽤 많이하네요?",
          "original_form": "와 꽤 많이하네요?",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100037581.1.9",
          "form": "언제부터 하셨어요?",
          "original_form": "언제부터 하셨어요?",
          "speaker_id": "1",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100037581.1.10",
          "form": "1달정도요",
          "original_form": "1달정도요",
          "speaker_id": "2",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100037581.1.11",
          "form": "얼마안됐어요 ㅎㅎ",
          "original_form": "얼마안됐어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100037581.1.12",
          "form": "아 ! 올림픽보고",
          "original_form": "아 ! 올림픽보고",
          "speaker_id": "1",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100037581.1.13",
          "form": "하신건가요 ?",
          "original_form": "하신건가요 ?",
          "speaker_id": "1",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100037581.1.14",
          "form": "그건 아닌데요. 옛날에 수련회에서 체험 학습? 한적이 있는데",
          "original_form": "그건 아닌데요. 옛날에 수련회에서 체험 학습? 한적이 있는데",
          "speaker_id": "2",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100037581.1.15",
          "form": "너무 재밌는거에요.  근데 고딩이 그걸 계속 할수 있을리가...",
          "original_form": "너무 재밌는거에요.  근데 고딩이 그걸 계속 할수 있을리가...",
          "speaker_id": "2",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100037581.1.16",
          "form": "근처에 딱히 할만한곳도 없었고",
          "original_form": "근처에 딱히 할만한곳도 없었고",
          "speaker_id": "2",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100037581.1.17",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100037581.1.18",
          "form": "헐 저도 고딩때 한번 해본거같아요 ㅋㅋㅋ",
          "original_form": "헐 저도 고딩때 한번 해본거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:33"
        },
        {
          "id": "MDRW2100037581.1.19",
          "form": "그러다 이젠 정말 운동을 해야한다! 하는 때가 온거죠",
          "original_form": "그러다 이젠 정말 운동을 해야한다! 하는 때가 온거죠",
          "speaker_id": "2",
          "time": "20210901 12:33"
        },
        {
          "id": "MDRW2100037581.1.20",
          "form": "아 !",
          "original_form": "아 !",
          "speaker_id": "1",
          "time": "20210901 12:33"
        },
        {
          "id": "MDRW2100037581.1.21",
          "form": "고민하다가 검색했는데!",
          "original_form": "고민하다가 검색했는데!",
          "speaker_id": "2",
          "time": "20210901 12:33"
        },
        {
          "id": "MDRW2100037581.1.22",
          "form": "네네 !",
          "original_form": "네네 !",
          "speaker_id": "1",
          "time": "20210901 12:33"
        },
        {
          "id": "MDRW2100037581.1.23",
          "form": "어머나, 집근처에 실내교육장이 있네??!!",
          "original_form": "어머나, 집근처에 실내교육장이 있네??!!",
          "speaker_id": "2",
          "time": "20210901 12:34"
        },
        {
          "id": "MDRW2100037581.1.24",
          "form": "헐 완전 ! 그건 가야되네요 !",
          "original_form": "헐 완전 ! 그건 가야되네요 !",
          "speaker_id": "1",
          "time": "20210901 12:34"
        },
        {
          "id": "MDRW2100037581.1.25",
          "form": "집 근처라니 ㅎㅎㅎ",
          "original_form": "집 근처라니 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:34"
        }
      ]
    }
  ]
