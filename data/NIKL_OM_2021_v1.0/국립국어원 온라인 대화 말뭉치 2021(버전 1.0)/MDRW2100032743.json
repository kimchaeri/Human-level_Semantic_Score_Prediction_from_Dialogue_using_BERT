      "utterance": [
        {
          "id": "MDRW2100032743.1.1",
          "form": "요즘 뭐보는거있어?",
          "original_form": "요즘 뭐보는거있어?",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032743.1.2",
          "form": "난 지금 대탈출4 보고있어",
          "original_form": "난 지금 대탈출4 보고있어",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032743.1.3",
          "form": "나는 전한길쌤",
          "original_form": "나는 전한길쌤",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032743.1.4",
          "form": "유투브로 봐",
          "original_form": "유투브로 봐",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032743.1.5",
          "form": "언니는?",
          "original_form": "언니는?",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032743.1.6",
          "form": "티비",
          "original_form": "티비",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032743.1.7",
          "form": "tvn",
          "original_form": "tvn",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032743.1.8",
          "form": "대탈출 지금 보는중",
          "original_form": "대탈출 지금 보는중",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032743.1.9",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100032743.1.10",
          "form": "나는대탈출보면서 방탈출게임도 저거랑 비슷할까?",
          "original_form": "나는대탈출보면서 방탈출게임도 저거랑 비슷할까?",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032743.1.11",
          "form": "궁금해",
          "original_form": "궁금해",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032743.1.12",
          "form": "ㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032743.1.13",
          "form": "역시 탈출 재밌어",
          "original_form": "역시 탈출 재밌어",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032743.1.14",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032743.1.15",
          "form": "방탈출이랑 완전 다르지",
          "original_form": "방탈출이랑 완전 다르지",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032743.1.16",
          "form": "대탈출은 방탈출카페랑 연계하면",
          "original_form": "대탈출은 방탈출카페랑 연계하면",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032743.1.17",
          "form": "재밌을듯",
          "original_form": "재밌을듯",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100032743.1.18",
          "form": "오오 넘좋을듯",
          "original_form": "오오 넘좋을듯",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032743.1.19",
          "form": "근데 그러기에 인원을 너무 많이쓰고 시간도 많이 써서",
          "original_form": "근데 그러기에 인원을 너무 많이쓰고 시간도 많이 써서",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032743.1.20",
          "form": "대신 비싸지겠지ㅜ",
          "original_form": "대신 비싸지겠지ㅜ",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032743.1.21",
          "form": "걍 방송으로 보는게 제일 나을걸 ㅋㅋㅋ",
          "original_form": "걍 방송으로 보는게 제일 나을걸 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032743.1.22",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032743.1.23",
          "form": "엄마도 자주보시더라구",
          "original_form": "엄마도 자주보시더라구",
          "speaker_id": "2",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032743.1.24",
          "form": "근데 보다보면 강호동이나 김동현 볼때",
          "original_form": "근데 보다보면 강호동이나 김동현 볼때",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032743.1.25",
          "form": "답답해 ㅋㅋㅋ",
          "original_form": "답답해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032743.1.26",
          "form": "힘으로만 쓱 밀거나 소리만 지를땐 좀 넘기고싶어",
          "original_form": "힘으로만 쓱 밀거나 소리만 지를땐 좀 넘기고싶어",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100032743.1.27",
          "form": "tv로 보는거라 넘길수없는게 아쉬워",
          "original_form": "tv로 보는거라 넘길수없는게 아쉬워",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032743.1.28",
          "form": "유투브나 영상이였으면 1.5배속으로 봤을지도",
          "original_form": "유투브나 영상이였으면 1.5배속으로 봤을지도",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100032743.1.29",
          "form": "그치..",
          "original_form": "그치..",
          "speaker_id": "2",
          "time": "20210905 23:15"
        },
        {
          "id": "MDRW2100032743.1.30",
          "form": "게다가 대탈출은 리액션 많이 비쳐줘서",
          "original_form": "게다가 대탈출은 리액션 많이 비쳐줘서",
          "speaker_id": "2",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100032743.1.31",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032743.1.32",
          "form": "그리고 고함지르거나 중요한 포인트라고 생각하는장면을",
          "original_form": "그리고 고함지르거나 중요한 포인트라고 생각하는장면을",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032743.1.33",
          "form": "자꾸 계속 보여줘서",
          "original_form": "자꾸 계속 보여줘서",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032743.1.34",
          "form": "그것도 좀 자제를했으면 ㅋㅋ",
          "original_form": "그것도 좀 자제를했으면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032743.1.35",
