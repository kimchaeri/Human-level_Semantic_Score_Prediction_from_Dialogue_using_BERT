      "utterance": [
        {
          "id": "MDRW2100043786.1.1",
          "form": "여행가고 싶은 곳 있어?",
          "original_form": "여행가고 싶은 곳 있어?",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043786.1.2",
          "form": "지금은 별로 없긴한데",
          "original_form": "지금은 별로 없긴한데",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100043786.1.3",
          "form": "국내위주 호캉스나 숲속엔 가고싶네",
          "original_form": "국내위주 호캉스나 숲속엔 가고싶네",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100043786.1.4",
          "form": "아 휴양을 목적으로",
          "original_form": "아 휴양을 목적으로",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100043786.1.5",
          "form": "여름에 휴양림 추첨 넣었는데",
          "original_form": "여름에 휴양림 추첨 넣었는데",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100043786.1.6",
          "form": "수도권 4단계라 휴양림 숲속의집 자체 운영을 안함ㅠ",
          "original_form": "수도권 4단계라 휴양림 숲속의집 자체 운영을 안함ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043786.1.7",
          "form": "헐 몰랐네",
          "original_form": "헐 몰랐네",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043786.1.8",
          "form": "아깝다",
          "original_form": "아깝다",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043786.1.9",
          "form": "야외인데 휴양림은 열어도 되는거 아닌가",
          "original_form": "야외인데 휴양림은 열어도 되는거 아닌가",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043786.1.10",
          "form": "응 아빠도 가입해서 추첨 넣었는데",
          "original_form": "응 아빠도 가입해서 추첨 넣었는데",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043786.1.11",
          "form": "숲속의집이 4인까지라",
          "original_form": "숲속의집이 4인까지라",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100043786.1.12",
          "form": "집합금지에 걸렸어 그땐",
          "original_form": "집합금지에 걸렸어 그땐",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043786.1.13",
          "form": "그래서 아예 취소됨",
          "original_form": "그래서 아예 취소됨",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043786.1.14",
          "form": "추첨전날ㅋㅋ",
          "original_form": "추첨전날ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043786.1.15",
          "form": "독채라 가평쪽 넣었는디",
          "original_form": "독채라 가평쪽 넣었는디",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043786.1.16",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043786.1.17",
          "form": "됐음 좋았을걸",
          "original_form": "됐음 좋았을걸",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043786.1.18",
          "form": "응 8월 초쯤 넣었거든",
          "original_form": "응 8월 초쯤 넣었거든",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043786.1.19",
          "form": "겨울쯤엔 어디 갈 수 있으려나",
          "original_form": "겨울쯤엔 어디 갈 수 있으려나",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043786.1.20",
          "form": "아님 언니집으로 휴가가지 뭐",
          "original_form": "아님 언니집으로 휴가가지 뭐",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043786.1.21",
          "form": "그래 ㅋㅋ 그것도 좋지",
          "original_form": "그래 ㅋㅋ 그것도 좋지",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043786.1.22",
          "form": "빔 틀고 뒹굴뒹굴",
          "original_form": "빔 틀고 뒹굴뒹굴",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043786.1.23",
          "form": "빈백이 그립네요",
          "original_form": "빈백이 그립네요",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043786.1.24",
          "form": "그러게 나도 좀 그립네",
          "original_form": "그러게 나도 좀 그립네",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043786.1.25",
          "form": "내방여행 흑흑",
          "original_form": "내방여행 흑흑",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043786.1.26",
          "form": "이번에 내려가면 누리고와",
          "original_form": "이번에 내려가면 누리고와",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043786.1.27",
          "form": "내방여행",
          "original_form": "내방여행",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043786.1.28",
          "form": "엉 일박이일 내방여행 즐기고 올게",
          "original_form": "엉 일박이일 내방여행 즐기고 올게",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100043786.1.29",
          "form": "바퀴가 얼마나 있을지 걱정이지만..",
          "original_form": "바퀴가 얼마나 있을지 걱정이지만..",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100043786.1.30",
          "form": "잘갔다와ㅜ",
          "original_form": "잘갔다와ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:39"
        }
      ]
    }
