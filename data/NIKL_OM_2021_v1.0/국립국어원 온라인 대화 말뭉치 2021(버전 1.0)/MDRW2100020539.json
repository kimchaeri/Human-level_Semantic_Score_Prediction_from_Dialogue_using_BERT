      "utterance": [
        {
          "id": "MDRW2100020539.1.1",
          "form": "특히 중국집..!",
          "original_form": "특히 중국집..!",
          "speaker_id": "2",
          "time": "20210828 09:29"
        },
        {
          "id": "MDRW2100020539.1.2",
          "form": "중국집!",
          "original_form": "중국집!",
          "speaker_id": "1",
          "time": "20210828 09:29"
        },
        {
          "id": "MDRW2100020539.1.3",
          "form": "해물 빼달라고 해도 어묵이나 다시다가 꼭 들어있어요!",
          "original_form": "해물 빼달라고 해도 어묵이나 다시다가 꼭 들어있어요!",
          "speaker_id": "2",
          "time": "20210828 09:29"
        },
        {
          "id": "MDRW2100020539.1.4",
          "form": "맞아요ㅡㅡ",
          "original_form": "맞아요ㅡㅡ",
          "speaker_id": "1",
          "time": "20210828 09:29"
        },
        {
          "id": "MDRW2100020539.1.5",
          "form": "저번엔 볶음밥에 오징어 빼달랬는데",
          "original_form": "저번엔 볶음밥에 오징어 빼달랬는데",
          "speaker_id": "2",
          "time": "20210828 09:29"
        },
        {
          "id": "MDRW2100020539.1.6",
          "form": "오징어 아니라 갑오징어라서 안뺐대요!",
          "original_form": "오징어 아니라 갑오징어라서 안뺐대요!",
          "speaker_id": "2",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020539.1.7",
          "form": "아니 안넣으면 될걸 굳이넣더라구요",
          "original_form": "아니 안넣으면 될걸 굳이넣더라구요",
          "speaker_id": "1",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020539.1.8",
          "form": "그게 말이야 장난이야",
          "original_form": "그게 말이야 장난이야",
          "speaker_id": "1",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020539.1.9",
          "form": "갑오징어는 오징어가 아닌가요!",
          "original_form": "갑오징어는 오징어가 아닌가요!",
          "speaker_id": "2",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020539.1.10",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020539.1.11",
          "form": "어휴 저는 그냥 짜증나고 말 일이지만",
          "original_form": "어휴 저는 그냥 짜증나고 말 일이지만",
          "speaker_id": "2",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020539.1.12",
          "form": "오징어가 소고기도아니고",
          "original_form": "오징어가 소고기도아니고",
          "speaker_id": "1",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020539.1.13",
          "form": "name1님은 알러지라 이러면 큰일이겠어요",
          "original_form": "&name1&님은 알러지라 이러면 큰일이겠어요",
          "speaker_id": "2",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020539.1.14",
          "form": "맞아요 그래서 두번세번 말하는데",
          "original_form": "맞아요 그래서 두번세번 말하는데",
          "speaker_id": "1",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020539.1.15",
          "form": "알러지라는것에 대해 약간",
          "original_form": "알러지라는것에 대해 약간",
          "speaker_id": "1",
          "time": "20210828 09:31"
        },
        {
          "id": "MDRW2100020539.1.16",
          "form": "알러지같은소리하네",
          "original_form": "알러지같은소리하네",
          "speaker_id": "1",
          "time": "20210828 09:31"
        },
        {
          "id": "MDRW2100020539.1.17",
          "form": "먹으면 낫는다 이런 얘기",
          "original_form": "먹으면 낫는다 이런 얘기",
          "speaker_id": "2",
          "time": "20210828 09:31"
        },
        {
          "id": "MDRW2100020539.1.18",
          "form": "이런 반응.. 있을 때 있어서ㅋㅋㅋㅋ",
          "original_form": "이런 반응.. 있을 때 있어서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:31"
        },
        {
          "id": "MDRW2100020539.1.19",
          "form": "많이들으셨을거같아요",
          "original_form": "많이들으셨을거같아요",
          "speaker_id": "2",
          "time": "20210828 09:31"
        },
        {
          "id": "MDRW2100020539.1.20",
          "form": "그얘기 너무싫어요",
          "original_form": "그얘기 너무싫어요",
          "speaker_id": "1",
          "time": "20210828 09:31"
        },
        {
          "id": "MDRW2100020539.1.21",
          "form": "짜증날듯",
          "original_form": "짜증날듯",
          "speaker_id": "2",
