      "utterance": [
        {
          "id": "MDRW2100035062.1.1",
          "form": "고민이 있니",
          "original_form": "고민이 있니",
          "speaker_id": "2",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100035062.1.2",
          "form": "미용실을 가야겠어",
          "original_form": "미용실을 가야겠어",
          "speaker_id": "1",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100035062.1.3",
          "form": "갑자기 왜",
          "original_form": "갑자기 왜",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100035062.1.4",
          "form": "여름이 되니까 머리 상태가 안 좋아",
          "original_form": "여름이 되니까 머리 상태가 안 좋아",
          "speaker_id": "1",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100035062.1.5",
          "form": "미용실 가서 어떤 거 하려고",
          "original_form": "미용실 가서 어떤 거 하려고",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100035062.1.6",
          "form": "그게 고민이야",
          "original_form": "그게 고민이야",
          "speaker_id": "1",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100035062.1.7",
          "form": "파마를 해보는 건 어때",
          "original_form": "파마를 해보는 건 어때",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100035062.1.8",
          "form": "긍정적으로 생각하고 있어",
          "original_form": "긍정적으로 생각하고 있어",
          "speaker_id": "1",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100035062.1.9",
          "form": "너는 에스컬 파마를 하면 어울릴 걸",
          "original_form": "너는 에스컬 파마를 하면 어울릴 걸",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100035062.1.10",
          "form": "매직도 고민하고 있어",
          "original_form": "매직도 고민하고 있어",
          "speaker_id": "1",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100035062.1.11",
          "form": "매직과 파마를 같이 하는 것도 괜찮지",
          "original_form": "매직과 파마를 같이 하는 것도 괜찮지",
          "speaker_id": "2",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100035062.1.12",
          "form": "그렇게 되면 가격이 너무 높잖아",
          "original_form": "그렇게 되면 가격이 너무 높잖아",
          "speaker_id": "1",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100035062.1.13",
          "form": "그래도 예뻐지면 기분이 좋아",
          "original_form": "그래도 예뻐지면 기분이 좋아",
