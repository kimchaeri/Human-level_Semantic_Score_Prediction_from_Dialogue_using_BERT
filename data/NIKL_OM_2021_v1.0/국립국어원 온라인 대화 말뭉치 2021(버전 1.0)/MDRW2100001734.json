      "utterance": [
        {
          "id": "MDRW2100001734.1.1",
          "form": "운동을 해야 건강한데~",
          "original_form": "운동을 해야 건강한데~",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001734.1.2",
          "form": "운동좀 하러갈깡?",
          "original_form": "운동좀 하러갈깡?",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001734.1.3",
          "form": "요즘 운동도 집에서 ㅎ",
          "original_form": "요즘 운동도 집에서 ㅎ",
          "speaker_id": "2",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001734.1.4",
          "form": "집에 러닝머신좀 해야겠다",
          "original_form": "집에 러닝머신좀 해야겠다",
          "speaker_id": "1",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001734.1.5",
          "form": "그래~ \n유산소운동이 최고지 살빼는데 ㅎ",
          "original_form": "그래~ \n유산소운동이 최고지 살빼는데 ㅎ",
          "speaker_id": "2",
          "time": "20210819 18:28"
        },
        {
          "id": "MDRW2100001734.1.6",
          "form": "그렇지 ㅋㅋ 근데 ㅠ 빨래걸이 됬잖ㅇ아",
          "original_form": "그렇지 ㅋㅋ 근데 ㅠ 빨래걸이 됬잖ㅇ아",
          "speaker_id": "1",
          "time": "20210819 18:29"
        },
        {
          "id": "MDRW2100001734.1.7",
          "form": "치우면되지~",
          "original_form": "치우면되지~",
          "speaker_id": "2",
          "time": "20210819 18:29"
        },
        {
          "id": "MDRW2100001734.1.8",
          "form": "오늘부터 시작해라",
          "original_form": "오늘부터 시작해라",
          "speaker_id": "2",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001734.1.9",
          "form": "구랭 이따 같이 치우자 ㅎ",
          "original_form": "구랭 이따 같이 치우자 ㅎ",
          "speaker_id": "1",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001734.1.10",
          "form": "몇분정도 달리면 좋지?",
          "original_form": "몇분정도 달리면 좋지?",
          "speaker_id": "1",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001734.1.11",
          "form": "20분 이상은 해야지",
          "original_form": "20분 이상은 해야지",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001734.1.12",
          "form": "글쿤 ㅋㅋ 이따 30분 달려야징",
          "original_form": "글쿤 ㅋㅋ 이따 30분 달려야징",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001734.1.13",
          "form": "그래 운동도 열심히!!",
          "original_form": "그래 운동도 열심히!!",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100001734.1.14",
          "form": "계단운동도 유산소 근력운동이다",
          "original_form": "계단운동도 유산소 근력운동이다",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100001734.1.15",
          "form": "오 계단운동이 있었군!",
          "original_form": "오 계단운동이 있었군!",
          "speaker_id": "1",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100001734.1.16",
