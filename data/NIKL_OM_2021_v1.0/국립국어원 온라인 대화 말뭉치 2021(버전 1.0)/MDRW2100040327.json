      "utterance": [
        {
          "id": "MDRW2100040327.1.1",
          "form": "점심 맛난거 드셨나용",
          "original_form": "점심 맛난거 드셨나용",
          "speaker_id": "1",
          "time": "20210904 16:18"
        },
        {
          "id": "MDRW2100040327.1.2",
          "form": "네! 서브웨이 샌드위치 먹었어요!",
          "original_form": "네! 서브웨이 샌드위치 먹었어요!",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040327.1.3",
          "form": "와우 맛있겠네요",
          "original_form": "와우 맛있겠네요",
          "speaker_id": "1",
          "time": "20210904 19:06"
        },
        {
          "id": "MDRW2100040327.1.4",
          "form": "무슨메뉴 좋아하세요?",
          "original_form": "무슨메뉴 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 19:06"
        },
        {
          "id": "MDRW2100040327.1.5",
          "form": "저는 에그마요 제일 많이 먹어요 ㅋㅋ",
          "original_form": "저는 에그마요 제일 많이 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:07"
        },
        {
          "id": "MDRW2100040327.1.6",
          "form": "드셔보신 적 있나요?",
          "original_form": "드셔보신 적 있나요?",
          "speaker_id": "2",
          "time": "20210904 20:07"
        },
        {
          "id": "MDRW2100040327.1.7",
          "form": "오 저도 에그마요 좋아해요",
          "original_form": "오 저도 에그마요 좋아해요",
          "speaker_id": "1",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100040327.1.8",
          "form": "b.l.t도 좋아해요",
          "original_form": "b.l.t도 좋아해요",
          "speaker_id": "1",
          "time": "20210904 20:26"
        },
        {
          "id": "MDRW2100040327.1.9",
          "form": "아 비엘티 비엠티 저도 가끔씩 먹어요 ㅋㅋ",
          "original_form": "아 비엘티 비엠티 저도 가끔씩 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100040327.1.10",
          "form": "빵은 뭘로 드시나요 ㅋㅋ",
          "original_form": "빵은 뭘로 드시나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100040327.1.11",
          "form": "저 그... 어 이름이 생각안나는데",
          "original_form": "저 그... 어 이름이 생각안나는데",
          "speaker_id": "1",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100040327.1.12",
          "form": "납작한거요",
          "original_form": "납작한거요",
          "speaker_id": "1",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100040327.1.13",
          "form": "아 플랫브래드요??",
          "original_form": "아 플랫브래드요??",
          "speaker_id": "2",
          "time": "20210904 20:40"
        },
        {
          "id": "MDRW2100040327.1.14",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100040327.1.15",
          "form": "제가 요즘 기억력이 떨어져서 이름이 잘 생각이 안나더라고요",
          "original_form": "제가 요즘 기억력이 떨어져서 이름이 잘 생각이 안나더라고요",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100040327.1.16",
          "form": "그 빵이 제일 먹기 쉽더라고요",
          "original_form": "그 빵이 제일 먹기 쉽더라고요",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100040327.1.17",
          "form": "그쵸 쫄깃해서 맛있어요 ㅋㅋ",
          "original_form": "그쵸 쫄깃해서 맛있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100040327.1.18",
          "form": "샐러드로도 먹을수 있는데 드셔보셨나요 전 아직 ㅋㅋ",
          "original_form": "샐러드로도 먹을수 있는데 드셔보셨나요 전 아직 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100040327.1.19",
          "form": "네!! 그건 배달로 먹어봤어여",
          "original_form": "네!! 그건 배달로 먹어봤어여",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040327.1.20",
          "form": "근데 그 때 뭔가 안맞았는지 장염걸렸어요 ㅎ",
          "original_form": "근데 그 때 뭔가 안맞았는지 장염걸렸어요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040327.1.21",
          "form": "아하 샐러드로 먹는거 궁금하긴 하더라고요 야채양이 더 많으니까.. 가격추가되긴 하지만요 ㅎㅎ",
          "original_form": "아하 샐러드로 먹는거 궁금하긴 하더라고요 야채양이 더 많으니까.. 가격추가되긴 하지만요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100040327.1.22",
          "form": "전 그냥 샌드위치가 더 맛있는거 같아요 ㅎ",
          "original_form": "전 그냥 샌드위치가 더 맛있는거 같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100040327.1.23",
          "form": "아무래도 야채를 잘게 다지는거라 호불호가 있더라고요 ㅎㄹ",
          "original_form": "아무래도 야채를 잘게 다지는거라 호불호가 있더라고요 ㅎㄹ",
