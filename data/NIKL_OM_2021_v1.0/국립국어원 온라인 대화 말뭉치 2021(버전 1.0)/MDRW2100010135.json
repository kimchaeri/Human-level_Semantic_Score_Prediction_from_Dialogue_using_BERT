      "utterance": [
        {
          "id": "MDRW2100010135.1.1",
          "form": "알바해본적있니",
          "original_form": "알바해본적있니",
          "speaker_id": "2",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010135.1.2",
          "form": "응알바해봤지",
          "original_form": "응알바해봤지",
          "speaker_id": "1",
          "time": "20210823 15:38"
        },
        {
          "id": "MDRW2100010135.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210823 15:38"
        },
        {
          "id": "MDRW2100010135.1.4",
          "form": "나도 해봤어",
          "original_form": "나도 해봤어",
          "speaker_id": "2",
          "time": "20210823 15:38"
        },
        {
          "id": "MDRW2100010135.1.5",
          "form": "어떤 알바해봤니",
          "original_form": "어떤 알바해봤니",
          "speaker_id": "2",
          "time": "20210823 15:38"
        },
        {
          "id": "MDRW2100010135.1.6",
          "form": "쿠팡알바",
          "original_form": "쿠팡알바",
          "speaker_id": "1",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100010135.1.7",
          "form": "물류 쪽에서 일했구나",
          "original_form": "물류 쪽에서 일했구나",
          "speaker_id": "2",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100010135.1.8",
          "form": "가서 뭐했어",
          "original_form": "가서 뭐했어",
          "speaker_id": "2",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100010135.1.9",
          "form": "포장도 해봤고 피킹도 해봤었지",
          "original_form": "포장도 해봤고 피킹도 해봤었지",
          "speaker_id": "1",
          "time": "20210823 15:42"
        },
        {
          "id": "MDRW2100010135.1.10",
          "form": "어떤 게 더 힘들었어",
          "original_form": "어떤 게 더 힘들었어",
          "speaker_id": "2",
          "time": "20210823 15:42"
        },
        {
          "id": "MDRW2100010135.1.11",
          "form": "포장이 힘들었어",
          "original_form": "포장이 힘들었어",
          "speaker_id": "1",
          "time": "20210823 15:42"
        },
        {
          "id": "MDRW2100010135.1.12",
          "form": "가만히 서 있어야 되거든",
          "original_form": "가만히 서 있어야 되거든",
          "speaker_id": "1",
          "time": "20210823 15:42"
        },
        {
          "id": "MDRW2100010135.1.13",
          "form": "맞아 서 있는 건 힘들어",
          "original_form": "맞아 서 있는 건 힘들어",
          "speaker_id": "2",
          "time": "20210823 15:42"
        },
        {
          "id": "MDRW2100010135.1.14",
          "form": "쿠팡 알바는 주로 어디서 보고 연락을 해?",
          "original_form": "쿠팡 알바는 주로 어디서 보고 연락을 해?",
          "speaker_id": "2",
          "time": "20210823 15:43"
        },
        {
          "id": "MDRW2100010135.1.15",
          "form": "알바몬에서",
          "original_form": "알바몬에서",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010135.1.16",
          "form": "그렇구나 해본 적이 없어서 궁금했어",
          "original_form": "그렇구나 해본 적이 없어서 궁금했어",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010135.1.17",
          "form": "급여는 당일 지급이야 ?",
          "original_form": "급여는 당일 지급이야 ?",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010135.1.18",
          "form": "응 다음날 바로 나와",
          "original_form": "응 다음날 바로 나와",
          "speaker_id": "1",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010135.1.19",
          "form": "알려줘서 고마워",
          "original_form": "알려줘서 고마워",
          "speaker_id": "2",
          "time": "20210823 15:46"
        }
      ]
    }
