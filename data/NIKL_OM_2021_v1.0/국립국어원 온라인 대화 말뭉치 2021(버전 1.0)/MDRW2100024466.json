      "utterance": [
        {
          "id": "MDRW2100024466.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100024466.1.2",
          "form": "네 대학생이요",
          "original_form": "네 대학생이요",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024466.1.3",
          "form": "어제개강했어요",
          "original_form": "어제개강했어요",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024466.1.4",
          "form": "으악.... 아직도 비대면 수업해요?",
          "original_form": "으악.... 아직도 비대면 수업해요?",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024466.1.5",
          "form": "학교마다 다르던데",
          "original_form": "학교마다 다르던데",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024466.1.6",
          "form": "저희 학교는 섞어서해요",
          "original_form": "저희 학교는 섞어서해요",
          "speaker_id": "2",
          "time": "20210831 10:57"
        },
        {
          "id": "MDRW2100024466.1.7",
          "form": "대면도 하고 비대면도 하고",
          "original_form": "대면도 하고 비대면도 하고",
          "speaker_id": "2",
          "time": "20210831 10:57"
        },
        {
          "id": "MDRW2100024466.1.8",
          "form": "아....싫겠다...ㅎㅎ",
          "original_form": "아....싫겠다...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:57"
        },
        {
          "id": "MDRW2100024466.1.9",
          "form": "대면수업은 어떤거해요?",
          "original_form": "대면수업은 어떤거해요?",
          "speaker_id": "1",
          "time": "20210831 10:57"
        },
        {
          "id": "MDRW2100024466.1.10",
          "form": "실습?",
          "original_form": "실습?",
          "speaker_id": "1",
          "time": "20210831 10:57"
        },
        {
          "id": "MDRW2100024466.1.11",
          "form": "이론인데 전공이요........",
          "original_form": "이론인데 전공이요........",
          "speaker_id": "2",
          "time": "20210831 10:59"
        },
        {
          "id": "MDRW2100024466.1.12",
          "form": "아..전공이면....대면 수업 하겠네요 ㅜㅜ",
          "original_form": "아..전공이면....대면 수업 하겠네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100024466.1.13",
          "form": "수강신청은 성공하셨어요?",
          "original_form": "수강신청은 성공하셨어요?",
          "speaker_id": "1",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100024466.1.14",
          "form": "실패까진 아니고",
          "original_form": "실패까진 아니고",
          "speaker_id": "2",
          "time": "20210831 11:01"
        },
        {
          "id": "MDRW2100024466.1.15",
          "form": "원하는 과목 두개못들었는데",
          "original_form": "원하는 과목 두개못들었는데",
          "speaker_id": "2",
          "time": "20210831 11:01"
        },
        {
          "id": "MDRW2100024466.1.16",
          "form": "어제 정정기간에 하나잡았어요",
          "original_form": "어제 정정기간에 하나잡았어요",
          "speaker_id": "2",
          "time": "20210831 11:01"
        },
        {
          "id": "MDRW2100024466.1.17",
          "form": "다행이네요ㅎㅎ",
          "original_form": "다행이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100024466.1.18",
          "form": "요즘도 수강신청 어렵더라고요ㅠ",
          "original_form": "요즘도 수강신청 어렵더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100024466.1.19",
          "form": "그래도 매크로 방지 생겨서 좋아요",
          "original_form": "그래도 매크로 방지 생겨서 좋아요",
          "speaker_id": "2",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100024466.1.20",
          "form": "어떤 과는 과에서 아예 시간표를 짜주더라고요",
          "original_form": "어떤 과는 과에서 아예 시간표를 짜주더라고요",
          "speaker_id": "1",
          "time": "20210831 11:03"
        },
        {
          "id": "MDRW2100024466.1.21",
          "form": "아 친구네 과가 그러는데",
          "original_form": "아 친구네 과가 그러는데",
          "speaker_id": "2",
          "time": "20210831 11:03"
        },
        {
          "id": "MDRW2100024466.1.22",
          "form": "전공만 잇으면그렇대요",
          "original_form": "전공만 잇으면그렇대요",
          "speaker_id": "2",
          "time": "20210831 11:03"
        },
        {
          "id": "MDRW2100024466.1.23",
          "form": "아하....",
          "original_form": "아하....",
          "speaker_id": "1",
          "time": "20210831 11:03"
        }
      ]
