      "utterance": [
        {
          "id": "MDRW2100030187.1.1",
          "form": "오늘도 게임 할라구?",
          "original_form": "오늘도 게임 할라구?",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.2",
          "form": "재밌는 게임 하구 싶어",
          "original_form": "재밌는 게임 하구 싶어",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.3",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.4",
          "form": "밤늦게까지 게임하면",
          "original_form": "밤늦게까지 게임하면",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.5",
          "form": "잠이 잘 안온대",
          "original_form": "잠이 잘 안온대",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.6",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.7",
          "form": "신기하다...",
          "original_form": "신기하다...",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.8",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.9",
          "form": "근데 내일은 주말이니까",
          "original_form": "근데 내일은 주말이니까",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.10",
          "form": "아 맞아!!",
          "original_form": "아 맞아!!",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.11",
          "form": "해도 괜찮을거같긴 해 ㅎㅎ",
          "original_form": "해도 괜찮을거같긴 해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.12",
          "form": "그런데 셧다운제말야",
          "original_form": "그런데 셧다운제말야",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.13",
          "form": "맞아 ㅎㅎ",
          "original_form": "맞아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030187.1.14",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.15",
          "form": "폐지된다는거 들었어?",
          "original_form": "폐지된다는거 들었어?",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.16",
          "form": "셧다운제 알쥐",
          "original_form": "셧다운제 알쥐",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.17",
          "form": "들엇어!",
          "original_form": "들엇어!",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.18",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.19",
          "form": "대박이지",
          "original_form": "대박이지",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.20",
          "form": "라떼는 셧다운제때문에 슬펐는데..",
          "original_form": "라떼는 셧다운제때문에 슬펐는데..",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.21",
          "form": "잘됏넹",
          "original_form": "잘됏넹",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.22",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.23",
          "form": "이제 학원끝나구 집와도",
          "original_form": "이제 학원끝나구 집와도",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.24",
          "form": "할수잇겟다 ㅎㅎ",
          "original_form": "할수잇겟다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.25",
          "form": "근데 마침 딱 일주일만있으면",
          "original_form": "근데 마침 딱 일주일만있으면",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.26",
          "form": "셧다운제 적용이 안돼거든",
          "original_form": "셧다운제 적용이 안돼거든",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.27",
          "form": "근데 왜 하필 이제와서 셧다운제 폐지를 하는지 몰르겠어.",
          "original_form": "근데 왜 하필 이제와서 셧다운제 폐지를 하는지 몰르겠어.",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.28",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.29",
          "form": "그럴 수 잇지 ㅎㅎ",
          "original_form": "그럴 수 잇지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.30",
          "form": "옛날엔 게임 인식이",
          "original_form": "옛날엔 게임 인식이",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.31",
          "form": "그럴수없어",
          "original_form": "그럴수없어",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.32",
          "form": "안좋앗으니까",
          "original_form": "안좋앗으니까",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100030187.1.33",
          "form": "ㅋㅋㅋㅋㅋ이제 없어졌으니까 됐잖어",
          "original_form": "ㅋㅋㅋㅋㅋ이제 없어졌으니까 됐잖어",
          "speaker_id": "2",
          "time": "20210903 22:52"
        }
      ]
