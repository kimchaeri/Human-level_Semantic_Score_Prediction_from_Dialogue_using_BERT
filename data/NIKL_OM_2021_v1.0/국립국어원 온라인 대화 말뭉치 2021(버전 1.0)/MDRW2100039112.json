      "utterance": [
        {
          "id": "MDRW2100039112.1.1",
          "form": "저 이사기념으로 거울장만하려구요",
          "original_form": "저 이사기념으로 거울장만하려구요",
          "speaker_id": "1",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100039112.1.2",
          "form": "거울잇자나요",
          "original_form": "거울잇자나요",
          "speaker_id": "2",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100039112.1.3",
          "form": "있는거말구요 반신거울 사고싶어요",
          "original_form": "있는거말구요 반신거울 사고싶어요",
          "speaker_id": "1",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100039112.1.4",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100039112.1.5",
          "form": "반신만 보고 싶으니까요..?",
          "original_form": "반신만 보고 싶으니까요..?",
          "speaker_id": "1",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100039112.1.6",
          "form": "왜요 전신이다 이쁘신데",
          "original_form": "왜요 전신이다 이쁘신데",
          "speaker_id": "2",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100039112.1.7",
          "form": "어머나..^^ 제 전신에 대해서 뭘아신다고.. 하지만 칭찬 감사합니다",
          "original_form": "어머나..^^ 제 전신에 대해서 뭘아신다고.. 하지만 칭찬 감사합니다",
          "speaker_id": "1",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100039112.1.8",
          "form": "반신거울 놓을 콘솔도 사고싶어요",
          "original_form": "반신거울 놓을 콘솔도 사고싶어요",
          "speaker_id": "1",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100039112.1.9",
          "form": "자리가 잇나유",
          "original_form": "자리가 잇나유",
          "speaker_id": "2",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100039112.1.10",
          "form": "글쎄요.. 자리가 없을 것 같긴해요 아 정말 독립하고싶네요!!",
          "original_form": "글쎄요.. 자리가 없을 것 같긴해요 아 정말 독립하고싶네요!!",
          "speaker_id": "1",
          "time": "20210903 10:27"
        },
        {
          "id": "MDRW2100039112.1.11",
          "form": "독립하면 더 자리가. ..",
          "original_form": "독립하면 더 자리가. ..",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100039112.1.12",
          "form": "넓은집으로 가고싶어요",
          "original_form": "넓은집으로 가고싶어요",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039112.1.13",
          "form": "ㅋㅋㅋㅋ 휴...저두요",
          "original_form": "ㅋㅋㅋㅋ 휴...저두요",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039112.1.14",
          "form": "드레스룸도 만들고 싶어요 비싸겠죠?",
          "original_form": "드레스룸도 만들고 싶어요 비싸겠죠?",
          "speaker_id": "1",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100039112.1.15",
          "form": "네... 저도로망 ㅎㅎㅎㅎ",
          "original_form": "네... 저도로망 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100039112.1.16",
          "form": "요즘 물건사도 놓을데가 없거든요...",
          "original_form": "요즘 물건사도 놓을데가 없거든요...",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100039112.1.17",
          "form": "이사는 어케하셧을까..",
          "original_form": "이사는 어케하셧을까..",
