      "utterance": [
        {
          "id": "MDRW2100041543.1.1",
          "form": "오늘도 바빠...",
          "original_form": "오늘도 바빠...",
          "speaker_id": "2",
          "time": "20210906 09:58"
        },
        {
          "id": "MDRW2100041543.1.2",
          "form": "주말에 드릉드릉하고 월요일 아침부터 몰려왔나봐",
          "original_form": "주말에 드릉드릉하고 월요일 아침부터 몰려왔나봐",
          "speaker_id": "2",
          "time": "20210906 09:58"
        },
        {
          "id": "MDRW2100041543.1.3",
          "form": "그리고 방금 내 월급일 25일이 되었다는",
          "original_form": "그리고 방금 내 월급일 25일이 되었다는",
          "speaker_id": "2",
          "time": "20210906 09:59"
        },
        {
          "id": "MDRW2100041543.1.4",
          "form": "충격적인",
          "original_form": "충격적인",
          "speaker_id": "2",
          "time": "20210906 09:59"
        },
        {
          "id": "MDRW2100041543.1.5",
          "form": "(두둥",
          "original_form": "(두둥",
          "speaker_id": "2",
          "time": "20210906 09:59"
        },
        {
          "id": "MDRW2100041543.1.6",
          "form": "아니..아아니... 한달일한급여가8일이나뒤에나온다뇨",
          "original_form": "아니..아아니... 한달일한급여가8일이나뒤에나온다뇨",
          "speaker_id": "2",
          "time": "20210906 10:00"
        },
        {
          "id": "MDRW2100041543.1.7",
          "form": "(싫다고 말도 못하는 을",
          "original_form": "(싫다고 말도 못하는 을",
          "speaker_id": "2",
          "time": "20210906 10:01"
        },
        {
          "id": "MDRW2100041543.1.8",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 10:01"
        },
        {
          "id": "MDRW2100041543.1.9",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "1",
          "time": "20210906 10:15"
        },
        {
          "id": "MDRW2100041543.1.10",
          "form": "엥 근데 25일 토요일이잖아",
          "original_form": "엥 근데 25일 토요일이잖아",
          "speaker_id": "1",
          "time": "20210906 10:15"
        },
        {
          "id": "MDRW2100041543.1.11",
          "form": "그럼 24일에 넣어주나?",
          "original_form": "그럼 24일에 넣어주나?",
          "speaker_id": "1",
          "time": "20210906 10:15"
        },
        {
          "id": "MDRW2100041543.1.12",
          "form": "우린 월급날에 주말 끼면 그 전날에 넣어주는데",
          "original_form": "우린 월급날에 주말 끼면 그 전날에 넣어주는데",
          "speaker_id": "1",
          "time": "20210906 10:16"
        },
        {
          "id": "MDRW2100041543.1.13",
          "form": "함 물어봐",
          "original_form": "함 물어봐",
          "speaker_id": "1",
          "time": "20210906 10:16"
        },
        {
          "id": "MDRW2100041543.1.14",
          "form": "아놔 나쁜말",
          "original_form": "아놔 나쁜말",
          "speaker_id": "2",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100041543.1.15",
          "form": "월급날이 주말이면 그 전인가 다음주인가 들어온다고",
          "original_form": "월급날이 주말이면 그 전인가 다음주인가 들어온다고",
          "speaker_id": "2",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100041543.1.16",
          "form": "엥....",
          "original_form": "엥....",
          "speaker_id": "1",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100041543.1.17",
          "form": "그 전과 그 다음주는",
          "original_form": "그 전과 그 다음주는",
          "speaker_id": "1",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100041543.1.18",
          "form": "계약서 쓸 때 본거 같은데 기억이 가물거리는",
          "original_form": "계약서 쓸 때 본거 같은데 기억이 가물거리는",
          "speaker_id": "2",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100041543.1.19",
          "form": "아예 다른 의미인디",
          "original_form": "아예 다른 의미인디",
          "speaker_id": "1",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100041543.1.20",
          "form": "ㅋㅋ큐ㅠ 아마 전에 주지 않을까......",
          "original_form": "ㅋㅋ큐ㅠ 아마 전에 주지 않을까......",
          "speaker_id": "2",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100041543.1.21",
          "form": "보통 전일..일걸..?ㅇㅇ..",
          "original_form": "보통 전일..일걸..?ㅇㅇ..",
          "speaker_id": "1",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100041543.1.22",
          "form": "ㅇㅇ..",
          "original_form": "ㅇㅇ..",
          "speaker_id": "2",
          "time": "20210906 11:14"
        },
        {
          "id": "MDRW2100041543.1.23",
          "form": "그럼.. 다행히 딱 걸리네..",
          "original_form": "그럼.. 다행히 딱 걸리네..",
          "speaker_id": "2",
          "time": "20210906 11:15"
        },
        {
          "id": "MDRW2100041543.1.24",
          "form": "아니 추석이 월급 전이라니 직장인 죽어유",
          "original_form": "아니 추석이 월급 전이라니 직장인 죽어유",
          "speaker_id": "2",
          "time": "20210906 11:15"
        },
        {
          "id": "MDRW2100041543.1.25",
          "form": "월급날 신라호텔 고고",
          "original_form": "월급날 신라호텔 고고",
          "speaker_id": "1",
          "time": "20210906 11:15"
        },
        {
          "id": "MDRW2100041543.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:15"
        },
        {
          "id": "MDRW2100041543.1.27",
