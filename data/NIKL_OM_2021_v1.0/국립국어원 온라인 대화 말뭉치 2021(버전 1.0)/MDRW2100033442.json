      "utterance": [
        {
          "id": "MDRW2100033442.1.1",
          "form": "기억에 남는 여행 경험이 있으신가요?",
          "original_form": "기억에 남는 여행 경험이 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100033442.1.2",
          "form": "스키장 갔던거 기억에  남아요",
          "original_form": "스키장 갔던거 기억에  남아요",
          "speaker_id": "2",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100033442.1.3",
          "form": "어디 스키장으로 가셨나요?",
          "original_form": "어디 스키장으로 가셨나요?",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100033442.1.4",
          "form": "어디였지 부산  옆에 뭔 밸리였던거 같은데",
          "original_form": "어디였지 부산  옆에 뭔 밸리였던거 같은데",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100033442.1.5",
          "form": "스키장 가보신 적 있으신가요",
          "original_form": "스키장 가보신 적 있으신가요",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100033442.1.6",
          "form": "스키장은 안 가봤어요, 스키 잘 타시나여??",
          "original_form": "스키장은 안 가봤어요, 스키 잘 타시나여??",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100033442.1.7",
          "form": "아니요ㅋㅋ.. 스키를 못타서 기억에 남아요",
          "original_form": "아니요ㅋㅋ.. 스키를 못타서 기억에 남아요",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100033442.1.8",
          "form": "남들은 쌩쌩 잘만타던데 저는 어렵더라구요",
          "original_form": "남들은 쌩쌩 잘만타던데 저는 어렵더라구요",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100033442.1.9",
          "form": "그래서 리프트 타고 올라갔다가 못타겠어서 걸어내려왔어요",
          "original_form": "그래서 리프트 타고 올라갔다가 못타겠어서 걸어내려왔어요",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100033442.1.10",
          "form": "생각보다 경사면이 가팔라서 가 볼 엄두도 안 나더라구요ㅠ",
          "original_form": "생각보다 경사면이 가팔라서 가 볼 엄두도 안 나더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100033442.1.11",
          "form": "좋아하는 여행지 있나요",
          "original_form": "좋아하는 여행지 있나요",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100033442.1.12",
          "form": "저는 동해바다 좋아해요",
          "original_form": "저는 동해바다 좋아해요",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100033442.1.13",
          "form": "동해 어디 좋아해요",
          "original_form": "동해 어디 좋아해요",
          "speaker_id": "2",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100033442.1.14",
          "form": "강원도 속초 많이 갔어요",
          "original_form": "강원도 속초 많이 갔어요",
          "speaker_id": "1",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100033442.1.15",
          "form": "속초 너무 좋다더라구요 속초 바다가 잔잔한 파도가 일어서 물놀이 하기도 좋다그러고.. 바다가면 주로 뭐 하시나요",
          "original_form": "속초 너무 좋다더라구요 속초 바다가 잔잔한 파도가 일어서 물놀이 하기도 좋다그러고.. 바다가면 주로 뭐 하시나요",
          "speaker_id": "2",
          "time": "20210823 23:04"
        },
        {
          "id": "MDRW2100033442.1.16",
          "form": "스노쿨링 많이해요!",
          "original_form": "스노쿨링 많이해요!",
          "speaker_id": "1",
          "time": "20210823 23:09"
        }
      ]
