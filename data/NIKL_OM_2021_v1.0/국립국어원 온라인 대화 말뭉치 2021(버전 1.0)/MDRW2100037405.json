      "utterance": [
        {
          "id": "MDRW2100037405.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037405.1.2",
          "form": "게임 많이는 안해봤어요 ㅎㅎ 저는 단순한게임만 좋아해서 지금은 쿠키런 킹덤하고있어요",
          "original_form": "게임 많이는 안해봤어요 ㅎㅎ 저는 단순한게임만 좋아해서 지금은 쿠키런 킹덤하고있어요",
          "speaker_id": "2",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037405.1.3",
          "form": "게임좋아하시나요??",
          "original_form": "게임좋아하시나요??",
          "speaker_id": "2",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037405.1.4",
          "form": "앗 저와 비슷하세요",
          "original_form": "앗 저와 비슷하세요",
          "speaker_id": "1",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037405.1.5",
          "form": "전 브롤해요",
          "original_form": "전 브롤해요",
          "speaker_id": "1",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100037405.1.6",
          "form": "오 안그래도 그거 친구가 해보자고 자꾸 꼬셔요 ㅎㅎ",
          "original_form": "오 안그래도 그거 친구가 해보자고 자꾸 꼬셔요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037405.1.7",
          "form": "재밌나요??",
          "original_form": "재밌나요??",
          "speaker_id": "2",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037405.1.8",
          "form": "아들 말리다 제가 해요ㅋㅋ",
          "original_form": "아들 말리다 제가 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:24"
        },
        {
          "id": "MDRW2100037405.1.9",
          "form": "ㅋㅋㅋㅋ오오 한번 해봐야겠어요",
          "original_form": "ㅋㅋㅋㅋ오오 한번 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210831 23:25"
        },
        {
          "id": "MDRW2100037405.1.10",
          "form": "초등학생들이 많이 하나바요",
          "original_form": "초등학생들이 많이 하나바요",
          "speaker_id": "1",
          "time": "20210831 23:26"
        },
        {
          "id": "MDRW2100037405.1.11",
          "form": "그속에서 활동중요ㅋ",
          "original_form": "그속에서 활동중요ㅋ",
          "speaker_id": "1",
          "time": "20210831 23:27"
        },
        {
          "id": "MDRW2100037405.1.12",
          "form": "ㅋㅋㅋㅋㅋ앗 그렇구낰ㅋㅋㅋ근데 재밌어보여요ㅎㅎ 마침 쿠킹덤 질려가는 참인데 갈아타야겠어요",
          "original_form": "ㅋㅋㅋㅋㅋ앗 그렇구낰ㅋㅋㅋ근데 재밌어보여요ㅎㅎ 마침 쿠킹덤 질려가는 참인데 갈아타야겠어요",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100037405.1.13",
          "form": "거의 중독되요ㅋ",
          "original_form": "거의 중독되요ㅋ",
          "speaker_id": "1",
          "time": "20210831 23:30"
        },
        {
          "id": "MDRW2100037405.1.14",
          "form": "ㅋㅋㅋname1님이 말리셔야겠는데요ㅎㅎ",
          "original_form": "ㅋㅋㅋ&name1&님이 말리셔야겠는데요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037405.1.15",
          "form": "이나이에 빠질줄몰랐어요",
          "original_form": "이나이에 빠질줄몰랐어요",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037405.1.16",
          "form": "ㅋㅋㅋㅋ에이 게임에 나이가 어디있나여 저도 30대입니다ㅎㅎ",
