      "utterance": [
        {
          "id": "MDRW2100036319.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100036319.1.2",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100036319.1.3",
          "form": "아니요 아직이요",
          "original_form": "아니요 아직이요",
          "speaker_id": "2",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100036319.1.4",
          "form": "언제 맞으세요?",
          "original_form": "언제 맞으세요?",
          "speaker_id": "1",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100036319.1.5",
          "form": "이번주에요",
          "original_form": "이번주에요",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100036319.1.6",
          "form": "어떤 백신 맞으세요?",
          "original_form": "어떤 백신 맞으세요?",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100036319.1.7",
          "form": "화이자더라구요",
          "original_form": "화이자더라구요",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100036319.1.8",
          "form": "저도 화이자 맞았어요 백신맞으실때 힘 최대한 빼셔야하는거 잊지마세요ㅎ",
          "original_form": "저도 화이자 맞았어요 백신맞으실때 힘 최대한 빼셔야하는거 잊지마세요ㅎ",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100036319.1.9",
          "form": "네! 기억할게요",
          "original_form": "네! 기억할게요",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100036319.1.10",
          "form": "사전예약 하신거 맞나요?",
          "original_form": "사전예약 하신거 맞나요?",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100036319.1.11",
          "form": "네 10부제때 신청했어요",
          "original_form": "네 10부제때 신청했어요",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100036319.1.12",
          "form": "글쿤요.. 요즘은 모더나가 좋다는 얘기가 많이 돌아서 좀 아쉽더라고요",
          "original_form": "글쿤요.. 요즘은 모더나가 좋다는 얘기가 많이 돌아서 좀 아쉽더라고요",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100036319.1.13",
          "form": "저는 요즘 모더나가 물량얘기나와서 당연히 모더나일줄 알았는데, 놀랐었어요",
          "original_form": "저는 요즘 모더나가 물량얘기나와서 당연히 모더나일줄 알았는데, 놀랐었어요",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100036319.1.14",
          "form": "어떻게 생각하면 지금까지 모더나 맞은 분들보다 화이자 맞은 분이 훨씬 많아서,\r\n모더나도 앞으로 맞는 분들이 늘어나면 또 다른 얘기가 나오지 않을까 싶기도해요",
          "original_form": "어떻게 생각하면 지금까지 모더나 맞은 분들보다 화이자 맞은 분이 훨씬 많아서,\r\n모더나도 앞으로 맞는 분들이 늘어나면 또 다른 얘기가 나오지 않을까 싶기도해요",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100036319.1.15",
          "form": "오 그렇게 생각할수도 있겠네요",
          "original_form": "오 그렇게 생각할수도 있겠네요",
          "speaker_id": "1",
          "time": "20210830 22:22"
        }
      ]
