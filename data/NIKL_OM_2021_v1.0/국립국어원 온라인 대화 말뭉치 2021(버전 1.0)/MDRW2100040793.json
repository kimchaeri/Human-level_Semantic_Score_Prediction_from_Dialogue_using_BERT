      "utterance": [
        {
          "id": "MDRW2100040793.1.1",
          "form": "한마리 인거죠??",
          "original_form": "한마리 인거죠??",
          "speaker_id": "1",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100040793.1.2",
          "form": "토끼키우는거 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "토끼키우는거 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100040793.1.3",
          "form": "네~햄스터는 한마리만 키우라고 하더라구요~",
          "original_form": "네~햄스터는 한마리만 키우라고 하더라구요~",
          "speaker_id": "2",
          "time": "20210905 07:21"
        },
        {
          "id": "MDRW2100040793.1.4",
          "form": "네 그렇게 들었어요",
          "original_form": "네 그렇게 들었어요",
          "speaker_id": "1",
          "time": "20210905 07:33"
        },
        {
          "id": "MDRW2100040793.1.5",
          "form": "햄스터도 만만치 않군요",
          "original_form": "햄스터도 만만치 않군요",
          "speaker_id": "1",
          "time": "20210905 07:33"
        },
        {
          "id": "MDRW2100040793.1.6",
          "form": "뭐가 됐든 생명 키우는 건 참 정성이 들어가야 하는 듯 해요",
          "original_form": "뭐가 됐든 생명 키우는 건 참 정성이 들어가야 하는 듯 해요",
          "speaker_id": "1",
          "time": "20210905 07:33"
        },
        {
          "id": "MDRW2100040793.1.7",
          "form": "맞아요~오래오래 살길.바랄뿐이예요~",
          "original_form": "맞아요~오래오래 살길.바랄뿐이예요~",
          "speaker_id": "2",
          "time": "20210905 07:34"
        },
        {
          "id": "MDRW2100040793.1.8",
          "form": "헤어짐은 슬프니까요;ㅠ",
          "original_form": "헤어짐은 슬프니까요;ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:34"
        },
        {
          "id": "MDRW2100040793.1.9",
          "form": "제가 그래서 포유류쪽은 이제 못키워요",
          "original_form": "제가 그래서 포유류쪽은 이제 못키워요",
          "speaker_id": "1",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100040793.1.10",
          "form": "정이 더 많이  가더라구요..",
          "original_form": "정이 더 많이  가더라구요..",
          "speaker_id": "1",
          "time": "20210905 07:35"
        },
        {
          "id": "MDRW2100040793.1.11",
          "form": "맞아요~항상 엄마는 안된다고 하는데 막상 데려오면",
          "original_form": "맞아요~항상 엄마는 안된다고 하는데 막상 데려오면",
          "speaker_id": "2",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100040793.1.12",
          "form": "더 정들어서",
          "original_form": "더 정들어서",
          "speaker_id": "2",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100040793.1.13",
          "form": "보내고나면 울고 그러잖아요ㅠ",
          "original_form": "보내고나면 울고 그러잖아요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:37"
        },
        {
          "id": "MDRW2100040793.1.14",
          "form": "왜냐면 엄마가 주로 돌보니까요",
          "original_form": "왜냐면 엄마가 주로 돌보니까요",
          "speaker_id": "1",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100040793.1.15",
          "form": "정이 가장 많이 들죠",
          "original_form": "정이 가장 많이 들죠",
          "speaker_id": "1",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100040793.1.16",
          "form": "손이 가는 만큼 애정이 커지는 듯 해요",
          "original_form": "손이 가는 만큼 애정이 커지는 듯 해요",
          "speaker_id": "1",
          "time": "20210905 07:38"
        },
        {
          "id": "MDRW2100040793.1.17",
          "form": "맞아요~애들이 보는건 잠깐 뿐ㅋㅋ",
          "original_form": "맞아요~애들이 보는건 잠깐 뿐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:39"
        },
        {
          "id": "MDRW2100040793.1.18",
          "form": "제일 오래사는 거북이도 키울까 했는데 ㅋㅋㅋㅋ",
          "original_form": "제일 오래사는 거북이도 키울까 했는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:40"
        },
        {
          "id": "MDRW2100040793.1.19",
          "form": "거북이도 잘 골라야지 엄청 커지는 거북이가 있더라구요",
          "original_form": "거북이도 잘 골라야지 엄청 커지는 거북이가 있더라구요",
          "speaker_id": "1",
          "time": "20210905 07:40"
        },
        {
          "id": "MDRW2100040793.1.20",
          "form": "그얘기 듣고 포기했어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그얘기 듣고 포기했어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:41"
        },
        {
          "id": "MDRW2100040793.1.21",
          "form": "진짜 커진다고하더라구요~",
          "original_form": "진짜 커진다고하더라구요~",
          "speaker_id": "2",
          "time": "20210905 07:41"
        },
        {
          "id": "MDRW2100040793.1.22",
          "form": "어떤  사진을 봤는데 알아서 냉장고 문도 열고 찾아먹더라구요",
          "original_form": "어떤  사진을 봤는데 알아서 냉장고 문도 열고 찾아먹더라구요",
          "speaker_id": "1",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100040793.1.23",
          "form": "아이들 올라탈 수 있을 정도로 크던데 이름은 생각이 안나네요",
          "original_form": "아이들 올라탈 수 있을 정도로 크던데 이름은 생각이 안나네요",
          "speaker_id": "1",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100040793.1.24",
          "form": "맞아요 ㅋㅋㅋㅋ외출하고오면 집이 초토화 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ외출하고오면 집이 초토화 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100040793.1.25",
          "form": "그래서 저수지에 풀어주는 사람들이 그렇게 많데요ㅠ",
          "original_form": "그래서 저수지에 풀어주는 사람들이 그렇게 많데요ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100040793.1.26",
          "form": "아이고.. 그러면 생태계에 안좋은데",
          "original_form": "아이고.. 그러면 생태계에 안좋은데",
          "speaker_id": "1",
          "time": "20210905 07:45"
        }
      ]
    }
