      "utterance": [
        {
          "id": "MDRW2100034490.1.1",
          "form": "싫어하는 계절도 있으세요?",
          "original_form": "싫어하는 계절도 있으세요?",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100034490.1.2",
          "form": "겨울이요",
          "original_form": "겨울이요",
          "speaker_id": "2",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100034490.1.3",
          "form": "너무 춥죠? ㅠㅠ",
          "original_form": "너무 춥죠? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100034490.1.4",
          "form": "너무 추운데 밖에 있음 서글퍼요ㅜ",
          "original_form": "너무 추운데 밖에 있음 서글퍼요ㅜ",
          "speaker_id": "2",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100034490.1.5",
          "form": "서글프기까짘ㅋㅋㅋ",
          "original_form": "서글프기까짘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100034490.1.6",
          "form": "전 추위 더위를 둘다 너무 타서",
          "original_form": "전 추위 더위를 둘다 너무 타서",
          "speaker_id": "1",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100034490.1.7",
          "form": "여름겨울 정하기 어려ㅇㄴ거같아요",
          "original_form": "여름겨울 정하기 어려ㅇㄴ거같아요",
          "speaker_id": "1",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100034490.1.8",
          "form": "저도 사실",
          "original_form": "저도 사실",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034490.1.9",
          "form": "여름에 너무 덥고 땀나는 것도 싫어요ㅋㅋ",
          "original_form": "여름에 너무 덥고 땀나는 것도 싫어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034490.1.10",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034490.1.11",
          "form": "전 더운건 견디겠는데 땀이 넘 싫어요",
          "original_form": "전 더운건 견디겠는데 땀이 넘 싫어요",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034490.1.12",
          "form": "습도 높은 것 힘들어요ㅜ",
          "original_form": "습도 높은 것 힘들어요ㅜ",
          "speaker_id": "2",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100034490.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100034490.1.14",
          "form": "끈적 축축...",
          "original_form": "끈적 축축...",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100034490.1.15",
          "form": "제습기트니 좀 낫더라구요",
          "original_form": "제습기트니 좀 낫더라구요",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100034490.1.16",
          "form": "네 요즘 제습은 필수죠",
          "original_form": "네 요즘 제습은 필수죠",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100034490.1.17",
          "form": "요샌 공청기 기능도 같이있더라고요",
          "original_form": "요샌 공청기 기능도 같이있더라고요",
          "speaker_id": "1",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034490.1.18",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034490.1.19",
          "form": "미세먼지나 이런 것도 심하니까",
          "original_form": "미세먼지나 이런 것도 심하니까",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034490.1.20",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:32"
        }
