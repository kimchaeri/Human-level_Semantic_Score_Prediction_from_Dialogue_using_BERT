      "utterance": [
        {
          "id": "MDRW2100033461.1.1",
          "form": "동물 좋아하시나요?",
          "original_form": "동물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100033461.1.2",
          "form": "실버푸들 진짜 귀엽더라고요",
          "original_form": "실버푸들 진짜 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100033461.1.3",
          "form": "방금 찾아보고 왔는데 귀욥네요~ 비숑도 귀엽던데요",
          "original_form": "방금 찾아보고 왔는데 귀욥네요~ 비숑도 귀엽던데요",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100033461.1.4",
          "form": "푸들이 또 애교도 많다고하고요",
          "original_form": "푸들이 또 애교도 많다고하고요",
          "speaker_id": "2",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100033461.1.5",
          "form": "예전에 제 친구가 푸들 키웠었거든요. 생각 나네요^^",
          "original_form": "예전에 제 친구가 푸들 키웠었거든요. 생각 나네요^^",
          "speaker_id": "1",
          "time": "20210824 08:07"
        },
        {
          "id": "MDRW2100033461.1.6",
          "form": "마음은 대형견도 키우고싶지만",
          "original_form": "마음은 대형견도 키우고싶지만",
          "speaker_id": "2",
          "time": "20210824 08:09"
        },
        {
          "id": "MDRW2100033461.1.7",
          "form": "소형견이여야 감당이될거같아요 ㅋㅋ",
          "original_form": "소형견이여야 감당이될거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:09"
        },
        {
          "id": "MDRW2100033461.1.8",
          "form": "전 대형견은 좀 무섭거든요~",
          "original_form": "전 대형견은 좀 무섭거든요~",
          "speaker_id": "1",
          "time": "20210824 08:13"
        },
        {
          "id": "MDRW2100033461.1.9",
          "form": "음 사실 저도 ㅋㅋ 개가 좀 무섭기도해서",
          "original_form": "음 사실 저도 ㅋㅋ 개가 좀 무섭기도해서",
          "speaker_id": "2",
          "time": "20210824 08:22"
        },
        {
          "id": "MDRW2100033461.1.10",
          "form": "사진을 열심히봐요 ㅋㅋ",
          "original_form": "사진을 열심히봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:22"
        },
        {
          "id": "MDRW2100033461.1.11",
          "form": "ㅋㅋㅋ 저도 귀엽긴 하지만 살짝 무서워요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 저도 귀엽긴 하지만 살짝 무서워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 08:43"
        },
        {
          "id": "MDRW2100033461.1.12",
          "form": "근데 누가 실버푸들 산책시키는거 봤는데 너무 귀여운 거에요 ㅋㅋ",
          "original_form": "근데 누가 실버푸들 산책시키는거 봤는데 너무 귀여운 거에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:49"
        },
        {
          "id": "MDRW2100033461.1.13",
          "form": "쌩판남인 주인한테 이건 무슨 종이냐고 물어봤어요 ㅋㅋ",
          "original_form": "쌩판남인 주인한테 이건 무슨 종이냐고 물어봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 08:58"
        },
        {
          "id": "MDRW2100033461.1.14",
          "form": "아~ 그래서 알게 되셨군요~ 그정도로 귀여워 하시면 한마리 분양 받으셔도 되겠는데요 ㅎ",
          "original_form": "아~ 그래서 알게 되셨군요~ 그정도로 귀여워 하시면 한마리 분양 받으셔도 되겠는데요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 09:07"
        },
        {
          "id": "MDRW2100033461.1.15",
          "form": "작더라고요 다커도 삼키로도 안나간대요",
          "original_form": "작더라고요 다커도 삼키로도 안나간대요",
          "speaker_id": "2",
          "time": "20210824 09:10"
        },
        {
          "id": "MDRW2100033461.1.16",
          "form": "그정도면 키울만 하겠는데요 ㅎ",
          "original_form": "그정도면 키울만 하겠는데요 ㅎ",
