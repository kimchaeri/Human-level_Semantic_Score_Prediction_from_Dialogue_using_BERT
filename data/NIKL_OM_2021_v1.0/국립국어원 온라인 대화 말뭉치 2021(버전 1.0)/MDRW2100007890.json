      "utterance": [
        {
          "id": "MDRW2100007890.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007890.1.2",
          "form": "이따가요ㅎㅎ",
          "original_form": "이따가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100007890.1.3",
          "form": "저는 청소담당이요",
          "original_form": "저는 청소담당이요",
          "speaker_id": "1",
          "time": "20210822 12:17"
        },
        {
          "id": "MDRW2100007890.1.4",
          "form": "무슨 청소하시나쇼",
          "original_form": "무슨 청소하시나쇼",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007890.1.5",
          "form": "화장실ㅋ",
          "original_form": "화장실ㅋ",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007890.1.6",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007890.1.7",
          "form": "ㅋㅋ어후 힘들겠어요",
          "original_form": "ㅋㅋ어후 힘들겠어요",
          "speaker_id": "2",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007890.1.8",
          "form": "혹시나 곰팡이 생길까봐 자주 해요",
          "original_form": "혹시나 곰팡이 생길까봐 자주 해요",
          "speaker_id": "1",
          "time": "20210822 12:20"
        },
        {
          "id": "MDRW2100007890.1.9",
          "form": "부지런하시네요",
          "original_form": "부지런하시네요",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007890.1.10",
          "form": "곰팡이가 호흡기에 안좋아서 코로나도 글코",
          "original_form": "곰팡이가 호흡기에 안좋아서 코로나도 글코",
          "speaker_id": "1",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007890.1.11",
          "form": "위생이",
          "original_form": "위생이",
          "speaker_id": "1",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007890.1.12",
          "form": "그렇죠! 저도 해야겠어요",
          "original_form": "그렇죠! 저도 해야겠어요",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007890.1.13",
          "form": "건강이 최고죠",
          "original_form": "건강이 최고죠",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007890.1.14",
          "form": "맞아요 바닥 쓸어야지",
          "original_form": "맞아요 바닥 쓸어야지",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007890.1.15",
          "form": "먼지도 걱정",
          "original_form": "먼지도 걱정",
