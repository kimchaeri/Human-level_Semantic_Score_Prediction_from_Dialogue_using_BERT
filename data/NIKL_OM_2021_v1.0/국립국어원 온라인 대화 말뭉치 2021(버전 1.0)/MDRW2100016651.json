      "utterance": [
        {
          "id": "MDRW2100016651.1.1",
          "form": "빵은 어떤 종류 좋아하세요?",
          "original_form": "빵은 어떤 종류 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016651.1.2",
          "form": "빵은 뭐 좋아하세요?",
          "original_form": "빵은 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016651.1.3",
          "form": "아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016651.1.4",
          "form": "저는 빵 다 좋아해요...버터프레첼? 크로와상 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저는 빵 다 좋아해요...버터프레첼? 크로와상 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016651.1.5",
          "form": "ㅋㅋㅋㅋㅋ통햇네요",
          "original_form": "ㅋㅋㅋㅋㅋ통햇네요",
          "speaker_id": "2",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016651.1.6",
          "form": "빵좋아하는걸로 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "빵좋아하는걸로 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016651.1.7",
          "form": "저도 빵 다 좋아해요",
          "original_form": "저도 빵 다 좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016651.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 버터 특히좋아해서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 버터 특히좋아해서",
          "speaker_id": "1",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016651.1.9",
          "form": "식빵에 버터 발라먹는거 좋으해요 ㅠㅠ",
          "original_form": "식빵에 버터 발라먹는거 좋으해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016651.1.10",
          "form": "버터 너무 맛잇죠 ㅠㅠ",
          "original_form": "버터 너무 맛잇죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016651.1.11",
          "form": "버터도 좋아하고 땅콩버터도 좋아해요ㅠㅠ",
          "original_form": "버터도 좋아하고 땅콩버터도 좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016651.1.12",
          "form": "맞아요 넘나 맛이성요...",
          "original_form": "맞아요 넘나 맛이성요...",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016651.1.13",
          "form": "카야잼 드셔보셧어요?",
          "original_form": "카야잼 드셔보셧어요?",
          "speaker_id": "1",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100016651.1.14",
          "form": "네 카야잼도 너무 맛잇죠",
          "original_form": "네 카야잼도 너무 맛잇죠",
          "speaker_id": "2",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016651.1.15",
          "form": "사실 잼은 그렇게 좋아하는 편이아닌데",
          "original_form": "사실 잼은 그렇게 좋아하는 편이아닌데",
          "speaker_id": "2",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016651.1.16",
          "form": "카야잼은 너무맛잇더라구요",
          "original_form": "카야잼은 너무맛잇더라구요",
          "speaker_id": "2",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016651.1.17",
          "form": "그쵸...버터덩어리랑 같이 먹으면 아휴",
          "original_form": "그쵸...버터덩어리랑 같이 먹으면 아휴",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100016651.1.18",
          "form": "이즈니버터 맛잇더라구요 추천드립이다!",
          "original_form": "이즈니버터 맛잇더라구요 추천드립이다!",
          "speaker_id": "1",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100016651.1.19",
          "form": "저도 이즈니버터 좋아해요",
          "original_form": "저도 이즈니버터 좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016651.1.20",
          "form": "너무 맛잇음 ㅠㅠ",
          "original_form": "너무 맛잇음 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016651.1.21",
          "form": "그쵸 ㅋㅋㅋㅋ얼마전에 첨먹었는데 계속 땡겨요 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋ얼마전에 첨먹었는데 계속 땡겨요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016651.1.22",
          "form": "식빵없어서 어제 파바갔다왓어요",
          "original_form": "식빵없어서 어제 파바갔다왓어요",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016651.1.23",
          "form": "집에 식빵 없으면 금단현상생겨요 ㅋㅋㅋㅋㅋ",
          "original_form": "집에 식빵 없으면 금단현상생겨요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016651.1.24",
          "form": "전 바게뜨도 좋아해요",
          "original_form": "전 바게뜨도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016651.1.25",
          "form": "아 저두요 ㅋㅋㅋㅋ파바 바게트랑 생크림 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "아 저두요 ㅋㅋㅋㅋ파바 바게트랑 생크림 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016651.1.26",
          "form": "지금 요기요 파바 할인하는데 이용하셧어요?",
          "original_form": "지금 요기요 파바 할인하는데 이용하셧어요?",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016651.1.27",
          "form": "네 놓칠수없죠 ㅋㅋㅋㅋㅋ",
          "original_form": "네 놓칠수없죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016651.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ역시 다녀오셧군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ역시 다녀오셧군요",
          "speaker_id": "1",
          "time": "20210826 15:49"
        }
      ]
