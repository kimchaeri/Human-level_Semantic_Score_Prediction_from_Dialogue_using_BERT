      "utterance": [
        {
          "id": "MDRW2100023749.1.1",
          "form": "혹시 좋아하시는 음식 있으신가요~?",
          "original_form": "혹시 좋아하시는 음식 있으신가요~?",
          "speaker_id": "1",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100023749.1.2",
          "form": "제가 며칠 입원하느라 답이 늦었네요",
          "original_form": "제가 며칠 입원하느라 답이 늦었네요",
          "speaker_id": "2",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100023749.1.3",
          "form": "생선요리 좋아합니다",
          "original_form": "생선요리 좋아합니다",
          "speaker_id": "2",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100023749.1.4",
          "form": "아하 그렇군요 ㅠㅠ",
          "original_form": "아하 그렇군요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100023749.1.5",
          "form": "생선요리하면",
          "original_form": "생선요리하면",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100023749.1.6",
          "form": "회나 매운탕? 좋아하시겠네요",
          "original_form": "회나 매운탕? 좋아하시겠네요",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100023749.1.7",
          "form": "다 좋아합니다..",
          "original_form": "다 좋아합니다..",
          "speaker_id": "2",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100023749.1.8",
          "form": "생선구이 생선조림도 좋아하고",
          "original_form": "생선구이 생선조림도 좋아하고",
          "speaker_id": "2",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100023749.1.9",
          "form": "회도 좋고 탕도 좋고",
          "original_form": "회도 좋고 탕도 좋고",
          "speaker_id": "2",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100023749.1.10",
          "form": "오.. 대박이네요",
          "original_form": "오.. 대박이네요",
          "speaker_id": "1",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100023749.1.11",
          "form": "갠적으로 연어회 엄청 좋아해서 ㅠㅠ",
          "original_form": "갠적으로 연어회 엄청 좋아해서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100023749.1.12",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100023749.1.13",
          "form": "노르웨이 여행갔을때 매끼 연어가 나오더군요",
          "original_form": "노르웨이 여행갔을때 매끼 연어가 나오더군요",
          "speaker_id": "2",
          "time": "20210903 14:59"
        },
        {
          "id": "MDRW2100023749.1.14",
          "form": "정말 그렇게 실컷 연어를 먹었던 적이 없었네요",
          "original_form": "정말 그렇게 실컷 연어를 먹었던 적이 없었네요",
          "speaker_id": "2",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100023749.1.15",
          "form": "노르웨이연어..ㅜㅜ 먹고싶네요",
          "original_form": "노르웨이연어..ㅜㅜ 먹고싶네요",
          "speaker_id": "1",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100023749.1.16",
          "form": "노르웨에가 연어 주 생산지잖아요",
          "original_form": "노르웨에가 연어 주 생산지잖아요",
          "speaker_id": "2",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100023749.1.17",
          "form": "거의 한국 밥상에 김치 올라오듯이",
          "original_form": "거의 한국 밥상에 김치 올라오듯이",
          "speaker_id": "2",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100023749.1.18",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100023749.1.19",
          "form": "매끼 연어요리가 나오더군요",
          "original_form": "매끼 연어요리가 나오더군요",
          "speaker_id": "2",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100023749.1.20",
          "form": "오 좋네요!",
          "original_form": "오 좋네요!",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100023749.1.21",
          "form": "연어훈제도 나오나요 ㄷㄷ",
          "original_form": "연어훈제도 나오나요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100023749.1.22",
          "form": "그랬던 거 같네요",
          "original_form": "그랬던 거 같네요",
          "speaker_id": "2",
          "time": "20210904 05:21"
        },
        {
          "id": "MDRW2100023749.1.23",
          "form": "연어를 활용한 요리를이 다양했었지요",
          "original_form": "연어를 활용한 요리를이 다양했었지요",
          "speaker_id": "2",
          "time": "20210904 05:21"
        },
        {
          "id": "MDRW2100023749.1.24",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210904 12:15"
        }
      ]
