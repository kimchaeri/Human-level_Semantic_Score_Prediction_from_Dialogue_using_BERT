      "utterance": [
        {
          "id": "MDRW2100013317.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013317.1.2",
          "form": "커피 좋아하시나요?",
          "original_form": "커피 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013317.1.3",
          "form": "하루에 한잔씩 마세요~",
          "original_form": "하루에 한잔씩 마세요~",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013317.1.4",
          "form": "저도 좋아해서 꼭 마시네요 ㅎㅎ",
          "original_form": "저도 좋아해서 꼭 마시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013317.1.5",
          "form": "어디 커피 제일 좋아하시나요",
          "original_form": "어디 커피 제일 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013317.1.6",
          "form": "스벅도 좋지만 텐퍼센트도 괜찮고 ㅋ",
          "original_form": "스벅도 좋지만 텐퍼센트도 괜찮고 ㅋ",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013317.1.7",
          "form": "텐퍼센트는 첨 들어봤어요 오 프랜차이즈인가요?",
          "original_form": "텐퍼센트는 첨 들어봤어요 오 프랜차이즈인가요?",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013317.1.8",
          "form": "아 여기 부산인데 부산에는 많아요. 프랜차이즈예요",
          "original_form": "아 여기 부산인데 부산에는 많아요. 프랜차이즈예요",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013317.1.9",
          "form": "아 그렇군요!! 찾아보니까 저희 동네에도 하나 있네요!",
          "original_form": "아 그렇군요!! 찾아보니까 저희 동네에도 하나 있네요!",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013317.1.10",
          "form": "맛있는 메뉴 있나요?",
          "original_form": "맛있는 메뉴 있나요?",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013317.1.11",
          "form": "뭐 저는 라떼나 비엔나커피 요런거 전 좋아해요",
          "original_form": "뭐 저는 라떼나 비엔나커피 요런거 전 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013317.1.12",
          "form": "비엔나커피도 파는군요 크림 들어가있나요?",
          "original_form": "비엔나커피도 파는군요 크림 들어가있나요?",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013317.1.13",
          "form": "비엔나는 없나 기억 안나네. 이건 제가 좋아해서ㅋ",
          "original_form": "비엔나는 없나 기억 안나네. 이건 제가 좋아해서ㅋ",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013317.1.14",
          "form": "네 ㅎㅎ 배달음식도 자주 드시나요?",
          "original_form": "네 ㅎㅎ 배달음식도 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013317.1.15",
          "form": "전 배달은 잘 안 시켜요",
          "original_form": "전 배달은 잘 안 시켜요",
          "speaker_id": "2",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013317.1.16",
          "form": "아 그러시구나 저도 자주는 안 시키고 치킨만 가끔 시키네요 ㅎㅎ",
          "original_form": "아 그러시구나 저도 자주는 안 시키고 치킨만 가끔 시키네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013317.1.17",
          "form": "좋아하는 음식이 어떻게 되세요",
          "original_form": "좋아하는 음식이 어떻게 되세요",
          "speaker_id": "1",
          "time": "20210825 14:16"
        }
      ]
    }
