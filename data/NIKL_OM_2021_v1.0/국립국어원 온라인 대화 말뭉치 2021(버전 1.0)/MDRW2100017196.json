        {
          "id": "MDRW2100017196.1.1",
          "form": "여름휴가 어디가셨어요?",
          "original_form": "여름휴가 어디가셨어요?",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100017196.1.2",
          "form": "코로나라 못갔네요이번엔ㅠ",
          "original_form": "코로나라 못갔네요이번엔ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100017196.1.3",
          "form": "가까운 국내도요? 흑흑 저희도 캠핑만..",
          "original_form": "가까운 국내도요? 흑흑 저희도 캠핑만..",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100017196.1.4",
          "form": "외국어디가보셨나요",
          "original_form": "외국어디가보셨나요",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100017196.1.5",
          "form": "주로 가까운곳들만 가봤어요 . 일본 필리핀 괌 사이판 대만 뭐이런... 아 신행은 멀리 미국 멕시코 칸쿤요",
          "original_form": "주로 가까운곳들만 가봤어요 . 일본 필리핀 괌 사이판 대만 뭐이런... 아 신행은 멀리 미국 멕시코 칸쿤요",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100017196.1.6",
          "form": "오저도 일본가봤어요",
          "original_form": "오저도 일본가봤어요",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100017196.1.7",
          "form": "일본 어디요?",
          "original_form": "일본 어디요?",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100017196.1.8",
          "form": "오사카 후쿠오카요ㅎㅎ",
          "original_form": "오사카 후쿠오카요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100017196.1.9",
          "form": "저는 오사카 도쿄 오키나와요",
          "original_form": "저는 오사카 도쿄 오키나와요",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100017196.1.10",
          "form": "오사카 참 좋았는데",
          "original_form": "오사카 참 좋았는데",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100017196.1.11",
          "form": "오사카맞아요ㅠㅠ 도코도좋나요",
          "original_form": "오사카맞아요ㅠㅠ 도코도좋나요",
          "speaker_id": "1",
          "time": "20210828 17:13"
        },
        {
          "id": "MDRW2100017196.1.12",
          "form": "도쿄는 그냥 도시 느낌이 쇼핑 재미엥ㅅ",
          "original_form": "도쿄는 그냥 도시 느낌이 쇼핑 재미엥ㅅ",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100017196.1.13",
          "form": "에요",
          "original_form": "에요",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100017196.1.14",
          "form": "좀 삭막할것같네요",
          "original_form": "좀 삭막할것같네요",
