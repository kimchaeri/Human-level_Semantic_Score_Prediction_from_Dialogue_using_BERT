      "utterance": [
        {
          "id": "MDRW2100022039.1.1",
          "form": "서울에는 없어",
          "original_form": "서울에는 없어",
          "speaker_id": "2",
          "time": "20210829 01:45"
        },
        {
          "id": "MDRW2100022039.1.2",
          "form": "슬퍼ㅜ",
          "original_form": "슬퍼ㅜ",
          "speaker_id": "2",
          "time": "20210829 01:45"
        },
        {
          "id": "MDRW2100022039.1.3",
          "form": "찾아보니까 봄봄 서울에도 있어!",
          "original_form": "찾아보니까 봄봄 서울에도 있어!",
          "speaker_id": "1",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022039.1.4",
          "form": "잠실 야구장에도 있고",
          "original_form": "잠실 야구장에도 있고",
          "speaker_id": "1",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022039.1.5",
          "form": "뚝섬역에 봄봄 있대!",
          "original_form": "뚝섬역에 봄봄 있대!",
          "speaker_id": "1",
          "time": "20210829 02:03"
        },
        {
          "id": "MDRW2100022039.1.6",
          "form": "근데 왜 배민에는 안뜨는걸까ㅜㅜ",
          "original_form": "근데 왜 배민에는 안뜨는걸까ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022039.1.7",
          "form": "시켜먹을수가없네ㅜㅜ",
          "original_form": "시켜먹을수가없네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022039.1.8",
          "form": "나도 봄봄 너무 가고 싶은데 배달비가 더 들어서 포기했어ㅠㅠ",
          "original_form": "나도 봄봄 너무 가고 싶은데 배달비가 더 들어서 포기했어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100022039.1.9",
          "form": "내일 뭐 먹을까?",
          "original_form": "내일 뭐 먹을까?",
          "speaker_id": "1",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022039.1.10",
          "form": "메뉴 추천 받아요",
          "original_form": "메뉴 추천 받아요",
          "speaker_id": "1",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100022039.1.11",
          "form": "죽 어때",
          "original_form": "죽 어때",
          "speaker_id": "2",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100022039.1.12",
          "form": "죽 있었지만 떡볶이 먹기로 했어 ㅎㅎ",
          "original_form": "죽 있었지만 떡볶이 먹기로 했어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100022039.1.13",
          "form": "점심에 뭐 먹었어?",
          "original_form": "점심에 뭐 먹었어?",
          "speaker_id": "1",
          "time": "20210829 12:59"
        },
        {
          "id": "MDRW2100022039.1.14",
          "form": "안먹었어ㅎ",
          "original_form": "안먹었어ㅎ",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022039.1.15",
          "form": "지금 배아파서,,,,",
          "original_form": "지금 배아파서,,,,",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022039.1.16",
          "form": "몇십분째,,,ㅎ",
          "original_form": "몇십분째,,,ㅎ",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022039.1.17",
          "form": "그럼 너가 죽 먹어야겠다",
          "original_form": "그럼 너가 죽 먹어야겠다",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022039.1.18",
          "form": "소고기죽 챙겨먹어",
          "original_form": "소고기죽 챙겨먹어",
          "speaker_id": "1",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022039.1.19",
          "form": "사실 그래서 죽말한고야",
          "original_form": "사실 그래서 죽말한고야",
          "speaker_id": "2",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022039.1.20",
          "form": "군데 죽이 없어",
          "original_form": "군데 죽이 없어",
          "speaker_id": "2",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022039.1.21",
          "form": "그냥 하루종일 굶을까바",
          "original_form": "그냥 하루종일 굶을까바",
          "speaker_id": "2",
          "time": "20210829 13:11"
        },
        {
          "id": "MDRW2100022039.1.22",
          "form": "그러다 위 아프다...",
          "original_form": "그러다 위 아프다...",
          "speaker_id": "1",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022039.1.23",
          "form": "주변에 본죽없어?",
          "original_form": "주변에 본죽없어?",
          "speaker_id": "1",
          "time": "20210829 13:12"
        },
        {
          "id": "MDRW2100022039.1.24",
          "form": "몰겟는데",
          "original_form": "몰겟는데",
          "speaker_id": "2",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022039.1.25",
          "form": "나가기귀차나",
          "original_form": "나가기귀차나",
          "speaker_id": "2",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022039.1.26",
          "form": "배달이라도 시켜먹어",
          "original_form": "배달이라도 시켜먹어",
          "speaker_id": "1",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022039.1.27",
          "form": "돈이,,,  없어,,,,,",
          "original_form": "돈이,,,  없어,,,,,",
          "speaker_id": "2",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022039.1.28",
          "form": "월급 언제들어오냐",
          "original_form": "월급 언제들어오냐",
          "speaker_id": "2",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022039.1.29",
          "form": "주변에 본죽 뜨나",
          "original_form": "주변에 본죽 뜨나",
          "speaker_id": "1",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100022039.1.30",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210829 13:26"
        }
      ]
    }
