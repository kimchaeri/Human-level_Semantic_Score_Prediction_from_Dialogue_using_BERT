      "utterance": [
        {
          "id": "MDRW2100022054.1.1",
          "form": "ㅇㅇ톡으로",
          "original_form": "ㅇㅇ톡으로",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100022054.1.2",
          "form": "함",
          "original_form": "함",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100022054.1.3",
          "form": "그 뭐냐",
          "original_form": "그 뭐냐",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100022054.1.4",
          "form": "그 김희진 선수도 그렇고",
          "original_form": "그 김희진 선수도 그렇고",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100022054.1.5",
          "form": "근대 5종도 그렇고",
          "original_form": "근대 5종도 그렇고",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100022054.1.6",
          "form": "잘생긴 사람 진짜 많은듯;;;;",
          "original_form": "잘생긴 사람 진짜 많은듯;;;;",
          "speaker_id": "2",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100022054.1.7",
          "form": "운동을하면",
          "original_form": "운동을하면",
          "speaker_id": "1",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022054.1.8",
          "form": "우리도잘생겨진다",
          "original_form": "우리도잘생겨진다",
          "speaker_id": "1",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022054.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ운동해도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ운동해도",
          "speaker_id": "2",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022054.1.10",
          "form": "못생긴사람들 많던데?",
          "original_form": "못생긴사람들 많던데?",
          "speaker_id": "2",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022054.1.11",
          "form": "나의 마지막 희망을",
          "original_form": "나의 마지막 희망을",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022054.1.12",
          "form": "짓밟지 말아줄래?",
          "original_form": "짓밟지 말아줄래?",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022054.1.13",
          "form": "아 뭔소리야 ㅋㅋㅋㅋ",
          "original_form": "아 뭔소리야 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022054.1.14",
          "form": "우리 올림픽 선수들이 운동하는거ㅜ말하는중이엇자나",
          "original_form": "우리 올림픽 선수들이 운동하는거ㅜ말하는중이엇자나",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022054.1.15",
          "form": "운동을 하면",
          "original_form": "운동을 하면",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100022054.1.16",
          "form": "잘생겨진다에대해",
          "original_form": "잘생겨진다에대해",
          "speaker_id": "1",
          "time": "20210830 01:43"
        },
        {
          "id": "MDRW2100022054.1.17",
          "form": "얘기하구잇엇어ㅡㅡ",
          "original_form": "얘기하구잇엇어ㅡㅡ",
          "speaker_id": "1",
          "time": "20210830 01:44"
        },
        {
          "id": "MDRW2100022054.1.18",
          "form": "음...그래 알겟어ㅡㅡ흥",
          "original_form": "음...그래 알겟어ㅡㅡ흥",
          "speaker_id": "2",
          "time": "20210830 03:00"
        },
        {
          "id": "MDRW2100022054.1.19",
          "form": "암튼 난 운동을",
          "original_form": "암튼 난 운동을",
          "speaker_id": "2",
          "time": "20210830 03:00"
        },
        {
          "id": "MDRW2100022054.1.20",
          "form": "하긴 할거야",
          "original_form": "하긴 할거야",
          "speaker_id": "2",
          "time": "20210830 03:00"
        },
        {
          "id": "MDRW2100022054.1.21",
          "form": "근 미래에",
          "original_form": "근 미래에",
          "speaker_id": "2",
          "time": "20210830 03:00"
        },
        {
          "id": "MDRW2100022054.1.22",
          "form": "근 미래라는건",
          "original_form": "근 미래라는건",
          "speaker_id": "1",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100022054.1.23",
          "form": "언제를 말하는거야",
          "original_form": "언제를 말하는거야",
          "speaker_id": "1",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100022054.1.24",
          "form": "일주일?한달?",
          "original_form": "일주일?한달?",
          "speaker_id": "1",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100022054.1.25",
          "form": "코로나끝나고?",
          "original_form": "코로나끝나고?",
          "speaker_id": "1",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100022054.1.26",
          "form": "미래를 누가 어떻게 알 수 있겠어",
          "original_form": "미래를 누가 어떻게 알 수 있겠어",
          "speaker_id": "2",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.27",
          "form": "미래는 알지못해",
          "original_form": "미래는 알지못해",
          "speaker_id": "2",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.28",
          "form": "미래는 미래니까!!",
          "original_form": "미래는 미래니까!!",
          "speaker_id": "2",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.29",
          "form": "그렇게 붕뜬말하지마",
          "original_form": "그렇게 붕뜬말하지마",
          "speaker_id": "1",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.30",
          "form": "미래를 예측하려고 하는건 인간들의 어리석은 짓이야",
          "original_form": "미래를 예측하려고 하는건 인간들의 어리석은 짓이야",
          "speaker_id": "2",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.31",
          "form": "나랑운동같이해",
          "original_form": "나랑운동같이해",
          "speaker_id": "1",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.32",
          "form": "같은동네살때",
          "original_form": "같은동네살때",
          "speaker_id": "1",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.33",
          "form": "8천걸음 걷기?",
          "original_form": "8천걸음 걷기?",
          "speaker_id": "2",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.34",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.35",
          "form": "헬스장이라도같이다닐걸",
          "original_form": "헬스장이라도같이다닐걸",
          "speaker_id": "1",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.36",
          "form": "미래에 실버타운에서",
          "original_form": "미래에 실버타운에서",
          "speaker_id": "2",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.37",
          "form": "아니그거말고",
          "original_form": "아니그거말고",
          "speaker_id": "1",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.38",
          "form": "그래실버타운에서",
          "original_form": "그래실버타운에서",
          "speaker_id": "1",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.39",
          "form": "매일매일 조깅과 운동장 걷기를 하자",
          "original_form": "매일매일 조깅과 운동장 걷기를 하자",
          "speaker_id": "2",
          "time": "20210830 03:09"
        },
        {
          "id": "MDRW2100022054.1.40",
          "form": "갤럭시워치차고",
          "original_form": "갤럭시워치차고",
          "speaker_id": "1",
          "time": "20210830 03:10"
        }
      ]
