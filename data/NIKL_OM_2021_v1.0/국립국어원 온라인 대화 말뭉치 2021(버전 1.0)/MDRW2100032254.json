      "utterance": [
        {
          "id": "MDRW2100032254.1.1",
          "form": "예 먹을거다",
          "original_form": "예 먹을거다",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032254.1.2",
          "form": "오예 신난다",
          "original_form": "오예 신난다",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032254.1.3",
          "form": "메일 가시는 카페의 이름은 어떻게 되나요",
          "original_form": "메일 가시는 카페의 이름은 어떻게 되나요",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032254.1.4",
          "form": "a.bout",
          "original_form": "a.bout",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032254.1.5",
          "form": "에이바우트에요",
          "original_form": "에이바우트에요",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032254.1.6",
          "form": "오 영어군요",
          "original_form": "오 영어군요",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032254.1.7",
          "form": "어바웃을 그리 발음해요",
          "original_form": "어바웃을 그리 발음해요",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032254.1.8",
          "form": "다른 메뉴도 드셔보셨나요",
          "original_form": "다른 메뉴도 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032254.1.9",
          "form": "커다란 체인점인데",
          "original_form": "커다란 체인점인데",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032254.1.10",
          "form": "커피는 아메리카노 밖에 안 드셔 보셨나요",
          "original_form": "커피는 아메리카노 밖에 안 드셔 보셨나요",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032254.1.11",
          "form": "집 근처에 만 3개가 있어요",
          "original_form": "집 근처에 만 3개가 있어요",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032254.1.12",
          "form": "우와 체인점인 줄은 몰랐습니다",
          "original_form": "우와 체인점인 줄은 몰랐습니다",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032254.1.13",
          "form": "저는 회사 근처에 스타벅스가 세 군데가 있어요",
          "original_form": "저는 회사 근처에 스타벅스가 세 군데가 있어요",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032254.1.14",
          "form": "오전에는 커피 한 잔",
          "original_form": "오전에는 커피 한 잔",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032254.1.15",
          "form": "오후에는 빵을 하나 먹습니다",
          "original_form": "오후에는 빵을 하나 먹습니다",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032254.1.16",
          "form": "회사 건물 지하에 하나 회사 건너편건너편에도 있고 조기 조금 몇 걸음 더 걸으면 거기에도 있고",
          "original_form": "회사 건물 지하에 하나 회사 건너편건너편에도 있고 조기 조금 몇 걸음 더 걸으면 거기에도 있고",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032254.1.17",
          "form": "커피 용량이 크기 때문에 하루 종일 먹거든요",
          "original_form": "커피 용량이 크기 때문에 하루 종일 먹거든요",
          "speaker_id": "2",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032254.1.18",
          "form": "매우 맛이 있네요 여유가 느껴집니다",
          "original_form": "매우 맛이 있네요 여유가 느껴집니다",
          "speaker_id": "1",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032254.1.19",
          "form": "그래도 손님이 많나요",
          "original_form": "그래도 손님이 많나요",
          "speaker_id": "2",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032254.1.20",
          "form": "아 아로 드시나요",
          "original_form": "아 아로 드시나요",
          "speaker_id": "1",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032254.1.21",
          "form": "제주도에서 가장 큰 체인점이에요",
          "original_form": "제주도에서 가장 큰 체인점이에요",
          "speaker_id": "2",
          "time": "20210905 14:34"
        }
      ]
