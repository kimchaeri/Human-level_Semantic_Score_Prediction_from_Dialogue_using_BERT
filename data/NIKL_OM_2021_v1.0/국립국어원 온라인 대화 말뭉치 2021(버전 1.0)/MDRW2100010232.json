      "utterance": [
        {
          "id": "MDRW2100010232.1.1",
          "form": "최근 구매하신 쇼핑템 있으신가요",
          "original_form": "최근 구매하신 쇼핑템 있으신가요",
          "speaker_id": "2",
          "time": "20210823 15:53"
        },
        {
          "id": "MDRW2100010232.1.2",
          "form": "전 요새는 육아템들요ㅎ",
          "original_form": "전 요새는 육아템들요ㅎ",
          "speaker_id": "1",
          "time": "20210823 15:54"
        },
        {
          "id": "MDRW2100010232.1.3",
          "form": "어떤거 사셨나요^^",
          "original_form": "어떤거 사셨나요^^",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010232.1.4",
          "form": "욕조요ㅎ",
          "original_form": "욕조요ㅎ",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010232.1.5",
          "form": "이것저것 살게많네요",
          "original_form": "이것저것 살게많네요",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010232.1.6",
          "form": "아 혹시 출산을 앞두고 계신가요?",
          "original_form": "아 혹시 출산을 앞두고 계신가요?",
          "speaker_id": "2",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010232.1.7",
          "form": "둘째에요ㅎ담달",
          "original_form": "둘째에요ㅎ담달",
          "speaker_id": "1",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100010232.1.8",
          "form": "그러시군요^^ 첫째는 몇살인가요?",
          "original_form": "그러시군요^^ 첫째는 몇살인가요?",
          "speaker_id": "2",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010232.1.9",
          "form": "19개월요ㅎ 연년생ㅜㅜ",
          "original_form": "19개월요ㅎ 연년생ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100010232.1.10",
          "form": "와 연년생 애쓰시겠어요",
          "original_form": "와 연년생 애쓰시겠어요",
          "speaker_id": "2",
          "time": "20210823 18:57"
        },
        {
          "id": "MDRW2100010232.1.11",
          "form": "몇년은 나죽었다 생각하면 된다더라구요ㅎ",
          "original_form": "몇년은 나죽었다 생각하면 된다더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210823 18:58"
        },
        {
          "id": "MDRW2100010232.1.12",
          "form": "맞아요 저는 3살 터울인데 키울땐 힘들어도 터울 적은게 좋아보이더라구요",
          "original_form": "맞아요 저는 3살 터울인데 키울땐 힘들어도 터울 적은게 좋아보이더라구요",
          "speaker_id": "2",
          "time": "20210823 18:59"
        },
        {
          "id": "MDRW2100010232.1.13",
          "form": "애들이 몇살이세요?",
          "original_form": "애들이 몇살이세요?",
          "speaker_id": "1",
          "time": "20210823 19:11"
        },
        {
          "id": "MDRW2100010232.1.14",
          "form": "3살 6살이요~^^",
          "original_form": "3살 6살이요~^^",
          "speaker_id": "2",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010232.1.15",
          "form": "와 부럽네요!! 이젠 좀 괜찮나요?",
          "original_form": "와 부럽네요!! 이젠 좀 괜찮나요?",
          "speaker_id": "1",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010232.1.16",
          "form": "ㅋㅋㅋ 3살이..흠..ㅋㅋ 5살은 되야 좀 편해진다고 하네요! 근데 두돌 지나고는 한결 낫긴 해요^^",
          "original_form": "ㅋㅋㅋ 3살이..흠..ㅋㅋ 5살은 되야 좀 편해진다고 하네요! 근데 두돌 지나고는 한결 낫긴 해요^^",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010232.1.17",
          "form": "두돌만 기다리고 있네요ㅎ",
          "original_form": "두돌만 기다리고 있네요ㅎ",
          "speaker_id": "1",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010232.1.18",
          "form": "19개월이면 몇년생인가요?",
          "original_form": "19개월이면 몇년생인가요?",
          "speaker_id": "2",
          "time": "20210823 19:22"
        },
        {
          "id": "MDRW2100010232.1.19",
          "form": "첫째 물건은 최근 뭐사셨나요",
          "original_form": "첫째 물건은 최근 뭐사셨나요",
          "speaker_id": "2",
          "time": "20210823 19:22"
        },
        {
          "id": "MDRW2100010232.1.20",
          "form": "2ㅇ년1월이요ㅎ",
          "original_form": "2ㅇ년1월이요ㅎ",
          "speaker_id": "1",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010232.1.21",
          "form": "허그붕붑욕조요ㅎ",
          "original_form": "허그붕붑욕조요ㅎ",
          "speaker_id": "1",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010232.1.22",
          "form": "그러시군요^^ 둘째가 19년생이라 비슷해보였어요~\r\n\r\n저는 육아템으로 최근에 푸시팝? 팝잇을 샀어요",
          "original_form": "그러시군요^^ 둘째가 19년생이라 비슷해보였어요~\r\n\r\n저는 육아템으로 최근에 푸시팝? 팝잇을 샀어요",
          "speaker_id": "2",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010232.1.23",
          "form": "아직까지 슈너슬 썻었거든요^^;;;;",
          "original_form": "아직까지 슈너슬 썻었거든요^^;;;;",
          "speaker_id": "1",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010232.1.24",
          "form": "슈너글. 허그붕붕 다 국민육아템이죠!!",
          "original_form": "슈너글. 허그붕붕 다 국민육아템이죠!!",
          "speaker_id": "2",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010232.1.25",
          "form": "아ㅎ저희도잇어요ㅎ",
          "original_form": "아ㅎ저희도잇어요ㅎ",
          "speaker_id": "1",
          "time": "20210823 19:24"
        },
        {
