      "utterance": [
        {
          "id": "MDRW2100009235.1.1",
          "form": "저 지금 미우새보는데 강철부대 사람들 나오네요",
          "original_form": "저 지금 미우새보는데 강철부대 사람들 나오네요",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009235.1.2",
          "form": "강철부대 아세요?",
          "original_form": "강철부대 아세요?",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009235.1.3",
          "form": "제로스프라이트는 완전 맛없더라고요",
          "original_form": "제로스프라이트는 완전 맛없더라고요",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100009235.1.4",
          "form": "저 지금 모두의 거짓말 보고 있어요",
          "original_form": "저 지금 모두의 거짓말 보고 있어요",
          "speaker_id": "1",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100009235.1.5",
          "form": "드라마요?",
          "original_form": "드라마요?",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100009235.1.6",
          "form": "네 넷플",
          "original_form": "네 넷플",
          "speaker_id": "1",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100009235.1.7",
          "form": "이민기 이유영 나와요",
          "original_form": "이민기 이유영 나와요",
          "speaker_id": "1",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100009235.1.8",
          "form": "재밌어요?",
          "original_form": "재밌어요?",
          "speaker_id": "2",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.9",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.10",
          "form": "재미없어보여서",
          "original_form": "재미없어보여서",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.11",
          "form": "그냥 틀어놀려고 튼건대",
          "original_form": "그냥 틀어놀려고 튼건대",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.12",
          "form": "역시 별로.. ㅋㅋ",
          "original_form": "역시 별로.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.13",
          "form": "ㅋㅋ 라디오군요",
          "original_form": "ㅋㅋ 라디오군요",
          "speaker_id": "2",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.14",
          "form": "네 5화인데",
          "original_form": "네 5화인데",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.15",
          "form": "아직도 별 재미가..",
          "original_form": "아직도 별 재미가..",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.16",
          "form": "많이 보셨네요",
          "original_form": "많이 보셨네요",
          "speaker_id": "2",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.17",
          "form": "네 정말 기대없이 틀어놔서..",
          "original_form": "네 정말 기대없이 틀어놔서..",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.18",
          "form": "무슨 내용이에요?",
          "original_form": "무슨 내용이에요?",
          "speaker_id": "2",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.19",
          "form": "모르겠어요 ㅋㅋㅋ",
          "original_form": "모르겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100009235.1.20",
          "form": "뭔지..",
          "original_form": "뭔지..",
          "speaker_id": "1",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100009235.1.21",
          "form": "계속 보실거에요??",
          "original_form": "계속 보실거에요??",
          "speaker_id": "2",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100009235.1.22",
          "form": "네 일단 ㅋㅋ",
          "original_form": "네 일단 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100009235.1.23",
          "form": "보긴 하는데..",
          "original_form": "보긴 하는데..",
          "speaker_id": "1",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100009235.1.24",
          "form": "지금이라도 바꾸심이..",
          "original_form": "지금이라도 바꾸심이..",
          "speaker_id": "2",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100009235.1.25",
          "form": "ㅋㅋㅋ 그럴까봐요",
          "original_form": "ㅋㅋㅋ 그럴까봐요",
          "speaker_id": "1",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100009235.1.26",
          "form": "왜 인기없었는지 알겠눈.",
          "original_form": "왜 인기없었는지 알겠눈.",
          "speaker_id": "1",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100009235.1.27",
          "form": "ㅎㅎ 처음 들었어요",
          "original_form": "ㅎㅎ 처음 들었어요",
          "speaker_id": "2",
          "time": "20210828 02:31"
        }
      ]
    }
  ]
