      "utterance": [
        {
          "id": "MDRW2100029485.1.1",
          "form": "도라에몽이랑",
          "original_form": "도라에몽이랑",
          "speaker_id": "2",
          "time": "20210903 09:38"
        },
        {
          "id": "MDRW2100029485.1.2",
          "form": "씽크홀봤어요",
          "original_form": "씽크홀봤어요",
          "speaker_id": "2",
          "time": "20210903 09:38"
        },
        {
          "id": "MDRW2100029485.1.3",
          "form": "오 도라에몽 재밋던가 요",
          "original_form": "오 도라에몽 재밋던가 요",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100029485.1.4",
          "form": "전 재미있었어요",
          "original_form": "전 재미있었어요",
          "speaker_id": "2",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100029485.1.5",
          "form": "조끔 부끄러윰을 감수해야하지만...",
          "original_form": "조끔 부끄러윰을 감수해야하지만...",
          "speaker_id": "2",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100029485.1.6",
          "form": "왜 부끄러워써요 ㅋㅋㅋ",
          "original_form": "왜 부끄러워써요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100029485.1.7",
          "form": "아가야들이랑같이보는데",
          "original_form": "아가야들이랑같이보는데",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100029485.1.8",
          "form": "아가들이엄마한테",
          "original_form": "아가들이엄마한테",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100029485.1.9",
          "form": "엄마이누나는 혼자 바!!이러면서",
          "original_form": "엄마이누나는 혼자 바!!이러면서",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100029485.1.10",
          "form": "말하던디...",
          "original_form": "말하던디...",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100029485.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100029485.1.12",
          "form": "헐 너무해",
          "original_form": "헐 너무해",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100029485.1.13",
          "form": "너도 크면 혼자보게 될거란다",
          "original_form": "너도 크면 혼자보게 될거란다",
          "speaker_id": "1",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100029485.1.14",
          "form": "ㅋㅋㅋㅋㅋ감동도있던데 잼더라규요",
          "original_form": "ㅋㅋㅋㅋㅋ감동도있던데 잼더라규요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029485.1.15",
          "form": "싱크홀은 저도 봐써요",
          "original_form": "싱크홀은 저도 봐써요",
          "speaker_id": "1",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029485.1.16",
          "form": "ㅣ그거재미있죠??",
          "original_form": "ㅣ그거재미있죠??",
          "speaker_id": "2",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100029485.1.17",
          "form": "마자요 이광수가 너무 웃겨요",
          "original_form": "마자요 이광수가 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029485.1.18",
          "form": "예능 보는느낌",
          "original_form": "예능 보는느낌",
          "speaker_id": "1",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100029485.1.19",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100029485.1.20",
          "form": "이제 무슨 영화 보실꺼이요",
          "original_form": "이제 무슨 영화 보실꺼이요",
          "speaker_id": "1",
          "time": "20210903 10:32"
        },
        {
          "id": "MDRW2100029485.1.21",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "2",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029485.1.22",
          "form": "재미있는게안나오던데",
          "original_form": "재미있는게안나오던데",
          "speaker_id": "2",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029485.1.23",
          "form": "맨인더다크는 어떠세요",
          "original_form": "맨인더다크는 어떠세요",
          "speaker_id": "1",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029485.1.24",
          "form": "무서운건 못봐요 ㅠㅠ",
          "original_form": "무서운건 못봐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029485.1.25",
          "form": "귀막고 보기 국룰",
          "original_form": "귀막고 보기 국룰",
          "speaker_id": "1",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029485.1.26",
          "form": "혼자는안될거같아요",
          "original_form": "혼자는안될거같아요",
          "speaker_id": "2",
          "time": "20210903 12:08"
        },
        {
          "id": "MDRW2100029485.1.27",
          "form": "저도 혼자는 절대 안봐요",
          "original_form": "저도 혼자는 절대 안봐요",
          "speaker_id": "1",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100029485.1.28",
          "form": "옆에 귀신각",
          "original_form": "옆에 귀신각",
          "speaker_id": "1",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100029485.1.29",
          "form": "ㅋㅋㅋㅋㅋ헐랭 무서워요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ헐랭 무서워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100029485.1.30",
          "form": "저는 토요일에 맨인더다크2 와 샹치를 봤어요",
          "original_form": "저는 토요일에 맨인더다크2 와 샹치를 봤어요",
          "speaker_id": "1",
          "time": "20210906 08:58"
        }
      ]
