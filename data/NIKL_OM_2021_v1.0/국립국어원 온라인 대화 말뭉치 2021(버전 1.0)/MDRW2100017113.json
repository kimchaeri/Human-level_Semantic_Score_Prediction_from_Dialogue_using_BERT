        },
        {
          "id": "MDRW2100017113.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017113.1.3",
          "form": "배민등급이 무엇인가요?",
          "original_form": "배민등급이 무엇인가요?",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017113.1.4",
          "form": "배민보단 요기요를 많이이용합니다 ㅎ",
          "original_form": "배민보단 요기요를 많이이용합니다 ㅎ",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017113.1.5",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017113.1.6",
          "form": "왜죠?",
          "original_form": "왜죠?",
          "speaker_id": "1",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100017113.1.7",
          "form": "할인 많아서 쓰다니 ㅎ",
          "original_form": "할인 많아서 쓰다니 ㅎ",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017113.1.8",
          "form": "ㅋㅋㅋ 뭘주로시키시나요",
          "original_form": "ㅋㅋㅋ 뭘주로시키시나요",
          "speaker_id": "1",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100017113.1.9",
          "form": "ㅋㅋ빵도시키고 ㅋㅋ 삼겹살도 시키고 뭐니뭐니해도 ㅋㅋㅋ치킨이 압도적이에요",
          "original_form": "ㅋㅋ빵도시키고 ㅋㅋ 삼겹살도 시키고 뭐니뭐니해도 ㅋㅋㅋ치킨이 압도적이에요",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100017113.1.10",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017113.1.11",
          "form": "어디브랜드좋아하세요?",
          "original_form": "어디브랜드좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100017113.1.12",
          "form": "빵은 파바랑 뚜레할인할때 이용하고",
          "original_form": "빵은 파바랑 뚜레할인할때 이용하고",
          "speaker_id": "2",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017113.1.13",
          "form": "치킨은 매번달라요 그때그때 ㅎㅎ",
          "original_form": "치킨은 매번달라요 그때그때 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100017113.1.14",
          "form": "오 좋네요ㅋㅋㅋ",
          "original_form": "오 좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017113.1.15",
          "form": "배민이 할인이 없긴하죠",
          "original_form": "배민이 할인이 없긴하죠",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017113.1.16",
          "form": "업계1등이라 그럴까요 ㅎㅎ",
          "original_form": "업계1등이라 그럴까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017113.1.17",
          "form": "전 지방이라 요기요에없는곳도많아요 ㅜ",
          "original_form": "전 지방이라 요기요에없는곳도많아요 ㅜ",
          "speaker_id": "2",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017113.1.18",
          "form": "요기요는 아무래도",
          "original_form": "요기요는 아무래도",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017113.1.19",
          "form": "좀 적더라고요 매장이 ㅠ",
          "original_form": "좀 적더라고요 매장이 ㅠ",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017113.1.20",
          "form": "제일아쉬운부분이예요",
          "original_form": "제일아쉬운부분이예요",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017113.1.21",
          "form": "수수료가 높다하더라구요ㅠ",
          "original_form": "수수료가 높다하더라구요ㅠ",
          "speaker_id": "2",
