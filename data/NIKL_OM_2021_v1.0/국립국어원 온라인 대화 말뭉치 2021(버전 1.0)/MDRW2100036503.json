      "utterance": [
        {
          "id": "MDRW2100036503.1.1",
          "form": "지금야식드셔용?!",
          "original_form": "지금야식드셔용?!",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036503.1.2",
          "form": "넹 닭껍질튀김과 떡갈비랑 혼술하고 있어요",
          "original_form": "넹 닭껍질튀김과 떡갈비랑 혼술하고 있어요",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036503.1.3",
          "form": "2차달리십니깍!!",
          "original_form": "2차달리십니깍!!",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036503.1.4",
          "form": "ㅎㅎ스아실 회식보단 혼술이죠!!!",
          "original_form": "ㅎㅎ스아실 회식보단 혼술이죠!!!",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036503.1.5",
          "form": "내일의 일은 내일의 내가 할거예요ㅎ",
          "original_form": "내일의 일은 내일의 내가 할거예요ㅎ",
          "speaker_id": "1",
          "time": "20210831 00:10"
        },
        {
          "id": "MDRW2100036503.1.6",
          "form": "대단하신데용ㅋㅋㅋ잘일ㅇ어나셨어용?",
          "original_form": "대단하신데용ㅋㅋㅋ잘일ㅇ어나셨어용?",
          "speaker_id": "2",
          "time": "20210831 09:11"
        },
        {
          "id": "MDRW2100036503.1.7",
          "form": "죽다 살아났어요ㅜ",
          "original_form": "죽다 살아났어요ㅜ",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100036503.1.8",
          "form": "술 덜 깬 채로 야근했더니ㅜ",
          "original_form": "술 덜 깬 채로 야근했더니ㅜ",
          "speaker_id": "1",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100036503.1.9",
          "form": "저녁은 드셨겠죠",
          "original_form": "저녁은 드셨겠죠",
          "speaker_id": "1",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100036503.1.10",
          "form": "대단하십니닼ㄱㅋㄱㅋ출근하신것도기적..!!!!! \r\n넵 간단히 먹었습니다  해장은 하셨어요ㅜㅜ?",
          "original_form": "대단하십니닼ㄱㅋㄱㅋ출근하신것도기적..!!!!! \r\n넵 간단히 먹었습니다  해장은 하셨어요ㅜㅜ?",
          "speaker_id": "2",
          "time": "20210831 21:49"
        },
        {
          "id": "MDRW2100036503.1.11",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100036503.1.12",
          "form": "점심에 쌀국수로 해장했죠ㅎ",
          "original_form": "점심에 쌀국수로 해장했죠ㅎ",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100036503.1.13",
          "form": "저삼각김밥2개요ㅋㅋㅋ진짜너무귀찮아서..\r\n오쌀국수해장좋죠ㅋㅋㄲㅋ",
          "original_form": "저삼각김밥2개요ㅋㅋㅋ진짜너무귀찮아서..\r\n오쌀국수해장좋죠ㅋㅋㄲㅋ",
          "speaker_id": "2",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100036503.1.14",
          "form": "삼각김밥이요...?",
          "original_form": "삼각김밥이요...?",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100036503.1.15",
          "form": "그건 간식 아닌가요?",
          "original_form": "그건 간식 아닌가요?",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100036503.1.16",
          "form": "위한테 사과하세욧!!",
          "original_form": "위한테 사과하세욧!!",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100036503.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ악ㅋㅋㅋㅋㅈㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ악ㅋㅋㅋㅋㅈㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100036503.1.18",
          "form": "어제과음하셨잖아요!!위한테사과하세요!!!!",
          "original_form": "어제과음하셨잖아요!!위한테사과하세요!!!!",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100036503.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄲ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100036503.1.20",
          "form": "저는...이제 위를 만족시켜줄 거라구요!",
          "original_form": "저는...이제 위를 만족시켜줄 거라구요!",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100036503.1.21",
          "form": "내가 너를 살찌워주리니ㅎ",
          "original_form": "내가 너를 살찌워주리니ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100036503.1.22",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋ앗 야식가시나요",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋㅋ앗 야식가시나요",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100036503.1.23",
          "form": "피자 주문한거 집에 도착해 있네요ㅎ",
          "original_form": "피자 주문한거 집에 도착해 있네요ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100036503.1.24",
          "form": "오늘은 피맥할 거예요",
          "original_form": "오늘은 피맥할 거예요",
          "speaker_id": "1",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100036503.1.25",
          "form": "와.. 피맥까지 진짜 배우신분..\r\n아니근데 연속으로 달리셔도 되는겁니깍!!",
          "original_form": "와.. 피맥까지 진짜 배우신분..\r\n아니근데 연속으로 달리셔도 되는겁니깍!!",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100036503.1.26",
          "form": "님도 하다못해 미니컵라면이라도 드시는게 위를 향한 예의가 아닐까용ㅎㅎ",
          "original_form": "님도 하다못해 미니컵라면이라도 드시는게 위를 향한 예의가 아닐까용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100036503.1.27",
          "form": "어제는 밖에서 술먹고 집서 2차한 거고ㅎ",
          "original_form": "어제는 밖에서 술먹고 집서 2차한 거고ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100036503.1.28",
          "form": "오늘은 맑은 정신으로 홈술할거니 괜찮아요ㅎㅎ",
          "original_form": "오늘은 맑은 정신으로 홈술할거니 괜찮아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100036503.1.29",
          "form": "예의ㅋㅋㅋㅋㅋ미치겠습니닼ㅋㅋㅋㅋ\r\n맑은정신혼술이라....",
          "original_form": "예의ㅋㅋㅋㅋㅋ미치겠습니닼ㅋㅋㅋㅋ\r\n맑은정신혼술이라....",
          "speaker_id": "2",
          "time": "20210831 22:08"
        },
        {
          "id": "MDRW2100036503.1.30",
          "form": "이거그냥말을잘하시는거같은데 ...!???!!!",
          "original_form": "이거그냥말을잘하시는거같은데 ...!???!!!",
