      "utterance": [
        {
          "id": "MDRW2100016399.1.1",
          "form": "아니요 저는 안키워요 ㅠㅠ",
          "original_form": "아니요 저는 안키워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016399.1.2",
          "form": "키우고 싶은데 가족이 반대해서요 ㅠ",
          "original_form": "키우고 싶은데 가족이 반대해서요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016399.1.3",
          "form": "독립할 나이가 아니신가요?",
          "original_form": "독립할 나이가 아니신가요?",
          "speaker_id": "1",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100016399.1.4",
          "form": "그럴 나이이긴해요 ㅠ 근데 아직 취준생이라서요 ㅠㅠ",
          "original_form": "그럴 나이이긴해요 ㅠ 근데 아직 취준생이라서요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016399.1.5",
          "form": "취업하면 독립하구 고양이 키우고싶어요",
          "original_form": "취업하면 독립하구 고양이 키우고싶어요",
          "speaker_id": "2",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016399.1.6",
          "form": "고양이 키우실거면 두마리 키우세요",
          "original_form": "고양이 키우실거면 두마리 키우세요",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016399.1.7",
          "form": "관리비용도 한마리랑 크게 차이 없어오",
          "original_form": "관리비용도 한마리랑 크게 차이 없어오",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016399.1.8",
          "form": "아아 그게 고양이에게는 덜 외롭겟죠?",
          "original_form": "아아 그게 고양이에게는 덜 외롭겟죠?",
          "speaker_id": "2",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016399.1.9",
          "form": "네. 한마리는 외로워해요",
          "original_form": "네. 한마리는 외로워해요",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016399.1.10",
          "form": "그렇군요 ㅠㅠ 근데 관리비 차이 적게나면 두마리가 괜찮겟네요",
          "original_form": "그렇군요 ㅠㅠ 근데 관리비 차이 적게나면 두마리가 괜찮겟네요",
          "speaker_id": "2",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100016399.1.11",
          "form": "고양이 키울때 주의점 있나요!?",
          "original_form": "고양이 키울때 주의점 있나요!?",
          "speaker_id": "2",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100016399.1.12",
          "form": "그냥 뭐 잘 알아보고 계획 잘 세워서 키워야죠",
          "original_form": "그냥 뭐 잘 알아보고 계획 잘 세워서 키워야죠",
          "speaker_id": "1",
          "time": "20210826 13:40"
        },
        {
          "id": "MDRW2100016399.1.13",
          "form": "뭐 맞는말이네요 ㅋㅋㅋㅋ",
          "original_form": "뭐 맞는말이네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:41"
        },
        {
          "id": "MDRW2100016399.1.14",
          "form": "고양이 이름이 어떻게 되요?",
          "original_form": "고양이 이름이 어떻게 되요?",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016399.1.15",
          "form": "name3요",
          "original_form": "&name3&요",
          "speaker_id": "1",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100016399.1.16",
          "form": "아ㅋㅋㅋㅋ귀엽네요",
          "original_form": "아ㅋㅋㅋㅋ귀엽네요",
          "speaker_id": "2",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016399.1.17",
          "form": "캣타워랑 장난감 많겟네요?",
          "original_form": "캣타워랑 장난감 많겟네요?",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100016399.1.18",
          "form": "캣폴 캣타워 있고 장난감은 진짜많아요",
          "original_form": "캣폴 캣타워 있고 장난감은 진짜많아요",
          "speaker_id": "1",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016399.1.19",
          "form": "옹 그렇군요 고양이도 장난감 질려하고 그러나요?",
          "original_form": "옹 그렇군요 고양이도 장난감 질려하고 그러나요?",
          "speaker_id": "2",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016399.1.20",
          "form": "금방질려해요",
          "original_form": "금방질려해요",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016399.1.21",
          "form": "하루에 하나씩 바꿔줘야돼요",
          "original_form": "하루에 하나씩 바꿔줘야돼요",
          "speaker_id": "1",
          "time": "20210826 15:26"
        }
      ]
