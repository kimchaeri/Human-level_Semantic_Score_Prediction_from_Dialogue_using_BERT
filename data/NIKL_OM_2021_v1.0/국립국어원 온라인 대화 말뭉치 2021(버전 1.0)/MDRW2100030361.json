      "utterance": [
        {
          "id": "MDRW2100030361.1.1",
          "form": "남편이 있다는 건 어떤 기분일까요",
          "original_form": "남편이 있다는 건 어떤 기분일까요",
          "speaker_id": "2",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030361.1.2",
          "form": "나의 편인가요 남의 편인가요",
          "original_form": "나의 편인가요 남의 편인가요",
          "speaker_id": "2",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030361.1.3",
          "form": "실수로 눌렀어요",
          "original_form": "실수로 눌렀어요",
          "speaker_id": "1",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030361.1.4",
          "form": "그래도 이야기하죠",
          "original_form": "그래도 이야기하죠",
          "speaker_id": "1",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030361.1.5",
          "form": "괜찮아요 이걸로 얘기해도 돼요",
          "original_form": "괜찮아요 이걸로 얘기해도 돼요",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.6",
          "form": "메뉴를 2개 시킬 수 있어요",
          "original_form": "메뉴를 2개 시킬 수 있어요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.7",
          "form": "큰 장점이에요",
          "original_form": "큰 장점이에요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.8",
          "form": "개이득 이네요",
          "original_form": "개이득 이네요",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.9",
          "form": "절친이 생긴 거죠",
          "original_form": "절친이 생긴 거죠",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.10",
          "form": "여러 가지 맛을 맛 보다니 정말 좋네요",
          "original_form": "여러 가지 맛을 맛 보다니 정말 좋네요",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.11",
          "form": "세 개 시켜도 돼요",
          "original_form": "세 개 시켜도 돼요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.12",
          "form": "코로나로 남친을 자주 못 봐요",
          "original_form": "코로나로 남친을 자주 못 봐요",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.13",
          "form": "많이 먹거든요",
          "original_form": "많이 먹거든요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.14",
          "form": "남친 보러 갈 때 맛있는 맛집이 있는데",
          "original_form": "남친 보러 갈 때 맛있는 맛집이 있는데",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.15",
          "form": "보고 싶겠어요",
          "original_form": "보고 싶겠어요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.16",
          "form": "북촌 피만두 라고요",
          "original_form": "북촌 피만두 라고요",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.17",
          "form": "인천이라고 했나요",
          "original_form": "인천이라고 했나요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030361.1.18",
          "form": "데이트를 항상 거기에서 합니다",
          "original_form": "데이트를 항상 거기에서 합니다",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100030361.1.19",
          "form": "결국 음식 이군요",
          "original_form": "결국 음식 이군요",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100030361.1.20",
          "form": "광명역에서 만나요",
          "original_form": "광명역에서 만나요",
          "speaker_id": "2",
