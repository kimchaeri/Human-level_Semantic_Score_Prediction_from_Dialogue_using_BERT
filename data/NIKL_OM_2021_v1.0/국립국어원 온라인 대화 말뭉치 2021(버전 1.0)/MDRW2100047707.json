      "utterance": [
        {
          "id": "MDRW2100047707.1.1",
          "form": "운동 하세요?",
          "original_form": "운동 하세요?",
          "speaker_id": "1",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100047707.1.2",
          "form": "저는 수영해요!ㅎㅎ",
          "original_form": "저는 수영해요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 03:50"
        },
        {
          "id": "MDRW2100047707.1.3",
          "form": "가끔 싸이클도 탑니당",
          "original_form": "가끔 싸이클도 탑니당",
          "speaker_id": "2",
          "time": "20210828 03:51"
        },
        {
          "id": "MDRW2100047707.1.4",
          "form": "오.. 수영",
          "original_form": "오.. 수영",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100047707.1.5",
          "form": "어떤가요",
          "original_form": "어떤가요",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100047707.1.6",
          "form": "저도 배우고싶어요",
          "original_form": "저도 배우고싶어요",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100047707.1.7",
          "form": "전신운동이라 그런지 전체적으로 살이 빠지는 느낌이에요!",
          "original_form": "전신운동이라 그런지 전체적으로 살이 빠지는 느낌이에요!",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100047707.1.8",
          "form": "어깨도 넓어지고 좋아요",
          "original_form": "어깨도 넓어지고 좋아요",
          "speaker_id": "2",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100047707.1.9",
          "form": "넓어지고 싶어서",
          "original_form": "넓어지고 싶어서",
          "speaker_id": "1",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100047707.1.10",
          "form": "배우고싶네요",
          "original_form": "배우고싶네요",
          "speaker_id": "1",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100047707.1.11",
          "form": "대신 좀 오래해야해요ㅋㅋ",
          "original_form": "대신 좀 오래해야해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100047707.1.12",
          "form": "어느정도요????",
          "original_form": "어느정도요????",
          "speaker_id": "1",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100047707.1.13",
          "form": "몸이 수영으로 달라지고싶으면 최소 5년이상은 해야되용ㅋㅋㅋ",
          "original_form": "몸이 수영으로 달라지고싶으면 최소 5년이상은 해야되용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100047707.1.14",
          "form": "진짜 꾸준하게",
          "original_form": "진짜 꾸준하게",
          "speaker_id": "2",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100047707.1.15",
          "form": "허그. ㅋㅋ ㅋㅋㅋ ㅋㅋㅋ 5년이요??",
          "original_form": "허그. ㅋㅋ ㅋㅋㅋ ㅋㅋㅋ 5년이요??",
          "speaker_id": "1",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100047707.1.16",
          "form": "너무 기네여",
          "original_form": "너무 기네여",
          "speaker_id": "1",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100047707.1.17",
          "form": "ㅋㅋㅋ 중간에 쉬면 안된다고 하더라구요",
          "original_form": "ㅋㅋㅋ 중간에 쉬면 안된다고 하더라구요",
          "speaker_id": "2",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100047707.1.18",
          "form": "ㅋㅋㅋㅋㅋㅠ 어릴 땨",
          "original_form": "ㅋㅋㅋㅋㅋㅠ 어릴 땨",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100047707.1.19",
          "form": "해야하네요",
          "original_form": "해야하네요",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100047707.1.20",
          "form": "네 어릴때해야 어깨도 넓어져요",
          "original_form": "네 어릴때해야 어깨도 넓어져요",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100047707.1.21",
          "form": "성인되서하면 좀 발달이 더뎌요",
          "original_form": "성인되서하면 좀 발달이 더뎌요",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100047707.1.22",
          "form": "그럼 포기네요 ㅠ흑",
          "original_form": "그럼 포기네요 ㅠ흑",
          "speaker_id": "1",
          "time": "20210828 20:45"
        }
      ]
