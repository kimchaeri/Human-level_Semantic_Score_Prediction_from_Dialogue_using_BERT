      "utterance": [
        {
          "id": "MDRW2100013976.1.1",
          "form": "지금 넷플로 응답하라 1988 보고 있어요",
          "original_form": "지금 넷플로 응답하라 1988 보고 있어요",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013976.1.2",
          "form": "응답하라 재밌죠:)",
          "original_form": "응답하라 재밌죠:)",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013976.1.3",
          "form": "저는 슬의생 보는데",
          "original_form": "저는 슬의생 보는데",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013976.1.4",
          "form": "응답하라랑 슬의생\n둘다 ost가 너무 좋아요",
          "original_form": "응답하라랑 슬의생\n둘다 ost가 너무 좋아요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013976.1.5",
          "form": "저는 슬의생 다 끝나면 몰아서 보려고요...",
          "original_form": "저는 슬의생 다 끝나면 몰아서 보려고요...",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013976.1.6",
          "form": "기다리는 거 너무 힘들어요 흑흑",
          "original_form": "기다리는 거 너무 힘들어요 흑흑",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013976.1.7",
          "form": "그쵸ㅜㅜ 제가 1은\n한번에 몰아봐서 몰랐는데",
          "original_form": "그쵸ㅜㅜ 제가 1은\n한번에 몰아봐서 몰랐는데",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013976.1.8",
          "form": "2는 매주 보다보니\n정말 너무 괴롭네요ㅠㅋㅋ",
          "original_form": "2는 매주 보다보니\n정말 너무 괴롭네요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013976.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 넷플이나 이런 거 나온 뒤로 전 계속",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 넷플이나 이런 거 나온 뒤로 전 계속",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013976.1.10",
          "form": "기다렸다가 몰아보는 거 같아요",
          "original_form": "기다렸다가 몰아보는 거 같아요",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013976.1.11",
          "form": "그게 제일 마음 편한,,,",
          "original_form": "그게 제일 마음 편한,,,",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013976.1.12",
          "form": "ㅋㅋ그게 편하긴 하죠",
          "original_form": "ㅋㅋ그게 편하긴 하죠",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013976.1.13",
          "form": "킹덤도 보셨어요?",
          "original_form": "킹덤도 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013976.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013976.1.15",
          "form": "아니요 공포는 안 봐요ㅠㅠㅠ",
          "original_form": "아니요 공포는 안 봐요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013976.1.16",
          "form": "넷플하면 거의 킹덤이라ㅋㅋ",
          "original_form": "넷플하면 거의 킹덤이라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013976.1.17",
          "form": "혹시 보셨나해서 여쭤봤어요",
          "original_form": "혹시 보셨나해서 여쭤봤어요",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013976.1.18",
          "form": "참고로 저도 안 본 1인요ㅋㅋ",
          "original_form": "참고로 저도 안 본 1인요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013976.1.19",
          "form": "ㅋㅋㅋㅋㅋ 넷플에서 재미있게 보신 거",
          "original_form": "ㅋㅋㅋㅋㅋ 넷플에서 재미있게 보신 거",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013976.1.20",
          "form": "뭐뭐있으세요?",
          "original_form": "뭐뭐있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013976.1.21",
          "form": "키씽부스? ㅋㅋ",
          "original_form": "키씽부스? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100013976.1.22",
          "form": "알고있지만?",
          "original_form": "알고있지만?",
          "speaker_id": "1",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013976.1.23",
          "form": "아 약간",
          "original_form": "아 약간",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013976.1.24",
          "form": "로맨스쪽이 취향이신가봐요",
          "original_form": "로맨스쪽이 취향이신가봐요",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013976.1.25",
          "form": "어쩌다 보다보니ㅋㅋ",
          "original_form": "어쩌다 보다보니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013976.1.26",
          "form": "다 추천으로 본거에요ㅋ",
          "original_form": "다 추천으로 본거에요ㅋ",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013976.1.27",
          "form": "키싱부스는...",
          "original_form": "키싱부스는...",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100013976.1.28",
          "form": "좀 너무 유치한 거 같아요 개연성도 너무 없고",
          "original_form": "좀 너무 유치한 거 같아요 개연성도 너무 없고",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100013976.1.29",
          "form": "ㅋㅋ갈수록 별로인거 같긴 해요",
          "original_form": "ㅋㅋ갈수록 별로인거 같긴 해요",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100013976.1.30",
          "form": "3는 평이 넘 안좋길래 아예 안봤어요",
          "original_form": "3는 평이 넘 안좋길래 아예 안봤어요",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013976.1.31",
          "form": "넷플 통해 본게\n슬의생 빼곤 저게 다에요 저는 ㅠㅋ",
          "original_form": "넷플 통해 본게\n슬의생 빼곤 저게 다에요 저는 ㅠㅋ",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013976.1.32",
          "form": "ㅋㅋㅋ 저도 2까지만 봤어요",
          "original_form": "ㅋㅋㅋ 저도 2까지만 봤어요",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013976.1.33",
          "form": "1은 남주 피지컬이 개연성이긴 했는데",
          "original_form": "1은 남주 피지컬이 개연성이긴 했는데",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013976.1.34",
          "form": "후......",
          "original_form": "후......",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013976.1.35",
          "form": "ㅋㅋㅋㅋ피지컬 ",
          "original_form": "ㅋㅋㅋㅋ피지컬 {emoji:♡}",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100013976.1.36",
          "form": "노아랑 엘이랑\n실제로도 사겼었다면서요ㅋㅋ",
          "original_form": "노아랑 엘이랑\n실제로도 사겼었다면서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100013976.1.37",
          "form": "맞아요 근데 2를",
          "original_form": "맞아요 근데 2를",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100013976.1.38",
          "form": "헤어지고나서 찍은 거라던데",
          "original_form": "헤어지고나서 찍은 거라던데",
          "speaker_id": "2",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100013976.1.39",
          "form": "헐 2를 헤어지고나서 찍은거에요?",
          "original_form": "헐 2를 헤어지고나서 찍은거에요?",
