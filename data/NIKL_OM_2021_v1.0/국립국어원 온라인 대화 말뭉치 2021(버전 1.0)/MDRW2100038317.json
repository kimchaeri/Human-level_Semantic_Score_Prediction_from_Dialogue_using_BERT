      "utterance": [
        {
          "id": "MDRW2100038317.1.1",
          "form": "유가네드시고싶단다",
          "original_form": "유가네드시고싶단다",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100038317.1.2",
          "form": "드리고 싶음 먹어야쥬",
          "original_form": "드리고 싶음 먹어야쥬",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100038317.1.3",
          "form": "난 오예스먹고",
          "original_form": "난 오예스먹고",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100038317.1.4",
          "form": "커피마시고 있슈",
          "original_form": "커피마시고 있슈",
          "speaker_id": "2",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100038317.1.5",
          "form": "나도 커피마시고있음ㅋ",
          "original_form": "나도 커피마시고있음ㅋ",
          "speaker_id": "1",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100038317.1.6",
          "form": "어제받은쿠폰으로",
          "original_form": "어제받은쿠폰으로",
          "speaker_id": "1",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100038317.1.7",
          "form": "맛나네 공짜커피",
          "original_form": "맛나네 공짜커피",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100038317.1.8",
          "form": "나도 주말에 공짜 커피 마셔야지",
          "original_form": "나도 주말에 공짜 커피 마셔야지",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038317.1.9",
          "form": "좋구만",
          "original_form": "좋구만",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038317.1.10",
          "form": "언니 핑퐁 커피가 봤어??",
          "original_form": "언니 핑퐁 커피가 봤어??",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038317.1.11",
          "form": "아니맛있나?",
          "original_form": "아니맛있나?",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100038317.1.12",
          "form": "핑퐁?",
          "original_form": "핑퐁?",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100038317.1.13",
          "form": "어 핑퐁 커피에 핑퐁 라떼 엄청 맛있어",
          "original_form": "어 핑퐁 커피에 핑퐁 라떼 엄청 맛있어",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100038317.1.14",
          "form": "근데 좀 비싸 응",
          "original_form": "근데 좀 비싸 응",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100038317.1.15",
          "form": "가봐야겠군",
          "original_form": "가봐야겠군",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100038317.1.16",
          "form": "어디쯤있노?",
          "original_form": "어디쯤있노?",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100038317.1.17",
          "form": "엠 허브 건물 일층인가",
          "original_form": "엠 허브 건물 일층인가",
          "speaker_id": "2",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100038317.1.18",
          "form": "거기 파리바게트 있잖아 그 근처",
          "original_form": "거기 파리바게트 있잖아 그 근처",
          "speaker_id": "2",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100038317.1.19",
          "form": "저쪽이네 현풍ㅋ",
          "original_form": "저쪽이네 현풍ㅋ",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100038317.1.20",
          "form": "가는길인데 가봐야겠네ㅋㅋ",
          "original_form": "가는길인데 가봐야겠네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:18"
        },
        {
          "id": "MDRW2100038317.1.21",
          "form": "니는 점심뭐먹노",
          "original_form": "니는 점심뭐먹노",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038317.1.22",
          "form": "나 오늘 치즈 돈까스",
          "original_form": "나 오늘 치즈 돈까스",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100038317.1.23",
          "form": "맛있겠지??",
          "original_form": "맛있겠지??",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100038317.1.24",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038317.1.25",
          "form": "치돈ㅋㅋ",
          "original_form": "치돈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038317.1.26",
          "form": "맛있게먹어라ㅋ",
          "original_form": "맛있게먹어라ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038317.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 먹었지 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 먹었지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100038317.1.28",
          "form": "맛있다",
          "original_form": "맛있다",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100038317.1.29",
          "form": "먹고 커피마시고 있음\r\nㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "먹고 커피마시고 있음\r\nㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100038317.1.30",
          "form": "유가네 먹었슈??",
          "original_form": "유가네 먹었슈??",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100038317.1.31",
          "form": "어ㅋㅋ",
          "original_form": "어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100038317.1.32",
          "form": "먹고있다",
          "original_form": "먹고있다",
