      "utterance": [
        {
          "id": "MDRW2100039687.1.1",
          "form": "배신감까지 ㅎㅎ",
          "original_form": "배신감까지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100039687.1.2",
          "form": "영화가 원래 그렇죠!",
          "original_form": "영화가 원래 그렇죠!",
          "speaker_id": "1",
          "time": "20210903 20:33"
        },
        {
          "id": "MDRW2100039687.1.3",
          "form": "그쵸ㅜㅜ",
          "original_form": "그쵸ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039687.1.4",
          "form": "저는 프리가이도 재밌게봤어요",
          "original_form": "저는 프리가이도 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039687.1.5",
          "form": "그건 첨들어요",
          "original_form": "그건 첨들어요",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100039687.1.6",
          "form": "어떤 영화인가요?",
          "original_form": "어떤 영화인가요?",
          "speaker_id": "1",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100039687.1.7",
          "form": "그..킬링타임용 영화인데",
          "original_form": "그..킬링타임용 영화인데",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039687.1.8",
          "form": "npc가 인공지능으로 자아가 설계됐어요",
          "original_form": "npc가 인공지능으로 자아가 설계됐어요",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100039687.1.9",
          "form": "오홍 그런것두 재밌죠 ㅎㅎ",
          "original_form": "오홍 그런것두 재밌죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039687.1.10",
          "form": "근데 결론은 사랑야기",
          "original_form": "근데 결론은 사랑야기",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039687.1.11",
          "form": "ㅋㄱㄱㅋ",
          "original_form": "ㅋㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039687.1.12",
          "form": "ㅋㅋㅋ기승전 사랑",
          "original_form": "ㅋㅋㅋ기승전 사랑",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039687.1.13",
          "form": "사랑이 진짜 중요한 감정인가봐요",
          "original_form": "사랑이 진짜 중요한 감정인가봐요",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039687.1.14",
          "form": "에휴..",
          "original_form": "에휴..",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039687.1.15",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039687.1.16",
          "form": "근데 저는 잘모르겠어요",
          "original_form": "근데 저는 잘모르겠어요",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039687.1.17",
          "form": "왜요?? ㅎㅎㅎㅎ",
          "original_form": "왜요?? ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100039687.1.18",
          "form": "왜 모르시겠다고 하는지가 귱금해요",
          "original_form": "왜 모르시겠다고 하는지가 귱금해요",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100039687.1.19",
          "form": "저도 물론 잘 모르겠습니다!",
          "original_form": "저도 물론 잘 모르겠습니다!",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100039687.1.20",
          "form": "사랑이 그렇게나 중요한 감정일까",
          "original_form": "사랑이 그렇게나 중요한 감정일까",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039687.1.21",
          "form": "모든 개연성을 사랑으로 엮으면 되는긘가",
          "original_form": "모든 개연성을 사랑으로 엮으면 되는긘가",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039687.1.22",
          "form": "그러니까요!!! 왜 그렇게 중요한 감정 마냥 이야기 하는지...",
          "original_form": "그러니까요!!! 왜 그렇게 중요한 감정 마냥 이야기 하는지...",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039687.1.23",
          "form": "아 물론 중요할 수도 있긴란데",
          "original_form": "아 물론 중요할 수도 있긴란데",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039687.1.24",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039687.1.25",
          "form": "개연성없는 것도 사랑만 넣으면 다 커버되는건지 이해안가서요",
          "original_form": "개연성없는 것도 사랑만 넣으면 다 커버되는건지 이해안가서요",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039687.1.26",
          "form": "맞아요 정말...이해안가요 ㅠ",
          "original_form": "맞아요 정말...이해안가요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039687.1.27",
          "form": "사랑...흠",
          "original_form": "사랑...흠",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039687.1.28",
          "form": "ㅜㅜ그쵸",
          "original_form": "ㅜㅜ그쵸",
          "speaker_id": "2",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039687.1.29",
          "form": "그래도 해피엔딩이라 좋았어오",
          "original_form": "그래도 해피엔딩이라 좋았어오",
