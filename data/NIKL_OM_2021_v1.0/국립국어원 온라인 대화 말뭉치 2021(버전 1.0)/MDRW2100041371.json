          "speaker_id": "2",
          "time": "20210906 08:47"
        },
        {
          "id": "MDRW2100041371.1.3",
          "form": "오늘날씨가선선하조ㅡ",
          "original_form": "오늘날씨가선선하조ㅡ",
          "speaker_id": "1",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100041371.1.4",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "2",
          "time": "20210906 17:15"
        },
        {
          "id": "MDRW2100041371.1.5",
          "form": "음 산책이랑 플랭크정도해요!",
          "original_form": "음 산책이랑 플랭크정도해요!",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041371.1.6",
          "form": "많이안하는수준이죵ㅎㅎ",
          "original_form": "많이안하는수준이죵ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041371.1.7",
          "form": "오 플랭크",
          "original_form": "오 플랭크",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041371.1.8",
          "form": "잘하시나요",
          "original_form": "잘하시나요",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100041371.1.9",
          "form": "음 1분씩 끊어서 10분짜리해요",
          "original_form": "음 1분씩 끊어서 10분짜리해요",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041371.1.10",
          "form": "오 꽤 하시군요",
          "original_form": "오 꽤 하시군요",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041371.1.11",
          "form": "전 못하거든요",
          "original_form": "전 못하거든요",
          "speaker_id": "2",
          "time": "20210906 17:36"
        },
        {
          "id": "MDRW2100041371.1.12",
          "form": "ㅎㅎ운동하시나요?",
          "original_form": "ㅎㅎ운동하시나요?",
          "speaker_id": "1",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041371.1.13",
          "form": "홈트하려고 하는제",
          "original_form": "홈트하려고 하는제",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041371.1.14",
          "form": "귀차니즘으로 매일 미루네요 ㅎ",
          "original_form": "귀차니즘으로 매일 미루네요 ㅎ",
          "speaker_id": "2",
          "time": "20210906 17:37"
        },
        {
          "id": "MDRW2100041371.1.15",
          "form": "내일의제가해줄꺼라생각하고ㅎㅎ",
          "original_form": "내일의제가해줄꺼라생각하고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041371.1.16",
          "form": "건강하면됬죵",
          "original_form": "건강하면됬죵",
          "speaker_id": "1",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041371.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아 저도 그래요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아 저도 그래요",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041371.1.18",
          "form": "미안하지만 내일의 내가 ㅎ",
          "original_form": "미안하지만 내일의 내가 ㅎ",
          "speaker_id": "2",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041371.1.19",
          "form": "ㅎㅎㅎ백신맞으셨나요",
          "original_form": "ㅎㅎㅎ백신맞으셨나요",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041371.1.20",
          "form": "아직요 ㅠㅠ ㅠㅠ",
