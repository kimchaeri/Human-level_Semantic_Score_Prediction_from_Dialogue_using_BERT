      "utterance": [
        {
          "id": "MDRW2100041604.1.1",
          "form": "붓카게",
          "original_form": "붓카게",
          "speaker_id": "1",
          "time": "20210906 12:23"
        },
        {
          "id": "MDRW2100041604.1.2",
          "form": "우동",
          "original_form": "우동",
          "speaker_id": "1",
          "time": "20210906 12:23"
        },
        {
          "id": "MDRW2100041604.1.3",
          "form": "먹을거야",
          "original_form": "먹을거야",
          "speaker_id": "1",
          "time": "20210906 12:23"
        },
        {
          "id": "MDRW2100041604.1.4",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210906 12:23"
        },
        {
          "id": "MDRW2100041604.1.5",
          "form": "멋있는 거 드시네요",
          "original_form": "멋있는 거 드시네요",
          "speaker_id": "2",
          "time": "20210906 12:23"
        },
        {
          "id": "MDRW2100041604.1.6",
          "form": "걍 우동면 우동다시 계란 쪽파만 잇으면 더ㅣ어서",
          "original_form": "걍 우동면 우동다시 계란 쪽파만 잇으면 더ㅣ어서",
          "speaker_id": "1",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041604.1.7",
          "form": "간단함미다",
          "original_form": "간단함미다",
          "speaker_id": "1",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041604.1.8",
          "form": "해먹는 거에요?",
          "original_form": "해먹는 거에요?",
          "speaker_id": "2",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041604.1.9",
          "form": "우앙",
          "original_form": "우앙",
          "speaker_id": "2",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041604.1.10",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041604.1.11",
          "form": "집에 우동면만 잇어서",
          "original_form": "집에 우동면만 잇어서",
          "speaker_id": "1",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041604.1.12",
          "form": "소비해야해",
          "original_form": "소비해야해",
          "speaker_id": "1",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041604.1.13",
          "form": "근데 그동안 쪽파가 없어서",
          "original_form": "근데 그동안 쪽파가 없어서",
          "speaker_id": "1",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041604.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ맛있겠다!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맛있겠다!!",
          "speaker_id": "2",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041604.1.15",
          "form": "못했어",
          "original_form": "못했어",
          "speaker_id": "1",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041604.1.16",
          "form": "가쓰오부시는 없어도ㅠ되는데",
          "original_form": "가쓰오부시는 없어도ㅠ되는데",
          "speaker_id": "1",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100041604.1.17",
          "form": "쪽파는",
          "original_form": "쪽파는",
          "speaker_id": "1",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100041604.1.18",
          "form": "있어야한대",
          "original_form": "있어야한대",
          "speaker_id": "1",
          "time": "20210906 12:25"
        },
        {
          "id": "MDRW2100041604.1.19",
          "form": "파는 중요해",
          "original_form": "파는 중요해",
          "speaker_id": "2",
          "time": "20210906 12:26"
        },
        {
          "id": "MDRW2100041604.1.20",
          "form": "오늘은 잇구나",
          "original_form": "오늘은 잇구나",
          "speaker_id": "2",
          "time": "20210906 12:26"
        },
        {
          "id": "MDRW2100041604.1.21",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "2",
          "time": "20210906 12:26"
        },
        {
          "id": "MDRW2100041604.1.22",
          "form": "오늘을 위해 삿지",
          "original_form": "오늘을 위해 삿지",
          "speaker_id": "1",
          "time": "20210906 12:34"
        },
        {
          "id": "MDRW2100041604.1.23",
          "form": "언제해?",
          "original_form": "언제해?",
          "speaker_id": "2",
          "time": "20210906 12:35"
        },
        {
          "id": "MDRW2100041604.1.24",
          "form": "자 요리법을 적어",
          "original_form": "자 요리법을 적어",
          "speaker_id": "2",
          "time": "20210906 12:35"
        },
        {
          "id": "MDRW2100041604.1.25",
          "form": "지금 물끓이는 중",
          "original_form": "지금 물끓이는 중",
          "speaker_id": "1",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100041604.1.26",
          "form": "1. 우동면을 삶는다",
          "original_form": "1. 우동면을 삶는다",
          "speaker_id": "1",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100041604.1.27",
          "form": "2. 찬물에 식힌다",
          "original_form": "2. 찬물에 식힌다",
          "speaker_id": "1",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100041604.1.28",
          "form": "예",
          "original_form": "예",
          "speaker_id": "2",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100041604.1.29",
          "form": "3. 물을 끓인다",
          "original_form": "3. 물을 끓인다",
          "speaker_id": "1",
          "time": "20210906 12:38"
        },
        {
          "id": "MDRW2100041604.1.30",
          "form": "얼마나삶아",
          "original_form": "얼마나삶아",
          "speaker_id": "2",
          "time": "20210906 12:38"
        },
        {
          "id": "MDRW2100041604.1.31",
          "form": "우동면은 걍 익을 정도면 돼",
          "original_form": "우동면은 걍 익을 정도면 돼",
          "speaker_id": "1",
          "time": "20210906 12:38"
        },
        {
          "id": "MDRW2100041604.1.32",
          "form": "난 끓인물에 같이 하는 줄",
          "original_form": "난 끓인물에 같이 하는 줄",
          "speaker_id": "2",
          "time": "20210906 12:38"
        },
        {
          "id": "MDRW2100041604.1.33",
          "form": "육수업ㄱ어",
          "original_form": "육수업ㄱ어",
          "speaker_id": "1",
          "time": "20210906 12:38"
        },
        {
          "id": "MDRW2100041604.1.34",
          "form": "4. 불을 끄고 찬물 넣은 후 상온에 둔 계란을 집어넣고",
          "original_form": "4. 불을 끄고 찬물 넣은 후 상온에 둔 계란을 집어넣고",
          "speaker_id": "1",
          "time": "20210906 12:38"
        },
        {
          "id": "MDRW2100041604.1.35",
          "form": "10~13분 대기한다",
          "original_form": "10~13분 대기한다",
          "speaker_id": "1",
          "time": "20210906 12:38"
        },
        {
          "id": "MDRW2100041604.1.36",
          "form": "그럼 말랑말랑한 온센타마고가 된다",
          "original_form": "그럼 말랑말랑한 온센타마고가 된다",
          "speaker_id": "1",
          "time": "20210906 12:39"
        },
        {
          "id": "MDRW2100041604.1.37",
          "form": "오 불을 끄고",
          "original_form": "오 불을 끄고",
          "speaker_id": "2",
          "time": "20210906 12:39"
        },
        {
          "id": "MDRW2100041604.1.38",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 12:39"
        },
        {
          "id": "MDRW2100041604.1.39",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210906 12:39"
        },
        {
          "id": "MDRW2100041604.1.40",
          "form": "5. 면에다 온겐타마도 넣고(날계란처럼 안터짐) or 날계란을 넣어두 댐",
          "original_form": "5. 면에다 온겐타마도 넣고(날계란처럼 안터짐) or 날계란을 넣어두 댐",
          "speaker_id": "1",
          "time": "20210906 12:39"
        },
        {
          "id": "MDRW2100041604.1.41",
          "form": "6. 쯔유 2~3 숟가락에 온센타마고 넣고",
          "original_form": "6. 쯔유 2~3 숟가락에 온센타마고 넣고",
          "speaker_id": "1",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100041604.1.42",
          "form": "7. 쪽파를 썰어넣는다",
          "original_form": "7. 쪽파를 썰어넣는다",
          "speaker_id": "1",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100041604.1.43",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100041604.1.44",
          "form": "8. 취향에 따라 튀김 가스오 김가루도 ㅇㅇ",
          "original_form": "8. 취향에 따라 튀김 가스오 김가루도 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100041604.1.45",
          "form": "물을 두번 끓어야해서",
          "original_form": "물을 두번 끓어야해서",
          "speaker_id": "1",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100041604.1.46",
          "form": "귀찮아서 글치",
          "original_form": "귀찮아서 글치",
          "speaker_id": "1",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100041604.1.47",
          "form": "어렵진않음",
          "original_form": "어렵진않음",
          "speaker_id": "1",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100041604.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ간단하내",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ간단하내",
          "speaker_id": "2",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100041604.1.49",
          "form": "물을 두번 끓이고 대기하는게",
          "original_form": "물을 두번 끓이고 대기하는게",
          "speaker_id": "1",
          "time": "20210906 12:41"
        },
        {
          "id": "MDRW2100041604.1.50",
          "form": "귀찮을 뿐임",
          "original_form": "귀찮을 뿐임",
          "speaker_id": "1",
          "time": "20210906 12:41"
        },
        {
          "id": "MDRW2100041604.1.51",
          "form": "귀찮아서 강조함",
          "original_form": "귀찮아서 강조함",
          "speaker_id": "1",
          "time": "20210906 12:41"
        }
      ]
    }
  ]
