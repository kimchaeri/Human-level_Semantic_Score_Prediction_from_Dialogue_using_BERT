      "utterance": [
        {
          "id": "MDRW2100030637.1.1",
          "form": "근데 청주가 확실히 따뜻하더라",
          "original_form": "근데 청주가 확실히 따뜻하더라",
          "speaker_id": "2",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030637.1.2",
          "form": "날씨 좋은데?ㅎ",
          "original_form": "날씨 좋은데?ㅎ",
          "speaker_id": "2",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030637.1.3",
          "form": "맞아 더워졌어",
          "original_form": "맞아 더워졌어",
          "speaker_id": "2",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030637.1.4",
          "form": "청주? 살기좋지 환경도 원래좋았는데  날씨도",
          "original_form": "청주? 살기좋지 환경도 원래좋았는데  날씨도",
          "speaker_id": "1",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030637.1.5",
          "form": "청주가 맑아 별이 잘보여",
          "original_form": "청주가 맑아 별이 잘보여",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030637.1.6",
          "form": "그정돈 아니다",
          "original_form": "그정돈 아니다",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030637.1.7",
          "form": "그래?? 서울보단 잘 보이더라고 ㅋㅋㅋㅋㅋ",
          "original_form": "그래?? 서울보단 잘 보이더라고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030637.1.8",
          "form": "날씨좋은건 맞지만 별은 다른지역이 더 잘보이지",
          "original_form": "날씨좋은건 맞지만 별은 다른지역이 더 잘보이지",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030637.1.9",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030637.1.10",
          "form": "서울에선 가끔 긴팔도 입었는데",
          "original_form": "서울에선 가끔 긴팔도 입었는데",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030637.1.11",
          "form": "청주는 아직 무리더라",
          "original_form": "청주는 아직 무리더라",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030637.1.12",
          "form": "으쨌든 나가야되니까 나가기 귀찮아 밖에더워보이네 ^^,,,",
          "original_form": "으쨌든 나가야되니까 나가기 귀찮아 밖에더워보이네 ^^,,,",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030637.1.13",
          "form": "그건아닌데? ^15@9",
          "original_form": "그건아닌데? ^15@9",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030637.1.14",
          "form": "나긴팔입어",
          "original_form": "나긴팔입어",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030637.1.15",
          "form": "저녁엔",
          "original_form": "저녁엔",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030637.1.16",
          "form": "그래???",
          "original_form": "그래???",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030637.1.17",
          "form": "알겠어 너가 맞아",
          "original_form": "알겠어 너가 맞아",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030637.1.18",
          "form": "긴팔 입어야겠다",
          "original_form": "긴팔 입어야겠다",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030637.1.19",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030637.1.20",
          "form": "응응 저녑에 입어 긴팔 !",
          "original_form": "응응 저녑에 입어 긴팔 !",
          "speaker_id": "1",
          "time": "20210904 10:52"
        },
        {
          "id": "MDRW2100030637.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ알겠어 저녁에 잘 안나가서 모르나봐",
          "original_form": "ㅋㅋㅋㅋㅋㅋ알겠어 저녁에 잘 안나가서 모르나봐",
          "speaker_id": "2",
          "time": "20210904 10:52"
        },
        {
          "id": "MDRW2100030637.1.22",
          "form": "긴팔 입을게!",
          "original_form": "긴팔 입을게!",
          "speaker_id": "2",
          "time": "20210904 10:52"
        },
        {
          "id": "MDRW2100030637.1.23",
          "form": "굿굿 근데 지금 나오니까 또",
          "original_form": "굿굿 근데 지금 나오니까 또",
          "speaker_id": "1",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100030637.1.24",
          "form": "선선해",
          "original_form": "선선해",
          "speaker_id": "1",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100030637.1.25",
          "form": "가을은 가을이야",
          "original_form": "가을은 가을이야",
          "speaker_id": "2",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100030637.1.26",
          "form": "금방 겨울오겠어",
          "original_form": "금방 겨울오겠어",
          "speaker_id": "2",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100030637.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ너무 갔다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ너무 갔다",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030637.1.28",
          "form": "가을 이제 시작인데",
          "original_form": "가을 이제 시작인데",
