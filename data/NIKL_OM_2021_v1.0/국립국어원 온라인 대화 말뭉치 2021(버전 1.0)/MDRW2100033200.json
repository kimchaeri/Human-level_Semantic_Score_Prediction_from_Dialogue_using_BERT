      "utterance": [
        {
          "id": "MDRW2100033200.1.1",
          "form": "커피 좋아하니",
          "original_form": "커피 좋아하니",
          "speaker_id": "1",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100033200.1.2",
          "form": "커피 종류에 따라 다른 듯",
          "original_form": "커피 종류에 따라 다른 듯",
          "speaker_id": "2",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100033200.1.3",
          "form": "그 카페모카는 좋아",
          "original_form": "그 카페모카는 좋아",
          "speaker_id": "2",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100033200.1.4",
          "form": "라떼는 어때?",
          "original_form": "라떼는 어때?",
          "speaker_id": "1",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100033200.1.5",
          "form": "녹차라떼, 초코라떼는 좋아",
          "original_form": "녹차라떼, 초코라떼는 좋아",
          "speaker_id": "2",
          "time": "20210906 18:46"
        },
        {
          "id": "MDRW2100033200.1.6",
          "form": "바닐라라떼도 그럭저럭 괜찮은 듯",
          "original_form": "바닐라라떼도 그럭저럭 괜찮은 듯",
          "speaker_id": "2",
          "time": "20210906 18:46"
        },
        {
          "id": "MDRW2100033200.1.7",
          "form": "배고프다 저녁 메뉴 추천 좀 해봐",
          "original_form": "배고프다 저녁 메뉴 추천 좀 해봐",
          "speaker_id": "2",
          "time": "20210906 18:47"
        },
        {
          "id": "MDRW2100033200.1.8",
          "form": "우동 어때",
          "original_form": "우동 어때",
          "speaker_id": "1",
          "time": "20210906 18:47"
        },
        {
          "id": "MDRW2100033200.1.9",
          "form": "따끈한 우동ㅋㅋㅋ 버섯이랑 어묵 넣은거",
          "original_form": "따끈한 우동ㅋㅋㅋ 버섯이랑 어묵 넣은거",
          "speaker_id": "1",
          "time": "20210906 18:47"
        },
        {
          "id": "MDRW2100033200.1.10",
          "form": "우동 좋지 근데 집에 우동이 있나",
          "original_form": "우동 좋지 근데 집에 우동이 있나",
          "speaker_id": "2",
          "time": "20210906 18:47"
        },
        {
          "id": "MDRW2100033200.1.11",
          "form": "사먹어",
          "original_form": "사먹어",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100033200.1.12",
          "form": "최고의 치킨은 뭐라 생각해",
          "original_form": "최고의 치킨은 뭐라 생각해",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100033200.1.13",
          "form": "젠장 라면 먹고 있음",
          "original_form": "젠장 라면 먹고 있음",
          "speaker_id": "2",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100033200.1.14",
          "form": "최고의 치킨은 맛있는 치킨",
          "original_form": "최고의 치킨은 맛있는 치킨",
          "speaker_id": "2",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100033200.1.15",
          "form": "우리 꼭 고바삭치킨 먹자",
          "original_form": "우리 꼭 고바삭치킨 먹자",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100033200.1.16",
          "form": "뭐야ㅋㅋ 그럼 나는 짬바삭ㅋㅋ",
          "original_form": "뭐야ㅋㅋ 그럼 나는 짬바삭ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100033200.1.17",
          "form": "\r\n짬뽕바사삭으로 알게",
          "original_form": "\r\n짬뽕바사삭으로 알게",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100033200.1.18",
          "form": "짬뽕치킨도 맛있으려나",
          "original_form": "짬뽕치킨도 맛있으려나",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100033200.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100033200.1.20",
          "form": "진짬뽕 맛이면 괜찮겠다",
          "original_form": "진짬뽕 맛이면 괜찮겠다",
          "speaker_id": "1",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100033200.1.21",
          "form": "그럼 너의 최애는 고추바사삭?",
          "original_form": "그럼 너의 최애는 고추바사삭?",
          "speaker_id": "1",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100033200.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100033200.1.23",
          "form": "음.. 그건 잘 모르겠어 요즘엔 처갓집 양념치킨인 것 같기도",
          "original_form": "음.. 그건 잘 모르겠어 요즘엔 처갓집 양념치킨인 것 같기도",
          "speaker_id": "2",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100033200.1.24",
          "form": "그것도 맛있지....",
          "original_form": "그것도 맛있지....",
          "speaker_id": "1",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100033200.1.25",
          "form": "근데 확실한건 피자는 반올림이라는거야",
          "original_form": "근데 확실한건 피자는 반올림이라는거야",
          "speaker_id": "1",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100033200.1.26",
          "form": "인정.. 청년피자도 다시 도전해봐야하는데",
          "original_form": "인정.. 청년피자도 다시 도전해봐야하는데",
          "speaker_id": "2",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100033200.1.27",
          "form": "맞아.... 태연 포카가 포함된 피자",
          "original_form": "맞아.... 태연 포카가 포함된 피자",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100033200.1.28",
          "form": "감자튀김이 잔뜩 올려진 피자 엄청 맛있어보이더라",
          "original_form": "감자튀김이 잔뜩 올려진 피자 엄청 맛있어보이더라",
          "speaker_id": "2",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100033200.1.29",
          "form": "태연포카 포함된 피자 얼른 내놔라",
          "original_form": "태연포카 포함된 피자 얼른 내놔라",
          "speaker_id": "2",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100033200.1.30",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100033200.1.31",
          "form": "사실 감자는 뭐든 맛있지ㅋㅋ",
          "original_form": "사실 감자는 뭐든 맛있지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100033200.1.32",
          "form": "진심 감자는 뭐든 맛있어",
          "original_form": "진심 감자는 뭐든 맛있어",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100033200.1.33",
          "form": "에그타르트 먹으러 크루아 가고싶다",
          "original_form": "에그타르트 먹으러 크루아 가고싶다",
          "speaker_id": "1",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100033200.1.34",
          "form": "감자 포타쥬인가 그거 먹어보고싶음",
          "original_form": "감자 포타쥬인가 그거 먹어보고싶음",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100033200.1.35",
          "form": "아 크루아 진짜 가야되는데",
          "original_form": "아 크루아 진짜 가야되는데",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100033200.1.36",
          "form": "샹치보러 갈 때 크루아 가서 샌드위치 먹을까",
          "original_form": "샹치보러 갈 때 크루아 가서 샌드위치 먹을까",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100033200.1.37",
          "form": "시간이 되려나ㅠㅠ",
          "original_form": "시간이 되려나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100033200.1.38",
          "form": "재난지원금 받은 김에 디저트 많이 사야하는데",
          "original_form": "재난지원금 받은 김에 디저트 많이 사야하는데",
          "speaker_id": "1",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100033200.1.39",
          "form": "그니까 ㅠㅠㅠ 크루아 샌드위치 진짜 꼭 먹고 싶은데",
          "original_form": "그니까 ㅠㅠㅠ 크루아 샌드위치 진짜 꼭 먹고 싶은데",
          "speaker_id": "2",
          "time": "20210906 19:23"
        },
        {
          "id": "MDRW2100033200.1.40",
          "form": "계절과일 디저트도먹고ㅠㅠ",
          "original_form": "계절과일 디저트도먹고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100033200.1.41",
          "form": "그 다음날 갈까 ㅋㅋㅋㅋ",
          "original_form": "그 다음날 갈까 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:24"
        },
        {
          "id": "MDRW2100033200.1.42",
          "form": "ㅋㅋㅋㅋㅋ 그럴까",
          "original_form": "ㅋㅋㅋㅋㅋ 그럴까",
