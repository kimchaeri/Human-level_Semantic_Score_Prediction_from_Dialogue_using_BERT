      "utterance": [
        {
          "id": "MDRW2100032194.1.1",
          "form": "여행가고 싶군요",
          "original_form": "여행가고 싶군요",
          "speaker_id": "2",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100032194.1.2",
          "form": "그러니까요..",
          "original_form": "그러니까요..",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100032194.1.3",
          "form": "다시 가고 싶은곳 있으세요?",
          "original_form": "다시 가고 싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100032194.1.4",
          "form": "해외 좀 더 갈껄",
          "original_form": "해외 좀 더 갈껄",
          "speaker_id": "2",
          "time": "20210906 14:20"
        },
        {
          "id": "MDRW2100032194.1.5",
          "form": "싶네요ㅠ",
          "original_form": "싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210906 14:20"
        },
        {
          "id": "MDRW2100032194.1.6",
          "form": "전 유럽쪽을 못가봐서 가고싶은데..",
          "original_form": "전 유럽쪽을 못가봐서 가고싶은데..",
          "speaker_id": "1",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100032194.1.7",
          "form": "유럽도 가보고 싶은데 무섭ㅠㅠ",
          "original_form": "유럽도 가보고 싶은데 무섭ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100032194.1.8",
          "form": "그러니까요.. 근데 당장은 아니더라도 가고싶어요..",
          "original_form": "그러니까요.. 근데 당장은 아니더라도 가고싶어요..",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100032194.1.9",
          "form": "나중에도 유럽을 못가면 어디든 무서워서 못갈거예요..",
          "original_form": "나중에도 유럽을 못가면 어디든 무서워서 못갈거예요..",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100032194.1.10",
          "form": "아 코로나때문에",
          "original_form": "아 코로나때문에",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100032194.1.11",
          "form": "아시안포비아....ㅜ",
          "original_form": "아시안포비아....ㅜ",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100032194.1.12",
          "form": "그건 그사람들이 문제라고 생각해요 ",
          "original_form": "그건 그사람들이 문제라고 생각해요 ",
          "speaker_id": "1",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100032194.1.13",
          "form": "외국 어디가셨었나요?",
          "original_form": "외국 어디가셨었나요?",
          "speaker_id": "1",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100032194.1.14",
          "form": "저 대만 베트남 홍콩 태국",
          "original_form": "저 대만 베트남 홍콩 태국",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100032194.1.15",
          "form": "재작년에 일케 갔네요",
          "original_form": "재작년에 일케 갔네요",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100032194.1.16",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210906 20:38"
        },
        {
          "id": "MDRW2100032194.1.17",
          "form": "한해에 다가셨나요?",
          "original_form": "한해에 다가셨나요?",
          "speaker_id": "1",
          "time": "20210906 20:38"
        },
        {
          "id": "MDRW2100032194.1.18",
          "form": "네 3 5 8 11인가 ㅎㅎ",
          "original_form": "네 3 5 8 11인가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100032194.1.19",
          "form": "대단ㅋㅋㅋㅋㅋ",
          "original_form": "대단ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100032194.1.20",
          "form": "전 그해에 2번째로 태국을 갔어요.. 방콕좋아요ㅋㅋㅋㅋ",
          "original_form": "전 그해에 2번째로 태국을 갔어요.. 방콕좋아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:01"
        },
        {
          "id": "MDRW2100032194.1.21",
          "form": "저도 방콕이요 ㅎㅎ",
          "original_form": "저도 방콕이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:01"
        },
        {
          "id": "MDRW2100032194.1.22",
          "form": "3박5일이라 좀 힘들었던",
          "original_form": "3박5일이라 좀 힘들었던",
          "speaker_id": "2",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100032194.1.23",
          "form": "전 2번째라 멀리 안가고 방콕이랑 그 주변만 다녀서 생각보다 괜찮았어요",
          "original_form": "전 2번째라 멀리 안가고 방콕이랑 그 주변만 다녀서 생각보다 괜찮았어요",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100032194.1.24",
          "form": "음식이 괜찮았어요",
          "original_form": "음식이 괜찮았어요",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100032194.1.25",
          "form": "맞아요!!!! 전 뿌빳뽕커리가 맛있더라구요..ㅋㅋ",
          "original_form": "맞아요!!!! 전 뿌빳뽕커리가 맛있더라구요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100032194.1.26",
          "form": "뭐가 좋으셨나요?",
          "original_form": "뭐가 좋으셨나요?",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100032194.1.27",
          "form": "팟프랏캐오?",
          "original_form": "팟프랏캐오?",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100032194.1.28",
          "form": "고기 볶음밥인데",
          "original_form": "고기 볶음밥인데",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100032194.1.29",
          "form": "기찻길시장",
          "original_form": "기찻길시장",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100032194.1.30",
          "form": "에서 먹었는데 가성비 최고였어요",
          "original_form": "에서 먹었는데 가성비 최고였어요",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100032194.1.31",
          "form": "기찻길시장 들어봤어요!!!!!",
          "original_form": "기찻길시장 들어봤어요!!!!!",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100032194.1.32",
          "form": "ㅋㅋ신기해요",
          "original_form": "ㅋㅋ신기해요",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100032194.1.33",
