      "utterance": [
        {
          "id": "MDRW2100026367.1.1",
          "form": "기분좋게 여행이야기해요!",
          "original_form": "기분좋게 여행이야기해요!",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026367.1.2",
          "form": "여행가는거 좋아하세요?",
          "original_form": "여행가는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026367.1.3",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026367.1.4",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026367.1.5",
          "form": "네! 완전 좋아해요",
          "original_form": "네! 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026367.1.6",
          "form": "국내 여행지 어디 가보셨어요?",
          "original_form": "국내 여행지 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026367.1.7",
          "form": "저는 안동, 경주, 전주, 광주, 포항, 강릉,하동",
          "original_form": "저는 안동, 경주, 전주, 광주, 포항, 강릉,하동",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026367.1.8",
          "form": "기억나는건 이정도예요",
          "original_form": "기억나는건 이정도예요",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026367.1.9",
          "form": "어디가보셨나요?",
          "original_form": "어디가보셨나요?",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026367.1.10",
          "form": "우와 많이 가보셨네요!!",
          "original_form": "우와 많이 가보셨네요!!",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026367.1.11",
          "form": "저는 부산이랑 여수랑 강릉이 기억나요",
          "original_form": "저는 부산이랑 여수랑 강릉이 기억나요",
          "speaker_id": "2",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026367.1.12",
          "form": "부산바다는 언제봐도 좋더라고요",
          "original_form": "부산바다는 언제봐도 좋더라고요",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026367.1.13",
          "form": "저 부산사람이예요",
          "original_form": "저 부산사람이예요",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026367.1.14",
          "form": "부산바다 좋죠",
          "original_form": "부산바다 좋죠",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026367.1.15",
          "form": "어디가셨나요?",
          "original_form": "어디가셨나요?",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026367.1.16",
          "form": "오!! 전 송정이랑 그 다른곳 하나랑 해운대랑 갔었는데",
          "original_form": "오!! 전 송정이랑 그 다른곳 하나랑 해운대랑 갔었는데",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026367.1.17",
          "form": "해운대가 좋더라고요",
          "original_form": "해운대가 좋더라고요",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026367.1.18",
          "form": "해운대가 좋으셨구나!",
          "original_form": "해운대가 좋으셨구나!",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026367.1.19",
          "form": "저는 개인적으로 광안리좋아해요",
          "original_form": "저는 개인적으로 광안리좋아해요",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026367.1.20",
          "form": "부산 자주오시나요?",
          "original_form": "부산 자주오시나요?",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026367.1.21",
          "form": "아 광안리 저 다른곳이 광안리였어요",
          "original_form": "아 광안리 저 다른곳이 광안리였어요",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026367.1.22",
          "form": "종종 가요",
          "original_form": "종종 가요",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026367.1.23",
          "form": "광안리가 사진은 제일 잘나오더라그요",
          "original_form": "광안리가 사진은 제일 잘나오더라그요",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026367.1.24",
          "form": "그쵸 저 자취하느라 서울에 있어서",
          "original_form": "그쵸 저 자취하느라 서울에 있어서",
          "speaker_id": "1",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026367.1.25",
          "form": "최신정보는 모르긴한데",
          "original_form": "최신정보는 모르긴한데",
          "speaker_id": "1",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026367.1.26",
          "form": "맛집이나 가볼만한곳 추천드릴까요?",
          "original_form": "맛집이나 가볼만한곳 추천드릴까요?",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026367.1.27",
          "form": "오 네! 추천해주세요!!",
          "original_form": "오 네! 추천해주세요!!",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026367.1.28",
          "form": "부산역으로 주로 오시니까 부산역은 그집 이라는 곱도리탕  맛집이 있어서 거기서 한끼드시구요",
          "original_form": "부산역으로 주로 오시니까 부산역은 그집 이라는 곱도리탕  맛집이 있어서 거기서 한끼드시구요",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026367.1.29",
          "form": "대신 거기는 서비스가 좀 애매해요",
          "original_form": "대신 거기는 서비스가 좀 애매해요",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026367.1.30",
          "form": "맛집의 배짱아시나요...",
          "original_form": "맛집의 배짱아시나요...",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026367.1.31",
          "form": "그런게 좀 있어서...",
          "original_form": "그런게 좀 있어서...",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026367.1.32",
          "form": "아 맞아요 맛집은 약간... 서비스가 미쓰여도",
          "original_form": "아 맞아요 맛집은 약간... 서비스가 미쓰여도",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026367.1.33",
          "form": "오는 사람들이 있으니까...",
          "original_form": "오는 사람들이 있으니까...",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026367.1.34",
          "form": "아 호떡집 유명하신곳 아세요?",
          "original_form": "아 호떡집 유명하신곳 아세요?",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026367.1.35",
          "form": "떡볶이도 같이 하는집인데",
          "original_form": "떡볶이도 같이 하는집인데",
          "speaker_id": "2",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026367.1.36",
          "form": "헉 어디요?",
          "original_form": "헉 어디요?",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026367.1.37",
          "form": "동래쪽이었던거같은데",
          "original_form": "동래쪽이었던거같은데",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026367.1.38",
          "form": "신가네 호떡 떡볶이 김밥이었나",
          "original_form": "신가네 호떡 떡볶이 김밥이었나",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026367.1.39",
          "form": "여기는 제 인생 호떡집으로 등극했어요",
          "original_form": "여기는 제 인생 호떡집으로 등극했어요",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026367.1.40",
          "form": "분식을 안좋아해서 잘모르나봐요",
          "original_form": "분식을 안좋아해서 잘모르나봐요",
          "speaker_id": "1",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026367.1.41",
          "form": "모르는곳이예요",
          "original_form": "모르는곳이예요",
          "speaker_id": "1",
          "time": "20210901 22:05"
        }
      ]
    }
