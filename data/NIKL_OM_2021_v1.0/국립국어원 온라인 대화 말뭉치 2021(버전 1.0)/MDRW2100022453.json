      "utterance": [
        {
          "id": "MDRW2100022453.1.1",
          "form": "이번주말이요",
          "original_form": "이번주말이요",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022453.1.2",
          "form": "두근거리시겠네요",
          "original_form": "두근거리시겠네요",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022453.1.3",
          "form": "네 살짝 건강 걱정도 되고",
          "original_form": "네 살짝 건강 걱정도 되고",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100022453.1.4",
          "form": "전 어제 맞아서",
          "original_form": "전 어제 맞아서",
          "speaker_id": "1",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100022453.1.5",
          "form": "아직도 멀었네요",
          "original_form": "아직도 멀었네요",
          "speaker_id": "1",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100022453.1.6",
          "form": "오늘 저녁은 열 나지 않고 건강히 지나가시길 바라요!!",
          "original_form": "오늘 저녁은 열 나지 않고 건강히 지나가시길 바라요!!",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100022453.1.7",
          "form": "저는 건강한 사람들이 더 아프대서",
          "original_form": "저는 건강한 사람들이 더 아프대서",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100022453.1.8",
          "form": "이번주는 운동도 쉬고 백신 후유증을 무사히 넘길 준비만 하려고요ㅎㅎ",
          "original_form": "이번주는 운동도 쉬고 백신 후유증을 무사히 넘길 준비만 하려고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100022453.1.9",
          "form": "거의 대다수 사람이 아프다고 하니",
          "original_form": "거의 대다수 사람이 아프다고 하니",
          "speaker_id": "1",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100022453.1.10",
          "form": "조심하셔야죠",
          "original_form": "조심하셔야죠",
          "speaker_id": "1",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100022453.1.11",
          "form": "푹 쉬세요",
          "original_form": "푹 쉬세요",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022453.1.12",
          "form": "전 2차가 월요일이라서",
          "original_form": "전 2차가 월요일이라서",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022453.1.13",
          "form": "큰일입니다",
          "original_form": "큰일입니다",
          "speaker_id": "1",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022453.1.14",
          "form": "어이쿠 월요일이면",
          "original_form": "어이쿠 월요일이면",
          "speaker_id": "2",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022453.1.15",
          "form": "출근하셔야 하나요?",
          "original_form": "출근하셔야 하나요?",
          "speaker_id": "2",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100022453.1.16",
          "form": "네 그래서 연차쓸까 싶기도 하고",
          "original_form": "네 그래서 연차쓸까 싶기도 하고",
          "speaker_id": "1",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100022453.1.17",
          "form": "저도 금요일 오전이라 반나절 공가 쓰고",
          "original_form": "저도 금요일 오전이라 반나절 공가 쓰고",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022453.1.18",
          "form": "몇일 앞으로 땡기는거 가능한지 물어보려구요",
          "original_form": "몇일 앞으로 땡기는거 가능한지 물어보려구요",
          "speaker_id": "1",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022453.1.19",
          "form": "10월이니",
          "original_form": "10월이니",
          "speaker_id": "1",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022453.1.20",
          "form": "오후엔 출근해야해요ㅜ",
          "original_form": "오후엔 출근해야해요ㅜ",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022453.1.21",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "1",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022453.1.22",
          "form": "가능하시겠어요?",
          "original_form": "가능하시겠어요?",
          "speaker_id": "1",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022453.1.23",
          "form": "아프면 다시 조퇴하라고는 하던데",
          "original_form": "아프면 다시 조퇴하라고는 하던데",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022453.1.24",
          "form": "걱정도 되고",
          "original_form": "걱정도 되고",
          "speaker_id": "2",
          "time": "20210829 14:43"
        },
        {
          "id": "MDRW2100022453.1.25",
          "form": "사실 늦게 출근하기도 귀찮고ㅜ 아플까 걱정도 되고요",
          "original_form": "사실 늦게 출근하기도 귀찮고ㅜ 아플까 걱정도 되고요",
          "speaker_id": "2",
          "time": "20210829 14:44"
        },
        {
          "id": "MDRW2100022453.1.26",
          "form": "백신 맞으라면서 일은 일대로 해야되고",
          "original_form": "백신 맞으라면서 일은 일대로 해야되고",
          "speaker_id": "1",
          "time": "20210829 14:44"
        },
        {
          "id": "MDRW2100022453.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 14:44"
        },
        {
          "id": "MDRW2100022453.1.28",
          "form": "그러게 말에요",
          "original_form": "그러게 말에요",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022453.1.29",
          "form": "건강하려고 맞는 건데",
          "original_form": "건강하려고 맞는 건데",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022453.1.30",
          "form": "오히려 더 아프기도 하고",
          "original_form": "오히려 더 아프기도 하고",
          "speaker_id": "2",
          "time": "20210829 15:11"
        }
      ]
    }
  ]
