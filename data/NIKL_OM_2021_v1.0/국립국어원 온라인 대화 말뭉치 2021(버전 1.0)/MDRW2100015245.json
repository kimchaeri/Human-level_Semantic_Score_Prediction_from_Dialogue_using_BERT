      "utterance": [
        {
          "id": "MDRW2100015245.1.1",
          "form": "여행 계획 있으신가요",
          "original_form": "여행 계획 있으신가요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015245.1.2",
          "form": "지금 당장은 없어요ㅜㅜ",
          "original_form": "지금 당장은 없어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015245.1.3",
          "form": "여행계획 있으신가요?",
          "original_form": "여행계획 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015245.1.4",
          "form": "저도 당장은 없는데 코로나 끝나면 해외여행 가고 싶어요",
          "original_form": "저도 당장은 없는데 코로나 끝나면 해외여행 가고 싶어요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015245.1.5",
          "form": "유럽 못 가봐서 유럽 여행 가는게 목표에요",
          "original_form": "유럽 못 가봐서 유럽 여행 가는게 목표에요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015245.1.6",
          "form": "어디 가 보고 싶은곳 있으신가요",
          "original_form": "어디 가 보고 싶은곳 있으신가요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015245.1.7",
          "form": "원래 홍콩을 가고싶었는데",
          "original_form": "원래 홍콩을 가고싶었는데",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015245.1.8",
          "form": "벗.... 중국이 되어버린....",
          "original_form": "벗.... 중국이 되어버린....",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015245.1.9",
          "form": "해외 여행은 가고싶은데 딱히 생각나는 나라는 없네요",
          "original_form": "해외 여행은 가고싶은데 딱히 생각나는 나라는 없네요",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015245.1.10",
          "form": "앗.....ㅜㅜ",
          "original_form": "앗.....ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015245.1.11",
          "form": "그러고보니 홍콩도 항상 가봐야지 가봐야지 했는데",
          "original_form": "그러고보니 홍콩도 항상 가봐야지 가봐야지 했는데",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015245.1.12",
          "form": "타이밍을 놓쳤네요ㅠㅠ",
          "original_form": "타이밍을 놓쳤네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015245.1.13",
          "form": "ㅜㅜㅜ 이제 못가요....",
          "original_form": "ㅜㅜㅜ 이제 못가요....",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015245.1.14",
          "form": "ㅜㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015245.1.15",
          "form": "중국은 가고싶지 않네요..",
          "original_form": "중국은 가고싶지 않네요..",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015245.1.16",
          "form": "이제 홍콩 가 봤자 같은 느낌은 절대 안 나겠죠ㅠㅠ",
          "original_form": "이제 홍콩 가 봤자 같은 느낌은 절대 안 나겠죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015245.1.17",
          "form": "옛날엔 중국도 한번 가 보고 싶었는데",
          "original_form": "옛날엔 중국도 한번 가 보고 싶었는데",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015245.1.18",
          "form": "요즘은 약간 꺼림찍해요",
          "original_form": "요즘은 약간 꺼림찍해요",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015245.1.19",
          "form": "갈수록 그렇죠...",
          "original_form": "갈수록 그렇죠...",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015245.1.20",
          "form": "비자 내는 순간 내 개인정보는 중국 것이 될 것 같은",
          "original_form": "비자 내는 순간 내 개인정보는 중국 것이 될 것 같은",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015245.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015245.1.22",
          "form": "유럽은 혹시 어디 가고싶으세요?",
          "original_form": "유럽은 혹시 어디 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015245.1.23",
          "form": "앗.....ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗.....ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015245.1.24",
          "form": "유럽에 스위스 가서",
          "original_form": "유럽에 스위스 가서",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015245.1.25",
          "form": "트래킹 한번 해 보고 싶어요",
          "original_form": "트래킹 한번 해 보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015245.1.26",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015245.1.27",
          "form": "저는 유럽은 갔는데 스위스는 못갔어요",
          "original_form": "저는 유럽은 갔는데 스위스는 못갔어요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015245.1.28",
          "form": "스위스 넘 비싸서",
          "original_form": "스위스 넘 비싸서",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015245.1.29",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015245.1.30",
          "form": "스위스 물가 진짜 비싸기로 유명하죠ㅠㅠ",
          "original_form": "스위스 물가 진짜 비싸기로 유명하죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015245.1.31",
          "form": "그래도 스위스에 마테호른 한번 보고 트래킹 하는게 평생 소원(?)이라 언젠가 한번 갈것 같아요",
          "original_form": "그래도 스위스에 마테호른 한번 보고 트래킹 하는게 평생 소원(?)이라 언젠가 한번 갈것 같아요",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015245.1.32",
          "form": "알프스에서 먹는 컵라면 맛도 궁굼하구요ㅋㅋㅋ",
          "original_form": "알프스에서 먹는 컵라면 맛도 궁굼하구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015245.1.33",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015245.1.34",
          "form": "육개장 사발면?",
          "original_form": "육개장 사발면?",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015245.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015245.1.36",
          "form": "소원이시라면 돈을 열심히 모아서",
          "original_form": "소원이시라면 돈을 열심히 모아서",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015245.1.37",
          "form": "신라면 판다고 하더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "신라면 판다고 하더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015245.1.38",
          "form": "스위스에서 탕진해봅시다!",
          "original_form": "스위스에서 탕진해봅시다!",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015245.1.39",
          "form": "저는 스위스 갈 생각을 못해서 못갔어요ㅠㅠ",
          "original_form": "저는 스위스 갈 생각을 못해서 못갔어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015245.1.40",
          "form": "좋아요!!",
          "original_form": "좋아요!!",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015245.1.41",
          "form": "유럽에 워낙 갈데가 많으니깐요ㅠㅠ",
          "original_form": "유럽에 워낙 갈데가 많으니깐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015245.1.42",
          "form": "근데 가본 사람들은 다들 좋다고 하더라구요",
          "original_form": "근데 가본 사람들은 다들 좋다고 하더라구요",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015245.1.43",
          "form": "신라면 값도 준비하셔야겠네요",
          "original_form": "신라면 값도 준비하셔야겠네요",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015245.1.44",
          "form": "맞아요ㅋㅋㅋㅋ 돈 많이 모아야 해요",
          "original_form": "맞아요ㅋㅋㅋㅋ 돈 많이 모아야 해요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015245.1.45",
          "form": "이제 돈 모으면 여행 갈 시간이 없는게 문제겠지만요",
          "original_form": "이제 돈 모으면 여행 갈 시간이 없는게 문제겠지만요",
          "speaker_id": "1",
          "time": "20210825 22:36"
        }
      ]
    }
