        },
        {
          "id": "MDRW2100020118.1.2",
          "form": "장윤정이 결혼한 거 알고 있니",
          "original_form": "장윤정이 결혼한 거 알고 있니",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020118.1.3",
          "form": "당연하지 나 슈퍼맨이 돌아왔다 도 챙겨 봤어",
          "original_form": "당연하지 나 슈퍼맨이 돌아왔다 도 챙겨 봤어",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020118.1.4",
          "form": "거기 나오는 장윤정 아이들 진짜 귀여워",
          "original_form": "거기 나오는 장윤정 아이들 진짜 귀여워",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020118.1.5",
          "form": "하영이 너무 귀여워",
          "original_form": "하영이 너무 귀여워",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020118.1.6",
          "form": "큰애인 연우 도 귀엽지 않아",
          "original_form": "큰애인 연우 도 귀엽지 않아",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020118.1.7",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020118.1.8",
          "form": "맞아 엄청 착한 오빠 라서 부럽더라",
          "original_form": "맞아 엄청 착한 오빠 라서 부럽더라",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020118.1.9",
          "form": "하영이는 말을 잘 정말 잘하더라",
          "original_form": "하영이는 말을 잘 정말 잘하더라",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020118.1.10",
          "form": "그런 남매 있으면 행복하겠다",
          "original_form": "그런 남매 있으면 행복하겠다",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020118.1.11",
          "form": "하영이가 착해서 둘이 사이가 좋은가봐",
          "original_form": "하영이가 착해서 둘이 사이가 좋은가봐",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020118.1.12",
          "form": "근데 하영이랑 장윤정이랑 진짜 닮았더라",
          "original_form": "근데 하영이랑 장윤정이랑 진짜 닮았더라",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020118.1.13",
          "form": "맞아 둘이 웃는 게 똑같아",
          "original_form": "맞아 둘이 웃는 게 똑같아",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020118.1.14",
          "form": "요즘도 아나운서가 유튜브 도 한데",
          "original_form": "요즘도 아나운서가 유튜브 도 한데",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020118.1.15",
          "form": "도경완",
          "original_form": "도경완",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020118.1.16",
          "form": "몰랐네 구독해야겠다",
          "original_form": "몰랐네 구독해야겠다",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020118.1.17",
          "form": "꼭 봐하영이 진짜 귀여워",
          "original_form": "꼭 봐하영이 진짜 귀여워",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020118.1.18",
          "form": "그새 많이 컸겠는데",
          "original_form": "그새 많이 컸겠는데",
