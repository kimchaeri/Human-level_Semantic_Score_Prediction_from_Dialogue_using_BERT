      "utterance": [
        {
          "id": "MDRW2100044770.1.1",
          "form": "와 이것 부업이라 생각하고 여유롭게 했느뉘ㅣ",
          "original_form": "와 이것 부업이라 생각하고 여유롭게 했느뉘ㅣ",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044770.1.2",
          "form": "했는데",
          "original_form": "했는데",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044770.1.3",
          "form": "마감시간이 당겨지니 힘드네요ㅜ",
          "original_form": "마감시간이 당겨지니 힘드네요ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044770.1.4",
          "form": "ㅋㅋㅋㅋ부업 충분히 하셨나요?",
          "original_form": "ㅋㅋㅋㅋ부업 충분히 하셨나요?",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044770.1.5",
          "form": "저도그래요",
          "original_form": "저도그래요",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044770.1.6",
          "form": "저는 이제 8개정도",
          "original_form": "저는 이제 8개정도",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044770.1.7",
          "form": "남았어요",
          "original_form": "남았어요",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044770.1.8",
          "form": "많이 남으셨나요?",
          "original_form": "많이 남으셨나요?",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044770.1.9",
          "form": "저는 대여섯개요!",
          "original_form": "저는 대여섯개요!",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044770.1.10",
          "form": "비슷하네요ㅋㅋ",
          "original_form": "비슷하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044770.1.11",
          "form": "내일 출근 있으세요?",
          "original_form": "내일 출근 있으세요?",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044770.1.12",
          "form": "네 출근도 하고",
          "original_form": "네 출근도 하고",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044770.1.13",
          "form": "오전에 행사도 있어요ㅋㅋ",
          "original_form": "오전에 행사도 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044770.1.14",
          "form": "ㅋㅋㅋㄱㄱㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044770.1.15",
          "form": "지금 마지막 불꽃이신가요ㅋㅋ",
          "original_form": "지금 마지막 불꽃이신가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044770.1.16",
          "form": "네 내일 아침엔 본업땜에 바쁠 예정이라",
          "original_form": "네 내일 아침엔 본업땜에 바쁠 예정이라",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044770.1.17",
          "form": "전 출근은하지만 행사는 없는데요!!",
          "original_form": "전 출근은하지만 행사는 없는데요!!",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044770.1.18",
          "form": "ㅋㅋ오늘 다하고 주무셔야겠네요",
          "original_form": "ㅋㅋ오늘 다하고 주무셔야겠네요",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044770.1.19",
          "form": "네네 막판스퍼트 내고",
          "original_form": "네네 막판스퍼트 내고",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044770.1.20",
          "form": "맘편히 자야죠",
          "original_form": "맘편히 자야죠",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
