      "utterance": [
        {
          "id": "MDRW2100002518.1.1",
          "form": "요즘 운동 하는거 있어?",
          "original_form": "요즘 운동 하는거 있어?",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002518.1.2",
          "form": "헬스같은경우는 코로나땜에",
          "original_form": "헬스같은경우는 코로나땜에",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002518.1.3",
          "form": "못다니고 있고",
          "original_form": "못다니고 있고",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002518.1.4",
          "form": "이제 홈트?",
          "original_form": "이제 홈트?",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002518.1.5",
          "form": "요즘 집에서 하는 운동 하고잇어",
          "original_form": "요즘 집에서 하는 운동 하고잇어",
          "speaker_id": "2",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002518.1.6",
          "form": "유튜브에 엄청 많더라",
          "original_form": "유튜브에 엄청 많더라",
          "speaker_id": "2",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002518.1.7",
          "form": "운동 어떻게 하고 있는데?",
          "original_form": "운동 어떻게 하고 있는데?",
          "speaker_id": "1",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002518.1.8",
          "form": "내가 요근래에 뱃살이 고민인지라",
          "original_form": "내가 요근래에 뱃살이 고민인지라",
          "speaker_id": "2",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002518.1.9",
          "form": "코어 운동이나 뱃살빼는 위주로 하고있지",
          "original_form": "코어 운동이나 뱃살빼는 위주로 하고있지",
          "speaker_id": "2",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002518.1.10",
          "form": "뱃살이 늦게 빠진다고 들었는데",
          "original_form": "뱃살이 늦게 빠진다고 들었는데",
          "speaker_id": "1",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002518.1.11",
          "form": "안힘들어?",
          "original_form": "안힘들어?",
          "speaker_id": "1",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002518.1.12",
          "form": "안그래도",
          "original_form": "안그래도",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002518.1.13",
          "form": "몇달동안 하고있는데",
          "original_form": "몇달동안 하고있는데",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002518.1.14",
          "form": "엄청 힘들더라",
          "original_form": "엄청 힘들더라",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002518.1.15",
          "form": "먹는것도 조절하면서 하고있는데",
          "original_form": "먹는것도 조절하면서 하고있는데",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002518.1.16",
          "form": "조금씩은 빠지고 있는거같아",
          "original_form": "조금씩은 빠지고 있는거같아",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002518.1.17",
          "form": "근데 힘들긴해",
          "original_form": "근데 힘들긴해",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002518.1.18",
          "form": "그래도 살빠지는건 좋은 일이네",
          "original_form": "그래도 살빠지는건 좋은 일이네",
          "speaker_id": "1",
          "time": "20210820 13:09"
        },
        {
          "id": "MDRW2100002518.1.19",
          "form": "나처럼 운동 아에 안하는 것 보단 낫다",
          "original_form": "나처럼 운동 아에 안하는 것 보단 낫다",
          "speaker_id": "1",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002518.1.20",
          "form": "다른 운동 하는건 있어?",
          "original_form": "다른 운동 하는건 있어?",
          "speaker_id": "1",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002518.1.21",
          "form": "요가도 하고있어!",
          "original_form": "요가도 하고있어!",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002518.1.22",
          "form": "짐볼을 이용한 요가같은거 주로해",
          "original_form": "짐볼을 이용한 요가같은거 주로해",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002518.1.23",
          "form": "그거 유연성 있어야 되지 않아?",
          "original_form": "그거 유연성 있어야 되지 않아?",
          "speaker_id": "1",
          "time": "20210820 13:11"
        },
        {
          "id": "MDRW2100002518.1.24",
          "form": "유연성이 좀 있는편이라",
          "original_form": "유연성이 좀 있는편이라",
          "speaker_id": "2",
          "time": "20210820 13:12"
        },
        {
          "id": "MDRW2100002518.1.25",
          "form": "그래도 따라갈 정도는 하고있어",
          "original_form": "그래도 따라갈 정도는 하고있어",
          "speaker_id": "2",
          "time": "20210820 13:12"
        },
        {
          "id": "MDRW2100002518.1.26",
          "form": "운동에 쉬운게 하나없더라 으",
          "original_form": "운동에 쉬운게 하나없더라 으",
          "speaker_id": "2",
          "time": "20210820 13:12"
        },
        {
          "id": "MDRW2100002518.1.27",
          "form": "운동이 쉬웠으면 개나소나 다하지",
          "original_form": "운동이 쉬웠으면 개나소나 다하지",
          "speaker_id": "1",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002518.1.28",
          "form": "살빼는게 쉬우면 막 먹었겠지",
          "original_form": "살빼는게 쉬우면 막 먹었겠지",
          "speaker_id": "1",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002518.1.29",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002518.1.30",
          "form": "먹는건 쉬운데 빼는게 어려워!!!!",
          "original_form": "먹는건 쉬운데 빼는게 어려워!!!!",
          "speaker_id": "2",
          "time": "20210820 13:15"
        },
        {
          "id": "MDRW2100002518.1.31",
          "form": "먹는만큼 운동해라",
          "original_form": "먹는만큼 운동해라",
          "speaker_id": "1",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100002518.1.32",
          "form": "고통 그자체",
          "original_form": "고통 그자체",
          "speaker_id": "1",
          "time": "20210820 13:16"
        }
      ]
    }
