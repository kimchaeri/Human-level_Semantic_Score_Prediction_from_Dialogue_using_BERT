      "utterance": [
        {
          "id": "MDRW2100003642.1.1",
          "form": "안녕하세요 요즘 운동하시는거 있으세요?",
          "original_form": "안녕하세요 요즘 운동하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210821 09:38"
        },
        {
          "id": "MDRW2100003642.1.2",
          "form": "저 요즘 숨만쉬고있어요 ㅋㅋ",
          "original_form": "저 요즘 숨만쉬고있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:38"
        },
        {
          "id": "MDRW2100003642.1.3",
          "form": "넘 더워서 운동할 엄두가 안나더라고요",
          "original_form": "넘 더워서 운동할 엄두가 안나더라고요",
          "speaker_id": "2",
          "time": "20210821 09:38"
        },
        {
          "id": "MDRW2100003642.1.4",
          "form": "저도 운동해야하는데 산책정도만 겨우하고있어요ㅎㅎ",
          "original_form": "저도 운동해야하는데 산책정도만 겨우하고있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 09:39"
        },
        {
          "id": "MDRW2100003642.1.5",
          "form": "ㅋㅋ 운동하는걸 워낙 싫어해서",
          "original_form": "ㅋㅋ 운동하는걸 워낙 싫어해서",
          "speaker_id": "2",
          "time": "20210821 09:39"
        },
        {
          "id": "MDRW2100003642.1.6",
          "form": "근데 이제 밤산책 정도는 하려고요",
          "original_form": "근데 이제 밤산책 정도는 하려고요",
          "speaker_id": "2",
          "time": "20210821 09:39"
        },
        {
          "id": "MDRW2100003642.1.7",
          "form": "요즘 밤에 시원해서 산책 좋아요ㅎㅎ 산책하는 습관길러서 런닝도 해보려구요",
          "original_form": "요즘 밤에 시원해서 산책 좋아요ㅎㅎ 산책하는 습관길러서 런닝도 해보려구요",
          "speaker_id": "1",
          "time": "20210821 09:40"
        },
        {
          "id": "MDRW2100003642.1.8",
          "form": "저도 넘 운동을 안해서 밤산책이랑 스트레칭은 꼭 하려고요",
          "original_form": "저도 넘 운동을 안해서 밤산책이랑 스트레칭은 꼭 하려고요",
          "speaker_id": "2",
          "time": "20210821 09:41"
        },
        {
          "id": "MDRW2100003642.1.9",
          "form": "전엔 운동을 찾아서 했는데 이젠 넘 귀찮아요",
          "original_form": "전엔 운동을 찾아서 했는데 이젠 넘 귀찮아요",
          "speaker_id": "2",
          "time": "20210821 09:41"
        },
        {
          "id": "MDRW2100003642.1.10",
          "form": "저도 너무 안해서 홈트하려고 이것저것 샀는데 집안에선 도저히ㅋㅋㅋ안하게되네요",
          "original_form": "저도 너무 안해서 홈트하려고 이것저것 샀는데 집안에선 도저히ㅋㅋㅋ안하게되네요",
          "speaker_id": "1",
          "time": "20210821 09:41"
        },
        {
          "id": "MDRW2100003642.1.11",
          "form": "그쵸? ㅎㅎ",
          "original_form": "그쵸? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 09:42"
        },
        {
          "id": "MDRW2100003642.1.12",
          "form": "바로 누워버리는 ㅋㅋㅋㅋㅋ",
          "original_form": "바로 누워버리는 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:42"
        },
        {
          "id": "MDRW2100003642.1.13",
          "form": "저는 스쿼트 참 재밌게 했었어요",
          "original_form": "저는 스쿼트 참 재밌게 했었어요",
          "speaker_id": "2",
          "time": "20210821 09:42"
        },
        {
          "id": "MDRW2100003642.1.14",
          "form": "스쿼트 ㅠㅠ 하루에 백개씩 하는걸 목표로 했었는데 진짜 쉽지않더라구요ㅋㅋ",
          "original_form": "스쿼트 ㅠㅠ 하루에 백개씩 하는걸 목표로 했었는데 진짜 쉽지않더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 09:43"
        },
        {
          "id": "MDRW2100003642.1.15",
          "form": "어머 저 스쿼시를 잘못쳤어요 ㅋㅋ",
          "original_form": "어머 저 스쿼시를 잘못쳤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:44"
        },
        {
          "id": "MDRW2100003642.1.16",
          "form": "스쿼트는 훠이 ㅋㅋㅋㅋㅋ",
          "original_form": "스쿼트는 훠이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:44"
        },
        {
          "id": "MDRW2100003642.1.17",
          "form": "스쿼시는 재밌어보여요ㅋㅋ스쿼트는 전혀 재미없는ㅠㅠㅠㅋㅋ",
          "original_form": "스쿼시는 재밌어보여요ㅋㅋ스쿼트는 전혀 재미없는ㅠㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 09:45"
        },
        {
          "id": "MDRW2100003642.1.18",
          "form": "그니까요 왠 막말 ㅎㅎ",
          "original_form": "그니까요 왠 막말 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 09:45"
        },
        {
          "id": "MDRW2100003642.1.19",
          "form": "스쿼시였어요",
          "original_form": "스쿼시였어요",
          "speaker_id": "2",
          "time": "20210821 09:45"
        },
        {
          "id": "MDRW2100003642.1.20",
          "form": "그건 재밌더라고요",
          "original_form": "그건 재밌더라고요",
          "speaker_id": "2",
          "time": "20210821 09:45"
        },
        {
          "id": "MDRW2100003642.1.21",
          "form": "ㅋㅋㅋ테니스 비슷한 운동같아보여요",
          "original_form": "ㅋㅋㅋ테니스 비슷한 운동같아보여요",
          "speaker_id": "1",
          "time": "20210821 09:45"
        }
      ]
    }
