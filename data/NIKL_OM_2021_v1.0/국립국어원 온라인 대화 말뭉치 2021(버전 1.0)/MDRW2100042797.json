      "utterance": [
        {
          "id": "MDRW2100042797.1.1",
          "form": "한 87 정도가도",
          "original_form": "한 87 정도가도",
          "speaker_id": "2",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042797.1.2",
          "form": "안 걸리는데",
          "original_form": "안 걸리는데",
          "speaker_id": "2",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042797.1.3",
          "form": "사람들이 속도를 정말 잘 지킨다",
          "original_form": "사람들이 속도를 정말 잘 지킨다",
          "speaker_id": "2",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042797.1.4",
          "form": "언니는 이 근처로 올 순 없어요?",
          "original_form": "언니는 이 근처로 올 순 없어요?",
          "speaker_id": "1",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042797.1.5",
          "form": "출퇴근 만 가까워도 참 좋은데",
          "original_form": "출퇴근 만 가까워도 참 좋은데",
          "speaker_id": "1",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042797.1.6",
          "form": "지금 내가 시간제근무 라고",
          "original_form": "지금 내가 시간제근무 라고",
          "speaker_id": "2",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042797.1.7",
          "form": "짧게 근무를 하고 있잖아",
          "original_form": "짧게 근무를 하고 있잖아",
          "speaker_id": "2",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042797.1.8",
          "form": "근데 달서 가",
          "original_form": "근데 달서 가",
          "speaker_id": "2",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042797.1.9",
          "form": "그 시간제 근무하기가 편하거든",
          "original_form": "그 시간제 근무하기가 편하거든",
          "speaker_id": "2",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042797.1.10",
          "form": "달성 은 촌이라서 시간제 쓰는 사람이 없어가지고",
          "original_form": "달성 은 촌이라서 시간제 쓰는 사람이 없어가지고",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.11",
          "form": "아이디 별로 아 일이 별로 없는갑네요",
          "original_form": "아이디 별로 아 일이 별로 없는갑네요",
          "speaker_id": "1",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.12",
          "form": "눈치 좀 보인다고 하더라고",
          "original_form": "눈치 좀 보인다고 하더라고",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.13",
          "form": "달서는 바쁜데",
          "original_form": "달서는 바쁜데",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.14",
          "form": "하긴 눈치 주면은 짜증남",
          "original_form": "하긴 눈치 주면은 짜증남",
          "speaker_id": "1",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.15",
          "form": "시간제 근무를",
          "original_form": "시간제 근무를",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.16",
          "form": "플러스 알파 같은 느낌으로 하고 있어서",
          "original_form": "플러스 알파 같은 느낌으로 하고 있어서",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.17",
          "form": "약간 깍두기 같은 느낌이거든",
          "original_form": "약간 깍두기 같은 느낌이거든",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.18",
          "form": "그럼 가벼운 마음으로 가면 되겠네요",
          "original_form": "그럼 가벼운 마음으로 가면 되겠네요",
          "speaker_id": "1",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.19",
          "form": "근데 달성은",
          "original_form": "근데 달성은",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.20",
          "form": "사람이 없어서",
          "original_form": "사람이 없어서",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.21",
          "form": "한 사람 모을 해야 되는 거네",
          "original_form": "한 사람 모을 해야 되는 거네",
          "speaker_id": "1",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.22",
          "form": "시간제 근무를 해도",
          "original_form": "시간제 근무를 해도",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.23",
          "form": "몫",
          "original_form": "몫",
          "speaker_id": "1",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.24",
          "form": "온 근무를 다 해야 돼",
          "original_form": "온 근무를 다 해야 돼",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.25",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.26",
          "form": "그건 좀 그렇네요",
          "original_form": "그건 좀 그렇네요",
          "speaker_id": "1",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.27",
          "form": "그래서 지금 도시에 있는 거여",
          "original_form": "그래서 지금 도시에 있는 거여",
          "speaker_id": "2",
          "time": "20210907 09:18"
        },
        {
          "id": "MDRW2100042797.1.28",
          "form": "촌이",
          "original_form": "촌이",
          "speaker_id": "1",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042797.1.29",
          "form": "아우",
          "original_form": "아우",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042797.1.30",
          "form": "사람들이야 박함",
          "original_form": "사람들이야 박함",
          "speaker_id": "1",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042797.1.31",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042797.1.32",
          "form": "촌에 사람이 없어가지고",
          "original_form": "촌에 사람이 없어가지고",
          "speaker_id": "2",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042797.1.33",
          "form": "시골 인심은 옛말 임ㅋㅋㅋㅋ",
          "original_form": "시골 인심은 옛말 임ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:19"
        }
      ]
