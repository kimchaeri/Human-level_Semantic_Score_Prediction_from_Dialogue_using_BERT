        },
        {
          "id": "MDRW2100027232.1.2",
          "form": "선선하죠?",
          "original_form": "선선하죠?",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027232.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027232.1.4",
          "form": "가을 다 됐죠",
          "original_form": "가을 다 됐죠",
          "speaker_id": "2",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027232.1.5",
          "form": "겉옷을챙겨야지여",
          "original_form": "겉옷을챙겨야지여",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027232.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027232.1.7",
          "form": "이제 겉옷 필수죠",
          "original_form": "이제 겉옷 필수죠",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027232.1.8",
          "form": "그냥 긴팔옷도 꺼냈어요",
          "original_form": "그냥 긴팔옷도 꺼냈어요",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027232.1.9",
          "form": "맞죠",
          "original_form": "맞죠",
          "speaker_id": "1",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100027232.1.10",
          "form": "좋아하는 옷 스타일 있으세요?",
          "original_form": "좋아하는 옷 스타일 있으세요?",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
