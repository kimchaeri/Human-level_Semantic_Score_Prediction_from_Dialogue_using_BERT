        {
          "id": "MDRW2100027232.1.15",
          "form": "가끔요",
          "original_form": "가끔요",
          "speaker_id": "1",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100027232.1.16",
          "form": "저도 옷을 자주 사는 편은 아니에요ㅋㅋ",
          "original_form": "저도 옷을 자주 사는 편은 아니에요ㅋㅋ",
