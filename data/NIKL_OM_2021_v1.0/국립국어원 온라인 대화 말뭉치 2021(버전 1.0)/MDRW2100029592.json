        },
        {
          "id": "MDRW2100029592.1.6",
          "form": "1년이요",
          "original_form": "1년이요",
          "speaker_id": "1",
          "time": "20210903 14:20"
        },
        {
          "id": "MDRW2100029592.1.7",
          "form": "오ㅎㅎ저랑같네요ㅋ",
          "original_form": "오ㅎㅎ저랑같네요ㅋ",
          "speaker_id": "2",
          "time": "20210903 15:03"
        },
        {
          "id": "MDRW2100029592.1.8",
          "form": "저는 애들 둘도있어요ㅋ",
          "original_form": "저는 애들 둘도있어요ㅋ",
          "speaker_id": "2",
          "time": "20210903 15:03"
        },
        {
          "id": "MDRW2100029592.1.9",
          "form": "아들 둘이오",
          "original_form": "아들 둘이오",
          "speaker_id": "1",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100029592.1.10",
          "form": "ㅎㅎ아고 저는 딸아들인데 둘다 활발해서ㅠ",
          "original_form": "ㅎㅎ아고 저는 딸아들인데 둘다 활발해서ㅠ",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029592.1.11",
          "form": "힘드네요ㅋㅋ",
          "original_form": "힘드네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029592.1.12",
