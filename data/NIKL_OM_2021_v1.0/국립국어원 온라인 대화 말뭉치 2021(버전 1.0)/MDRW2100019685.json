      "utterance": [
        {
          "id": "MDRW2100019685.1.1",
          "form": "크으으 콘치즈....콘치즈 자체도 너무 맛있는데 맛있음+맛있음인가봐요!ㅋㅋㅋㅋ",
          "original_form": "크으으 콘치즈....콘치즈 자체도 너무 맛있는데 맛있음+맛있음인가봐요!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019685.1.2",
          "form": "심지어 더블포크!!!!",
          "original_form": "심지어 더블포크!!!!",
          "speaker_id": "2",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019685.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019685.1.4",
          "form": "토핑이 실해서 좋아요!",
          "original_form": "토핑이 실해서 좋아요!",
          "speaker_id": "1",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019685.1.5",
          "form": "꼭 먹어볼게요 맛있는거 알려주셔서 감사해요",
          "original_form": "꼭 먹어볼게요 맛있는거 알려주셔서 감사해요",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019685.1.6",
          "form": "안 그래도 피자 먹고 싶었는데 ㅠㅠ",
          "original_form": "안 그래도 피자 먹고 싶었는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019685.1.7",
          "form": "하 강추입니다",
          "original_form": "하 강추입니다",
          "speaker_id": "1",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019685.1.8",
          "form": "하도 안 먹어서 뭘 먹어야할지 모르겠더라구요 ㅋㅋ",
          "original_form": "하도 안 먹어서 뭘 먹어야할지 모르겠더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019685.1.9",
          "form": "콘치즈를 꼭 많이 달라고 하셔용",
          "original_form": "콘치즈를 꼭 많이 달라고 하셔용",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019685.1.10",
          "form": "ㅋㅋㅋㅋㅋ 아 그거도 맛있어요!",
          "original_form": "ㅋㅋㅋㅋㅋ 아 그거도 맛있어요!",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019685.1.11",
          "form": "도미노 드세요?",
          "original_form": "도미노 드세요?",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019685.1.12",
          "form": "네!ㅋㅋㅋ 콘치즈 꼭 많이!!",
          "original_form": "네!ㅋㅋㅋ 콘치즈 꼭 많이!!",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019685.1.13",
          "form": "도미노요? 네 도미노 가까운데 있어요!",
          "original_form": "도미노요? 네 도미노 가까운데 있어요!",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019685.1.14",
          "form": "그 스타셰프 시그니처 맛나더라구요!",
          "original_form": "그 스타셰프 시그니처 맛나더라구요!",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019685.1.15",
          "form": "스파셰프 시그니처요? 새로 나온 건가요?!",
          "original_form": "스파셰프 시그니처요? 새로 나온 건가요?!",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019685.1.16",
          "form": "나온지 조금 된건데 아직도 파는지는 모르겠어요 ㅋㅋㅋ",
          "original_form": "나온지 조금 된건데 아직도 파는지는 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019685.1.17",
          "form": "스테이크가 많아서 좋아요",
          "original_form": "스테이크가 많아서 좋아요",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019685.1.18",
          "form": "있네요! 방금 찾아봤더니 ㅋㅋㅋ",
          "original_form": "있네요! 방금 찾아봤더니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019685.1.19",
          "form": "스테이크 맛있죠ㅠㅠㅠㅠ",
          "original_form": "스테이크 맛있죠ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019685.1.20",
          "form": "와 아직 파는군요 ㅋㅋㅋ",
          "original_form": "와 아직 파는군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100019685.1.21",
          "form": "전 고기들어간 피자가 좋더라구요",
          "original_form": "전 고기들어간 피자가 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100019685.1.22",
          "form": "저두요!!! 고기가 최고 아닌가요???",
          "original_form": "저두요!!! 고기가 최고 아닌가요???",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
