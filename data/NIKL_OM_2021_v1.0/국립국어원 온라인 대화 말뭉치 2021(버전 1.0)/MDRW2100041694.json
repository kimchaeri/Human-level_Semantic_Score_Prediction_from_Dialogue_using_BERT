      "utterance": [
        {
          "id": "MDRW2100041694.1.1",
          "form": "마지막에 안된다 카면 기분이 나쁘지 ㅋㅋㅋㅋ",
          "original_form": "마지막에 안된다 카면 기분이 나쁘지 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041694.1.2",
          "form": "오늘은 알바갔다옴?",
          "original_form": "오늘은 알바갔다옴?",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041694.1.3",
          "form": "응응 알바 다녀왔지",
          "original_form": "응응 알바 다녀왔지",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041694.1.4",
          "form": "해피오더 하고싶었는데 ㅋㅋ",
          "original_form": "해피오더 하고싶었는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041694.1.5",
          "form": "하지도 못하구 ㅋㅋ 일했다",
          "original_form": "하지도 못하구 ㅋㅋ 일했다",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041694.1.6",
          "form": "속터지드라",
          "original_form": "속터지드라",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041694.1.7",
          "form": "그러니깐 카톡보니",
          "original_form": "그러니깐 카톡보니",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041694.1.8",
          "form": "뭐 내가 했었어도 못했겟다 싶더라고 ㅋㅋㅋㅋ 속만 터지고....",
          "original_form": "뭐 내가 했었어도 못했겟다 싶더라고 ㅋㅋㅋㅋ 속만 터지고....",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041694.1.9",
          "form": "헬이었음 name3꺼 봐주는데",
          "original_form": "헬이었음 &name3&꺼 봐주는데",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041694.1.10",
          "form": "ㅋㅋㅋㅋ그러니깐 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그러니깐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041694.1.11",
          "form": "니도 대단하다 ㅋㅋㅋ",
          "original_form": "니도 대단하다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041694.1.12",
          "form": "나는 다해서 더 안되가지고ㅠ 로그아웃해서 들어가보니까 로그인 자체가 안됨",
          "original_form": "나는 다해서 더 안되가지고ㅠ 로그아웃해서 들어가보니까 로그인 자체가 안됨",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041694.1.13",
          "form": "그거보느라 애밥도 늦게 먹이고",
          "original_form": "그거보느라 애밥도 늦게 먹이고",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041694.1.14",
          "form": "아아아~~~~~3개까지가?",
          "original_form": "아아아~~~~~3개까지가?",
          "speaker_id": "1",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041694.1.15",
          "form": "아니 오더는 두개",
          "original_form": "아니 오더는 두개",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041694.1.16",
          "form": "더샀지",
          "original_form": "더샀지",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041694.1.17",
          "form": "하나언니줬어",
          "original_form": "하나언니줬어",
          "speaker_id": "2",
          "time": "20210906 14:38"
        },
        {
          "id": "MDRW2100041694.1.18",
          "form": "왜 대단하다",
          "original_form": "왜 대단하다",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041694.1.19",
          "form": "자기도 하나더하면 팔고싶어라길래 address가 아직 어찌될지 모르니까",
          "original_form": "자기도 하나더하면 팔고싶어라길래 &address&가 아직 어찌될지 모르니까",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100041694.1.20",
          "form": "오더로 샀단말이가?",
          "original_form": "오더로 샀단말이가?",
