        },
        {
          "id": "MDRW2100046195.1.2",
          "form": "요즘 챙겨보는 건 놀토 뿐이에요",
          "original_form": "요즘 챙겨보는 건 놀토 뿐이에요",
          "speaker_id": "2",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046195.1.3",
          "form": "저는 환승연애욧",
          "original_form": "저는 환승연애욧",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046195.1.4",
          "form": "오 구건 무슨 내용인가요?",
          "original_form": "오 구건 무슨 내용인가요?",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046195.1.5",
          "form": "일반인들 썸타는거에여",
          "original_form": "일반인들 썸타는거에여",
          "speaker_id": "1",
          "time": "20210908 11:08"
