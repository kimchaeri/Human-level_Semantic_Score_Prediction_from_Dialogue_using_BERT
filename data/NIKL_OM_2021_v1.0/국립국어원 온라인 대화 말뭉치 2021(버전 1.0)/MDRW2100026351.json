        },
        {
          "id": "MDRW2100026351.1.2",
          "form": "좋아요 ㅎㅎ",
          "original_form": "좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.3",
          "form": "제주도 사시니까",
          "original_form": "제주도 사시니까",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.4",
          "form": "제주도 언제오셧어요?",
          "original_form": "제주도 언제오셧어요?",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.5",
          "form": "어디가젤좋아요?",
          "original_form": "어디가젤좋아요?",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.6",
          "form": "어디를 많이 자주 가요???",
          "original_form": "어디를 많이 자주 가요???",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.7",
          "form": "집이요ㅡㅡㅋ",
          "original_form": "집이요ㅡㅡㅋ",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.8",
          "form": "저는 제주시에 사는데",
          "original_form": "저는 제주시에 사는데",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.9",
          "form": "7월달에요!",
          "original_form": "7월달에요!",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.10",
          "form": "관광객이 많아서",
          "original_form": "관광객이 많아서",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.11",
          "form": "비행기값 아주 비쌀땤ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "비행기값 아주 비쌀땤ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.12",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.13",
          "form": "서귀포로 잘 안가요",
          "original_form": "서귀포로 잘 안가요",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.14",
          "form": "코로나 이놈ㅜ",
          "original_form": "코로나 이놈ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026351.1.15",
          "form": "최근에 오셧군요^",
          "original_form": "최근에 오셧군요^",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026351.1.16",
          "form": "아아 ㅠㅠㅠㅠㅠ",
          "original_form": "아아 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026351.1.17",
          "form": "네 생각보다 최근에 갔죠 ㅎㅎ",
          "original_form": "네 생각보다 최근에 갔죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026351.1.18",
          "form": "오히려 육지서 오는분이 좋은곳 잘아시던뎅ᆢ",
          "original_form": "오히려 육지서 오는분이 좋은곳 잘아시던뎅ᆢ",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026351.1.19",
          "form": "맛집좀알려주세요ㅎㅎ",
          "original_form": "맛집좀알려주세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026351.1.20",
          "form": "음.....뭐 그건 제주도 사셔서그런게아닐까요",
          "original_form": "음.....뭐 그건 제주도 사셔서그런게아닐까요",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026351.1.21",
          "form": "저도 부산살지만",
          "original_form": "저도 부산살지만",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026351.1.22",
          "form": "네 부산사람 해운대안가져",
          "original_form": "네 부산사람 해운대안가져",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026351.1.23",
          "form": "부산관광지모르듯이..?",
          "original_form": "부산관광지모르듯이..?",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026351.1.24",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
