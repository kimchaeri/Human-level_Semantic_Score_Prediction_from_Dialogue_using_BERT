      "utterance": [
        {
          "id": "MDRW2100025914.1.1",
          "form": "나도 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100025914.1.2",
          "form": "코로나 끝나야 해외라도 나가지..",
          "original_form": "코로나 끝나야 해외라도 나가지..",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100025914.1.3",
          "form": "너는 해외여행 가본적 있어?",
          "original_form": "너는 해외여행 가본적 있어?",
          "speaker_id": "1",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100025914.1.4",
          "form": "나 초5때였나 초6때 중국ㅋㅋㅋ",
          "original_form": "나 초5때였나 초6때 중국ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100025914.1.5",
          "form": "그때 베이징올림픽 전이라서 그 뭐지 중국 유명한..",
          "original_form": "그때 베이징올림픽 전이라서 그 뭐지 중국 유명한..",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100025914.1.6",
          "form": "거기 보수공사중이어서 제대로못봄ㅠ",
          "original_form": "거기 보수공사중이어서 제대로못봄ㅠ",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100025914.1.7",
          "form": "만리장성은 갔다가 너무 힘들었지..",
          "original_form": "만리장성은 갔다가 너무 힘들었지..",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100025914.1.8",
          "form": "와 부럽다 나는 한번도 못가봤다ㅎ",
          "original_form": "와 부럽다 나는 한번도 못가봤다ㅎ",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025914.1.9",
          "form": "근데 나 저때 컨디션별로여서 좋은기억이 아님ㅋㅋㅋㅋㅋ",
          "original_form": "근데 나 저때 컨디션별로여서 좋은기억이 아님ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100025914.1.10",
          "form": "진짜 제대로 못놀았지..",
          "original_form": "진짜 제대로 못놀았지..",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100025914.1.11",
          "form": "여행도 진짜 컨디션, 날씨가 중요해",
          "original_form": "여행도 진짜 컨디션, 날씨가 중요해",
          "speaker_id": "1",
          "time": "20210901 19:25"
        },
        {
          "id": "MDRW2100025914.1.12",
          "form": "인정~~~",
          "original_form": "인정~~~",
          "speaker_id": "2",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100025914.1.13",
          "form": "근데 외국나가면 고추장 햇반 필요하댘ㅋㅋㅋ",
          "original_form": "근데 외국나가면 고추장 햇반 필요하댘ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100025914.1.14",
          "form": "나 완전 코리아 입맛",
          "original_form": "나 완전 코리아 입맛",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100025914.1.15",
          "form": "너는 국내여행지 어디가 좋았어?",
          "original_form": "너는 국내여행지 어디가 좋았어?",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100025914.1.16",
          "form": "나 가까운건 강릉!",
          "original_form": "나 가까운건 강릉!",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100025914.1.17",
          "form": "먼곳은 여수?",
          "original_form": "먼곳은 여수?",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100025914.1.18",
          "form": "국내여행도 골고루 못갔엌ㅋㅋㅋㅋ",
          "original_form": "국내여행도 골고루 못갔엌ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100025914.1.19",
          "form": "여수는 여수밤바다 노래때문에 더 가고싶었지..",
          "original_form": "여수는 여수밤바다 노래때문에 더 가고싶었지..",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100025914.1.20",
          "form": "강릉 좋았구나ㅋㅋ나는 4년살면서 딱히 구경을 못해봐서",
          "original_form": "강릉 좋았구나ㅋㅋ나는 4년살면서 딱히 구경을 못해봐서",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100025914.1.21",
          "form": "ㅋㅋㅋ나는 그냥 바다보는게 좋아섴ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ나는 그냥 바다보는게 좋아섴ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100025914.1.22",
          "form": "나는 바다도 그렇게 선호하질않아서",
          "original_form": "나는 바다도 그렇게 선호하질않아서",
          "speaker_id": "1",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100025914.1.23",
          "form": "대신 경포 벚꽃축제는 엄청 좋았어",
          "original_form": "대신 경포 벚꽃축제는 엄청 좋았어",
          "speaker_id": "1",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100025914.1.24",
          "form": "나는 강릉 여름보다 봄이 좋아",
          "original_form": "나는 강릉 여름보다 봄이 좋아",
          "speaker_id": "1",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100025914.1.25",
          "form": "오 그래??",
          "original_form": "오 그래??",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100025914.1.26",
          "form": "나는 여름이나 겨울에 안목해변 카페거리인가 거기서 오선뷰 좋더라",
          "original_form": "나는 여름이나 겨울에 안목해변 카페거리인가 거기서 오선뷰 좋더라",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100025914.1.27",
          "form": "사실 생각해보면 여름 겨울은 방학이라 제대로 못보긴했다",
          "original_form": "사실 생각해보면 여름 겨울은 방학이라 제대로 못보긴했다",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100025914.1.28",
          "form": "봄에 무조건 경포호수야",
          "original_form": "봄에 무조건 경포호수야",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100025914.1.29",
          "form": "오 담엔 봄에 경포호수가본다ㅋㅋㅋ",
          "original_form": "오 담엔 봄에 경포호수가본다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100025914.1.30",
          "form": "나는 바닷가 근처 사는건 ㅂㄹ인데 이렇게 가끔 연례행사로 바다보러가는건 좋아ㅋㅋㅋ",
          "original_form": "나는 바닷가 근처 사는건 ㅂㄹ인데 이렇게 가끔 연례행사로 바다보러가는건 좋아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100025914.1.31",
          "form": "그래서 동해안이 그나마 가까우니까 강릉 속초 양양쪽으로 많이 간당ㅋㅋ",
          "original_form": "그래서 동해안이 그나마 가까우니까 강릉 속초 양양쪽으로 많이 간당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100025914.1.32",
          "form": "바닷가 살잖아? 바닷바람이라 머리 엄청 떡져",
          "original_form": "바닷가 살잖아? 바닷바람이라 머리 엄청 떡져",
          "speaker_id": "1",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100025914.1.33",
          "form": "1년에 한두번 보는 바다는 좋아",
          "original_form": "1년에 한두번 보는 바다는 좋아",
          "speaker_id": "1",
          "time": "20210902 17:12"
        }
      ]
    }
  ]
