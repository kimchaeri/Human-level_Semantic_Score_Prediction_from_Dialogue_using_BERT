      "utterance": [
        {
          "id": "MDRW2100015409.1.1",
          "form": "저 쇼핑 되게 좋아해요",
          "original_form": "저 쇼핑 되게 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015409.1.2",
          "form": "저두요! 돈쓰는게 제일 재밌어요!",
          "original_form": "저두요! 돈쓰는게 제일 재밌어요!",
          "speaker_id": "2",
          "time": "20210826 09:46"
        },
        {
          "id": "MDRW2100015409.1.3",
          "form": "최근에 산 것 중에 제일 맘에 드는 물건은 뭐가 있나요?",
          "original_form": "최근에 산 것 중에 제일 맘에 드는 물건은 뭐가 있나요?",
          "speaker_id": "2",
          "time": "20210826 09:46"
        },
        {
          "id": "MDRW2100015409.1.4",
          "form": "저는 양말이요",
          "original_form": "저는 양말이요",
          "speaker_id": "1",
          "time": "20210826 09:51"
        },
        {
          "id": "MDRW2100015409.1.5",
          "form": "0원에 샀어요 적립금털어서요",
          "original_form": "0원에 샀어요 적립금털어서요",
          "speaker_id": "1",
          "time": "20210826 09:52"
        },
        {
          "id": "MDRW2100015409.1.6",
          "form": "오 실결제 0원일 때 제일 기분 좋죠!!!",
          "original_form": "오 실결제 0원일 때 제일 기분 좋죠!!!",
          "speaker_id": "2",
          "time": "20210826 09:52"
        },
        {
          "id": "MDRW2100015409.1.7",
          "form": "너무 신기하더라고요 ㅎㅎ",
          "original_form": "너무 신기하더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:53"
        },
        {
          "id": "MDRW2100015409.1.8",
          "form": "적립금 안까먹고 모으신게 대단해요! 전 출석 꼭 하루씩 구멍나더라구요 ㅠㅠ",
          "original_form": "적립금 안까먹고 모으신게 대단해요! 전 출석 꼭 하루씩 구멍나더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 09:54"
        },
        {
          "id": "MDRW2100015409.1.9",
          "form": "헛 많이 까먹었어요 ㅜ ㅜ",
          "original_form": "헛 많이 까먹었어요 ㅜ ㅜ",
          "speaker_id": "1",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100015409.1.10",
          "form": "그래서 아까웠어요",
          "original_form": "그래서 아까웠어요",
          "speaker_id": "1",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100015409.1.11",
          "form": "에고 0원에 사놓고 아깝다네요 ㅋㅋㅋ",
          "original_form": "에고 0원에 사놓고 아깝다네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100015409.1.12",
          "form": "ㅋㅋ안까먹었으면 양말 말고 더 비싼걸 살 수 있었을테니 아까운거죠!!",
          "original_form": "ㅋㅋ안까먹었으면 양말 말고 더 비싼걸 살 수 있었을테니 아까운거죠!!",
          "speaker_id": "2",
          "time": "20210826 10:08"
        },
        {
          "id": "MDRW2100015409.1.13",
          "form": "아 ㅋㅋㅋ맞아요",
          "original_form": "아 ㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100015409.1.14",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:12"
        },
        {
          "id": "MDRW2100015409.1.15",
          "form": "ㅋㅋ내돈내산 물건 중에 가장 맘에 드는 것도 있나요?!",
          "original_form": "ㅋㅋ내돈내산 물건 중에 가장 맘에 드는 것도 있나요?!",
          "speaker_id": "2",
          "time": "20210826 10:14"
        },
        {
          "id": "MDRW2100015409.1.16",
          "form": "아이섀도 팔레트 좋아해요",
          "original_form": "아이섀도 팔레트 좋아해요",
          "speaker_id": "1",
          "time": "20210826 10:22"
        },
        {
          "id": "MDRW2100015409.1.17",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 10:22"
        },
        {
          "id": "MDRW2100015409.1.18",
          "form": "전 최근에 큰 맘 먹고 지른 다이슨 에어랩이요! ㅋㅋㅋ",
          "original_form": "전 최근에 큰 맘 먹고 지른 다이슨 에어랩이요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:23"
        },
        {
          "id": "MDRW2100015409.1.19",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100015409.1.20",
          "form": "그거 진짜 좋아요?",
          "original_form": "그거 진짜 좋아요?",
          "speaker_id": "1",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100015409.1.21",
          "form": "똥손한테 좋다고 해서 샀는데 제가 생각보다 더 똥손이더라구요!! ㅋㅋㅋㅋㅋ",
          "original_form": "똥손한테 좋다고 해서 샀는데 제가 생각보다 더 똥손이더라구요!! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100015409.1.22",
          "form": "좋긴한데 아직 가격만큼 뽕을 못뽑은거같아요 ㅋㅋ",
          "original_form": "좋긴한데 아직 가격만큼 뽕을 못뽑은거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100015409.1.23",
          "form": "아아 점점 잘된다니까",
          "original_form": "아아 점점 잘된다니까",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100015409.1.24",
          "form": "기대하세요 ㅎㅎㅎ",
          "original_form": "기대하세요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100015409.1.25",
          "form": "ㅎㅎ네 더 열심히 연습할게요!!",
          "original_form": "ㅎㅎ네 더 열심히 연습할게요!!",
