      "utterance": [
        {
          "id": "MDRW2100046070.1.1",
          "form": "어떤 과일 좋아해",
          "original_form": "어떤 과일 좋아해",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046070.1.2",
          "form": "나는 메론",
          "original_form": "나는 메론",
          "speaker_id": "1",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046070.1.3",
          "form": "비싼 거 좋아하네",
          "original_form": "비싼 거 좋아하네",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046070.1.4",
          "form": "달달해서 맛있어",
          "original_form": "달달해서 맛있어",
          "speaker_id": "1",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046070.1.5",
          "form": "나는 사과 좋아해",
          "original_form": "나는 사과 좋아해",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046070.1.6",
          "form": "나는 사과는 너무 딱딱해",
          "original_form": "나는 사과는 너무 딱딱해",
          "speaker_id": "1",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046070.1.7",
          "form": "딱딱해 도달해",
          "original_form": "딱딱해 도달해",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046070.1.8",
          "form": "과일은 사실 전부 맛있어",
          "original_form": "과일은 사실 전부 맛있어",
          "speaker_id": "1",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046070.1.9",
          "form": "신 거는 조금 힘들어",
          "original_form": "신 거는 조금 힘들어",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046070.1.10",
          "form": "나도 신 건 잘 못 먹어",
          "original_form": "나도 신 건 잘 못 먹어",
          "speaker_id": "1",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100046070.1.11",
          "form": "싱거에 소금을 뿌려 먹으면 맛있다던데",
          "original_form": "싱거에 소금을 뿌려 먹으면 맛있다던데",
          "speaker_id": "2",
          "time": "20210908 10:03"
        },
        {
          "id": "MDRW2100046070.1.12",
          "form": "그럼 단맛이 극대화된다더라",
          "original_form": "그럼 단맛이 극대화된다더라",
