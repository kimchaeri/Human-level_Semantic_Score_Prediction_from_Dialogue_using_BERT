      "utterance": [
        {
          "id": "MDRW2100029295.1.1",
          "form": "감사합니다ㅎㅎ",
          "original_form": "감사합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:13"
        },
        {
          "id": "MDRW2100029295.1.2",
          "form": "먹는거 조아하시나요",
          "original_form": "먹는거 조아하시나요",
          "speaker_id": "1",
          "time": "20210903 02:16"
        },
        {
          "id": "MDRW2100029295.1.3",
          "form": "좋아합니다!!",
          "original_form": "좋아합니다!!",
          "speaker_id": "2",
          "time": "20210903 02:17"
        },
        {
          "id": "MDRW2100029295.1.4",
          "form": "머조아하세요",
          "original_form": "머조아하세요",
          "speaker_id": "1",
          "time": "20210903 02:17"
        },
        {
          "id": "MDRW2100029295.1.5",
          "form": "두루두루 좋아합니다!! 한식중식일식양식",
          "original_form": "두루두루 좋아합니다!! 한식중식일식양식",
          "speaker_id": "2",
          "time": "20210903 02:18"
        },
        {
          "id": "MDRW2100029295.1.6",
          "form": "해산물과 육류는 필수",
          "original_form": "해산물과 육류는 필수",
          "speaker_id": "2",
          "time": "20210903 02:19"
        },
        {
          "id": "MDRW2100029295.1.7",
          "form": "유제품도 먹구요ㅎㅎ",
          "original_form": "유제품도 먹구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:19"
        },
        {
          "id": "MDRW2100029295.1.8",
          "form": "머조아하세영 ㅎ",
          "original_form": "머조아하세영 ㅎ",
          "speaker_id": "1",
          "time": "20210903 02:20"
        },
        {
          "id": "MDRW2100029295.1.9",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210903 02:20"
        },
        {
          "id": "MDRW2100029295.1.10",
          "form": "육류어떤거?",
          "original_form": "육류어떤거?",
          "speaker_id": "1",
          "time": "20210903 02:20"
        },
        {
          "id": "MDRW2100029295.1.11",
          "form": "소보단 돼지, 스테이크와 육회는 사랑, 오리와 닭은 한세트 양고기도 가리지 않습니다",
          "original_form": "소보단 돼지, 스테이크와 육회는 사랑, 오리와 닭은 한세트 양고기도 가리지 않습니다",
          "speaker_id": "2",
          "time": "20210903 02:22"
        },
        {
          "id": "MDRW2100029295.1.12",
          "form": "싫어하는 분들도 많지만 내장류도 거침없이 클리어 가능하죠ㅎㅎ",
          "original_form": "싫어하는 분들도 많지만 내장류도 거침없이 클리어 가능하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:22"
        },
        {
          "id": "MDRW2100029295.1.13",
          "form": "아이고 잘드시네요",
          "original_form": "아이고 잘드시네요",
          "speaker_id": "1",
          "time": "20210903 02:22"
        },
        {
          "id": "MDRW2100029295.1.14",
          "form": "잘챙겨드시나요항상",
          "original_form": "잘챙겨드시나요항상",
          "speaker_id": "1",
          "time": "20210903 02:22"
        },
        {
          "id": "MDRW2100029295.1.15",
          "form": "아이구 제가 너무 급발진했나요..ㅎㅎ",
          "original_form": "아이구 제가 너무 급발진했나요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:22"
        },
        {
          "id": "MDRW2100029295.1.16",
          "form": "잘챙겨먹진 않지만 컨디션 난조를 해결하는 황금열쇠로 씁니다",
          "original_form": "잘챙겨먹진 않지만 컨디션 난조를 해결하는 황금열쇠로 씁니다",
          "speaker_id": "2",
          "time": "20210903 02:23"
        },
        {
          "id": "MDRW2100029295.1.17",
          "form": "간단하게 먹는경우가 많아요ㅎㅎ",
          "original_form": "간단하게 먹는경우가 많아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:24"
        },
        {
          "id": "MDRW2100029295.1.18",
          "form": "아하 골든키 ~~",
          "original_form": "아하 골든키 ~~",
          "speaker_id": "1",
          "time": "20210903 02:24"
        },
        {
          "id": "MDRW2100029295.1.19",
          "form": "혼자사세요?",
          "original_form": "혼자사세요?",
          "speaker_id": "1",
          "time": "20210903 02:24"
        },
        {
          "id": "MDRW2100029295.1.20",
          "form": "아뇨 가족들과 같이 살고있어요",
          "original_form": "아뇨 가족들과 같이 살고있어요",
          "speaker_id": "2",
          "time": "20210903 02:24"
        },
        {
          "id": "MDRW2100029295.1.21",
          "form": "백수로 살면서 자취한다는건... 끔찍하네요ㅠ",
          "original_form": "백수로 살면서 자취한다는건... 끔찍하네요ㅠ",
          "speaker_id": "2",
          "time": "20210903 02:25"
        },
        {
          "id": "MDRW2100029295.1.22",
          "form": "식사 직접 챙기시나요? 부모님이?",
          "original_form": "식사 직접 챙기시나요? 부모님이?",
          "speaker_id": "1",
          "time": "20210903 02:25"
        },
        {
          "id": "MDRW2100029295.1.23",
          "form": "제가 해먹기도 하고 있는 반찬을 먹기도 하죠",
          "original_form": "제가 해먹기도 하고 있는 반찬을 먹기도 하죠",
          "speaker_id": "2",
          "time": "20210903 02:26"
        },
        {
          "id": "MDRW2100029295.1.24",
          "form": "장은 주로 제가 보고",
          "original_form": "장은 주로 제가 보고",
          "speaker_id": "2",
          "time": "20210903 02:26"
        },
        {
          "id": "MDRW2100029295.1.25",
          "form": "음식은 같이하는 편이에요",
          "original_form": "음식은 같이하는 편이에요",
          "speaker_id": "2",
          "time": "20210903 02:26"
        },
        {
          "id": "MDRW2100029295.1.26",
          "form": "아하 도와주시는구나",
          "original_form": "아하 도와주시는구나",
          "speaker_id": "1",
          "time": "20210903 02:27"
        },
        {
          "id": "MDRW2100029295.1.27",
          "form": "밥드셧나요",
          "original_form": "밥드셧나요",
          "speaker_id": "1",
          "time": "20210904 14:04"
        }
      ]
    }
