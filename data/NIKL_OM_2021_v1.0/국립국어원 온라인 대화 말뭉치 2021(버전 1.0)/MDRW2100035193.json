      "utterance": [
        {
          "id": "MDRW2100035193.1.1",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100035193.1.2",
          "form": "전 좋아하는 음식이 많아요 그런데 오늘은 게장이 생각나네요",
          "original_form": "전 좋아하는 음식이 많아요 그런데 오늘은 게장이 생각나네요",
          "speaker_id": "1",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100035193.1.3",
          "form": "게장을 좋아하시나요?",
          "original_form": "게장을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100035193.1.4",
          "form": "전 게장을 좋아하는데 자주 먹진 못해요",
          "original_form": "전 게장을 좋아하는데 자주 먹진 못해요",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100035193.1.5",
          "form": "저도 게장 좋아해요. 저희 가족은 다 안좋아하시만요",
          "original_form": "저도 게장 좋아해요. 저희 가족은 다 안좋아하시만요",
          "speaker_id": "2",
          "time": "20210901 06:53"
        },
        {
          "id": "MDRW2100035193.1.6",
          "form": "그럼 자주 드시기 힘드시겠네요",
          "original_form": "그럼 자주 드시기 힘드시겠네요",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100035193.1.7",
          "form": "네 거의 1년에 한번 먹을까 말까하네요",
          "original_form": "네 거의 1년에 한번 먹을까 말까하네요",
          "speaker_id": "2",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100035193.1.8",
          "form": "저두요, 좀 더 자주 먹고 싶지만 게장집에 가서 먹어요,,",
          "original_form": "저두요, 좀 더 자주 먹고 싶지만 게장집에 가서 먹어요,,",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100035193.1.9",
          "form": "혼자 먹으려고 담그기도 그렇고,,",
          "original_form": "혼자 먹으려고 담그기도 그렇고,,",
          "speaker_id": "1",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100035193.1.10",
          "form": "담그실 줄 아세요?",
          "original_form": "담그실 줄 아세요?",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100035193.1.11",
          "form": "아뇨 하지만 인터넷이 알려주니까요 ㅋㅋㅋ",
          "original_form": "아뇨 하지만 인터넷이 알려주니까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100035193.1.12",
          "form": "게가 살아있잖아요 으흐흐... ㅠ",
          "original_form": "게가 살아있잖아요 으흐흐... ㅠ",
          "speaker_id": "2",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100035193.1.13",
          "form": "그럼 냉동게를 쓰심 되죠  dㅓㄹ어",
          "original_form": "그럼 냉동게를 쓰심 되죠  dㅓㄹ어",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100035193.1.14",
          "form": "동사한 게는 더 저렴해요",
          "original_form": "동사한 게는 더 저렴해요",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100035193.1.15",
          "form": "전 레시피대로 해도 이상하게 맛이 없던데요.. ㅎㅎㅎ",
          "original_form": "전 레시피대로 해도 이상하게 맛이 없던데요.. ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 07:46"
        },
        {
          "id": "MDRW2100035193.1.16",
          "form": "전 안만들어봐서뭐라 드릴 조언이,, ㅠ",
          "original_form": "전 안만들어봐서뭐라 드릴 조언이,, ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100035193.1.17",
          "form": "언젠간 맛있게 만들겠죠",
          "original_form": "언젠간 맛있게 만들겠죠",
