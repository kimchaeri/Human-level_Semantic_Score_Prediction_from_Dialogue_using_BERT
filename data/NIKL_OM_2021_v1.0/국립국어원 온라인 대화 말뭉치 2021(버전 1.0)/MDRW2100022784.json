      "utterance": [
        {
          "id": "MDRW2100022784.1.1",
          "form": "이거 웹툰도 해당되려나?",
          "original_form": "이거 웹툰도 해당되려나?",
          "speaker_id": "1",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022784.1.2",
          "form": "그렇지 않을까",
          "original_form": "그렇지 않을까",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022784.1.3",
          "form": "아 근데 영상이랑 음원 콘텐츠인가",
          "original_form": "아 근데 영상이랑 음원 콘텐츠인가",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022784.1.4",
          "form": "영상 콘텐츠 음원 콘텐츠 이렇게면...",
          "original_form": "영상 콘텐츠 음원 콘텐츠 이렇게면...",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022784.1.5",
          "form": "모르겠네",
          "original_form": "모르겠네",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022784.1.6",
          "form": "암튼 왜? 재밌는 웹툰 있어?",
          "original_form": "암튼 왜? 재밌는 웹툰 있어?",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022784.1.7",
          "form": "아니 추천해달라고 할라했지ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 추천해달라고 할라했지ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022784.1.8",
          "form": "요즘 드라마는 보는거있어?",
          "original_form": "요즘 드라마는 보는거있어?",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022784.1.9",
          "form": "나 이제",
          "original_form": "나 이제",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022784.1.10",
          "form": "김선호 드라마 봐야지",
          "original_form": "김선호 드라마 봐야지",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022784.1.11",
          "form": "근데 좀 몰아보고 싶어서",
          "original_form": "근데 좀 몰아보고 싶어서",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100022784.1.12",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022784.1.13",
          "form": "참고 있는데 사람들이",
          "original_form": "참고 있는데 사람들이",
          "speaker_id": "2",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022784.1.14",
          "form": "인스타에 자꾸 올림",
          "original_form": "인스타에 자꾸 올림",
          "speaker_id": "2",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022784.1.15",
          "form": "오늘자 김선호",
          "original_form": "오늘자 김선호",
          "speaker_id": "2",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022784.1.16",
          "form": "이런식으로...",
          "original_form": "이런식으로...",
          "speaker_id": "2",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022784.1.17",
          "form": "이거도 말해도되나?",
          "original_form": "이거도 말해도되나?",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022784.1.18",
          "form": "나저번에 트위터하다가",
          "original_form": "나저번에 트위터하다가",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022784.1.19",
          "form": "그 알티많이탄 글은 사람들이 막 영업하잖아",
          "original_form": "그 알티많이탄 글은 사람들이 막 영업하잖아",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022784.1.20",
          "form": "거기서 누가 김선호랑 딘딘 커플링 영업하는거봑음.....ㄷㄷ",
          "original_form": "거기서 누가 김선호랑 딘딘 커플링 영업하는거봑음.....ㄷㄷ",
          "speaker_id": "1",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022784.1.21",
          "form": "참...별별걸 다 판다 싶더라",
          "original_form": "참...별별걸 다 판다 싶더라",
          "speaker_id": "1",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022784.1.22",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022784.1.23",
          "form": "잠시만 설명좀 해줄래?",
          "original_form": "잠시만 설명좀 해줄래?",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022784.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
        {
          "id": "MDRW2100022784.1.25",
          "form": "아니 웃지만 말고",
          "original_form": "아니 웃지만 말고",
          "speaker_id": "2",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022784.1.26",
          "form": "커플링을 꼈다고?",
          "original_form": "커플링을 꼈다고?",
          "speaker_id": "2",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022784.1.27",
          "form": "엥 아니 ㄴㄴㄴㄴ",
          "original_form": "엥 아니 ㄴㄴㄴㄴ",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022784.1.28",
          "form": "커플링이 반지말하는게 아니고",
          "original_form": "커플링이 반지말하는게 아니고",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022784.1.29",
          "form": "둘이 엮어서 덕질하는거말하는거임",
          "original_form": "둘이 엮어서 덕질하는거말하는거임",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022784.1.30",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "2",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022784.1.31",
          "form": "그건 이해감",
          "original_form": "그건 이해감",
          "speaker_id": "2",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022784.1.32",
          "form": "알페스=씨피=커플링",
          "original_form": "알페스=씨피=커플링",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022784.1.33",
          "form": "그 일박이일 안에서",
          "original_form": "그 일박이일 안에서",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022784.1.34",
          "form": "딘선 커플 이러면서",
          "original_form": "딘선 커플 이러면서",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022784.1.35",
          "form": "아진짜??",
          "original_form": "아진짜??",
          "speaker_id": "1",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022784.1.36",
          "form": "둘이 엄청 애증 관계로 나오긴 함",
          "original_form": "둘이 엄청 애증 관계로 나오긴 함",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022784.1.37",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022784.1.38",
          "form": "깜짝놀랬네",
          "original_form": "깜짝놀랬네",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022784.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022784.1.40",
          "form": "둘이 서로를 라이벌로 의식하거든",
          "original_form": "둘이 서로를 라이벌로 의식하거든",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022784.1.41",
          "form": "둘다 춤도 엄청 못추고",
          "original_form": "둘다 춤도 엄청 못추고",
          "speaker_id": "2",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100022784.1.42",
          "form": "그래가지곸ㅋㅋㅋㅋㅋ",
          "original_form": "그래가지곸ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:04"
        }
      ]
