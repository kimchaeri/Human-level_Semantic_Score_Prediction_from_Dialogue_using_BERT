          "speaker_id": "1",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100024796.1.4",
          "form": "세번?...",
          "original_form": "세번?...",
          "speaker_id": "1",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.5",
          "form": "지리죠",
          "original_form": "지리죠",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.6",
          "form": "내년에차살고에요",
          "original_form": "내년에차살고에요",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.7",
          "form": "열정열정",
          "original_form": "열정열정",
          "speaker_id": "1",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.8",
          "form": "ㅠ 엄마때매자꾸 못사는데",
          "original_form": "ㅠ 엄마때매자꾸 못사는데",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.9",
          "form": "넘무스트레스",
          "original_form": "넘무스트레스",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.10",
          "form": "그래서 차 노래를 부르시는규나",
          "original_form": "그래서 차 노래를 부르시는규나",
          "speaker_id": "1",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.11",
          "form": "살만하네",
          "original_form": "살만하네",
          "speaker_id": "1",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.12",
          "form": "예",
          "original_form": "예",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.13",
          "form": "차끌고가면 빠르면20분",
          "original_form": "차끌고가면 빠르면20분",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.14",
          "form": "25분걸림",
          "original_form": "25분걸림",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.15",
          "form": "버스타면 1시간",
          "original_form": "버스타면 1시간",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024796.1.16",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210831 18:48"
        },
        {
          "id": "MDRW2100024796.1.17",
          "form": "고생하시네",
          "original_form": "고생하시네",
          "speaker_id": "1",
          "time": "20210831 18:48"
        },
        {
          "id": "MDRW2100024796.1.18",
          "form": "저랑 비슷하겠는데요",
          "original_form": "저랑 비슷하겠는데요",
          "speaker_id": "1",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024796.1.19",
          "form": "이짓 5년함",
          "original_form": "이짓 5년함",
          "speaker_id": "2",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024796.1.20",
          "form": "저도 대중교통 겁나 불편함",
          "original_form": "저도 대중교통 겁나 불편함",
          "speaker_id": "1",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024796.1.21",
          "form": "어디보자...",
          "original_form": "어디보자...",
          "speaker_id": "1",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024796.1.22",
          "form": "저는 버스 2~3번 갈아타고 30분가량 도보",
          "original_form": "저는 버스 2~3번 갈아타고 30분가량 도보",
          "speaker_id": "1",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024796.1.23",
          "form": "차타면 25~30분",
          "original_form": "차타면 25~30분",
          "speaker_id": "1",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024796.1.24",
          "form": "왘ㅋㅋㅋㅋ",
          "original_form": "왘ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024796.1.25",
          "form": "직장이어딘데요?",
          "original_form": "직장이어딘데요?",
          "speaker_id": "2",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024796.1.26",
          "form": "시골인가?",
          "original_form": "시골인가?",
          "speaker_id": "2",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024796.1.27",
          "form": "비밀입니다",
          "original_form": "비밀입니다",
          "speaker_id": "1",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024796.1.28",
          "form": "프라이버시",
          "original_form": "프라이버시",
          "speaker_id": "1",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024796.1.29",
          "form": "시골이라뇨ㅋㅋㅋㅋㅋㅋ",
          "original_form": "시골이라뇨ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024796.1.30",
          "form": "경기도면",
          "original_form": "경기도면",
          "speaker_id": "2",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024796.1.31",
          "form": "시골많은데",
          "original_form": "시골많은데",
          "speaker_id": "2",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024796.1.32",
          "form": "대전은 많은 발전이 있나봐요?",
          "original_form": "대전은 많은 발전이 있나봐요?",
          "speaker_id": "1",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024796.1.33",
          "form": "머 시골이라면 시골일듯",
          "original_form": "머 시골이라면 시골일듯",
          "speaker_id": "1",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024796.1.34",
          "form": "예",
          "original_form": "예",
          "speaker_id": "2",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024796.1.35",
          "form": "뭐많이짓드라구요",
          "original_form": "뭐많이짓드라구요",
          "speaker_id": "2",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024796.1.36",
          "form": "서울사람은 뭐 다 시골일테니",
          "original_form": "서울사람은 뭐 다 시골일테니",
          "speaker_id": "1",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024796.1.37",
          "form": "시와 시 경계선에 있어서 버스가 회사쪽으로 안가요",
          "original_form": "시와 시 경계선에 있어서 버스가 회사쪽으로 안가요",
          "speaker_id": "1",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024796.1.38",
          "form": "서울은 언제가도재밋어요",
          "original_form": "서울은 언제가도재밋어요",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024796.1.39",
          "form": "그지같음",
          "original_form": "그지같음",
          "speaker_id": "1",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024796.1.40",
          "form": "아 최억이네요ㅠ",
          "original_form": "아 최억이네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024796.1.41",
          "form": "그래서 스쿠터 사서 타고 댕겼었는데",
          "original_form": "그래서 스쿠터 사서 타고 댕겼었는데",
          "speaker_id": "1",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024796.1.42",
          "form": "출퇴근불편하면진짜 답도없움",
          "original_form": "출퇴근불편하면진짜 답도없움",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024796.1.43",
          "form": "2년? 3년 안타서 걍 팜요 올해",
          "original_form": "2년? 3년 안타서 걍 팜요 올해",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024796.1.44",
          "form": "진짜 짱남..",
          "original_form": "진짜 짱남..",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024796.1.45",
          "form": "그래도 나머지가 괜찮아서 걍 다니는데",
          "original_form": "그래도 나머지가 괜찮아서 걍 다니는데",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024796.1.46",
          "form": "님은 더 하네요",
          "original_form": "님은 더 하네요",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024796.1.47",
          "form": "차 뭐로 사십니까?",
          "original_form": "차 뭐로 사십니까?",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024796.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024796.1.49",
          "form": "아 근데 차 사면 돈 못 모으는데",
          "original_form": "아 근데 차 사면 돈 못 모으는데",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024796.1.50",
          "form": "원래 아이오닉 예약햇엇거든요?",
          "original_form": "원래 아이오닉 예약햇엇거든요?",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024796.1.51",
          "form": "그래도 뭐 여자니까",
          "original_form": "그래도 뭐 여자니까",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024796.1.52",
          "form": "오우야",
          "original_form": "오우야",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024796.1.53",
          "form": "비싼거 주문하셧네",
          "original_form": "비싼거 주문하셧네",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024796.1.54",
          "form": "3~4천 아니었나",
          "original_form": "3~4천 아니었나",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.55",
          "form": "근데 옵션끼니까 6천넘어서 보조금못받드라구요",
          "original_form": "근데 옵션끼니까 6천넘어서 보조금못받드라구요",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.56",
          "form": "오우야",
          "original_form": "오우야",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.57",
          "form": "그래서포기함",
          "original_form": "그래서포기함",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.58",
          "form": "가격 미쳐따리",
          "original_form": "가격 미쳐따리",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.59",
          "form": "청년적금 모아서 전기차 뽑을랬는데",
          "original_form": "청년적금 모아서 전기차 뽑을랬는데",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.60",
          "form": "베뉴 이런거사고싶 ㅠ",
          "original_form": "베뉴 이런거사고싶 ㅠ",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.61",
          "form": "꿈도 못꿀듯",
          "original_form": "꿈도 못꿀듯",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.62",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.63",
          "form": "살수잇자나요",
          "original_form": "살수잇자나요",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.64",
          "form": "이번이 딱 반 지났는데",
          "original_form": "이번이 딱 반 지났는데",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.65",
          "form": "이거 기다려서 3천 모우면",
          "original_form": "이거 기다려서 3천 모우면",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024796.1.66",
          "form": "차값이 7천씯 할듯",
