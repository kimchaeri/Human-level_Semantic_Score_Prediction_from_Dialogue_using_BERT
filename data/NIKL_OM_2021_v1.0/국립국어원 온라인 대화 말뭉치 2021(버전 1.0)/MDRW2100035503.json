      "utterance": [
        {
          "id": "MDRW2100035503.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210830 12:21"
        },
        {
          "id": "MDRW2100035503.1.2",
          "form": "아직이요! 식사하셨어요?",
          "original_form": "아직이요! 식사하셨어요?",
          "speaker_id": "1",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100035503.1.3",
          "form": "저는 이따 섭웨갈려고 버티고있어요 ㅎㅎ",
          "original_form": "저는 이따 섭웨갈려고 버티고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100035503.1.4",
          "form": "헉 저도 샌드위치먹긴할거예요!",
          "original_form": "헉 저도 샌드위치먹긴할거예요!",
          "speaker_id": "1",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100035503.1.5",
          "form": "홈메이드지만요...",
          "original_form": "홈메이드지만요...",
          "speaker_id": "1",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100035503.1.6",
          "form": "홈메이드 샌드위치 짱맛있잖아요",
          "original_form": "홈메이드 샌드위치 짱맛있잖아요",
          "speaker_id": "2",
          "time": "20210830 18:53"
        },
        {
          "id": "MDRW2100035503.1.7",
          "form": "뭐 넣어 드셨나요?",
          "original_form": "뭐 넣어 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 18:53"
        },
        {
          "id": "MDRW2100035503.1.8",
          "form": "양상추랑 치즈 햄 계란 넣어먹었어요",
          "original_form": "양상추랑 치즈 햄 계란 넣어먹었어요",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100035503.1.9",
          "form": "섭웨 뭐드셨나요?",
          "original_form": "섭웨 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100035503.1.10",
          "form": "로티세리 치킨이요!",
          "original_form": "로티세리 치킨이요!",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100035503.1.11",
          "form": "정석 햄치즈 샌드유ㅣ치네요",
          "original_form": "정석 햄치즈 샌드유ㅣ치네요",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100035503.1.12",
          "form": "소스가 젤 중요한데 뭐낳으셨나요 ㅎㅎ",
          "original_form": "소스가 젤 중요한데 뭐낳으셨나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100035503.1.13",
          "form": "홈메이드다보니 저는 케챱 룸메는 케챱마요 였어요",
          "original_form": "홈메이드다보니 저는 케챱 룸메는 케챱마요 였어요",
          "speaker_id": "1",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100035503.1.14",
          "form": "맛있었겠어요 섭웨 왠지 채소섭취도하고 건강한 느낌...",
          "original_form": "맛있었겠어요 섭웨 왠지 채소섭취도하고 건강한 느낌...",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100035503.1.15",
          "form": "섭웨 맛있긴하죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "섭웨 맛있긴하죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100035503.1.16",
          "form": "하지만.. 작은걸..",
          "original_form": "하지만.. 작은걸..",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100035503.1.17",
          "form": "30은 많고...15는 적고...",
          "original_form": "30은 많고...15는 적고...",
          "speaker_id": "1",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100035503.1.18",
          "form": "맞아요 진짜 ㅠㅠ",
          "original_form": "맞아요 진짜 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 11:40"
        },
        {
          "id": "MDRW2100035503.1.19",
          "form": "그냥 꼭꼭 씹어먹는 수밖애..",
          "original_form": "그냥 꼭꼭 씹어먹는 수밖애..",
          "speaker_id": "2",
          "time": "20210831 11:40"
        },
        {
          "id": "MDRW2100035503.1.20",
          "form": "먹구 쿠키까지 먹어야 한끼인거 같아요",
          "original_form": "먹구 쿠키까지 먹어야 한끼인거 같아요",
          "speaker_id": "1",
          "time": "20210831 11:55"
        },
        {
          "id": "MDRW2100035503.1.21",
          "form": "쿠키 진짜 맛있죠",
          "original_form": "쿠키 진짜 맛있죠",
          "speaker_id": "2",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100035503.1.22",
          "form": "뭐 젤 좋아하새여?",
          "original_form": "뭐 젤 좋아하새여?",
          "speaker_id": "2",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100035503.1.23",
          "form": "샌드위치중 에서요?",
          "original_form": "샌드위치중 에서요?",
          "speaker_id": "1",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100035503.1.24",
          "form": "섭웨꺼요!",
          "original_form": "섭웨꺼요!",
          "speaker_id": "2",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100035503.1.25",
          "form": "저는 스테이크 어쩌구랑 에그마요 좋아해요",
          "original_form": "저는 스테이크 어쩌구랑 에그마요 좋아해요",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100035503.1.26",
          "form": "저는 섭웨 에그마요 좋아해요",
          "original_form": "저는 섭웨 에그마요 좋아해요",
          "speaker_id": "1",
          "time": "20210831 23:45"
        }
      ]
    }
