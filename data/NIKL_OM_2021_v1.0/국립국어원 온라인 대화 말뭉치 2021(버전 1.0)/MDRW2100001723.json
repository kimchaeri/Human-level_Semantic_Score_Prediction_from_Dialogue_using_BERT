      "utterance": [
        {
          "id": "MDRW2100001723.1.1",
          "form": "게임 하는거 있어요?",
          "original_form": "게임 하는거 있어요?",
          "speaker_id": "2",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100001723.1.2",
          "form": "게임 미지막으로한게 모바일 카트라이더요 평소에 안해요~",
          "original_form": "게임 미지막으로한게 모바일 카트라이더요 평소에 안해요~",
          "speaker_id": "1",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100001723.1.3",
          "form": "하시는거 있으시나요?",
          "original_form": "하시는거 있으시나요?",
          "speaker_id": "1",
          "time": "20210826 00:27"
        },
        {
          "id": "MDRW2100001723.1.4",
          "form": "저두 하는건 없어요 모바일배그는 좀 했었어요",
          "original_form": "저두 하는건 없어요 모바일배그는 좀 했었어요",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100001723.1.5",
          "form": "어릴땐 게임 많이 했었는데 그러셨나요???",
          "original_form": "어릴땐 게임 많이 했었는데 그러셨나요???",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100001723.1.6",
          "form": "그럼요!",
          "original_form": "그럼요!",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100001723.1.7",
          "form": "어릴땐 크아도 하고 테일즈런너도 하고",
          "original_form": "어릴땐 크아도 하고 테일즈런너도 하고",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100001723.1.8",
          "form": "재밌는 게임 많았죠",
          "original_form": "재밌는 게임 많았죠",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100001723.1.9",
          "form": "아 크아 추억이네요ㅋㅋㅋ 크아도 했고 카드라이더랑 저는 바람의나라 엄청했어요",
          "original_form": "아 크아 추억이네요ㅋㅋㅋ 크아도 했고 카드라이더랑 저는 바람의나라 엄청했어요",
          "speaker_id": "1",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100001723.1.10",
          "form": "ㅋㅋㅋㅋ요즘엔 왜 그런 재밌는 게임이 안보이는지ㅠ",
          "original_form": "ㅋㅋㅋㅋ요즘엔 왜 그런 재밌는 게임이 안보이는지ㅠ",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100001723.1.11",
          "form": "어른이란 이런걸까요 ....?",
          "original_form": "어른이란 이런걸까요 ....?",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100001723.1.12",
          "form": "모바일 게임은 많이는 안해봣는데 다들 먼가 자동?으로 돌리더라구요",
          "original_form": "모바일 게임은 많이는 안해봣는데 다들 먼가 자동?으로 돌리더라구요",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100001723.1.13",
          "form": "자동사냥?",
          "original_form": "자동사냥?",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100001723.1.14",
          "form": "자동화인데 재미없을꺼같은 생각만드네요ㅋㅋㅋ",
          "original_form": "자동화인데 재미없을꺼같은 생각만드네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100001723.1.15",
          "form": "맞아요 저도 해봤는데",
          "original_form": "맞아요 저도 해봤는데",
          "speaker_id": "2",
          "time": "20210826 08:20"
        },
        {
          "id": "MDRW2100001723.1.16",
          "form": "그냥 자동으로 알아서 레벨업 해주고 나는 아이템만 줍는....",
          "original_form": "그냥 자동으로 알아서 레벨업 해주고 나는 아이템만 줍는....",
          "speaker_id": "2",
          "time": "20210826 08:21"
        },
        {
          "id": "MDRW2100001723.1.17",
          "form": "아하~~ 반자동이긴하네요",
          "original_form": "아하~~ 반자동이긴하네요",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100001723.1.18",
          "form": "네네 그래도 일할때 옆에 켜놓으면 알아서 거진 하니까 그건 좋긴 하더라구요",
          "original_form": "네네 그래도 일할때 옆에 켜놓으면 알아서 거진 하니까 그건 좋긴 하더라구요",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100001723.1.19",
          "form": "친구는 세컨폰으로 밤새 자동으로 돌리더라구요 신기신기",
          "original_form": "친구는 세컨폰으로 밤새 자동으로 돌리더라구요 신기신기",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100001723.1.20",
          "form": "ㅋㅋㅋ맞아요 밤새 돌려놔요",
          "original_form": "ㅋㅋㅋ맞아요 밤새 돌려놔요",
          "speaker_id": "2",
          "time": "20210826 21:30"
        }
      ]
    }
