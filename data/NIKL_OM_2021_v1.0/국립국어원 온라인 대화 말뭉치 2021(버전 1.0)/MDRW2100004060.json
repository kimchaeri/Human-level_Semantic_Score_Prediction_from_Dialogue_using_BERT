      "utterance": [
        {
          "id": "MDRW2100004060.1.1",
          "form": "혹시 건강관리 하고 계세요?",
          "original_form": "혹시 건강관리 하고 계세요?",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004060.1.2",
          "form": "잘 못 하고있는 것같네요",
          "original_form": "잘 못 하고있는 것같네요",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004060.1.3",
          "form": "영양제는 챙겨먹어요",
          "original_form": "영양제는 챙겨먹어요",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004060.1.4",
          "form": "다이어트도 하고 계세요?",
          "original_form": "다이어트도 하고 계세요?",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004060.1.5",
          "form": "해야하는데 의지가 안생기네요",
          "original_form": "해야하는데 의지가 안생기네요",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004060.1.6",
          "form": "아픈곳은 없어요?",
          "original_form": "아픈곳은 없어요?",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004060.1.7",
          "form": "네 괜찮아요~ 오늘은 치과에서 스케일링했네요",
          "original_form": "네 괜찮아요~ 오늘은 치과에서 스케일링했네요",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004060.1.8",
          "form": "지인님은 건강 괜찮으세요?",
          "original_form": "지인님은 건강 괜찮으세요?",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004060.1.9",
          "form": "저도 건강은 괜찮은데",
          "original_form": "저도 건강은 괜찮은데",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004060.1.10",
          "form": "어제부터 교정시작해서 너무 아프네요",
          "original_form": "어제부터 교정시작해서 너무 아프네요",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004060.1.11",
          "form": "교정 시작하셨군요 힘드시겠어요",
          "original_form": "교정 시작하셨군요 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004060.1.12",
          "form": "어제는 입안에 악궁확장?하느라 이 사이에 고무넣고 왔는데 되게 아픈거같아요",
          "original_form": "어제는 입안에 악궁확장?하느라 이 사이에 고무넣고 왔는데 되게 아픈거같아요",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004060.1.13",
          "form": "아 듣기만해도 넘 힘들것같아요ㅠ",
          "original_form": "아 듣기만해도 넘 힘들것같아요ㅠ",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004060.1.14",
          "form": "발치도 하셨어요?",
          "original_form": "발치도 하셨어요?",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004060.1.15",
          "form": "저는 비발치로 해야 입안이 넓어진다고 해서",
          "original_form": "저는 비발치로 해야 입안이 넓어진다고 해서",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004060.1.16",
          "form": "다음주에 사랑니만 뺄거같아요",
          "original_form": "다음주에 사랑니만 뺄거같아요",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004060.1.17",
          "form": "근데 수평매복이라 대학병원 가서 빼야한다더라구요",
          "original_form": "근데 수평매복이라 대학병원 가서 빼야한다더라구요",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004060.1.18",
          "form": "저도 아래쪽 사랑니는 수평인데 안빼도된대서 두려구요",
          "original_form": "저도 아래쪽 사랑니는 수평인데 안빼도된대서 두려구요",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004060.1.19",
          "form": "위에꺼 뽑을땐 해골뽑히는줄 알았어요ㅠㅠ",
          "original_form": "위에꺼 뽑을땐 해골뽑히는줄 알았어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004060.1.20",
          "form": "뽑을때가 아프셨어요 뽑고 나서가 아프셨어요?",
          "original_form": "뽑을때가 아프셨어요 뽑고 나서가 아프셨어요?",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004060.1.21",
          "form": "뽑는순간만 아팠어요ㅋ",
          "original_form": "뽑는순간만 아팠어요ㅋ",
          "speaker_id": "2",
          "time": "20210821 19:47"
        }
      ]
