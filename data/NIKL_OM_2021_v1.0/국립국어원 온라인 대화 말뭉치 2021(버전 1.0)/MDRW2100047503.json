          "speaker_id": "1",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100047503.1.3",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "1",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100047503.1.4",
          "form": "아직요ㅋㅋㅋㅋ name1님은요?!",
          "original_form": "아직요ㅋㅋㅋㅋ &name1&님은요?!",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100047503.1.5",
          "form": "저는 이제 안주상을 차리려고요ㅎ",
          "original_form": "저는 이제 안주상을 차리려고요ㅎ",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100047503.1.6",
          "form": "와우...",
          "original_form": "와우...",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100047503.1.7",
          "form": "저도갈래요....",
          "original_form": "저도갈래요....",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100047503.1.8",
          "form": "오시지요ㅎㅎ",
          "original_form": "오시지요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100047503.1.9",
          "form": "닭근위볶음과 곱창볶음과 닭발 몬도가네 쓰리콤보요ㅎㅎ",
          "original_form": "닭근위볶음과 곱창볶음과 닭발 몬도가네 쓰리콤보요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100047503.1.10",
          "form": "와우.. 제손보이시나요?",
          "original_form": "와우.. 제손보이시나요?",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100047503.1.11",
          "form": "술챙겼어요..",
          "original_form": "술챙겼어요..",
          "speaker_id": "2",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100047503.1.12",
          "form": "ㅎㅎ 예의를 아시는군요",
          "original_form": "ㅎㅎ 예의를 아시는군요",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047503.1.13",
          "form": "무슨술로 챙기셨나요",
          "original_form": "무슨술로 챙기셨나요",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047503.1.14",
          "form": "으음 안주를보니 소주여야할것같지만\r\n하이볼",
          "original_form": "으음 안주를보니 소주여야할것같지만\r\n하이볼",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100047503.1.15",
          "form": "아니 ㅋㅋ",
          "original_form": "아니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100047503.1.16",
          "form": "여기에도요?",
          "original_form": "여기에도요?",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100047503.1.17",
          "form": "ㅋㅋㅋㅋ 거기에도요!!!!!!!!!",
          "original_form": "ㅋㅋㅋㅋ 거기에도요!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210827 00:17"
        },
        {
          "id": "MDRW2100047503.1.18",
          "form": "와 저녁먹고졸았네요",
          "original_form": "와 저녁먹고졸았네요",
          "speaker_id": "2",
          "time": "20210827 00:17"
        },
        {
          "id": "MDRW2100047503.1.19",
          "form": "식곤증이 몰려왔었군요ㅎㅎ",
          "original_form": "식곤증이 몰려왔었군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100047503.1.20",
          "form": "바로 잤는데 소화는 되시나요?",
          "original_form": "바로 잤는데 소화는 되시나요?",
