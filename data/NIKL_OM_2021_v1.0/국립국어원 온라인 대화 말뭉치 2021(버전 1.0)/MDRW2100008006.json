      "utterance": [
        {
          "id": "MDRW2100008006.1.1",
          "form": "먼저 움직이는 사람이 하는 거지",
          "original_form": "먼저 움직이는 사람이 하는 거지",
          "speaker_id": "2",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100008006.1.2",
          "form": "지금 부엌에 비린내 장난아님",
          "original_form": "지금 부엌에 비린내 장난아님",
          "speaker_id": "2",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100008006.1.3",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100008006.1.4",
          "form": "냉동실에 있던 전어를 점심식사용으로 내놓았더니",
          "original_form": "냉동실에 있던 전어를 점심식사용으로 내놓았더니",
          "speaker_id": "1",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100008006.1.5",
          "form": "그 비린내를 처리하는 일이 또 생겼군",
          "original_form": "그 비린내를 처리하는 일이 또 생겼군",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100008006.1.6",
          "form": "먹고 바로 식기세척기 돌려야겠다",
          "original_form": "먹고 바로 식기세척기 돌려야겠다",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100008006.1.7",
          "form": "굽는동안도 비린내 많이 나려나?",
          "original_form": "굽는동안도 비린내 많이 나려나?",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100008006.1.8",
          "form": "모르겠어",
          "original_form": "모르겠어",
          "speaker_id": "2",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100008006.1.9",
          "form": "일단 걔가 녹으면서 나는 냄새가..",
          "original_form": "일단 걔가 녹으면서 나는 냄새가..",
          "speaker_id": "2",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100008006.1.10",
          "form": "뭔가 일이 커지는 느낌이군",
          "original_form": "뭔가 일이 커지는 느낌이군",
          "speaker_id": "1",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100008006.1.11",
          "form": "오늘 집안 청소는 안하겠지?",
          "original_form": "오늘 집안 청소는 안하겠지?",
          "speaker_id": "1",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100008006.1.12",
          "form": "한동안 정리 및 청소에 관심을 가졌으나",
          "original_form": "한동안 정리 및 청소에 관심을 가졌으나",
          "speaker_id": "1",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100008006.1.13",
          "form": "지금은 집안일 아무것도 하기 싫군",
          "original_form": "지금은 집안일 아무것도 하기 싫군",
          "speaker_id": "1",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100008006.1.14",
          "form": "휴가때나 관심갖고 하는거지",
          "original_form": "휴가때나 관심갖고 하는거지",
          "speaker_id": "2",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100008006.1.15",
          "form": "그렇습니다",
          "original_form": "그렇습니다",
          "speaker_id": "1",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100008006.1.16",
          "form": "출근하면서 황금같은 주말에 집안일은 있을 수가 없어",
          "original_form": "출근하면서 황금같은 주말에 집안일은 있을 수가 없어",
          "speaker_id": "2",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100008006.1.17",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100008006.1.18",
          "form": "수저 없어서 식기세척기 돌리는거 말고 뭘 더한다?",
          "original_form": "수저 없어서 식기세척기 돌리는거 말고 뭘 더한다?",
          "speaker_id": "2",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100008006.1.19",
          "form": "불 가 능",
          "original_form": "불 가 능",
          "speaker_id": "2",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100008006.1.20",
          "form": "퇴근하고 틈틈이 집안일 하는 건 더더욱 불가능",
          "original_form": "퇴근하고 틈틈이 집안일 하는 건 더더욱 불가능",
          "speaker_id": "1",
          "time": "20210822 12:42"
        },
        {
          "id": "MDRW2100008006.1.21",
          "form": "아 그치",
          "original_form": "아 그치",
          "speaker_id": "2",
          "time": "20210822 12:42"
        },
        {
          "id": "MDRW2100008006.1.22",
          "form": "우리처럼 번갈아가면서 출퇴근멤버가 좀 바뀌어야 가능",
          "original_form": "우리처럼 번갈아가면서 출퇴근멤버가 좀 바뀌어야 가능",
          "speaker_id": "2",
          "time": "20210822 12:42"
        },
        {
          "id": "MDRW2100008006.1.23",
          "form": "응 집안일하면서 일하는 분들은 진짜 대단해",
          "original_form": "응 집안일하면서 일하는 분들은 진짜 대단해",
          "speaker_id": "1",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100008006.1.24",
          "form": "가사 노동 분담하라!",
          "original_form": "가사 노동 분담하라!",
          "speaker_id": "2",
          "time": "20210822 12:45"
        },
        {
          "id": "MDRW2100008006.1.25",
          "form": "동일노동 동일임금!",
          "original_form": "동일노동 동일임금!",
          "speaker_id": "2",
          "time": "20210822 12:45"
        }
      ]
    }
  ]
