      "utterance": [
        {
          "id": "MDRW2100011221.1.1",
          "form": "편의점에서 자주 사는 물건이 있나요",
          "original_form": "편의점에서 자주 사는 물건이 있나요",
          "speaker_id": "1",
          "time": "20210824 08:32"
        },
        {
          "id": "MDRW2100011221.1.2",
          "form": "저는 과자나 삼각김밥 많이사요ㅎㅎ",
          "original_form": "저는 과자나 삼각김밥 많이사요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:03"
        },
        {
          "id": "MDRW2100011221.1.3",
          "form": "지원님은 뭘 사시나용?",
          "original_form": "지원님은 뭘 사시나용?",
          "speaker_id": "2",
          "time": "20210824 12:04"
        },
        {
          "id": "MDRW2100011221.1.4",
          "form": "저는 핫바 같은 거 좋아해요",
          "original_form": "저는 핫바 같은 거 좋아해요",
          "speaker_id": "1",
          "time": "20210824 12:04"
        },
        {
          "id": "MDRW2100011221.1.5",
          "form": "그래서 핫바 종류 자주 사 먹어요",
          "original_form": "그래서 핫바 종류 자주 사 먹어요",
          "speaker_id": "1",
          "time": "20210824 12:04"
        },
        {
          "id": "MDRW2100011221.1.6",
          "form": "저도 핫바 좋아해요 ㅎㅎ 주로 무슨 핫바 드세요?",
          "original_form": "저도 핫바 좋아해요 ㅎㅎ 주로 무슨 핫바 드세요?",
          "speaker_id": "2",
          "time": "20210824 12:05"
        },
        {
          "id": "MDRW2100011221.1.7",
          "form": "그 씨제이에서 나온 매운 핫바 있어요 아시나요",
          "original_form": "그 씨제이에서 나온 매운 핫바 있어요 아시나요",
          "speaker_id": "1",
          "time": "20210824 12:06"
        },
        {
          "id": "MDRW2100011221.1.8",
          "form": "그거 맛있죠 ㅎㅎ 저는 의성마늘핫바 먹는데 그것도 진짜 맛있어요!!",
          "original_form": "그거 맛있죠 ㅎㅎ 저는 의성마늘핫바 먹는데 그것도 진짜 맛있어요!!",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011221.1.9",
          "form": "의성 마늘 핫바 그것도 맛있죠",
          "original_form": "의성 마늘 핫바 그것도 맛있죠",
          "speaker_id": "1",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011221.1.10",
          "form": "식감이 진짜 탱탱하다고 해야하나",
          "original_form": "식감이 진짜 탱탱하다고 해야하나",
          "speaker_id": "1",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011221.1.11",
          "form": "삼각김밥은 무슨 맛 주로 사시나요",
          "original_form": "삼각김밥은 무슨 맛 주로 사시나요",
          "speaker_id": "1",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011221.1.12",
          "form": "저는 무조건 전주비빔이용!!",
          "original_form": "저는 무조건 전주비빔이용!!",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011221.1.13",
          "form": "그게 제일 맛있어요ㅎㅎ",
          "original_form": "그게 제일 맛있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011221.1.14",
          "form": "맞아요 전주비빔밥 맛있어요",
          "original_form": "맞아요 전주비빔밥 맛있어요",
          "speaker_id": "1",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011221.1.15",
          "form": "아 자는 주로 어떤 걸 사시나요",
          "original_form": "아 자는 주로 어떤 걸 사시나요",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011221.1.16",
          "form": "과자는 주로 어떤걸 사시나요",
          "original_form": "과자는 주로 어떤걸 사시나요",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011221.1.17",
          "form": "과자는 그냥 신상과자 있으면 사는것같아요ㅋㅋㅋ",
          "original_form": "과자는 그냥 신상과자 있으면 사는것같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011221.1.18",
          "form": "편의점은 24시간 여니까 새벽에 배고플때 가면 딱인것같아요 ㅋㅋ",
          "original_form": "편의점은 24시간 여니까 새벽에 배고플때 가면 딱인것같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011221.1.19",
          "form": "최근에 산 과자 있나요",
          "original_form": "최근에 산 과자 있나요",
