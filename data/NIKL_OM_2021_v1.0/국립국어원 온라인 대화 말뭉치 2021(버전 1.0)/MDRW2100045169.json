        },
        {
          "id": "MDRW2100045169.1.2",
          "form": "마썸카 말만 들었는데",
          "original_form": "마썸카 말만 들었는데",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045169.1.3",
          "form": "진짜 재밌어요",
          "original_form": "진짜 재밌어요",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045169.1.4",
          "form": "뭐하는 게임인가요",
          "original_form": "뭐하는 게임인가요",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045169.1.5",
          "form": "음.. 차고치는 게임이긴한데 ㅋㅋ",
          "original_form": "음.. 차고치는 게임이긴한데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045169.1.6",
          "form": "차만 고치는데 재밌을까요??",
          "original_form": "차만 고치는데 재밌을까요??",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045169.1.7",
          "form": "아니에요 ㅋㅋ 알바도함",
          "original_form": "아니에요 ㅋㅋ 알바도함",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045169.1.8",
          "form": "알바요??",
          "original_form": "알바요??",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045169.1.9",
          "form": "무슨 심즈급임 ㅋㅋㅋ",
          "original_form": "무슨 심즈급임 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045169.1.10",
          "form": "심즈라니 상상이 안가네요",
          "original_form": "심즈라니 상상이 안가네요",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045169.1.11",
          "form": "장도보고 춤도추러가야하고 ㅋㅋ",
          "original_form": "장도보고 춤도추러가야하고 ㅋㅋ",
