      "utterance": [
        {
          "id": "MDRW2100019802.1.1",
          "form": "간식은 좀 먹고 일했어??",
          "original_form": "간식은 좀 먹고 일했어??",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019802.1.2",
          "form": "담주 금요일에 어머님 생신이라고 밥먹어야 하는데 어차피 코로나라서 배달음식 먹을것같아",
          "original_form": "담주 금요일에 어머님 생신이라고 밥먹어야 하는데 어차피 코로나라서 배달음식 먹을것같아",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019802.1.3",
          "form": "그랬구만 뭘 하니 귀찮게",
          "original_form": "그랬구만 뭘 하니 귀찮게",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019802.1.4",
          "form": "못해~ 힘들어서 사먹어야지",
          "original_form": "못해~ 힘들어서 사먹어야지",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019802.1.5",
          "form": "시켜먹는게 최고야 배달음식 잘생각했어",
          "original_form": "시켜먹는게 최고야 배달음식 잘생각했어",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019802.1.6",
          "form": "코로나때문에 외식도 2인이상 안되니까 ㅋㅋㅋ",
          "original_form": "코로나때문에 외식도 2인이상 안되니까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019802.1.7",
          "form": "맛있는 곳에서 센스있게 시켜만 드려도 효도",
          "original_form": "맛있는 곳에서 센스있게 시켜만 드려도 효도",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019802.1.8",
          "form": "니가 먹을거니까 맛있는 걸로 먹어 ㅋㅋ",
          "original_form": "니가 먹을거니까 맛있는 걸로 먹어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019802.1.9",
          "form": "회나 초밥 좋아하셔서 그거 시키면 될것같아",
          "original_form": "회나 초밥 좋아하셔서 그거 시키면 될것같아",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019802.1.10",
          "form": "근데 날씨가 좀 그래서 위험하다",
          "original_form": "근데 날씨가 좀 그래서 위험하다",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019802.1.11",
          "form": "입맛도 없어... 다음주면 좀 더 시원하지않을까?",
          "original_form": "입맛도 없어... 다음주면 좀 더 시원하지않을까?",
          "speaker_id": "2",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019802.1.12",
          "form": "오히려 여름보다 어중간할때가 위험하다고 했거덩",
          "original_form": "오히려 여름보다 어중간할때가 위험하다고 했거덩",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019802.1.13",
          "form": "그럴거 같긴 하다",
          "original_form": "그럴거 같긴 하다",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019802.1.14",
          "form": "드시고싶은걸로 시켜드리고 디저트는 내가 먹고싶은걸오 해야지 ㅋㅋㅋㅋ",
          "original_form": "드시고싶은걸로 시켜드리고 디저트는 내가 먹고싶은걸오 해야지 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019802.1.15",
          "form": "ㅋㅋㅋ 케이크 먹고 싶은걸로 먹어",
          "original_form": "ㅋㅋㅋ 케이크 먹고 싶은걸로 먹어",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019802.1.16",
          "form": "요즘 디저트가게 진짜 많더라고",
          "original_form": "요즘 디저트가게 진짜 많더라고",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019802.1.17",
          "form": "크로플도 맛있고",
          "original_form": "크로플도 맛있고",
          "speaker_id": "2",
          "time": "20210827 22:09"
        }
      ]
