        },
        {
          "id": "MDRW2100030006.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100030006.1.3",
          "form": "전 대학생이에요!! 당신은요?!",
          "original_form": "전 대학생이에요!! 당신은요?!",
          "speaker_id": "1",
          "time": "20210903 18:30"
        },
        {
          "id": "MDRW2100030006.1.4",
          "form": "졸업한지 오래됐네요 ㅎㅎ",
          "original_form": "졸업한지 오래됐네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100030006.1.5",
          "form": "개강해서 바쁘겠네요",
          "original_form": "개강해서 바쁘겠네요",
          "speaker_id": "2",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100030006.1.6",
          "form": "앗 언니셨네요!!",
          "original_form": "앗 언니셨네요!!",
          "speaker_id": "1",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100030006.1.7",
          "form": "전 계속 비대면이라 수업만 듣구 있어용ㅋㅋㅋ",
          "original_form": "전 계속 비대면이라 수업만 듣구 있어용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100030006.1.8",
          "form": "코로나라서 그렇겠네요",
          "original_form": "코로나라서 그렇겠네요",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100030006.1.9",
          "form": "학교 갈 일은 없나요?",
          "original_form": "학교 갈 일은 없나요?",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030006.1.10",
          "form": "시험 칠때 갔다오려구요!!",
          "original_form": "시험 칠때 갔다오려구요!!",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030006.1.11",
          "form": "언니는 동아리 뭐뭐 들으셨어요!?",
          "original_form": "언니는 동아리 뭐뭐 들으셨어요!?",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030006.1.12",
          "form": "전 아직 고민중이라서요!!",
          "original_form": "전 아직 고민중이라서요!!",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030006.1.13",
          "form": "전공 관련 동아리랑 봉사동아리요",
          "original_form": "전공 관련 동아리랑 봉사동아리요",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030006.1.14",
          "form": "우와 멋져용!!",
          "original_form": "우와 멋져용!!",
          "speaker_id": "1",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100030006.1.15",
          "form": "저도 봉사 동아리 생각중이에요!",
          "original_form": "저도 봉사 동아리 생각중이에요!",
          "speaker_id": "1",
          "time": "20210903 18:40"
        },
        {
          "id": "MDRW2100030006.1.16",
          "form": "오 코로나인데 동아리 활동은 어떻게 진행되나요?",
          "original_form": "오 코로나인데 동아리 활동은 어떻게 진행되나요?",
          "speaker_id": "2",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100030006.1.17",
          "form": "비대면으로 만나서 회의하기도 하구",
          "original_form": "비대면으로 만나서 회의하기도 하구",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030006.1.18",
          "form": "막 공모전도 내요!!",
          "original_form": "막 공모전도 내요!!",
          "speaker_id": "1",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100030006.1.19",
          "form": "비대면이 일상화가 됐네요",
          "original_form": "비대면이 일상화가 됐네요",
          "speaker_id": "2",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100030006.1.20",
          "form": "저 때는 갑자기 공강나면 좋아하고 그랬는데요",
          "original_form": "저 때는 갑자기 공강나면 좋아하고 그랬는데요",
          "speaker_id": "2",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100030006.1.21",
          "form": "온라인도 보충으로 하는 정도로만 하고요",
          "original_form": "온라인도 보충으로 하는 정도로만 하고요",
          "speaker_id": "2",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100030006.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ요새도 수업 안하면 좋아해요!!!ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ요새도 수업 안하면 좋아해요!!!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100030006.1.23",
          "form": "비대면이니까 다 온라인이긴 하지만요!",
          "original_form": "비대면이니까 다 온라인이긴 하지만요!",
          "speaker_id": "1",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100030006.1.24",
          "form": "하긴 그렇겠죠 ㅋㅋ 전 통학이라서 공강이면 더 좋았어요",
          "original_form": "하긴 그렇겠죠 ㅋㅋ 전 통학이라서 공강이면 더 좋았어요",
