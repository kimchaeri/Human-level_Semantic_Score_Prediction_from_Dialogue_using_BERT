      "utterance": [
        {
          "id": "MDRW2100012429.1.1",
          "form": "게임 뭐 하세용?",
          "original_form": "게임 뭐 하세용?",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012429.1.2",
          "form": "피파온라인이요!",
          "original_form": "피파온라인이요!",
          "speaker_id": "1",
          "time": "20210824 22:17"
        },
        {
          "id": "MDRW2100012429.1.3",
          "form": "축구게임이죠?",
          "original_form": "축구게임이죠?",
          "speaker_id": "2",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100012429.1.4",
          "form": "네 맞아요ㅎㅎ",
          "original_form": "네 맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100012429.1.5",
          "form": "그거 말고는 뭐하셨나용",
          "original_form": "그거 말고는 뭐하셨나용",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012429.1.6",
          "form": "예전에 배그했었네요",
          "original_form": "예전에 배그했었네요",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012429.1.7",
          "form": "배그도 핫했었죠",
          "original_form": "배그도 핫했었죠",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012429.1.8",
          "form": "크아는 안하시나용",
          "original_form": "크아는 안하시나용",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012429.1.9",
          "form": "크아 예전에 했었죻ㅎ",
          "original_form": "크아 예전에 했었죻ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012429.1.10",
          "form": "캐릭 뭐 자주 했나용",
          "original_form": "캐릭 뭐 자주 했나용",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012429.1.11",
          "form": "크아캐릭 잘 기억이안나네요ㅎㅎ",
          "original_form": "크아캐릭 잘 기억이안나네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012429.1.12",
          "form": "아 전 배찌 자주 했어요ㅋㅋㅋㅋㅋ",
          "original_form": "아 전 배찌 자주 했어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012429.1.13",
          "form": "요즘도 하시나요?",
          "original_form": "요즘도 하시나요?",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012429.1.14",
          "form": "요새는 자주 안해요",
          "original_form": "요새는 자주 안해요",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012429.1.15",
          "form": "몇달 된 거 같아요",
          "original_form": "몇달 된 거 같아요",
          "speaker_id": "2",
          "time": "20210824 23:08"
        }
