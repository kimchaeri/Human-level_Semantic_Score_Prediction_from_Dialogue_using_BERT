      "utterance": [
        {
          "id": "MDRW2100034172.1.1",
          "form": "네 안녕하세요 ㅎㅎ",
          "original_form": "네 안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100034172.1.2",
          "form": "전 저녁에 모듬전울 시켰는데",
          "original_form": "전 저녁에 모듬전울 시켰는데",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100034172.1.3",
          "form": "식사하셨나용?",
          "original_form": "식사하셨나용?",
          "speaker_id": "1",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100034172.1.4",
          "form": "앗 여쭤보기전에 먼저 답을 주시는 센스쟁이",
          "original_form": "앗 여쭤보기전에 먼저 답을 주시는 센스쟁이",
          "speaker_id": "1",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100034172.1.5",
          "form": "저번주 시킨집 아닌데 헉",
          "original_form": "저번주 시킨집 아닌데 헉",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100034172.1.6",
          "form": "너무 맛이 없네요 ㅜㅜ",
          "original_form": "너무 맛이 없네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100034172.1.7",
          "form": "이럴수도 있군요",
          "original_form": "이럴수도 있군요",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100034172.1.8",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100034172.1.9",
          "form": "ㅋㅋ네 전 호 박죽을 먹었어요",
          "original_form": "ㅋㅋ네 전 호 박죽을 먹었어요",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100034172.1.10",
          "form": "모듬전이 맛이없다니 식어서 맛이 없느 ㄴ걸까요?",
          "original_form": "모듬전이 맛이없다니 식어서 맛이 없느 ㄴ걸까요?",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100034172.1.11",
          "form": "호박죽이요",
          "original_form": "호박죽이요",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034172.1.12",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034172.1.13",
          "form": "아예 암맛이 않나요",
          "original_form": "아예 암맛이 않나요",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034172.1.14",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034172.1.15",
          "form": "간을 안했나봐요",
          "original_form": "간을 안했나봐요",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034172.1.16",
          "form": "이상하다 냉동전도 맛은 날텐데",
          "original_form": "이상하다 냉동전도 맛은 날텐데",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034172.1.17",
          "form": "아 그런 문제군요",
          "original_form": "아 그런 문제군요",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034172.1.18",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034172.1.19",
          "form": "소금 치고",
          "original_form": "소금 치고",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034172.1.20",
          "form": "조미료도 뿌려 봤지만 ㅜㅜ",
          "original_form": "조미료도 뿌려 봤지만 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034172.1.21",
          "form": "살아나질 않네요 ㅜㅜ",
          "original_form": "살아나질 않네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034172.1.22",
          "form": "돈을 2000원 더주고 시켰는데",
          "original_form": "돈을 2000원 더주고 시켰는데",
          "speaker_id": "2",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100034172.1.23",
          "form": "소금을 전을 다 부치고 넣어봤자 튀김옷에만 붙어서 ㅋㅋ 안은 맹맹하겠어요",
          "original_form": "소금을 전을 다 부치고 넣어봤자 튀김옷에만 붙어서 ㅋㅋ 안은 맹맹하겠어요",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100034172.1.24",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100034172.1.25",
          "form": "요리 못하는 집이네요",
          "original_form": "요리 못하는 집이네요",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100034172.1.26",
          "form": "배달요리가 맛이 없을때 제일 난감해요 버리기도 아깝고",
          "original_form": "배달요리가 맛이 없을때 제일 난감해요 버리기도 아깝고",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100034172.1.27",
          "form": "그러게요 버렸어요 돈 아깝네요",
          "original_form": "그러게요 버렸어요 돈 아깝네요",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100034172.1.28",
          "form": "음쓰 버리는 비용과 시간도 아까워요",
          "original_form": "음쓰 버리는 비용과 시간도 아까워요",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100034172.1.29",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100034172.1.30",
          "form": "요긴 아파트가 봉지에 넣어야 해서 다 돈인데",
          "original_form": "요긴 아파트가 봉지에 넣어야 해서 다 돈인데",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100034172.1.31",
          "form": "호박죽 직접 하셨어요?",
          "original_form": "호박죽 직접 하셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100034172.1.32",
          "form": "요즘엔 한번에 버리는 음쓰도 무게를 달아서 무게별로 돈받아요 ㅠㅠ",
          "original_form": "요즘엔 한번에 버리는 음쓰도 무게를 달아서 무게별로 돈받아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 20:15"
        }
      ]
