      "utterance": [
        {
          "id": "MDRW2100020999.1.1",
          "form": "개 키워요?",
          "original_form": "개 키워요?",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100020999.1.2",
          "form": "아뇨 항상 생각만요",
          "original_form": "아뇨 항상 생각만요",
          "speaker_id": "1",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100020999.1.3",
          "form": "키우세요?",
          "original_form": "키우세요?",
          "speaker_id": "1",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100020999.1.4",
          "form": "ㅜㅠ 키워보고 싶어요",
          "original_form": "ㅜㅠ 키워보고 싶어요",
          "speaker_id": "2",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100020999.1.5",
          "form": "파충류는 어때요?ㅎㅎ",
          "original_form": "파충류는 어때요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100020999.1.6",
          "form": "아 파충류는 좀 ㅎㅎ",
          "original_form": "아 파충류는 좀 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020999.1.7",
          "form": "파충류보다 먹는먹이가 더싫어요",
          "original_form": "파충류보다 먹는먹이가 더싫어요",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100020999.1.8",
          "form": "아 파충류가 먹는 먹이요?",
          "original_form": "아 파충류가 먹는 먹이요?",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100020999.1.9",
          "form": "벌레 같은 것들이요?ㅋㅋㅋ",
          "original_form": "벌레 같은 것들이요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100020999.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100020999.1.11",
          "form": "벌레 완전싫어요",
          "original_form": "벌레 완전싫어요",
          "speaker_id": "1",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100020999.1.12",
          "form": "그거 미저 생각도 못했는데 생각해보니까",
          "original_form": "그거 미저 생각도 못했는데 생각해보니까",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100020999.1.13",
          "form": "저도 먹이 줄 자신이 없네요 ㅋㅋㅋㅋ",
          "original_form": "저도 먹이 줄 자신이 없네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100020999.1.14",
          "form": "name1님은 뭐 키워보고 싶어요?",
          "original_form": "&name1&님은 뭐 키워보고 싶어요?",
          "speaker_id": "2",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100020999.1.15",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "1",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100020999.1.16",
          "form": "차라리 뱀이 낳지 ㅜㅜㅜ벌레는",
          "original_form": "차라리 뱀이 낳지 ㅜㅜㅜ벌레는",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100020999.1.17",
          "form": "진짜 뱀이 낫다고 생각해요?",
          "original_form": "진짜 뱀이 낫다고 생각해요?",
          "speaker_id": "2",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100020999.1.18",
          "form": "ㄷㄷ",
          "original_form": "ㄷㄷ",
          "speaker_id": "2",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100020999.1.19",
          "form": "네 독만 없으면요",
          "original_form": "네 독만 없으면요",
          "speaker_id": "1",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100020999.1.20",
          "form": "독있는 애들은 무서워요",
          "original_form": "독있는 애들은 무서워요",
          "speaker_id": "1",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100020999.1.21",
          "form": "ㅠㅠ독 있는 얘들은 반려동물로 키우지도 못해요..",
