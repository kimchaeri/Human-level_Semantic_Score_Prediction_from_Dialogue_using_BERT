      "utterance": [
        {
          "id": "MDRW2100032101.1.1",
          "form": "싸이월드 하셨나요?",
          "original_form": "싸이월드 하셨나요?",
          "speaker_id": "1",
          "time": "20210905 12:24"
        },
        {
          "id": "MDRW2100032101.1.2",
          "form": "네 한참 때는 했었죠ㅎㅎ",
          "original_form": "네 한참 때는 했었죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100032101.1.3",
          "form": "저도요 그러다 금방 탈퇴했어요",
          "original_form": "저도요 그러다 금방 탈퇴했어요",
          "speaker_id": "1",
          "time": "20210905 12:26"
        },
        {
          "id": "MDRW2100032101.1.4",
          "form": "저는 그래도 2-3년은 한 것 같아요",
          "original_form": "저는 그래도 2-3년은 한 것 같아요",
          "speaker_id": "2",
          "time": "20210905 12:28"
        },
        {
          "id": "MDRW2100032101.1.5",
          "form": "도토리를 왕창 받은 일이 있어서ㅎㅎ",
          "original_form": "도토리를 왕창 받은 일이 있어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:28"
        },
        {
          "id": "MDRW2100032101.1.6",
          "form": "그걸로 부유하게 미니홈피 꾸미고 그랬었죠",
          "original_form": "그걸로 부유하게 미니홈피 꾸미고 그랬었죠",
          "speaker_id": "2",
          "time": "20210905 12:28"
        },
        {
          "id": "MDRW2100032101.1.7",
          "form": "와 추억의 도토리 ㅎㅎ",
          "original_form": "와 추억의 도토리 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 12:28"
        },
        {
          "id": "MDRW2100032101.1.8",
          "form": "네 싸이월드 도토리ㅎㅎ",
          "original_form": "네 싸이월드 도토리ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:29"
        },
        {
          "id": "MDRW2100032101.1.9",
          "form": "추억이 방울방울이네요",
          "original_form": "추억이 방울방울이네요",
          "speaker_id": "2",
          "time": "20210905 12:29"
        },
        {
          "id": "MDRW2100032101.1.10",
          "form": "그러게요 싸이월드 도토리도 미니홈피도 옛날 추억이 되어버렸네요",
          "original_form": "그러게요 싸이월드 도토리도 미니홈피도 옛날 추억이 되어버렸네요",
          "speaker_id": "1",
          "time": "20210905 12:30"
        },
        {
          "id": "MDRW2100032101.1.11",
          "form": "근데 싸이월드 부활은 왜 하는 걸까요?",
          "original_form": "근데 싸이월드 부활은 왜 하는 걸까요?",
          "speaker_id": "1",
          "time": "20210905 12:30"
        },
        {
          "id": "MDRW2100032101.1.12",
          "form": "글쎄요",
          "original_form": "글쎄요",
          "speaker_id": "2",
          "time": "20210905 12:35"
        },
        {
          "id": "MDRW2100032101.1.13",
          "form": "그때의 추억을 잊지못한 사람들이 많아서 아닐까요?",
          "original_form": "그때의 추억을 잊지못한 사람들이 많아서 아닐까요?",
          "speaker_id": "2",
          "time": "20210905 12:35"
        },
        {
          "id": "MDRW2100032101.1.14",
          "form": "좋은 추억이 많으셨나봐요 대부분 흑역사라고 하더라구요",
          "original_form": "좋은 추억이 많으셨나봐요 대부분 흑역사라고 하더라구요",
          "speaker_id": "1",
          "time": "20210905 12:36"
        },
        {
          "id": "MDRW2100032101.1.15",
          "form": "ㅎㅎ저는 그때가 고등학교 때부터 대학생활 초반까지라",
          "original_form": "ㅎㅎ저는 그때가 고등학교 때부터 대학생활 초반까지라",
          "speaker_id": "2",
          "time": "20210905 12:43"
        },
        {
          "id": "MDRW2100032101.1.16",
          "form": "보면 새롭고 귀엽더라고요",
          "original_form": "보면 새롭고 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210905 12:43"
        },
        {
          "id": "MDRW2100032101.1.17",
          "form": "전 방치해서 그냥 했구나 정도에요 다른 sns로 넘어갔어요",
          "original_form": "전 방치해서 그냥 했구나 정도에요 다른 sns로 넘어갔어요",
          "speaker_id": "1",
          "time": "20210905 12:48"
        },
        {
          "id": "MDRW2100032101.1.18",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100032101.1.19",
          "form": "다시 찾아보라는데 굳이 싸이월드 접속은 안했어요",
          "original_form": "다시 찾아보라는데 굳이 싸이월드 접속은 안했어요",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100032101.1.20",
          "form": "요즘은 워낙 sns가 많아서 솔직히 부활 할 필요가 없었다고 생각해요",
          "original_form": "요즘은 워낙 sns가 많아서 솔직히 부활 할 필요가 없었다고 생각해요",
          "speaker_id": "1",
          "time": "20210905 12:51"
        }
