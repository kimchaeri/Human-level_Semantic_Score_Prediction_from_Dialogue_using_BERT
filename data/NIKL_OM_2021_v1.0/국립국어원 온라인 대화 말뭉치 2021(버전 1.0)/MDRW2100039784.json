      "utterance": [
        {
          "id": "MDRW2100039784.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039784.1.2",
          "form": "네 안녕하세요!",
          "original_form": "네 안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039784.1.3",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039784.1.4",
          "form": "네 집밥 먹었습니다 ㅎㅎ",
          "original_form": "네 집밥 먹었습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039784.1.5",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039784.1.6",
          "form": "네 방금요~ 저는 고기먹었어요~",
          "original_form": "네 방금요~ 저는 고기먹었어요~",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039784.1.7",
          "form": "와 고기 너무 맛있었겠네요!!",
          "original_form": "와 고기 너무 맛있었겠네요!!",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039784.1.8",
          "form": "네 오랜만에 먹어서 맛있었어요",
          "original_form": "네 오랜만에 먹어서 맛있었어요",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039784.1.9",
          "form": "고기 좋아하시나요?",
          "original_form": "고기 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039784.1.10",
          "form": "네 좋아하죠 ㅎㅎ",
          "original_form": "네 좋아하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039784.1.11",
          "form": "요새 코로나때문에 밖에서 먹기 어려워서 좀 아쉬워요 ㅠㅠ",
          "original_form": "요새 코로나때문에 밖에서 먹기 어려워서 좀 아쉬워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039784.1.12",
          "form": "어떤 고기 좋아하세요?",
          "original_form": "어떤 고기 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039784.1.13",
          "form": "저도 집에서 먹었어요ㅜㅜ",
          "original_form": "저도 집에서 먹었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100039784.1.14",
          "form": "저는 돼지목살이 제일 좋은 것 같아요",
          "original_form": "저는 돼지목살이 제일 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039784.1.15",
          "form": "소는 좋은데 가격이 사악해서 ㅎㅎ",
          "original_form": "소는 좋은데 가격이 사악해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039784.1.16",
          "form": "오늘 어떤 고기 드셨나요?",
          "original_form": "오늘 어떤 고기 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039784.1.17",
          "form": "마자요ㅠㅠㅠㅠ",
          "original_form": "마자요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039784.1.18",
          "form": "오늘 돼지갈비 먹었어요",
          "original_form": "오늘 돼지갈비 먹었어요",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039784.1.19",
          "form": "오랜만에 먹어서 좋더라고요",
          "original_form": "오랜만에 먹어서 좋더라고요",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039784.1.20",
          "form": "와 돼지갈비ㅠㅠㅠ 너무 맛있을거 같네요",
          "original_form": "와 돼지갈비ㅠㅠㅠ 너무 맛있을거 같네요",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039784.1.21",
          "form": "저도 조만간 먹어야 겠어요 ㅎㅎ",
          "original_form": "저도 조만간 먹어야 겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039784.1.22",
          "form": "그죠 맛있어요ㅎㅎ",
          "original_form": "그죠 맛있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039784.1.23",
          "form": "생선 좋아하세요?",
          "original_form": "생선 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039784.1.24",
          "form": "생선은 구운 것 보다는 회나 초밥이 좋은 것 같아여!",
          "original_form": "생선은 구운 것 보다는 회나 초밥이 좋은 것 같아여!",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039784.1.25",
          "form": "생선 좋아하시나요?",
          "original_form": "생선 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039784.1.26",
          "form": "오 회 맛있죠",
          "original_form": "오 회 맛있죠",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039784.1.27",
          "form": "저는 고등어도 좋아햐요",
          "original_form": "저는 고등어도 좋아햐요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039784.1.28",
          "form": "고등어회 좋아하세요?",
          "original_form": "고등어회 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039784.1.29",
          "form": "고등어회는 자주 먹어보지는 못해서 맛이 어땠는지 기억이 안나네요 ㅎㅎ",
          "original_form": "고등어회는 자주 먹어보지는 못해서 맛이 어땠는지 기억이 안나네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039784.1.30",
          "form": "고등어 회 좋아하시나요?",
          "original_form": "고등어 회 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039784.1.31",
          "form": "고등어회 꼭 조만간 먹어봐야겠네여 ㅎㅎ",
          "original_form": "고등어회 꼭 조만간 먹어봐야겠네여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:41"
        }
      ]
