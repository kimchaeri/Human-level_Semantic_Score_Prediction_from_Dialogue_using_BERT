      "utterance": [
        {
          "id": "MDRW2100015975.1.1",
          "form": "어쩌다가 이시간까지 안주무고 계세요",
          "original_form": "어쩌다가 이시간까지 안주무고 계세요",
          "speaker_id": "2",
          "time": "20210826 04:31"
        },
        {
          "id": "MDRW2100015975.1.2",
          "form": "일찍자야 피부가 좋아진다는데..",
          "original_form": "일찍자야 피부가 좋아진다는데..",
          "speaker_id": "2",
          "time": "20210826 04:31"
        },
        {
          "id": "MDRW2100015975.1.3",
          "form": "그러게요ㅠㅠㅠ",
          "original_form": "그러게요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 04:31"
        },
        {
          "id": "MDRW2100015975.1.4",
          "form": "피부는",
          "original_form": "피부는",
          "speaker_id": "1",
          "time": "20210826 04:31"
        },
        {
          "id": "MDRW2100015975.1.5",
          "form": "물 많이 마시고",
          "original_form": "물 많이 마시고",
          "speaker_id": "1",
          "time": "20210826 04:31"
        },
        {
          "id": "MDRW2100015975.1.6",
          "form": "유산균 먹어보려구요..ㅎㅎ",
          "original_form": "유산균 먹어보려구요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 04:31"
        },
        {
          "id": "MDRW2100015975.1.7",
          "form": "물 많이 마시는게 진짜 중요한 것 같아요",
          "original_form": "물 많이 마시는게 진짜 중요한 것 같아요",
          "speaker_id": "2",
          "time": "20210826 04:31"
        },
        {
          "id": "MDRW2100015975.1.8",
          "form": "물 많이 마시는게",
          "original_form": "물 많이 마시는게",
          "speaker_id": "1",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015975.1.9",
          "form": "물 많이마신다음날은 왠지 피부가 매끈한 느낌이에요 ㅋㅋㅋ",
          "original_form": "물 많이마신다음날은 왠지 피부가 매끈한 느낌이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015975.1.10",
          "form": "효과있다없다 갈리던데",
          "original_form": "효과있다없다 갈리던데",
          "speaker_id": "1",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015975.1.11",
          "form": "저는 방광이 작아서 그런가",
          "original_form": "저는 방광이 작아서 그런가",
          "speaker_id": "1",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015975.1.12",
          "form": "저는 있다!!!",
          "original_form": "저는 있다!!!",
          "speaker_id": "2",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015975.1.13",
          "form": "화장실 가는게 무서워욬ㅋㅋㅋ",
          "original_form": "화장실 가는게 무서워욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015975.1.14",
          "form": "앜ㅋㅋㅋㅋㅋ 자다가 화장실이 급하면 당황스럽쥬 ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 자다가 화장실이 급하면 당황스럽쥬 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015975.1.15",
          "form": "네네 마려워서 깨면 기분이 안좋더라구요ㅠ",
          "original_form": "네네 마려워서 깨면 기분이 안좋더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015975.1.16",
          "form": "술마시고 난 다음날 손이 쪼글쪼글해져있잖아요",
          "original_form": "술마시고 난 다음날 손이 쪼글쪼글해져있잖아요",
          "speaker_id": "2",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015975.1.17",
          "form": "그런거 보면 수분이 정말 중요하구나 싶더라고요",
          "original_form": "그런거 보면 수분이 정말 중요하구나 싶더라고요",
          "speaker_id": "2",
          "time": "20210826 04:33"
        },
        {
          "id": "MDRW2100015975.1.18",
          "form": "그렇다면 자기 전에만 물을 덜마시는걸로...?",
          "original_form": "그렇다면 자기 전에만 물을 덜마시는걸로...?",
          "speaker_id": "2",
          "time": "20210826 04:33"
        },
        {
          "id": "MDRW2100015975.1.19",
          "form": "아! 맞아요ㅋㅋㅋ 수분ㅠㅠ 쪼글탱...",
          "original_form": "아! 맞아요ㅋㅋㅋ 수분ㅠㅠ 쪼글탱...",
          "speaker_id": "1",
          "time": "20210826 04:33"
        }
