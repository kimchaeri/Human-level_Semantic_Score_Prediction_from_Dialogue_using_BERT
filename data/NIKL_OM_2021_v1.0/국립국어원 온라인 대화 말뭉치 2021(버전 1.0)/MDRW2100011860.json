      "utterance": [
        {
          "id": "MDRW2100011860.1.1",
          "form": "난 전시회도 보고 싶고 영화도 보고 싶어",
          "original_form": "난 전시회도 보고 싶고 영화도 보고 싶어",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100011860.1.2",
          "form": "나도. 미술관이랑 영화 보고 싶어",
          "original_form": "나도. 미술관이랑 영화 보고 싶어",
          "speaker_id": "1",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100011860.1.3",
          "form": "고흐 전시회 했으면 좋겠다",
          "original_form": "고흐 전시회 했으면 좋겠다",
          "speaker_id": "2",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100011860.1.4",
          "form": "고흐는 예전에 봤으니까 다른 화가 보러 가자",
          "original_form": "고흐는 예전에 봤으니까 다른 화가 보러 가자",
          "speaker_id": "1",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100011860.1.5",
          "form": "난 고흐 봐도 봐도 좋던데",
          "original_form": "난 고흐 봐도 봐도 좋던데",
          "speaker_id": "2",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100011860.1.6",
          "form": "난 둘다 상관없어",
          "original_form": "난 둘다 상관없어",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011860.1.7",
          "form": "영화는 베놈 보고 싶다고 했었나",
          "original_form": "영화는 베놈 보고 싶다고 했었나",
          "speaker_id": "2",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011860.1.8",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011860.1.9",
          "form": "걔 진짜 징그럽게 생겼어",
          "original_form": "걔 진짜 징그럽게 생겼어",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011860.1.10",
          "form": "근데 멋있잖아 ㅎㅎ",
          "original_form": "근데 멋있잖아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011860.1.11",
          "form": "그러니까 걔가 영웅인 거지?",
          "original_form": "그러니까 걔가 영웅인 거지?",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011860.1.12",
          "form": "악당이랑 영웅 그 사이야 정확히는",
          "original_form": "악당이랑 영웅 그 사이야 정확히는",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011860.1.13",
          "form": "아 약간 다크히어로물이야?",
          "original_form": "아 약간 다크히어로물이야?",
