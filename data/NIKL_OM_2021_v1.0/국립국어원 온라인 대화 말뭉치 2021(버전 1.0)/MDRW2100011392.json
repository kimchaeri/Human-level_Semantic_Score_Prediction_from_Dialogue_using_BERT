      "utterance": [
        {
          "id": "MDRW2100011392.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100011392.1.2",
          "form": "넵 강아지있어요~",
          "original_form": "넵 강아지있어요~",
          "speaker_id": "2",
          "time": "20210825 08:47"
        },
        {
          "id": "MDRW2100011392.1.3",
          "form": "종이 뭐에요??",
          "original_form": "종이 뭐에요??",
          "speaker_id": "1",
          "time": "20210825 11:28"
        },
        {
          "id": "MDRW2100011392.1.4",
          "form": "말티즈에요!",
          "original_form": "말티즈에요!",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100011392.1.5",
          "form": "어멋귀엽겠다 ㅠ ㅠ",
          "original_form": "어멋귀엽겠다 ㅠ ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100011392.1.6",
          "form": "귀여워요ㅋㅋ 이제 한살반정도 돼서 똥꼬발랄하답니다",
          "original_form": "귀여워요ㅋㅋ 이제 한살반정도 돼서 똥꼬발랄하답니다",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100011392.1.7",
          "form": "오 ㅋㅋㅋㄱ저는 햄스터키우고있어요",
          "original_form": "오 ㅋㅋㅋㄱ저는 햄스터키우고있어요",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100011392.1.8",
          "form": "헉 햄스터도 엄청 귀엽죠ㅠㅠ 어떤 종류에요?",
          "original_form": "헉 햄스터도 엄청 귀엽죠ㅠㅠ 어떤 종류에요?",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100011392.1.9",
          "form": "정글리안이요 ㅋㅋ벌써 키운지 일년 다 되가요 ㅋㅋ",
          "original_form": "정글리안이요 ㅋㅋ벌써 키운지 일년 다 되가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100011392.1.10",
          "form": "오~ 그 전에도 햄스터 키우셨어요? 아니면 처음이신거에요?",
          "original_form": "오~ 그 전에도 햄스터 키우셨어요? 아니면 처음이신거에요?",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100011392.1.11",
          "form": "초등학생때 키우고 이번이 두번째요 ㅠ 그땐 아무것도 모르고 합사시켜서 ㅠ 암컷이 수컷 잡아먹었어요 ㅠ",
          "original_form": "초등학생때 키우고 이번이 두번째요 ㅠ 그땐 아무것도 모르고 합사시켜서 ㅠ 암컷이 수컷 잡아먹었어요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100011392.1.12",
          "form": "헉 완전 놀랐겠어요ㅠㅠ 어릴때라 몰랐으니..",
          "original_form": "헉 완전 놀랐겠어요ㅠㅠ 어릴때라 몰랐으니..",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100011392.1.13",
          "form": "그때 보고 너무 충격먹어서 오열했던 기억이 나요 ㅠ",
          "original_form": "그때 보고 너무 충격먹어서 오열했던 기억이 나요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100011392.1.14",
          "form": "그럴만하죠ㅠㅠ 지금 키우는 햄스터는 주인을 알아보나요? 종마다 다르다던데~",
          "original_form": "그럴만하죠ㅠㅠ 지금 키우는 햄스터는 주인을 알아보나요? 종마다 다르다던데~",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100011392.1.15",
          "form": "자기자식도 못 알아보는걸요 ㅠ",
          "original_form": "자기자식도 못 알아보는걸요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100011392.1.16",
          "form": "앗..그렇군요..",
          "original_form": "앗..그렇군요..",
          "speaker_id": "2",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100011392.1.17",
          "form": "ㅋㅋㅋ강아지키우고싶었는데 지금사는곳이 반려동물 금지에요 ㅠ",
          "original_form": "ㅋㅋㅋ강아지키우고싶었는데 지금사는곳이 반려동물 금지에요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100011392.1.18",
          "form": "은근 금지인곳이 많더라구요ㅠㅠ",
          "original_form": "은근 금지인곳이 많더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 08:28"
        },
        {
          "id": "MDRW2100011392.1.19",
          "form": "제가 세들어사는곳은 냄새나고 벽지 물어뜯는다고 안된댔어요 ㅠ",
          "original_form": "제가 세들어사는곳은 냄새나고 벽지 물어뜯는다고 안된댔어요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:12"
        }
      ]
