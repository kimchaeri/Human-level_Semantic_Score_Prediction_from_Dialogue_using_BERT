      "utterance": [
        {
          "id": "MDRW2100019680.1.1",
          "form": "나도 한동안 했었지",
          "original_form": "나도 한동안 했었지",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019680.1.2",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019680.1.3",
          "form": "예전에 집에서 염색했었을 때",
          "original_form": "예전에 집에서 염색했었을 때",
          "speaker_id": "2",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019680.1.4",
          "form": "그건 염색하면서 영양 넣는 거고",
          "original_form": "그건 염색하면서 영양 넣는 거고",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019680.1.5",
          "form": "아 그래? 나 미용실에서 염색해본 적 없어서",
          "original_form": "아 그래? 나 미용실에서 염색해본 적 없어서",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019680.1.6",
          "form": "보통 약간이라도 다 영양 넣어줘",
          "original_form": "보통 약간이라도 다 영양 넣어줘",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019680.1.7",
          "form": "그건 그렀지",
          "original_form": "그건 그렀지",
          "speaker_id": "2",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019680.1.8",
          "form": "머리결이 너무 상하다 보니까",
          "original_form": "머리결이 너무 상하다 보니까",
          "speaker_id": "1",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019680.1.9",
          "form": "어쩔 수 없지",
          "original_form": "어쩔 수 없지",
          "speaker_id": "2",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019680.1.10",
          "form": "그래도 한번씩 하고싶어져ㅋㅋㅋ",
          "original_form": "그래도 한번씩 하고싶어져ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019680.1.11",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019680.1.12",
          "form": "나 또 드릉드릉해 요즘",
          "original_form": "나 또 드릉드릉해 요즘",
          "speaker_id": "1",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019680.1.13",
          "form": "예전에는 염색보다 파마가 더 하고싶었는데 지금은 반대야",
          "original_form": "예전에는 염색보다 파마가 더 하고싶었는데 지금은 반대야",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019680.1.14",
          "form": "염색이 더 하고싶어?",
          "original_form": "염색이 더 하고싶어?",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019680.1.15",
