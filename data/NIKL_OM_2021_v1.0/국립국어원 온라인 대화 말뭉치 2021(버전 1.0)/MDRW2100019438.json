      "utterance": [
        {
          "id": "MDRW2100019438.1.1",
          "form": "아 맛있겠다",
          "original_form": "아 맛있겠다",
          "speaker_id": "2",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019438.1.2",
          "form": "케이크랑 커피랑 퍼묵퍼묵하고싶어요",
          "original_form": "케이크랑 커피랑 퍼묵퍼묵하고싶어요",
          "speaker_id": "2",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019438.1.3",
          "form": "저도요ㅜㅜ 빵이랑",
          "original_form": "저도요ㅜㅜ 빵이랑",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019438.1.4",
          "form": "우유랑",
          "original_form": "우유랑",
          "speaker_id": "2",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019438.1.5",
          "form": "아 진짜 맛있겠다",
          "original_form": "아 진짜 맛있겠다",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019438.1.6",
          "form": "떡볶이보다 빵이 당기는 날이네요",
          "original_form": "떡볶이보다 빵이 당기는 날이네요",
          "speaker_id": "2",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019438.1.7",
          "form": "진심...",
          "original_form": "진심...",
          "speaker_id": "1",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019438.1.8",
          "form": "러스크도 먹고싶어요",
          "original_form": "러스크도 먹고싶어요",
          "speaker_id": "2",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019438.1.9",
          "form": "진짜 뚱카롱 다섯개 먹고싶어요",
          "original_form": "진짜 뚱카롱 다섯개 먹고싶어요",
          "speaker_id": "1",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019438.1.10",
          "form": "ㅠㅠ 근처에 없어요?",
          "original_form": "ㅠㅠ 근처에 없어요?",
          "speaker_id": "2",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019438.1.11",
          "form": "배달시키면 그만인데 참는중이에요",
          "original_form": "배달시키면 그만인데 참는중이에요",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019438.1.12",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "2",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019438.1.13",
          "form": "밀가루 끊는 중이거든요",
          "original_form": "밀가루 끊는 중이거든요",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019438.1.14",
          "form": "휴...",
          "original_form": "휴...",
          "speaker_id": "1",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019438.1.15",
          "form": "마카롱 너무 먹고싶어요..",
          "original_form": "마카롱 너무 먹고싶어요..",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019438.1.16",
          "form": "그냥 드세요ㅋㅋ",
          "original_form": "그냥 드세요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019438.1.17",
          "form": "먹는 얘기가 젤 재밌죠",
          "original_form": "먹는 얘기가 젤 재밌죠",
