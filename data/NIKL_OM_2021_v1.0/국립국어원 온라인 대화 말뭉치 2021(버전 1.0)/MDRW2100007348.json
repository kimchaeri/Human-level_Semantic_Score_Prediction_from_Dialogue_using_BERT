      "utterance": [
        {
          "id": "MDRW2100007348.1.1",
          "form": "좋은 날 올거에요^^",
          "original_form": "좋은 날 올거에요^^",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007348.1.2",
          "form": "말씀 감사합니다ㅎㅎ",
          "original_form": "말씀 감사합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007348.1.3",
          "form": "연애는 잘하고 계십니까 ㅎㅎ",
          "original_form": "연애는 잘하고 계십니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007348.1.4",
          "form": "앗.. 아뇨ㅋㅋㅋㅋ 연애 쪽은 잼병입니다ㅋㅋㅋ",
          "original_form": "앗.. 아뇨ㅋㅋㅋㅋ 연애 쪽은 잼병입니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007348.1.5",
          "form": "혹시.. 하고 계신가요..?",
          "original_form": "혹시.. 하고 계신가요..?",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007348.1.6",
          "form": "아니요 연애는 끝난지 오래죠",
          "original_form": "아니요 연애는 끝난지 오래죠",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007348.1.7",
          "form": "결혼했습니다ㅎㅎ",
          "original_form": "결혼했습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007348.1.8",
          "form": "오 결혼하셨군요..",
          "original_form": "오 결혼하셨군요..",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007348.1.9",
          "form": "얼마나 되셨을까요?",
          "original_form": "얼마나 되셨을까요?",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007348.1.10",
          "form": "작년에 했습니다!!!",
          "original_form": "작년에 했습니다!!!",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007348.1.11",
          "form": "헉 얼마 안되셨네요...",
          "original_form": "헉 얼마 안되셨네요...",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007348.1.12",
          "form": "그럼 신혼..?이시겠네요..",
          "original_form": "그럼 신혼..?이시겠네요..",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007348.1.13",
          "form": "ㅎㅎ 같이 산지는 1년이 넘어서",
          "original_form": "ㅎㅎ 같이 산지는 1년이 넘어서",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007348.1.14",
          "form": "아아 그래도 배우자분이랑 행복하실 것 같네요ㅎㅎ",
          "original_form": "아아 그래도 배우자분이랑 행복하실 것 같네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007348.1.15",
          "form": "작년이면.. 그때도 코로나때문에 결혼식하기 조금 힘들었을 것 같은데..",
          "original_form": "작년이면.. 그때도 코로나때문에 결혼식하기 조금 힘들었을 것 같은데..",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007348.1.16",
          "form": "다행스럽게 1단계였습니다!",
          "original_form": "다행스럽게 1단계였습니다!",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007348.1.17",
          "form": "연애는 왜 안하세요ㅎㅎ",
          "original_form": "연애는 왜 안하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007348.1.18",
          "form": "연애.. 제가 주변에 이성친구도 많이 없고",
          "original_form": "연애.. 제가 주변에 이성친구도 많이 없고",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007348.1.19",
          "form": "성격상 연애랑은 거리가 먼 편이라..ㅋㅋㅋ",
          "original_form": "성격상 연애랑은 거리가 먼 편이라..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007348.1.20",
          "form": "ㅎㅎ인연이 있으시면 그때 만날겁니다",
          "original_form": "ㅎㅎ인연이 있으시면 그때 만날겁니다",
          "speaker_id": "1",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007348.1.21",
          "form": "감사합니다..ㅋㅋ 빨리 만나보고 싶네요..",
          "original_form": "감사합니다..ㅋㅋ 빨리 만나보고 싶네요..",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007348.1.22",
          "form": "ㅎㅎ 인연은 언젠가 올거에요",
          "original_form": "ㅎㅎ 인연은 언젠가 올거에요",
          "speaker_id": "1",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007348.1.23",
          "form": "ㅎㅎ 열심히 준비해야겠네요",
          "original_form": "ㅎㅎ 열심히 준비해야겠네요",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007348.1.24",
          "form": "지금 삶에 충실하시다보면 어느 순간에",
          "original_form": "지금 삶에 충실하시다보면 어느 순간에",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007348.1.25",
          "form": "아아..",
          "original_form": "아아..",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007348.1.26",
          "form": "일단 현재에 충실해야겠군요..",
          "original_form": "일단 현재에 충실해야겠군요..",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007348.1.27",
          "form": "네 일단 행복하게 사세요",
          "original_form": "네 일단 행복하게 사세요",
          "speaker_id": "1",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007348.1.28",
          "form": "행복하게...",
          "original_form": "행복하게...",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007348.1.29",
          "form": "최대한 행복하게 살아보겠습니다...",
          "original_form": "최대한 행복하게 살아보겠습니다...",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007348.1.30",
          "form": "카르페디엠!",
          "original_form": "카르페디엠!",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007348.1.31",
          "form": "현실에 충실하십셔",
          "original_form": "현실에 충실하십셔",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007348.1.32",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007348.1.33",
          "form": "조언 정말 감사합니다",
          "original_form": "조언 정말 감사합니다",
          "speaker_id": "2",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007348.1.34",
          "form": "네 언제 주무세요?",
          "original_form": "네 언제 주무세요?",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007348.1.35",
          "form": "저 이제 곧 자려구요ㅎㅎ",
          "original_form": "저 이제 곧 자려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007348.1.36",
          "form": "네 졸리시면 주무시고 내일 또 이야기 나눠요",
          "original_form": "네 졸리시면 주무시고 내일 또 이야기 나눠요",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007348.1.37",
          "form": "넵.. 그럼 전 먼저 자러 가겠습니다",
          "original_form": "넵.. 그럼 전 먼저 자러 가겠습니다",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007348.1.38",
          "form": "오늘 즐거웠어요!",
          "original_form": "오늘 즐거웠어요!",
          "speaker_id": "2",
          "time": "20210822 02:06"
        }
      ]
    }
