        {
          "id": "MDRW2100043472.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043472.1.2",
          "form": "안녕하세요 식사하셨나요?",
          "original_form": "안녕하세요 식사하셨나요?",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043472.1.3",
          "form": "지금막 베이컨가지말이에 먹엇어요",
          "original_form": "지금막 베이컨가지말이에 먹엇어요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043472.1.4",
          "form": "우와 그럼 구운 건가요?",
          "original_form": "우와 그럼 구운 건가요?",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043472.1.5",
          "form": "에어프라이어에 햇어요ㅋ",
          "original_form": "에어프라이어에 햇어요ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043472.1.6",
          "form": "간이 딱 맞아서 맛있겠네요 ㅋㅋ",
          "original_form": "간이 딱 맞아서 맛있겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043472.1.7",
          "form": "네 요리못하는데 간 따로안해도 되니 좋구요",
          "original_form": "네 요리못하는데 간 따로안해도 되니 좋구요",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043472.1.8",
          "form": "집에 가지는 있는데 베이컨 사다 한 번 해봐야겠어요 ㅋㅋ",
          "original_form": "집에 가지는 있는데 베이컨 사다 한 번 해봐야겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043472.1.9",
          "form": "네 그거 속에 버섯같은거도 넣어도 대고 좋아요",
          "original_form": "네 그거 속에 버섯같은거도 넣어도 대고 좋아요",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043472.1.10",
          "form": "새송이버섯이 좋으려나요?",
          "original_form": "새송이버섯이 좋으려나요?",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043472.1.11",
          "form": "어떤 모양인지 찾아봐야겠네요ㅎㅎ",
          "original_form": "어떤 모양인지 찾아봐야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043472.1.12",
          "form": "안에 아무야채나 다 될거같아요",
          "original_form": "안에 아무야채나 다 될거같아요",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043472.1.13",
          "form": "있는 거 넣고 해봐야겠네요",
          "original_form": "있는 거 넣고 해봐야겠네요",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043472.1.14",
          "form": "네 함 해보세요",
          "original_form": "네 함 해보세요",
          "speaker_id": "1",
