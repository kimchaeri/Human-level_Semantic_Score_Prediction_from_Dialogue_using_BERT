      "utterance": [
        {
          "id": "MDRW2100047193.1.1",
          "form": "딱 그 상태인 식도염. 화합니다아주",
          "original_form": "딱 그 상태인 식도염. 화합니다아주",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100047193.1.2",
          "form": "님 그 달고사는 병 없어요?",
          "original_form": "님 그 달고사는 병 없어요?",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100047193.1.3",
          "form": "만성적인...병?",
          "original_form": "만성적인...병?",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047193.1.4",
          "form": "저 변비",
          "original_form": "저 변비",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047193.1.5",
          "form": "변비...변비요...!!!!!",
          "original_form": "변비...변비요...!!!!!",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047193.1.6",
          "form": "차전자피인가 그거 먹고 겨우 싸요",
          "original_form": "차전자피인가 그거 먹고 겨우 싸요",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047193.1.7",
          "form": "변비...제외라고 할걸.",
          "original_form": "변비...제외라고 할걸.",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047193.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047193.1.9",
          "form": "저는 푸룬 푸룬이요",
          "original_form": "저는 푸룬 푸룬이요",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047193.1.10",
          "form": "푸룬 직빵이에요",
          "original_form": "푸룬 직빵이에요",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047193.1.11",
          "form": "그 자두 알맹이 그거",
          "original_form": "그 자두 알맹이 그거",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.12",
          "form": "푸룬 효과 있어요?!?!?",
          "original_form": "푸룬 효과 있어요?!?!?",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.13",
          "form": "그게짱이거든요",
          "original_form": "그게짱이거든요",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.14",
          "form": "네!!!!!!!!",
          "original_form": "네!!!!!!!!",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.15",
          "form": "홀",
          "original_form": "홀",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.16",
          "form": "주스말고 알맹이있잖아요!!!!!",
          "original_form": "주스말고 알맹이있잖아요!!!!!",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.17",
          "form": "그자체를 먹어야해요",
          "original_form": "그자체를 먹어야해요",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.18",
          "form": "걔가진짜...짱",
          "original_form": "걔가진짜...짱",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.19",
          "form": "홀 저는...전...!! 그닥..!!",
          "original_form": "홀 저는...전...!! 그닥..!!",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.20",
          "form": "아 헐 주스로 먹어서 그런가보ㅓ요",
          "original_form": "아 헐 주스로 먹어서 그런가보ㅓ요",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.21",
          "form": "아놔...!!!!!!",
          "original_form": "아놔...!!!!!!",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.22",
          "form": "사바사가 있구나.....하 씁...",
          "original_form": "사바사가 있구나.....하 씁...",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.23",
          "form": "근데 전 주스는 효과없었ㅇ",
          "original_form": "근데 전 주스는 효과없었ㅇ",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.24",
          "form": "네!!!!!!!!",
          "original_form": "네!!!!!!!!",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.25",
          "form": "저 알맹이 먹어야만 효과보거든요",
          "original_form": "저 알맹이 먹어야만 효과보거든요",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047193.1.26",
          "form": "알맹이는 어디서 팔지",
          "original_form": "알맹이는 어디서 팔지",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047193.1.27",
          "form": "올영인가?!!",
          "original_form": "올영인가?!!",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047193.1.28",
          "form": "전 약국에서 샀는데 아마 올영도 팔거예요!!",
          "original_form": "전 약국에서 샀는데 아마 올영도 팔거예요!!",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047193.1.29",
          "form": "그거 진짜 효과좋아요 제기준으로...",
          "original_form": "그거 진짜 효과좋아요 제기준으로...",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100047193.1.30",
          "form": "오케이 차전자피로도 안뚫리는 날",
          "original_form": "오케이 차전자피로도 안뚫리는 날",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100047193.1.31",
          "form": "도전한다 푸룬",
          "original_form": "도전한다 푸룬",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100047193.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100047193.1.33",
          "form": "님 다른 만성병은요?!",
          "original_form": "님 다른 만성병은요?!",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100047193.1.34",
          "form": "아 오늘 그 눈썹에 모낭염 난 것 같아서",
          "original_form": "아 오늘 그 눈썹에 모낭염 난 것 같아서",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100047193.1.35",
          "form": "불안혀요",
          "original_form": "불안혀요",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100047193.1.36",
          "form": "전. 위염식도염인데. 변비까지 있는 아주 완전체라고요",
          "original_form": "전. 위염식도염인데. 변비까지 있는 아주 완전체라고요",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100047193.1.37",
          "form": "아헐",
          "original_form": "아헐",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100047193.1.38",
          "form": "이런적이..업ㄱ었는디?",
          "original_form": "이런적이..업ㄱ었는디?",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100047193.1.39",
          "form": "헐.....",
          "original_form": "헐.....",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100047193.1.40",
          "form": "모낭염이면...피부과가서...",
          "original_form": "모낭염이면...피부과가서...",
