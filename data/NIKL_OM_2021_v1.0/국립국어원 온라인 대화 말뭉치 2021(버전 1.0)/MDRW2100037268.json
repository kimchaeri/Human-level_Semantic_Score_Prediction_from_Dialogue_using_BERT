      "utterance": [
        {
          "id": "MDRW2100037268.1.1",
          "form": "지금 어떤 헤어스탈이세요?",
          "original_form": "지금 어떤 헤어스탈이세요?",
          "speaker_id": "1",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100037268.1.2",
          "form": "어깨 밑정도 오는 머리에요!",
          "original_form": "어깨 밑정도 오는 머리에요!",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037268.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 22:06"
        },
        {
          "id": "MDRW2100037268.1.4",
          "form": "전 단발이요 !",
          "original_form": "전 단발이요 !",
          "speaker_id": "1",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037268.1.5",
          "form": "곧 거지존으로 갈듯합니다 ㅎㅎ",
          "original_form": "곧 거지존으로 갈듯합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100037268.1.6",
          "form": "앗ㅋㅋ 자르실 계획 있으신가요?",
          "original_form": "앗ㅋㅋ 자르실 계획 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037268.1.7",
          "form": "추석전에 한번 정리하려구여!",
          "original_form": "추석전에 한번 정리하려구여!",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037268.1.8",
          "form": "거지존이 정말 안어울리거든요 ㅎㅎ",
          "original_form": "거지존이 정말 안어울리거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037268.1.9",
          "form": "앗ㅋㅋ 그래두 단발 어울리시니 부럽습니닷",
          "original_form": "앗ㅋㅋ 그래두 단발 어울리시니 부럽습니닷",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037268.1.10",
          "form": "저는 더 자르고싶은데 용기가 안나네요",
          "original_form": "저는 더 자르고싶은데 용기가 안나네요",
          "speaker_id": "2",
          "time": "20210831 22:14"
        },
        {
          "id": "MDRW2100037268.1.11",
          "form": "헐 왜요ㅜㅜ !!!",
          "original_form": "헐 왜요ㅜㅜ !!!",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037268.1.12",
          "form": "얼굴이 커서 그런지 양락씨.. 그게 저에요ㅋㅋ",
          "original_form": "얼굴이 커서 그런지 양락씨.. 그게 저에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037268.1.13",
          "form": "헐 저두 얼굴 큰데",
          "original_form": "헐 저두 얼굴 큰데",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037268.1.14",
          "form": "단발하구 펌하면 좀 가려져서",
          "original_form": "단발하구 펌하면 좀 가려져서",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037268.1.15",
          "form": "괜찮던데요 !!",
          "original_form": "괜찮던데요 !!",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037268.1.16",
          "form": "어엇 많이 커서.. 가족 인정이라 용기가 안났어욤",
          "original_form": "어엇 많이 커서.. 가족 인정이라 용기가 안났어욤",
          "speaker_id": "2",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100037268.1.17",
          "form": "펌 하면 좀 나아보일 수 있는거군요?!",
          "original_form": "펌 하면 좀 나아보일 수 있는거군요?!",
          "speaker_id": "2",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100037268.1.18",
          "form": "네네 !! 펌하시구 옆머리 내달라고 해보셔요 !",
          "original_form": "네네 !! 펌하시구 옆머리 내달라고 해보셔요 !",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100037268.1.19",
          "form": "c컬 말구 cs 섞인걸루 하세요!",
          "original_form": "c컬 말구 cs 섞인걸루 하세요!",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100037268.1.20",
          "form": "오오.. 앞머리도 있으신가용?",
          "original_form": "오오.. 앞머리도 있으신가용?",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100037268.1.21",
          "form": "네 ! 근데 앞머리 없을때도 해봤는데  괜찮아요 ㅎㅎ",
          "original_form": "네 ! 근데 앞머리 없을때도 해봤는데  괜찮아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037268.1.22",
          "form": "지금은 있는데 없는것도 괜찮았어요!",
          "original_form": "지금은 있는데 없는것도 괜찮았어요!",
          "speaker_id": "1",
          "time": "20210831 22:27"
        }
