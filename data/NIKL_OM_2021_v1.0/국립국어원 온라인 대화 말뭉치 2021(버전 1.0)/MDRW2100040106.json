      "utterance": [
        {
          "id": "MDRW2100040106.1.1",
          "form": "젤 사랑하지만 보내쥬ㅓ여할지",
          "original_form": "젤 사랑하지만 보내쥬ㅓ여할지",
          "speaker_id": "1",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100040106.1.2",
          "form": "반려동뮬 키우세요,",
          "original_form": "반려동뮬 키우세요,",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040106.1.3",
          "form": "무슨 어풀이에요?",
          "original_form": "무슨 어풀이에요?",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040106.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040106.1.5",
          "form": "저도 과제로 발표영상 찍어봤는데 넘 힘둘었어요 파워초인트 녹화",
          "original_form": "저도 과제로 발표영상 찍어봤는데 넘 힘둘었어요 파워초인트 녹화",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040106.1.6",
          "form": "저는 키웁니다 !",
          "original_form": "저는 키웁니다 !",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040106.1.7",
          "form": "vllo",
          "original_form": "vllo",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040106.1.8",
          "form": "오 저도 그고 써요",
          "original_form": "오 저도 그고 써요",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040106.1.9",
          "form": "유튜브 편집할때",
          "original_form": "유튜브 편집할때",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040106.1.10",
          "form": "찍는것도 블로로 되는균요",
          "original_form": "찍는것도 블로로 되는균요",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040106.1.11",
          "form": "미술이라 실기모습찍고 목소리 녹음하고",
          "original_form": "미술이라 실기모습찍고 목소리 녹음하고",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040106.1.12",
          "form": "핸드폰으로 동영상찍고 편집만~",
          "original_form": "핸드폰으로 동영상찍고 편집만~",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040106.1.13",
          "form": "아하 영상찍고 저걸로 편집",
          "original_form": "아하 영상찍고 저걸로 편집",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040106.1.14",
          "form": "저는 반려동물 유튜브 하거든요",
          "original_form": "저는 반려동물 유튜브 하거든요",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040106.1.15",
          "form": "그거 블로로 편집 합니다",
          "original_form": "그거 블로로 편집 합니다",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040106.1.16",
          "form": "아~~어떤동물키우세요?",
          "original_form": "아~~어떤동물키우세요?",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040106.1.17",
          "form": "전 애셋 키우다보니동물키울 여유가",
          "original_form": "전 애셋 키우다보니동물키울 여유가",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100040106.1.18",
          "form": "오 아이를 셋",
          "original_form": "오 아이를 셋",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040106.1.19",
          "form": "전 미혼이어서",
          "original_form": "전 미혼이어서",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040106.1.20",
          "form": "강아지키워요",
          "original_form": "강아지키워요",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040106.1.21",
          "form": "온 정성을 다해",
          "original_form": "온 정성을 다해",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040106.1.22",
          "form": "이쁘겠네요",
          "original_form": "이쁘겠네요",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040106.1.23",
          "form": "정성을다해 키우시니 얼마나 더 이쁠까?",
          "original_form": "정성을다해 키우시니 얼마나 더 이쁠까?",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040106.1.24",
          "form": "맞아요 넘 이쁜데",
          "original_form": "맞아요 넘 이쁜데",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040106.1.25",
          "form": "아프면 넘 슬퍼요",
          "original_form": "아프면 넘 슬퍼요",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100040106.1.26",
          "form": "나쁜말이 뭘까요?",
          "original_form": "나쁜말이 뭘까요?",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040106.1.27",
          "form": "이상하네요",
          "original_form": "이상하네요",
          "speaker_id": "2",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040106.1.28",
          "form": "띄어쓰기 안하묜 그렇더라고요",
          "original_form": "띄어쓰기 안하묜 그렇더라고요",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100040106.1.29",
          "form": "아~~그렇군요",
          "original_form": "아~~그렇군요",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100040106.1.30",
          "form": "몰랐네요",
          "original_form": "몰랐네요",
          "speaker_id": "2",
          "time": "20210904 15:51"
        }
      ]
    }
  ]
