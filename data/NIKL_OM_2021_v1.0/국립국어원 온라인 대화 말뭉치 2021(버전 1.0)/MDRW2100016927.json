      "utterance": [
        {
          "id": "MDRW2100016927.1.1",
          "form": "먹는 음식에는 돈을 아끼지 않는 자칭 미식가에여",
          "original_form": "먹는 음식에는 돈을 아끼지 않는 자칭 미식가에여",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016927.1.2",
          "form": "훠궈 좋아하시죠",
          "original_form": "훠궈 좋아하시죠",
          "speaker_id": "1",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016927.1.3",
          "form": "그럴것같은데",
          "original_form": "그럴것같은데",
          "speaker_id": "1",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016927.1.4",
          "form": "양꼬치랑 훠궈 좋아하실 상인데",
          "original_form": "양꼬치랑 훠궈 좋아하실 상인데",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016927.1.5",
          "form": "훠궈는 매워서 못먹고",
          "original_form": "훠궈는 매워서 못먹고",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016927.1.6",
          "form": "양꼬치는 먹죠ㅋ",
          "original_form": "양꼬치는 먹죠ㅋ",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016927.1.7",
          "form": "양갈비 먹어봤어요?",
          "original_form": "양갈비 먹어봤어요?",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016927.1.8",
          "form": "(먹부심ㅋㅋㅋㅋㅋㅋ)",
          "original_form": "(먹부심ㅋㅋㅋㅋㅋㅋ)",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016927.1.9",
          "form": "양갈비 집에서 구워먹어봤습니다 :(",
          "original_form": "양갈비 집에서 구워먹어봤습니다 :(",
          "speaker_id": "1",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016927.1.10",
          "form": "딱 한번이지만!",
          "original_form": "딱 한번이지만!",
          "speaker_id": "1",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016927.1.11",
          "form": "그거 연기많이 날텐데...!",
          "original_form": "그거 연기많이 날텐데...!",
          "speaker_id": "2",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016927.1.12",
          "form": "냄새는 어떻게...!",
          "original_form": "냄새는 어떻게...!",
          "speaker_id": "2",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016927.1.13",
          "form": "별로 안났던것같은데",
          "original_form": "별로 안났던것같은데",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016927.1.14",
          "form": "혹시 그냥 소갈비를 양갈비라고 속여서 먹은건가...!",
          "original_form": "혹시 그냥 소갈비를 양갈비라고 속여서 먹은건가...!",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016927.1.15",
          "form": "누나가 구워줬는데!",
          "original_form": "누나가 구워줬는데!",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016927.1.16",
          "form": "누님이 고기굽기 마스터신가",
          "original_form": "누님이 고기굽기 마스터신가",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016927.1.17",
          "form": "갑자기 갈비먹고 싶어요",
          "original_form": "갑자기 갈비먹고 싶어요",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016927.1.18",
          "form": "평양냉면에 갈비",
          "original_form": "평양냉면에 갈비",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016927.1.19",
          "form": "크 배우신분",
          "original_form": "크 배우신분",
          "speaker_id": "1",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016927.1.20",
          "form": "저는 부산밀면에 삼겹살 땡기네요",
          "original_form": "저는 부산밀면에 삼겹살 땡기네요",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016927.1.21",
          "form": "ㅜㅜ 죽이나 먹어야한다니",
          "original_form": "ㅜㅜ 죽이나 먹어야한다니",
          "speaker_id": "2",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016927.1.22",
          "form": "전 백신 2차 맞으면 당장 평냉에 갈비먹으러 갈거에요",
          "original_form": "전 백신 2차 맞으면 당장 평냉에 갈비먹으러 갈거에요",
          "speaker_id": "2",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016927.1.23",
          "form": "갈비먹고 제것도 주머니에 한점만 넣어와요",
          "original_form": "갈비먹고 제것도 주머니에 한점만 넣어와요",
          "speaker_id": "1",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100016927.1.24",
          "form": "갈비 못먹은지 3년쯤 된듯...",
          "original_form": "갈비 못먹은지 3년쯤 된듯...",
          "speaker_id": "1",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100016927.1.25",
          "form": "...?저 혼자 다 먹을건데요",
          "original_form": "...?저 혼자 다 먹을건데요",
          "speaker_id": "2",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016927.1.26",
          "form": "양심은 없으면서 욕심은 그득하시네요",
          "original_form": "양심은 없으면서 욕심은 그득하시네요",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016927.1.27",
          "form": "쒸익쒸익",
          "original_form": "쒸익쒸익",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016927.1.28",
          "form": "아 아쉽다",
          "original_form": "아 아쉽다",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016927.1.29",
          "form": "어림도없지",
