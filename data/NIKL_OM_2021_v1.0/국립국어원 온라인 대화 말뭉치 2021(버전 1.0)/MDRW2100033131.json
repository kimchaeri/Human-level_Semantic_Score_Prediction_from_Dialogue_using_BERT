      "utterance": [
        {
          "id": "MDRW2100033131.1.1",
          "form": "안녕하십니까",
          "original_form": "안녕하십니까",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100033131.1.2",
          "form": "저 말투 봇이야 아님 name1 따라한거야",
          "original_form": "저 말투 봇이야 아님 &name1& 따라한거야",
          "speaker_id": "2",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100033131.1.3",
          "form": "건강을 위해서는 체혈요법이 탁월합니다",
          "original_form": "건강을 위해서는 체혈요법이 탁월합니다",
          "speaker_id": "1",
          "time": "20210906 17:03"
        },
        {
          "id": "MDRW2100033131.1.4",
          "form": "그렇군요 오이는 어때요",
          "original_form": "그렇군요 오이는 어때요",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033131.1.5",
          "form": "저에게 검진받아보시는거 어떤가요",
          "original_form": "저에게 검진받아보시는거 어떤가요",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033131.1.6",
          "form": "에이 의사양반 난 멀쩡한걸",
          "original_form": "에이 의사양반 난 멀쩡한걸",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033131.1.7",
          "form": "오이는 제 고향에서 나지 않아서 모릅니다..",
          "original_form": "오이는 제 고향에서 나지 않아서 모릅니다..",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033131.1.8",
          "form": "다이어트도 은근 하고 있다고",
          "original_form": "다이어트도 은근 하고 있다고",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033131.1.9",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033131.1.10",
          "form": "다이어트에 탁월한 구토제는 어떻습니까?",
          "original_form": "다이어트에 탁월한 구토제는 어떻습니까?",
          "speaker_id": "1",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033131.1.11",
          "form": "그러고 보니 당신 옷 너무 더워보인달까?",
          "original_form": "그러고 보니 당신 옷 너무 더워보인달까?",
          "speaker_id": "2",
          "time": "20210906 17:04"
        },
        {
          "id": "MDRW2100033131.1.12",
          "form": "사양하겟어",
          "original_form": "사양하겟어",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033131.1.13",
          "form": "나처럼 가슴을 뽝",
          "original_form": "나처럼 가슴을 뽝",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033131.1.14",
          "form": "어때",
          "original_form": "어때",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033131.1.15",
          "form": "당신은 까발리고 다니시잖아요",
          "original_form": "당신은 까발리고 다니시잖아요",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033131.1.16",
          "form": "경찰에 잡혀갑니다",
          "original_form": "경찰에 잡혀갑니다",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033131.1.17",
          "form": "이정도는 패션이라고~",
          "original_form": "이정도는 패션이라고~",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033131.1.18",
          "form": "모자만 빼보는건 어때",
          "original_form": "모자만 빼보는건 어때",
          "speaker_id": "2",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033131.1.19",
          "form": "건강에 좋은 수술 꼭 받아보세요",
          "original_form": "건강에 좋은 수술 꼭 받아보세요",
          "speaker_id": "1",
          "time": "20210906 17:05"
        },
        {
          "id": "MDRW2100033131.1.20",
          "form": "모자는 포기할 수 없어요",
          "original_form": "모자는 포기할 수 없어요",
          "speaker_id": "1",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100033131.1.21",
          "form": "난 멀쩡하다니까",
          "original_form": "난 멀쩡하다니까",
          "speaker_id": "2",
          "time": "20210906 17:06"
        }
