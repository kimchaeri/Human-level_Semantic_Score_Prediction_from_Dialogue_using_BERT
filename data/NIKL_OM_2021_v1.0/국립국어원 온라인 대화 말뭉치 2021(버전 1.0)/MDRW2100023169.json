        {
          "id": "MDRW2100023169.1.1",
          "form": "무화과 재배지라",
          "original_form": "무화과 재배지라",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023169.1.2",
          "form": "숙소 근처에 백화점이 있어서",
          "original_form": "숙소 근처에 백화점이 있어서",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023169.1.3",
          "form": "엄마가 철되면",
          "original_form": "엄마가 철되면",
          "speaker_id": "2",
          "time": "20210829 22:24"
