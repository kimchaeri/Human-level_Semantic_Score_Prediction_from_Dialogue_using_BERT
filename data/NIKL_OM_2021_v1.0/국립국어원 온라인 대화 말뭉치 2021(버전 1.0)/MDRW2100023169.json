          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023169.1.5",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023169.1.6",
          "form": "어디서 잔뜩 들고와",
          "original_form": "어디서 잔뜩 들고와",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023169.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023169.1.8",
          "form": "나도 무화과",
          "original_form": "나도 무화과",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023169.1.9",
          "form": "수도권에선 좀 귀하다고 하긴 하더라",
          "original_form": "수도권에선 좀 귀하다고 하긴 하더라",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023169.1.10",
          "form": "숙소 근처에 백화점 있어거",
          "original_form": "숙소 근처에 백화점 있어거",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023169.1.11",
          "form": "원래는 메론으로 해먹는건데",
          "original_form": "원래는 메론으로 해먹는건데",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023169.1.12",
          "form": "아 하몽메론",
          "original_form": "아 하몽메론",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023169.1.13",
          "form": "메론 깎고 자를 자신이 없으니까",
          "original_form": "메론 깎고 자를 자신이 없으니까",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023169.1.14",
          "form": "무화과 사다 해먹었거든",
          "original_form": "무화과 사다 해먹었거든",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
