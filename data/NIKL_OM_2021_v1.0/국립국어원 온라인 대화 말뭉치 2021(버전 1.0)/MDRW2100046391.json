      "utterance": [
        {
          "id": "MDRW2100046391.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211020 13:44"
        },
        {
          "id": "MDRW2100046391.1.2",
          "form": "안녕하세요 점심 드셨나요?",
          "original_form": "안녕하세요 점심 드셨나요?",
          "speaker_id": "2",
          "time": "20211020 13:44"
        },
        {
          "id": "MDRW2100046391.1.3",
          "form": "네 식사하셨나요?",
          "original_form": "네 식사하셨나요?",
          "speaker_id": "1",
          "time": "20211020 13:44"
        },
        {
          "id": "MDRW2100046391.1.4",
          "form": "넵 저는 마라샹궈먹었는데 뭐드셨나요?",
          "original_form": "넵 저는 마라샹궈먹었는데 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20211020 13:44"
        },
        {
          "id": "MDRW2100046391.1.5",
          "form": "\r\n저는 짜장면요",
          "original_form": "\r\n저는 짜장면요",
          "speaker_id": "1",
          "time": "20211020 13:45"
        },
        {
          "id": "MDRW2100046391.1.6",
          "form": "오 짜장면 맛있겠어요 ㅎㅎ",
          "original_form": "오 짜장면 맛있겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 13:45"
        },
        {
          "id": "MDRW2100046391.1.7",
          "form": "가장좋아하는 음식 있으신가요?",
          "original_form": "가장좋아하는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20211020 13:45"
        },
        {
          "id": "MDRW2100046391.1.8",
          "form": "항상 맛남 ㅋ",
          "original_form": "항상 맛남 ㅋ",
          "speaker_id": "1",
          "time": "20211020 13:45"
        },
        {
          "id": "MDRW2100046391.1.9",
          "form": "치킨 족발요 ㅋㅋ",
          "original_form": "치킨 족발요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 13:45"
        },
        {
          "id": "MDRW2100046391.1.10",
          "form": "치킨 제일 좋아하는 브랜드있나요?",
          "original_form": "치킨 제일 좋아하는 브랜드있나요?",
          "speaker_id": "2",
          "time": "20211020 13:46"
        },
        {
          "id": "MDRW2100046391.1.11",
          "form": "교촌요",
          "original_form": "교촌요",
          "speaker_id": "1",
          "time": "20211020 13:46"
        },
        {
          "id": "MDRW2100046391.1.12",
          "form": "허니콤보...?",
          "original_form": "허니콤보...?",
          "speaker_id": "2",
          "time": "20211020 13:46"
        },
        {
          "id": "MDRW2100046391.1.13",
          "form": "레드콤보",
          "original_form": "레드콤보",
          "speaker_id": "1",
          "time": "20211020 13:46"
        },
        {
          "id": "MDRW2100046391.1.14",
          "form": "앗! 레드콤보는 조금 맵더라구요 ㅎㅎ",
          "original_form": "앗! 레드콤보는 조금 맵더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 13:46"
        },
        {
          "id": "MDRW2100046391.1.15",
          "form": "전 허니콤보 좋아합니다 ㅎㅎ",
          "original_form": "전 허니콤보 좋아합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 13:46"
        },
        {
          "id": "MDRW2100046391.1.16",
          "form": "네 그러시군요 ㅋ",
          "original_form": "네 그러시군요 ㅋ",
          "speaker_id": "1",
          "time": "20211020 13:47"
        },
        {
          "id": "MDRW2100046391.1.17",
          "form": "뿌링클은 안좋아시나요?",
          "original_form": "뿌링클은 안좋아시나요?",
          "speaker_id": "2",
          "time": "20211020 13:47"
        },
        {
          "id": "MDRW2100046391.1.18",
          "form": "뿌링클은 안좋아해요",
          "original_form": "뿌링클은 안좋아해요",
          "speaker_id": "1",
          "time": "20211020 13:47"
        },
        {
          "id": "MDRW2100046391.1.19",
          "form": "매운걸 좋아함",
          "original_form": "매운걸 좋아함",
          "speaker_id": "1",
          "time": "20211020 13:47"
        },
        {
          "id": "MDRW2100046391.1.20",
          "form": "아 매운음식을 좋아하시나봐요 ㅎㅎ 스트레스엔 매운음식이 최고이긴해요!\r\n날씨가 점점 추워지는데 감기조심하시고 오늘 하루도 화이팅하세요!",
          "original_form": "아 매운음식을 좋아하시나봐요 ㅎㅎ 스트레스엔 매운음식이 최고이긴해요!\r\n날씨가 점점 추워지는데 감기조심하시고 오늘 하루도 화이팅하세요!",
          "speaker_id": "2",
          "time": "20211020 13:49"
        }
      ]
