        },
        {
          "id": "MDRW2100008289.1.6",
          "form": "싸이에",
          "original_form": "싸이에",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008289.1.7",
          "form": "감성게시물올렷엇니",
          "original_form": "감성게시물올렷엇니",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008289.1.8",
          "form": "아니?",
          "original_form": "아니?",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008289.1.9",
          "form": "난비ㅣ밀다이어리만 썼어",
          "original_form": "난비ㅣ밀다이어리만 썼어",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008289.1.10",
          "form": "비밀다이어리는",
          "original_form": "비밀다이어리는",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008289.1.11",
          "form": "감성적이 아니었나",
          "original_form": "감성적이 아니었나",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008289.1.12",
          "form": "니",
          "original_form": "니",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008289.1.13",
          "form": "뭔가 감성일것같은디",
          "original_form": "뭔가 감성일것같은디",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008289.1.14",
          "form": "무슨 노래로 했었니",
          "original_form": "무슨 노래로 했었니",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008289.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008289.1.16",
          "form": "음,,",
          "original_form": "음,,",
          "speaker_id": "1",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008289.1.17",
          "form": "죽일놈,,",
          "original_form": "죽일놈,,",
          "speaker_id": "1",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008289.1.18",
          "form": "글고 꽃남 bgm",
          "original_form": "글고 꽃남 bgm",
          "speaker_id": "1",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008289.1.19",
          "form": "나는 팝.송이었제",
          "original_form": "나는 팝.송이었제",
          "speaker_id": "2",
          "time": "20210822 16:29"
        },
        {
          "id": "MDRW2100008289.1.20",
          "form": "도토리 사봤니",
          "original_form": "도토리 사봤니",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008289.1.21",
          "form": "엥당연",
          "original_form": "엥당연",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008289.1.22",
          "form": "요즘엔 잘 안주는데",
          "original_form": "요즘엔 잘 안주는데",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008289.1.23",
          "form": "옛날엔 교회같은데에서",
          "original_form": "옛날엔 교회같은데에서",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008289.1.24",
          "form": "문상 많이 받았는데",
          "original_form": "문상 많이 받았는데",
          "speaker_id": "1",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008289.1.25",
          "form": "그럴때마다 거의 도토리에 씀",
          "original_form": "그럴때마다 거의 도토리에 씀",
          "speaker_id": "1",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008289.1.26",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008289.1.27",
          "form": "싸이 탈퇴해서",
          "original_form": "싸이 탈퇴해서",
          "speaker_id": "1",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008289.1.28",
          "form": "난 한번도 안사봤어",
          "original_form": "난 한번도 안사봤어",
          "speaker_id": "2",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008289.1.29",
