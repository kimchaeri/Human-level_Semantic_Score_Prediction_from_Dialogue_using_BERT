      "utterance": [
        {
          "id": "MDRW2100035649.1.1",
          "form": "내일부터 계속 비소식이 있던데",
          "original_form": "내일부터 계속 비소식이 있던데",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100035649.1.2",
          "form": "빨래는 많이 해두셨나용?",
          "original_form": "빨래는 많이 해두셨나용?",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035649.1.3",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035649.1.4",
          "form": "네 오늘 날이 좋길래",
          "original_form": "네 오늘 날이 좋길래",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035649.1.5",
          "form": "마지막찬스다 하고 널었습니다",
          "original_form": "마지막찬스다 하고 널었습니다",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035649.1.6",
          "form": "다행이네요ㅋㅋㅋ 저는 귀찮아서 미루다가 방금했는데",
          "original_form": "다행이네요ㅋㅋㅋ 저는 귀찮아서 미루다가 방금했는데",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035649.1.7",
          "form": "망한것같아요",
          "original_form": "망한것같아요",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035649.1.8",
          "form": "내일 계속 비오면 안마를거에용...",
          "original_form": "내일 계속 비오면 안마를거에용...",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035649.1.9",
          "form": "ㅋㅋㅋ 아녀요 낼까지 마를거에용..ㅜ",
          "original_form": "ㅋㅋㅋ 아녀요 낼까지 마를거에용..ㅜ",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035649.1.10",
          "form": "날이 추워져서",
          "original_form": "날이 추워져서",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035649.1.11",
          "form": "가을옷을 꺼낼까 싶기도하다가도",
          "original_form": "가을옷을 꺼낼까 싶기도하다가도",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100035649.1.12",
          "form": "보통 추석전쯤",
          "original_form": "보통 추석전쯤",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035649.1.13",
          "form": "확",
          "original_form": "확",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035649.1.14",
          "form": "더워지잖아용",
          "original_form": "더워지잖아용",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035649.1.15",
          "form": "이제 여름옷에 가디간 계절이죠",
          "original_form": "이제 여름옷에 가디간 계절이죠",
          "speaker_id": "2",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035649.1.16",
          "form": "아아 맞네욬ㅋㅋㅋ 안에는 반팔 위에는 긴팔",
          "original_form": "아아 맞네욬ㅋㅋㅋ 안에는 반팔 위에는 긴팔",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035649.1.17",
          "form": "이불도 바꾸고싶은데",
          "original_form": "이불도 바꾸고싶은데",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035649.1.18",
          "form": "약간 교복처럼",
          "original_form": "약간 교복처럼",
          "speaker_id": "2",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035649.1.19",
          "form": "그냥 버티고있어요ㅠㅠ",
          "original_form": "그냥 버티고있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035649.1.20",
          "form": "날이 언제 더워질지 몰라서...",
          "original_form": "날이 언제 더워질지 몰라서...",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035649.1.21",
          "form": "저도 아직 여름이불이에요",
          "original_form": "저도 아직 여름이불이에요",
          "speaker_id": "2",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035649.1.22",
          "form": "빨리 가을이 왔으면 좋겠네용ㅋㅋㅋ",
          "original_form": "빨리 가을이 왔으면 좋겠네용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035649.1.23",
          "form": "추워지면 바꾸면되지만 더워지면 일이 두배니까여..",
          "original_form": "추워지면 바꾸면되지만 더워지면 일이 두배니까여..",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035649.1.24",
          "form": "맞아요ㅋㅋㅋ 일이 두배! 빨래도 두배!",
          "original_form": "맞아요ㅋㅋㅋ 일이 두배! 빨래도 두배!",
          "speaker_id": "1",
          "time": "20210829 21:59"
        }
      ]
    }
  ]
