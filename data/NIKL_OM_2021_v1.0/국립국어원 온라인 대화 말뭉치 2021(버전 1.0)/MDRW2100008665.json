      "utterance": [
        {
          "id": "MDRW2100008665.1.1",
          "form": "결혼관이 있나요? 저는 결혼은 인생에서 딱히 중요하다 생각하지 않는 사람입니다",
          "original_form": "결혼관이 있나요? 저는 결혼은 인생에서 딱히 중요하다 생각하지 않는 사람입니다",
          "speaker_id": "1",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008665.1.2",
          "form": "오 동의합니다",
          "original_form": "오 동의합니다",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008665.1.3",
          "form": "전적으로요",
          "original_form": "전적으로요",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008665.1.4",
          "form": "전 8살 때 부터 혼자 살 것을 다짐했어요",
          "original_form": "전 8살 때 부터 혼자 살 것을 다짐했어요",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008665.1.5",
          "form": "8살 때부터요?????",
          "original_form": "8살 때부터요?????",
          "speaker_id": "2",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008665.1.6",
          "form": "결혼은 부질없는 짓이라 생각해서",
          "original_form": "결혼은 부질없는 짓이라 생각해서",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008665.1.7",
          "form": "웃기죠ㅋㅋ8살 애가 뭘 안다고ㅋㅋ",
          "original_form": "웃기죠ㅋㅋ8살 애가 뭘 안다고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008665.1.8",
          "form": "와 8살 때 신기해요",
          "original_form": "와 8살 때 신기해요",
          "speaker_id": "2",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100008665.1.9",
          "form": "그 때 저는 꼭두각시 삼매경이었어섴ㅋㅋ",
          "original_form": "그 때 저는 꼭두각시 삼매경이었어섴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100008665.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100008665.1.11",
          "form": "그랬더니 어른들이 그러더라구요",
          "original_form": "그랬더니 어른들이 그러더라구요",
          "speaker_id": "1",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100008665.1.12",
          "form": "너 같은 애가 제일 먼저 결혼 한다",
          "original_form": "너 같은 애가 제일 먼저 결혼 한다",
          "speaker_id": "1",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100008665.1.13",
          "form": "저는 그 말을 속으로 비웃었어요",
          "original_form": "저는 그 말을 속으로 비웃었어요",
          "speaker_id": "1",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008665.1.14",
          "form": "8살 때요?????",
          "original_form": "8살 때요?????",
          "speaker_id": "2",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008665.1.15",
          "form": "매번 그 말 하는 것 같아요",
          "original_form": "매번 그 말 하는 것 같아요",
          "speaker_id": "2",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008665.1.16",
          "form": "네... ㅋㅋㅋㅋㅋ",
          "original_form": "네... ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008665.1.17",
          "form": "지금 생각은 그때랑 변함은 없는데",
          "original_form": "지금 생각은 그때랑 변함은 없는데",
          "speaker_id": "1",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008665.1.18",
          "form": "보면 8살이 참",
          "original_form": "보면 8살이 참",
          "speaker_id": "1",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008665.1.19",
          "form": "웃겨요 그쵸",
          "original_form": "웃겨요 그쵸",
          "speaker_id": "1",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008665.1.20",
          "form": "현명했네요",
          "original_form": "현명했네요",
          "speaker_id": "2",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008665.1.21",
          "form": "과거에 남자 못 잃어 흑흑 이런 경험도 적을 것 아니에요 ㅎㅎㅎ",
          "original_form": "과거에 남자 못 잃어 흑흑 이런 경험도 적을 것 아니에요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008665.1.22",
          "form": "없습니다",
          "original_form": "없습니다",
          "speaker_id": "1",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008665.1.23",
          "form": "저는 남자라는 생물을",
          "original_form": "저는 남자라는 생물을",
          "speaker_id": "1",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008665.1.24",
          "form": "좋아하지않아서",
          "original_form": "좋아하지않아서",
          "speaker_id": "1",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008665.1.25",
          "form": "ㅋㅋㅋㅋㅋ 맨날 싸웠어요",
          "original_form": "ㅋㅋㅋㅋㅋ 맨날 싸웠어요",
          "speaker_id": "1",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008665.1.26",
          "form": "그게 현명한 것 같아여",
          "original_form": "그게 현명한 것 같아여",
          "speaker_id": "2",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008665.1.27",
          "form": "부끄러운 과거가 없잖아여",
          "original_form": "부끄러운 과거가 없잖아여",
          "speaker_id": "2",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008665.1.28",
          "form": "아앗.. 그렇게 되는건가요",
          "original_form": "아앗.. 그렇게 되는건가요",
          "speaker_id": "1",
