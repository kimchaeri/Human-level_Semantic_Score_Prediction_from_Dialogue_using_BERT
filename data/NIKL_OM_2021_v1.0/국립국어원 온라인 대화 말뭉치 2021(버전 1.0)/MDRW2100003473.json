      "utterance": [
        {
          "id": "MDRW2100003473.1.1",
          "form": "3개이상 되는거 같아",
          "original_form": "3개이상 되는거 같아",
          "speaker_id": "2",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003473.1.2",
          "form": "다른 운동 종목도 하지 골프를 너무 많이 하네",
          "original_form": "다른 운동 종목도 하지 골프를 너무 많이 하네",
          "speaker_id": "1",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003473.1.3",
          "form": "그러게 근데 골프가 접근하기 쉬우니까",
          "original_form": "그러게 근데 골프가 접근하기 쉬우니까",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003473.1.4",
          "form": "그럴수도 있겠다!",
          "original_form": "그럴수도 있겠다!",
          "speaker_id": "1",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003473.1.5",
          "form": "볼링이나 탁구 배드민턴 같은 프로도 괜찮을거 같은더",
          "original_form": "볼링이나 탁구 배드민턴 같은 프로도 괜찮을거 같은더",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003473.1.6",
          "form": "볼링프로 괜찮겠다",
          "original_form": "볼링프로 괜찮겠다",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003473.1.7",
          "form": "이승기도 예능 많이 하네",
          "original_form": "이승기도 예능 많이 하네",
          "speaker_id": "2",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003473.1.8",
          "form": "눕방 골퍼래 ㅋㅋ",
          "original_form": "눕방 골퍼래 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003473.1.9",
          "form": "티샷하고 눕네 골린인가봐",
          "original_form": "티샷하고 눕네 골린인가봐",
          "speaker_id": "2",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003473.1.10",
          "form": "골프주린이 맞는듯",
          "original_form": "골프주린이 맞는듯",
          "speaker_id": "1",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003473.1.11",
          "form": "근데 배정남 복장이 젤 이상하다",
          "original_form": "근데 배정남 복장이 젤 이상하다",
          "speaker_id": "2",
          "time": "20210820 23:39"
        },
        {
          "id": "MDRW2100003473.1.12",
          "form": "뭐야 배정남빼고 다 흰바지네",
          "original_form": "뭐야 배정남빼고 다 흰바지네",
          "speaker_id": "1",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003473.1.13",
          "form": "ㅋㅋ심지어 상의도 흰색이 많음",
          "original_form": "ㅋㅋ심지어 상의도 흰색이 많음",
          "speaker_id": "2",
          "time": "20210820 23:40"
        },
        {
          "id": "MDRW2100003473.1.14",
          "form": "승기는 성장깨란다 ㅋㅋ 골린인가보다",
          "original_form": "승기는 성장깨란다 ㅋㅋ 골린인가보다",
          "speaker_id": "1",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003473.1.15",
          "form": "현주엽은 그냥 프로인가본데",
          "original_form": "현주엽은 그냥 프로인가본데",
          "speaker_id": "2",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003473.1.16",
          "form": "그러게 엄청 잘하네",
          "original_form": "그러게 엄청 잘하네",
          "speaker_id": "1",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003473.1.17",
          "form": "탁재훈도 골프 시작한지 얼마 안되었나봐",
          "original_form": "탁재훈도 골프 시작한지 얼마 안되었나봐",
          "speaker_id": "2",
          "time": "20210820 23:43"
        }
