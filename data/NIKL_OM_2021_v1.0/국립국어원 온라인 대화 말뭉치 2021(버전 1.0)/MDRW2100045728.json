      "utterance": [
        {
          "id": "MDRW2100045728.1.1",
          "form": "보라색인가요? 야채가!",
          "original_form": "보라색인가요? 야채가!",
          "speaker_id": "2",
          "time": "20210908 06:15"
        },
        {
          "id": "MDRW2100045728.1.2",
          "form": "네 보라색이에요 ㅋㅋㅋㅋ",
          "original_form": "네 보라색이에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:16"
        },
        {
          "id": "MDRW2100045728.1.3",
          "form": "아! 길쭉하고 보라색이고 물컹한 거",
          "original_form": "아! 길쭉하고 보라색이고 물컹한 거",
          "speaker_id": "2",
          "time": "20210908 06:17"
        },
        {
          "id": "MDRW2100045728.1.4",
          "form": "정답! 가지!",
          "original_form": "정답! 가지!",
          "speaker_id": "2",
          "time": "20210908 06:17"
        },
        {
          "id": "MDRW2100045728.1.5",
          "form": "정답!!",
          "original_form": "정답!!",
          "speaker_id": "1",
          "time": "20210908 06:17"
        },
        {
          "id": "MDRW2100045728.1.6",
          "form": "가지나물을 엄청 싫어해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "가지나물을 엄청 싫어해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:17"
        },
        {
          "id": "MDRW2100045728.1.7",
          "form": "와아~",
          "original_form": "와아~",
          "speaker_id": "2",
          "time": "20210908 06:17"
        },
        {
          "id": "MDRW2100045728.1.8",
          "form": "쒯 저도 가지 싫어해요",
          "original_form": "쒯 저도 가지 싫어해요",
          "speaker_id": "2",
          "time": "20210908 06:17"
        },
        {
          "id": "MDRW2100045728.1.9",
          "form": "가지냉국도 싫어요",
          "original_form": "가지냉국도 싫어요",
          "speaker_id": "2",
          "time": "20210908 06:17"
        },
        {
          "id": "MDRW2100045728.1.10",
          "form": "가지는 생긴것부터 독성있게 생겼어요",
          "original_form": "가지는 생긴것부터 독성있게 생겼어요",
          "speaker_id": "2",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045728.1.11",
          "form": "보라색을 말하면 바로 정답스포수준이라 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "보라색을 말하면 바로 정답스포수준이라 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045728.1.12",
          "form": "ㅌㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045728.1.13",
          "form": "물컹한 보라색 하믄",
          "original_form": "물컹한 보라색 하믄",
          "speaker_id": "2",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045728.1.14",
          "form": "색깔힌트를 못드렷어욬ㅋㅋㅋㅋㅋ",
          "original_form": "색깔힌트를 못드렷어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045728.1.15",
          "form": "가지밖에 없잖아요",
          "original_form": "가지밖에 없잖아요",
          "speaker_id": "2",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045728.1.16",
          "form": "ㅋㅋㅋㅋㅋ그니까요",
          "original_form": "ㅋㅋㅋㅋㅋ그니까요",
          "speaker_id": "2",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045728.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래서 계속 아니라고만 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래서 계속 아니라고만 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:19"
        },
        {
          "id": "MDRW2100045728.1.18",
          "form": "초록색 아니에요 빨간색 아니에요 ㅋㅋㅋㅋㅋ",
          "original_form": "초록색 아니에요 빨간색 아니에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:19"
        },
        {
          "id": "MDRW2100045728.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:20"
        },
        {
          "id": "MDRW2100045728.1.20",
          "form": "다 빠꾸먹었잖아요",
          "original_form": "다 빠꾸먹었잖아요",
          "speaker_id": "2",
          "time": "20210908 06:20"
        },
        {
          "id": "MDRW2100045728.1.21",
          "form": "우수수수",
          "original_form": "우수수수",
          "speaker_id": "2",
          "time": "20210908 06:20"
        },
        {
          "id": "MDRW2100045728.1.22",
          "form": "이번엔 제가 싫어하는 음싣 맞춰보실래요?",
          "original_form": "이번엔 제가 싫어하는 음싣 맞춰보실래요?",
          "speaker_id": "2",
          "time": "20210908 06:21"
        },
        {
          "id": "MDRW2100045728.1.23",
          "form": "앗 가지말고 또 잇으세요?",
          "original_form": "앗 가지말고 또 잇으세요?",
          "speaker_id": "1",
          "time": "20210908 06:21"
        },
        {
          "id": "MDRW2100045728.1.24",
          "form": "그럼 맞춰볼게요!",
          "original_form": "그럼 맞춰볼게요!",
          "speaker_id": "1",
          "time": "20210908 06:21"
        },
        {
          "id": "MDRW2100045728.1.25",
