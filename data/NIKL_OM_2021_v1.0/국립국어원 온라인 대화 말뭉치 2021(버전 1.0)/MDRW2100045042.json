      "utterance": [
        {
          "id": "MDRW2100045042.1.1",
          "form": "배달음식 뭐 좋아하세요?",
          "original_form": "배달음식 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045042.1.2",
          "form": "저 치킨 좋아해요",
          "original_form": "저 치킨 좋아해요",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045042.1.3",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045042.1.4",
          "form": "전 별로",
          "original_form": "전 별로",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045042.1.5",
          "form": "왜죠,",
          "original_form": "왜죠,",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045042.1.6",
          "form": "그냥 있으면 먹는 정도에요",
          "original_form": "그냥 있으면 먹는 정도에요",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045042.1.7",
          "form": "넘 질려",
          "original_form": "넘 질려",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045042.1.8",
          "form": "질릴 정도로 많이 드셨나요?",
          "original_form": "질릴 정도로 많이 드셨나요?",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045042.1.9",
          "form": "그냥 사람들이 많이 시켜서요",
          "original_form": "그냥 사람들이 많이 시켜서요",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045042.1.10",
          "form": "가끔은 땡기죠",
          "original_form": "가끔은 땡기죠",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045042.1.11",
          "form": "왜죠?",
          "original_form": "왜죠?",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045042.1.12",
          "form": "항상 먹어도 안 질리는게 치킨 아닌가요",
          "original_form": "항상 먹어도 안 질리는게 치킨 아닌가요",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045042.1.13",
          "form": "치알못 이신가요?",
          "original_form": "치알못 이신가요?",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045042.1.14",
          "form": "네 그러든가요...",
          "original_form": "네 그러든가요...",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045042.1.15",
          "form": "아 진짜",
          "original_form": "아 진짜",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045042.1.16",
          "form": "청양마요는 좋아요",
          "original_form": "청양마요는 좋아요",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045042.1.17",
          "form": "유ㅏㅅ",
          "original_form": "유ㅏㅅ",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045042.1.18",
          "form": "맛",
          "original_form": "맛",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045042.1.19",
          "form": "반반 무많이의 멋짐을 모르는 당신",
          "original_form": "반반 무많이의 멋짐을 모르는 당신",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045042.1.20",
          "form": "불쌍하군요",
          "original_form": "불쌍하군요",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045042.1.21",
          "form": "알긴 아는데",
          "original_form": "알긴 아는데",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045042.1.22",
          "form": "자주먹진 않음",
          "original_form": "자주먹진 않음",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045042.1.23",
          "form": "아 느끼 매콤 맛나요",
          "original_form": "아 느끼 매콤 맛나요",
          "speaker_id": "1",
          "time": "20210908 00:42"
        }
      ]
