      "utterance": [
        {
          "id": "MDRW2100003341.1.1",
          "form": "주말은 뭐하고보내시나요?",
          "original_form": "주말은 뭐하고보내시나요?",
          "speaker_id": "1",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003341.1.2",
          "form": "주로 가족과 집에서 쉬어요",
          "original_form": "주로 가족과 집에서 쉬어요",
          "speaker_id": "2",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003341.1.3",
          "form": "저는 주말이나 평일이나 다른게 없는것 같아오ㅡㅠ",
          "original_form": "저는 주말이나 평일이나 다른게 없는것 같아오ㅡㅠ",
          "speaker_id": "1",
          "time": "20210820 21:59"
        },
        {
          "id": "MDRW2100003341.1.4",
          "form": "퇴근하고싶네요ㅋㅋ",
          "original_form": "퇴근하고싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:59"
        },
        {
          "id": "MDRW2100003341.1.5",
          "form": "혼자 쉬셔본적 있으세요?",
          "original_form": "혼자 쉬셔본적 있으세요?",
          "speaker_id": "2",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003341.1.6",
          "form": "둘째 태어나곤 없는것 같아요ㅠㅠ",
          "original_form": "둘째 태어나곤 없는것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 22:02"
        },
        {
          "id": "MDRW2100003341.1.7",
          "form": "아~~에구~쉬시면 좋은데",
          "original_form": "아~~에구~쉬시면 좋은데",
          "speaker_id": "2",
          "time": "20210820 22:03"
        },
        {
          "id": "MDRW2100003341.1.8",
          "form": "그래서 밤에 늦게 까지 폰 보며 놀다보면 ㅋㅋ다음날 피곤하고ㅋ반복인것 같아요ㅋㅋㅣㅣ",
          "original_form": "그래서 밤에 늦게 까지 폰 보며 놀다보면 ㅋㅋ다음날 피곤하고ㅋ반복인것 같아요ㅋㅋㅣㅣ",
          "speaker_id": "1",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003341.1.9",
          "form": "육아가 젤로 힘든것 같아요",
          "original_form": "육아가 젤로 힘든것 같아요",
          "speaker_id": "2",
          "time": "20210820 22:04"
        },
        {
          "id": "MDRW2100003341.1.10",
          "form": "맞아요 진짜 육아가ㅠㅠ 일다니는것 보다 더힘든것 같아요ㅠㅠ",
          "original_form": "맞아요 진짜 육아가ㅠㅠ 일다니는것 보다 더힘든것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 22:06"
        },
        {
          "id": "MDRW2100003341.1.11",
          "form": "네~저도 애는 없지만 있으면 일 병행하기 힘들것 같아요",
          "original_form": "네~저도 애는 없지만 있으면 일 병행하기 힘들것 같아요",
          "speaker_id": "2",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003341.1.12",
          "form": "계획은 있으신가요?",
          "original_form": "계획은 있으신가요?",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003341.1.13",
          "form": "아이를 갖고 싶은데 힘드네요",
          "original_form": "아이를 갖고 싶은데 힘드네요",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003341.1.14",
          "form": "아 그게 정말 힘든것 같아요~ ㅠㅠ",
          "original_form": "아 그게 정말 힘든것 같아요~ ㅠㅠ",
