      "utterance": [
        {
          "id": "MDRW2100042666.1.1",
          "form": "흑당 가게?",
          "original_form": "흑당 가게?",
          "speaker_id": "2",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100042666.1.2",
          "form": "네 그거 있으면 카페 망해도 흑당 먹을수 있으니까 ㅎㅎ",
          "original_form": "네 그거 있으면 카페 망해도 흑당 먹을수 있으니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100042666.1.3",
          "form": "흑화당이 망하면 어떡해",
          "original_form": "흑화당이 망하면 어떡해",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100042666.1.4",
          "form": "그건 어쩔수 없으니까... 그래도 흑당 맛있잖아요",
          "original_form": "그건 어쩔수 없으니까... 그래도 흑당 맛있잖아요",
          "speaker_id": "1",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100042666.1.5",
          "form": "엄마도 먹고싶다 공차갈까",
          "original_form": "엄마도 먹고싶다 공차갈까",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100042666.1.6",
          "form": "공차두 맛있죠 버블티맛집",
          "original_form": "공차두 맛있죠 버블티맛집",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100042666.1.7",
          "form": "버블티는 공차가 젤 맛있는거같은데..",
          "original_form": "버블티는 공차가 젤 맛있는거같은데..",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100042666.1.8",
          "form": "우유 많이 들어있어서 그런가봐요",
          "original_form": "우유 많이 들어있어서 그런가봐요",
          "speaker_id": "1",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100042666.1.9",
          "form": "우유가 많이 들어있어서 그런지",
          "original_form": "우유가 많이 들어있어서 그런지",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100042666.1.10",
          "form": "공차에서 흑당먹으면 입에서 냄새나더라",
          "original_form": "공차에서 흑당먹으면 입에서 냄새나더라",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100042666.1.11",
          "form": "우유 마시면 냄새나니까 어쩔수없나봐요 ㅋㅋㅋ",
          "original_form": "우유 마시면 냄새나니까 어쩔수없나봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100042666.1.12",
          "form": "혀나 깨끗하게 닦아야지",
          "original_form": "혀나 깨끗하게 닦아야지",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100042666.1.13",
          "form": "너나 잘 닦아 냄새 많이 나니까",
          "original_form": "너나 잘 닦아 냄새 많이 나니까",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100042666.1.14",
          "form": "혀 클리너 살까요",
          "original_form": "혀 클리너 살까요",
          "speaker_id": "1",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100042666.1.15",
          "form": "혀 세척기 살까요 두개",
          "original_form": "혀 세척기 살까요 두개",
          "speaker_id": "1",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100042666.1.16",
          "form": "세척기 살까 두개 혀 닦는거",
          "original_form": "세척기 살까 두개 혀 닦는거",
          "speaker_id": "1",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100042666.1.17",
          "form": "그것두 좋구",
          "original_form": "그것두 좋구",
          "speaker_id": "2",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100042666.1.18",
          "form": "유당불내증 있어서 많이 마시면 안되는데.. 맛있으니까...",
          "original_form": "유당불내증 있어서 많이 마시면 안되는데.. 맛있으니까...",
          "speaker_id": "1",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100042666.1.19",
          "form": "치료해야겠다 위장 많이 안좋아가지구",
          "original_form": "치료해야겠다 위장 많이 안좋아가지구",
          "speaker_id": "2",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100042666.1.20",
          "form": "ㅠㅠ 저두 건강하게 살구싶음",
          "original_form": "ㅠㅠ 저두 건강하게 살구싶음",
          "speaker_id": "1",
          "time": "20210907 13:31"
        },
        {
          "id": "MDRW2100042666.1.21",
          "form": "아몬드브리즈 흑당밀크티 안되나",
          "original_form": "아몬드브리즈 흑당밀크티 안되나",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100042666.1.22",
          "form": "사서 해볼까요?? 시럽이랑 ㅎㅎ",
          "original_form": "사서 해볼까요?? 시럽이랑 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100042666.1.23",
          "form": "너 저번에두 먹고 버렸잖아",
          "original_form": "너 저번에두 먹고 버렸잖아",
          "speaker_id": "2",
          "time": "20210907 13:33"
        }
      ]
