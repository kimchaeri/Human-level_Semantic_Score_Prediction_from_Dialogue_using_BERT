      "utterance": [
        {
          "id": "MDRW2100026833.1.1",
          "form": "성게알 핵존맛",
          "original_form": "성게알 핵존맛",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026833.1.2",
          "form": "성게알파스타는 천사가 해줬나요!",
          "original_form": "성게알파스타는 천사가 해줬나요!",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026833.1.3",
          "form": "아ㅜㅜ 우니는 인정이죠",
          "original_form": "아ㅜㅜ 우니는 인정이죠",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026833.1.4",
          "form": "네 그리고 천사와 악마라고 해두죠",
          "original_form": "네 그리고 천사와 악마라고 해두죠",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026833.1.5",
          "form": "댄브라운의 천사와악마가 생각나네요",
          "original_form": "댄브라운의 천사와악마가 생각나네요",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026833.1.6",
          "form": "ㅋㅋㅋ 천사는 바지락을 좋아하남요ㅎ",
          "original_form": "ㅋㅋㅋ 천사는 바지락을 좋아하남요ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026833.1.7",
          "form": "아 그걸 우니라고 하는 군요",
          "original_form": "아 그걸 우니라고 하는 군요",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026833.1.8",
          "form": "아니ㅋㅋㅋ",
          "original_form": "아니ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026833.1.9",
          "form": "천사는 운동을 싫어합니다",
          "original_form": "천사는 운동을 싫어합니다",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026833.1.10",
          "form": "쇼핑을 좋아합니다",
          "original_form": "쇼핑을 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026833.1.11",
          "form": "아ㅎㅎ 다행...인가...요?",
          "original_form": "아ㅎㅎ 다행...인가...요?",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026833.1.12",
          "form": "슥배송 하더라구요",
          "original_form": "슥배송 하더라구요",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026833.1.13",
          "form": "ㅎㅎ 쇼핑도 체력소모 장난아닌데",
          "original_form": "ㅎㅎ 쇼핑도 체력소모 장난아닌데",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026833.1.14",
          "form": "훌륭하시군요ㅎ",
          "original_form": "훌륭하시군요ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026833.1.15",
          "form": "아 방구석쇼핑입니다",
          "original_form": "아 방구석쇼핑입니다",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026833.1.16",
          "form": "집순이에요",
          "original_form": "집순이에요",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026833.1.17",
          "form": "어디 나가는거 귀찮아합니다",
          "original_form": "어디 나가는거 귀찮아합니다",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026833.1.18",
          "form": "저는 활동적이라 성산일..출봉",
          "original_form": "저는 활동적이라 성산일..출봉",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026833.1.19",
          "form": "코시국에 바람직한 모델이군요ㅎㅎ",
          "original_form": "코시국에 바람직한 모델이군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026833.1.20",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026833.1.21",
          "form": "바지락은 활동적이지 않았남요ㅋㅋ",
          "original_form": "바지락은 활동적이지 않았남요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026833.1.22",
          "form": "아 좋아는 하는데",
          "original_form": "아 좋아는 하는데",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026833.1.23",
          "form": "정적이라",
          "original_form": "정적이라",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026833.1.24",
          "form": "그때 나간 내 무릎연골아 잘있냐!",
          "original_form": "그때 나간 내 무릎연골아 잘있냐!",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026833.1.25",
          "form": "아 식음료",
          "original_form": "아 식음료",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026833.1.26",
          "form": "ㅋㅋㅋ 많은걸 잃으셨군요ㅋㅋ",
          "original_form": "ㅋㅋㅋ 많은걸 잃으셨군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026833.1.27",
          "form": "바지락 술찜 해드셔보셧어요?",
          "original_form": "바지락 술찜 해드셔보셧어요?",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026833.1.28",
          "form": "해보진 않고 먹어는 봤죠ㅎ",
          "original_form": "해보진 않고 먹어는 봤죠ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:37"
        },
        {
