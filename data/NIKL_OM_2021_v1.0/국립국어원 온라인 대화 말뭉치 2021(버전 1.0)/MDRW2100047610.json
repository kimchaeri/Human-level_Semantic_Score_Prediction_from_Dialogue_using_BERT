      "utterance": [
        {
          "id": "MDRW2100047610.1.1",
          "form": "여행은 좋아하시나요?",
          "original_form": "여행은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100047610.1.2",
          "form": "음 저 드라이브요",
          "original_form": "음 저 드라이브요",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100047610.1.3",
          "form": "요샌 드라이브 코로나 때문에요",
          "original_form": "요샌 드라이브 코로나 때문에요",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100047610.1.4",
          "form": "여행 언제 갔다 오셨어요?",
          "original_form": "여행 언제 갔다 오셨어요?",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100047610.1.5",
          "form": "제대로된 여행은 코로나 이후로 가본적이 없어요 ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "제대로된 여행은 코로나 이후로 가본적이 없어요 ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100047610.1.6",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100047610.1.7",
          "form": "작년에 제주도는 갔러요",
          "original_form": "작년에 제주도는 갔러요",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100047610.1.8",
          "form": "name2님은 어디 다녀오셧어요?",
          "original_form": "&name2&님은 어디 다녀오셧어요?",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100047610.1.9",
          "form": "그땐 좀 덜해서요",
          "original_form": "그땐 좀 덜해서요",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100047610.1.10",
          "form": "그냥 딱안정하고",
          "original_form": "그냥 딱안정하고",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100047610.1.11",
          "form": "동굴 사격장",
          "original_form": "동굴 사격장",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100047610.1.12",
          "form": "아 제주도 좋죠",
          "original_form": "아 제주도 좋죠",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100047610.1.13",
          "form": "바다 둘러보고 왔어요",
          "original_form": "바다 둘러보고 왔어요",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100047610.1.14",
          "form": "이름도 기억안나네요",
          "original_form": "이름도 기억안나네요",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100047610.1.15",
          "form": "저희 일부러 아는곳들 유명한데 안가고",
          "original_form": "저희 일부러 아는곳들 유명한데 안가고",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100047610.1.16",
          "form": "발길닿는대로 갔어요",
          "original_form": "발길닿는대로 갔어요",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100047610.1.17",
          "form": "오 발길닿는대로 다니는거 좋네요",
          "original_form": "오 발길닿는대로 다니는거 좋네요",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100047610.1.18",
          "form": "은근 시적인 느낌 ㅋㅋㅋㅋ",
          "original_form": "은근 시적인 느낌 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100047610.1.19",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100047610.1.20",
          "form": "좋아요 그러다 우연히 이쁜 커피숍 발견",
          "original_form": "좋아요 그러다 우연히 이쁜 커피숍 발견",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100047610.1.21",
          "form": "진짜 크고 조용하고 전망이 좋았어요",
          "original_form": "진짜 크고 조용하고 전망이 좋았어요",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100047610.1.22",
          "form": "주인분이 풍경을 떡메로 만들어서 주셨어요",
          "original_form": "주인분이 풍경을 떡메로 만들어서 주셨어요",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100047610.1.23",
          "form": "그렇게 우연히 가게되는 곳이 보물같을때 너무 좋아요",
          "original_form": "그렇게 우연히 가게되는 곳이 보물같을때 너무 좋아요",
          "speaker_id": "1",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100047610.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100047610.1.25",
          "form": "우연히 간집이 맛집이고",
          "original_form": "우연히 간집이 맛집이고",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100047610.1.26",
          "form": "가끔 맛났던데",
          "original_form": "가끔 맛났던데",
          "speaker_id": "2",
          "time": "20210826 22:19"
        },
        {
          "id": "MDRW2100047610.1.27",
          "form": "다시 가는데",
          "original_form": "다시 가는데",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100047610.1.28",
          "form": "애들이 또 거기 ㅡㅡ싫러해요",
          "original_form": "애들이 또 거기 ㅡㅡ싫러해요",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100047610.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 싫대요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 싫대요?",
          "speaker_id": "1",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100047610.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100047610.1.31",
          "form": "맨날 거기가냐고",
          "original_form": "맨날 거기가냐고",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100047610.1.32",
          "form": "참나 잘먹으면서",
          "original_form": "참나 잘먹으면서",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100047610.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ반항하는 아이들",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ반항하는 아이들",
          "speaker_id": "1",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100047610.1.34",
          "form": "네 컸다고 엄나말 안들어요",
          "original_form": "네 컸다고 엄나말 안들어요",
          "speaker_id": "2",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100047610.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ사춘기가 왔나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ사춘기가 왔나요",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100047610.1.36",
          "form": "네 사춘기죠",
          "original_form": "네 사춘기죠",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047610.1.37",
          "form": "어딜 안가려고해요",
          "original_form": "어딜 안가려고해요",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047610.1.38",
          "form": "집에붙어서",
          "original_form": "집에붙어서",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047610.1.39",
          "form": "여행 이제 가기 힘들어요",
          "original_form": "여행 이제 가기 힘들어요",
          "speaker_id": "2",
          "time": "20210826 22:24"
        }
      ]
    }
