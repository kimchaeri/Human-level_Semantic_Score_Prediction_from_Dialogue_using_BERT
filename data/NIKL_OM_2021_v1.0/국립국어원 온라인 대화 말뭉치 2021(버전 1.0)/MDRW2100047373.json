      "utterance": [
        {
          "id": "MDRW2100047373.1.1",
          "form": "아 사장님께 말씀드림?",
          "original_form": "아 사장님께 말씀드림?",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100047373.1.2",
          "form": "말좀한번은해주지",
          "original_form": "말좀한번은해주지",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100047373.1.3",
          "form": "홀 아까비",
          "original_form": "홀 아까비",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100047373.1.4",
          "form": "많이두마셨구만",
          "original_form": "많이두마셨구만",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100047373.1.5",
          "form": "아녀 알게된날도",
          "original_form": "아녀 알게된날도",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100047373.1.6",
          "form": "알바가있었음",
          "original_form": "알바가있었음",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100047373.1.7",
          "form": "말해봐야",
          "original_form": "말해봐야",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100047373.1.8",
          "form": "알바는 모르제",
          "original_form": "알바는 모르제",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100047373.1.9",
          "form": "그러니까 따져도 주인 있을때 말해야할거고",
          "original_form": "그러니까 따져도 주인 있을때 말해야할거고",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100047373.1.10",
          "form": "말해도 쪼잔해보이고",
          "original_form": "말해도 쪼잔해보이고",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100047373.1.11",
          "form": "바로 손절함",
          "original_form": "바로 손절함",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100047373.1.12",
          "form": "이힝ㅜ",
          "original_form": "이힝ㅜ",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100047373.1.13",
          "form": "아깝네",
          "original_form": "아깝네",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100047373.1.14",
          "form": "근데 생각해보면 더 마셔봐야 싶음",
          "original_form": "근데 생각해보면 더 마셔봐야 싶음",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100047373.1.15",
          "form": "살찌고돈나가고",
          "original_form": "살찌고돈나가고",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100047373.1.16",
          "form": "건강해치고",
          "original_form": "건강해치고",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100047373.1.17",
          "form": "삼콤보밖에더함?",
          "original_form": "삼콤보밖에더함?",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100047373.1.18",
          "form": "그냥 물떠다녀야겠다",
          "original_form": "그냥 물떠다녀야겠다",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100047373.1.19",
          "form": "몸에좋은 물",
          "original_form": "몸에좋은 물",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100047373.1.20",
          "form": "그리고 버거킹 앱도 지움",
          "original_form": "그리고 버거킹 앱도 지움",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100047373.1.21",
          "form": "현타와서",
          "original_form": "현타와서",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100047373.1.22",
          "form": "홀",
          "original_form": "홀",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100047373.1.23",
          "form": "밥은 어째먹노",
          "original_form": "밥은 어째먹노",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100047373.1.24",
          "form": "거긴 쿠폰아닌디ㅜ",
          "original_form": "거긴 쿠폰아닌디ㅜ",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100047373.1.25",
          "form": "집밥먹어야죠",
          "original_form": "집밥먹어야죠",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100047373.1.26",
          "form": "사실 다이어트할거라고 다이어트 쉐이크도 사놓고 뭐하는짓인가싶음",
          "original_form": "사실 다이어트할거라고 다이어트 쉐이크도 사놓고 뭐하는짓인가싶음",
          "speaker_id": "2",
          "time": "20210826 19:45"
        },
        {
          "id": "MDRW2100047373.1.27",
          "form": "나도 집에 밥무러가고있음",
          "original_form": "나도 집에 밥무러가고있음",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100047373.1.28",
          "form": "쉐이크 사놨구만",
          "original_form": "쉐이크 사놨구만",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100047373.1.29",
          "form": "퇴근길인거?",
          "original_form": "퇴근길인거?",
          "speaker_id": "2",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100047373.1.30",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210826 19:49"
        },
        {
          "id": "MDRW2100047373.1.31",
          "form": "name1님도 집밥 파 시군요",
          "original_form": "&name1&님도 집밥 파 시군요",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100047373.1.32",
          "form": "그러고보니 난 귀하게커서 사과도 못깎음 많이 무리네 ㅋㅋㄲ",
          "original_form": "그러고보니 난 귀하게커서 사과도 못깎음 많이 무리네 ㅋㅋㄲ",
