      "utterance": [
        {
          "id": "MDRW2100024367.1.1",
          "form": "그러면 고양이랑 사이는 좋아요?",
          "original_form": "그러면 고양이랑 사이는 좋아요?",
          "speaker_id": "2",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100024367.1.2",
          "form": "강아지가..",
          "original_form": "강아지가..",
          "speaker_id": "2",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100024367.1.3",
          "form": "강아지가 원래 외동견이었어서 그런지",
          "original_form": "강아지가 원래 외동견이었어서 그런지",
          "speaker_id": "1",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100024367.1.4",
          "form": "고양이의 존재를 참을 수 없어하긴 해요...",
          "original_form": "고양이의 존재를 참을 수 없어하긴 해요...",
          "speaker_id": "1",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100024367.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100024367.1.6",
          "form": "아..이런..ㅠ 얼마나 속상해할지 둘 다 이해가네요",
          "original_form": "아..이런..ㅠ 얼마나 속상해할지 둘 다 이해가네요",
          "speaker_id": "2",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100024367.1.7",
          "form": "근데 또 지 시야에 없으면 찾아다니고 하는 거 보면",
          "original_form": "근데 또 지 시야에 없으면 찾아다니고 하는 거 보면",
          "speaker_id": "1",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100024367.1.8",
          "form": "근데 어떻게 같이잘수잇죠",
          "original_form": "근데 어떻게 같이잘수잇죠",
          "speaker_id": "2",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100024367.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100024367.1.10",
          "form": "아예 싫어하는 것 같진않고ㅠㅠ",
          "original_form": "아예 싫어하는 것 같진않고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 02:38"
        },
        {
          "id": "MDRW2100024367.1.11",
          "form": "그니까욬ㅋㅋㅋ",
          "original_form": "그니까욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100024367.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ경쟁상대인가봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ경쟁상대인가봐요",
          "speaker_id": "2",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100024367.1.13",
          "form": "ㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100024367.1.14",
          "form": "너무 복받으신거아닌가요 ㅋㅋㅋㅋname1님",
          "original_form": "너무 복받으신거아닌가요 ㅋㅋㅋㅋ&name1&님",
          "speaker_id": "2",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100024367.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100024367.1.16",
          "form": "털색깔은 어때요??",
          "original_form": "털색깔은 어때요??",
          "speaker_id": "2",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100024367.1.17",
          "form": "후후후후 그렇죠 고양이와 강아지를 동시에 키우는 건 쉽지않다구요!",
          "original_form": "후후후후 그렇죠 고양이와 강아지를 동시에 키우는 건 쉽지않다구요!",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100024367.1.18",
          "form": "고양이요?",
          "original_form": "고양이요?",
          "speaker_id": "1",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100024367.1.19",
          "form": "그렇됴.. 둘다용 ㅎㅎㅎ",
          "original_form": "그렇됴.. 둘다용 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 02:39"
        },
        {
          "id": "MDRW2100024367.1.20",
          "form": "강아지는ㅋㅋㅋ말티즈라 흰색이고",
          "original_form": "강아지는ㅋㅋㅋ말티즈라 흰색이고",
          "speaker_id": "1",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100024367.1.21",
          "form": "고양이는 삼색이에요!",
          "original_form": "고양이는 삼색이에요!",
          "speaker_id": "1",
          "time": "20210831 02:40"
        },
        {
          "id": "MDRW2100024367.1.22",
          "form": "네네 ㅋㅋㅋ흑 너무 예쁘겟다 ㅜㅜ",
          "original_form": "네네 ㅋㅋㅋ흑 너무 예쁘겟다 ㅜㅜ",
          "speaker_id": "2",
