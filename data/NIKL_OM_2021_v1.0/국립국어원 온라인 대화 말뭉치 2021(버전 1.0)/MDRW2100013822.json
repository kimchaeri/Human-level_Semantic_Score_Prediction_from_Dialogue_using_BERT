      "utterance": [
        {
          "id": "MDRW2100013822.1.1",
          "form": "name2은 가족관계가 어떻게 되세요?",
          "original_form": "&name2&은 가족관계가 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013822.1.2",
          "form": "저 와이프랑 둘이요ㅋㅋ",
          "original_form": "저 와이프랑 둘이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013822.1.3",
          "form": "아 결혼하셨구나~",
          "original_form": "아 결혼하셨구나~",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100013822.1.4",
          "form": "형제자매는 없으세요?",
          "original_form": "형제자매는 없으세요?",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100013822.1.5",
          "form": "아 동생 있어요 ㅎㅎ 남자",
          "original_form": "아 동생 있어요 ㅎㅎ 남자",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013822.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013822.1.7",
          "form": "저는 언니 한명 있어요!",
          "original_form": "저는 언니 한명 있어요!",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100013822.1.8",
          "form": "동생이랑 사이 좋은 편이신가요?",
          "original_form": "동생이랑 사이 좋은 편이신가요?",
          "speaker_id": "1",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100013822.1.9",
          "form": "남자끼리는 그닥그닥 ㅎㅎ",
          "original_form": "남자끼리는 그닥그닥 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100013822.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ저희도 좋은 편은 아닌데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저희도 좋은 편은 아닌데",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100013822.1.11",
          "form": "투닥투닥? 고런 느낌",
          "original_form": "투닥투닥? 고런 느낌",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100013822.1.12",
          "form": "자매는 친하지 않나요 보통",
          "original_form": "자매는 친하지 않나요 보통",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100013822.1.13",
          "form": "사람마다 다르죠ㅋㅋㅋㅋ",
          "original_form": "사람마다 다르죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100013822.1.14",
          "form": "사이좋은 형제도 있잖아요",
          "original_form": "사이좋은 형제도 있잖아요",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100013822.1.15",
          "form": "아 맞네요 ㅎㅎ 사바사는 진리죠",
          "original_form": "아 맞네요 ㅎㅎ 사바사는 진리죠",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100013822.1.16",
          "form": "남동생말고 다른 형제자매를 원했던 적 있으세요?",
          "original_form": "남동생말고 다른 형제자매를 원했던 적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100013822.1.17",
          "form": "전 옛날엔 오빠가 있었으면 좋겠다 생각했지만",
          "original_form": "전 옛날엔 오빠가 있었으면 좋겠다 생각했지만",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100013822.1.18",
          "form": "여동생이요!!!",
          "original_form": "여동생이요!!!",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100013822.1.19",
          "form": "크면서 외동이었음 좋겠다로 바뀌었슴다",
          "original_form": "크면서 외동이었음 좋겠다로 바뀌었슴다",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100013822.1.20",
          "form": "지금도 여동생 원하시나요??ㅎㅎ",
          "original_form": "지금도 여동생 원하시나요??ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100013822.1.21",
          "form": "네ㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100013822.1.22",
          "form": "용돈 잘 줄수",
          "original_form": "용돈 잘 줄수",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100013822.1.23",
          "form": "있는데 ㅎㅎ",
          "original_form": "있는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100013822.1.24",
          "form": "앗 그럼 제가 하면 안되나요",
          "original_form": "앗 그럼 제가 하면 안되나요",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100013822.1.25",
          "form": "말 잘ㄹ 들ㄹ늘겡요",
          "original_form": "말 잘ㄹ 들ㄹ늘겡요",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100013822.1.26",
          "form": "ㅋㅋ그건 지켜봐야되지 않을까요",
          "original_form": "ㅋㅋ그건 지켜봐야되지 않을까요",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100013822.1.27",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100013822.1.28",
          "form": "저희 자매는 둘다 결혼을 안해서",
          "original_form": "저희 자매는 둘다 결혼을 안해서",
          "speaker_id": "1",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100013822.1.29",
          "form": "아직 모르는데 한명이라도 결혼하면",
          "original_form": "아직 모르는데 한명이라도 결혼하면",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100013822.1.30",
          "form": "전보다 연락이 뜸하나요??",
          "original_form": "전보다 연락이 뜸하나요??",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100013822.1.31",
          "form": "아 그게 원래 뜸해서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그게 원래 뜸해서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100013822.1.32",
          "form": "같이 살때도",
          "original_form": "같이 살때도",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100013822.1.33",
          "form": "보통 부모님 생신 전날",
          "original_form": "보통 부모님 생신 전날",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100013822.1.34",
          "form": "술먹고 안오면 엄마가 시켜서",
          "original_form": "술먹고 안오면 엄마가 시켜서",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100013822.1.35",
          "form": "이런때 연락해서 ㅎㅎ",
          "original_form": "이런때 연락해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:15"
        },
        {
          "id": "MDRW2100013822.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ하긴 엄청 사이가 좋은게 아니면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ하긴 엄청 사이가 좋은게 아니면",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100013822.1.37",
          "form": "딱히 연락할 이유가 없죠ㅋㅋㅋㅋ",
          "original_form": "딱히 연락할 이유가 없죠ㅋㅋㅋㅋ",
