      "utterance": [
        {
          "id": "MDRW2100035311.1.1",
          "form": "떡볶이 이야기는 끝이없네요",
          "original_form": "떡볶이 이야기는 끝이없네요",
          "speaker_id": "2",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100035311.1.2",
          "form": "역시 떡볶이",
          "original_form": "역시 떡볶이",
          "speaker_id": "2",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100035311.1.3",
          "form": "저 하루 죙일도 할 수 있어요 ㅋㅋ",
          "original_form": "저 하루 죙일도 할 수 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100035311.1.4",
          "form": "홍대 또보겠지 떡볶이는 달고 느끼한 감튀가 되게 포인튼데",
          "original_form": "홍대 또보겠지 떡볶이는 달고 느끼한 감튀가 되게 포인튼데",
          "speaker_id": "1",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100035311.1.5",
          "form": "사람이 늘 많았었구 코로나이후엔 안갔어요",
          "original_form": "사람이 늘 많았었구 코로나이후엔 안갔어요",
          "speaker_id": "1",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100035311.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035311.1.7",
          "form": "감튀!",
          "original_form": "감튀!",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035311.1.8",
          "form": "진짜 감튀랑 찰떡궁합",
          "original_form": "진짜 감튀랑 찰떡궁합",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035311.1.9",
          "form": "버터갈릭 소스가 듬뿍 올라가있어요",
          "original_form": "버터갈릭 소스가 듬뿍 올라가있어요",
          "speaker_id": "1",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035311.1.10",
          "form": "감튀사이드 유행시킨 가게인데",
          "original_form": "감튀사이드 유행시킨 가게인데",
          "speaker_id": "1",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035311.1.11",
          "form": "와 진짜 단짠맵 제대로네요",
          "original_form": "와 진짜 단짠맵 제대로네요",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035311.1.12",
          "form": "지금은 어떨지,,",
          "original_form": "지금은 어떨지,,",
          "speaker_id": "1",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035311.1.13",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035311.1.14",
          "form": "거기가 원조였군요",
          "original_form": "거기가 원조였군요",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035311.1.15",
          "form": "전 청년다방에서 처음 먹어봤는데",
          "original_form": "전 청년다방에서 처음 먹어봤는데",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035311.1.16",
          "form": "원조는 아닐수 있어요 근데 거기서 히트친건 맞아요",
          "original_form": "원조는 아닐수 있어요 근데 거기서 히트친건 맞아요",
          "speaker_id": "1",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035311.1.17",
          "form": "그랗군요",
          "original_form": "그랗군요",
          "speaker_id": "2",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100035311.1.18",
          "form": "우리나라 음식에는 진심이에요 진짜",
          "original_form": "우리나라 음식에는 진심이에요 진짜",
          "speaker_id": "2",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100035311.1.19",
          "form": "먹잘알",
          "original_form": "먹잘알",
          "speaker_id": "2",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100035311.1.20",
          "form": "감튀사이드도 좋아하는데 이런 떡볶이는 3-4인팟이어야해서",
          "original_form": "감튀사이드도 좋아하는데 이런 떡볶이는 3-4인팟이어야해서",
          "speaker_id": "1",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100035311.1.21",
          "form": "모르는 사이에 대화하기엔 음식이 제일 또 공감가는 주제구요 ㅋㅋㅋ",
          "original_form": "모르는 사이에 대화하기엔 음식이 제일 또 공감가는 주제구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100035311.1.22",
          "form": "모르는 분이랑 학창시절에 어떠셨나요 호구조사하기도 그렇구,, 님 애인있으신가요 이런걸로 결혼얘기 물어보기도 전좀 싫구",
          "original_form": "모르는 분이랑 학창시절에 어떠셨나요 호구조사하기도 그렇구,, 님 애인있으신가요 이런걸로 결혼얘기 물어보기도 전좀 싫구",
          "speaker_id": "1",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100035311.1.23",
          "form": "그래서전 밥먹었나요 로봇이되었어요",
          "original_form": "그래서전 밥먹었나요 로봇이되었어요",
          "speaker_id": "1",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100035311.1.24",
          "form": "아 그건그래요",
          "original_form": "아 그건그래요",
          "speaker_id": "2",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100035311.1.25",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100035311.1.26",
          "form": "맞아요 어떤주제로 할지도 고민이고",
          "original_form": "맞아요 어떤주제로 할지도 고민이고",
          "speaker_id": "2",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100035311.1.27",
          "form": "식사는 하셨나요?^^ 아침은 드셨나요? 식사하셨습니까?",
          "original_form": "식사는 하셨나요?^^ 아침은 드셨나요? 식사하셨습니까?",
          "speaker_id": "1",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035311.1.28",
          "form": "실제로도 만날때도",
          "original_form": "실제로도 만날때도",
          "speaker_id": "2",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035311.1.29",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035311.1.30",
          "form": "맞아욬ㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035311.1.31",
          "form": "안궁금해도 그냥 물어보는 인사말ㅋㅋㅋ",
          "original_form": "안궁금해도 그냥 물어보는 인사말ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035311.1.32",
          "form": "안먹었다고 하면 왜 안먹었ㅇ어요 빨리 머거요 ㅠㅠㅠㅠ 굶지마세요",
          "original_form": "안먹었다고 하면 왜 안먹었ㅇ어요 빨리 머거요 ㅠㅠㅠㅠ 굶지마세요",
          "speaker_id": "1",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035311.1.33",
          "form": "라고 말하면 되고",
          "original_form": "라고 말하면 되고",
          "speaker_id": "1",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035311.1.34",
          "form": "남이 밥먹었다고 하면 네 저도 먹었어요 그럼 빠이빠이 하면 되고욬ㅋ",
          "original_form": "남이 밥먹었다고 하면 네 저도 먹었어요 그럼 빠이빠이 하면 되고욬ㅋ",
          "speaker_id": "1",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035311.1.35",
          "form": "하긴 그렇네요",
          "original_form": "하긴 그렇네요",
          "speaker_id": "2",
          "time": "20210829 14:33"
        },
        {
          "id": "MDRW2100035311.1.36",
          "form": "우리나라 밥에 진심인 국가",
          "original_form": "우리나라 밥에 진심인 국가",
