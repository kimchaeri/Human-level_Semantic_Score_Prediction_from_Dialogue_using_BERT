      "utterance": [
        {
          "id": "MDRW2100004817.1.1",
          "form": "돈 쓸 얘기 나오면 갑자기 즐거워짐",
          "original_form": "돈 쓸 얘기 나오면 갑자기 즐거워짐",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004817.1.2",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004817.1.3",
          "form": "나 옷 사고싶어",
          "original_form": "나 옷 사고싶어",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004817.1.4",
          "form": "100만원있다면",
          "original_form": "100만원있다면",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004817.1.5",
          "form": "가을옷 정말 그득 사고싶은데",
          "original_form": "가을옷 정말 그득 사고싶은데",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004817.1.6",
          "form": "옹",
          "original_form": "옹",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004817.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004817.1.8",
          "form": "먼저 말해",
          "original_form": "먼저 말해",
          "speaker_id": "2",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004817.1.9",
          "form": "아니야ㅋㅋ",
          "original_form": "아니야ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004817.1.10",
          "form": "옷 좋지ㅠ",
          "original_form": "옷 좋지ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004817.1.11",
          "form": "나도 싼 옷 말고",
          "original_form": "나도 싼 옷 말고",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004817.1.12",
          "form": "좀 이쁘고 좋은옷 사고싶어ㅠ",
          "original_form": "좀 이쁘고 좋은옷 사고싶어ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004817.1.13",
          "form": "적어도 준 명품급",
          "original_form": "적어도 준 명품급",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004817.1.14",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:43"
        },
        {
          "id": "MDRW2100004817.1.15",
          "form": "요즘 옷도 두세벌 사면 10만원 금방 넘더라",
          "original_form": "요즘 옷도 두세벌 사면 10만원 금방 넘더라",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004817.1.16",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004817.1.17",
          "form": "손 떨려서 못사겠어",
          "original_form": "손 떨려서 못사겠어",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004817.1.18",
          "form": "맞아ㅠ",
          "original_form": "맞아ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004817.1.19",
          "form": "그것말고도 살거많은데",
          "original_form": "그것말고도 살거많은데",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004817.1.20",
          "form": "후",
          "original_form": "후",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004817.1.21",
          "form": "물가가 왜이리 비싸졌지",
          "original_form": "물가가 왜이리 비싸졌지",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004817.1.22",
          "form": "원피스 너무 예쁜거 봤었는데",
          "original_form": "원피스 너무 예쁜거 봤었는데",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004817.1.23",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004817.1.24",
          "form": "5만원정도라 결국 고민하다 못샀어",
          "original_form": "5만원정도라 결국 고민하다 못샀어",
          "speaker_id": "2",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004817.1.25",
          "form": "결혼식때 입고가고 싶었는데",
          "original_form": "결혼식때 입고가고 싶었는데",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004817.1.26",
          "form": "근데 원피스는 기본 5만부터인것같어",
          "original_form": "근데 원피스는 기본 5만부터인것같어",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004817.1.27",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004817.1.28",
          "form": "부담스러워",
          "original_form": "부담스러워",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004817.1.29",
          "form": "결혼식 있나?",
          "original_form": "결혼식 있나?",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004817.1.30",
          "form": "저번에 말했던 친척언니 결혼식~",
          "original_form": "저번에 말했던 친척언니 결혼식~",
          "speaker_id": "2",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004817.1.31",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210821 18:45"
        },
        {
          "id": "MDRW2100004817.1.32",
          "form": "이제 여름 끝나서 그거 품절됐더라고",
          "original_form": "이제 여름 끝나서 그거 품절됐더라고",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004817.1.33",
          "form": "원피스 얼마가 적당하다고 생각해?",
          "original_form": "원피스 얼마가 적당하다고 생각해?",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004817.1.34",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004817.1.35",
          "form": "심지어 품절ㅠ",
          "original_form": "심지어 품절ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004817.1.36",
          "form": "3-4만원",
          "original_form": "3-4만원",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004817.1.37",
          "form": "5만원 넘어가면 안 사",
          "original_form": "5만원 넘어가면 안 사",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004817.1.38",
          "form": "백수는 돈이 없으니까",
          "original_form": "백수는 돈이 없으니까",
          "speaker_id": "2",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004817.1.39",
          "form": "그럼 진짜 살거 없을것같은데ㅠ",
          "original_form": "그럼 진짜 살거 없을것같은데ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:46"
        }
      ]
