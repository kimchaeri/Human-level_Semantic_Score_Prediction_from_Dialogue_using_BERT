      "utterance": [
        {
          "id": "MDRW2100014866.1.1",
          "form": "알고싶지않네요",
          "original_form": "알고싶지않네요",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014866.1.2",
          "form": "나 오늘 화이자 백신 맞았어",
          "original_form": "나 오늘 화이자 백신 맞았어",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014866.1.3",
          "form": "오 잘했어요",
          "original_form": "오 잘했어요",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014866.1.4",
          "form": "아직까진 아무 증상이 없는 것 같네",
          "original_form": "아직까진 아무 증상이 없는 것 같네",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014866.1.5",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014866.1.6",
          "form": "근데 방귀가 계속나와",
          "original_form": "근데 방귀가 계속나와",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014866.1.7",
          "form": "그런건 알고싶지 않아요",
          "original_form": "그런건 알고싶지 않아요",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014866.1.8",
          "form": "사양할게요",
          "original_form": "사양할게요",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014866.1.9",
          "form": "넣어둬요",
          "original_form": "넣어둬요",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014866.1.10",
          "form": "이건 넣어두는게 아니야 먹이는 거야",
          "original_form": "이건 넣어두는게 아니야 먹이는 거야",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014866.1.11",
          "form": "우리한테 먹이지마요 냄쉬나요",
          "original_form": "우리한테 먹이지마요 냄쉬나요",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014866.1.12",
          "form": "장청소를 해야 해",
          "original_form": "장청소를 해야 해",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014866.1.13",
          "form": "하는 김에 집안 청소도 하고",
          "original_form": "하는 김에 집안 청소도 하고",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014866.1.14",
          "form": "어찌나 방구를 꼈는지 안방에서도 냄새나요",
          "original_form": "어찌나 방구를 꼈는지 안방에서도 냄새나요",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014866.1.15",
          "form": "안 그래도 방바닥 끈적거려서 청소기 자주 밀어야할듯요",
          "original_form": "안 그래도 방바닥 끈적거려서 청소기 자주 밀어야할듯요",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014866.1.16",
          "form": "끈적거리는 거는 습기 때문이야",
          "original_form": "끈적거리는 거는 습기 때문이야",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014866.1.17",
          "form": "그래서 그런가 먼지도 잘 달라붙는거 가타요",
          "original_form": "그래서 그런가 먼지도 잘 달라붙는거 가타요",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014866.1.18",
          "form": "제습기를 돌려야 해",
          "original_form": "제습기를 돌려야 해",
          "speaker_id": "2",
          "time": "20210825 21:07"
        }
      ]
