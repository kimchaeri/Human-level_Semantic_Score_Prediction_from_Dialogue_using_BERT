      "utterance": [
        {
          "id": "MDRW2100019693.1.1",
          "form": "가을옷살꼬예요?",
          "original_form": "가을옷살꼬예요?",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019693.1.2",
          "form": "맘에 드는 가을옷을 못 만나서",
          "original_form": "맘에 드는 가을옷을 못 만나서",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019693.1.3",
          "form": "장바구니 채웠다가 비웠다가 하는데",
          "original_form": "장바구니 채웠다가 비웠다가 하는데",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019693.1.4",
          "form": "뭔가 남친이 내 옷 좋아하길래 사야하나 고민이 되기도",
          "original_form": "뭔가 남친이 내 옷 좋아하길래 사야하나 고민이 되기도",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100019693.1.5",
          "form": "name2님의스타일이맘에드시나봐용ㅋㅋㅋㅋ",
          "original_form": "&name2&님의스타일이맘에드시나봐용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019693.1.6",
          "form": "사던대로사면될거같아요 잘  어울리는  이쁜옷잘고르는듯",
          "original_form": "사던대로사면될거같아요 잘  어울리는  이쁜옷잘고르는듯",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019693.1.7",
          "form": "근데 올해 가을은 그냥 넘어가도 될거같아요",
          "original_form": "근데 올해 가을은 그냥 넘어가도 될거같아요",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019693.1.8",
          "form": "봄옷 쇼핑 많이 했으니까 똑같이 입으면 되지않을까하며",
          "original_form": "봄옷 쇼핑 많이 했으니까 똑같이 입으면 되지않을까하며",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019693.1.9",
          "form": "그치그치 가을이나봄이나비슷하게가면되지",
          "original_form": "그치그치 가을이나봄이나비슷하게가면되지",
          "speaker_id": "1",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100019693.1.10",
          "form": "그러서가을봄이좋은건데짧아지니아쉬웡",
          "original_form": "그러서가을봄이좋은건데짧아지니아쉬웡",
          "speaker_id": "1",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019693.1.11",
          "form": "마자여 하지만 겨울 코트 안에 입으면 되니까",
          "original_form": "마자여 하지만 겨울 코트 안에 입으면 되니까",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019693.1.12",
          "form": "사실 봄가을 옷도 중요한데",
          "original_form": "사실 봄가을 옷도 중요한데",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019693.1.13",
          "form": "아 겨울코트안에 \r\n맞네요",
          "original_form": "아 겨울코트안에 \r\n맞네요",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019693.1.14",
          "form": "ㅋㄱㅋㅈㄱㅈㄱㅈㅋ",
          "original_form": "ㅋㄱㅋㅈㄱㅈㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019693.1.15",
          "form": "그렇게입으면되네요",
          "original_form": "그렇게입으면되네요",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019693.1.16",
          "form": "네 계속 장바구니 채워보겠습니다",
          "original_form": "네 계속 장바구니 채워보겠습니다",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019693.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019693.1.18",
          "form": "(넘쳐나는장바구니)",
          "original_form": "(넘쳐나는장바구니)",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019693.1.19",
          "form": "요새 물욕이 좀 떨어져서",
          "original_form": "요새 물욕이 좀 떨어져서",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019693.1.20",
          "form": "채웟다가도 다 비우고",
          "original_form": "채웟다가도 다 비우고",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019693.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019693.1.22",
          "form": "요즘 해워니의 잔바구니엔 뭐가 있나요",
          "original_form": "요즘 해워니의 잔바구니엔 뭐가 있나요",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019693.1.23",
          "form": "요즘 주식만사서 장바구니없어요..  이제진짜옷안살거예요!!!!! \r\n이러고또셔츠사겠지..",
          "original_form": "요즘 주식만사서 장바구니없어요..  이제진짜옷안살거예요!!!!! \r\n이러고또셔츠사겠지..",
          "speaker_id": "1",
