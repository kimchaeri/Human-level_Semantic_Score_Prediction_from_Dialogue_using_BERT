      "utterance": [
        {
          "id": "MDRW2100034407.1.1",
          "form": "백신 맞으셨오요?",
          "original_form": "백신 맞으셨오요?",
          "speaker_id": "1",
          "time": "20210828 09:06"
        },
        {
          "id": "MDRW2100034407.1.2",
          "form": "1차만요 name1님은요?",
          "original_form": "1차만요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100034407.1.3",
          "form": "저두 아직 1차만 맞았어요",
          "original_form": "저두 아직 1차만 맞았어요",
          "speaker_id": "1",
          "time": "20210828 09:26"
        },
        {
          "id": "MDRW2100034407.1.4",
          "form": "담주에 2차 맞으러가요!",
          "original_form": "담주에 2차 맞으러가요!",
          "speaker_id": "1",
          "time": "20210828 09:26"
        },
        {
          "id": "MDRW2100034407.1.5",
          "form": "화이지 맞으셨나요?",
          "original_form": "화이지 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100034407.1.6",
          "form": "네네 화이자",
          "original_form": "네네 화이자",
          "speaker_id": "1",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100034407.1.7",
          "form": "저는 Az맞았어요",
          "original_form": "저는 Az맞았어요",
          "speaker_id": "2",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100034407.1.8",
          "form": "아 그러셨군요 Az가 1차가 좀 힘들었다던데",
          "original_form": "아 그러셨군요 Az가 1차가 좀 힘들었다던데",
          "speaker_id": "1",
          "time": "20210828 09:53"
        },
        {
          "id": "MDRW2100034407.1.9",
          "form": "2차도 곧 맞으러 가시겠네요?",
          "original_form": "2차도 곧 맞으러 가시겠네요?",
          "speaker_id": "1",
          "time": "20210828 09:53"
        },
        {
          "id": "MDRW2100034407.1.10",
          "form": "네 고열나더라구요 2차는 2주후에 맞아요",
          "original_form": "네 고열나더라구요 2차는 2주후에 맞아요",
          "speaker_id": "2",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100034407.1.11",
          "form": "2차는 화이자에요?",
          "original_form": "2차는 화이자에요?",
          "speaker_id": "1",
          "time": "20210828 09:54"
        },
        {
          "id": "MDRW2100034407.1.12",
          "form": "네 교차접종이더라구요",
          "original_form": "네 교차접종이더라구요",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100034407.1.13",
          "form": "저는 둘다 화이자라ㅋㅋ 교차접종이 더 안전하다는 말도 잇던데 뭐가 좋은건지 도통 모르겠네요",
          "original_form": "저는 둘다 화이자라ㅋㅋ 교차접종이 더 안전하다는 말도 잇던데 뭐가 좋은건지 도통 모르겠네요",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100034407.1.14",
          "form": "맞아요 부작용도 천차만별이고",
          "original_form": "맞아요 부작용도 천차만별이고",
          "speaker_id": "2",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100034407.1.15",
          "form": "이제 가족분들은 다 맞으셨어요?",
          "original_form": "이제 가족분들은 다 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100034407.1.16",
          "form": "아이들 빼곤 완료했네요",
          "original_form": "아이들 빼곤 완료했네요",
          "speaker_id": "2",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100034407.1.17",
          "form": "애기들두 있으시구나! 애기들은 어리나요?",
          "original_form": "애기들두 있으시구나! 애기들은 어리나요?",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100034407.1.18",
          "form": "네 영유아요^^",
          "original_form": "네 영유아요^^",
          "speaker_id": "2",
          "time": "20210828 12:31"
        }
      ]
