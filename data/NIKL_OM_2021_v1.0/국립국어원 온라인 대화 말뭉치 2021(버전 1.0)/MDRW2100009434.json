      "utterance": [
        {
          "id": "MDRW2100009434.1.1",
          "form": "예쁜분이 판매도 하시고",
          "original_form": "예쁜분이 판매도 하시고",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009434.1.2",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009434.1.3",
          "form": "공련 봤어요",
          "original_form": "공련 봤어요",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009434.1.4",
          "form": "공연",
          "original_form": "공연",
          "speaker_id": "2",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009434.1.5",
          "form": "어디서 그런공연을 보셨나요",
          "original_form": "어디서 그런공연을 보셨나요",
          "speaker_id": "1",
          "time": "20210822 23:25"
        },
        {
          "id": "MDRW2100009434.1.6",
          "form": "임진각 통일행사요",
          "original_form": "임진각 통일행사요",
          "speaker_id": "2",
          "time": "20210822 23:26"
        },
        {
          "id": "MDRW2100009434.1.7",
          "form": "백지영도 오고",
          "original_form": "백지영도 오고",
          "speaker_id": "2",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009434.1.8",
          "form": "큰행사였던거 같아요",
          "original_form": "큰행사였던거 같아요",
          "speaker_id": "2",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009434.1.9",
          "form": "오래전이네요",
          "original_form": "오래전이네요",
          "speaker_id": "2",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009434.1.10",
          "form": "군데 걱정되는게",
          "original_form": "군데 걱정되는게",
          "speaker_id": "2",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009434.1.11",
          "form": "시대가 변하는데 공연이 너무 북한식 옛날 공연이라 호응도가 떨어져요",
          "original_form": "시대가 변하는데 공연이 너무 북한식 옛날 공연이라 호응도가 떨어져요",
          "speaker_id": "2",
          "time": "20210822 23:27"
        },
        {
          "id": "MDRW2100009434.1.12",
          "form": "안타까웠어요",
          "original_form": "안타까웠어요",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009434.1.13",
          "form": "문화차가 많이나네요",
          "original_form": "문화차가 많이나네요",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009434.1.14",
          "form": "넘옛날이라 전안가본거 같아요",
          "original_form": "넘옛날이라 전안가본거 같아요",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009434.1.15",
          "form": "저는 북한분들과 함께하는 통일운동회같은데 참여했었는데요",
          "original_form": "저는 북한분들과 함께하는 통일운동회같은데 참여했었는데요",
          "speaker_id": "1",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009434.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009434.1.17",
          "form": "북한이 1인 1악기나 공연 교육하잖아요",
          "original_form": "북한이 1인 1악기나 공연 교육하잖아요",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009434.1.18",
          "form": "모두 장기가 하나씩 있으시더라구요",
          "original_form": "모두 장기가 하나씩 있으시더라구요",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009434.1.19",
          "form": "노래면 노래 악기연주",
          "original_form": "노래면 노래 악기연주",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009434.1.20",
          "form": "다재다능하셨어요",
          "original_form": "다재다능하셨어요",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009434.1.21",
          "form": "같이 어울리면서 제기차기하고",
          "original_form": "같이 어울리면서 제기차기하고",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009434.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009434.1.23",
          "form": "줄다리기했엇어요",
          "original_form": "줄다리기했엇어요",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009434.1.24",
          "form": "진짜 관심 많으시네요 ㅎㅎ",
          "original_form": "진짜 관심 많으시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009434.1.25",
          "form": "어머 정말 잘어울려서 함께 하시네요",
          "original_form": "어머 정말 잘어울려서 함께 하시네요",
          "speaker_id": "2",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009434.1.26",
          "form": "아 뭐 판매 할때도 공연 많이 오시고 시에서 행사할때도 오던데",
          "original_form": "아 뭐 판매 할때도 공연 많이 오시고 시에서 행사할때도 오던데",
          "speaker_id": "2",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009434.1.27",
          "form": "코로나로 문화공연행사가 다 취소돼서",
          "original_form": "코로나로 문화공연행사가 다 취소돼서",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009434.1.28",
          "form": "요새 공연이없어서 경제적으로도 힘드실것 같아요",
          "original_form": "요새 공연이없어서 경제적으로도 힘드실것 같아요",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009434.1.29",
          "form": "그나마도 얼굴내밀 자리가 없어지셨대요",
          "original_form": "그나마도 얼굴내밀 자리가 없어지셨대요",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009434.1.30",
          "form": "코로나가 공연계에 직격탄을 날렸어요",
          "original_form": "코로나가 공연계에 직격탄을 날렸어요",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009434.1.31",
          "form": "에긍 힘드시지만 악착같은분들이시라",
          "original_form": "에긍 힘드시지만 악착같은분들이시라",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009434.1.32",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009434.1.33",
          "form": "그런것 같아요",
          "original_form": "그런것 같아요",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009434.1.34",
          "form": "저보다 배든든하게 사실분들이실것같아요",
          "original_form": "저보다 배든든하게 사실분들이실것같아요",
          "speaker_id": "1",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009434.1.35",
          "form": "존경스러워요ㅋㅋ",
          "original_form": "존경스러워요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009434.1.36",
          "form": "ㅎㅎ그럼좋죠",
          "original_form": "ㅎㅎ그럼좋죠",
          "speaker_id": "2",
          "time": "20210822 23:34"
        }
      ]
