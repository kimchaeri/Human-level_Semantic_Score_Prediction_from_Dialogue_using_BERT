      "utterance": [
        {
          "id": "MDRW2100043448.1.1",
          "form": "여행!",
          "original_form": "여행!",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043448.1.2",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043448.1.3",
          "form": "네 여행 좋아해요",
          "original_form": "네 여행 좋아해요",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043448.1.4",
          "form": "기억나는 여행지 있으세요?",
          "original_form": "기억나는 여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043448.1.5",
          "form": "저는 동유럽 여행 갔던 것 좋았어요",
          "original_form": "저는 동유럽 여행 갔던 것 좋았어요",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043448.1.6",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043448.1.7",
          "form": "저는 유럽가보고 싶었는데 휴가가 없어서 못갛거든요",
          "original_form": "저는 유럽가보고 싶었는데 휴가가 없어서 못갛거든요",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043448.1.8",
          "form": "길게 시간을 못빼서",
          "original_form": "길게 시간을 못빼서",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043448.1.9",
          "form": "빨간지붕이랑 고성들이 좋았어요",
          "original_form": "빨간지붕이랑 고성들이 좋았어요",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043448.1.10",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043448.1.11",
          "form": "아 유럽은 좀 길게 다녀오는 게 좋긴하죠",
          "original_form": "아 유럽은 좀 길게 다녀오는 게 좋긴하죠",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043448.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043448.1.13",
          "form": "그래서 죽기전에 가볼수이슬까 싶어요",
          "original_form": "그래서 죽기전에 가볼수이슬까 싶어요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043448.1.14",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043448.1.15",
          "form": "에고 가실 수 있을거예요!!",
          "original_form": "에고 가실 수 있을거예요!!",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043448.1.16",
          "form": "여행 어디가 좋으셨어요?",
          "original_form": "여행 어디가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043448.1.17",
          "form": "저는 싱가포르랑 대만요",
          "original_form": "저는 싱가포르랑 대만요",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043448.1.18",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043448.1.19",
          "form": "오 싱가폴이랑 대만도 좋죠!!",
          "original_form": "오 싱가폴이랑 대만도 좋죠!!",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043448.1.20",
          "form": "전 싱가폴은 갔을 때 동행이 아파서 제대로 못 누려서",
          "original_form": "전 싱가폴은 갔을 때 동행이 아파서 제대로 못 누려서",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043448.1.21",
          "form": "다시 꼭 가보고 싶은 곳이예요",
          "original_form": "다시 꼭 가보고 싶은 곳이예요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043448.1.22",
          "form": "아이구ㅠ 아쉬웠겠네요",
          "original_form": "아이구ㅠ 아쉬웠겠네요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043448.1.23",
          "form": "저는 둘다 혼자갔었어요!",
          "original_form": "저는 둘다 혼자갔었어요!",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043448.1.24",
          "form": "너무 좋았던",
          "original_form": "너무 좋았던",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043448.1.25",
          "form": "또 가고싶어요",
          "original_form": "또 가고싶어요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043448.1.26",
          "form": "네네 싱가포르는 아쉬움이 남아요",
          "original_form": "네네 싱가포르는 아쉬움이 남아요",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043448.1.27",
          "form": "대만 좋죠",
          "original_form": "대만 좋죠",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043448.1.28",
          "form": "맛있는 것도 많고",
          "original_form": "맛있는 것도 많고",
          "speaker_id": "1",
          "time": "20210907 21:31"
        }
      ]
    }
