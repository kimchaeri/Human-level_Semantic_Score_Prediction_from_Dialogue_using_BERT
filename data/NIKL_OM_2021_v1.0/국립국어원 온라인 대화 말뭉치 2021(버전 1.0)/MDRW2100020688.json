      "utterance": [
        {
          "id": "MDRW2100020688.1.1",
          "form": "박지윤 님 장진감독님 김지훈님 홍진호님",
          "original_form": "박지윤 님 장진감독님 김지훈님 홍진호님",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020688.1.2",
          "form": "크라임씬 짭밥이 있어서 그런지",
          "original_form": "크라임씬 짭밥이 있어서 그런지",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020688.1.3",
          "form": "요렇게 네분은 진짜 꼭 하셨으면 좋겠어요",
          "original_form": "요렇게 네분은 진짜 꼭 하셨으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020688.1.4",
          "form": "추리력도 짱짱이고",
          "original_form": "추리력도 짱짱이고",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020688.1.5",
          "form": "장진감독님이요 ??",
          "original_form": "장진감독님이요 ??",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020688.1.6",
          "form": "아 박지윤님",
          "original_form": "아 박지윤님",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020688.1.7",
          "form": "맞아요 ㅌㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅌㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020688.1.8",
          "form": "넹넹ㅎㅎㅎ 김지훈 그 도라이",
          "original_form": "넹넹ㅎㅎㅎ 김지훈 그 도라이",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020688.1.9",
          "form": "김건반ㅋㅋㅋㅋㅋ",
          "original_form": "김건반ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020688.1.10",
          "form": "여추반 그 케미도 너무 좋았어요",
          "original_form": "여추반 그 케미도 너무 좋았어요",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020688.1.11",
          "form": "아 개웃겨요 ㅌㅌㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 개웃겨요 ㅌㅌㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020688.1.12",
          "form": "마자요ㅠㅠ 그거때문에 티빙 결제해서 봤을정도",
          "original_form": "마자요ㅠㅠ 그거때문에 티빙 결제해서 봤을정도",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020688.1.13",
          "form": "김지훈도 근데 진짜 범인일때가ㅜ재밌은것같아요",
          "original_form": "김지훈도 근데 진짜 범인일때가ㅜ재밌은것같아요",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020688.1.14",
          "form": "와 맞아요 그러신분들 많으시더라구요",
          "original_form": "와 맞아요 그러신분들 많으시더라구요",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020688.1.15",
          "form": "연기자라그런지",
          "original_form": "연기자라그런지",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020688.1.16",
          "form": "연기력이 대박이에요ㅋㅋㅋ",
          "original_form": "연기력이 대박이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020688.1.17",
          "form": "마저요 진짜",
          "original_form": "마저요 진짜",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020688.1.18",
          "form": "좋아하시는 영화 있으세용??",
          "original_form": "좋아하시는 영화 있으세용??",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020688.1.19",
          "form": "저 ㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋ 조금 나잇값 못하기는 한데 혹시 애니메이션 영화 업 아세요 ??",
          "original_form": "저 ㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋ 조금 나잇값 못하기는 한데 혹시 애니메이션 영화 업 아세요 ??",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020688.1.20",
          "form": "저 그거 진짜 재미있게 봤어요",
          "original_form": "저 그거 진짜 재미있게 봤어요",
          "speaker_id": "2",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020688.1.21",
          "form": "볼때마다 울엇ㄱ어요",
          "original_form": "볼때마다 울엇ㄱ어요",
          "speaker_id": "2",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020688.1.22",
          "form": "아 봤죠봤죠ㅠㅠ",
          "original_form": "아 봤죠봤죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020688.1.23",
          "form": "그거 시작한지 십분만에",
          "original_form": "그거 시작한지 십분만에",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020688.1.24",
          "form": "눈물쏟는 영화로",
          "original_form": "눈물쏟는 영화로",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020688.1.25",
          "form": "유명하자나욬ㅋㅋㅋㅋ",
          "original_form": "유명하자나욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020688.1.26",
          "form": "ㅠㅠㅠ 도요새야~",
          "original_form": "ㅠㅠㅠ 도요새야~",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020688.1.27",
          "form": "하 역시 ㅠ ㅠ ㅠ ㅠ 아시는군요",
          "original_form": "하 역시 ㅠ ㅠ ㅠ ㅠ 아시는군요",
          "speaker_id": "2",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020688.1.28",
          "form": "진짜.맞아요 시작하자마자 울엇ㄱ어요 ㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜.맞아요 시작하자마자 울엇ㄱ어요 ㅌㅌㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020688.1.29",
          "form": "아 또보고싶네요 또봐야겠어요",
          "original_form": "아 또보고싶네요 또봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020688.1.30",
          "form": "ㅠㅠㅠ 무슨 한 사람 인생이 십분만에 담겨있는데",
          "original_form": "ㅠㅠㅠ 무슨 한 사람 인생이 십분만에 담겨있는데",
          "speaker_id": "1",
