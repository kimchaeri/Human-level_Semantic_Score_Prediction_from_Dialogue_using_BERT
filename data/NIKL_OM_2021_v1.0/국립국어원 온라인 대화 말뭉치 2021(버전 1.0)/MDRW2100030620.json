        {
          "id": "MDRW2100030620.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100030620.1.2",
          "form": "할려고는 하는데 잘 안되요",
          "original_form": "할려고는 하는데 잘 안되요",
          "speaker_id": "2",
          "time": "20210904 11:01"
        },
        {
          "id": "MDRW2100030620.1.3",
          "form": "전 홈트요",
          "original_form": "전 홈트요",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030620.1.4",
          "form": "오 홈트 어떤거 하세요?",
          "original_form": "오 홈트 어떤거 하세요?",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030620.1.5",
          "form": "플랭크해요",
          "original_form": "플랭크해요",
          "speaker_id": "1",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030620.1.6",
          "form": "오 그거 해보고 힘들어서 전 포기했어요. 아예 운동을 안하네요 요즘",
          "original_form": "오 그거 해보고 힘들어서 전 포기했어요. 아예 운동을 안하네요 요즘",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030620.1.7",
          "form": "유투브봐요",
          "original_form": "유투브봐요",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030620.1.8",
          "form": "유튜브보면서 순환운동 그런건 가끔하긴하는데",
          "original_form": "유튜브보면서 순환운동 그런건 가끔하긴하는데",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030620.1.9",
          "form": "건강위해서",
