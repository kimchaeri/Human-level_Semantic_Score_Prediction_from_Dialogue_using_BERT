      "utterance": [
        {
          "id": "MDRW2100009154.1.1",
          "form": "영화보러 자주가요?",
          "original_form": "영화보러 자주가요?",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009154.1.2",
          "form": "아뇨 요즘 코로나 때매 자주는 못가요..ㅠ",
          "original_form": "아뇨 요즘 코로나 때매 자주는 못가요..ㅠ",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009154.1.3",
          "form": "집에서는 안보나용 ?",
          "original_form": "집에서는 안보나용 ?",
          "speaker_id": "1",
          "time": "20210822 21:48"
        },
        {
          "id": "MDRW2100009154.1.4",
          "form": "잘은 안보는데 최근에 보스베이비 친구들이랑 봣어요 ㅋㅋ",
          "original_form": "잘은 안보는데 최근에 보스베이비 친구들이랑 봣어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009154.1.5",
          "form": "옼ㅋㅋ 저도 보스베이비 오늘봤어요",
          "original_form": "옼ㅋㅋ 저도 보스베이비 오늘봤어요",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009154.1.6",
          "form": "매귀 ㅠ",
          "original_form": "매귀 ㅠ",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009154.1.7",
          "form": "오오 대박 ㅋㅋㅋㅋ",
          "original_form": "오오 대박 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009154.1.8",
          "form": "맞아요 애기들 귀엽죠!!",
          "original_form": "맞아요 애기들 귀엽죠!!",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009154.1.9",
          "form": "ㅋㅋㅋㅋ재밌더라구요",
          "original_form": "ㅋㅋㅋㅋ재밌더라구요",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009154.1.10",
          "form": "애기들 넘 귀여워여",
          "original_form": "애기들 넘 귀여워여",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009154.1.11",
          "form": "무서운거 잘봐영?",
          "original_form": "무서운거 잘봐영?",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009154.1.12",
          "form": "무서운거는 보는데 잔인한건 잘 못봐요",
          "original_form": "무서운거는 보는데 잔인한건 잘 못봐요",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009154.1.13",
          "form": "맞아여 무서운건 그나마 보겟는데",
          "original_form": "맞아여 무서운건 그나마 보겟는데",
          "speaker_id": "1",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009154.1.14",
          "form": "징그러운건 질색 ..",
          "original_form": "징그러운건 질색 ..",
          "speaker_id": "1",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009154.1.15",
          "form": "피나오는걸 보면 속이 안좋아지더라구요 ㅋㅋ",
          "original_form": "피나오는걸 보면 속이 안좋아지더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009154.1.16",
          "form": "너무 사실적이게 징그러운거 정말 못보겟어요 ㅠ 내몸이 다아파..",
          "original_form": "너무 사실적이게 징그러운거 정말 못보겟어요 ㅠ 내몸이 다아파..",
          "speaker_id": "1",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100009154.1.17",
          "form": "맞아요ㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009154.1.18",
          "form": "로맨스 영화 좋아하나요",
          "original_form": "로맨스 영화 좋아하나요",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009154.1.19",
          "form": "로맨스는 영화보다 드라마로 많이 봣어요",
          "original_form": "로맨스는 영화보다 드라마로 많이 봣어요",
          "speaker_id": "2",
