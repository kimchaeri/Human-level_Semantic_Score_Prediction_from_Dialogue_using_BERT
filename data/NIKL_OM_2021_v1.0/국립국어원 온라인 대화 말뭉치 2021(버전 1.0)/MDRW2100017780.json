      "utterance": [
        {
          "id": "MDRW2100017780.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017780.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017780.1.3",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "2",
          "time": "20210826 21:05"
        },
        {
          "id": "MDRW2100017780.1.4",
          "form": "네네 빵먹었어요ㅋㅋㅋㅋ",
          "original_form": "네네 빵먹었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017780.1.5",
          "form": "저녁 뭐 드셧어요?",
          "original_form": "저녁 뭐 드셧어요?",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017780.1.6",
          "form": "저는 된장국 먹었어요",
          "original_form": "저는 된장국 먹었어요",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017780.1.7",
          "form": "무슨 빵드셨나요",
          "original_form": "무슨 빵드셨나요",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017780.1.8",
          "form": "저 식빵이랑 도넛먹었어요ㅋㅋㅋ",
          "original_form": "저 식빵이랑 도넛먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017780.1.9",
          "form": "파바 할인중이라서 다녀왓어요",
          "original_form": "파바 할인중이라서 다녀왓어요",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017780.1.10",
          "form": "파바 할인중이군요",
          "original_form": "파바 할인중이군요",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017780.1.11",
          "form": "무슨 식빵드셨나요",
          "original_form": "무슨 식빵드셨나요",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017780.1.12",
          "form": "토종효모 상미종 꿀토스트",
          "original_form": "토종효모 상미종 꿀토스트",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017780.1.13",
          "form": "어떤걸 드셨으려나",
          "original_form": "어떤걸 드셨으려나",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017780.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ우유식빵이요 디테일하게 잘 아시네요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ우유식빵이요 디테일하게 잘 아시네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017780.1.15",
          "form": "파리바게트를 좋아합니다ㅎ",
          "original_form": "파리바게트를 좋아합니다ㅎ",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017780.1.16",
          "form": "그럼 도넛은 왕크림도넛을 드셨나요",
          "original_form": "그럼 도넛은 왕크림도넛을 드셨나요",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017780.1.17",
          "form": "네 맞아요ㅋㅋㅋㅋㅋㅋ그거랑 그 동글동글하고 쫄깃한 초코도넛?",
          "original_form": "네 맞아요ㅋㅋㅋㅋㅋㅋ그거랑 그 동글동글하고 쫄깃한 초코도넛?",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017780.1.18",
          "form": "파바에서 도넛 첨먹었는데 맛잇더라구요",
          "original_form": "파바에서 도넛 첨먹었는데 맛잇더라구요",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017780.1.19",
          "form": "맛있는거만 쏙쏙 골라드셨네요",
          "original_form": "맛있는거만 쏙쏙 골라드셨네요",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017780.1.20",
          "form": "맞아요 파바가 은근맛집",
          "original_form": "맞아요 파바가 은근맛집",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017780.1.21",
          "form": "다음에 아이스크림도 먹어봐요",
          "original_form": "다음에 아이스크림도 먹어봐요",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017780.1.22",
          "form": "파바 아이스크림 찐이에요",
          "original_form": "파바 아이스크림 찐이에요",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017780.1.23",
          "form": "오 진짜요? 아이스크림은 생각도못했는데",
          "original_form": "오 진짜요? 아이스크림은 생각도못했는데",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017780.1.24",
          "form": "추천해주세요!",
          "original_form": "추천해주세요!",
          "speaker_id": "1",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017780.1.25",
          "form": "그 캔디바 처럼 생긴거 있는데 깔라만씨도 맛있구요 초코 딸기 다 맛있어요. 뭔가 뻔한 맛같아서 기대도 없다가 맛있어서 오? 하게되는 맛 그리고 이번에 나온 좀 비싼 막대있는데 그거 이름이 뭐더라",
          "original_form": "그 캔디바 처럼 생긴거 있는데 깔라만씨도 맛있구요 초코 딸기 다 맛있어요. 뭔가 뻔한 맛같아서 기대도 없다가 맛있어서 오? 하게되는 맛 그리고 이번에 나온 좀 비싼 막대있는데 그거 이름이 뭐더라",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017780.1.26",
          "form": "오오오 자세한 설명 감사해요! ㅋㅋㅋㅋㅋ내일 사러가야겠어요ㅋㅋㅋㅋ",
          "original_form": "오오오 자세한 설명 감사해요! ㅋㅋㅋㅋㅋ내일 사러가야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:22"
        }
      ]
    }
