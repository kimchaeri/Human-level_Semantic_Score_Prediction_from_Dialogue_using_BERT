      "utterance": [
        {
          "id": "MDRW2100012843.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012843.1.2",
          "form": "여행경비만 있으면 ㅎ 어디든 갑니다 무척 좋아해요",
          "original_form": "여행경비만 있으면 ㅎ 어디든 갑니다 무척 좋아해요",
          "speaker_id": "2",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012843.1.3",
          "form": "어디 가보셨나요?",
          "original_form": "어디 가보셨나요?",
          "speaker_id": "1",
          "time": "20210825 02:36"
        },
        {
          "id": "MDRW2100012843.1.4",
          "form": "제 기억으론 중국에 2번 간거같아요 청소년시기때 가서..",
          "original_form": "제 기억으론 중국에 2번 간거같아요 청소년시기때 가서..",
          "speaker_id": "2",
          "time": "20210825 02:37"
        },
        {
          "id": "MDRW2100012843.1.5",
          "form": "시간과 비용이 부족해서.. 맨날 못가네요",
          "original_form": "시간과 비용이 부족해서.. 맨날 못가네요",
          "speaker_id": "2",
          "time": "20210825 02:38"
        },
        {
          "id": "MDRW2100012843.1.6",
          "form": "그쪽은 여행 어디가보셨나요?",
          "original_form": "그쪽은 여행 어디가보셨나요?",
          "speaker_id": "2",
          "time": "20210825 02:38"
        },
        {
          "id": "MDRW2100012843.1.7",
          "form": "제주도 가봤어요",
          "original_form": "제주도 가봤어요",
          "speaker_id": "1",
          "time": "20210825 02:40"
        },
        {
          "id": "MDRW2100012843.1.8",
          "form": "좋은곳 가셨군요  관광 명소 아닙니까~",
          "original_form": "좋은곳 가셨군요  관광 명소 아닙니까~",
          "speaker_id": "2",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100012843.1.9",
          "form": "기념품도 충분히 사올 수있는 여행지잖아요",
          "original_form": "기념품도 충분히 사올 수있는 여행지잖아요",
          "speaker_id": "2",
          "time": "20210825 02:42"
        },
        {
          "id": "MDRW2100012843.1.10",
          "form": "제주도 초콜릿 국룰이죠 ㅋㅋ",
          "original_form": "제주도 초콜릿 국룰이죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:43"
        },
        {
          "id": "MDRW2100012843.1.11",
          "form": "역시 똑똑합니다 ",
          "original_form": "역시 똑똑합니다 ",
          "speaker_id": "2",
          "time": "20210825 02:45"
        },
        {
          "id": "MDRW2100012843.1.12",
          "form": "근데 해외여행은 안가보셨나요?",
          "original_form": "근데 해외여행은 안가보셨나요?",
          "speaker_id": "2",
          "time": "20210825 02:45"
        },
        {
          "id": "MDRW2100012843.1.13",
          "form": "홍콩 가봤던 것 같아요",
          "original_form": "홍콩 가봤던 것 같아요",
          "speaker_id": "1",
          "time": "20210825 02:46"
        },
        {
          "id": "MDRW2100012843.1.14",
          "form": "제가 못가본 해외나라를 가보셨다니 부럽네요",
          "original_form": "제가 못가본 해외나라를 가보셨다니 부럽네요",
          "speaker_id": "2",
          "time": "20210825 02:49"
        },
        {
          "id": "MDRW2100012843.1.15",
          "form": "국내 여행지 추천좀 해주세요.",
          "original_form": "국내 여행지 추천좀 해주세요.",
          "speaker_id": "2",
          "time": "20210825 02:50"
        },
        {
          "id": "MDRW2100012843.1.16",
          "form": "앗 제가 추천 받고싶었는데",
          "original_form": "앗 제가 추천 받고싶었는데",
          "speaker_id": "1",
          "time": "20210825 02:51"
        },
        {
          "id": "MDRW2100012843.1.17",
          "form": "나중에 국내여행 가려고 했거든요",
          "original_form": "나중에 국내여행 가려고 했거든요",
          "speaker_id": "1",
          "time": "20210825 02:51"
        },
        {
          "id": "MDRW2100012843.1.18",
          "form": "코로나 끝나면 마음껏 놀려가요 강원도가 그나마 나을듯합니다.",
          "original_form": "코로나 끝나면 마음껏 놀려가요 강원도가 그나마 나을듯합니다.",
          "speaker_id": "2",
          "time": "20210825 02:52"
        },
        {
          "id": "MDRW2100012843.1.19",
          "form": "타지역 어디든 가세요 그게 곧 여행이니",
          "original_form": "타지역 어디든 가세요 그게 곧 여행이니",
          "speaker_id": "2",
          "time": "20210825 02:54"
        },
        {
          "id": "MDRW2100012843.1.20",
          "form": "ㅋㅋㅋㅋㅋ 맞는말이죠",
          "original_form": "ㅋㅋㅋㅋㅋ 맞는말이죠",
