      },
      "utterance": [
        {
          "id": "MDRW2100043315.1.1",
          "form": "수입이야? 뭘 들어와",
          "original_form": "수입이야? 뭘 들어와",
          "speaker_id": "2",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043315.1.2",
          "form": "다른나라 요리 그런 느낌은 아닌데 해외 케이에프씨에서 팔았나봐요 그걸",
          "original_form": "다른나라 요리 그런 느낌은 아닌데 해외 케이에프씨에서 팔았나봐요 그걸",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043315.1.3",
          "form": "반응 좋으니까 들여왔나보네",
          "original_form": "반응 좋으니까 들여왔나보네",
          "speaker_id": "2",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043315.1.4",
          "form": "그 그말도 맞는데 그게 아니고 ㅋㅋㅋ 어떤 사람이 해외에 나갔다가 너무 맛있어서",
          "original_form": "그 그말도 맞는데 그게 아니고 ㅋㅋㅋ 어떤 사람이 해외에 나갔다가 너무 맛있어서",
          "speaker_id": "1",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100043315.1.5",
          "form": "맨날 본사에 글올렸대요",
          "original_form": "맨날 본사에 글올렸대요",
          "speaker_id": "1",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100043315.1.6",
          "form": "그사람 소원성취했네",
          "original_form": "그사람 소원성취했네",
          "speaker_id": "2",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100043315.1.7",
          "form": "그걸 인터넷에 올렸는데 그게 화제가 돼서 출시하기로 했다나봐요",
          "original_form": "그걸 인터넷에 올렸는데 그게 화제가 돼서 출시하기로 했다나봐요",
          "speaker_id": "1",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100043315.1.8",
          "form": "잘됐다 그래도 잘팔리는거같으니까",
          "original_form": "잘됐다 그래도 잘팔리는거같으니까",
          "speaker_id": "2",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100043315.1.9",
          "form": "그쵸 ㅋㅋㅋ 소원성취하고 맛있는것도 가까운데에서 먹고",
          "original_form": "그쵸 ㅋㅋㅋ 소원성취하고 맛있는것도 가까운데에서 먹고",
          "speaker_id": "1",
          "time": "20210907 17:34"
        },
        {
          "id": "MDRW2100043315.1.10",
          "form": "그거 유명해져서 다른곳에서도 팔게됐으니까",
          "original_form": "그거 유명해져서 다른곳에서도 팔게됐으니까",
          "speaker_id": "2",
          "time": "20210907 17:34"
        },
        {
          "id": "MDRW2100043315.1.11",
          "form": "맞아요 그래서 살 많이 없는 부위도 유행타기 시작한거같아요",
          "original_form": "맞아요 그래서 살 많이 없는 부위도 유행타기 시작한거같아요",
          "speaker_id": "1",
          "time": "20210907 17:34"
        },
        {
          "id": "MDRW2100043315.1.12",
          "form": "네네치킨인가 페리카나에서도 꼬들목 나왔더라",
          "original_form": "네네치킨인가 페리카나에서도 꼬들목 나왔더라",
          "speaker_id": "2",
          "time": "20210907 17:34"
        },
        {
          "id": "MDRW2100043315.1.13",
          "form": "목살 야들야들하지 않나? 어떻게 꼬들목이지",
          "original_form": "목살 야들야들하지 않나? 어떻게 꼬들목이지",
          "speaker_id": "1",
          "time": "20210907 17:34"
        },
        {
          "id": "MDRW2100043315.1.14",
          "form": "그러게 연골까지 넣어서 튀기나 연골 맛있긴 한데",
          "original_form": "그러게 연골까지 넣어서 튀기나 연골 맛있긴 한데",
          "speaker_id": "2",
          "time": "20210907 17:35"
        },
        {
          "id": "MDRW2100043315.1.15",
          "form": "물렁뼈 먹다가 이빨 나가거든요 ㅋㅋㅋ 조심하세요",
          "original_form": "물렁뼈 먹다가 이빨 나가거든요 ㅋㅋㅋ 조심하세요",
          "speaker_id": "1",
          "time": "20210907 17:35"
        }
      ]
