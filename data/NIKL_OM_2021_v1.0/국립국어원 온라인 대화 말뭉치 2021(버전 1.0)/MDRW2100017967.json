      "utterance": [
        {
          "id": "MDRW2100017967.1.1",
          "form": "지금 뉴스 보세요?",
          "original_form": "지금 뉴스 보세요?",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017967.1.2",
          "form": "어 스포츠뉴스 보고있어",
          "original_form": "어 스포츠뉴스 보고있어",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017967.1.3",
          "form": "저도 보고 있어요 오늘 야구 어떻게 됐는지 모르겠네",
          "original_form": "저도 보고 있어요 오늘 야구 어떻게 됐는지 모르겠네",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017967.1.4",
          "form": "엘지가 이겨야 이자가 붙을텐데요",
          "original_form": "엘지가 이겨야 이자가 붙을텐데요",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017967.1.5",
          "form": "아까 보니까 이긴거 같던데 요즘 야구 인기가 떨어졌어",
          "original_form": "아까 보니까 이긴거 같던데 요즘 야구 인기가 떨어졌어",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017967.1.6",
          "form": "야구선수들이 구려서",
          "original_form": "야구선수들이 구려서",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017967.1.7",
          "form": "지금 보니 뉴스에 배구도 나오네",
          "original_form": "지금 보니 뉴스에 배구도 나오네",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017967.1.8",
          "form": "올림픽에서 야구가 너무 못해서 그런가봐",
          "original_form": "올림픽에서 야구가 너무 못해서 그런가봐",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017967.1.9",
          "form": "올림픽 재미있었는데",
          "original_form": "올림픽 재미있었는데",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017967.1.10",
          "form": "맞아요 진짜 못했어요",
          "original_form": "맞아요 진짜 못했어요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017967.1.11",
          "form": "패럴림픽도 중계해주던데 아까 보니 탁구 잘하는 선수있더라",
          "original_form": "패럴림픽도 중계해주던데 아까 보니 탁구 잘하는 선수있더라",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017967.1.12",
          "form": "야구는 이제 별로 기대가 안되는거 같아요",
          "original_form": "야구는 이제 별로 기대가 안되는거 같아요",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017967.1.13",
          "form": "그래요? 올림픽 끝나서 아쉬웠는데 패럴림픽이라도 볼까봐요",
          "original_form": "그래요? 올림픽 끝나서 아쉬웠는데 패럴림픽이라도 볼까봐요",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017967.1.14",
          "form": "야구선수들이 정신을 차려야지",
          "original_form": "야구선수들이 정신을 차려야지",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017967.1.15",
          "form": "쓱이나 엘지가 이겨야 이자율이 올라가요",
          "original_form": "쓱이나 엘지가 이겨야 이자율이 올라가요",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017967.1.16",
          "form": "그래도 아까 보니 고등학교 선수들 선발하는거 며칠전에 했나보던데",
          "original_form": "그래도 아까 보니 고등학교 선수들 선발하는거 며칠전에 했나보던데",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017967.1.17",
          "form": "쓱은 처음에 투수 뽑은거 같던데요",
          "original_form": "쓱은 처음에 투수 뽑은거 같던데요",
          "speaker_id": "1",
          "time": "20210826 21:59"
        }
      ]
