      "utterance": [
        {
          "id": "MDRW2100024230.1.1",
          "form": "저 오늘 시작했어요ㅋㅋㅋ",
          "original_form": "저 오늘 시작했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024230.1.2",
          "form": "배스킨 좋지요ㅋㅋㅋ",
          "original_form": "배스킨 좋지요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024230.1.3",
          "form": "베스킨이 목표였었답니다 ㅋㅋㅋㅋ 이제는 삼천원만 받아도",
          "original_form": "베스킨이 목표였었답니다 ㅋㅋㅋㅋ 이제는 삼천원만 받아도",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024230.1.4",
          "form": "될거 같은 느낌이에요",
          "original_form": "될거 같은 느낌이에요",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024230.1.5",
          "form": "name2님처럼 슥슥 잘 지나가는 대화가 많으면 좋은데 세상일이 그렇지만은 않네여...",
          "original_form": "&name2&님처럼 슥슥 잘 지나가는 대화가 많으면 좋은데 세상일이 그렇지만은 않네여...",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024230.1.6",
          "form": "이번에 베스킨에서 맥심맛 아이스크림 나왔대요ㅋㅋㅋ",
          "original_form": "이번에 베스킨에서 맥심맛 아이스크림 나왔대요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024230.1.7",
          "form": "배스킨이라니깐 급 생각났어요^^ㅋ",
          "original_form": "배스킨이라니깐 급 생각났어요^^ㅋ",
          "speaker_id": "2",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024230.1.8",
          "form": "맛있을거 같네요 근데 그거 제돈 주고 사먹기 조금 아깝",
          "original_form": "맛있을거 같네요 근데 그거 제돈 주고 사먹기 조금 아깝",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100024230.1.9",
          "form": "ㅋㅋㅋㅋㅋ 맥심이라니까 좀",
          "original_form": "ㅋㅋㅋㅋㅋ 맥심이라니까 좀",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024230.1.10",
          "form": "더위사냥같을거 같아요",
          "original_form": "더위사냥같을거 같아요",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024230.1.11",
          "form": "회사에 늘 구비되어있는 맥심ㅋㅋㅋㅋ",
          "original_form": "회사에 늘 구비되어있는 맥심ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024230.1.12",
          "form": "그러니까욬ㅋㅋㅋ",
          "original_form": "그러니까욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024230.1.13",
          "form": "저도 오ㅋㅋ맛있겠다 근데 굳이...ㅋ",
          "original_form": "저도 오ㅋㅋ맛있겠다 근데 굳이...ㅋ",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024230.1.14",
          "form": "이생각이 들더라고요ㅋㅋ",
          "original_form": "이생각이 들더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024230.1.15",
          "form": "ㅋㅋㅋㅋ 아무래도 할게 떨어졌나봐요",
          "original_form": "ㅋㅋㅋㅋ 아무래도 할게 떨어졌나봐요",
          "speaker_id": "1",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024230.1.16",
          "form": "차라리 한여름 출시라면 라떼 대신 한 번 먹을텐데ㅋㅋ",
          "original_form": "차라리 한여름 출시라면 라떼 대신 한 번 먹을텐데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:29"
        },
        {
          "id": "MDRW2100024230.1.17",
          "form": "좀 아쉬운느낌이여요ㅋㅋ",
          "original_form": "좀 아쉬운느낌이여요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024230.1.18",
          "form": "그러니까요 ㅋㅋㅋ",
          "original_form": "그러니까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024230.1.19",
          "form": "쫀떡궁합 이후로 신제품 못먹었어요 ㅋㅋㅋ",
          "original_form": "쫀떡궁합 이후로 신제품 못먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024230.1.20",
          "form": "맞아요ㅋㅋㅋ은근히 끌리는게 없음...ㅠㅠㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ은근히 끌리는게 없음...ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024230.1.21",
          "form": "그거 맛있더라고요 레몬 무슨 샤베트 그것도 괜찮았는데 너무 검은색이라",
          "original_form": "그거 맛있더라고요 레몬 무슨 샤베트 그것도 괜찮았는데 너무 검은색이라",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024230.1.22",
          "form": "아ㅋㅋㅋ그거ㅋㅋ쇼베르트?이런이름이였는데ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ그거ㅋㅋ쇼베르트?이런이름이였는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024230.1.23",
          "form": "넹 가끔 먹고 싶은거 있어도 지점마다 안들어올때도 있어서 못먹고 지나가요 ㅋㅋ",
          "original_form": "넹 가끔 먹고 싶은거 있어도 지점마다 안들어올때도 있어서 못먹고 지나가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024230.1.24",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024230.1.25",
          "form": "이번에 꼬북칩콜라보도 별로였음...ㅋㅋ너무 유행에만 편승하는 기분ㅋ",
          "original_form": "이번에 꼬북칩콜라보도 별로였음...ㅋㅋ너무 유행에만 편승하는 기분ㅋ",
          "speaker_id": "2",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024230.1.26",
          "form": "그거 맛없었어요? 늦게 갔더니 들어오지도 않아서 못먹었어요 ㅋㅋㅋ",
          "original_form": "그거 맛없었어요? 늦게 갔더니 들어오지도 않아서 못먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024230.1.27",
          "form": "그냥 저는 안먹었어요ㅋㅋㅋ뭔가 뻔할 것 같아서요",
          "original_form": "그냥 저는 안먹었어요ㅋㅋㅋ뭔가 뻔할 것 같아서요",
          "speaker_id": "2",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024230.1.28",
          "form": "ㅋㅋㅋ 그럴거 같긴해요",
          "original_form": "ㅋㅋㅋ 그럴거 같긴해요",
