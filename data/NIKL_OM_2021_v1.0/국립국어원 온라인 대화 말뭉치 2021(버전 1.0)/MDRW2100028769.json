      "utterance": [
        {
          "id": "MDRW2100028769.1.1",
          "form": "일광?",
          "original_form": "일광?",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028769.1.2",
          "form": "일광 쪽인지는 정확히 모르겠다",
          "original_form": "일광 쪽인지는 정확히 모르겠다",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028769.1.3",
          "form": "그럼 부산대 안가고 거기서",
          "original_form": "그럼 부산대 안가고 거기서",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028769.1.4",
          "form": "케이크 살래?",
          "original_form": "케이크 살래?",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028769.1.5",
          "form": "엄청 크게 생기긴했는데 카페겸 베이커리래",
          "original_form": "엄청 크게 생기긴했는데 카페겸 베이커리래",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028769.1.6",
          "form": "홀케이크도 파는 지는 모르겠다",
          "original_form": "홀케이크도 파는 지는 모르겠다",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028769.1.7",
          "form": "반찬사러 갈 때 들릴래?",
          "original_form": "반찬사러 갈 때 들릴래?",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028769.1.8",
          "form": "ㅇㅋㅇㅋ좋음",
          "original_form": "ㅇㅋㅇㅋ좋음",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028769.1.9",
          "form": "기장 드라이브 갈때나 우리 반찬 사러 갈 때 들렸다 오자",
          "original_form": "기장 드라이브 갈때나 우리 반찬 사러 갈 때 들렸다 오자",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028769.1.10",
          "form": "공유 한거 봤어?",
          "original_form": "공유 한거 봤어?",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028769.1.11",
          "form": "부산대 앞 빵은 맛있는데",
          "original_form": "부산대 앞 빵은 맛있는데",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028769.1.12",
          "form": "여기도 맛있을지 모르겠다",
          "original_form": "여기도 맛있을지 모르겠다",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028769.1.13",
          "form": "크긴 크더라",
          "original_form": "크긴 크더라",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028769.1.14",
          "form": "ㅇㅇ 풍경도 바다 보여서 좋은듯",
          "original_form": "ㅇㅇ 풍경도 바다 보여서 좋은듯",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028769.1.15",
          "form": "바닷간가봐",
          "original_form": "바닷간가봐",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028769.1.16",
          "form": "가서 커피도 한잔하고 오지 머",
          "original_form": "가서 커피도 한잔하고 오지 머",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028769.1.17",
          "form": "ㅇㅇ사람 많음 포장해오자",
          "original_form": "ㅇㅇ사람 많음 포장해오자",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028769.1.18",
          "form": "가까운데 생겨서 좋네",
          "original_form": "가까운데 생겨서 좋네",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100028769.1.19",
          "form": "맞아 부대앞에 볼일 있을 때나 들렀었는데",
          "original_form": "맞아 부대앞에 볼일 있을 때나 들렀었는데",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028769.1.20",
          "form": "좋음ㅋㅋ",
          "original_form": "좋음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028769.1.21",
          "form": "그렇긴해 그래도 가봐야 알지",
