      "utterance": [
        {
          "id": "MDRW2100031199.1.1",
          "form": "안녕하세용~",
          "original_form": "안녕하세용~",
          "speaker_id": "1",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031199.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031199.1.3",
          "form": "책 읽는거 좋아하세요?",
          "original_form": "책 읽는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031199.1.4",
          "form": "요즘 웹소설만 읽어요 ㅎ",
          "original_form": "요즘 웹소설만 읽어요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031199.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031199.1.6",
          "form": "오 웹소설도 책이죠ㅋㅋㅋ",
          "original_form": "오 웹소설도 책이죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031199.1.7",
          "form": "저는 안읽어요....ㅋㅋㅋㅋ 안읽은지 오래된것 같네요",
          "original_form": "저는 안읽어요....ㅋㅋㅋㅋ 안읽은지 오래된것 같네요",
          "speaker_id": "1",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031199.1.8",
          "form": "그런거 있짆아요 한번 느낌오면 쫘아아악 읽다가 지치는",
          "original_form": "그런거 있짆아요 한번 느낌오면 쫘아아악 읽다가 지치는",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031199.1.9",
          "form": "저는 그래요..ㅠㅠㅋㅋ",
          "original_form": "저는 그래요..ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031199.1.10",
          "form": "어떤 웹소설이에요?",
          "original_form": "어떤 웹소설이에요?",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031199.1.11",
          "form": "아 요즘 오래 집중하기가 힘든거같긴해요",
          "original_form": "아 요즘 오래 집중하기가 힘든거같긴해요",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031199.1.12",
          "form": "그냥 판타지요",
          "original_form": "그냥 판타지요",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031199.1.13",
          "form": "카카오페이지에서요 ㅎ",
          "original_form": "카카오페이지에서요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031199.1.14",
          "form": "헐 저 판타지 좋아해요",
          "original_form": "헐 저 판타지 좋아해요",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031199.1.15",
          "form": "어떤 내용인지 알려주실 수 있으세요?",
          "original_form": "어떤 내용인지 알려주실 수 있으세요?",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031199.1.16",
          "form": "들려드리긴 내용이 유치해서",
          "original_form": "들려드리긴 내용이 유치해서",
          "speaker_id": "2",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031199.1.17",
          "form": "부끄럽네요",
          "original_form": "부끄럽네요",
          "speaker_id": "2",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031199.1.18",
          "form": "곤란하게 하려던건 아닙니다  죄송해용 ㅎㅎㅎ",
          "original_form": "곤란하게 하려던건 아닙니다  죄송해용 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031199.1.19",
          "form": "카카오페이지는",
          "original_form": "카카오페이지는",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031199.1.20",
          "form": "아니예요 ㅎㅎ",
          "original_form": "아니예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031199.1.21",
          "form": "카카오톡에 있는건가요?",
          "original_form": "카카오톡에 있는건가요?",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031199.1.22",
          "form": "아뇨 따로 앱이 있어요",
          "original_form": "아뇨 따로 앱이 있어요",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031199.1.23",
          "form": "같은 카카오계열이긴하지만요",
          "original_form": "같은 카카오계열이긴하지만요",
          "speaker_id": "2",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031199.1.24",
          "form": "아아 그럼 카카오페이지는 웹소설만을 위한 앱인가요?",
          "original_form": "아아 그럼 카카오페이지는 웹소설만을 위한 앱인가요?",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031199.1.25",
          "form": "아니면 윕툰도?",
          "original_form": "아니면 윕툰도?",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031199.1.26",
          "form": "네 웹소설이랑 웹툰도 있어요",
          "original_form": "네 웹소설이랑 웹툰도 있어요",
          "speaker_id": "2",
          "time": "20210904 18:14"
        }
      ]
