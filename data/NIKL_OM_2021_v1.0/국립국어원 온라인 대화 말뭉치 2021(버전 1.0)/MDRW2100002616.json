      "utterance": [
        {
          "id": "MDRW2100002616.1.1",
          "form": "너 요즘 게임 하는거 있어?",
          "original_form": "너 요즘 게임 하는거 있어?",
          "speaker_id": "1",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002616.1.2",
          "form": "모바일게임은",
          "original_form": "모바일게임은",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002616.1.3",
          "form": "몬스터길들이기",
          "original_form": "몬스터길들이기",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002616.1.4",
          "form": "PC게임은 배틀그라운드나 이터널 리턴 정도?",
          "original_form": "PC게임은 배틀그라운드나 이터널 리턴 정도?",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002616.1.5",
          "form": "이제 새로 나온 슈퍼피플 정식 오픈하면 할까 생각중이기도 해",
          "original_form": "이제 새로 나온 슈퍼피플 정식 오픈하면 할까 생각중이기도 해",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002616.1.6",
          "form": "너도 슈퍼피플 소식 들었구나?",
          "original_form": "너도 슈퍼피플 소식 들었구나?",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002616.1.7",
          "form": "요즘 배그도 재미없고 질려서 찾아보고 있었는데",
          "original_form": "요즘 배그도 재미없고 질려서 찾아보고 있었는데",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002616.1.8",
          "form": "응응 유튜브로도 봤고 소식도 들었고",
          "original_form": "응응 유튜브로도 봤고 소식도 들었고",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002616.1.9",
          "form": "출시하면 같이 할래?",
          "original_form": "출시하면 같이 할래?",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002616.1.10",
          "form": "그래 좋지~",
          "original_form": "그래 좋지~",
          "speaker_id": "2",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002616.1.11",
          "form": "혹시 다른 게임 소식같은거 들은거 없어?",
          "original_form": "혹시 다른 게임 소식같은거 들은거 없어?",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002616.1.12",
          "form": "딱히? 못들은거같아",
          "original_form": "딱히? 못들은거같아",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002616.1.13",
          "form": "넷마블에서 뭐 신작나온다는",
          "original_form": "넷마블에서 뭐 신작나온다는",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002616.1.14",
          "form": "얘기는 들은거같은데",
          "original_form": "얘기는 들은거같은데",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002616.1.15",
          "form": "아 혹시 모바일 겜 말하는거야?",
          "original_form": "아 혹시 모바일 겜 말하는거야?",
          "speaker_id": "1",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002616.1.16",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002616.1.17",
          "form": "모바일게임인거같은데",
          "original_form": "모바일게임인거같은데",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002616.1.18",
          "form": "아 그리고 배그에서도 이번에 모바일 신작 나오는거같고",
          "original_form": "아 그리고 배그에서도 이번에 모바일 신작 나오는거같고",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002616.1.19",
          "form": "모배는 별로인거같은데 그건 재밌어보이더라고",
          "original_form": "모배는 별로인거같은데 그건 재밌어보이더라고",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002616.1.20",
          "form": "기존은 배그인데 추가를 음청 많이함",
          "original_form": "기존은 배그인데 추가를 음청 많이함",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002616.1.21",
          "form": "그것도 나오면 같이 해볼래?",
          "original_form": "그것도 나오면 같이 해볼래?",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002616.1.22",
          "form": "그래 그러자!",
          "original_form": "그래 그러자!",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002616.1.23",
          "form": "게임 재밌는게 없어서 맨날 심심해 했었거든",
          "original_form": "게임 재밌는게 없어서 맨날 심심해 했었거든",
          "speaker_id": "1",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002616.1.24",
          "form": "맞다 너 드래곤블러드라고 알아?",
          "original_form": "맞다 너 드래곤블러드라고 알아?",
          "speaker_id": "1",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002616.1.25",
          "form": "들어는봤어!",
          "original_form": "들어는봤어!",
          "speaker_id": "2",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002616.1.26",
          "form": "나 그거 뭐 시켜먹다가 쿠폰이 6장 왔더라고",
          "original_form": "나 그거 뭐 시켜먹다가 쿠폰이 6장 왔더라고",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002616.1.27",
          "form": "그래서 사전 예약 했었는데",
          "original_form": "그래서 사전 예약 했었는데",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002616.1.28",
          "form": "오 사전예약중이구나",
          "original_form": "오 사전예약중이구나",
          "speaker_id": "2",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002616.1.29",
          "form": "예약만하고 출시했는데 깔아보지도 않고 있어",
          "original_form": "예약만하고 출시했는데 깔아보지도 않고 있어",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002616.1.30",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002616.1.31",
          "form": "해볼 생각이면 쿠폰 줄게",
          "original_form": "해볼 생각이면 쿠폰 줄게",
          "speaker_id": "1",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002616.1.32",
          "form": "한번 찾아보고",
          "original_form": "한번 찾아보고",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002616.1.33",
          "form": "괜찮으면 따로 연락줄께~",
          "original_form": "괜찮으면 따로 연락줄께~",
          "speaker_id": "2",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002616.1.34",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210820 14:09"
        }
      ]
    }
