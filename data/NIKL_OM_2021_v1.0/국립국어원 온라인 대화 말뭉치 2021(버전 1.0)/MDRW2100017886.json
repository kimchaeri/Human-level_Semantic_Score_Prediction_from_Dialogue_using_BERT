      "utterance": [
        {
          "id": "MDRW2100017886.1.1",
          "form": "청각 예민하시다구요?!",
          "original_form": "청각 예민하시다구요?!",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017886.1.2",
          "form": "최근에 치과간적있나요",
          "original_form": "최근에 치과간적있나요",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017886.1.3",
          "form": "그럼 혹시 돌고래",
          "original_form": "그럼 혹시 돌고래",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017886.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017886.1.5",
          "form": "네 저 청각에 예민한데",
          "original_form": "네 저 청각에 예민한데",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017886.1.6",
          "form": "돌고래까진 아니고",
          "original_form": "돌고래까진 아니고",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017886.1.7",
          "form": "치과가서 뭐했어요",
          "original_form": "치과가서 뭐했어요",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017886.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017886.1.9",
          "form": "스케일링이여",
          "original_form": "스케일링이여",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017886.1.10",
          "form": "6개월마다 한 번 씩 가줘야한대요",
          "original_form": "6개월마다 한 번 씩 가줘야한대요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017886.1.11",
          "form": "치과 가셨나요",
          "original_form": "치과 가셨나요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017886.1.12",
          "form": "1년아니고요?",
          "original_form": "1년아니고요?",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017886.1.13",
          "form": "1년마다오라던데",
          "original_form": "1년마다오라던데",
          "speaker_id": "1",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017886.1.14",
          "form": "전 치아교정중이라 한달에 한번씩 간다는게 함정",
          "original_form": "전 치아교정중이라 한달에 한번씩 간다는게 함정",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017886.1.15",
          "form": "1년은 이제 건강검진 겸 보험되니까고",
          "original_form": "1년은 이제 건강검진 겸 보험되니까고",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017886.1.16",
          "form": "오 지금 치아교정중이세요?!!!",
          "original_form": "오 지금 치아교정중이세요?!!!",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017886.1.17",
          "form": "몇살때부터 하셖어요??",
          "original_form": "몇살때부터 하셖어요??",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017886.1.18",
          "form": "그렇습니다",
          "original_form": "그렇습니다",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017886.1.19",
          "form": "이제 2년되어가는거같은데",
          "original_form": "이제 2년되어가는거같은데",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017886.1.20",
          "form": "진행중이에요",
          "original_form": "진행중이에요",
          "speaker_id": "1",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017886.1.21",
          "form": "오 끝나가네여?",
          "original_form": "오 끝나가네여?",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017886.1.22",
          "form": "근데 유지장치를 평생 해야한다는 얘기 들으셨나여",
          "original_form": "근데 유지장치를 평생 해야한다는 얘기 들으셨나여",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017886.1.23",
          "form": "저는 못듣고",
          "original_form": "저는 못듣고",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017886.1.24",
          "form": "해가지고",
          "original_form": "해가지고",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017886.1.25",
          "form": "글쎄요.. 안끝날거같아.. 뭐가 잘 안맞는대요 연장삘..",
          "original_form": "글쎄요.. 안끝날거같아.. 뭐가 잘 안맞는대요 연장삘..",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017886.1.26",
          "form": "맞아요 유치장치 평생해야한다는데 그거 불편해서 할 수 있으려나 모르겠더라구요",
          "original_form": "맞아요 유치장치 평생해야한다는데 그거 불편해서 할 수 있으려나 모르겠더라구요",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017886.1.27",
          "form": "어때요",
          "original_form": "어때요",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017886.1.28",
          "form": "유치장치",
          "original_form": "유치장치",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017886.1.29",
          "form": "괜찮아요 근데 철사 빠지면 꾸준히ㅜ가야해서",
          "original_form": "괜찮아요 근데 철사 빠지면 꾸준히ㅜ가야해서",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017886.1.30",
          "form": "교정했던 곳 안망하길 바라고 있어요",
          "original_form": "교정했던 곳 안망하길 바라고 있어요",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017886.1.31",
          "form": "아 이거 나중에 시간지나면 벌어진다고 하긴 하더라구요",
          "original_form": "아 이거 나중에 시간지나면 벌어진다고 하긴 하더라구요",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017886.1.32",
          "form": "치아가 원래자리 찾아가려한다고",
          "original_form": "치아가 원래자리 찾아가려한다고",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017886.1.33",
          "form": "아니 비싼돈들여서 맞춰놨더니 왜 돌아가",
          "original_form": "아니 비싼돈들여서 맞춰놨더니 왜 돌아가",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017886.1.34",
          "form": "맞아요...하..",
          "original_form": "맞아요...하..",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017886.1.35",
          "form": "글고 잇몸이 망가지는거라",
          "original_form": "글고 잇몸이 망가지는거라",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017886.1.36",
          "form": "40대 되면 뭐 거의 임플란트 해야한다 그러더라고요",
          "original_form": "40대 되면 뭐 거의 임플란트 해야한다 그러더라고요",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017886.1.37",
          "form": "맞아요 솔직히 무서워..",
          "original_form": "맞아요 솔직히 무서워..",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017886.1.38",
          "form": "리얼..",
          "original_form": "리얼..",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017886.1.39",
          "form": "내 잇몸 버텨줘 100세시대",
          "original_form": "내 잇몸 버텨줘 100세시대",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017886.1.40",
          "form": "이빨 새로 나게 해줘...걍..",
          "original_form": "이빨 새로 나게 해줘...걍..",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017886.1.41",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210826 21:38"
        }
      ]
    }
