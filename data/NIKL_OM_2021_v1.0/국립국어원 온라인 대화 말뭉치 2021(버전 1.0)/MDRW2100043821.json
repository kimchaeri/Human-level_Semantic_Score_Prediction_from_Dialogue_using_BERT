      "utterance": [
        {
          "id": "MDRW2100043821.1.1",
          "form": "네 시간있을땐 자주 들어요",
          "original_form": "네 시간있을땐 자주 들어요",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043821.1.2",
          "form": "어떤장르주로들으세요?",
          "original_form": "어떤장르주로들으세요?",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043821.1.3",
          "form": "추천받은 노래 자주 들어요",
          "original_form": "추천받은 노래 자주 들어요",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043821.1.4",
          "form": "아 저도 하나 추천해주실 수 있을까요",
          "original_form": "아 저도 하나 추천해주실 수 있을까요",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043821.1.5",
          "form": "이무진의 신호등이요",
          "original_form": "이무진의 신호등이요",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043821.1.6",
          "form": "노래가 밝고 재밌더라구요",
          "original_form": "노래가 밝고 재밌더라구요",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043821.1.7",
          "form": "아 제목은 들어봤어요",
          "original_form": "아 제목은 들어봤어요",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043821.1.8",
          "form": "좋아하는 노래있으면 공연같은것도 가시는편인가요?",
          "original_form": "좋아하는 노래있으면 공연같은것도 가시는편인가요?",
          "speaker_id": "1",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100043821.1.9",
          "form": "아뇨ㅋㅋ 공연은 티켓도 사야되고 해서 힘들더라구요",
          "original_form": "아뇨ㅋㅋ 공연은 티켓도 사야되고 해서 힘들더라구요",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043821.1.10",
          "form": "맞아요 티켓팅이 어렵더라구요",
          "original_form": "맞아요 티켓팅이 어렵더라구요",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043821.1.11",
          "form": "남들은 어찌나 척척 잘하는지",
          "original_form": "남들은 어찌나 척척 잘하는지",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043821.1.12",
          "form": "방탄팬인 친구있는데 진짜 신기하더라구요ㅋㅋ",
          "original_form": "방탄팬인 친구있는데 진짜 신기하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043821.1.13",
          "form": "와 제가 방탄팬이면 전 포기했을듯요",
          "original_form": "와 제가 방탄팬이면 전 포기했을듯요",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043821.1.14",
          "form": "아님 암표사거나",
          "original_form": "아님 암표사거나",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043821.1.15",
          "form": "다른 티켓팅도 문제없이 잡더라구요ㅋㅋ",
          "original_form": "다른 티켓팅도 문제없이 잡더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043821.1.16",
          "form": "와 능력자네요",
          "original_form": "와 능력자네요",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043821.1.17",
          "form": "그런친구있으면 좋겠어요",
          "original_form": "그런친구있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100043821.1.18",
          "form": "네 다른 공연 보러가는 친구들이 자주 부탁하더라구요",
          "original_form": "네 다른 공연 보러가는 친구들이 자주 부탁하더라구요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043821.1.19",
          "form": "친구는 좀귀찮기도하겠네요ㅋㅋ",
          "original_form": "친구는 좀귀찮기도하겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:19"
        }
