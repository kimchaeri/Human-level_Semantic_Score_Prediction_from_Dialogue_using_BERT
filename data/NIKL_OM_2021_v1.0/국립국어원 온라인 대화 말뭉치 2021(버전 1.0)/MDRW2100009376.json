      "utterance": [
        {
          "id": "MDRW2100009376.1.1",
          "form": "온라인 쇼핑 많이하세요?",
          "original_form": "온라인 쇼핑 많이하세요?",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009376.1.2",
          "form": "넵 즐겨해요",
          "original_form": "넵 즐겨해요",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009376.1.3",
          "form": "당근과 쿠팡은 사랑이죠",
          "original_form": "당근과 쿠팡은 사랑이죠",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009376.1.4",
          "form": "요즘은 생필품도 인터넷이 훨저렴해서 많이하죠 ㅎㅎ",
          "original_form": "요즘은 생필품도 인터넷이 훨저렴해서 많이하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009376.1.5",
          "form": "넵ㅎ",
          "original_form": "넵ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009376.1.6",
          "form": "당근하세용? 전 아직 안해봤어요 ㅠㅠ",
          "original_form": "당근하세용? 전 아직 안해봤어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009376.1.7",
          "form": "자주해요ㅎ",
          "original_form": "자주해요ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009376.1.8",
          "form": "요즘엔 다 당근으로 통하더라구요 ㅋㅋ",
          "original_form": "요즘엔 다 당근으로 통하더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009376.1.9",
          "form": "제껏도 팔고 ㅎ",
          "original_form": "제껏도 팔고 ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009376.1.10",
          "form": "추석선물은 주로 뭐하세요?",
          "original_form": "추석선물은 주로 뭐하세요?",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009376.1.11",
          "form": "전 과일이나 홍삼류요",
          "original_form": "전 과일이나 홍삼류요",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009376.1.12",
          "form": "이번추석도 고민이네요 ㅠ",
          "original_form": "이번추석도 고민이네요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009376.1.13",
          "form": "저도요ㅠ 매년",
          "original_form": "저도요ㅠ 매년",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009376.1.14",
          "form": "안주고안받기 좋은데요ㅠ",
          "original_form": "안주고안받기 좋은데요ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009376.1.15",
          "form": "그러니깐요 .. 매번 같은거 주기도 뭐하고 ㅋㅋ",
          "original_form": "그러니깐요 .. 매번 같은거 주기도 뭐하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009376.1.16",
          "form": "ㅋ돈이 최고라는데 부담되구요",
          "original_form": "ㅋ돈이 최고라는데 부담되구요",
