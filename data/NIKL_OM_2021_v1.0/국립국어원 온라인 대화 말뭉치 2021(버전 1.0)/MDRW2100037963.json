      "utterance": [
        {
          "id": "MDRW2100037963.1.1",
          "form": "맞아요 사이즈가 없어요",
          "original_form": "맞아요 사이즈가 없어요",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100037963.1.2",
          "form": "언니 마스크 트러블 심하네요",
          "original_form": "언니 마스크 트러블 심하네요",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037963.1.3",
          "form": "그러이 ㅠ 사이즈만 ㅠ",
          "original_form": "그러이 ㅠ 사이즈만 ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037963.1.4",
          "form": "그런갑다",
          "original_form": "그런갑다",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037963.1.5",
          "form": "이마만 낫는데",
          "original_form": "이마만 낫는데",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037963.1.6",
          "form": "요즘 마스크 때문에 피부가 그렇게 많이 간데요",
          "original_form": "요즘 마스크 때문에 피부가 그렇게 많이 간데요",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037963.1.7",
          "form": "턱에도 자잘한게 나네",
          "original_form": "턱에도 자잘한게 나네",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037963.1.8",
          "form": "여기에서 들은 얘기ㅋㅌㅌ",
          "original_form": "여기에서 들은 얘기ㅋㅌㅌ",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037963.1.9",
          "form": "이게 큰것도 아니니까 잘 짜지지도 않고",
          "original_form": "이게 큰것도 아니니까 잘 짜지지도 않고",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037963.1.10",
          "form": "눈에\r\n잘 보이지도 않고",
          "original_form": "눈에\r\n잘 보이지도 않고",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037963.1.11",
          "form": "근데 오돌도 더라고",
          "original_form": "근데 오돌도 더라고",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037963.1.12",
          "form": "세수하고 크림바를때마다",
          "original_form": "세수하고 크림바를때마다",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037963.1.13",
          "form": "오돌토돌",
          "original_form": "오돌토돌",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100037963.1.14",
          "form": "언니만 아는 그 찝찝함",
          "original_form": "언니만 아는 그 찝찝함",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100037963.1.15",
          "form": "내손에만 느껴지는",
          "original_form": "내손에만 느껴지는",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100037963.1.16",
          "form": "어 그거 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어 그거 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100037963.1.17",
          "form": "뭔지 알 거 같아요",
          "original_form": "뭔지 알 거 같아요",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100037963.1.18",
          "form": "꼬치만 계속 그것만 보임",
          "original_form": "꼬치만 계속 그것만 보임",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100037963.1.19",
          "form": "꼬치 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "꼬치 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100037963.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100037963.1.21",
          "form": "똑바로 써줄래??",
          "original_form": "똑바로 써줄래??",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037963.1.22",
          "form": "꽂히면 이라고",
          "original_form": "꽂히면 이라고",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037963.1.23",
          "form": "턱도 레이저해야되나 ㅋㅋ",
          "original_form": "턱도 레이저해야되나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037963.1.24",
          "form": "발음 나는 대로",
          "original_form": "발음 나는 대로",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037963.1.25",
          "form": "저 마이 된 발음이다ㅋㅋㅋ",
          "original_form": "저 마이 된 발음이다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037963.1.26",
          "form": "턱은 조금도 낙 같네요",
          "original_form": "턱은 조금도 낙 같네요",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037963.1.27",
          "form": "돈 아깝네요",
          "original_form": "돈 아깝네요",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037963.1.28",
          "form": "조금도 낙 같다고?? ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "조금도 낙 같다고?? ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037963.1.29",
          "form": "그래 마스크쓰면  어차피 다시 날 거",
          "original_form": "그래 마스크쓰면  어차피 다시 날 거",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037963.1.30",
          "form": "그냥 마스크 쓰고 다녀야지",
          "original_form": "그냥 마스크 쓰고 다녀야지",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037963.1.31",
          "form": "그러니까요 마스크 벗어야",
          "original_form": "그러니까요 마스크 벗어야",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100037963.1.32",
          "form": "뭘 할 수 있어요",
          "original_form": "뭘 할 수 있어요",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100037963.1.33",
          "form": "마스크 쓰면 또 똑같음",
          "original_form": "마스크 쓰면 또 똑같음",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100037963.1.34",
          "form": "그러니",
          "original_form": "그러니",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100037963.1.35",
          "form": "응 견과류 좀 사야겠다",
          "original_form": "응 견과류 좀 사야겠다",
          "speaker_id": "1",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100037963.1.36",
          "form": "아침에 뭐라도 먹고 나와야겠어",
          "original_form": "아침에 뭐라도 먹고 나와야겠어",
          "speaker_id": "1",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100037963.1.37",
          "form": "맨날 빈속에 커피를 마시니",
          "original_form": "맨날 빈속에 커피를 마시니",
          "speaker_id": "1",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100037963.1.38",
          "form": "슬슬 건강이 걱정되네",
          "original_form": "슬슬 건강이 걱정되네",
          "speaker_id": "1",
          "time": "20210903 09:52"
        }
      ]
