      "utterance": [
        {
          "id": "MDRW2100046252.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20211016 00:08"
        },
        {
          "id": "MDRW2100046252.1.2",
          "form": "이제 점심 드시나요?",
          "original_form": "이제 점심 드시나요?",
          "speaker_id": "2",
          "time": "20211016 00:10"
        },
        {
          "id": "MDRW2100046252.1.3",
          "form": "네~ 콘푸로스트 먹고 있습니다ㅎㅎㅎ",
          "original_form": "네~ 콘푸로스트 먹고 있습니다ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20211016 00:10"
        },
        {
          "id": "MDRW2100046252.1.4",
          "form": "name2님은 점심 안 드세요?",
          "original_form": "&name2&님은 점심 안 드세요?",
          "speaker_id": "1",
          "time": "20211016 00:10"
        },
        {
          "id": "MDRW2100046252.1.5",
          "form": "헑 점심으로 시리얼인가요?",
          "original_form": "헑 점심으로 시리얼인가요?",
          "speaker_id": "2",
          "time": "20211016 00:10"
        },
        {
          "id": "MDRW2100046252.1.6",
          "form": "저도 곧 먹으러 갈 거예요! ",
          "original_form": "저도 곧 먹으러 갈 거예요! ",
          "speaker_id": "2",
          "time": "20211016 00:11"
        },
        {
          "id": "MDRW2100046252.1.7",
          "form": "네ㅎㅎㅎ 요리하거나 설거지하는 걸 너무 귀찮아해서",
          "original_form": "네ㅎㅎㅎ 요리하거나 설거지하는 걸 너무 귀찮아해서",
          "speaker_id": "1",
          "time": "20211016 00:12"
        },
        {
          "id": "MDRW2100046252.1.8",
          "form": "콘푸로스트 많이 먹어요ㅎㅎ",
          "original_form": "콘푸로스트 많이 먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20211016 00:12"
        },
        {
          "id": "MDRW2100046252.1.9",
          "form": "name2님은 뭐 드세요?",
          "original_form": "&name2&님은 뭐 드세요?",
          "speaker_id": "1",
          "time": "20211016 00:12"
        },
        {
          "id": "MDRW2100046252.1.10",
          "form": "간편식이네요 ! 전 아침에는 종종 먹는데 시리얼 점심까지는 못 먹겠어요 ㅋㅋ",
          "original_form": "간편식이네요 ! 전 아침에는 종종 먹는데 시리얼 점심까지는 못 먹겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211016 00:13"
        },
        {
          "id": "MDRW2100046252.1.11",
          "form": "평소에 도시락 싸오다가 오늘 늦잠 자서 오랜만에 사먹으러 나가야돼요 ㅠㅠ ",
          "original_form": "평소에 도시락 싸오다가 오늘 늦잠 자서 오랜만에 사먹으러 나가야돼요 ㅠㅠ ",
          "speaker_id": "2",
          "time": "20211016 00:13"
        },
        {
          "id": "MDRW2100046252.1.12",
          "form": "떠오르는 메뉴 하나 추천해주시겠어요 ?ㅋㅋ",
          "original_form": "떠오르는 메뉴 하나 추천해주시겠어요 ?ㅋㅋ",
          "speaker_id": "2",
          "time": "20211016 00:14"
        },
        {
          "id": "MDRW2100046252.1.13",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20211016 00:14"
        },
        {
          "id": "MDRW2100046252.1.14",
          "form": "다른 것보다",
          "original_form": "다른 것보다",
          "speaker_id": "1",
          "time": "20211016 00:14"
        },
        {
          "id": "MDRW2100046252.1.15",
          "form": "밥을 먹고 싶긴한데",
          "original_form": "밥을 먹고 싶긴한데",
          "speaker_id": "1",
          "time": "20211016 00:14"
        },
        {
          "id": "MDRW2100046252.1.16",
          "form": "거창한 거는 말고",
          "original_form": "거창한 거는 말고",
          "speaker_id": "1",
          "time": "20211016 00:15"
        },
        {
          "id": "MDRW2100046252.1.17",
          "form": "그냥 된장찌개나 불백 같은..",
          "original_form": "그냥 된장찌개나 불백 같은..",
          "speaker_id": "1",
          "time": "20211016 00:15"
        },
        {
          "id": "MDRW2100046252.1.18",
          "form": "집밥느낌",
          "original_form": "집밥느낌",
          "speaker_id": "1",
          "time": "20211016 00:15"
        },
        {
          "id": "MDRW2100046252.1.19",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20211016 00:15"
        },
        {
          "id": "MDRW2100046252.1.20",
          "form": "혹시 집밥이 그리우신가요 ㅠ ",
          "original_form": "혹시 집밥이 그리우신가요 ㅠ ",
          "speaker_id": "2",
          "time": "20211016 00:15"
        },
        {
          "id": "MDRW2100046252.1.21",
          "form": "그런게있으면 좋을 거 같아요",
          "original_form": "그런게있으면 좋을 거 같아요",
          "speaker_id": "1",
          "time": "20211016 00:15"
        },
        {
          "id": "MDRW2100046252.1.22",
          "form": "ㅎㅎㅎ 아녀.. ㅋㅋ 그립다기보다는..",
          "original_form": "ㅎㅎㅎ 아녀.. ㅋㅋ 그립다기보다는..",
          "speaker_id": "1",
          "time": "20211016 00:16"
        },
        {
          "id": "MDRW2100046252.1.23",
          "form": "그냥 먹고싶은걸요?ㅋㅋ",
          "original_form": "그냥 먹고싶은걸요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20211016 00:16"
        },
        {
          "id": "MDRW2100046252.1.24",
          "form": "배가고픈건가ㅋㅋ",
          "original_form": "배가고픈건가ㅋㅋ",
          "speaker_id": "1",
          "time": "20211016 00:16"
        },
        {
          "id": "MDRW2100046252.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211016 00:17"
        },
        {
          "id": "MDRW2100046252.1.26",
          "form": "역시 시리얼로는 부족한 거예요!",
          "original_form": "역시 시리얼로는 부족한 거예요!",
          "speaker_id": "2",
          "time": "20211016 00:17"
        },
        {
          "id": "MDRW2100046252.1.27",
          "form": "그럴지도 모르겠어요",
          "original_form": "그럴지도 모르겠어요",
          "speaker_id": "1",
          "time": "20211016 00:20"
        },
        {
          "id": "MDRW2100046252.1.28",
          "form": "저녁은 맛있는 거 먹어야겠어요",
          "original_form": "저녁은 맛있는 거 먹어야겠어요",
          "speaker_id": "1",
          "time": "20211016 00:20"
        },
        {
          "id": "MDRW2100046252.1.29",
          "form": "맞아요 저녁엔 꼭 맛있는거 드세요 ! ",
          "original_form": "맞아요 저녁엔 꼭 맛있는거 드세요 ! ",
