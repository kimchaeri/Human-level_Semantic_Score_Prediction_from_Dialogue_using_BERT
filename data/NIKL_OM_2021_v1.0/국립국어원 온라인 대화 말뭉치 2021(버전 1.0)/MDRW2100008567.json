      "utterance": [
        {
          "id": "MDRW2100008567.1.1",
          "form": "결혼했어요?",
          "original_form": "결혼했어요?",
          "speaker_id": "1",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008567.1.2",
          "form": "아니요! 저는 아직 대학생이에요",
          "original_form": "아니요! 저는 아직 대학생이에요",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100008567.1.3",
          "form": "학생 좋네여",
          "original_form": "학생 좋네여",
          "speaker_id": "1",
          "time": "20210822 16:46"
        },
        {
          "id": "MDRW2100008567.1.4",
          "form": "네 그리고 결혼 생각도 없어요",
          "original_form": "네 그리고 결혼 생각도 없어요",
          "speaker_id": "2",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008567.1.5",
          "form": "결혼 하셨나요?",
          "original_form": "결혼 하셨나요?",
          "speaker_id": "2",
          "time": "20210822 16:49"
        },
        {
          "id": "MDRW2100008567.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008567.1.7",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008567.1.8",
          "form": "저는 모솔이에요ㅋㅋ",
          "original_form": "저는 모솔이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008567.1.9",
          "form": "저도 모솔이었어요",
          "original_form": "저도 모솔이었어요",
          "speaker_id": "1",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008567.1.10",
          "form": "와 그럼 첫사랑이랑 결혼하신 거예요?",
          "original_form": "와 그럼 첫사랑이랑 결혼하신 거예요?",
          "speaker_id": "2",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008567.1.11",
          "form": "첫사랑은 아니죠",
          "original_form": "첫사랑은 아니죠",
          "speaker_id": "1",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008567.1.12",
          "form": "ㅋㅋㅋㅋ그럼 모솔이었다고 하시면 안돼죠ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ그럼 모솔이었다고 하시면 안돼죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008567.1.13",
          "form": "첫사랑은 짝사랑이었아오ㅠㅠ",
          "original_form": "첫사랑은 짝사랑이었아오ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008567.1.14",
          "form": "저만 힘든",
          "original_form": "저만 힘든",
          "speaker_id": "1",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008567.1.15",
          "form": "아ㅠㅠ 그러셨군요",
          "original_form": "아ㅠㅠ 그러셨군요",
          "speaker_id": "2",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008567.1.16",
          "form": "그래도 인연이 있더라구요",
          "original_form": "그래도 인연이 있더라구요",
          "speaker_id": "1",
