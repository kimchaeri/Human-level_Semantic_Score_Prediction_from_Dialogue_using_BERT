      "utterance": [
        {
          "id": "MDRW2100005108.1.1",
          "form": "저는 무교예요. 종교 있으세요?",
          "original_form": "저는 무교예요. 종교 있으세요?",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005108.1.2",
          "form": "저도 무교에요",
          "original_form": "저도 무교에요",
          "speaker_id": "1",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100005108.1.3",
          "form": "종교를 가져본적 었어요",
          "original_form": "종교를 가져본적 었어요",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005108.1.4",
          "form": "저도 어릴때 잠시 기독교",
          "original_form": "저도 어릴때 잠시 기독교",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005108.1.5",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005108.1.6",
          "form": "놀러갔어요",
          "original_form": "놀러갔어요",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005108.1.7",
          "form": "성경학교요 ㅎㅎ",
          "original_form": "성경학교요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005108.1.8",
          "form": "크리스마스랑요",
          "original_form": "크리스마스랑요",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005108.1.9",
          "form": "아 저는 그래도 몇 년 정도",
          "original_form": "아 저는 그래도 몇 년 정도",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005108.1.10",
          "form": "전 며칠씩요",
          "original_form": "전 며칠씩요",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005108.1.11",
          "form": "가끔 요새 답답하때 절에 구경가고 산채 비빔밥 먹으러 가요",
          "original_form": "가끔 요새 답답하때 절에 구경가고 산채 비빔밥 먹으러 가요",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005108.1.12",
          "form": "다양하게 가시네요ㅋ",
          "original_form": "다양하게 가시네요ㅋ",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005108.1.13",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005108.1.14",
          "form": "절은 불교를 믿지 않지만 산에 있러서 공기가 좋아서 가끔 가요",
          "original_form": "절은 불교를 믿지 않지만 산에 있러서 공기가 좋아서 가끔 가요",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005108.1.15",
          "form": "겨회는 일요일에 가서 싫었어요",
          "original_form": "겨회는 일요일에 가서 싫었어요",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005108.1.16",
          "form": "주위에 교회 다니는 사람들이 많아요",
          "original_form": "주위에 교회 다니는 사람들이 많아요",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005108.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005108.1.18",
          "form": "거의 다 다녀요 전 일요일을 양보 못해서 못가요",
          "original_form": "거의 다 다녀요 전 일요일을 양보 못해서 못가요",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005108.1.19",
          "form": "다시 안가세요?",
          "original_form": "다시 안가세요?",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005108.1.20",
          "form": "저도 게을러서 가기 싫어요",
          "original_form": "저도 게을러서 가기 싫어요",
          "speaker_id": "2",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005108.1.21",
          "form": "그렇죠 ㅋㅋ",
          "original_form": "그렇죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005108.1.22",
          "form": "그리고 코로나 감염이 심해서 사실 무서워요",
          "original_form": "그리고 코로나 감염이 심해서 사실 무서워요",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005108.1.23",
          "form": "교회 감염이 심하더라구요",
          "original_form": "교회 감염이 심하더라구요",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005108.1.24",
          "form": "너무 심하드라구요",
          "original_form": "너무 심하드라구요",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005108.1.25",
          "form": "그렇죠 코로나가 참 세상을 다 변하게 해요",
          "original_form": "그렇죠 코로나가 참 세상을 다 변하게 해요",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005108.1.26",
          "form": "성당도 좋아 보이긴 한데 믿진 않아요",
          "original_form": "성당도 좋아 보이긴 한데 믿진 않아요",
          "speaker_id": "1",
          "time": "20210821 21:06"
        }
      ]
    }
