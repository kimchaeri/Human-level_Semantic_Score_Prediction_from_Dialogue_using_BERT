      "utterance": [
        {
          "id": "MDRW2100030420.1.1",
          "form": "그분것도 패턴이 있죠",
          "original_form": "그분것도 패턴이 있죠",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030420.1.2",
          "form": "네 그래서 안본지 십년 넘은것 같아요",
          "original_form": "네 그래서 안본지 십년 넘은것 같아요",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030420.1.3",
          "form": "아 혹시 정유정 신작 보셨어요?",
          "original_form": "아 혹시 정유정 신작 보셨어요?",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030420.1.4",
          "form": "신작이라면 어떤?",
          "original_form": "신작이라면 어떤?",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030420.1.5",
          "form": "종의기원까진 읽었는데...",
          "original_form": "종의기원까진 읽었는데...",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030420.1.6",
          "form": "완전한 행복이요ㅎㅎ",
          "original_form": "완전한 행복이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030420.1.7",
          "form": "아직 못읽었어요",
          "original_form": "아직 못읽었어요",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030420.1.8",
          "form": "읽으셨어요?",
          "original_form": "읽으셨어요?",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030420.1.9",
          "form": "저도 아직..도서관에 늘 대출중이라ㅜㅜ",
          "original_form": "저도 아직..도서관에 늘 대출중이라ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030420.1.10",
          "form": "워낙 인기작가니까요.",
          "original_form": "워낙 인기작가니까요.",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030420.1.11",
          "form": "추리소설은 사기엔 좀 아깝더라구요",
          "original_form": "추리소설은 사기엔 좀 아깝더라구요",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030420.1.12",
          "form": "음... 그럴수있죠.",
          "original_form": "음... 그럴수있죠.",
          "speaker_id": "2",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030420.1.13",
          "form": "전 가벼운 에세이나 수필류가 그렇더라구요",
          "original_form": "전 가벼운 에세이나 수필류가 그렇더라구요",
          "speaker_id": "2",
          "time": "20210904 00:53"
        },
        {
          "id": "MDRW2100030420.1.14",
          "form": "전 오히려 에세이 중에 소장하고 싶은 책들이 간혹 있더라구요ㅎㅎ",
          "original_form": "전 오히려 에세이 중에 소장하고 싶은 책들이 간혹 있더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030420.1.15",
          "form": "에세이도 술술 읽혀서 좋아하는 편이라",
          "original_form": "에세이도 술술 읽혀서 좋아하는 편이라",
          "speaker_id": "1",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100030420.1.16",
          "form": "술술 읽히긴 하죠.",
          "original_form": "술술 읽히긴 하죠.",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100030420.1.17",
          "form": "아무튼 시리즈 보셨어요?",
          "original_form": "아무튼 시리즈 보셨어요?",
          "speaker_id": "2",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100030420.1.18",
          "form": "아니요 그게 뭔가요?",
          "original_form": "아니요 그게 뭔가요?",
          "speaker_id": "1",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100030420.1.19",
          "form": "에세이 시리즈인데 가볍게 술술 읽혀요",
          "original_form": "에세이 시리즈인데 가볍게 술술 읽혀요",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100030420.1.20",
          "form": "아무튼 술, 아무튼 예능, 아무튼 떡볶이...",
          "original_form": "아무튼 술, 아무튼 예능, 아무튼 떡볶이...",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030420.1.21",
          "form": "뭐 엄청 다양해요",
          "original_form": "뭐 엄청 다양해요",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100030420.1.22",
          "form": "맞네요 정말 다양한 작가가 참여했네요",
          "original_form": "맞네요 정말 다양한 작가가 참여했네요",
          "speaker_id": "1",
          "time": "20210904 00:58"
        }
      ]
