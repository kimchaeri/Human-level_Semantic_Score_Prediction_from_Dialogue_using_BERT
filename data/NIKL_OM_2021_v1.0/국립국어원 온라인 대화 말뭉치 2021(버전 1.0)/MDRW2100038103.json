      "utterance": [
        {
          "id": "MDRW2100038103.1.1",
          "form": "있어",
          "original_form": "있어",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100038103.1.2",
          "form": "나 완전 전문가야",
          "original_form": "나 완전 전문가야",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100038103.1.3",
          "form": "너는 있어?",
          "original_form": "너는 있어?",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100038103.1.4",
          "form": "같이 살 때 가사는 어떻게 분담했어?",
          "original_form": "같이 살 때 가사는 어떻게 분담했어?",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100038103.1.5",
          "form": "우리는 거의 개인공간은 알아서하고 공용공간은 번갈아가며 했어",
          "original_form": "우리는 거의 개인공간은 알아서하고 공용공간은 번갈아가며 했어",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.6",
          "form": "아니 나는 없어",
          "original_form": "아니 나는 없어",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.7",
          "form": "나는 완전 독고다이로 살아왔어",
          "original_form": "나는 완전 독고다이로 살아왔어",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.8",
          "form": "나는 더러운편인데 의외로 나보다 더러운 사람들이 꽤 있었어",
          "original_form": "나는 더러운편인데 의외로 나보다 더러운 사람들이 꽤 있었어",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.9",
          "form": "독고다이라니 멋있다",
          "original_form": "독고다이라니 멋있다",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.10",
          "form": "먼지와 공생한거랴?",
          "original_form": "먼지와 공생한거랴?",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.11",
          "form": "그럼 청소를 어떻게 나눠서했어?",
          "original_form": "그럼 청소를 어떻게 나눠서했어?",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.12",
          "form": "응 나는 먼지랑 룸메이트야",
          "original_form": "응 나는 먼지랑 룸메이트야",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.13",
          "form": "일주일에 한번 당번을 정했어",
          "original_form": "일주일에 한번 당번을 정했어",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.14",
          "form": "아아 그럼 룸메경험이 있네!",
          "original_form": "아아 그럼 룸메경험이 있네!",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.15",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.16",
          "form": "그럼 한명이 집 전체를 청소한거야?",
          "original_form": "그럼 한명이 집 전체를 청소한거야?",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100038103.1.17",
          "form": "응 근데 내가 심심해서 할 때도 많았어",
          "original_form": "응 근데 내가 심심해서 할 때도 많았어",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038103.1.18",
          "form": "ㅋㅋㅋㅋ착한 룸메이트였네",
          "original_form": "ㅋㅋㅋㅋ착한 룸메이트였네",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038103.1.19",
          "form": "룸메는 주로 화장실청소를 잘했어",
          "original_form": "룸메는 주로 화장실청소를 잘했어",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038103.1.20",
          "form": "음식은 보통 누가했어?",
          "original_form": "음식은 보통 누가했어?",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038103.1.21",
          "form": "나는 화장실청소가 싫어",
          "original_form": "나는 화장실청소가 싫어",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038103.1.22",
          "form": "음식은 반반.",
          "original_form": "음식은 반반.",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038103.1.23",
          "form": "나도 곧 손님이오는데",
          "original_form": "나도 곧 손님이오는데",
          "speaker_id": "1",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038103.1.24",
          "form": "나랑 친구가 먹고싶은게 달랐어",
          "original_form": "나랑 친구가 먹고싶은게 달랐어",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100038103.1.25",
          "form": "화장실청소가 하기싫어...",
          "original_form": "화장실청소가 하기싫어...",
          "speaker_id": "1",
          "time": "20210901 23:15"
        }
      ]
    }
  ]
