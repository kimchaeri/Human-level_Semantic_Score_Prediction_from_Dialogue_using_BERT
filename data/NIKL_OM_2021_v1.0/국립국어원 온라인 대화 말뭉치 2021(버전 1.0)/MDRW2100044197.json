      "utterance": [
        {
          "id": "MDRW2100044197.1.1",
          "form": "난 솔직히 그래서",
          "original_form": "난 솔직히 그래서",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044197.1.2",
          "form": "그닥 연애하고싶지 않음",
          "original_form": "그닥 연애하고싶지 않음",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044197.1.3",
          "form": "휴..결혼으로 넘어온 이야기",
          "original_form": "휴..결혼으로 넘어온 이야기",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044197.1.4",
          "form": "2233",
          "original_form": "2233",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044197.1.5",
          "form": "굳이.. 아까운 시간과 돈을 써가면서",
          "original_form": "굳이.. 아까운 시간과 돈을 써가면서",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044197.1.6",
          "form": "내 동생만한 남자를 만나가지고",
          "original_form": "내 동생만한 남자를 만나가지고",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044197.1.7",
          "form": "사귀어야할 이유가..?(?",
          "original_form": "사귀어야할 이유가..?(?",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044197.1.8",
          "form": "ㅇㅇ 진짜 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 진짜 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044197.1.9",
          "form": "내 동생만한 남자를 만나야 할 이유가..?",
          "original_form": "내 동생만한 남자를 만나야 할 이유가..?",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044197.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 완전...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 완전...",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044197.1.11",
          "form": "심지어 그 얘기는",
          "original_form": "심지어 그 얘기는",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044197.1.12",
          "form": "그 집에",
          "original_form": "그 집에",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044197.1.13",
          "form": "엄마같은 시어머니와",
          "original_form": "엄마같은 시어머니와",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044197.1.14",
          "form": "아빠같은 시아버지가 잇다는건데",
          "original_form": "아빠같은 시아버지가 잇다는건데",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044197.1.15",
          "form": "첨엠 멀쩡하게 보여도 알다보면 아닌거 같은 부분도 많을텐데",
          "original_form": "첨엠 멀쩡하게 보여도 알다보면 아닌거 같은 부분도 많을텐데",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044197.1.16",
          "form": "오 지저스",
          "original_form": "오 지저스",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044197.1.17",
          "form": "내가 내 부모도 벅찬데",
          "original_form": "내가 내 부모도 벅찬데",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044197.1.18",
          "form": "그치 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그치 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044197.1.19",
          "form": "남의 부모도 모시라고?",
          "original_form": "남의 부모도 모시라고?",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044197.1.20",
          "form": "싸우지도 못하고",
          "original_form": "싸우지도 못하고",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044197.1.21",
          "form": "고객님 대하듯?",
          "original_form": "고객님 대하듯?",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044197.1.22",
          "form": "안해",
          "original_form": "안해",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044197.1.23",
          "form": "ㅇㅇ ㅋㅋㅋㅋ 상상만 해도 숨막혀",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋ 상상만 해도 숨막혀",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044197.1.24",
          "form": "어휴..",
          "original_form": "어휴..",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044197.1.25",
          "form": "난 그냥 독거노인으로 살다가",
          "original_form": "난 그냥 독거노인으로 살다가",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044197.1.26",
          "form": "불쌍하게 고독사할게",
          "original_form": "불쌍하게 고독사할게",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044197.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ 솔직히 암",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 솔직히 암",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044197.1.28",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044197.1.29",
          "form": "나중에 나이들어서",
          "original_form": "나중에 나이들어서",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044197.1.30",
          "form": "딸 없으면 어차피 독거노인되더라...",
          "original_form": "딸 없으면 어차피 독거노인되더라...",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044197.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ 딸 있어도 독거노인될 수 있는..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 딸 있어도 독거노인될 수 있는..",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044197.1.32",
          "form": "내 무관심을 보여주지(급기야맨",
          "original_form": "내 무관심을 보여주지(급기야맨",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044197.1.33",
          "form": "글킨 해 ㅋㅋㅋㅋㅋ",
          "original_form": "글킨 해 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044197.1.34",
          "form": "그리고 사실",
          "original_form": "그리고 사실",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044197.1.35",
          "form": "걍 혼자서 살면서",
          "original_form": "걍 혼자서 살면서",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044197.1.36",
          "form": "내 스케쥴에 맞춰 회사다니고",
          "original_form": "내 스케쥴에 맞춰 회사다니고",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044197.1.37",
          "form": "퇴근하고 주3회 운동가고",
          "original_form": "퇴근하고 주3회 운동가고",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044197.1.38",
          "form": "주 2회 친구랑 만나고",
          "original_form": "주 2회 친구랑 만나고",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044197.1.39",
          "form": "주말엔 뒹굴거리는 삶이",
          "original_form": "주말엔 뒹굴거리는 삶이",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044197.1.40",
          "form": "훨씬 질 좋아보인",
          "original_form": "훨씬 질 좋아보인",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044197.1.41",
          "form": "보임",
          "original_form": "보임",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044197.1.42",
          "form": "걍 결혼 안하고 내 건강에 신경쓰면서 살래...",
