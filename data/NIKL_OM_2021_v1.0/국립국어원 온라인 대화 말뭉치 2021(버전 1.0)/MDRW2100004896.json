        {
          "id": "MDRW2100004896.1.1",
          "form": "넓게 바다 전망이 탁 트인 호텔 예약하고싶어",
          "original_form": "넓게 바다 전망이 탁 트인 호텔 예약하고싶어",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100004896.1.2",
          "form": "그리스",
          "original_form": "그리스",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100004896.1.3",
          "form": "산토리니",
          "original_form": "산토리니",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100004896.1.4",
          "form": "리조트",
          "original_form": "리조트",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100004896.1.5",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100004896.1.6",
          "form": "그..호텔여행을머라고부르죠??",
          "original_form": "그..호텔여행을머라고부르죠??",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100004896.1.7",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100004896.1.8",
          "form": "호텔여행하고싶어요...",
          "original_form": "호텔여행하고싶어요...",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100004896.1.9",
          "form": "호캉스요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "호캉스요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100004896.1.10",
          "form": "가혹한 필터링이네요..",
          "original_form": "가혹한 필터링이네요..",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100004896.1.11",
          "form": "호캉스 가고싶네요....",
          "original_form": "호캉스 가고싶네요....",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100004896.1.12",
          "form": "일단 누워서",
          "original_form": "일단 누워서",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100004896.1.13",
          "form": "마구먹고",
          "original_form": "마구먹고",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100004896.1.14",
          "form": "물에 누워있고싶어요",
          "original_form": "물에 누워있고싶어요",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100004896.1.15",
          "form": "햇빛이 은은하게 내리고",
          "original_form": "햇빛이 은은하게 내리고",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100004896.1.16",
          "form": "나무그늘이 살랑거리는",
          "original_form": "나무그늘이 살랑거리는",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100004896.1.17",
          "form": "수영장..",
          "original_form": "수영장..",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100004896.1.18",
          "form": "하...반짝거리는물...",
          "original_form": "하...반짝거리는물...",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100004896.1.19",
          "form": "해파리처럼떠다니고싶어요",
          "original_form": "해파리처럼떠다니고싶어요",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100004896.1.20",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100004896.1.21",
          "form": "저랑 댕댕이 한마리도 같이",
          "original_form": "저랑 댕댕이 한마리도 같이",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100004896.1.22",
          "form": "둥둥 누워있어야함",
          "original_form": "둥둥 누워있어야함",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100004896.1.23",
          "form": "악어까지 ok",
          "original_form": "악어까지 ok",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100004896.1.24",
          "form": "예...?",
          "original_form": "예...?",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100004896.1.25",
          "form": "그건..그건...좋지만...좋지만...",
          "original_form": "그건..그건...좋지만...좋지만...",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100004896.1.26",
          "form": "그건...",
          "original_form": "그건...",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100004896.1.27",
          "form": "그래요.....",
          "original_form": "그래요.....",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100004896.1.28",
          "form": "악어밥이되는걸 즐기시나요?",
          "original_form": "악어밥이되는걸 즐기시나요?",
          "speaker_id": "2",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100004896.1.29",
          "form": "갱얼쥐도 고영이도 입질 하는데요",
          "original_form": "갱얼쥐도 고영이도 입질 하는데요",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004896.1.30",
          "form": "악어 차별하시나요?",
          "original_form": "악어 차별하시나요?",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004896.1.31",
          "form": "제 반려악어한테 사.과.하세요",
          "original_form": "제 반려악어한테 사.과.하세요",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004896.1.32",
          "form": "일단 반려악어부터 주세요",
          "original_form": "일단 반려악어부터 주세요",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100004896.1.33",
          "form": "늪지대로 여행가셔야겠네요....",
          "original_form": "늪지대로 여행가셔야겠네요....",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100004896.1.34",
          "form": "저기요 햇빛이 은은하게 내리고 나무그늘이 살랑거리는 곳은",
          "original_form": "저기요 햇빛이 은은하게 내리고 나무그늘이 살랑거리는 곳은",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100004896.1.35",
          "form": "아무리봐도 늪지대는 아니죠",
          "original_form": "아무리봐도 늪지대는 아니죠",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100004896.1.36",
          "form": "햇빛이은은한 늪지대",
          "original_form": "햇빛이은은한 늪지대",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100004896.1.37",
          "form": "거긴 뙤악볕이 내리쬐고 나뭇잎에 살 베이잖아요",
          "original_form": "거긴 뙤악볕이 내리쬐고 나뭇잎에 살 베이잖아요",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004896.1.38",
          "form": "비슷하잖아요",
          "original_form": "비슷하잖아요",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004896.1.39",
          "form": "이 친구 안되겠네",
          "original_form": "이 친구 안되겠네",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004896.1.40",
          "form": "뙤악볕이라니 웃기네요",
          "original_form": "뙤악볕이라니 웃기네요",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004896.1.41",
          "form": "비속어 갖고와",
          "original_form": "비속어 갖고와",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004896.1.42",
          "form": "뙤악볕",
          "original_form": "뙤악볕",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004896.1.43",
          "form": "살인볕",
          "original_form": "살인볕",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004896.1.44",
          "form": "킄킄",
          "original_form": "킄킄",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004896.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004896.1.46",
          "form": "암살여행",
          "original_form": "암살여행",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004896.1.47",
          "form": "호텔여행",
          "original_form": "호텔여행",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004896.1.48",
          "form": "걍 출장이잖아...",
          "original_form": "걍 출장이잖아...",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004896.1.49",
          "form": "호캉스갈기고싶어",
          "original_form": "호캉스갈기고싶어",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004896.1.50",
          "form": "출장.",
          "original_form": "출장.",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004896.1.51",
          "form": "호캉스",
          "original_form": "호캉스",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004896.1.52",
          "form": "호캉스~",
          "original_form": "호캉스~",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004896.1.53",
          "form": "저 호텔 한번도안가봣어요",
          "original_form": "저 호텔 한번도안가봣어요",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004896.1.54",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004896.1.55",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004896.1.56",
          "form": "생각해보니 저도요",
          "original_form": "생각해보니 저도요",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004896.1.57",
          "form": "그러고보니 유스호스텔이나",
          "original_form": "그러고보니 유스호스텔이나",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100004896.1.58",
          "form": "레지던스?",
          "original_form": "레지던스?",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100004896.1.59",
          "form": "여..관, 민.박은 가봤는데",
          "original_form": "여..관, 민.박은 가봤는데",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100004896.1.60",
          "form": "호텔은 함도 안가봤네요",
          "original_form": "호텔은 함도 안가봤네요",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100004896.1.61",
          "form": "미지의영역이다",
          "original_form": "미지의영역이다",
          "speaker_id": "1",
          "time": "20210821 21:17"
        }
      ]
