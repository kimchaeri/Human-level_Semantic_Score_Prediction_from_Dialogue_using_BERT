      "utterance": [
        {
          "id": "MDRW2100007978.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007978.1.2",
          "form": "좋아하는 게임 있으신가요?",
          "original_form": "좋아하는 게임 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007978.1.3",
          "form": "제가 아는 게임은 테일즈런너.. 카트라이더.. 크레이지아케이드 같은 거 뿐이라.. ㅎㅎ",
          "original_form": "제가 아는 게임은 테일즈런너.. 카트라이더.. 크레이지아케이드 같은 거 뿐이라.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100007978.1.4",
          "form": "와 카트라이더 크레이지아케이드",
          "original_form": "와 카트라이더 크레이지아케이드",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100007978.1.5",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100007978.1.6",
          "form": "저는 거기에 더해서 동물농장",
          "original_form": "저는 거기에 더해서 동물농장",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100007978.1.7",
          "form": "슈게임",
          "original_form": "슈게임",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100007978.1.8",
          "form": "아 쥬니어 네이버 꿀잼이였죠",
          "original_form": "아 쥬니어 네이버 꿀잼이였죠",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100007978.1.9",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100007978.1.10",
          "form": "예전에 플레시게임 이런게 참 재밌얼는데",
          "original_form": "예전에 플레시게임 이런게 참 재밌얼는데",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100007978.1.11",
          "form": "혹시 야후꾸러기도 하셨나요",
          "original_form": "혹시 야후꾸러기도 하셨나요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100007978.1.12",
          "form": "요즘 게임들은 너무 정신사나워서 못하겠어요.. ㅎㅎ",
          "original_form": "요즘 게임들은 너무 정신사나워서 못하겠어요.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100007978.1.13",
          "form": "아르피나였나 그것두 잼있었는데",
          "original_form": "아르피나였나 그것두 잼있었는데",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100007978.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100007978.1.15",
          "form": "와 당근이죠 야후 꾸러기가 저의 첫 게임",
          "original_form": "와 당근이죠 야후 꾸러기가 저의 첫 게임",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100007978.1.16",
          "form": "그쳐 다들 거기서부터 시작하는것같아요",
          "original_form": "그쳐 다들 거기서부터 시작하는것같아요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100007978.1.17",
          "form": "그거랑 거기서 처음 포밍뿌였나? 그게 거기 있었나요?",
          "original_form": "그거랑 거기서 처음 포밍뿌였나? 그게 거기 있었나요?",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100007978.1.18",
          "form": "포밍뿌? 그건 처음들어요",
          "original_form": "포밍뿌? 그건 처음들어요",
          "speaker_id": "1",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100007978.1.19",
          "form": "그것도 엄청 재밌게 하고 무슨 학습 게임인제 레몬에이드 만들기 그것도 엄청 많이 햤는데 이름이 기억이 안나네요",
          "original_form": "그것도 엄청 재밌게 하고 무슨 학습 게임인제 레몬에이드 만들기 그것도 엄청 많이 햤는데 이름이 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100007978.1.20",
          "form": "검색해보니 알피게임이라고 하네요",
          "original_form": "검색해보니 알피게임이라고 하네요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100007978.1.21",
          "form": "오.....",
          "original_form": "오.....",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100007978.1.22",
          "form": "저는 그 컴퓨터시간에 피카츄 배구게임",
          "original_form": "저는 그 컴퓨터시간에 피카츄 배구게임",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100007978.1.23",
          "form": "그것도 기억나요",
          "original_form": "그것도 기억나요",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100007978.1.24",
          "form": "바탕화면 부서보기..? 그런것도 했던것같아요",
          "original_form": "바탕화면 부서보기..? 그런것도 했던것같아요",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100007978.1.25",
          "form": "앜ㅋㅋㅋㅋ 맞아요 그것도 진짜 많이했죠 그땐 졸라맨하는 게임도 많았었는데 추억이에요",
          "original_form": "앜ㅋㅋㅋㅋ 맞아요 그것도 진짜 많이했죠 그땐 졸라맨하는 게임도 많았었는데 추억이에요",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100007978.1.26",
          "form": "\r\n혹시 이 링크 들어가 지나요? 지니게임이라고 아세요? ㅎㅎ",
          "original_form": "{share:url}\r\n혹시 이 링크 들어가 지나요? 지니게임이라고 아세요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100007978.1.27",
          "form": "링크가 복사가 안되요..ㅠㅠㅠ",
          "original_form": "링크가 복사가 안되요..ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100007978.1.28",
          "form": "아 안들어가지네요.. 지니게임이라고 이것도 어릴때 엄청 했어요 지금은 싹 바뀌어있더라구요 ㅜㅜ",
          "original_form": "아 안들어가지네요.. 지니게임이라고 이것도 어릴때 엄청 했어요 지금은 싹 바뀌어있더라구요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100007978.1.29",
          "form": "앜ㅋㅋㅋㅋㅋ 지니키즈 대박",
          "original_form": "앜ㅋㅋㅋㅋㅋ 지니키즈 대박",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100007978.1.30",
          "form": "이름 첨 알았어요",
          "original_form": "이름 첨 알았어요",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100007978.1.31",
          "form": "정말 추억의 게임들이에요 ㅎㅎ",
          "original_form": "정말 추억의 게임들이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100007978.1.32",
          "form": "그 이누야샤? 게임도 있었는데",
          "original_form": "그 이누야샤? 게임도 있었는데",
          "speaker_id": "1",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100007978.1.33",
          "form": "맨날 어떻게 플레이하는지 잘몰라서 대충하고",
          "original_form": "맨날 어떻게 플레이하는지 잘몰라서 대충하고",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100007978.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 어릴땐 그냥 옆에서 하는 거 따라하기만 했죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 어릴땐 그냥 옆에서 하는 거 따라하기만 했죠",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100007978.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100007978.1.36",
          "form": "슈 라면끓이기 게임은 성공한적이 없는것같아요",
          "original_form": "슈 라면끓이기 게임은 성공한적이 없는것같아요",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100007978.1.37",
          "form": "와.. 저는 그거랑 고향만두 게임.. 뭐 만들기란 만들기는 다 어려웠어요",
          "original_form": "와.. 저는 그거랑 고향만두 게임.. 뭐 만들기란 만들기는 다 어려웠어요",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100007978.1.38",
          "form": "그니까요! 아니 애들게임을 왤케 어렵게 만들었는지",
          "original_form": "그니까요! 아니 애들게임을 왤케 어렵게 만들었는지",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100007978.1.39",
          "form": "그림만 샤랄라 해서 예뼜지",
          "original_form": "그림만 샤랄라 해서 예뼜지",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100007978.1.40",
          "form": "난이도는 너무 어려웠어요",
          "original_form": "난이도는 너무 어려웠어요",
