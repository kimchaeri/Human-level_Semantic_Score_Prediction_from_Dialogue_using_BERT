      "utterance": [
        {
          "id": "MDRW2100041160.1.1",
          "form": "감겻구만ㅋㅋㅋㅋ",
          "original_form": "감겻구만ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100041160.1.2",
          "form": "ㅋㅋ오늘은 딘딘 덜 얄미움",
          "original_form": "ㅋㅋ오늘은 딘딘 덜 얄미움",
          "speaker_id": "1",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100041160.1.3",
          "form": "윙? 왠일로?",
          "original_form": "윙? 왠일로?",
          "speaker_id": "2",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100041160.1.4",
          "form": "1박 봐?",
          "original_form": "1박 봐?",
          "speaker_id": "2",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041160.1.5",
          "form": "뭔내용인데?",
          "original_form": "뭔내용인데?",
          "speaker_id": "2",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041160.1.6",
          "form": "김선호가 이김ㅋㅋ",
          "original_form": "김선호가 이김ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:56"
        },
        {
          "id": "MDRW2100041160.1.7",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100041160.1.8",
          "form": "김선호가 밥먹나요?",
          "original_form": "김선호가 밥먹나요?",
          "speaker_id": "2",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100041160.1.9",
          "form": "혼자 먹네요",
          "original_form": "혼자 먹네요",
          "speaker_id": "1",
          "time": "20210905 18:58"
        },
        {
          "id": "MDRW2100041160.1.10",
          "form": "근데 안얄밉네여ㅋㅋ",
          "original_form": "근데 안얄밉네여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:58"
        },
        {
          "id": "MDRW2100041160.1.11",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210905 18:59"
        },
        {
          "id": "MDRW2100041160.1.12",
          "form": "왜 혼자먹어?",
          "original_form": "왜 혼자먹어?",
          "speaker_id": "2",
          "time": "20210905 18:59"
        },
        {
          "id": "MDRW2100041160.1.13",
          "form": "ㅋㅋ 딘딘이 이겼음 얄미웠을턴더",
          "original_form": "ㅋㅋ 딘딘이 이겼음 얄미웠을턴더",
          "speaker_id": "1",
          "time": "20210905 18:59"
        },
        {
          "id": "MDRW2100041160.1.14",
          "form": "그렇군ㅋㅋㅋㅋ",
          "original_form": "그렇군ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:00"
        },
        {
          "id": "MDRW2100041160.1.15",
          "form": "끝까지 혼자 먹어?",
          "original_form": "끝까지 혼자 먹어?",
          "speaker_id": "2",
          "time": "20210905 19:02"
        },
        {
          "id": "MDRW2100041160.1.16",
          "form": "나눠먹어!",
          "original_form": "나눠먹어!",
          "speaker_id": "1",
          "time": "20210905 19:04"
        },
        {
          "id": "MDRW2100041160.1.17",
          "form": "시루떡 부럽",
          "original_form": "시루떡 부럽",
          "speaker_id": "1",
          "time": "20210905 19:04"
        },
        {
          "id": "MDRW2100041160.1.18",
          "form": "엄마께 물어봐",
          "original_form": "엄마께 물어봐",
          "speaker_id": "2",
          "time": "20210905 19:05"
        },
        {
          "id": "MDRW2100041160.1.19",
          "form": "(ว ̇ ̇)ง",
          "original_form": "(ว ̇{emoji:∇} ̇)ง",
          "speaker_id": "2",
          "time": "20210905 19:05"
        },
        {
          "id": "MDRW2100041160.1.20",
          "form": "호감덩어리 선호ㅋㅋ",
          "original_form": "호감덩어리 선호ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:07"
        },
        {
          "id": "MDRW2100041160.1.21",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210905 19:08"
        },
        {
          "id": "MDRW2100041160.1.22",
          "form": "뭐했는데..?",
          "original_form": "뭐했는데..?",
          "speaker_id": "2",
          "time": "20210905 19:08"
        },
        {
          "id": "MDRW2100041160.1.23",
          "form": "재환이 버리고 김선호로 갈아탐?",
          "original_form": "재환이 버리고 김선호로 갈아탐?",
          "speaker_id": "2",
          "time": "20210905 19:08"
        },
        {
          "id": "MDRW2100041160.1.24",
          "form": "그거 아니죠!",
          "original_form": "그거 아니죠!",
          "speaker_id": "1",
          "time": "20210905 19:09"
        },
        {
          "id": "MDRW2100041160.1.25",
          "form": "예능과 가수는 달라!",
          "original_form": "예능과 가수는 달라!",
          "speaker_id": "1",
          "time": "20210905 19:09"
        },
        {
          "id": "MDRW2100041160.1.26",
          "form": "일박이일 안에서 제일 낫다는거죠",
