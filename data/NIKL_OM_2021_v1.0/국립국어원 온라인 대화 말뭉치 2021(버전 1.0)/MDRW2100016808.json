        {
          "id": "MDRW2100016808.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100016808.1.2",
          "form": "안녕하세요~ 스포츠 좋아하세요??",
          "original_form": "안녕하세요~ 스포츠 좋아하세요??",
          "speaker_id": "2",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016808.1.3",
          "form": "그닥 좋아하진 않는데",
          "original_form": "그닥 좋아하진 않는데",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016808.1.4",
          "form": "올림픽때문에 관심은 많이 있어요!",
          "original_form": "올림픽때문에 관심은 많이 있어요!",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016808.1.5",
          "form": "아 그래요??",
          "original_form": "아 그래요??",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016808.1.6",
          "form": "어떤 종목에 가장 관심이 많이 가시나요??",
          "original_form": "어떤 종목에 가장 관심이 많이 가시나요??",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016808.1.7",
          "form": "저는 양궁이요!!",
          "original_form": "저는 양궁이요!!",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016808.1.8",
          "form": "진짜 멋있더라구요 ㅜㅜ",
          "original_form": "진짜 멋있더라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016808.1.9",
          "form": "아 맞아요 엄청 잘하고ㅎㅎ",
          "original_form": "아 맞아요 엄청 잘하고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016808.1.10",
          "form": "양궁카페 가고싶은데",
          "original_form": "양궁카페 가고싶은데",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016808.1.11",
          "form": "코로나라서 좀 꺼려져요ㅜㅜ",
          "original_form": "코로나라서 좀 꺼려져요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016808.1.12",
          "form": "아 좀 그렇죠..ㅠㅠ",
          "original_form": "아 좀 그렇죠..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016808.1.13",
          "form": "저희 빕 근처에도 양궁카페 있는데 코로나때문에 쉬는것같더라구요ㅠㅠ",
          "original_form": "저희 빕 근처에도 양궁카페 있는데 코로나때문에 쉬는것같더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016808.1.14",
          "form": "그쵸ㅜㅜ 참 요즘 시기가 시기라.. name2님은 스포츠 좋아하세요?",
          "original_form": "그쵸ㅜㅜ 참 요즘 시기가 시기라.. &name2&님은 스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016808.1.15",
          "form": "전 보는거 좋아해요ㅎㅎ",
          "original_form": "전 보는거 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016808.1.16",
          "form": "요즘엔 이스포츠 주로 봐요",
          "original_form": "요즘엔 이스포츠 주로 봐요",
          "speaker_id": "2",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016808.1.17",
          "form": "오 롤챔 이런거 보시는건가요??",
          "original_form": "오 롤챔 이런거 보시는건가요??",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016808.1.18",
          "form": "저는 페이커 한명 알아요ㅋㅋㅋ",
          "original_form": "저는 페이커 한명 알아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016808.1.19",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016808.1.20",
          "form": "저도 롤 보기 전에는 페이커 한 명 알았어요ㅋㅋ",
          "original_form": "저도 롤 보기 전에는 페이커 한 명 알았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016808.1.21",
          "form": "그분 인터뷰봤는데",
          "original_form": "그분 인터뷰봤는데",
          "speaker_id": "1",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016808.1.22",
          "form": "되게어린데 멘탈이 대단하시더라구요",
          "original_form": "되게어린데 멘탈이 대단하시더라구요",
          "speaker_id": "1",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016808.1.23",
          "form": "멋있는거같아요ㅎㅎ",
          "original_form": "멋있는거같아요ㅎㅎ",
