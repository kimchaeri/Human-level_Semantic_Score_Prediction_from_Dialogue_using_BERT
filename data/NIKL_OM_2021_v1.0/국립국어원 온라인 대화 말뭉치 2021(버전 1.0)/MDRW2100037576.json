      "utterance": [
        {
          "id": "MDRW2100037576.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100037576.1.2",
          "form": "네 부대찌개 먹었어요",
          "original_form": "네 부대찌개 먹었어요",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100037576.1.3",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100037576.1.4",
          "form": "전 김치찌개요",
          "original_form": "전 김치찌개요",
          "speaker_id": "2",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100037576.1.5",
          "form": "김치찌개요!!",
          "original_form": "김치찌개요!!",
          "speaker_id": "2",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100037576.1.6",
          "form": "반찬은 별로였어요",
          "original_form": "반찬은 별로였어요",
          "speaker_id": "2",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100037576.1.7",
          "form": "안 좋아하는거라도 나왔나요?",
          "original_form": "안 좋아하는거라도 나왔나요?",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100037576.1.8",
          "form": "그건 아닌데",
          "original_form": "그건 아닌데",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100037576.1.9",
          "form": "맛이 ㅠㅠㅠ",
          "original_form": "맛이 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100037576.1.10",
          "form": "앗ㅠㅠ 김치찌개는 맛있었나요?",
          "original_form": "앗ㅠㅠ 김치찌개는 맛있었나요?",
          "speaker_id": "1",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100037576.1.11",
          "form": "네.. 조금짜서",
          "original_form": "네.. 조금짜서",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100037576.1.12",
          "form": "물을 넣었지만요ㅋㅋ",
          "original_form": "물을 넣었지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100037576.1.13",
          "form": "전 배달이 오거든요!",
          "original_form": "전 배달이 오거든요!",
          "speaker_id": "2",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100037576.1.14",
          "form": "ㅋㅋ왠만한 건 다 배달 되서 좋은거 같아요.",
          "original_form": "ㅋㅋ왠만한 건 다 배달 되서 좋은거 같아요.",
          "speaker_id": "1",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100037576.1.15",
          "form": "저번엔 닭갈비도 배달해먹었어요.",
          "original_form": "저번엔 닭갈비도 배달해먹었어요.",
          "speaker_id": "1",
          "time": "20210901 15:32"
        },
        {
          "id": "MDRW2100037576.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100037576.1.17",
          "form": "만들어 먹는 거 보가",
          "original_form": "만들어 먹는 거 보가",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100037576.1.18",
          "form": "배달이 최고죠",
          "original_form": "배달이 최고죠",
          "speaker_id": "2",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100037576.1.19",
          "form": "그쵸. 어지간하면 다 맛있구요.",
          "original_form": "그쵸. 어지간하면 다 맛있구요.",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037576.1.20",
          "form": "자주 먹는 메뉴 있어요?",
          "original_form": "자주 먹는 메뉴 있어요?",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037576.1.21",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037576.1.22",
          "form": "근데 너무 비싸요 ㅠ",
          "original_form": "근데 너무 비싸요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037576.1.23",
          "form": "저 다양하게요!!",
          "original_form": "저 다양하게요!!",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037576.1.24",
          "form": "맞아요 그래도 주말엔 시켜먹게 되요",
          "original_form": "맞아요 그래도 주말엔 시켜먹게 되요",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037576.1.25",
          "form": "정말요",
          "original_form": "정말요",
          "speaker_id": "2",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037576.1.26",
          "form": "그리고 여름에요",
          "original_form": "그리고 여름에요",
          "speaker_id": "2",
          "time": "20210901 17:03"
        }
      ]
