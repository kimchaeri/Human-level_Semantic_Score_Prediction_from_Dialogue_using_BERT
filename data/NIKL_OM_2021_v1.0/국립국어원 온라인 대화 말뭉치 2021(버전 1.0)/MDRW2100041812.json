        },
        {
          "id": "MDRW2100041812.1.2",
          "form": "요즘에는 오빠 일찍 퇴근한다",
          "original_form": "요즘에는 오빠 일찍 퇴근한다",
          "speaker_id": "2",
          "time": "20210906 16:27"
        },
        {
          "id": "MDRW2100041812.1.3",
          "form": "오 다행이네 ㅎㅎ",
