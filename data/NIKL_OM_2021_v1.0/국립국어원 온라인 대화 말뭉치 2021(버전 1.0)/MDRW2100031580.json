      "utterance": [
        {
          "id": "MDRW2100031580.1.1",
          "form": "순대좋아해?",
          "original_form": "순대좋아해?",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100031580.1.2",
          "form": "좋아하는 음식있어?",
          "original_form": "좋아하는 음식있어?",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100031580.1.3",
          "form": "ㅋㅋㅋ응",
          "original_form": "ㅋㅋㅋ응",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100031580.1.4",
          "form": "떡순튀 진리지",
          "original_form": "떡순튀 진리지",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100031580.1.5",
          "form": "ㅋㅋㅋㅋㅋ나도 순대 좋아해!!",
          "original_form": "ㅋㅋㅋㅋㅋ나도 순대 좋아해!!",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100031580.1.6",
          "form": "찹쌀순대가 제일 좋고 내장은 간만 취급하지만...!",
          "original_form": "찹쌀순대가 제일 좋고 내장은 간만 취급하지만...!",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100031580.1.7",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100031580.1.8",
          "form": "맞아 내장은 은근 호불호 갈리지",
          "original_form": "맞아 내장은 은근 호불호 갈리지",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100031580.1.9",
          "form": "나는 다 먹긴 해",
          "original_form": "나는 다 먹긴 해",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100031580.1.10",
          "form": "어떤 부위가 제일 좋아?",
          "original_form": "어떤 부위가 제일 좋아?",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031580.1.11",
          "form": "내 친구는 허파 좋아하던데!",
          "original_form": "내 친구는 허파 좋아하던데!",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031580.1.12",
          "form": "나도 허파!",
          "original_form": "나도 허파!",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031580.1.13",
          "form": "폭신하잖아",
          "original_form": "폭신하잖아",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031580.1.14",
          "form": "역시 허파가 메이저구만",
          "original_form": "역시 허파가 메이저구만",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031580.1.15",
          "form": "그리고 나 좋아하는 음식은 타코!!",
          "original_form": "그리고 나 좋아하는 음식은 타코!!",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031580.1.16",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031580.1.17",
          "form": "나 얼마전에 타코먹고왔는데",
          "original_form": "나 얼마전에 타코먹고왔는데",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031580.1.18",
          "form": "ㅋㅋㅋㅋ동질감",
          "original_form": "ㅋㅋㅋㅋ동질감",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031580.1.19",
          "form": "타코 좋아해....?(두근)",
          "original_form": "타코 좋아해....?(두근)",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100031580.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100031580.1.21",
          "form": "당연!!",
          "original_form": "당연!!",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100031580.1.22",
          "form": "근데 근처에 잘없어서 잘 못먹어",
          "original_form": "근데 근처에 잘없어서 잘 못먹어",
          "speaker_id": "2",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031580.1.23",
          "form": "마저 집근처에는 잘 안팔지 ㅠㅠ 나도 약속있을 때나 친구 꼬셔서 먹으러가",
          "original_form": "마저 집근처에는 잘 안팔지 ㅠㅠ 나도 약속있을 때나 친구 꼬셔서 먹으러가",
          "speaker_id": "1",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100031580.1.24",
          "form": "고수도 좋아해?",
          "original_form": "고수도 좋아해?",
          "speaker_id": "1",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100031580.1.25",
          "form": "응! 고수 잘먹어ㅋㅋㅋ",
          "original_form": "응! 고수 잘먹어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100031580.1.26",
          "form": "난 걍 다 잘먹어",
          "original_form": "난 걍 다 잘먹어",
          "speaker_id": "2",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100031580.1.27",
          "form": "짱부러워!!! 나는 사실 고수는 다 빼고 먹어....트랜스 타코러버...",
          "original_form": "짱부러워!!! 나는 사실 고수는 다 빼고 먹어....트랜스 타코러버...",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100031580.1.28",
          "form": "베트남쌀국수도 좋아하고 타코도 좋아하지만 현지의 맛은 절대 느낄 수 없지",
          "original_form": "베트남쌀국수도 좋아하고 타코도 좋아하지만 현지의 맛은 절대 느낄 수 없지",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100031580.1.29",
          "form": "ㅋㅋㅋㅋ 뭐 고수는 워낙 호불호 갈리잖아",
          "original_form": "ㅋㅋㅋㅋ 뭐 고수는 워낙 호불호 갈리잖아",
          "speaker_id": "2",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100031580.1.30",
          "form": "예전에 쌀국수집 알바 했었는데 고수 싫어하던 애들중에서 나중엔 잘 먹는 애들도 있더라고! 그래서 가끔씩 도잔해보고있어 고수 넣고 먹기...",
          "original_form": "예전에 쌀국수집 알바 했었는데 고수 싫어하던 애들중에서 나중엔 잘 먹는 애들도 있더라고! 그래서 가끔씩 도잔해보고있어 고수 넣고 먹기...",
          "speaker_id": "1",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100031580.1.31",
          "form": "지금은 그냥 고수빼고 양파나 팍팍 넣는 게 좋아",
          "original_form": "지금은 그냥 고수빼고 양파나 팍팍 넣는 게 좋아",
          "speaker_id": "1",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100031580.1.32",
          "form": "ㅋㅋㅋㅋ근데 그게 유전적으로 고수먹으면 비누맛? 나는사람들이 있대",
          "original_form": "ㅋㅋㅋㅋ근데 그게 유전적으로 고수먹으면 비누맛? 나는사람들이 있대",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100031580.1.33",
          "form": "안좋아하면 그냥 안먹는게 좋은거같아",
          "original_form": "안좋아하면 그냥 안먹는게 좋은거같아",
          "speaker_id": "2",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100031580.1.34",
          "form": "마저 약간 그런 맛 느껴져 ㅠㅠㅠ\r\n그래 좋아하는 거나 많이 먹어야겠어 ㅠㅠ",
          "original_form": "마저 약간 그런 맛 느껴져 ㅠㅠㅠ\r\n그래 좋아하는 거나 많이 먹어야겠어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031580.1.35",
          "form": "앗 그럼 너는 그런 유전자??가 있나봐",
          "original_form": "앗 그럼 너는 그런 유전자??가 있나봐",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031580.1.36",
          "form": "그랬던 건가봐ㅋㅋㅋㅋ",
          "original_form": "그랬던 건가봐ㅋㅋㅋㅋ",
