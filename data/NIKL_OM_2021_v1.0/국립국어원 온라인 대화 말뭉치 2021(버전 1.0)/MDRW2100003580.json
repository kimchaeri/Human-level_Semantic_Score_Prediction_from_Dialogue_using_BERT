      "utterance": [
        {
          "id": "MDRW2100003580.1.1",
          "form": "최근에 구매하신 물건 있으세요?",
          "original_form": "최근에 구매하신 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003580.1.2",
          "form": "주요 먹는거 사요ㅎ",
          "original_form": "주요 먹는거 사요ㅎ",
          "speaker_id": "1",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003580.1.3",
          "form": "저는 인터넷 쇼핑 많이 해요",
          "original_form": "저는 인터넷 쇼핑 많이 해요",
          "speaker_id": "2",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003580.1.4",
          "form": "엊그제 정원샵에서 곱창이랑 불막창샀네요",
          "original_form": "엊그제 정원샵에서 곱창이랑 불막창샀네요",
          "speaker_id": "1",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003580.1.5",
          "form": "주로 뭐쇼핑하세요?",
          "original_form": "주로 뭐쇼핑하세요?",
          "speaker_id": "1",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003580.1.6",
          "form": "모든 생필품을 인터넷에서 쇼핑하는 편이에요",
          "original_form": "모든 생필품을 인터넷에서 쇼핑하는 편이에요",
          "speaker_id": "2",
          "time": "20210821 02:38"
        },
        {
          "id": "MDRW2100003580.1.7",
          "form": "얼마 전에는 스마트폰 거치대 구매했어요",
          "original_form": "얼마 전에는 스마트폰 거치대 구매했어요",
          "speaker_id": "2",
          "time": "20210821 02:39"
        },
        {
          "id": "MDRW2100003580.1.8",
          "form": "저도요 인터넷이 쿠폰도주고 집까지 갖다주니 편하고 좋아요",
          "original_form": "저도요 인터넷이 쿠폰도주고 집까지 갖다주니 편하고 좋아요",
          "speaker_id": "1",
          "time": "20210821 02:39"
        },
        {
          "id": "MDRW2100003580.1.9",
          "form": "전 그러고보니 마스크도 샀네요",
          "original_form": "전 그러고보니 마스크도 샀네요",
          "speaker_id": "1",
          "time": "20210821 02:40"
        },
        {
          "id": "MDRW2100003580.1.10",
          "form": "아 마스크 필수죠ㅎㅎ",
          "original_form": "아 마스크 필수죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 02:41"
        },
        {
          "id": "MDRW2100003580.1.11",
          "form": "11번가서 특가떠서 94 100매 8900원에샀어요",
          "original_form": "11번가서 특가떠서 94 100매 8900원에샀어요",
          "speaker_id": "1",
          "time": "20210821 02:41"
        },
        {
          "id": "MDRW2100003580.1.12",
          "form": "와 한창 대란 날 때보다 가격 확 내려갔네요",
          "original_form": "와 한창 대란 날 때보다 가격 확 내려갔네요",
          "speaker_id": "2",
          "time": "20210821 02:42"
        },
        {
          "id": "MDRW2100003580.1.13",
          "form": "4겹이라 겨울에쓰기 괜찮을거같아요",
          "original_form": "4겹이라 겨울에쓰기 괜찮을거같아요",
          "speaker_id": "1",
          "time": "20210821 02:42"
        },
        {
          "id": "MDRW2100003580.1.14",
          "form": "원가격은 23000원인가 그렇더라구요",
          "original_form": "원가격은 23000원인가 그렇더라구요",
          "speaker_id": "1",
          "time": "20210821 02:42"
        },
        {
          "id": "MDRW2100003580.1.15",
          "form": "핫딜뜬거라 5000개한정",
          "original_form": "핫딜뜬거라 5000개한정",
          "speaker_id": "1",
          "time": "20210821 02:42"
        },
        {
          "id": "MDRW2100003580.1.16",
          "form": "저도 11번가에서 쿠폰 받아서 자주 쇼핑해요",
          "original_form": "저도 11번가에서 쿠폰 받아서 자주 쇼핑해요",
          "speaker_id": "2",
          "time": "20210821 02:44"
        },
        {
          "id": "MDRW2100003580.1.17",
          "form": "지마켓이랑 11번가가 좋은거같아요",
          "original_form": "지마켓이랑 11번가가 좋은거같아요",
          "speaker_id": "1",
          "time": "20210821 02:44"
        },
        {
          "id": "MDRW2100003580.1.18",
          "form": "인터넷 쇼핑이 확실히 저렴한 것 같아요",
          "original_form": "인터넷 쇼핑이 확실히 저렴한 것 같아요",
          "speaker_id": "2",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003580.1.19",
          "form": "요즘 당근도 많이 하던데 해보신 적 있나요?",
          "original_form": "요즘 당근도 많이 하던데 해보신 적 있나요?",
