      "utterance": [
        {
          "id": "MDRW2100013117.1.1",
          "form": "너무 바쁜 하루지만 카레 먹었습니다ㅋ",
          "original_form": "너무 바쁜 하루지만 카레 먹었습니다ㅋ",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013117.1.2",
          "form": "채소만 넣으니 담백하더라고요",
          "original_form": "채소만 넣으니 담백하더라고요",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013117.1.3",
          "form": "직접해드셨어요?",
          "original_form": "직접해드셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013117.1.4",
          "form": "아뇨식당에서ㅋㅋㅋ",
          "original_form": "아뇨식당에서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013117.1.5",
          "form": "자주 해드세요?",
          "original_form": "자주 해드세요?",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013117.1.6",
          "form": "그냥 가끔 해먹어요 ㅎ",
          "original_form": "그냥 가끔 해먹어요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013117.1.7",
          "form": "주로 간단하게 고기를 구워먹죠",
          "original_form": "주로 간단하게 고기를 구워먹죠",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013117.1.8",
          "form": "소가 좋으세요 돼지가 좋으세요?ㅋㅋ",
          "original_form": "소가 좋으세요 돼지가 좋으세요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013117.1.9",
          "form": "둘다요ㅎ 소고기를더좋아하는듯",
          "original_form": "둘다요ㅎ 소고기를더좋아하는듯",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013117.1.10",
          "form": "음 질문이 역시 잘못됐어요ㅋㅋ",
          "original_form": "음 질문이 역시 잘못됐어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013117.1.11",
          "form": "비싼 소고기 비싼 돼지고기가 좋은데ㅋㅋ",
          "original_form": "비싼 소고기 비싼 돼지고기가 좋은데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013117.1.12",
          "form": "ㅋㅋ한우 투뿔ㅎ",
          "original_form": "ㅋㅋ한우 투뿔ㅎ",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013117.1.13",
          "form": "투뿔은ㅋㅋ 거의 못먹어봤네요ㅋ",
          "original_form": "투뿔은ㅋㅋ 거의 못먹어봤네요ㅋ",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013117.1.14",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013117.1.15",
          "form": "누가사줄질 않아서ㅡㅡ",
          "original_form": "누가사줄질 않아서ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100013117.1.16",
          "form": "역시 비싼 음식은 법카죠ㅋㅋ",
          "original_form": "역시 비싼 음식은 법카죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013117.1.17",
          "form": "저녁엔 뭐드시나요",
          "original_form": "저녁엔 뭐드시나요",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013117.1.18",
          "form": "고건 아직 미정이요ㅎ아직 퇴근시간많이 남아서",
          "original_form": "고건 아직 미정이요ㅎ아직 퇴근시간많이 남아서",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100013117.1.19",
          "form": "메뉴정하셨어요?",
          "original_form": "메뉴정하셨어요?",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100013117.1.20",
          "form": "저도요.",
          "original_form": "저도요.",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100013117.1.21",
          "form": "퇴근이 늦으시나봐요",
          "original_form": "퇴근이 늦으시나봐요",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
