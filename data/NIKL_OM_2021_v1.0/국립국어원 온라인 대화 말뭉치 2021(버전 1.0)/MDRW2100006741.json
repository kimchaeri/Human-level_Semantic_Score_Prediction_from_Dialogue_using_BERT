        },
        {
          "id": "MDRW2100006741.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100006741.1.3",
          "form": "혹시 지금 계신곳은 날씨가 어떠신가요??",
          "original_form": "혹시 지금 계신곳은 날씨가 어떠신가요??",
          "speaker_id": "1",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006741.1.4",
          "form": "흐린 편이에요",
          "original_form": "흐린 편이에요",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006741.1.5",
          "form": "그곳은요?",
          "original_form": "그곳은요?",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006741.1.6",
          "form": "어제는 계속 비오고 그랬는데",
          "original_form": "어제는 계속 비오고 그랬는데",
          "speaker_id": "1",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006741.1.7",
          "form": "오늘은 좀 맑은 거 같네요",
          "original_form": "오늘은 좀 맑은 거 같네요",
          "speaker_id": "1",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006741.1.8",
          "form": "그래도 혹시 몰라 우산은 챙겼습니다",
          "original_form": "그래도 혹시 몰라 우산은 챙겼습니다",
          "speaker_id": "1",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006741.1.9",
          "form": "오 맑아요?",
          "original_form": "오 맑아요?",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006741.1.10",
          "form": "도로 더워졋나요?",
          "original_form": "도로 더워졋나요?",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006741.1.11",
          "form": "별로 덥진 않은 거 같아요",
          "original_form": "별로 덥진 않은 거 같아요",
          "speaker_id": "1",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100006741.1.12",
          "form": "이제 진짜 가을이 왓나보네요",
          "original_form": "이제 진짜 가을이 왓나보네요",
          "speaker_id": "2",
          "time": "20210908 09:45"
        },
        {
          "id": "MDRW2100006741.1.13",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "1",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100006741.1.14",
          "form": "여름 끝나서 너무 좋아요",
          "original_form": "여름 끝나서 너무 좋아요",
          "speaker_id": "1",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100006741.1.15",
          "form": "우허님은 가을 제일 좋아하세요?",
          "original_form": "우허님은 가을 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006741.1.16",
          "form": "여름 빼고는 다 좋아해요 ㅋㅋㅋ",
          "original_form": "여름 빼고는 다 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100006741.1.17",
          "form": "저는 겨울이 젤 좋은거 같아요",
          "original_form": "저는 겨울이 젤 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100006741.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ앗",
          "original_form": "ㅋㅋㅋㅋㅋㅋ앗",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100006741.1.19",
          "form": "저는 겨울이 제일 싫...ㅋㅋㅋㅋㅋ",
          "original_form": "저는 겨울이 제일 싫...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100006741.1.20",
          "form": "헉 그러시구나",
          "original_form": "헉 그러시구나",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100006741.1.21",
          "form": "혹시 겨울의 어떤 점이 싫으신거에요??",
          "original_form": "혹시 겨울의 어떤 점이 싫으신거에요??",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100006741.1.22",
          "form": "추위를 많이타요 ㅋㅋㅋㅋ",
          "original_form": "추위를 많이타요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100006741.1.23",
          "form": "아 저는 반대로 더위를 많이 타서...",
          "original_form": "아 저는 반대로 더위를 많이 타서...",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100006741.1.24",
          "form": "땀 진짜 많이 흘려요...",
          "original_form": "땀 진짜 많이 흘려요...",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100006741.1.25",
          "form": "반대네요 ㅋㅋㅋㅋ",
          "original_form": "반대네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
