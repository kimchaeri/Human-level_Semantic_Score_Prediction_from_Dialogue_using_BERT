      "utterance": [
        {
          "id": "MDRW2100013256.1.1",
          "form": "넌 안성탕면에 계란 넣는거 조음?",
          "original_form": "넌 안성탕면에 계란 넣는거 조음?",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.2",
          "form": "나는 완전 좋아",
          "original_form": "나는 완전 좋아",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.3",
          "form": "나도나도",
          "original_form": "나도나도",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.4",
          "form": "진짜사랑하지",
          "original_form": "진짜사랑하지",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.5",
          "form": "근데 반숙은 좀",
          "original_form": "근데 반숙은 좀",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.6",
          "form": "나도 반숙은 싫다",
          "original_form": "나도 반숙은 싫다",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.7",
          "form": "그냥 오나숙",
          "original_form": "그냥 오나숙",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.8",
          "form": "완숙만 좋아",
          "original_form": "완숙만 좋아",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.9",
          "form": "글고 너구리에는 노노",
          "original_form": "글고 너구리에는 노노",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.10",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.11",
          "form": "너구리도 싫어",
          "original_form": "너구리도 싫어",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.12",
          "form": "왜넣는지 모르겠어",
          "original_form": "왜넣는지 모르겠어",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.13",
          "form": "짜파게티는 걍 ..",
          "original_form": "짜파게티는 걍 ..",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.14",
          "form": "악최악아다",
          "original_form": "악최악아다",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.15",
          "form": "최악이다",
          "original_form": "최악이다",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.16",
          "form": "ㅋㅋㅋㅋㅋ그치",
          "original_form": "ㅋㅋㅋㅋㅋ그치",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100013256.1.17",
          "form": "역시 기본이 제일맛나!",
          "original_form": "역시 기본이 제일맛나!",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.18",
          "form": "마잡",
          "original_form": "마잡",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.19",
          "form": "아 왜근데 자꾸 배고프게해",
          "original_form": "아 왜근데 자꾸 배고프게해",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.20",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.21",
          "form": "오늘 점심은",
          "original_form": "오늘 점심은",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.22",
          "form": "맛없엇어?",
          "original_form": "맛없엇어?",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.23",
          "form": "그저그랬어ㅜ",
          "original_form": "그저그랬어ㅜ",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.24",
          "form": "걍 항상 비슷했어",
          "original_form": "걍 항상 비슷했어",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.25",
          "form": "아휴",
          "original_form": "아휴",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.26",
          "form": "우리학교 급식이 그랫지뭐",
          "original_form": "우리학교 급식이 그랫지뭐",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.27",
          "form": "이제 졸업이다 그래도",
          "original_form": "이제 졸업이다 그래도",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.28",
          "form": "그래도 나중엔",
          "original_form": "그래도 나중엔",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.29",
          "form": "급식이 그리울지두",
          "original_form": "급식이 그리울지두",
          "speaker_id": "2",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.30",
          "form": "나도드러드라",
          "original_form": "나도드러드라",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013256.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100013256.1.32",
          "form": "나도럴까?",
          "original_form": "나도럴까?",
          "speaker_id": "2",
          "time": "20210825 13:46"
        }
      ]
    }
