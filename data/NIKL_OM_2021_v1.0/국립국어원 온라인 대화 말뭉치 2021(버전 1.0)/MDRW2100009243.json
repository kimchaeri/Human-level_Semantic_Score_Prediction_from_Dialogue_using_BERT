      "utterance": [
        {
          "id": "MDRW2100009243.1.1",
          "form": "날이 시원하네요",
          "original_form": "날이 시원하네요",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009243.1.2",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009243.1.3",
          "form": "오늘도 비 많이 올 줄 알았는데",
          "original_form": "오늘도 비 많이 올 줄 알았는데",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009243.1.4",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009243.1.5",
          "form": "태풍이라고 하던데",
          "original_form": "태풍이라고 하던데",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009243.1.6",
          "form": "네 근데 내일 태풍 오는 것 같았어요",
          "original_form": "네 근데 내일 태풍 오는 것 같았어요",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009243.1.7",
          "form": "오긴 오는 군요",
          "original_form": "오긴 오는 군요",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009243.1.8",
          "form": "네 근데 그것도 남해쪽이라서",
          "original_form": "네 근데 그것도 남해쪽이라서",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009243.1.9",
          "form": "남해분들은 걱정이 많이 될 것 같아요",
          "original_form": "남해분들은 걱정이 많이 될 것 같아요",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009243.1.10",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009243.1.11",
          "form": "네 얼마 전에도 피해입었을텐데",
          "original_form": "네 얼마 전에도 피해입었을텐데",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009243.1.12",
          "form": "네 무사해야 할텐데요...",
          "original_form": "네 무사해야 할텐데요...",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009243.1.13",
          "form": "위쪽은 시원해서 그저 좋네요",
          "original_form": "위쪽은 시원해서 그저 좋네요",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009243.1.14",
          "form": "네 저희동네도 요즘 딱 자기 좋아요",
          "original_form": "네 저희동네도 요즘 딱 자기 좋아요",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009243.1.15",
          "form": "맨날 선풍기 틀어놓고 이불덮고 자는 ㅋㅋ",
          "original_form": "맨날 선풍기 틀어놓고 이불덮고 자는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009243.1.16",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009243.1.17",
          "form": "약간 에어컨 틀고 이불 덮은 느낌 ㅎㄹ",
          "original_form": "약간 에어컨 틀고 이불 덮은 느낌 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009243.1.18",
          "form": "네 맞아요 ㅋㅋㅋ",
          "original_form": "네 맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009243.1.19",
          "form": "사치부리는 느낌?? ㅋㅋㅋㅋ",
          "original_form": "사치부리는 느낌?? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009243.1.20",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009243.1.21",
          "form": "곧 추워지면 이것도 못하겠지만요",
          "original_form": "곧 추워지면 이것도 못하겠지만요",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009243.1.22",
          "form": "네.. 그래도 추워지면 그것 나름대로도 좋은 것 같아요",
          "original_form": "네.. 그래도 추워지면 그것 나름대로도 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009243.1.23",
          "form": "지금 비 정말 많이 오네요",
          "original_form": "지금 비 정말 많이 오네요",
