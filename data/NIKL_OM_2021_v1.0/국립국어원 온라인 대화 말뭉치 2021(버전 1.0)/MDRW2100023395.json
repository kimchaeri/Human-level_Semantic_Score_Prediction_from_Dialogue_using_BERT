      "utterance": [
        {
          "id": "MDRW2100023395.1.1",
          "form": "name1때문에ㅎㅎ 감사해요ㅎㅎ",
          "original_form": "&name1&때문에ㅎㅎ 감사해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023395.1.2",
          "form": "결혼해서 행복할줄알았어요ㅎ 정말ㅎㅎ",
          "original_form": "결혼해서 행복할줄알았어요ㅎ 정말ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023395.1.3",
          "form": "감사는요",
          "original_form": "감사는요",
          "speaker_id": "1",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023395.1.4",
          "form": "수다쟁이 이야기들어주셔서ㅎㅎ 감사하죠ㄹ",
          "original_form": "수다쟁이 이야기들어주셔서ㅎㅎ 감사하죠ㄹ",
          "speaker_id": "2",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023395.1.5",
          "form": "그쵸 ㅎㅎㅎ 내맘같지 않은게 현실",
          "original_form": "그쵸 ㅎㅎㅎ 내맘같지 않은게 현실",
          "speaker_id": "1",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023395.1.6",
          "form": "저도 name2덕에 수다쟁이 됨요",
          "original_form": "저도 &name2&덕에 수다쟁이 됨요",
          "speaker_id": "1",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023395.1.7",
          "form": "ㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100023395.1.8",
          "form": "일할때 거의 말안하거든요",
          "original_form": "일할때 거의 말안하거든요",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023395.1.9",
          "form": "수다쟁이가 안좋은데 ㅎ",
          "original_form": "수다쟁이가 안좋은데 ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023395.1.10",
          "form": "저두요ㅎ",
          "original_form": "저두요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023395.1.11",
          "form": "전 배나갈때 잔소리하고 그때빼곤 회사에서 완전 합죽이에요ㅎ",
          "original_form": "전 배나갈때 잔소리하고 그때빼곤 회사에서 완전 합죽이에요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023395.1.12",
          "form": "감정소비가 싫어서 ㅎㅎㅎ",
          "original_form": "감정소비가 싫어서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023395.1.13",
          "form": "가끔 누가 뭐시키면 ㅎ",
          "original_form": "가끔 누가 뭐시키면 ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023395.1.14",
          "form": "안되요ㅎ 그건그렇게하는거아니에요ㅎ",
          "original_form": "안되요ㅎ 그건그렇게하는거아니에요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023395.1.15",
          "form": "뭐이러거나ㅎ 아니면 거의말안해요ㅎ",
          "original_form": "뭐이러거나ㅎ 아니면 거의말안해요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023395.1.16",
          "form": "미투여",
          "original_form": "미투여",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100023395.1.17",
          "form": "name1때문에 그래도 많이 이야기하는거애요ㅎ",
          "original_form": "&name1&때문에 그래도 많이 이야기하는거애요ㅎ",
