      "utterance": [
        {
          "id": "MDRW2100003561.1.1",
          "form": "늦은시간인데 안주무세요?",
          "original_form": "늦은시간인데 안주무세요?",
          "speaker_id": "1",
          "time": "20210821 01:25"
        },
        {
          "id": "MDRW2100003561.1.2",
          "form": "네 아직요",
          "original_form": "네 아직요",
          "speaker_id": "2",
          "time": "20210821 01:26"
        },
        {
          "id": "MDRW2100003561.1.3",
          "form": "안 피곤하세요?",
          "original_form": "안 피곤하세요?",
          "speaker_id": "1",
          "time": "20210821 01:27"
        },
        {
          "id": "MDRW2100003561.1.4",
          "form": "전 야행성이라 늦게자거든요",
          "original_form": "전 야행성이라 늦게자거든요",
          "speaker_id": "1",
          "time": "20210821 01:27"
        },
        {
          "id": "MDRW2100003561.1.5",
          "form": "오늘 토욜일자나요 ㅅㅎ",
          "original_form": "오늘 토욜일자나요 ㅅㅎ",
          "speaker_id": "2",
          "time": "20210821 01:30"
        },
        {
          "id": "MDRW2100003561.1.6",
          "form": "저도 그래서 오늘은 늦게자려구요",
          "original_form": "저도 그래서 오늘은 늦게자려구요",
          "speaker_id": "1",
          "time": "20210821 01:32"
        },
        {
          "id": "MDRW2100003561.1.7",
          "form": "요즘 운동 하시는거 있으세요?",
          "original_form": "요즘 운동 하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210821 01:32"
        },
        {
          "id": "MDRW2100003561.1.8",
          "form": "걷디 하는데 더워서 못하다 이제좀해요 ㅎㅎ",
          "original_form": "걷디 하는데 더워서 못하다 이제좀해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 01:38"
        },
        {
          "id": "MDRW2100003561.1.9",
          "form": "걷기운동 건강에 좋다더라구요",
          "original_form": "걷기운동 건강에 좋다더라구요",
          "speaker_id": "1",
          "time": "20210821 01:38"
        },
        {
          "id": "MDRW2100003561.1.10",
          "form": "ㅎㅎ 근데 귀니차니즘이 ㅜㅜㅋ",
          "original_form": "ㅎㅎ 근데 귀니차니즘이 ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210821 01:41"
        },
        {
          "id": "MDRW2100003561.1.11",
          "form": "저는 혼자 운동은 의지가 약해서 못하겠어요",
          "original_form": "저는 혼자 운동은 의지가 약해서 못하겠어요",
          "speaker_id": "1",
          "time": "20210821 01:42"
        },
        {
          "id": "MDRW2100003561.1.12",
          "form": "누구랑 가팅 하는게 최고인데",
          "original_form": "누구랑 가팅 하는게 최고인데",
          "speaker_id": "2",
          "time": "20210821 01:42"
        },
        {
          "id": "MDRW2100003561.1.13",
          "form": "아님 돈들여 배우던지요ㅎ",
          "original_form": "아님 돈들여 배우던지요ㅎ",
          "speaker_id": "1",
          "time": "20210821 01:43"
        },
        {
          "id": "MDRW2100003561.1.14",
          "form": "ㅎㅎ 그것도 좋죠",
          "original_form": "ㅎㅎ 그것도 좋죠",
          "speaker_id": "2",
          "time": "20210821 01:43"
        },
        {
          "id": "MDRW2100003561.1.15",
          "form": "줌바하고싶은데 코로나땜에 못가고있네요",
          "original_form": "줌바하고싶은데 코로나땜에 못가고있네요",
