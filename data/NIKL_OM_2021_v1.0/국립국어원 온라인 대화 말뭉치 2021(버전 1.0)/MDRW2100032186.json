      "utterance": [
        {
          "id": "MDRW2100032186.1.1",
          "form": "병장,,,?",
          "original_form": "병장,,,?",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032186.1.2",
          "form": "나에게 쥐어지는 계급,,,?",
          "original_form": "나에게 쥐어지는 계급,,,?",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032186.1.3",
          "form": "그렇디 말입니다",
          "original_form": "그렇디 말입니다",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032186.1.4",
          "form": "병장님이 병장님이지 말입니다",
          "original_form": "병장님이 병장님이지 말입니다",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032186.1.5",
          "form": "넌 이등병이야?",
          "original_form": "넌 이등병이야?",
          "speaker_id": "1",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032186.1.6",
          "form": "이등병 같은 쑥맥 까까중이 좋으신 겁니까???",
          "original_form": "이등병 같은 쑥맥 까까중이 좋으신 겁니까???",
          "speaker_id": "2",
          "time": "20210905 13:55"
        },
        {
          "id": "MDRW2100032186.1.7",
          "form": "그럼 갓 입대한 병아리병장 컨셉으로 가겠습니다",
          "original_form": "그럼 갓 입대한 병아리병장 컨셉으로 가겠습니다",
          "speaker_id": "2",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032186.1.8",
          "form": "ㅋㄱㄱㄱㄱㅋㄱㄱㄱ병아리병장은 뭐야 ㅋㄱㄱㄱㄱㅋㅋㅋㅈㅋ",
          "original_form": "ㅋㄱㄱㄱㄱㅋㄱㄱㄱ병아리병장은 뭐야 ㅋㄱㄱㄱㄱㅋㅋㅋㅈㅋ",
          "speaker_id": "1",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032186.1.9",
          "form": "지독하다지독해",
          "original_form": "지독하다지독해",
          "speaker_id": "1",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032186.1.10",
          "form": "병아리병사인데 말실수했지 말입니다",
          "original_form": "병아리병사인데 말실수했지 말입니다",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032186.1.11",
          "form": "가족이 제대하고 가져온 군대굿즈 잘 쓰는 중",
          "original_form": "가족이 제대하고 가져온 군대굿즈 잘 쓰는 중",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032186.1.12",
          "form": "티셔츠 질이 진짜 좋아서",
          "original_form": "티셔츠 질이 진짜 좋아서",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032186.1.13",
          "form": "아 부러워",
          "original_form": "아 부러워",
          "speaker_id": "1",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032186.1.14",
          "form": "로카티셔츠?!",
          "original_form": "로카티셔츠?!",
          "speaker_id": "1",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032186.1.15",
          "form": "나도그거갖고싶어",
          "original_form": "나도그거갖고싶어",
          "speaker_id": "1",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032186.1.16",
          "form": "내가족은 빈손으로 왔어 진짜 도움이 안돼",
          "original_form": "내가족은 빈손으로 왔어 진짜 도움이 안돼",
          "speaker_id": "1",
          "time": "20210905 13:58"
        },
        {
          "id": "MDRW2100032186.1.17",
          "form": "어째서",
          "original_form": "어째서",
          "speaker_id": "2",
          "time": "20210905 14:00"
        },
        {
          "id": "MDRW2100032186.1.18",
          "form": "그거 다 내가 낸 세금인데",
          "original_form": "그거 다 내가 낸 세금인데",
          "speaker_id": "2",
          "time": "20210905 14:00"
        },
        {
          "id": "MDRW2100032186.1.19",
          "form": "바리바리 챙겨 왔어야지",
          "original_form": "바리바리 챙겨 왔어야지",
          "speaker_id": "2",
          "time": "20210905 14:00"
        },
        {
          "id": "MDRW2100032186.1.20",
          "form": "런데이 나가면 그 티셔츠입은사람 많더라,,",
          "original_form": "런데이 나가면 그 티셔츠입은사람 많더라,,",
          "speaker_id": "1",
          "time": "20210905 14:01"
        },
        {
          "id": "MDRW2100032186.1.21",
          "form": "그 중 하나 나인 듯...",
          "original_form": "그 중 하나 나인 듯...",
          "speaker_id": "2",
          "time": "20210905 14:01"
        },
        {
          "id": "MDRW2100032186.1.22",
          "form": "군인다됐네,,",
          "original_form": "군인다됐네,,",
          "speaker_id": "1",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032186.1.23",
          "form": "성충~!",
          "original_form": "성충~!",
          "speaker_id": "2",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100032186.1.24",
          "form": "지독하다 지독해,,",
          "original_form": "지독하다 지독해,,",
          "speaker_id": "1",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100032186.1.25",
          "form": "만기전역해라,,",
          "original_form": "만기전역해라,,",
