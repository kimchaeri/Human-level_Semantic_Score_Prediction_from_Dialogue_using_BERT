      "utterance": [
        {
          "id": "MDRW2100009979.1.1",
          "form": "싱크홀 봤는데 재밌더라",
          "original_form": "싱크홀 봤는데 재밌더라",
          "speaker_id": "1",
          "time": "20210823 12:42"
        },
        {
          "id": "MDRW2100009979.1.2",
          "form": "차승원이 하드캐리",
          "original_form": "차승원이 하드캐리",
          "speaker_id": "1",
          "time": "20210823 12:42"
        },
        {
          "id": "MDRW2100009979.1.3",
          "form": "오..싱크홀 재밌냐",
          "original_form": "오..싱크홀 재밌냐",
          "speaker_id": "2",
          "time": "20210823 12:42"
        },
        {
          "id": "MDRW2100009979.1.4",
          "form": "단기간 관객수 많다고 기사뜬거같던데",
          "original_form": "단기간 관객수 많다고 기사뜬거같던데",
          "speaker_id": "2",
          "time": "20210823 12:43"
        },
        {
          "id": "MDRW2100009979.1.5",
          "form": "보고싶다",
          "original_form": "보고싶다",
          "speaker_id": "2",
          "time": "20210823 12:43"
        },
        {
          "id": "MDRW2100009979.1.6",
          "form": "싱크홀에서 탈출하는내용임?",
          "original_form": "싱크홀에서 탈출하는내용임?",
          "speaker_id": "2",
          "time": "20210823 12:43"
        },
        {
          "id": "MDRW2100009979.1.7",
          "form": "어 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:23"
        },
        {
          "id": "MDRW2100009979.1.8",
          "form": "벌써 한편 다 봤네",
          "original_form": "벌써 한편 다 봤네",
          "speaker_id": "1",
          "time": "20210823 13:23"
        },
        {
          "id": "MDRW2100009979.1.9",
          "form": "차승원 연기가 대박이야",
          "original_form": "차승원 연기가 대박이야",
          "speaker_id": "1",
          "time": "20210823 13:23"
        },
        {
          "id": "MDRW2100009979.1.10",
          "form": "오호 수어사이드스쿼드도 재밌었다",
          "original_form": "오호 수어사이드스쿼드도 재밌었다",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100009979.1.11",
          "form": "그거보고 1편보고 할리퀸도 봄 ㅋㅋ",
          "original_form": "그거보고 1편보고 할리퀸도 봄 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100009979.1.12",
          "form": "아 그거 재밌어...? 나는 뭔가 코믹스꺼랑 안맞아",
          "original_form": "아 그거 재밌어...? 나는 뭔가 코믹스꺼랑 안맞아",
          "speaker_id": "1",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100009979.1.13",
          "form": "코믹스 영화들 다 안봄 ㅜㅜ",
          "original_form": "코믹스 영화들 다 안봄 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100009979.1.14",
          "form": "ㅋㅋㅋ나도 안봤었는데 재밌더라 ㅋㅋ",
          "original_form": "ㅋㅋㅋ나도 안봤었는데 재밌더라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 07:14"
        },
        {
          "id": "MDRW2100009979.1.15",
          "form": "너 뮤지컬 본적있음?",
          "original_form": "너 뮤지컬 본적있음?",
          "speaker_id": "2",
          "time": "20210825 07:14"
        },
        {
          "id": "MDRW2100009979.1.16",
          "form": "뮤지컬은 안본듯 연극은 봤는데",
          "original_form": "뮤지컬은 안본듯 연극은 봤는데",
          "speaker_id": "1",
          "time": "20210825 09:57"
        },
        {
          "id": "MDRW2100009979.1.17",
          "form": "조승우꺼 가고싶다....",
          "original_form": "조승우꺼 가고싶다....",
          "speaker_id": "1",
          "time": "20210825 09:57"
        },
        {
          "id": "MDRW2100009979.1.18",
          "form": "조승우 뮤지컬도하나보네",
          "original_form": "조승우 뮤지컬도하나보네",
          "speaker_id": "2",
          "time": "20210825 11:13"
        },
        {
          "id": "MDRW2100009979.1.19",
          "form": "난 안재욱꺼 황태자루돌프 옛날에보다 졸고",
          "original_form": "난 안재욱꺼 황태자루돌프 옛날에보다 졸고",
          "speaker_id": "2",
          "time": "20210825 11:13"
        },
        {
          "id": "MDRW2100009979.1.20",
          "form": "남우현꺼 메피스토 1편보고나옴 ㅋㅋ",
          "original_form": "남우현꺼 메피스토 1편보고나옴 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:13"
        },
        {
          "id": "MDRW2100009979.1.21",
          "form": "아 웃겨 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 웃겨 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100009979.1.22",
          "form": "뭔가 다 옛날 사람들 같다",
          "original_form": "뭔가 다 옛날 사람들 같다",
          "speaker_id": "1",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100009979.1.23",
          "form": "조승우 뮤지컬 원탑이잖아",
          "original_form": "조승우 뮤지컬 원탑이잖아",
          "speaker_id": "1",
          "time": "20210825 12:46"
        },
        {
          "id": "MDRW2100009979.1.24",
          "form": "오 그래? 노래하는거 들어본적이없네 ㅋㅋㅋ",
          "original_form": "오 그래? 노래하는거 들어본적이없네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100009979.1.25",
          "form": "그치 안재욱꺼 ㅇㄱㅇ이랑 봤었지",
          "original_form": "그치 안재욱꺼 ㅇㄱㅇ이랑 봤었지",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100009979.1.26",
          "form": "오래됐네",
          "original_form": "오래됐네",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100009979.1.27",
          "form": "넌 전시회는 많이 다녀봤겠다",
          "original_form": "넌 전시회는 많이 다녀봤겠다",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100009979.1.28",
          "form": "겁나 웃겨 실명거론 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "겁나 웃겨 실명거론 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100009979.1.29",
          "form": "전시는 자주 가지....",
          "original_form": "전시는 자주 가지....",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100009979.1.30",
          "form": "요즘은 자제한다",
          "original_form": "요즘은 자제한다",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100009979.1.31",
          "form": "자제해야지",
          "original_form": "자제해야지",
          "speaker_id": "2",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100009979.1.32",
          "form": "코로나 언제끝나나",
          "original_form": "코로나 언제끝나나",
          "speaker_id": "2",
          "time": "20210825 15:21"
        },
        {
          "id": "MDRW2100009979.1.33",
          "form": "끝나면 영화관람이나 실컷 하러가야겠다",
          "original_form": "끝나면 영화관람이나 실컷 하러가야겠다",
          "speaker_id": "2",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100009979.1.34",
          "form": "나도...... 즐겨야지",
          "original_form": "나도...... 즐겨야지",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100009979.1.35",
          "form": "영화광이었는데 내려놨다",
          "original_form": "영화광이었는데 내려놨다",
          "speaker_id": "1",
          "time": "20210825 15:25"
        }
      ]
