      "utterance": [
        {
          "id": "MDRW2100024624.1.1",
          "form": "운동하시나요",
          "original_form": "운동하시나요",
          "speaker_id": "1",
          "time": "20210831 16:02"
        },
        {
          "id": "MDRW2100024624.1.2",
          "form": "음 가벼운 러닝과 근력운동이요",
          "original_form": "음 가벼운 러닝과 근력운동이요",
          "speaker_id": "2",
          "time": "20210831 16:03"
        },
        {
          "id": "MDRW2100024624.1.3",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210831 16:03"
        },
        {
          "id": "MDRW2100024624.1.4",
          "form": "러닝은 몇분정도 뛰세요",
          "original_form": "러닝은 몇분정도 뛰세요",
          "speaker_id": "1",
          "time": "20210831 16:03"
        },
        {
          "id": "MDRW2100024624.1.5",
          "form": "1시간정도요!",
          "original_form": "1시간정도요!",
          "speaker_id": "2",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100024624.1.6",
          "form": "오 쉬지않고 쭉이에요?",
          "original_form": "오 쉬지않고 쭉이에요?",
          "speaker_id": "1",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100024624.1.7",
          "form": "네네네!",
          "original_form": "네네네!",
          "speaker_id": "2",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100024624.1.8",
          "form": "인간이세요....?",
          "original_form": "인간이세요....?",
          "speaker_id": "1",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100024624.1.9",
          "form": "그게가능한거였군요..",
          "original_form": "그게가능한거였군요..",
          "speaker_id": "1",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100024624.1.10",
          "form": "음...적당한페이스로처음부터쭉,,,",
          "original_form": "음...적당한페이스로처음부터쭉,,,",
          "speaker_id": "2",
          "time": "20210831 16:08"
        },
        {
          "id": "MDRW2100024624.1.11",
          "form": "러닝언제부터하셨어요 꽤된거같은데요",
          "original_form": "러닝언제부터하셨어요 꽤된거같은데요",
          "speaker_id": "1",
          "time": "20210831 16:10"
        },
        {
          "id": "MDRW2100024624.1.12",
          "form": "1년쯤된거같아요!",
          "original_form": "1년쯤된거같아요!",
          "speaker_id": "2",
          "time": "20210831 16:14"
        },
        {
          "id": "MDRW2100024624.1.13",
          "form": "역시 그래도 엄청나시네요",
          "original_form": "역시 그래도 엄청나시네요",
          "speaker_id": "1",
          "time": "20210831 16:18"
        },
        {
          "id": "MDRW2100024624.1.14",
          "form": "근력운동은 헬스장가시나요",
          "original_form": "근력운동은 헬스장가시나요",
          "speaker_id": "1",
          "time": "20210831 16:18"
        },
        {
          "id": "MDRW2100024624.1.15",
          "form": "네네!!헬스장이요!",
          "original_form": "네네!!헬스장이요!",
          "speaker_id": "2",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100024624.1.16",
          "form": "헬스장가시면 그냥 혼자운동하시는건가요?",
          "original_form": "헬스장가시면 그냥 혼자운동하시는건가요?",
          "speaker_id": "1",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100024624.1.17",
          "form": "네네네 그렇죠!",
          "original_form": "네네네 그렇죠!",
          "speaker_id": "2",
