      "utterance": [
        {
          "id": "MDRW2100042883.1.1",
          "form": "집안일 마니 허시나요",
          "original_form": "집안일 마니 허시나요",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042883.1.2",
          "form": "네ㅜ주부라ㅠ",
          "original_form": "네ㅜ주부라ㅠ",
          "speaker_id": "1",
          "time": "20210907 11:31"
        },
        {
          "id": "MDRW2100042883.1.3",
          "form": "전담이져ㅜㅁ",
          "original_form": "전담이져ㅜㅁ",
          "speaker_id": "1",
          "time": "20210907 11:31"
        },
        {
          "id": "MDRW2100042883.1.4",
          "form": "애는귀여운데 왠종일볼라니..ㅋ헬",
          "original_form": "애는귀여운데 왠종일볼라니..ㅋ헬",
          "speaker_id": "1",
          "time": "20210907 11:31"
        },
        {
          "id": "MDRW2100042883.1.5",
          "form": "아 그러쿤요",
          "original_form": "아 그러쿤요",
          "speaker_id": "2",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042883.1.6",
          "form": "저는 엄마가 넘 바쁘셔서",
          "original_form": "저는 엄마가 넘 바쁘셔서",
          "speaker_id": "2",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042883.1.7",
          "form": "저도 많이하능 편이에요",
          "original_form": "저도 많이하능 편이에요",
          "speaker_id": "2",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042883.1.8",
          "form": "뭐가 제일 싫으세요?ㅋㅋㅋ",
          "original_form": "뭐가 제일 싫으세요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100042883.1.9",
          "form": "청소요ㅜ빨래개는거랑",
          "original_form": "청소요ㅜ빨래개는거랑",
          "speaker_id": "1",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042883.1.10",
          "form": "아아ㅠㅠㅠ 저는 설거지가 그렇게 싫슴다 ...",
          "original_form": "아아ㅠㅠㅠ 저는 설거지가 그렇게 싫슴다 ...",
          "speaker_id": "2",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042883.1.11",
          "form": "설것이는...식세기가 있는ㄷ ㅔㅠㅠㅠㅠ 빨래는",
          "original_form": "설것이는...식세기가 있는ㄷ ㅔㅠㅠㅠㅠ 빨래는",
          "speaker_id": "1",
          "time": "20210907 12:36"
        },
        {
          "id": "MDRW2100042883.1.12",
          "form": "아 식기세척기 좋아요?",
          "original_form": "아 식기세척기 좋아요?",
          "speaker_id": "2",
          "time": "20210907 12:36"
        },
        {
          "id": "MDRW2100042883.1.13",
          "form": "네 대충 헹궈서 넣음 !! 끝ㅋㅋㅋㅋ",
          "original_form": "네 대충 헹궈서 넣음 !! 끝ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:36"
        },
        {
          "id": "MDRW2100042883.1.14",
          "form": "밥풀은 좀 떼주고",
          "original_form": "밥풀은 좀 떼주고",
          "speaker_id": "1",
          "time": "20210907 12:36"
        },
        {
          "id": "MDRW2100042883.1.15",
          "form": "와와 그렇균요",
          "original_form": "와와 그렇균요",
          "speaker_id": "2",
          "time": "20210907 12:37"
        },
        {
          "id": "MDRW2100042883.1.16",
          "form": "어디거 쓰세요?",
          "original_form": "어디거 쓰세요?",
          "speaker_id": "2",
          "time": "20210907 12:37"
        },
        {
          "id": "MDRW2100042883.1.17",
          "form": "전기료 수도료 마니 안나오나요,",
          "original_form": "전기료 수도료 마니 안나오나요,",
          "speaker_id": "2",
          "time": "20210907 12:37"
        },
        {
          "id": "MDRW2100042883.1.18",
          "form": "엘지12인용이요 네 딱히 많이 안드는것 같아요 저는 설것이도 오래해서ㅠㅠ",
          "original_form": "엘지12인용이요 네 딱히 많이 안드는것 같아요 저는 설것이도 오래해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 12:37"
        },
        {
          "id": "MDRW2100042883.1.19",
          "form": "엘지 12인치요? 오오",
          "original_form": "엘지 12인치요? 오오",
          "speaker_id": "2",
          "time": "20210907 12:39"
        },
        {
          "id": "MDRW2100042883.1.20",
          "form": "저도 사고싶어요 식기세척기",
          "original_form": "저도 사고싶어요 식기세척기",
          "speaker_id": "2",
