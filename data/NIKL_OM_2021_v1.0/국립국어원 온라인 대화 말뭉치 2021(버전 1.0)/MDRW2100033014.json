      "utterance": [
        {
          "id": "MDRW2100033014.1.1",
          "form": "우와 처음 보는 주제예요",
          "original_form": "우와 처음 보는 주제예요",
          "speaker_id": "2",
          "time": "20210906 13:55"
        },
        {
          "id": "MDRW2100033014.1.2",
          "form": "지분 조각 투자...",
          "original_form": "지분 조각 투자...",
          "speaker_id": "2",
          "time": "20210906 13:55"
        },
        {
          "id": "MDRW2100033014.1.3",
          "form": "재산을 나누어서",
          "original_form": "재산을 나누어서",
          "speaker_id": "1",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.6",
          "form": "여기저기투자한다는 말인가요?",
          "original_form": "여기저기투자한다는 말인가요?",
          "speaker_id": "1",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.7",
          "form": "약간 이것저것 재테크 한다는 것 같아요",
          "original_form": "약간 이것저것 재테크 한다는 것 같아요",
          "speaker_id": "2",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.8",
          "form": "검색해보니까 요즘은",
          "original_form": "검색해보니까 요즘은",
          "speaker_id": "2",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.9",
          "form": "주식 예금 부동산",
          "original_form": "주식 예금 부동산",
          "speaker_id": "1",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.10",
          "form": "음악 저작권을 사기도 하고",
          "original_form": "음악 저작권을 사기도 하고",
          "speaker_id": "2",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.11",
          "form": "투자하는거 있으세요?",
          "original_form": "투자하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.12",
          "form": "아 그런것이ㅋㅋ",
          "original_form": "아 그런것이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.13",
          "form": "저는 주식이랑",
          "original_form": "저는 주식이랑",
          "speaker_id": "2",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.15",
          "form": "저희 연구실 선배는 미술품에",
          "original_form": "저희 연구실 선배는 미술품에",
          "speaker_id": "2",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.16",
          "form": "투자하더라구요...",
          "original_form": "투자하더라구요...",
          "speaker_id": "2",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.17",
          "form": "대신 주식은 안하시고?",
          "original_form": "대신 주식은 안하시고?",
          "speaker_id": "2",
          "time": "20210906 13:56"
        },
        {
          "id": "MDRW2100033014.1.18",
          "form": "우와 그거 돈많아야하는거아닌가요",
          "original_form": "우와 그거 돈많아야하는거아닌가요",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033014.1.19",
          "form": "저는 딱 주식만 해요! 그것도 우량주 위주로",
          "original_form": "저는 딱 주식만 해요! 그것도 우량주 위주로",
          "speaker_id": "2",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033014.1.20",
          "form": "음 몇 백 만원 정도",
          "original_form": "음 몇 백 만원 정도",
          "speaker_id": "2",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033014.1.21",
          "form": "삼전 네이버같은거요?",
          "original_form": "삼전 네이버같은거요?",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033014.1.22",
          "form": "있으면 되는 것 같아요",
          "original_form": "있으면 되는 것 같아요",
          "speaker_id": "2",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033014.1.23",
          "form": "오오 그래도 대단하시네요",
          "original_form": "오오 그래도 대단하시네요",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033014.1.24",
          "form": "네네 근데 네이버는 너무 비싸서",
          "original_form": "네네 근데 네이버는 너무 비싸서",
          "speaker_id": "2",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100033014.1.25",
          "form": "삼전만 모으고 있어요",
          "original_form": "삼전만 모으고 있어요",
          "speaker_id": "2",
          "time": "20210906 13:57"
        }
      ]
    }
