      "utterance": [
        {
          "id": "MDRW2100047561.1.1",
          "form": "다음 생에도 결혼 하실건가요?",
          "original_form": "다음 생에도 결혼 하실건가요?",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100047561.1.2",
          "form": "아니여ㅋㅋㅋ",
          "original_form": "아니여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100047561.1.3",
          "form": "왜했는지 의문이에요",
          "original_form": "왜했는지 의문이에요",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100047561.1.4",
          "form": "그럼 육아도 안하시고싶나요? 출산?",
          "original_form": "그럼 육아도 안하시고싶나요? 출산?",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100047561.1.5",
          "form": "하..그냥 비혼으로ㅠ",
          "original_form": "하..그냥 비혼으로ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100047561.1.6",
          "form": "님은 어떠세요?",
          "original_form": "님은 어떠세요?",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100047561.1.7",
          "form": "음 저도 ... 비혼..비출산도 괜찮을것 같아요ㅡ 한번 경험해봤으니..ㅋ",
          "original_form": "음 저도 ... 비혼..비출산도 괜찮을것 같아요ㅡ 한번 경험해봤으니..ㅋ",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100047561.1.8",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100047561.1.9",
          "form": "주말엔 아이들이랑 보통 뭐하시나요",
          "original_form": "주말엔 아이들이랑 보통 뭐하시나요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100047561.1.10",
          "form": "야구하거나 티비보",
          "original_form": "야구하거나 티비보",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100047561.1.11",
          "form": "하는게별로 없네요",
          "original_form": "하는게별로 없네요",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100047561.1.12",
          "form": "오 야구요? 직접 하시는거죠? 애들이 졸아하나봐요",
          "original_form": "오 야구요? 직접 하시는거죠? 애들이 졸아하나봐요",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100047561.1.13",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100047561.1.14",
          "form": "외동이라 제가 해줘야ㅠ",
          "original_form": "외동이라 제가 해줘야ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100047561.1.15",
          "form": "아.. 그럼 배트.글러브 이런게 있는거죠?",
          "original_form": "아.. 그럼 배트.글러브 이런게 있는거죠?",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100047561.1.16",
          "form": "별게다있더라구요ㅠ",
          "original_form": "별게다있더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100047561.1.17",
          "form": "야구용품이여?",
          "original_form": "야구용품이여?",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100047561.1.18",
          "form": "네 끝도없는ㅠ",
          "original_form": "네 끝도없는ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:45"
        }
      ]
