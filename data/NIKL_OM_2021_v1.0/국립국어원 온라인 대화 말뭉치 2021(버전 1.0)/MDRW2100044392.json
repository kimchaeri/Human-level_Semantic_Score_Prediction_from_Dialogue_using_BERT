      "utterance": [
        {
          "id": "MDRW2100044392.1.1",
          "form": "엉 지금 원트라는 그룹에서 추는거야",
          "original_form": "엉 지금 원트라는 그룹에서 추는거야",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044392.1.2",
          "form": "대결중이라 경쟁하는거 춤추면서",
          "original_form": "대결중이라 경쟁하는거 춤추면서",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044392.1.3",
          "form": "오 엠마가 됐네 저기 아이즈원 채연도 있음",
          "original_form": "오 엠마가 됐네 저기 아이즈원 채연도 있음",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044392.1.4",
          "form": "오 그렇귄ㆍ",
          "original_form": "오 그렇귄ㆍ",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044392.1.5",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044392.1.6",
          "form": "메인댄서 뽑는다고?",
          "original_form": "메인댄서 뽑는다고?",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044392.1.7",
          "form": "팀 꾸리는거야?",
          "original_form": "팀 꾸리는거야?",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044392.1.8",
          "form": "어어 지금 각 팀별로 리더그룹 서브그룹 이런식으로 모아서",
          "original_form": "어어 지금 각 팀별로 리더그룹 서브그룹 이런식으로 모아서",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044392.1.9",
          "form": "그들끼리 경쟁시키는거",
          "original_form": "그들끼리 경쟁시키는거",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044392.1.10",
          "form": "안무도 짜고 거기서 메인댄서 뽑고",
          "original_form": "안무도 짜고 거기서 메인댄서 뽑고",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044392.1.11",
          "form": "이제 세컨드 계급 그룹",
          "original_form": "이제 세컨드 계급 그룹",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044392.1.12",
          "form": "아아 그 사이에",
          "original_form": "아아 그 사이에",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044392.1.13",
          "form": "많은 진행이 있었군",
          "original_form": "많은 진행이 있었군",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044392.1.14",
          "form": "지난주 못봐서 글쿠나",
          "original_form": "지난주 못봐서 글쿠나",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044392.1.15",
          "form": "한회 놓쳤네",
          "original_form": "한회 놓쳤네",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044392.1.16",
          "form": "같은 댄스를 춰서",
          "original_form": "같은 댄스를 춰서",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044392.1.17",
          "form": "경쟁하다니",
          "original_form": "경쟁하다니",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044392.1.18",
          "form": "대단하지",
          "original_form": "대단하지",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044392.1.19",
          "form": "안무는 누가 짜는겨?",
          "original_form": "안무는 누가 짜는겨?",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044392.1.20",
          "form": "각자 짜서 서로 투표한거야",
          "original_form": "각자 짜서 서로 투표한거야",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044392.1.21",
          "form": "근데 안무를 짰어도 자기가 메인댄서가 안되면 최악인거래",
          "original_form": "근데 안무를 짰어도 자기가 메인댄서가 안되면 최악인거래",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044392.1.22",
          "form": "로잘린 대단하다",
          "original_form": "로잘린 대단하다",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044392.1.23",
          "form": "잘추네",
          "original_form": "잘추네",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044392.1.24",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044392.1.25",
          "form": "저걸 누가 짰는가도 중요하네",
          "original_form": "저걸 누가 짰는가도 중요하네",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044392.1.26",
          "form": "자기가 돋보이는 안무를 해야하고",
          "original_form": "자기가 돋보이는 안무를 해야하고",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044392.1.27",
          "form": "그치 그래서",
          "original_form": "그치 그래서",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044392.1.28",
          "form": "어려운안무 아니고 뺏어먹기 좋은걸 고른댔어",
          "original_form": "어려운안무 아니고 뺏어먹기 좋은걸 고른댔어",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044392.1.29",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044392.1.30",
          "form": "계급 짱이되면 다른 안무들을 자기 들러리로 쓸 수 있지",
          "original_form": "계급 짱이되면 다른 안무들을 자기 들러리로 쓸 수 있지",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044392.1.31",
          "form": "다들 대단하다 진짜",
          "original_form": "다들 대단하다 진짜",
          "speaker_id": "1",
          "time": "20210907 22:38"
        }
      ]
    }
  ]
