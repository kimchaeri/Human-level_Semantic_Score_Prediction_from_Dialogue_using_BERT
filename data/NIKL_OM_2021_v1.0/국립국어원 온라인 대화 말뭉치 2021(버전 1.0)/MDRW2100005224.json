        },
        {
          "id": "MDRW2100005224.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100005224.1.12",
          "form": "겨울이면 다 좋아해요",
          "original_form": "겨울이면 다 좋아해요",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100005224.1.13",
          "form": "눈이 예쁜 쓰레기인 건 아는데",
          "original_form": "눈이 예쁜 쓰레기인 건 아는데",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100005224.1.14",
          "form": "어쨌든 덥지는 않다는 증거니까요",
          "original_form": "어쨌든 덥지는 않다는 증거니까요",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100005224.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100005224.1.16",
          "form": "사시는 지역에 눈 많이 와요?",
          "original_form": "사시는 지역에 눈 많이 와요?",
          "speaker_id": "2",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100005224.1.17",
          "form": "제가 사는 지역은",
          "original_form": "제가 사는 지역은",
          "speaker_id": "1",
          "time": "20210823 00:29"
        },
        {
          "id": "MDRW2100005224.1.18",
          "form": "남쪽이라 눈이 잘 안와요",
          "original_form": "남쪽이라 눈이 잘 안와요",
          "speaker_id": "1",
          "time": "20210823 00:29"
        },
        {
          "id": "MDRW2100005224.1.19",
          "form": "오 저도 지금 남쪽에 살고 있어요",
          "original_form": "오 저도 지금 남쪽에 살고 있어요",
          "speaker_id": "2",
          "time": "20210823 00:30"
        },
        {
          "id": "MDRW2100005224.1.20",
          "form": "폭설 오는 지역에 살다가 내려갔어요",
          "original_form": "폭설 오는 지역에 살다가 내려갔어요",
          "speaker_id": "2",
          "time": "20210823 00:30"
        },
        {
          "id": "MDRW2100005224.1.21",
          "form": "간 첫날 눈이 왔는데",
          "original_form": "간 첫날 눈이 왔는데",
          "speaker_id": "2",
          "time": "20210823 00:30"
        },
        {
          "id": "MDRW2100005224.1.22",
          "form": "다들 핸드폰을 꺼내서 사진을 찍더라고요",
          "original_form": "다들 핸드폰을 꺼내서 사진을 찍더라고요",
          "speaker_id": "2",
          "time": "20210823 00:30"
        },
        {
          "id": "MDRW2100005224.1.23",
          "form": "당황해서 생각했죠",
          "original_form": "당황해서 생각했죠",
          "speaker_id": "2",
          "time": "20210823 00:30"
        },
        {
          "id": "MDRW2100005224.1.24",
          "form": "첫눈인가?",
          "original_form": "첫눈인가?",
