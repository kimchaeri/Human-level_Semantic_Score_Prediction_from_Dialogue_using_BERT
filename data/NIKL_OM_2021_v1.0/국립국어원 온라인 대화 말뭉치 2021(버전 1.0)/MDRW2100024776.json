      "utterance": [
        {
          "id": "MDRW2100024776.1.1",
          "form": "혹시 좋았던 데이트장소가 있으신가요?",
          "original_form": "혹시 좋았던 데이트장소가 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024776.1.2",
          "form": "저는 그냥 한강이요 ㅋㅋㅋ 한강변",
          "original_form": "저는 그냥 한강이요 ㅋㅋㅋ 한강변",
          "speaker_id": "2",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100024776.1.3",
          "form": "전 지방에 살아서 강은 좀 부럽네요ㅜ",
          "original_form": "전 지방에 살아서 강은 좀 부럽네요ㅜ",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024776.1.4",
          "form": "밤에 보는 강이 좋은가요 낮이 좋은가요?",
          "original_form": "밤에 보는 강이 좋은가요 낮이 좋은가요?",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024776.1.5",
          "form": "저두지방이에요 ㅋㅋㅋ 날좋으면낮+해질녘이 젤 좋아용 ㅋㅋ",
          "original_form": "저두지방이에요 ㅋㅋㅋ 날좋으면낮+해질녘이 젤 좋아용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024776.1.6",
          "form": "그러셨구나~~",
          "original_form": "그러셨구나~~",
          "speaker_id": "1",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100024776.1.7",
          "form": "해질녘 노을이 참 좋죠?",
          "original_form": "해질녘 노을이 참 좋죠?",
          "speaker_id": "1",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100024776.1.8",
          "form": "넹... 넘 이쁘죵name1님은 연애중이신가요",
          "original_form": "넹... 넘 이쁘죵{emoji:…}&name1&님은 연애중이신가요",
          "speaker_id": "2",
          "time": "20210831 18:41"
        },
        {
          "id": "MDRW2100024776.1.9",
          "form": "연애쉰지 삼년 넘은것 같네요ㅜ",
          "original_form": "연애쉰지 삼년 넘은것 같네요ㅜ",
          "speaker_id": "1",
          "time": "20210831 18:42"
        },
        {
          "id": "MDRW2100024776.1.10",
          "form": "맞춰간다는게 너무 힘들더라구요",
          "original_form": "맞춰간다는게 너무 힘들더라구요",
          "speaker_id": "1",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100024776.1.11",
          "form": "그죠.. 잘맞는사람 찾기 힘드네요",
          "original_form": "그죠.. 잘맞는사람 찾기 힘드네요",
          "speaker_id": "2",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100024776.1.12",
          "form": "연애중이세요~~?",
          "original_form": "연애중이세요~~?",
          "speaker_id": "1",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100024776.1.13",
          "form": "ㅋㅋㅋㅋ아뇨 이제끝낫네욤",
          "original_form": "ㅋㅋㅋㅋ아뇨 이제끝낫네욤",
          "speaker_id": "2",
          "time": "20210831 18:45"
        },
        {
          "id": "MDRW2100024776.1.14",
          "form": "ㅠㅠ연애 너무 힘든거 같아요",
          "original_form": "ㅠㅠ연애 너무 힘든거 같아요",
          "speaker_id": "1",
          "time": "20210831 18:45"
        },
        {
          "id": "MDRW2100024776.1.15",
          "form": "혼자가 더 편해져서ㅋㅋ",
          "original_form": "혼자가 더 편해져서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100024776.1.16",
          "form": "돈만있으면 굳이 결혼할 필요가 있을까 싶다가도",
          "original_form": "돈만있으면 굳이 결혼할 필요가 있을까 싶다가도",
          "speaker_id": "1",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100024776.1.17",
          "form": "그래도 외롭지 않을까 생각도 들고ㅎㅎ",
          "original_form": "그래도 외롭지 않을까 생각도 들고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100024776.1.18",
          "form": "ㅋㅋㅋㅋㅋ 그죠...누군가 필요해요ㅠㅠ 친구들이 ... 조금씩 결혼하니가 느껴져요. ..",
          "original_form": "ㅋㅋㅋㅋㅋ 그죠...누군가 필요해요ㅠㅠ 친구들이 ... 조금씩 결혼하니가 느껴져요. ..",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024776.1.19",
          "form": "맞아요\r\n근데 취미가 많으니까 것도 의미가 없더라구요ㅎㅎ",
          "original_form": "맞아요\r\n근데 취미가 많으니까 것도 의미가 없더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024776.1.20",
          "form": "오 취미가 많으세요?",
          "original_form": "오 취미가 많으세요?",
          "speaker_id": "2",
          "time": "20210831 18:48"
        },
        {
          "id": "MDRW2100024776.1.21",
          "form": "저는 많지는 않는데 소설 좋아해서 로맨스소설을 봤는데",
          "original_form": "저는 많지는 않는데 소설 좋아해서 로맨스소설을 봤는데",
          "speaker_id": "1",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024776.1.22",
          "form": "못보겠더라구요ㅎ",
          "original_form": "못보겠더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024776.1.23",
          "form": "현실이랑 너무 달라서ㅎㅎ",
          "original_form": "현실이랑 너무 달라서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024776.1.24",
          "form": "아 ㅋㅋㅋㅋ 그래서 사람들이 좋아하는듯...",
          "original_form": "아 ㅋㅋㅋㅋ 그래서 사람들이 좋아하는듯...",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024776.1.25",
          "form": "현실반영하면 ptsd올듯",
          "original_form": "현실반영하면 ptsd올듯",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024776.1.26",
          "form": "그쵸\r\n소설속 남주들은 어찌 다들 그리 잘 생겼는지ㅋㅋ",
          "original_form": "그쵸\r\n소설속 남주들은 어찌 다들 그리 잘 생겼는지ㅋㅋ",
