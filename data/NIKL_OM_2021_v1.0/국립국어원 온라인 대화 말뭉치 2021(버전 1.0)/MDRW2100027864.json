      "utterance": [
        {
          "id": "MDRW2100027864.1.1",
          "form": "수업 시간에 마이크로 의견 묻는 교수님이 싫다.",
          "original_form": "수업 시간에 마이크로 의견 묻는 교수님이 싫다.",
          "speaker_id": "2",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100027864.1.2",
          "form": "싫은 교수님 무더기",
          "original_form": "싫은 교수님 무더기",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027864.1.3",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027864.1.4",
          "form": "자율로 시켜도 싫고 선택해서 시켜도 싫다",
          "original_form": "자율로 시켜도 싫고 선택해서 시켜도 싫다",
          "speaker_id": "1",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027864.1.5",
          "form": "진짜싫다",
          "original_form": "진짜싫다",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027864.1.6",
          "form": "방금 후자인 교수님 수업 듣고 옴",
          "original_form": "방금 후자인 교수님 수업 듣고 옴",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027864.1.7",
          "form": "심지어 이런 교수님들",
          "original_form": "심지어 이런 교수님들",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027864.1.8",
          "form": "꼭 수업 끝나갈 때 물어봄",
          "original_form": "꼭 수업 끝나갈 때 물어봄",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027864.1.9",
          "form": "whyrano",
          "original_form": "whyrano",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027864.1.10",
          "form": "우린 하루종일 물어봄(;",
          "original_form": "우린 하루종일 물어봄(;",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027864.1.11",
          "form": "아 얼마전에 충격적인 교수님 봄",
          "original_form": "아 얼마전에 충격적인 교수님 봄",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027864.1.12",
          "form": "본 건 아니고 내 얘기긴 한데",
          "original_form": "본 건 아니고 내 얘기긴 한데",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027864.1.13",
          "form": "웃겨",
          "original_form": "웃겨",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027864.1.14",
          "form": "내가 봄",
          "original_form": "내가 봄",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027864.1.15",
          "form": "수업참여도 점수를",
          "original_form": "수업참여도 점수를",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027864.1.16",
          "form": "ㄹㅇ 내가봄",
          "original_form": "ㄹㅇ 내가봄",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027864.1.17",
          "form": "채팅으로 얼마나 열심히 반응하는지를 기준으로 부여한대",
          "original_form": "채팅으로 얼마나 열심히 반응하는지를 기준으로 부여한대",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027864.1.18",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027864.1.19",
          "form": "진짜 싫다",
          "original_form": "진짜 싫다",
          "speaker_id": "1",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027864.1.20",
          "form": "채팅하면 +0.5점",
          "original_form": "채팅하면 +0.5점",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027864.1.21",
          "form": "음성대답은 +1.0점이라는",
          "original_form": "음성대답은 +1.0점이라는",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027864.1.22",
          "form": "나같으면 드랍했다",
          "original_form": "나같으면 드랍했다",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027864.1.23",
          "form": "ㅠㅜㅠㅜㅠㅜㅠㅠ근데 진짜 듣고 싶던 수업이라",
          "original_form": "ㅠㅜㅠㅜㅠㅜㅠㅠ근데 진짜 듣고 싶던 수업이라",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027864.1.24",
          "form": "하......",
          "original_form": "하......",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027864.1.25",
          "form": "존버하면 교수가 바뀌는 경우가",
          "original_form": "존버하면 교수가 바뀌는 경우가",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027864.1.26",
          "form": "없죠",
          "original_form": "없죠",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027864.1.27",
          "form": "있지 않을까 우리는 그렇던데",
          "original_form": "있지 않을까 우리는 그렇던데",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027864.1.28",
          "form": "보통은 없음",
          "original_form": "보통은 없음",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027864.1.29",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027864.1.30",
          "form": "그럼 난 걍 안 들음",
          "original_form": "그럼 난 걍 안 들음",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027864.1.31",
          "form": "타과 타캠 수업인데 첫날부터 채팅창이 뜨겁길래",
          "original_form": "타과 타캠 수업인데 첫날부터 채팅창이 뜨겁길래",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027864.1.32",
          "form": "아니 심지어 타과 타캠 수업이냐고",
          "original_form": "아니 심지어 타과 타캠 수업이냐고",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027864.1.33",
          "form": "내가 모르는 이 캠만의 분위긴가 싶었는데",
          "original_form": "내가 모르는 이 캠만의 분위긴가 싶었는데",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027864.1.34",
          "form": "ㅇㅇ;",
          "original_form": "ㅇㅇ;",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027864.1.35",
          "form": "그런 비하인드가 있었던 거임",
          "original_form": "그런 비하인드가 있었던 거임",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027864.1.36",
          "form": "끔찍하다...",
          "original_form": "끔찍하다...",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027864.1.37",
          "form": "고생이 많으시네",
          "original_form": "고생이 많으시네",
          "speaker_id": "1",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027864.1.38",
