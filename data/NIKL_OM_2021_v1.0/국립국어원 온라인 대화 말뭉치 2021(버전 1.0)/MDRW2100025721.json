      "utterance": [
        {
          "id": "MDRW2100025721.1.1",
          "form": "알고리즘에 이끌려",
          "original_form": "알고리즘에 이끌려",
          "speaker_id": "2",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025721.1.2",
          "form": "보는 편이긴한데",
          "original_form": "보는 편이긴한데",
          "speaker_id": "2",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025721.1.3",
          "form": "최근엔 트와일라잇시리즈 재탕했어요",
          "original_form": "최근엔 트와일라잇시리즈 재탕했어요",
          "speaker_id": "2",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025721.1.4",
          "form": "트와일라잇이요? 오...",
          "original_form": "트와일라잇이요? 오...",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025721.1.5",
          "form": "전 첫편은 재밌었는데 그 다음부턴 약간..?",
          "original_form": "전 첫편은 재밌었는데 그 다음부턴 약간..?",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025721.1.6",
          "form": "사실 책도 1편만 재밌었던거 같아요",
          "original_form": "사실 책도 1편만 재밌었던거 같아요",
          "speaker_id": "1",
          "time": "20210901 17:59"
        },
        {
          "id": "MDRW2100025721.1.7",
          "form": "오.. 책도 보셨구나",
          "original_form": "오.. 책도 보셨구나",
          "speaker_id": "2",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100025721.1.8",
          "form": "저는 영화만 봤어요",
          "original_form": "저는 영화만 봤어요",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025721.1.9",
          "form": "맞아요 1편 감독 특유의 연출과 감성이",
          "original_form": "맞아요 1편 감독 특유의 연출과 감성이",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025721.1.10",
          "form": "대박인데",
          "original_form": "대박인데",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025721.1.11",
          "form": "다음 시리즈들은 의리로 봤어요ㅋㅋㅋ",
          "original_form": "다음 시리즈들은 의리로 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025721.1.12",
          "form": "그러고보니 1편은 여자감독이었는데",
          "original_form": "그러고보니 1편은 여자감독이었는데",
          "speaker_id": "1",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100025721.1.13",
          "form": "생각이상으로 잘되니까 남감독으로 제작사가 갈아치웠다고 하더라구요",
          "original_form": "생각이상으로 잘되니까 남감독으로 제작사가 갈아치웠다고 하더라구요",
          "speaker_id": "1",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100025721.1.14",
          "form": "진실을 알고나니까 뭔가 다 납득을 할수 있었어요... 하하",
          "original_form": "진실을 알고나니까 뭔가 다 납득을 할수 있었어요... 하하",
          "speaker_id": "1",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100025721.1.15",
          "form": "그러니까요......",
          "original_form": "그러니까요......",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100025721.1.16",
          "form": "트와는 1편 덕이 흥한건데",
          "original_form": "트와는 1편 덕이 흥한건데",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100025721.1.17",
          "form": "어휴... 뭐가 중요한건지도 모르고",
          "original_form": "어휴... 뭐가 중요한건지도 모르고",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100025721.1.18",
          "form": "자기 밥그릇에만 급급해서...",
          "original_form": "자기 밥그릇에만 급급해서...",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100025721.1.19",
          "form": "ㅉㅉㅉ",
          "original_form": "ㅉㅉㅉ",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100025721.1.20",
          "form": "꼭 영화보면 시즌제할 때",
          "original_form": "꼭 영화보면 시즌제할 때",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100025721.1.21",
          "form": "노잼으로 변하면 감독바뀐건지부터",
          "original_form": "노잼으로 변하면 감독바뀐건지부터",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100025721.1.22",
          "form": "확인하잖아요ㅎ",
          "original_form": "확인하잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100025721.1.23",
          "form": "그런 경우가 많군요...",
          "original_form": "그런 경우가 많군요...",
          "speaker_id": "1",
          "time": "20210902 00:23"
        },
        {
          "id": "MDRW2100025721.1.24",
          "form": "전 영화 시리즈라고 딱히 챙겨보는 타입은 아니라서 잘 몰랐어요",
          "original_form": "전 영화 시리즈라고 딱히 챙겨보는 타입은 아니라서 잘 몰랐어요",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100025721.1.25",
          "form": "마블영화도 드문드문 봤어요 ㅋㅋㅋㅋ",
          "original_form": "마블영화도 드문드문 봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100025721.1.26",
          "form": "저는 마블시리즈 안 좋아해서..ㅋㅋㅋ",
          "original_form": "저는 마블시리즈 안 좋아해서..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100025721.1.27",
          "form": "뭐가 복잡하더라고요 이해하기",
          "original_form": "뭐가 복잡하더라고요 이해하기",
          "speaker_id": "2",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100025721.1.28",
          "form": "그렇긴 해요.",
          "original_form": "그렇긴 해요.",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100025721.1.29",
          "form": "그럼 어떤 시리즈 영화를 보셨어요?",
          "original_form": "그럼 어떤 시리즈 영화를 보셨어요?",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100025721.1.30",
          "form": "젤 유명한건",
          "original_form": "젤 유명한건",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100025721.1.31",
          "form": "해리포터요!",
          "original_form": "해리포터요!",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100025721.1.32",
          "form": "아 스파이더맨홈커밍은 재밋게봣는데",
          "original_form": "아 스파이더맨홈커밍은 재밋게봣는데",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100025721.1.33",
          "form": "이건 마블께 아닌가",
          "original_form": "이건 마블께 아닌가",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100025721.1.34",
          "form": "마블 맞아요",
          "original_form": "마블 맞아요",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100025721.1.35",
          "form": "아! 해리포터!",
          "original_form": "아! 해리포터!",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100025721.1.36",
          "form": "책은 진짜 수십번 읽었는데",
          "original_form": "책은 진짜 수십번 읽었는데",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100025721.1.37",
          "form": "영화는 4편부터 살짝 아쉬웠어요",
          "original_form": "영화는 4편부터 살짝 아쉬웠어요",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100025721.1.38",
          "form": "하긴 압축률이 달라졌으니 어쩔수 없나...",
          "original_form": "하긴 압축률이 달라졌으니 어쩔수 없나...",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100025721.1.39",
          "form": "해리포터도 감독이 중간에 계속 바뀌고ㅜ",
          "original_form": "해리포터도 감독이 중간에 계속 바뀌고ㅜ",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100025721.1.40",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100025721.1.41",
          "form": "책 내용 삭제된거 너무 많죠",
          "original_form": "책 내용 삭제된거 너무 많죠",
          "speaker_id": "2",
          "time": "20210902 00:36"
        }
      ]
