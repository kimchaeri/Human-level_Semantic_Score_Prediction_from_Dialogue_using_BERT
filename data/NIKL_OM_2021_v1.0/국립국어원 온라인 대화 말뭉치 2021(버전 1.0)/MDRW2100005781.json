      "utterance": [
        {
          "id": "MDRW2100005781.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 11:45"
        },
        {
          "id": "MDRW2100005781.1.2",
          "form": "모임 좋아하시나요",
          "original_form": "모임 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 11:45"
        },
        {
          "id": "MDRW2100005781.1.3",
          "form": "저는 고등학교 친구들끼리 자주만나요ㅎㅎ",
          "original_form": "저는 고등학교 친구들끼리 자주만나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:45"
        },
        {
          "id": "MDRW2100005781.1.4",
          "form": "코로나라서 엄청자주는 아닌데.. ㅎ",
          "original_form": "코로나라서 엄청자주는 아닌데.. ㅎ",
          "speaker_id": "1",
          "time": "20210907 11:45"
        },
        {
          "id": "MDRW2100005781.1.5",
          "form": "저도 중학교 친구 모임은 하죠",
          "original_form": "저도 중학교 친구 모임은 하죠",
          "speaker_id": "2",
          "time": "20210907 11:48"
        },
        {
          "id": "MDRW2100005781.1.6",
          "form": "보통 만나면 뭐하세요?",
          "original_form": "보통 만나면 뭐하세요?",
          "speaker_id": "1",
          "time": "20210907 11:48"
        },
        {
          "id": "MDRW2100005781.1.7",
          "form": "저희는 맨날 카페에서 커피마시고",
          "original_form": "저희는 맨날 카페에서 커피마시고",
          "speaker_id": "1",
          "time": "20210907 11:48"
        },
        {
          "id": "MDRW2100005781.1.8",
          "form": "마라탕먹고 헤어져요ㅋㅋㅠ",
          "original_form": "마라탕먹고 헤어져요ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210907 11:48"
        },
        {
          "id": "MDRW2100005781.1.9",
          "form": "매번 다른걸 해보자! 이러고 똑같이하네요",
          "original_form": "매번 다른걸 해보자! 이러고 똑같이하네요",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100005781.1.10",
          "form": "저녁 먹고 커피마시고 수다떨고 하죠",
          "original_form": "저녁 먹고 커피마시고 수다떨고 하죠",
          "speaker_id": "2",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100005781.1.11",
          "form": "ㅋㅋ다비슷한가봐요",
          "original_form": "ㅋㅋ다비슷한가봐요",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100005781.1.12",
          "form": "동호회나 봉사같은것도 하시나요?",
          "original_form": "동호회나 봉사같은것도 하시나요?",
          "speaker_id": "1",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100005781.1.13",
          "form": "저는봉사는 하고 잇어요!",
          "original_form": "저는봉사는 하고 잇어요!",
          "speaker_id": "1",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100005781.1.14",
          "form": "다 똑같은 패턴 이네요",
          "original_form": "다 똑같은 패턴 이네요",
          "speaker_id": "2",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100005781.1.15",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100005781.1.16",
          "form": "어떤 봉사를 하시나요",
          "original_form": "어떤 봉사를 하시나요",
          "speaker_id": "2",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100005781.1.17",
          "form": "저는 야간학교교사를 하고 있어요!",
          "original_form": "저는 야간학교교사를 하고 있어요!",
          "speaker_id": "1",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100005781.1.18",
          "form": "제가 교사가 되는게 목표라서",
          "original_form": "제가 교사가 되는게 목표라서",
          "speaker_id": "1",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100005781.1.19",
          "form": "어르신들 검정고시도와드려요!",
          "original_form": "어르신들 검정고시도와드려요!",
          "speaker_id": "1",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100005781.1.20",
          "form": "name2은 봉사 하시나요?아니면 했던것중에 뭐가 있으신가요?",
          "original_form": "&name2&은 봉사 하시나요?아니면 했던것중에 뭐가 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100005781.1.21",
          "form": "전에 요양원봉사는 했어지요",
          "original_form": "전에 요양원봉사는 했어지요",
          "speaker_id": "2",
          "time": "20210907 11:53"
        },
        {
          "id": "MDRW2100005781.1.22",
          "form": "좋은일 하시네요",
          "original_form": "좋은일 하시네요",
          "speaker_id": "2",
          "time": "20210907 11:54"
        },
        {
          "id": "MDRW2100005781.1.23",
          "form": "name2도 좋은 일 하셨네요!!  대단해요",
          "original_form": "&name2&도 좋은 일 하셨네요!!  대단해요",
          "speaker_id": "1",
          "time": "20210907 11:54"
        },
        {
          "id": "MDRW2100005781.1.24",
          "form": "요양원봉사는 어떤거하나요?",
          "original_form": "요양원봉사는 어떤거하나요?",
          "speaker_id": "1",
          "time": "20210907 11:54"
        },
        {
          "id": "MDRW2100005781.1.25",
          "form": "고등학교다닐때 봉사동아리라 요양병원갔었을때 그냥 청소밖에 한기억이 없네요ㅎㅎ",
          "original_form": "고등학교다닐때 봉사동아리라 요양병원갔었을때 그냥 청소밖에 한기억이 없네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:55"
        },
        {
          "id": "MDRW2100005781.1.26",
          "form": "어르신들과 말벗도 해드리고 식사 챙겨드리고 화장실 도와드리고 간단한 일이예요",
          "original_form": "어르신들과 말벗도 해드리고 식사 챙겨드리고 화장실 도와드리고 간단한 일이예요",
          "speaker_id": "2",
          "time": "20210907 11:55"
        },
        {
          "id": "MDRW2100005781.1.27",
          "form": "와,..글로 읽기만해도",
          "original_form": "와,..글로 읽기만해도",
          "speaker_id": "1",
          "time": "20210907 11:56"
        },
        {
          "id": "MDRW2100005781.1.28",
          "form": "벌써 힘들것같아요",
          "original_form": "벌써 힘들것같아요",
          "speaker_id": "1",
          "time": "20210907 11:56"
        },
        {
          "id": "MDRW2100005781.1.29",
          "form": "정말 대단한 일 하셨군요",
