      "utterance": [
        {
          "id": "MDRW2100002689.1.1",
          "form": "급여는 만족하시나여?",
          "original_form": "급여는 만족하시나여?",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002689.1.2",
          "form": "현재 일을 하지는 않아서 급여랄건 없어요",
          "original_form": "현재 일을 하지는 않아서 급여랄건 없어요",
          "speaker_id": "1",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002689.1.3",
          "form": "name2님은 직장에 다니시나요?",
          "original_form": "&name2&님은 직장에 다니시나요?",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002689.1.4",
          "form": "아 안하시는군요",
          "original_form": "아 안하시는군요",
          "speaker_id": "2",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002689.1.5",
          "form": "네 전 치과요",
          "original_form": "네 전 치과요",
          "speaker_id": "2",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002689.1.6",
          "form": "치과면",
          "original_form": "치과면",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002689.1.7",
          "form": "치위생사나 의사이신가요?",
          "original_form": "치위생사나 의사이신가요?",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002689.1.8",
          "form": "아니면 치아코디네이터",
          "original_form": "아니면 치아코디네이터",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002689.1.9",
          "form": "치위생사입니다 :)",
          "original_form": "치위생사입니다 :)",
          "speaker_id": "2",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002689.1.10",
          "form": "하고싶으신 일 없으세요?",
          "original_form": "하고싶으신 일 없으세요?",
          "speaker_id": "2",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002689.1.11",
          "form": "멋진 직업을 가지고 계시네요",
          "original_form": "멋진 직업을 가지고 계시네요",
          "speaker_id": "1",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002689.1.12",
          "form": "음.. 저는 사실 딱히 하고싶다는 일은 없어서..",
          "original_form": "음.. 저는 사실 딱히 하고싶다는 일은 없어서..",
          "speaker_id": "1",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002689.1.13",
          "form": "아니예요 ㅎㅎ",
          "original_form": "아니예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002689.1.14",
          "form": "찾고있어요 내가 뭘 하고싶은지",
          "original_form": "찾고있어요 내가 뭘 하고싶은지",
          "speaker_id": "1",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002689.1.15",
          "form": "아르바이트도 안하고 쉬고계셔용?",
          "original_form": "아르바이트도 안하고 쉬고계셔용?",
          "speaker_id": "2",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002689.1.16",
          "form": "아니 뭐가 나쁜말이지",
          "original_form": "아니 뭐가 나쁜말이지",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002689.1.17",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002689.1.18",
          "form": "오ㅑ영",
          "original_form": "오ㅑ영",
          "speaker_id": "2",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002689.1.19",
          "form": "작.년까지 하다가 번아웃이 좀 와서",
          "original_form": "작.년까지 하다가 번아웃이 좀 와서",
          "speaker_id": "1",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002689.1.20",
          "form": "쉬고있어요",
          "original_form": "쉬고있어요",
          "speaker_id": "1",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002689.1.21",
          "form": "살면서 준비기간도 필요한거같아요 ㅠ",
          "original_form": "살면서 준비기간도 필요한거같아요 ㅠ",
