      "utterance": [
        {
          "id": "MDRW2100046777.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 10:40"
        },
        {
          "id": "MDRW2100046777.1.2",
          "form": "안녕하세요! 불금인데 계획있으신가요",
          "original_form": "안녕하세요! 불금인데 계획있으신가요",
          "speaker_id": "2",
          "time": "20211105 10:40"
        },
        {
          "id": "MDRW2100046777.1.3",
          "form": "야근입니다ㅠㅠ",
          "original_form": "야근입니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20211105 10:41"
        },
        {
          "id": "MDRW2100046777.1.4",
          "form": "아이고ㅠㅠㅠ 그럼 야식계획이라도 ㅠㅠ",
          "original_form": "아이고ㅠㅠㅠ 그럼 야식계획이라도 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211105 10:41"
        },
        {
          "id": "MDRW2100046777.1.5",
          "form": "네 치맥이요",
          "original_form": "네 치맥이요",
          "speaker_id": "1",
          "time": "20211105 10:41"
        },
        {
          "id": "MDRW2100046777.1.6",
          "form": "영화보연서",
          "original_form": "영화보연서",
          "speaker_id": "1",
          "time": "20211105 10:41"
        },
        {
          "id": "MDRW2100046777.1.7",
          "form": "엇 저두 오늘 치맥하면서 넷플 볼 예정입니다 ㅎㅎ",
          "original_form": "엇 저두 오늘 치맥하면서 넷플 볼 예정입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046777.1.8",
          "form": "오저도요",
          "original_form": "오저도요",
          "speaker_id": "1",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046777.1.9",
          "form": "마이네임ㅋㅋ",
          "original_form": "마이네임ㅋㅋ",
          "speaker_id": "1",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046777.1.10",
          "form": "헐 저두욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐 저두욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046777.1.11",
          "form": "요즘잼나서요",
          "original_form": "요즘잼나서요",
          "speaker_id": "1",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046777.1.12",
          "form": "저는 오늘 첫회부터 볼거라서 너무 기대됩니다",
          "original_form": "저는 오늘 첫회부터 볼거라서 너무 기대됩니다",
          "speaker_id": "2",
          "time": "20211105 10:42"
        },
        {
          "id": "MDRW2100046777.1.13",
          "form": "네네 중간쯤봤는데 재미가쏠쏠",
          "original_form": "네네 중간쯤봤는데 재미가쏠쏠",
          "speaker_id": "1",
          "time": "20211105 10:43"
        },
        {
          "id": "MDRW2100046777.1.14",
          "form": "배우들이 정말 멋있게 나오더라구요",
          "original_form": "배우들이 정말 멋있게 나오더라구요",
          "speaker_id": "2",
          "time": "20211105 10:43"
        },
        {
          "id": "MDRW2100046777.1.15",
          "form": "캐릭터가 매력적이고 ",
          "original_form": "캐릭터가 매력적이고 ",
          "speaker_id": "2",
