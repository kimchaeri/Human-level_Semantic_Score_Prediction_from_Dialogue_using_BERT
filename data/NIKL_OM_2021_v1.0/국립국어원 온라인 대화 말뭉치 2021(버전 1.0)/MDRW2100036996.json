      "utterance": [
        {
          "id": "MDRW2100036996.1.1",
          "form": "호우주의보 문자도 왔어",
          "original_form": "호우주의보 문자도 왔어",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100036996.1.2",
          "form": "어 완전 무섭게온다",
          "original_form": "어 완전 무섭게온다",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036996.1.3",
          "form": "그나마 지금 아주 조금 나아진듯",
          "original_form": "그나마 지금 아주 조금 나아진듯",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036996.1.4",
          "form": "아까 한 30분 전쯤이 젤 무섭게 내렸어 여긴",
          "original_form": "아까 한 30분 전쯤이 젤 무섭게 내렸어 여긴",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036996.1.5",
          "form": "비가와서 그런가 너 왔을때 먹었던 주꾸미볶음 생각나는군",
          "original_form": "비가와서 그런가 너 왔을때 먹었던 주꾸미볶음 생각나는군",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100036996.1.6",
          "form": "천사채볶음과 먹었던 것?",
          "original_form": "천사채볶음과 먹었던 것?",
          "speaker_id": "1",
          "time": "20210831 20:36"
        },
        {
          "id": "MDRW2100036996.1.7",
          "form": "비 오는 날 생각날 만하지",
          "original_form": "비 오는 날 생각날 만하지",
          "speaker_id": "1",
          "time": "20210831 20:36"
        },
        {
          "id": "MDRW2100036996.1.8",
          "form": "그러나 비오는 날 뜨끈한 순대국도 좋았다~",
          "original_form": "그러나 비오는 날 뜨끈한 순대국도 좋았다~",
          "speaker_id": "1",
          "time": "20210831 20:37"
        },
        {
          "id": "MDRW2100036996.1.9",
          "form": "그러게 계절에 어울리는 음식이 됐네",
          "original_form": "그러게 계절에 어울리는 음식이 됐네",
          "speaker_id": "2",
          "time": "20210831 20:39"
        },
        {
          "id": "MDRW2100036996.1.10",
          "form": "시원해져서",
          "original_form": "시원해져서",
          "speaker_id": "2",
          "time": "20210831 20:39"
        },
        {
          "id": "MDRW2100036996.1.11",
          "form": "응응 기온은 확실히 떨어진듯",
          "original_form": "응응 기온은 확실히 떨어진듯",
          "speaker_id": "1",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100036996.1.12",
          "form": "곧 겉옷 입고 다니게서",
          "original_form": "곧 겉옷 입고 다니게서",
          "speaker_id": "1",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100036996.1.13",
          "form": "가을옷들",
          "original_form": "가을옷들",
          "speaker_id": "1",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100036996.1.14",
          "form": "어 진짜",
          "original_form": "어 진짜",
          "speaker_id": "2",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100036996.1.15",
          "form": "긴팔입어야할듯",
          "original_form": "긴팔입어야할듯",
          "speaker_id": "2",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100036996.1.16",
          "form": "응 이제는 가을이여",
          "original_form": "응 이제는 가을이여",
          "speaker_id": "1",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100036996.1.17",
          "form": "오늘저녁은 창문 잘 닫고 자야할듯",
          "original_form": "오늘저녁은 창문 잘 닫고 자야할듯",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100036996.1.18",
          "form": "응 바람도 많이 부니까",
          "original_form": "응 바람도 많이 부니까",
          "speaker_id": "1",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100036996.1.19",
          "form": "새벽엔 쌀쌀해",
          "original_form": "새벽엔 쌀쌀해",
          "speaker_id": "2",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100036996.1.20",
          "form": "응 자다가 서늘해서 깸",
          "original_form": "응 자다가 서늘해서 깸",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100036996.1.21",
          "form": "이불 잘 덮고 자~",
          "original_form": "이불 잘 덮고 자~",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100036996.1.22",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100036996.1.23",
          "form": "비 많이 오네",
          "original_form": "비 많이 오네",
