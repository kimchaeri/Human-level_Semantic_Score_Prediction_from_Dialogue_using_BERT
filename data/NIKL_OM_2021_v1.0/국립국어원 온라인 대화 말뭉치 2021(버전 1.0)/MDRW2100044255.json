      "utterance": [
        {
          "id": "MDRW2100044255.1.1",
          "form": "김치참치치즈계란이라니",
          "original_form": "김치참치치즈계란이라니",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044255.1.2",
          "form": "집주변에 새로유 맛집 찾았니",
          "original_form": "집주변에 새로유 맛집 찾았니",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044255.1.3",
          "form": "맛이 없을 수가 없겠는걸",
          "original_form": "맛이 없을 수가 없겠는걸",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044255.1.4",
          "form": "아니.. 맛집 드럽게 없어",
          "original_form": "아니.. 맛집 드럽게 없어",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044255.1.5",
          "form": "아직도 식당들이 마땅한 게 안 들어왔는데",
          "original_form": "아직도 식당들이 마땅한 게 안 들어왔는데",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044255.1.6",
          "form": "진짜 마음마프네",
          "original_form": "진짜 마음마프네",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044255.1.7",
          "form": "마라탕이랑 치킨 집은 좀 있더라",
          "original_form": "마라탕이랑 치킨 집은 좀 있더라",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044255.1.8",
          "form": "마라탕 궁금하다",
          "original_form": "마라탕 궁금하다",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044255.1.9",
          "form": "마라탕한번 도전해봐",
          "original_form": "마라탕한번 도전해봐",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044255.1.10",
          "form": "100그람에 1600원이면",
          "original_form": "100그람에 1600원이면",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044255.1.11",
          "form": "고기를 많이 넣는게 포인트여",
          "original_form": "고기를 많이 넣는게 포인트여",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044255.1.12",
          "form": "평균 가격인가?",
          "original_form": "평균 가격인가?",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044255.1.13",
          "form": "마라탕집말야",
          "original_form": "마라탕집말야",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044255.1.14",
          "form": "그럴걸 아마",
          "original_form": "그럴걸 아마",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044255.1.15",
          "form": "한번가면 대략 1만원에서 2만원사이로 나와",
          "original_form": "한번가면 대략 1만원에서 2만원사이로 나와",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044255.1.16",
          "form": "생각보다 값이 좀 있구나",
          "original_form": "생각보다 값이 좀 있구나",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044255.1.17",
          "form": "가벼운 재료 위주로",
          "original_form": "가벼운 재료 위주로",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044255.1.18",
          "form": "왕창 넣어야징",
          "original_form": "왕창 넣어야징",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044255.1.19",
          "form": "넌 무슨 재료 좋아해",
          "original_form": "넌 무슨 재료 좋아해",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044255.1.20",
          "form": "나는 분모자랑",
          "original_form": "나는 분모자랑",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044255.1.21",
          "form": "납작당면",
          "original_form": "납작당면",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044255.1.22",
          "form": "단배추 숙주 팽이버섯",
          "original_form": "단배추 숙주 팽이버섯",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044255.1.23",
          "form": "소고기많이",
          "original_form": "소고기많이",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044255.1.24",
          "form": "비엔나랑 메추라기알도 넣어",
          "original_form": "비엔나랑 메추라기알도 넣어",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044255.1.25",
          "form": "청경채는 안 먹어?",
          "original_form": "청경채는 안 먹어?",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044255.1.26",
          "form": "넣을때도 있어",
          "original_form": "넣을때도 있어",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044255.1.27",
          "form": "두부류는 안 먹니",
          "original_form": "두부류는 안 먹니",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044255.1.28",
          "form": "두부류는 너무 크고 질겨서",
          "original_form": "두부류는 너무 크고 질겨서",
          "speaker_id": "1",
          "time": "20210907 22:08"
        }
      ]
    }
