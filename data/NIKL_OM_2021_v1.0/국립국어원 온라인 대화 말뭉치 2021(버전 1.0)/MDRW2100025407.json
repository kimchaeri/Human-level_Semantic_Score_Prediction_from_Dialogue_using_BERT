      "utterance": [
        {
          "id": "MDRW2100025407.1.1",
          "form": "최근 구매 물건 있으신가요?",
          "original_form": "최근 구매 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025407.1.2",
          "form": "예 주로 식품 구입해요",
          "original_form": "예 주로 식품 구입해요",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025407.1.3",
          "form": "당신은요",
          "original_form": "당신은요",
          "speaker_id": "2",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025407.1.4",
          "form": "저는 어제 홈플러스에서 장봤어요",
          "original_form": "저는 어제 홈플러스에서 장봤어요",
          "speaker_id": "1",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025407.1.5",
          "form": "요즘 꽃게가 제철이래요",
          "original_form": "요즘 꽃게가 제철이래요",
          "speaker_id": "1",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025407.1.6",
          "form": "그래요? 맛있겠네요",
          "original_form": "그래요? 맛있겠네요",
          "speaker_id": "2",
          "time": "20210901 11:02"
        },
        {
          "id": "MDRW2100025407.1.7",
          "form": "살아있는 꽃게라 손질이 무섭네요",
          "original_form": "살아있는 꽃게라 손질이 무섭네요",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025407.1.8",
          "form": "구매 희망 물건은 뭐가 있으세요",
          "original_form": "구매 희망 물건은 뭐가 있으세요",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025407.1.9",
          "form": "손 조심하세요",
          "original_form": "손 조심하세요",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025407.1.10",
          "form": "음 바람막이 구매하고 싶어서 찾아보고있어요",
          "original_form": "음 바람막이 구매하고 싶어서 찾아보고있어요",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025407.1.11",
          "form": "여름이 끝나가서 필요하긴 하네요",
          "original_form": "여름이 끝나가서 필요하긴 하네요",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025407.1.12",
          "form": "저는 운동하게 실내자전거 사고싶어요",
          "original_form": "저는 운동하게 실내자전거 사고싶어요",
          "speaker_id": "1",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025407.1.13",
          "form": "저도요 티비보면서 타면 좋을것같아요",
          "original_form": "저도요 티비보면서 타면 좋을것같아요",
          "speaker_id": "2",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025407.1.14",
          "form": "집이 좁아서 살까말까 고민이에요",
          "original_form": "집이 좁아서 살까말까 고민이에요",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025407.1.15",
          "form": "요즘은 접이식도 있다던데 찾아보면 부피 별로 크지 않은 자전거가 있지 않을까요?",
          "original_form": "요즘은 접이식도 있다던데 찾아보면 부피 별로 크지 않은 자전거가 있지 않을까요?",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025407.1.16",
          "form": "그럴까요?",
          "original_form": "그럴까요?",
          "speaker_id": "1",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025407.1.17",
          "form": "할인 소식 있나 찾아봐야겠어요",
          "original_form": "할인 소식 있나 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025407.1.18",
          "form": "예 근데 전 비싸보여서 못사겠어요",
          "original_form": "예 근데 전 비싸보여서 못사겠어요",
          "speaker_id": "2",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025407.1.19",
          "form": "그러게요 할인하면 좋겠네요",
          "original_form": "그러게요 할인하면 좋겠네요",
          "speaker_id": "2",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025407.1.20",
          "form": "당근마켓에도 찾아보니 많이 나와있네요",
          "original_form": "당근마켓에도 찾아보니 많이 나와있네요",
          "speaker_id": "1",
          "time": "20210901 11:22"
        }
      ]
