      "utterance": [
        {
          "id": "MDRW2100026135.1.1",
          "form": "저 친척이 제주에살아서 자주내려가요!",
          "original_form": "저 친척이 제주에살아서 자주내려가요!",
          "speaker_id": "1",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026135.1.2",
          "form": "연고가있어서..",
          "original_form": "연고가있어서..",
          "speaker_id": "1",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026135.1.3",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026135.1.4",
          "form": "삼촌 어디 감수광",
          "original_form": "삼촌 어디 감수광",
          "speaker_id": "2",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026135.1.5",
          "form": "약간 제2의 집같은 느낌..",
          "original_form": "약간 제2의 집같은 느낌..",
          "speaker_id": "1",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026135.1.6",
          "form": "이말을 하면 도민분이랑 친해진다는데 ㅋㅋㅋ",
          "original_form": "이말을 하면 도민분이랑 친해진다는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026135.1.7",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 사투리 쓰시는 분들 계시더라구요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 사투리 쓰시는 분들 계시더라구요",
          "speaker_id": "1",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026135.1.8",
          "form": "밥 먹엄져?",
          "original_form": "밥 먹엄져?",
          "speaker_id": "1",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026135.1.9",
          "form": "아직 알아듣진 못하는데 차차 알게되겠져",
          "original_form": "아직 알아듣진 못하는데 차차 알게되겠져",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026135.1.10",
          "form": "자연농 농부님한테 풋귤사서 청만들었어요 친구들 오면 기념품으로 주려구요",
          "original_form": "자연농 농부님한테 풋귤사서 청만들었어요 친구들 오면 기념품으로 주려구요",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026135.1.11",
          "form": "우와 대박 친구분들이 엄청좋아하실거같아요",
          "original_form": "우와 대박 친구분들이 엄청좋아하실거같아요",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026135.1.12",
          "form": "제주살이 제대로하고계시네요",
          "original_form": "제주살이 제대로하고계시네요",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026135.1.13",
          "form": "서울살다 내려오니 심심하고도 심심해서요",
          "original_form": "서울살다 내려오니 심심하고도 심심해서요",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026135.1.14",
          "form": "밤에 무지 심심하더라구요",
          "original_form": "밤에 무지 심심하더라구요",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026135.1.15",
          "form": "시내인데두 8시 이후면 조용해사",
          "original_form": "시내인데두 8시 이후면 조용해사",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026135.1.16",
          "form": "아예.밤삼책하기도 해요",
          "original_form": "아예.밤삼책하기도 해요",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026135.1.17",
          "form": "유럽같은 너낌으로다가",
          "original_form": "유럽같은 너낌으로다가",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026135.1.18",
          "form": "글구 제주도도 4단계라",
          "original_form": "글구 제주도도 4단계라",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100026135.1.19",
          "form": "9시이후에 뭐 못하죠 ㅜ",
          "original_form": "9시이후에 뭐 못하죠 ㅜ",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100026135.1.20",
          "form": "근데 낮에 여기저기 가니까 밤엔 사실 서울에서도 잘 안나가잖아요",
          "original_form": "근데 낮에 여기저기 가니까 밤엔 사실 서울에서도 잘 안나가잖아요",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100026135.1.21",
          "form": "다만 도시라 아니라서 불빛이 어색해요",
          "original_form": "다만 도시라 아니라서 불빛이 어색해요",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100026135.1.22",
          "form": "아 좀 어두운가보군요",
          "original_form": "아 좀 어두운가보군요",
          "speaker_id": "1",
          "time": "20210902 17:27"
        }
      ]
    }
  ]
