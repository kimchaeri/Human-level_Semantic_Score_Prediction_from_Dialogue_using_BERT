      "utterance": [
        {
          "id": "MDRW2100021798.1.1",
          "form": "쇼핑조아하시나요",
          "original_form": "쇼핑조아하시나요",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021798.1.2",
          "form": "그럼요~ 돈만 충분하면 뭔들 못사겠어요 ㅋ",
          "original_form": "그럼요~ 돈만 충분하면 뭔들 못사겠어요 ㅋ",
          "speaker_id": "2",
          "time": "20210829 08:57"
        },
        {
          "id": "MDRW2100021798.1.3",
          "form": "중고거래도하시나요",
          "original_form": "중고거래도하시나요",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100021798.1.4",
          "form": "그럼요~ 당근 좋아합니다. 당근 거래 하시나요?",
          "original_form": "그럼요~ 당근 좋아합니다. 당근 거래 하시나요?",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100021798.1.5",
          "form": "오 네하죠ㅎㅎㅎ",
          "original_form": "오 네하죠ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100021798.1.6",
          "form": "최근에 뭐 거래하셨나요?",
          "original_form": "최근에 뭐 거래하셨나요?",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100021798.1.7",
          "form": "일년째 안팔리는 옷이있네요ㅜ",
          "original_form": "일년째 안팔리는 옷이있네요ㅜ",
          "speaker_id": "1",
          "time": "20210829 11:23"
        },
        {
          "id": "MDRW2100021798.1.8",
          "form": "저도 그냥 올려놔요. 1년 지나도 필요한 사람이 산다고 하더라고요",
          "original_form": "저도 그냥 올려놔요. 1년 지나도 필요한 사람이 산다고 하더라고요",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100021798.1.9",
          "form": "빨리 팔렷음좋겠네요",
          "original_form": "빨리 팔렷음좋겠네요",
          "speaker_id": "1",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100021798.1.10",
          "form": "어떤 옷인가요?",
          "original_form": "어떤 옷인가요?",
          "speaker_id": "2",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100021798.1.11",
          "form": "털자켓이요ㅎㅎ",
          "original_form": "털자켓이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100021798.1.12",
          "form": "곧 추워지니 이제 팔릴거예요",
          "original_form": "곧 추워지니 이제 팔릴거예요",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100021798.1.13",
