      "utterance": [
        {
          "id": "MDRW2100021716.1.1",
          "form": "불토 맛난거 드셨나요?",
          "original_form": "불토 맛난거 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021716.1.2",
          "form": "저 롯데리아 햄버거 먹었어요~",
          "original_form": "저 롯데리아 햄버거 먹었어요~",
          "speaker_id": "1",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021716.1.3",
          "form": "외식하셨네요! 전 토스트해먹었어요",
          "original_form": "외식하셨네요! 전 토스트해먹었어요",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021716.1.4",
          "form": "외식이라 하기엔 ㅋㅋㅋ 포장인걸요",
          "original_form": "외식이라 하기엔 ㅋㅋㅋ 포장인걸요",
          "speaker_id": "1",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021716.1.5",
          "form": "토스트면 이삭 토스트?",
          "original_form": "토스트면 이삭 토스트?",
          "speaker_id": "1",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021716.1.6",
          "form": "파바할인한다고 빵을 사놔서ㅋㅋ 집에서 치즈토스트해먹었어요!",
          "original_form": "파바할인한다고 빵을 사놔서ㅋㅋ 집에서 치즈토스트해먹었어요!",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021716.1.7",
          "form": "헐 아 대박 식빵 사신거에요?",
          "original_form": "헐 아 대박 식빵 사신거에요?",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021716.1.8",
          "form": "파바 그거 요기요 할인?ㅋㅋㅋ",
          "original_form": "파바 그거 요기요 할인?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021716.1.9",
          "form": "넵ㅋㅋ식빵 냉동실에 얼려두고 비상식량이요",
          "original_form": "넵ㅋㅋ식빵 냉동실에 얼려두고 비상식량이요",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021716.1.10",
          "form": "내일 마지막이니까 한번 더 살까해요ㅋㅋ",
          "original_form": "내일 마지막이니까 한번 더 살까해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021716.1.11",
          "form": "저도 요기요에서 두번씩이나 사먹었는뎈ㅋㅋㅋ",
          "original_form": "저도 요기요에서 두번씩이나 사먹었는뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021716.1.12",
          "form": "나도낼 한번더 갈까 고민중이에요",
          "original_form": "나도낼 한번더 갈까 고민중이에요",
          "speaker_id": "1",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021716.1.13",
          "form": "추천 빵 있어요?",
          "original_form": "추천 빵 있어요?",
          "speaker_id": "1",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021716.1.14",
          "form": "저 생미종식빵?? 이거랑 버터식빵 사려고요",
          "original_form": "저 생미종식빵?? 이거랑 버터식빵 사려고요",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021716.1.15",
          "form": "파운드도 고민했는데 우선은 저렇게만ㅎㅎ",
          "original_form": "파운드도 고민했는데 우선은 저렇게만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021716.1.16",
          "form": "식빵류로 만원 채우기 어렵지 않아요?ㅠㅠ",
          "original_form": "식빵류로 만원 채우기 어렵지 않아요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021716.1.17",
          "form": "전 저번에 딱 만원 채워가지고 주문했어요 ㅎㅎ",
          "original_form": "전 저번에 딱 만원 채워가지고 주문했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021716.1.18",
          "form": "의도한건 아닌데 그렇게 되더라구요.. 재고가..",
          "original_form": "의도한건 아닌데 그렇게 되더라구요.. 재고가..",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021716.1.19",
          "form": "ㅋㅋ일찍 가야지 다양하게 있는거같아요 샐러드가 인기가 많더라고요",
          "original_form": "ㅋㅋ일찍 가야지 다양하게 있는거같아요 샐러드가 인기가 많더라고요",
          "speaker_id": "2",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021716.1.20",
          "form": "어머~제가 안 사는 것만 인기 많아서 다행이더라구요",
          "original_form": "어머~제가 안 사는 것만 인기 많아서 다행이더라구요",
          "speaker_id": "1",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021716.1.21",
          "form": "전 샌드위치 사요 둘이 가격도 비슷하던데 ㅜㅜ",
          "original_form": "전 샌드위치 사요 둘이 가격도 비슷하던데 ㅜㅜ",
