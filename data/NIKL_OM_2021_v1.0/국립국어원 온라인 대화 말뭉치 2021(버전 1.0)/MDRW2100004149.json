      "utterance": [
        {
          "id": "MDRW2100004149.1.1",
          "form": "좋아하는 라면 있어?",
          "original_form": "좋아하는 라면 있어?",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004149.1.2",
          "form": "불닭볶음면 제일 좋아",
          "original_form": "불닭볶음면 제일 좋아",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004149.1.3",
          "form": "불닭볶음면 맛있지.. 맵고....",
          "original_form": "불닭볶음면 맛있지.. 맵고....",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004149.1.4",
          "form": "ㅋㅋㅋ맞아 매워 그게 좋아",
          "original_form": "ㅋㅋㅋ맞아 매워 그게 좋아",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004149.1.5",
          "form": "핵불닭볶음면 먹어 봤어?",
          "original_form": "핵불닭볶음면 먹어 봤어?",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004149.1.6",
          "form": "아니 못구하겠던데",
          "original_form": "아니 못구하겠던데",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004149.1.7",
          "form": "엄청 맵다던데 궁금하다",
          "original_form": "엄청 맵다던데 궁금하다",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004149.1.8",
          "form": "매운 거 좋아해?",
          "original_form": "매운 거 좋아해?",
          "speaker_id": "1",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004149.1.9",
          "form": "응 잘 먹지는 못하는데 좋아해",
          "original_form": "응 잘 먹지는 못하는데 좋아해",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004149.1.10",
          "form": "나도 ㅋㅋㅋ맵찔이",
          "original_form": "나도 ㅋㅋㅋ맵찔이",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004149.1.11",
          "form": "스트레스 풀리고 기분 좋아",
          "original_form": "스트레스 풀리고 기분 좋아",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004149.1.12",
          "form": "우유랑 먹으면 그나마 괜찮더라",
          "original_form": "우유랑 먹으면 그나마 괜찮더라",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004149.1.13",
          "form": "맞아 단백질",
          "original_form": "맞아 단백질",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
