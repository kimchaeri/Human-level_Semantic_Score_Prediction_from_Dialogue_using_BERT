      "utterance": [
        {
          "id": "MDRW2100013241.1.1",
          "form": "안녕하세요! 점심 드셨나요?",
          "original_form": "안녕하세요! 점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100013241.1.2",
          "form": "지금 먹고있어요",
          "original_form": "지금 먹고있어요",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100013241.1.3",
          "form": "오 뭐 드시고 계신가요?",
          "original_form": "오 뭐 드시고 계신가요?",
          "speaker_id": "1",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100013241.1.4",
          "form": "우동먹고있어요 ㅎㅎ",
          "original_form": "우동먹고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013241.1.5",
          "form": "점심에 뭐드셨어요??",
          "original_form": "점심에 뭐드셨어요??",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100013241.1.6",
          "form": "오 우동 좋죠ㅎㅎ 저는 버터 간계밥 먹었어요!",
          "original_form": "오 우동 좋죠ㅎㅎ 저는 버터 간계밥 먹었어요!",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013241.1.7",
          "form": "맛있었겠네요 ㅎㅎㅎ",
          "original_form": "맛있었겠네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013241.1.8",
          "form": "후식은 뭐 드셨어요??",
          "original_form": "후식은 뭐 드셨어요??",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013241.1.9",
          "form": "후식은 마들렌 먹었어요!",
          "original_form": "후식은 마들렌 먹었어요!",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100013241.1.10",
          "form": "마들렌 맛있죠 !!ㅋㅋ 저도 오늘 마들렌먹으러고요.",
          "original_form": "마들렌 맛있죠 !!ㅋㅋ 저도 오늘 마들렌먹으러고요.",
          "speaker_id": "2",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013241.1.11",
          "form": "기본맛으로요 ㅎㅎ",
          "original_form": "기본맛으로요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013241.1.12",
          "form": "직접구워볼려고요 ㅋㅋ",
          "original_form": "직접구워볼려고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013241.1.13",
          "form": "오ㅋㅋㅋ 멋지시네요 레시피는 어떤 분 거 보고 하시나요?",
          "original_form": "오ㅋㅋㅋ 멋지시네요 레시피는 어떤 분 거 보고 하시나요?",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013241.1.14",
          "form": "유투브에서 쭈야공님 레시피 참고해서 구우려고요",
          "original_form": "유투브에서 쭈야공님 레시피 참고해서 구우려고요",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013241.1.15",
          "form": "동생이랑 엄마가 좋아하시거라고요ㅠㅎㅎ",
          "original_form": "동생이랑 엄마가 좋아하시거라고요ㅠㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013241.1.16",
          "form": "이번엔 너무 오래 냉장해둬서 붕어빵같아질것같아서아쉬워요",
          "original_form": "이번엔 너무 오래 냉장해둬서 붕어빵같아질것같아서아쉬워요",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013241.1.17",
          "form": "아이고...ㅠㅠㅠ 지금 만들고 계신 건가요??",
          "original_form": "아이고...ㅠㅠㅠ 지금 만들고 계신 건가요??",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013241.1.18",
          "form": "아니용 ㅋㅋ반죽을 틀에담고 냉장고에 뒀어요 ㅎㅎ 이제 구워야해요",
          "original_form": "아니용 ㅋㅋ반죽을 틀에담고 냉장고에 뒀어요 ㅎㅎ 이제 구워야해요",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013241.1.19",
          "form": "아하 그렇군요ㅋㅋㅋ",
          "original_form": "아하 그렇군요ㅋㅋㅋ",
