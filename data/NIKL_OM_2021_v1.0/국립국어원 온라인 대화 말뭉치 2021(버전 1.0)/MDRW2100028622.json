          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028622.1.3",
          "form": "여행 어디 가고싶으신가요?",
          "original_form": "여행 어디 가고싶으신가요?",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028622.1.4",
          "form": "저는 딱히..가고싶은곳 있나요?",
          "original_form": "저는 딱히..가고싶은곳 있나요?",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028622.1.5",
          "form": "애들이랑 제주도 가고싶어요",
          "original_form": "애들이랑 제주도 가고싶어요",
          "speaker_id": "2",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100028622.1.6",
          "form": "비행기타고ㅎㅎ",
          "original_form": "비행기타고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100028622.1.7",
          "form": "제주도는 금방이죠 날만 잡으세요",
          "original_form": "제주도는 금방이죠 날만 잡으세요",
          "speaker_id": "1",
          "time": "20210903 07:27"
        },
        {
          "id": "MDRW2100028622.1.8",
          "form": "요즘 사람들 너무 많이 가서 안 돼요ㅋㅋ",
          "original_form": "요즘 사람들 너무 많이 가서 안 돼요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100028622.1.9",
          "form": "코로나 2차 맞고 가면되죠",
          "original_form": "코로나 2차 맞고 가면되죠",
          "speaker_id": "1",
          "time": "20210903 07:29"
        },
        {
          "id": "MDRW2100028622.1.10",
          "form": "2차 안 아프던대요",
          "original_form": "2차 안 아프던대요",
          "speaker_id": "1",
          "time": "20210903 07:29"
        },
        {
          "id": "MDRW2100028622.1.11",
          "form": "ㅠㅠ애들은 안맞으니",
          "original_form": "ㅠㅠ애들은 안맞으니",
          "speaker_id": "2",
          "time": "20210903 07:30"
        },
        {
          "id": "MDRW2100028622.1.12",
          "form": "아.....코로나 끝나야 뭐가 되겠네요",
          "original_form": "아.....코로나 끝나야 뭐가 되겠네요",
          "speaker_id": "1",
          "time": "20210903 07:42"
        },
        {
          "id": "MDRW2100028622.1.13",
          "form": "네ㅠㅠ 비행기타면 좋아할텐데",
          "original_form": "네ㅠㅠ 비행기타면 좋아할텐데",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100028622.1.14",
          "form": "조만간 타야겠군요",
          "original_form": "조만간 타야겠군요",
