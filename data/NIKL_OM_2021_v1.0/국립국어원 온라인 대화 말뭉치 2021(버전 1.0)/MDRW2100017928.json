        },
        {
          "id": "MDRW2100017928.1.2",
          "form": "샌드우ㅣ치사옴",
          "original_form": "샌드우ㅣ치사옴",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017928.1.3",
          "form": "나 근데 씻고와야됔ㅋㅋㅋㅋ",
          "original_form": "나 근데 씻고와야됔ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017928.1.4",
          "form": "이제먹는거야??!",
          "original_form": "이제먹는거야??!",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017928.1.5",
          "form": "먹고 씻기 씻고 먹기?",
          "original_form": "먹고 씻기 씻고 먹기?",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017928.1.6",
          "form": "yes..먹고늦게자야지..",
          "original_form": "yes..먹고늦게자야지..",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017928.1.7",
          "form": "씻먹..^^",
          "original_form": "씻먹..^^",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017928.1.8",
          "form": "무슨 샌드위치야?",
          "original_form": "무슨 샌드위치야?",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017928.1.9",
          "form": "걍 햄치즈..?",
          "original_form": "걍 햄치즈..?",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100017928.1.10",
          "form": "그냥구랬어",
          "original_form": "그냥구랬어",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100017928.1.11",
          "form": "어디서샀어?",
          "original_form": "어디서샀어?",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100017928.1.12",
          "form": "씨유!",
          "original_form": "씨유!",
          "speaker_id": "2",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100017928.1.13",
          "form": "지에스가 더 나은듯",
          "original_form": "지에스가 더 나은듯",
          "speaker_id": "2",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100017928.1.14",
          "form": "그럼 다음에 나도 지에스먹어야겠다ㅋ 디저트는 씨유가 낫다고 들어본 것 같아",
          "original_form": "그럼 다음에 나도 지에스먹어야겠다ㅋ 디저트는 씨유가 낫다고 들어본 것 같아",
          "speaker_id": "1",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100017928.1.15",
          "form": "ㅁㅈㅁㅈ마카롱도 씨유가 낫대",
          "original_form": "ㅁㅈㅁㅈ마카롱도 씨유가 낫대",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100017928.1.16",
          "form": "마칼오 다음에 먹어봐야겠다",
          "original_form": "마칼오 다음에 먹어봐야겠다",
          "speaker_id": "1",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100017928.1.17",
          "form": "마카롱~~ 너 안먹어봤어?",
          "original_form": "마카롱~~ 너 안먹어봤어?",
          "speaker_id": "1",
          "time": "20210901 14:32"
        },
        {
          "id": "MDRW2100017928.1.18",
          "form": "엄청 예전에 먹어봤는데",
          "original_form": "엄청 예전에 먹어봤는데",
