      "utterance": [
        {
          "id": "MDRW2100036075.1.1",
          "form": "여행 많이 다니세요?",
          "original_form": "여행 많이 다니세요?",
          "speaker_id": "1",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100036075.1.2",
          "form": "저는 여행을 많이는 안 가는 편인 것 같아요",
          "original_form": "저는 여행을 많이는 안 가는 편인 것 같아요",
          "speaker_id": "2",
          "time": "20210830 15:53"
        },
        {
          "id": "MDRW2100036075.1.3",
          "form": "코로나땜에 못간지 오래됐어요",
          "original_form": "코로나땜에 못간지 오래됐어요",
          "speaker_id": "1",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100036075.1.4",
          "form": "가고 싶은 여행지 있으세요?",
          "original_form": "가고 싶은 여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100036075.1.5",
          "form": "전 싱가포르여",
          "original_form": "전 싱가포르여",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100036075.1.6",
          "form": "깨끗하고 쇼핑하기좋대요",
          "original_form": "깨끗하고 쇼핑하기좋대요",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100036075.1.7",
          "form": "아~싱가포르!! 풍경도 멋있을것 같아요",
          "original_form": "아~싱가포르!! 풍경도 멋있을것 같아요",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100036075.1.8",
          "form": "네 다들 좋다고 하더라구요",
          "original_form": "네 다들 좋다고 하더라구요",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100036075.1.9",
          "form": "싱가포르에 그 유명한 멀라이언상 있잖아요!",
          "original_form": "싱가포르에 그 유명한 멀라이언상 있잖아요!",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100036075.1.10",
          "form": "앗 그런가요?",
          "original_form": "앗 그런가요?",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036075.1.11",
          "form": "쇼핑만생각했네요ㅎ",
          "original_form": "쇼핑만생각했네요ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036075.1.12",
          "form": "네~사자머리인데 입에서 분수 나오는거요~",
          "original_form": "네~사자머리인데 입에서 분수 나오는거요~",
          "speaker_id": "2",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036075.1.13",
          "form": "아 그거 알아요ㅋ",
          "original_form": "아 그거 알아요ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036075.1.14",
          "form": "생각났네요ㅎ",
          "original_form": "생각났네요ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036075.1.15",
          "form": "네~~거기서 사진 많이 찍잖아요 ㅎ",
          "original_form": "네~~거기서 사진 많이 찍잖아요 ㅎ",
