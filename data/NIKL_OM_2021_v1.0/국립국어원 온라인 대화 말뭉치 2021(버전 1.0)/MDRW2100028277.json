      "utterance": [
        {
          "id": "MDRW2100028277.1.1",
          "form": "요즘 무슨 커뮤니티 하니?",
          "original_form": "요즘 무슨 커뮤니티 하니?",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028277.1.2",
          "form": "트위터 해요!!",
          "original_form": "트위터 해요!!",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028277.1.3",
          "form": "뭐가 재미있니",
          "original_form": "뭐가 재미있니",
          "speaker_id": "1",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028277.1.4",
          "form": "친구들이랑 얘기하는게 좋아요 ㅋㅋㅋ",
          "original_form": "친구들이랑 얘기하는게 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028277.1.5",
          "form": "관심사 맞는 사람들하고 놀면 좋죠",
          "original_form": "관심사 맞는 사람들하고 놀면 좋죠",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028277.1.6",
          "form": "엄마는요?",
          "original_form": "엄마는요?",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028277.1.7",
          "form": "엄마는 네이버 밴드 해",
          "original_form": "엄마는 네이버 밴드 해",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028277.1.8",
          "form": "밴드에서 친구 사귄단다",
          "original_form": "밴드에서 친구 사귄단다",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028277.1.9",
          "form": "요즘 나가시는 그거예요? 광양에도 사람 있나요?",
          "original_form": "요즘 나가시는 그거예요? 광양에도 사람 있나요?",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028277.1.10",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028277.1.11",
          "form": "사람 사는 곳인걸",
          "original_form": "사람 사는 곳인걸",
          "speaker_id": "1",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100028277.1.12",
          "form": "오 신기해요 ㅎㅎ",
          "original_form": "오 신기해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028277.1.13",
          "form": "뜨개질 재밌단다",
          "original_form": "뜨개질 재밌단다",
          "speaker_id": "1",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028277.1.14",
          "form": "같이 뜨개질 할래?",
          "original_form": "같이 뜨개질 할래?",
          "speaker_id": "1",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100028277.1.15",
          "form": "저 뜨개질 어렵더라구요 ㅠㅠ",
          "original_form": "저 뜨개질 어렵더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100028277.1.16",
          "form": "엄마가 알려줄게",
          "original_form": "엄마가 알려줄게",
          "speaker_id": "1",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100028277.1.17",
          "form": "코바늘만 조금 할수있어요",
          "original_form": "코바늘만 조금 할수있어요",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100028277.1.18",
          "form": "엄마는 대바늘 전문인데..",
          "original_form": "엄마는 대바늘 전문인데..",
          "speaker_id": "1",
          "time": "20210902 18:55"
        }
