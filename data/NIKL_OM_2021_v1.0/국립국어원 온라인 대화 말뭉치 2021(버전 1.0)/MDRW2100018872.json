      "utterance": [
        {
          "id": "MDRW2100018872.1.1",
          "form": "그렇긴한데",
          "original_form": "그렇긴한데",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018872.1.2",
          "form": "여름의 더위에 비할건 못 되는거 같아요",
          "original_form": "여름의 더위에 비할건 못 되는거 같아요",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018872.1.3",
          "form": "겨울은 정기장판이라는.. 구세주가 있어서",
          "original_form": "겨울은 정기장판이라는.. 구세주가 있어서",
          "speaker_id": "2",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018872.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그건 좋은 장점이죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그건 좋은 장점이죠",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018872.1.5",
          "form": "근데 저도 여름보다는 겨울이에요",
          "original_form": "근데 저도 여름보다는 겨울이에요",
          "speaker_id": "1",
          "time": "20210827 13:12"
        },
        {
          "id": "MDRW2100018872.1.6",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018872.1.7",
          "form": "그래도 여름 장점하면..",
          "original_form": "그래도 여름 장점하면..",
          "speaker_id": "2",
          "time": "20210827 13:14"
        },
        {
          "id": "MDRW2100018872.1.8",
          "form": "생각나는게..",
          "original_form": "생각나는게..",
          "speaker_id": "2",
          "time": "20210827 13:14"
        },
        {
          "id": "MDRW2100018872.1.9",
          "form": "어.. (어떻게든 여름을 미화시키려 노력중)",
          "original_form": "어.. (어떻게든 여름을 미화시키려 노력중)",
          "speaker_id": "2",
          "time": "20210827 13:14"
        },
        {
          "id": "MDRW2100018872.1.10",
          "form": "... 그냥.. 날 좋은...",
          "original_form": "... 그냥.. 날 좋은...",
          "speaker_id": "1",
          "time": "20210827 13:14"
        },
        {
          "id": "MDRW2100018872.1.11",
          "form": "아니면 물놀이...",
          "original_form": "아니면 물놀이...",
          "speaker_id": "1",
          "time": "20210827 13:14"
        },
        {
          "id": "MDRW2100018872.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018872.1.13",
          "form": "우리 노력하지말아요..",
          "original_form": "우리 노력하지말아요..",
          "speaker_id": "2",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018872.1.14",
          "form": "여름은.. 더위라는.. 단점이.. 너무 강력",
          "original_form": "여름은.. 더위라는.. 단점이.. 너무 강력",
          "speaker_id": "2",
          "time": "20210827 13:16"
        },
        {
          "id": "MDRW2100018872.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그렇죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그렇죠",
          "speaker_id": "1",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018872.1.16",
          "form": "그럼 사계절 중 최애가",
          "original_form": "그럼 사계절 중 최애가",
          "speaker_id": "1",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018872.1.17",
          "form": "겨울이에요?",
          "original_form": "겨울이에요?",
          "speaker_id": "1",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018872.1.18",
          "form": "아무래도요?",
          "original_form": "아무래도요?",
          "speaker_id": "2",
          "time": "20210827 13:18"
        },
        {
          "id": "MDRW2100018872.1.19",
          "form": "근데 겨울도 단점이 있어요",
          "original_form": "근데 겨울도 단점이 있어요",
          "speaker_id": "2",
          "time": "20210827 13:18"
        },
        {
          "id": "MDRW2100018872.1.20",
          "form": "눈오면 미끄러워서",
          "original_form": "눈오면 미끄러워서",
          "speaker_id": "2",
          "time": "20210827 13:18"
        },
        {
          "id": "MDRW2100018872.1.21",
          "form": "눈은 안좋아합니다",
          "original_form": "눈은 안좋아합니다",
          "speaker_id": "2",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100018872.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 전 눈 좋은데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 전 눈 좋은데",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018872.1.23",
          "form": "눈 좋아하시나요",
          "original_form": "눈 좋아하시나요",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018872.1.24",
          "form": "더러워지는게 싫어요",
          "original_form": "더러워지는게 싫어요",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018872.1.25",
          "form": "더러운 눈..",
          "original_form": "더러운 눈..",
          "speaker_id": "1",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018872.1.26",
          "form": "음 맞아요",
          "original_form": "음 맞아요",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018872.1.27",
          "form": "지저분해지는거 좀 별로...",
          "original_form": "지저분해지는거 좀 별로...",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018872.1.28",
          "form": "그것만 빼면 좋아여",
          "original_form": "그것만 빼면 좋아여",
          "speaker_id": "1",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100018872.1.29",
          "form": "또 추운 거 빼면..",
          "original_form": "또 추운 거 빼면..",
          "speaker_id": "1",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100018872.1.30",
          "form": "그럼 좋은 이유가 없는거네요ㅋㅋㅋㅋ",
          "original_form": "그럼 좋은 이유가 없는거네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100018872.1.31",
          "form": "아 웃기다",
          "original_form": "아 웃기다",
          "speaker_id": "2",
          "time": "20210827 18:10"
        }
      ]
    }
