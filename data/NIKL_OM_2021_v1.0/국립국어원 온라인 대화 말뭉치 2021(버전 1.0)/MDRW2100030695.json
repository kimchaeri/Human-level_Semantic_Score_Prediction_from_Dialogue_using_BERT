      "utterance": [
        {
          "id": "MDRW2100030695.1.1",
          "form": "짠 음식 좋아하는데 궁금하네요",
          "original_form": "짠 음식 좋아하는데 궁금하네요",
          "speaker_id": "2",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030695.1.2",
          "form": "내일 고모가 나가신 김에 예 감사 오셨어요",
          "original_form": "내일 고모가 나가신 김에 예 감사 오셨어요",
          "speaker_id": "2",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030695.1.3",
          "form": "아 언제나 먹어 볼 수 있을까요",
          "original_form": "아 언제나 먹어 볼 수 있을까요",
          "speaker_id": "1",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030695.1.4",
          "form": "예감 감자 과자",
          "original_form": "예감 감자 과자",
          "speaker_id": "2",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030695.1.5",
          "form": "반미도 좋아합니다",
          "original_form": "반미도 좋아합니다",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030695.1.6",
          "form": "드셔 보셨나요",
          "original_form": "드셔 보셨나요",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030695.1.7",
          "form": "코로나가 조금 괜찮아지면 언제든 가서 먹을 수 있지 않을까요",
          "original_form": "코로나가 조금 괜찮아지면 언제든 가서 먹을 수 있지 않을까요",
          "speaker_id": "2",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030695.1.8",
          "form": "안미는 못 들어봤어요 어떤 맛이죠",
          "original_form": "안미는 못 들어봤어요 어떤 맛이죠",
          "speaker_id": "2",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030695.1.9",
          "form": "차비가 비싸요",
          "original_form": "차비가 비싸요",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030695.1.10",
          "form": "바게트 샌드위치입니다",
          "original_form": "바게트 샌드위치입니다",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030695.1.11",
          "form": "고명을 다양하게 넣을 수 있어요",
          "original_form": "고명을 다양하게 넣을 수 있어요",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030695.1.12",
          "form": "바게트 샌드위치 라니 정말 쩝쩝 박사님들이군요",
          "original_form": "바게트 샌드위치 라니 정말 쩝쩝 박사님들이군요",
          "speaker_id": "2",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030695.1.13",
          "form": "제가 제일 좋아했던 반미는",
          "original_form": "제가 제일 좋아했던 반미는",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030695.1.14",
          "form": "오뎅반미에요",
          "original_form": "오뎅반미에요",
          "speaker_id": "1",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030695.1.15",
          "form": "오뎅이랑 닭고기를 넣어 주었어요",
          "original_form": "오뎅이랑 닭고기를 넣어 주었어요",
          "speaker_id": "1",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030695.1.16",
          "form": "오뎅 정말 맛이 있죠",
          "original_form": "오뎅 정말 맛이 있죠",
          "speaker_id": "2",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030695.1.17",
          "form": "땡초 넣고요",
          "original_form": "땡초 넣고요",
          "speaker_id": "1",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030695.1.18",
          "form": "마요네즈도 넣고요",
          "original_form": "마요네즈도 넣고요",
          "speaker_id": "1",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030695.1.19",
          "form": "오뎅을 면처럼 만들어서 국수로 삶아 먹는 거 있었는데 그것도 참 맛있었죠",
          "original_form": "오뎅을 면처럼 만들어서 국수로 삶아 먹는 거 있었는데 그것도 참 맛있었죠",
          "speaker_id": "2",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030695.1.20",
          "form": "고수도 들어 있었어요",
          "original_form": "고수도 들어 있었어요",
          "speaker_id": "1",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030695.1.21",
          "form": "들어가 매콤한 음식을 먹고 싶네요",
          "original_form": "들어가 매콤한 음식을 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030695.1.22",
          "form": "베트남 고추가 정말 매워요",
          "original_form": "베트남 고추가 정말 매워요",
          "speaker_id": "1",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030695.1.23",
          "form": "고수는 잘 못 먹어요 먹을 순 있는데 입맛에 안 맞아요",
          "original_form": "고수는 잘 못 먹어요 먹을 순 있는데 입맛에 안 맞아요",
          "speaker_id": "2",
          "time": "20210904 11:32"
        }
      ]
    }
