      "utterance": [
        {
          "id": "MDRW2100019841.1.1",
          "form": "응. 지금 선선하다",
          "original_form": "응. 지금 선선하다",
          "speaker_id": "2",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019841.1.2",
          "form": "밤에는 추울꺼같아",
          "original_form": "밤에는 추울꺼같아",
          "speaker_id": "1",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019841.1.3",
          "form": "응. 창문 쫌 닫고 자야할꺼같아",
          "original_form": "응. 창문 쫌 닫고 자야할꺼같아",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019841.1.4",
          "form": "내일은 비 안오겠지?",
          "original_form": "내일은 비 안오겠지?",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019841.1.5",
          "form": "일기예보안봐서 모르겠어",
          "original_form": "일기예보안봐서 모르겠어",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019841.1.6",
          "form": "지금봐서는 안올꺼같긴해",
          "original_form": "지금봐서는 안올꺼같긴해",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019841.1.7",
          "form": "비그만오고 해좀 나왔으면...넘 습해",
          "original_form": "비그만오고 해좀 나왔으면...넘 습해",
          "speaker_id": "2",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019841.1.8",
          "form": "끈적거리는게 나도 싫다",
          "original_form": "끈적거리는게 나도 싫다",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019841.1.9",
          "form": "빨래도 잘 안말라",
          "original_form": "빨래도 잘 안말라",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019841.1.10",
          "form": "냄새도 나고",
          "original_form": "냄새도 나고",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019841.1.11",
          "form": "그치? 건조기가 필요하지?",
          "original_form": "그치? 건조기가 필요하지?",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019841.1.12",
          "form": "아니. 제습기 물 자주 버리면 괜찮아",
          "original_form": "아니. 제습기 물 자주 버리면 괜찮아",
          "speaker_id": "1",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019841.1.13",
          "form": "ㅋ버려주고 말이나 하던지 ㅡㅡ",
          "original_form": "ㅋ버려주고 말이나 하던지 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019841.1.14",
          "form": "갑자기 추워진거같아",
          "original_form": "갑자기 추워진거같아",
