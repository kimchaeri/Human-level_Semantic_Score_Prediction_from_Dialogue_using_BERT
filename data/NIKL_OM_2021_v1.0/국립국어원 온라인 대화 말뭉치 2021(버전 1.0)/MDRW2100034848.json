      "utterance": [
        {
          "id": "MDRW2100034848.1.1",
          "form": "후식 드셨어요?",
          "original_form": "후식 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100034848.1.2",
          "form": "초코아몬드 먹었어요ㅎㅎ",
          "original_form": "초코아몬드 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100034848.1.3",
          "form": "그거 맛있죵ㅎㅎ",
          "original_form": "그거 맛있죵ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100034848.1.4",
          "form": "저는 포도 먹었어요!",
          "original_form": "저는 포도 먹었어요!",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100034848.1.5",
          "form": "포도 맛있으셨겠어여!! 올해는 아직 별로 못 먹었네요ㅠ",
          "original_form": "포도 맛있으셨겠어여!! 올해는 아직 별로 못 먹었네요ㅠ",
          "speaker_id": "1",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100034848.1.6",
          "form": "저도 오늘 처음이였어요!",
          "original_form": "저도 오늘 처음이였어요!",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034848.1.7",
          "form": "올해는 복숭아만 왕창 먹은거 같아요",
          "original_form": "올해는 복숭아만 왕창 먹은거 같아요",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034848.1.8",
          "form": "올해 복숭아 맛있었어요ㅋㅋㅋ 그릭복숭아 드셔보셨나여?",
          "original_form": "올해 복숭아 맛있었어요ㅋㅋㅋ 그릭복숭아 드셔보셨나여?",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100034848.1.9",
          "form": "아 그거 못먹어봤어요ㅜㅜㅠ",
          "original_form": "아 그거 못먹어봤어요ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034848.1.10",
          "form": "너무 물렁거려서 씨만 못빼겠더라구요ㅜㅜㅜ",
          "original_form": "너무 물렁거려서 씨만 못빼겠더라구요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034848.1.11",
          "form": "물컹여서 씨. 빼기 힘들더라구요ㅜㅜ",
          "original_form": "물컹여서 씨. 빼기 힘들더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034848.1.12",
          "form": "저는 안 해봤는데 나중에 카페 가서 먹어보는 게 낫겠네요...",
          "original_form": "저는 안 해봤는데 나중에 카페 가서 먹어보는 게 낫겠네요...",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100034848.1.13",
          "form": "네ㅜㅠㅜ 아 파는곳도 있나요??",
          "original_form": "네ㅜㅠㅜ 아 파는곳도 있나요??",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100034848.1.14",
          "form": "처음에 인스타 감성 카페? 에서 나온 레시피라고 알고 있어요!",
          "original_form": "처음에 인스타 감성 카페? 에서 나온 레시피라고 알고 있어요!",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100034848.1.15",
          "form": "오.. 저는 개인레시피인줄 알았어요",
          "original_form": "오.. 저는 개인레시피인줄 알았어요",
          "speaker_id": "2",
          "time": "20210829 08:01"
        },
        {
          "id": "MDRW2100034848.1.16",
          "form": "개인레시피를 카페에서 따라했을 수도 있을 것 같네요! 워낙 선후관계가 불분명해서요ㅠ",
          "original_form": "개인레시피를 카페에서 따라했을 수도 있을 것 같네요! 워낙 선후관계가 불분명해서요ㅠ",
          "speaker_id": "1",
          "time": "20210829 12:21"
        },
        {
          "id": "MDRW2100034848.1.17",
          "form": "그쵸ㅜ",
          "original_form": "그쵸ㅜ",
          "speaker_id": "2",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100034848.1.18",
          "form": "인터넷이 발달하다보니 그런문제도 클것같아오ㅜ",
          "original_form": "인터넷이 발달하다보니 그런문제도 클것같아오ㅜ",
          "speaker_id": "2",
          "time": "20210829 12:22"
        }
      ]
