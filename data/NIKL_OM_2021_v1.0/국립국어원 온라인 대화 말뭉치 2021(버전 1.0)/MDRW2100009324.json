      "utterance": [
        {
          "id": "MDRW2100009324.1.1",
          "form": "이번주 내내 비온대",
          "original_form": "이번주 내내 비온대",
          "speaker_id": "1",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100009324.1.2",
          "form": "태풍오나?",
          "original_form": "태풍오나?",
          "speaker_id": "1",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100009324.1.3",
          "form": "아니 지금 태풍 안와",
          "original_form": "아니 지금 태풍 안와",
          "speaker_id": "2",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100009324.1.4",
          "form": "비가 많이 오네",
          "original_form": "비가 많이 오네",
          "speaker_id": "1",
          "time": "20210823 16:10"
        },
        {
          "id": "MDRW2100009324.1.5",
          "form": "비만 와",
          "original_form": "비만 와",
          "speaker_id": "2",
          "time": "20210823 16:11"
        },
        {
          "id": "MDRW2100009324.1.6",
          "form": "낼 우산 들고 가",
          "original_form": "낼 우산 들고 가",
          "speaker_id": "1",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100009324.1.7",
          "form": "양산도 우산이야",
          "original_form": "양산도 우산이야",
          "speaker_id": "2",
          "time": "20210823 16:12"
        },
        {
          "id": "MDRW2100009324.1.8",
          "form": "비바람 많이불면 양산 부러지는거 아냐?",
          "original_form": "비바람 많이불면 양산 부러지는거 아냐?",
          "speaker_id": "1",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100009324.1.9",
          "form": "조심해",
          "original_form": "조심해",
          "speaker_id": "1",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100009324.1.10",
          "form": "안부러져",
          "original_form": "안부러져",
          "speaker_id": "2",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100009324.1.11",
          "form": "비와서 빨래는 못돌리겠다",
          "original_form": "비와서 빨래는 못돌리겠다",
          "speaker_id": "1",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100009324.1.12",
          "form": "언제 그칠까?",
          "original_form": "언제 그칠까?",
          "speaker_id": "1",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100009324.1.13",
          "form": "언제 그칠지 모르겠다",
          "original_form": "언제 그칠지 모르겠다",
          "speaker_id": "2",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100009324.1.14",
          "form": "장화살까?",
          "original_form": "장화살까?",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100009324.1.15",
          "form": "사고 싶지만 둘 자리가 없어",
          "original_form": "사고 싶지만 둘 자리가 없어",
          "speaker_id": "2",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100009324.1.16",
          "form": "너무 길어서",
          "original_form": "너무 길어서",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100009324.1.17",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100009324.1.18",
          "form": "나중에 큰 신발장 사면 되겠다",
          "original_form": "나중에 큰 신발장 사면 되겠다",
          "speaker_id": "1",
          "time": "20210823 16:16"
        },
        {
          "id": "MDRW2100009324.1.19",
          "form": "낼도 비오지?",
          "original_form": "낼도 비오지?",
          "speaker_id": "1",
          "time": "20210823 16:16"
        },
        {
          "id": "MDRW2100009324.1.20",
          "form": "큰 신발장 어디에 놔둬?",
          "original_form": "큰 신발장 어디에 놔둬?",
          "speaker_id": "2",
          "time": "20210823 16:16"
        },
        {
          "id": "MDRW2100009324.1.21",
          "form": "낼 비 올걸",
          "original_form": "낼 비 올걸",
          "speaker_id": "2",
          "time": "20210823 16:17"
        },
        {
          "id": "MDRW2100009324.1.22",
          "form": "조심히 갔다 와",
          "original_form": "조심히 갔다 와",
          "speaker_id": "1",
          "time": "20210823 16:17"
        }
      ]
