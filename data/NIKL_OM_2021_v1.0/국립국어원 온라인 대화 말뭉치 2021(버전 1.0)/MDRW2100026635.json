      "utterance": [
        {
          "id": "MDRW2100026635.1.1",
          "form": "신기하다 젓갈 조아하세요?",
          "original_form": "신기하다 젓갈 조아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026635.1.2",
          "form": "아뇨ㅋ 저는 초딩입맛이라 오징어젓만 먹을수있어요ㅋ",
          "original_form": "아뇨ㅋ 저는 초딩입맛이라 오징어젓만 먹을수있어요ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026635.1.3",
          "form": "근데 그게 오징어 젓이었어요  ㅎ",
          "original_form": "근데 그게 오징어 젓이었어요  ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026635.1.4",
          "form": "대전은 뭐가 맛난가요?",
          "original_form": "대전은 뭐가 맛난가요?",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026635.1.5",
          "form": "아하 ㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026635.1.6",
          "form": "대전은 성심당이요 ㅎㅎㅎ",
          "original_form": "대전은 성심당이요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026635.1.7",
          "form": "저 진짜 어렸을때 꿈돌이 보러가고 못가본거 같아여",
          "original_form": "저 진짜 어렸을때 꿈돌이 보러가고 못가본거 같아여",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026635.1.8",
          "form": "그리고 칼국수가많아요 ㅋㅋㅋ",
          "original_form": "그리고 칼국수가많아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026635.1.9",
          "form": "아 ㅋㅋㅋ 꿈돌이",
          "original_form": "아 ㅋㅋㅋ 꿈돌이",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026635.1.10",
          "form": "오 칼국수요?",
          "original_form": "오 칼국수요?",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026635.1.11",
          "form": "동년배시네",
          "original_form": "동년배시네",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026635.1.12",
          "form": "네 어딜가나 칼국수가 많아요 ㅋㅋㅋ",
          "original_form": "네 어딜가나 칼국수가 많아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026635.1.13",
          "form": "무슨칼국수 있가요?",
          "original_form": "무슨칼국수 있가요?",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026635.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ제나이가 뽀록났나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ제나이가 뽀록났나요",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026635.1.15",
          "form": "칼국수종류별로 다양 ㅋㅋㅋ",
          "original_form": "칼국수종류별로 다양 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026635.1.16",
          "form": "아주어렸을때라 기억이..",
          "original_form": "아주어렸을때라 기억이..",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026635.1.17",
          "form": "사골같은 칼국수도잇고",
          "original_form": "사골같은 칼국수도잇고",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026635.1.18",
          "form": "조개 같은해물 캉국수",
          "original_form": "조개 같은해물 캉국수",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026635.1.19",
          "form": "빨간양념칼국수",
          "original_form": "빨간양념칼국수",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026635.1.20",
          "form": "사골칼국수 못먹어 봤어요!",
          "original_form": "사골칼국수 못먹어 봤어요!",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026635.1.21",
          "form": "ㅋㅋㅋㅋㅋ 전 해물 맑은게젤 조아요",
          "original_form": "ㅋㅋㅋㅋㅋ 전 해물 맑은게젤 조아요",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026635.1.22",
          "form": "빨간양념도",
          "original_form": "빨간양념도",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026635.1.23",
          "form": "저두요ㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026635.1.24",
          "form": "오씨칼국수라고 잇는데",
          "original_form": "오씨칼국수라고 잇는데",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026635.1.25",
          "form": "칼칼하니맛나요",
          "original_form": "칼칼하니맛나요",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026635.1.26",
          "form": "물총이 조개 이름인가봐여",
          "original_form": "물총이 조개 이름인가봐여",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026635.1.27",
          "form": "우선 저장ㅋㅋ",
          "original_form": "우선 저장ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026635.1.28",
          "form": "대전도 가보고싶네여",
          "original_form": "대전도 가보고싶네여",
          "speaker_id": "2",
          "time": "20210901 23:27"
        }
      ]
    }
  ]
