      "utterance": [
        {
          "id": "MDRW2100032727.1.1",
          "form": "다이소에서 삿음?",
          "original_form": "다이소에서 삿음?",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032727.1.2",
          "form": "다이소에는안팔디싶은데 인터넷주문!",
          "original_form": "다이소에는안팔디싶은데 인터넷주문!",
          "speaker_id": "1",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032727.1.3",
          "form": "사선테이프 돌돌이치면나올거야",
          "original_form": "사선테이프 돌돌이치면나올거야",
          "speaker_id": "1",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100032727.1.4",
          "form": "정말 좋음",
          "original_form": "정말 좋음",
          "speaker_id": "1",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100032727.1.5",
          "form": "ㅇㅎ 그렇고먄 ㅋㅋㅋㅋ담번에 함 사본다",
          "original_form": "ㅇㅎ 그렇고먄 ㅋㅋㅋㅋ담번에 함 사본다",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100032727.1.6",
          "form": "뜯는 거 너무 어려움 진짜",
          "original_form": "뜯는 거 너무 어려움 진짜",
          "speaker_id": "2",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100032727.1.7",
          "form": "그리고 그 밀대같이 긴 작대기도 팔두라",
          "original_form": "그리고 그 밀대같이 긴 작대기도 팔두라",
          "speaker_id": "1",
          "time": "20210905 22:49"
        },
        {
          "id": "MDRW2100032727.1.8",
          "form": "그래서 밀대밀듯이돌돌이 쓸수잇음ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 밀대밀듯이돌돌이 쓸수잇음ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032727.1.9",
          "form": "ㅋㅋㅋ서서 쭉쭉 미는?",
          "original_form": "ㅋㅋㅋ서서 쭉쭉 미는?",
          "speaker_id": "2",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032727.1.10",
          "form": "ㅇㅇㅇ청소하기개퍈할듯ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅇ청소하기개퍈할듯ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032727.1.11",
          "form": "이것도 리스트업 한다 ㅋㅋㅋㅋ",
          "original_form": "이것도 리스트업 한다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032727.1.12",
          "form": "오날 벌써 3개나 햇다 ㅋㅋㅋㅋ",
          "original_form": "오날 벌써 3개나 햇다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032727.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:50"
        },
        {
          "id": "MDRW2100032727.1.14",
          "form": "설거지는 자주하나",
          "original_form": "설거지는 자주하나",
          "speaker_id": "1",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032727.1.15",
          "form": "설거지는 어쩔 수 옶이 하지..",
          "original_form": "설거지는 어쩔 수 옶이 하지..",
          "speaker_id": "2",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032727.1.16",
          "form": "미뤄둘 수 없어가지구..ㅋㅋㅋ",
          "original_form": "미뤄둘 수 없어가지구..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032727.1.17",
          "form": "그렇즤...",
          "original_form": "그렇즤...",
          "speaker_id": "1",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032727.1.18",
          "form": "그것도봣어 써보지는않앗는데",
          "original_form": "그것도봣어 써보지는않앗는데",
          "speaker_id": "1",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032727.1.19",
          "form": "요리하면 설거지랑 뒷정리 너무 힘들지 않나",
          "original_form": "요리하면 설거지랑 뒷정리 너무 힘들지 않나",
          "speaker_id": "2",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100032727.1.20",
          "form": "배수구 음식물찌꺼기모이는곳에",
          "original_form": "배수구 음식물찌꺼기모이는곳에",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032727.1.21",
          "form": "막같은 비닐 씌우는거?",
          "original_form": "막같은 비닐 씌우는거?",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032727.1.22",
          "form": "뭐되게편하다던데",
          "original_form": "뭐되게편하다던데",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032727.1.23",
          "form": "비닐?...을 씌우면 물은 어캐",
          "original_form": "비닐?...을 씌우면 물은 어캐",
          "speaker_id": "2",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032727.1.24",
          "form": "그뭐더라 물은 나가는거엿나..??",
          "original_form": "그뭐더라 물은 나가는거엿나..??",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100032727.1.25",
          "form": "담에발견하면 공유해주껰ㅋㅋㅋ",
          "original_form": "담에발견하면 공유해주껰ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100032727.1.26",
          "form": "ㅋㅋㅋㅋㅋ오킼ㅋㅋ근데 음쓰는 거의 안나옴..",
          "original_form": "ㅋㅋㅋㅋㅋ오킼ㅋㅋ근데 음쓰는 거의 안나옴..",
          "speaker_id": "2",
          "time": "20210905 22:53"
        }
      ]
    }
