      "utterance": [
        {
          "id": "MDRW2100043508.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043508.1.2",
          "form": "안녕하세요 저녁식사 하셨나요?",
          "original_form": "안녕하세요 저녁식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043508.1.3",
          "form": "가지 베이컨말이 해서 먹어보려고요",
          "original_form": "가지 베이컨말이 해서 먹어보려고요",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043508.1.4",
          "form": "와 메뉴 너무 좋은데요! 저는 배달도시락으로 오야코동을 먹었어요",
          "original_form": "와 메뉴 너무 좋은데요! 저는 배달도시락으로 오야코동을 먹었어요",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100043508.1.5",
          "form": "요리를 자주 하시나요?",
          "original_form": "요리를 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043508.1.6",
          "form": "간단하게 해먹는거 좋아해요",
          "original_form": "간단하게 해먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100043508.1.7",
          "form": "간단한 요리 배우고 싶어요! 저도 해먹는 걸 싫어하진 않는데 퇴근하고 나면 복잡한 요리 잘 안하게 되더라구요",
          "original_form": "간단한 요리 배우고 싶어요! 저도 해먹는 걸 싫어하진 않는데 퇴근하고 나면 복잡한 요리 잘 안하게 되더라구요",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043508.1.8",
          "form": "그건 맞아요 피곤해서 하기싫져",
          "original_form": "그건 맞아요 피곤해서 하기싫져",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043508.1.9",
          "form": "혹시 피곤해도 이런건 해먹을 만해 하는 메뉴 있으신가요?",
          "original_form": "혹시 피곤해도 이런건 해먹을 만해 하는 메뉴 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043508.1.10",
          "form": "전 물말아서 김치랑 먹는 게 전부인 것 같아요 ㅋㅋ",
          "original_form": "전 물말아서 김치랑 먹는 게 전부인 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043508.1.11",
          "form": "그것도맛잇져\r\n전 냉동식품 마니사놔요ㅋㅋ",
          "original_form": "그것도맛잇져\r\n전 냉동식품 마니사놔요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043508.1.12",
          "form": "오 저도 냉동 도시락 몇 개 사두긴 했어요",
          "original_form": "오 저도 냉동 도시락 몇 개 사두긴 했어요",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043508.1.13",
          "form": "어떤 냉동식품 주로 사세요?",
          "original_form": "어떤 냉동식품 주로 사세요?",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043508.1.14",
          "form": "쭈꾸미도 잇고요\r\n돈가스같은것도 잇고요",
          "original_form": "쭈꾸미도 잇고요\r\n돈가스같은것도 잇고요",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043508.1.15",
          "form": "오 돈가스는 사봤는데 쭈꾸미도 파는 줄은 몰랐어요",
          "original_form": "오 돈가스는 사봤는데 쭈꾸미도 파는 줄은 몰랐어요",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043508.1.16",
          "form": "볶음 상태로 파나요?",
          "original_form": "볶음 상태로 파나요?",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043508.1.17",
          "form": "양념상태요 볶아먹는거에요",
          "original_form": "양념상태요 볶아먹는거에요",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043508.1.18",
          "form": "아하 양념 쭈꾸미도 검색해봐야겠네요",
          "original_form": "아하 양념 쭈꾸미도 검색해봐야겠네요",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043508.1.19",
          "form": "쭈꾸미만도 볶고 삼겹살이랑도 볶고 하면 편리하면서도 다양하게 먹을 수 있겠어요",
          "original_form": "쭈꾸미만도 볶고 삼겹살이랑도 볶고 하면 편리하면서도 다양하게 먹을 수 있겠어요",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043508.1.20",
          "form": "맛아요 안질기고 맛잇어요",
          "original_form": "맛아요 안질기고 맛잇어요",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043508.1.21",
          "form": "쭈삼 맛잇죠",
          "original_form": "쭈삼 맛잇죠",
          "speaker_id": "1",
          "time": "20210907 19:49"
        }
      ]
    }
