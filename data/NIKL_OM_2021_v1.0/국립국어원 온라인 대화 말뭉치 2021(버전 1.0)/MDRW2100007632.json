      "utterance": [
        {
          "id": "MDRW2100007632.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 08:49"
        },
        {
          "id": "MDRW2100007632.1.2",
          "form": "집안일 당연히 하죠. 주로 설거지 청소",
          "original_form": "집안일 당연히 하죠. 주로 설거지 청소",
          "speaker_id": "2",
          "time": "20210822 08:50"
        },
        {
          "id": "MDRW2100007632.1.3",
          "form": "주로 어떤 집안일 하세요?",
          "original_form": "주로 어떤 집안일 하세요?",
          "speaker_id": "2",
          "time": "20210822 08:50"
        },
        {
          "id": "MDRW2100007632.1.4",
          "form": "똑같네요",
          "original_form": "똑같네요",
          "speaker_id": "1",
          "time": "20210822 08:51"
        },
        {
          "id": "MDRW2100007632.1.5",
          "form": "가족들과 함께 사시나요",
          "original_form": "가족들과 함께 사시나요",
          "speaker_id": "2",
          "time": "20210822 08:52"
        },
        {
          "id": "MDRW2100007632.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 08:52"
        },
        {
          "id": "MDRW2100007632.1.7",
          "form": "그럼 가사분담은 어떻게 하시나요",
          "original_form": "그럼 가사분담은 어떻게 하시나요",
          "speaker_id": "2",
          "time": "20210822 08:53"
        },
        {
          "id": "MDRW2100007632.1.8",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210822 08:54"
        },
        {
          "id": "MDRW2100007632.1.9",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210822 09:06"
        },
        {
          "id": "MDRW2100007632.1.10",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210822 09:09"
        },
        {
          "id": "MDRW2100007632.1.11",
          "form": "주로 부엌일을 합니다. 어머니 따라서 보조일이지만요",
          "original_form": "주로 부엌일을 합니다. 어머니 따라서 보조일이지만요",
          "speaker_id": "2",
          "time": "20210822 09:16"
        },
        {
          "id": "MDRW2100007632.1.12",
          "form": "집에서 로봇청소기 쓰시나요?",
          "original_form": "집에서 로봇청소기 쓰시나요?",
          "speaker_id": "2",
          "time": "20210822 09:16"
        },
        {
          "id": "MDRW2100007632.1.13",
          "form": "그러면 한결 편하다던데",
          "original_form": "그러면 한결 편하다던데",
          "speaker_id": "2",
          "time": "20210822 09:16"
        },
        {
          "id": "MDRW2100007632.1.14",
          "form": "아직 살 돈이 ㅋㅋ",
          "original_form": "아직 살 돈이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 09:17"
        },
        {
          "id": "MDRW2100007632.1.15",
          "form": "ㅎㅎ 요새는 저렴이도 많이 나와서 일반 청소기와 가격 차이 없대요",
          "original_form": "ㅎㅎ 요새는 저렴이도 많이 나와서 일반 청소기와 가격 차이 없대요",
          "speaker_id": "2",
          "time": "20210822 09:18"
        },
        {
          "id": "MDRW2100007632.1.16",
          "form": "그런데 우리집도 로봇청소기 안쓰긴 해요",
          "original_form": "그런데 우리집도 로봇청소기 안쓰긴 해요",
          "speaker_id": "2",
          "time": "20210822 09:18"
        },
        {
          "id": "MDRW2100007632.1.17",
          "form": "강아지가 무서워해서이긴 한데 요새 로봇청소기 성능 좋대서 고민중이에요",
          "original_form": "강아지가 무서워해서이긴 한데 요새 로봇청소기 성능 좋대서 고민중이에요",
          "speaker_id": "2",
          "time": "20210822 09:19"
        },
        {
          "id": "MDRW2100007632.1.18",
          "form": "청소하는데 한 얼만큼 걸리나요",
          "original_form": "청소하는데 한 얼만큼 걸리나요",
          "speaker_id": "2",
          "time": "20210822 09:19"
        },
        {
          "id": "MDRW2100007632.1.19",
          "form": "화장실 30분 하는 거 같아여",
          "original_form": "화장실 30분 하는 거 같아여",
          "speaker_id": "1",
          "time": "20210822 09:20"
        },
        {
          "id": "MDRW2100007632.1.20",
          "form": "아 화장실 청소가 참 곤욕이죠",
          "original_form": "아 화장실 청소가 참 곤욕이죠",
          "speaker_id": "2",
          "time": "20210822 09:20"
        },
        {
          "id": "MDRW2100007632.1.21",
          "form": "물때도 끼니까.",
          "original_form": "물때도 끼니까.",
          "speaker_id": "2",
          "time": "20210822 09:21"
        }
      ]
