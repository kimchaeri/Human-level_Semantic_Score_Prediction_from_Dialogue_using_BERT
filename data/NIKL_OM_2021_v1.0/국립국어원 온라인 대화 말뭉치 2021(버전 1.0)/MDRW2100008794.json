      "utterance": [
        {
          "id": "MDRW2100008794.1.1",
          "form": "팔만원 정도 하더라 사고싶은게",
          "original_form": "팔만원 정도 하더라 사고싶은게",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008794.1.2",
          "form": "로맨틱크라운비싸잖아",
          "original_form": "로맨틱크라운비싸잖아",
          "speaker_id": "1",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008794.1.3",
          "form": "그채서 안산거임",
          "original_form": "그채서 안산거임",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008794.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ니트하나에팔만원은",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ니트하나에팔만원은",
          "speaker_id": "1",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008794.1.5",
          "form": "좀비싸긴함",
          "original_form": "좀비싸긴함",
          "speaker_id": "1",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008794.1.6",
          "form": "ㅇㅈㅋㅌㅋㅋ 그래서 사고 싶은 거 영역에만 머물러 있는거지...",
          "original_form": "ㅇㅈㅋㅌㅋㅋ 그래서 사고 싶은 거 영역에만 머물러 있는거지...",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008794.1.7",
          "form": "약간 자린고비의 굴비같은 느낌",
          "original_form": "약간 자린고비의 굴비같은 느낌",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008794.1.8",
          "form": "너근데vr도사고싶다하지않았냐",
          "original_form": "너근데vr도사고싶다하지않았냐",
          "speaker_id": "1",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008794.1.9",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "1",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008794.1.10",
          "form": "닌텐도 였는데 안사기로 함",
          "original_form": "닌텐도 였는데 안사기로 함",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008794.1.11",
          "form": "흥미가 떨어졌ㅇ음",
          "original_form": "흥미가 떨어졌ㅇ음",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008794.1.12",
          "form": "그렇게빨리?",
          "original_form": "그렇게빨리?",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008794.1.13",
          "form": "아니 근데 그걸 어떻게 기억하고 있냐 기억력 대박이네",
          "original_form": "아니 근데 그걸 어떻게 기억하고 있냐 기억력 대박이네",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008794.1.14",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008794.1.15",
          "form": "ㅋㅋㅋㅋㅋ그냥갑자기생각남",
          "original_form": "ㅋㅋㅋㅋㅋ그냥갑자기생각남",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008794.1.16",
          "form": "샀으면큰일날뻔했네",
          "original_form": "샀으면큰일날뻔했네",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008794.1.17",
          "form": "아 샀으면 잘 랬겠지먼 그냥 안사고 언제 가격떨어지나 보는게 너무 귀찮아졌음",
          "original_form": "아 샀으면 잘 랬겠지먼 그냥 안사고 언제 가격떨어지나 보는게 너무 귀찮아졌음",
          "speaker_id": "2",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008794.1.18",
          "form": "그당시에가격엄청올리긴했어",
          "original_form": "그당시에가격엄청올리긴했어",
          "speaker_id": "1",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008794.1.19",
          "form": "원가에거의십만원은붙여서팔더라",
          "original_form": "원가에거의십만원은붙여서팔더라",
          "speaker_id": "1",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008794.1.20",
          "form": "그니까 그 돈이면 지금 갤위치 4랑 버즈 2 까지살 수 있음",
          "original_form": "그니까 그 돈이면 지금 갤위치 4랑 버즈 2 까지살 수 있음",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008794.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아 너무 비쌈",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아 너무 비쌈",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008794.1.22",
          "form": "동숲도 이미 유행 지나기도 했고",
          "original_form": "동숲도 이미 유행 지나기도 했고",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008794.1.23",
          "form": "맞지맞지 게다가 이번에 올림픽 보면서 정이 더 떨어져서 사기 싫어졌음",
          "original_form": "맞지맞지 게다가 이번에 올림픽 보면서 정이 더 떨어져서 사기 싫어졌음",
          "speaker_id": "2",
