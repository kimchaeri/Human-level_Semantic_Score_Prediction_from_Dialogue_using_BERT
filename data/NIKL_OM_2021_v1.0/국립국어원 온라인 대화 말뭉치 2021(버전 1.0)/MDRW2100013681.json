        {
          "id": "MDRW2100013681.1.1",
          "form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋ함정카드였냐고요",
          "original_form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋ함정카드였냐고요",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013681.1.2",
          "form": "곧 추석인데 또 뭐 잘해요",
          "original_form": "곧 추석인데 또 뭐 잘해요",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013681.1.3",
          "form": "뭘 잘만드냔 말입니까!!",
          "original_form": "뭘 잘만드냔 말입니까!!",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013681.1.4",
          "form": "ㅇㄴ 쓰읍 기다려바요",
          "original_form": "ㅇㄴ 쓰읍 기다려바요",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013681.1.5",
          "form": "기억 더듬어 볼개요",
          "original_form": "기억 더듬어 볼개요",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013681.1.6",
          "form": "동태전도 잘하고요",
          "original_form": "동태전도 잘하고요",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013681.1.7",
          "form": "와 동태전",
          "original_form": "와 동태전",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013681.1.8",
          "form": "카레가루넣습니까",
          "original_form": "카레가루넣습니까",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013681.1.9",
          "form": "전 카레가루넣어서 부칩니다",
          "original_form": "전 카레가루넣어서 부칩니다",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013681.1.10",
          "form": "와",
