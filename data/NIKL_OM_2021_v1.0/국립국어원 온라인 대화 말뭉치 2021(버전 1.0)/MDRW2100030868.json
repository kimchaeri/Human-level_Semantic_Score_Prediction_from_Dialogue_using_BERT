      "utterance": [
        {
          "id": "MDRW2100030868.1.1",
          "form": "여행가는 거 좋아하세요?",
          "original_form": "여행가는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030868.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100030868.1.3",
          "form": "지금은 코로나라서 못가서 아쉬워요",
          "original_form": "지금은 코로나라서 못가서 아쉬워요",
          "speaker_id": "2",
          "time": "20210904 13:40"
        },
        {
          "id": "MDRW2100030868.1.4",
          "form": "name1님은 코로나 전에 여행을 가신적이 있으신가요?",
          "original_form": "&name1&님은 코로나 전에 여행을 가신적이 있으신가요?",
          "speaker_id": "2",
          "time": "20210904 13:40"
        },
        {
          "id": "MDRW2100030868.1.5",
          "form": "네! 강릉에 다녀왔어요",
          "original_form": "네! 강릉에 다녀왔어요",
          "speaker_id": "1",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030868.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030868.1.7",
          "form": "네 해외도 다녀왔었고 국내도 자주 다녔어요",
          "original_form": "네 해외도 다녀왔었고 국내도 자주 다녔어요",
          "speaker_id": "2",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030868.1.8",
          "form": "계획은 미리 세우시는 편인가요?",
          "original_form": "계획은 미리 세우시는 편인가요?",
          "speaker_id": "2",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030868.1.9",
          "form": "계획은 잘 세우지 않는 편이에요. 요즘은 호캉스가 좋더라구요 ㅎㅎ",
          "original_form": "계획은 잘 세우지 않는 편이에요. 요즘은 호캉스가 좋더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030868.1.10",
          "form": "호캉스 최근에 다녀왔는데 좋더라구요",
          "original_form": "호캉스 최근에 다녀왔는데 좋더라구요",
          "speaker_id": "2",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100030868.1.11",
          "form": "name1님도 가보시길 바라요",
          "original_form": "&name1&님도 가보시길 바라요",
          "speaker_id": "2",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100030868.1.12",
          "form": "그래야겠어요! name2님도 호캉스 좋아하시나 봐요",
          "original_form": "그래야겠어요! &name2&님도 호캉스 좋아하시나 봐요",
          "speaker_id": "1",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100030868.1.13",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030868.1.14",
          "form": "힐링이 되더라구요",
          "original_form": "힐링이 되더라구요",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030868.1.15",
          "form": "맞아요! 요즘 나가기도 힘들어서ㅠㅠ 호캉스가 좋은 것 같죠",
          "original_form": "맞아요! 요즘 나가기도 힘들어서ㅠㅠ 호캉스가 좋은 것 같죠",
          "speaker_id": "1",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100030868.1.16",
          "form": "그렇죠 멀리가기 힘드니 호캉스라도 즐기는게 좋은거같아요",
          "original_form": "그렇죠 멀리가기 힘드니 호캉스라도 즐기는게 좋은거같아요",
          "speaker_id": "2",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030868.1.17",
          "form": "가까운 곳에서 잠깐 쉬어도 엄청 휴식이 되니까요!",
          "original_form": "가까운 곳에서 잠깐 쉬어도 엄청 휴식이 되니까요!",
          "speaker_id": "1",
          "time": "20210904 14:24"
        },
        {
          "id": "MDRW2100030868.1.18",
          "form": "그건 맞죠",
          "original_form": "그건 맞죠",
          "speaker_id": "2",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100030868.1.19",
          "form": "호캉스 추천드릴게요",
          "original_form": "호캉스 추천드릴게요",
          "speaker_id": "2",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100030868.1.20",
          "form": "만약 코로나가 끝나면 어디로 여행가고 싶으세요?",
          "original_form": "만약 코로나가 끝나면 어디로 여행가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210905 01:25"
        },
        {
          "id": "MDRW2100030868.1.21",
          "form": "저는 코로나가 끝나면 일본으로 가고싶어요",
          "original_form": "저는 코로나가 끝나면 일본으로 가고싶어요",
          "speaker_id": "2",
          "time": "20210905 07:49"
        }
      ]
    }
