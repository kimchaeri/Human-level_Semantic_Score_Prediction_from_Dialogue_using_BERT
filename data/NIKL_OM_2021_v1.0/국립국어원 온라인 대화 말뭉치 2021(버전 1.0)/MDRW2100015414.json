      "utterance": [
        {
          "id": "MDRW2100015414.1.1",
          "form": "지브리 좋아하시나요",
          "original_form": "지브리 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015414.1.2",
          "form": "센과치히로 보셨나요",
          "original_form": "센과치히로 보셨나요",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015414.1.3",
          "form": "가디언즈 보셨어요??",
          "original_form": "가디언즈 보셨어요??",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015414.1.4",
          "form": "비운의 작품",
          "original_form": "비운의 작품",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015414.1.5",
          "form": "잭프로스트 나온거",
          "original_form": "잭프로스트 나온거",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015414.1.6",
          "form": "아 그거 보려다 시간안맞아서 못봤어요",
          "original_form": "아 그거 보려다 시간안맞아서 못봤어요",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015414.1.7",
          "form": "왜 비운에요",
          "original_form": "왜 비운에요",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015414.1.8",
          "form": "그게 개봉할 때",
          "original_form": "그게 개봉할 때",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.9",
          "form": "내용 참 좋고 흥행도 예상됐는데",
          "original_form": "내용 참 좋고 흥행도 예상됐는데",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.10",
          "form": "개봉됐을 때 불다가 먼저 퍼졌어요",
          "original_form": "개봉됐을 때 불다가 먼저 퍼졌어요",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.11",
          "form": "그래서 흥행 참패했답니다",
          "original_form": "그래서 흥행 참패했답니다",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.12",
          "form": "그게 뭔 말이지",
          "original_form": "그게 뭔 말이지",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.14",
          "form": "불법 다운로드",
          "original_form": "불법 다운로드",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.15",
          "form": "불법다운로드",
          "original_form": "불법다운로드",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.16",
          "form": "그게 먼저 퍼젺어요",
          "original_form": "그게 먼저 퍼젺어요",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.18",
          "form": "아 진짜 비운이다",
          "original_form": "아 진짜 비운이다",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.19",
          "form": "잭만 남은",
          "original_form": "잭만 남은",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.20",
          "form": "어떻게 그런 재수없는일이",
          "original_form": "어떻게 그런 재수없는일이",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015414.1.21",
          "form": "원래 2도 계획되어 있었는데..엎어졌대요",
          "original_form": "원래 2도 계획되어 있었는데..엎어졌대요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015414.1.22",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015414.1.23",
          "form": "불다가 몇명의 희망을 아작내는거야",
          "original_form": "불다가 몇명의 희망을 아작내는거야",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015414.1.24",
          "form": "드래곤 길들이기 1,2,3 다 보셨어요??",
          "original_form": "드래곤 길들이기 1,2,3 다 보셨어요??",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015414.1.25",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015414.1.26",
          "form": "저 1편만 봤어요",
          "original_form": "저 1편만 봤어요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015414.1.27",
          "form": "글고 그때 드림웍스 인사정리하고 그랬어요",
          "original_form": "글고 그때 드림웍스 인사정리하고 그랬어요",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015414.1.28",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "1",
          "time": "20210825 23:03"
        }
      ]
