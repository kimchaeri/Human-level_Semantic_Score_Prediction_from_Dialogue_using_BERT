        },
        {
          "id": "MDRW2100002811.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100002811.1.3",
          "form": "스포츠좋아하시나요?",
          "original_form": "스포츠좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100002811.1.4",
          "form": "직접 가보진않고 티비로 가끔봐요",
          "original_form": "직접 가보진않고 티비로 가끔봐요",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100002811.1.5",
          "form": "올림픽은 좋아해요",
          "original_form": "올림픽은 좋아해요",
          "speaker_id": "2",
          "time": "20210823 16:34"
        },
        {
          "id": "MDRW2100002811.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 16:34"
        },
        {
          "id": "MDRW2100002811.1.7",
          "form": "저 좋아해요ㅋㅋㅋ",
          "original_form": "저 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:38"
        },
        {
          "id": "MDRW2100002811.1.8",
          "form": "요즘은 축구본답니다",
          "original_form": "요즘은 축구본답니다",
          "speaker_id": "1",
          "time": "20210823 16:38"
        },
        {
          "id": "MDRW2100002811.1.9",
          "form": "해외축구요?",
          "original_form": "해외축구요?",
          "speaker_id": "2",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100002811.1.10",
          "form": "한국축구요ㅋㅋㅋ 곧 월드컵",
          "original_form": "한국축구요ㅋㅋㅋ 곧 월드컵",
          "speaker_id": "1",
          "time": "20210823 16:42"
        },
        {
          "id": "MDRW2100002811.1.11",
          "form": "나가는거 경기하더라고요",
          "original_form": "나가는거 경기하더라고요",
          "speaker_id": "1",
          "time": "20210823 16:42"
        },
        {
          "id": "MDRW2100002811.1.12",
          "form": "어디팀 응원하세요?ㅎ",
          "original_form": "어디팀 응원하세요?ㅎ",
          "speaker_id": "2",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100002811.1.13",
          "form": "전 프로축구는 아는게 없어서",
          "original_form": "전 프로축구는 아는게 없어서",
          "speaker_id": "2",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100002811.1.14",
          "form": "해외축구로 따지면",
          "original_form": "해외축구로 따지면",
          "speaker_id": "1",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100002811.1.15",
          "form": "토트넘이요ㅋㅋㅋ",
          "original_form": "토트넘이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100002811.1.16",
          "form": "손흥민이 토트넘인가요?",
          "original_form": "손흥민이 토트넘인가요?",
          "speaker_id": "2",
          "time": "20210823 16:53"
        },
        {
          "id": "MDRW2100002811.1.17",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:00"
        },
        {
          "id": "MDRW2100002811.1.18",
          "form": "아시는군요",
          "original_form": "아시는군요",
          "speaker_id": "1",
          "time": "20210823 17:00"
        },
        {
          "id": "MDRW2100002811.1.19",
          "form": "손흥민정도는 워낙유명하니까요ㅎ",
          "original_form": "손흥민정도는 워낙유명하니까요ㅎ",
          "speaker_id": "2",
          "time": "20210823 17:00"
        },
        {
          "id": "MDRW2100002811.1.20",
          "form": "맞아요ㅋㅋㅋ어제도",
          "original_form": "맞아요ㅋㅋㅋ어제도",
          "speaker_id": "1",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100002811.1.21",
          "form": "경기가 있었답니다",
          "original_form": "경기가 있었답니다",
          "speaker_id": "1",
          "time": "20210823 17:04"
        },
        {
          "id": "MDRW2100002811.1.22",
          "form": "아 시즌첫골터진건 봤는데ㅎㅎ",
          "original_form": "아 시즌첫골터진건 봤는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100002811.1.23",
          "form": "맨시티전이요?",
          "original_form": "맨시티전이요?",
          "speaker_id": "1",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100002811.1.24",
          "form": "첫경기 였다니 그거같아요ㅎ",
          "original_form": "첫경기 였다니 그거같아요ㅎ",
          "speaker_id": "2",
          "time": "20210823 17:22"
        }
