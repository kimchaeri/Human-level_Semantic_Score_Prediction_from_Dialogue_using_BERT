          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029627.1.3",
          "form": "아니 맨날 먹는 얘기잖아요",
          "original_form": "아니 맨날 먹는 얘기잖아요",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029627.1.4",
          "form": "악기 머 배운거 없나요",
          "original_form": "악기 머 배운거 없나요",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029627.1.5",
          "form": "샐러드는 사두면 계속 먹어야댐",
          "original_form": "샐러드는 사두면 계속 먹어야댐",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029627.1.6",
          "form": "썩.음",
          "original_form": "썩.음",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029627.1.7",
          "form": "저 피아노로 예고갈랫음",
          "original_form": "저 피아노로 예고갈랫음",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029627.1.8",
          "form": "악기잘다룸",
          "original_form": "악기잘다룸",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029627.1.9",
          "form": "기타도 좀 침",
          "original_form": "기타도 좀 침",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029627.1.10",
          "form": "멋잇죠?",
          "original_form": "멋잇죠?",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029627.1.11",
          "form": "저 리코더 살인마였는데",
          "original_form": "저 리코더 살인마였는데",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029627.1.12",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029627.1.13",
          "form": "리코더로 킬좀 하셨나봐요?",
          "original_form": "리코더로 킬좀 하셨나봐요?",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029627.1.14",
          "form": "피아노 체르니 40까지 했었는데",
          "original_form": "피아노 체르니 40까지 했었는데",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029627.1.15",
          "form": "기타는 해보려다가",
          "original_form": "기타는 해보려다가",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029627.1.16",
          "form": "코드 키 잡는게 한두개가 아니라 접음",
          "original_form": "코드 키 잡는게 한두개가 아니라 접음",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029627.1.17",
          "form": "리코더로 휩쓸었죠 ㅎ",
          "original_form": "리코더로 휩쓸었죠 ㅎ",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029627.1.18",
          "form": "저는 체르니 다 뗌",
          "original_form": "저는 체르니 다 뗌",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029627.1.19",
          "form": "준대상까지탔었는데 관둠",
          "original_form": "준대상까지탔었는데 관둠",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029627.1.20",
          "form": "좀 아쉽긴함",
          "original_form": "좀 아쉽긴함",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029627.1.21",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029627.1.22",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029627.1.23",
          "form": "질렸나",
          "original_form": "질렸나",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029627.1.24",
          "form": "음,,",
          "original_form": "음,,",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029627.1.25",
          "form": "돈때문인것도있었음",
          "original_form": "돈때문인것도있었음",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029627.1.26",
          "form": "돈이너무많이들음ㅋㅋㅋㅋㅋ예체능",
          "original_form": "돈이너무많이들음ㅋㅋㅋㅋㅋ예체능",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.27",
          "form": "아ㅠ",
          "original_form": "아ㅠ",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.28",
          "form": "중1때부터 개인레슨붙여가며 해야하는데",
          "original_form": "중1때부터 개인레슨붙여가며 해야하는데",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.29",
          "form": "예체능 돈은 많이 드는데",
          "original_form": "예체능 돈은 많이 드는데",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.30",
          "form": "미래가 밝지도 않고",
          "original_form": "미래가 밝지도 않고",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.31",
          "form": "솔직히 피아노로 잘해봣자 나중에 뭐가되겠나 싶어서",
          "original_form": "솔직히 피아노로 잘해봣자 나중에 뭐가되겠나 싶어서",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.32",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.33",
          "form": "상위 1프로여야 하니",
          "original_form": "상위 1프로여야 하니",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.34",
          "form": "예고도 특목고니 돈도많이들고",
          "original_form": "예고도 특목고니 돈도많이들고",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.35",
          "form": "생각많이해서 걍 취미로 하자 했는데",
          "original_form": "생각많이해서 걍 취미로 하자 했는데",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.36",
          "form": "지금 취미는 무슨 ㅋㅋㅋㅋ",
          "original_form": "지금 취미는 무슨 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.37",
          "form": "손다굳음",
          "original_form": "손다굳음",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029627.1.38",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029627.1.39",
          "form": "전 첼로 했었어요",
          "original_form": "전 첼로 했었어요",
          "speaker_id": "1",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029627.1.40",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029627.1.41",
          "form": "제친구도 학교동아리에서 첼로하던데",
          "original_form": "제친구도 학교동아리에서 첼로하던데",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029627.1.42",
          "form": "재밋어요?",
          "original_form": "재밋어요?",
          "speaker_id": "2",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029627.1.43",
          "form": "ㅎ ㅎ ㅎ",
          "original_form": "ㅎ ㅎ ㅎ",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029627.1.44",
          "form": "어릴 때 재밌어서 했겠어요",
          "original_form": "어릴 때 재밌어서 했겠어요",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029627.1.45",
          "form": "시키니까 함 하는거지",
          "original_form": "시키니까 함 하는거지",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029627.1.46",
          "form": "일단 무거움",
          "original_form": "일단 무거움",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029627.1.47",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029627.1.48",
          "form": "버스타고 댕기면 어깨빠짐",
          "original_form": "버스타고 댕기면 어깨빠짐",
          "speaker_id": "1",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029627.1.49",
          "form": "사람만한데 당연히무겁죠",
          "original_form": "사람만한데 당연히무겁죠",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029627.1.50",
          "form": "어그로 쩔음",
          "original_form": "어그로 쩔음",
          "speaker_id": "1",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029627.1.51",
          "form": "다 쳐다봐요",
          "original_form": "다 쳐다봐요",
          "speaker_id": "1",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029627.1.52",
          "form": "관짝 같은거 메고다니니까",
          "original_form": "관짝 같은거 메고다니니까",
          "speaker_id": "1",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029627.1.53",
          "form": "악기재밋지암ㅎ아요?",
          "original_form": "악기재밋지암ㅎ아요?",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029627.1.54",
          "form": "솔직히 배우고싶음 뭐든",
          "original_form": "솔직히 배우고싶음 뭐든",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029627.1.55",
          "form": "하프배우고싶움",
          "original_form": "하프배우고싶움",
          "speaker_id": "2",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029627.1.56",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "1",
          "time": "20210903 11:13"
        },
        {
          "id": "MDRW2100029627.1.57",
          "form": "그리스로마신화세요?",
          "original_form": "그리스로마신화세요?",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029627.1.58",
          "form": "하프 4천이던데",
          "original_form": "하프 4천이던데",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029627.1.59",
          "form": "관리사 따로 있어야하고ㅋ",
          "original_form": "관리사 따로 있어야하고ㅋ",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029627.1.60",
          "form": "비싼건그렇고",
          "original_form": "비싼건그렇고",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029627.1.61",
          "form": "싼건 싸요?",
          "original_form": "싼건 싸요?",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029627.1.62",
          "form": "싼것도 백넘긴하두라구요",
          "original_form": "싼것도 백넘긴하두라구요",
          "speaker_id": "2",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029627.1.63",
          "form": "3d 프린터기 이런거로 만드나 왜케 쌈",
          "original_form": "3d 프린터기 이런거로 만드나 왜케 쌈",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100029627.1.64",
          "form": "아닠ㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029627.1.65",
          "form": "전문가도아닌데",
          "original_form": "전문가도아닌데",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029627.1.66",
          "form": "뭣하러좋은거씁니까 소리만잘나면되는건데",
          "original_form": "뭣하러좋은거씁니까 소리만잘나면되는건데",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029627.1.67",
          "form": "잘나면은 빼죠",
          "original_form": "잘나면은 빼죠",
          "speaker_id": "1",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029627.1.68",
          "form": "소리만 나면",
          "original_form": "소리만 나면",
          "speaker_id": "1",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029627.1.69",
          "form": "소리만 제대로 나면 ㅇㅇ",
          "original_form": "소리만 제대로 나면 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029627.1.70",
          "form": "다시 배워보고싶긴해요",
          "original_form": "다시 배워보고싶긴해요",
