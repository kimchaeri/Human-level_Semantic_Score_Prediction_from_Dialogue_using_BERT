      "utterance": [
        {
          "id": "MDRW2100009862.1.1",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009862.1.2",
          "form": "고양이 키워요",
          "original_form": "고양이 키워요",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009862.1.3",
          "form": "고양이 좋아하시나요",
          "original_form": "고양이 좋아하시나요",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009862.1.4",
          "form": "전 좋아하지만",
          "original_form": "전 좋아하지만",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009862.1.5",
          "form": "능력이 부족해서ㅠ",
          "original_form": "능력이 부족해서ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009862.1.6",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009862.1.7",
          "form": "기르시는 반려동물이 없으시구나",
          "original_form": "기르시는 반려동물이 없으시구나",
          "speaker_id": "1",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100009862.1.8",
          "form": "예전에 개 키웠었는데",
          "original_form": "예전에 개 키웠었는데",
          "speaker_id": "2",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100009862.1.9",
          "form": "너무 외로워하더라고요",
          "original_form": "너무 외로워하더라고요",
          "speaker_id": "2",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100009862.1.10",
          "form": "어떤 종류의 개를 키웠나요",
          "original_form": "어떤 종류의 개를 키웠나요",
          "speaker_id": "1",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100009862.1.11",
          "form": "믹스견이었어요",
          "original_form": "믹스견이었어요",
          "speaker_id": "2",
          "time": "20210823 11:05"
        },
        {
          "id": "MDRW2100009862.1.12",
          "form": "귀여웠겠다",
          "original_form": "귀여웠겠다",
          "speaker_id": "1",
          "time": "20210823 11:05"
        },
        {
          "id": "MDRW2100009862.1.13",
          "form": "고양이 품종은 어떻게 되세요?",
          "original_form": "고양이 품종은 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210823 11:05"
        },
        {
          "id": "MDRW2100009862.1.14",
          "form": "길고양이요",
          "original_form": "길고양이요",
          "speaker_id": "1",
          "time": "20210823 11:05"
        },
        {
          "id": "MDRW2100009862.1.15",
          "form": "주워왔어요",
          "original_form": "주워왔어요",
          "speaker_id": "1",
          "time": "20210823 11:05"
        },
        {
          "id": "MDRW2100009862.1.16",
          "form": "와 간택받으셨군요ㅎㅎ",
          "original_form": "와 간택받으셨군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:06"
        },
        {
          "id": "MDRW2100009862.1.17",
          "form": "후회는 없으세요?",
          "original_form": "후회는 없으세요?",
          "speaker_id": "2",
          "time": "20210823 11:06"
        },
        {
          "id": "MDRW2100009862.1.18",
          "form": "어.. 간택은아니고.. 죽을것같아서 주워왔는데 후회는 없어요",
          "original_form": "어.. 간택은아니고.. 죽을것같아서 주워왔는데 후회는 없어요",
          "speaker_id": "1",
          "time": "20210823 11:06"
        },
        {
          "id": "MDRW2100009862.1.19",
          "form": "강아지 이름은 뭐였나요",
          "original_form": "강아지 이름은 뭐였나요",
          "speaker_id": "1",
          "time": "20210823 11:06"
        },
        {
