      "utterance": [
        {
          "id": "MDRW2100003281.1.1",
          "form": "요즘 즐겨보는 프로그램 있어?",
          "original_form": "요즘 즐겨보는 프로그램 있어?",
          "speaker_id": "1",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003281.1.2",
          "form": "난 체인지데이즈 완전 과몰입 중이야ㅋㅋㅋ",
          "original_form": "난 체인지데이즈 완전 과몰입 중이야ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003281.1.3",
          "form": "나는 돌싱글즈 ㅋㅋㅋㅋ",
          "original_form": "나는 돌싱글즈 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:54"
        },
        {
          "id": "MDRW2100003281.1.4",
          "form": "프로그램 소개만 봤는데 벌써 재밌다ㅋㅋㅋ",
          "original_form": "프로그램 소개만 봤는데 벌써 재밌다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:21"
        },
        {
          "id": "MDRW2100003281.1.5",
          "form": "봐야겠어!",
          "original_form": "봐야겠어!",
          "speaker_id": "1",
          "time": "20210820 23:21"
        },
        {
          "id": "MDRW2100003281.1.6",
          "form": "환승연애도 봐야하는데",
          "original_form": "환승연애도 봐야하는데",
          "speaker_id": "1",
          "time": "20210820 23:21"
        },
        {
          "id": "MDRW2100003281.1.7",
          "form": "요즘 이런 프로그램 많은 거 같아",
          "original_form": "요즘 이런 프로그램 많은 거 같아",
          "speaker_id": "1",
          "time": "20210820 23:21"
        },
        {
          "id": "MDRW2100003281.1.8",
          "form": "좋다ㅋㅋㅋ",
          "original_form": "좋다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:21"
        },
        {
          "id": "MDRW2100003281.1.9",
          "form": "ㅋㅋㅋ글게 환승연애도 재밌을듯",
          "original_form": "ㅋㅋㅋ글게 환승연애도 재밌을듯",
          "speaker_id": "2",
          "time": "20210821 07:54"
        },
        {
          "id": "MDRW2100003281.1.10",
          "form": "요즘엔 ott 너무 좋아서 티비는 잘 안 보게 돼",
          "original_form": "요즘엔 ott 너무 좋아서 티비는 잘 안 보게 돼",
          "speaker_id": "1",
          "time": "20210821 12:09"
        },
        {
          "id": "MDRW2100003281.1.11",
          "form": "맞앙 시간도 단축되고 좋음",
          "original_form": "맞앙 시간도 단축되고 좋음",
          "speaker_id": "2",
          "time": "20210821 12:15"
        },
        {
          "id": "MDRW2100003281.1.12",
          "form": "맞아 근데 뭐 볼까 고르는 시간이 더 많은 거 같기도 해",
          "original_form": "맞아 근데 뭐 볼까 고르는 시간이 더 많은 거 같기도 해",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100003281.1.13",
          "form": "아 맞아용 추천받음 편하고",
          "original_form": "아 맞아용 추천받음 편하고",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100003281.1.14",
          "form": "제목이나 포스터보고 보는 거 보단 추천 받아서 보는 경우가 더 많아요",
          "original_form": "제목이나 포스터보고 보는 거 보단 추천 받아서 보는 경우가 더 많아요",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100003281.1.15",
          "form": "영화 추천할거있어요?",
          "original_form": "영화 추천할거있어요?",
          "speaker_id": "2",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100003281.1.16",
          "form": "좀 된 영화긴 한데 걸캅스 너무 재밌게 봤어요",
          "original_form": "좀 된 영화긴 한데 걸캅스 너무 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100003281.1.17",
          "form": "재밌더라고요 티비에서 한거봤어요",
          "original_form": "재밌더라고요 티비에서 한거봤어요",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100003281.1.18",
          "form": "영화장르 뭐 좋아하세요?",
          "original_form": "영화장르 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100003281.1.19",
          "form": "로맨스요",
          "original_form": "로맨스요",
