      "utterance": [
        {
          "id": "MDRW2100039755.1.1",
          "form": "토마토 달걀볶음 해줄까?\r\n베이글 빵도 먹고",
          "original_form": "토마토 달걀볶음 해줄까?\r\n베이글 빵도 먹고",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039755.1.2",
          "form": "토마토달걀은 이제 그만.......",
          "original_form": "토마토달걀은 이제 그만.......",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039755.1.3",
          "form": "엄만 해주기만하고 잘 먹어보지 못해서 먹을래",
          "original_form": "엄만 해주기만하고 잘 먹어보지 못해서 먹을래",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039755.1.4",
          "form": "그럼 혼자서 다 먹어",
          "original_form": "그럼 혼자서 다 먹어",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039755.1.5",
          "form": "넌 베이컨볶음밥 해줄게",
          "original_form": "넌 베이컨볶음밥 해줄게",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039755.1.6",
          "form": "음.............. 그래",
          "original_form": "음.............. 그래",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039755.1.7",
          "form": "베이글도 먹을래",
          "original_form": "베이글도 먹을래",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039755.1.8",
          "form": "별로인것같네\r\n그래 저번에 산 땅콩잼이랑 먹어",
          "original_form": "별로인것같네\r\n그래 저번에 산 땅콩잼이랑 먹어",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039755.1.9",
          "form": "딴 건 없어?",
          "original_form": "딴 건 없어?",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039755.1.10",
          "form": "두부감자조림 깻잎",
          "original_form": "두부감자조림 깻잎",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039755.1.11",
          "form": "ㅋㅋ 내일 아침은 베이글이다",
          "original_form": "ㅋㅋ 내일 아침은 베이글이다",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039755.1.12",
          "form": "로제떡볶이 먹고싶어",
          "original_form": "로제떡볶이 먹고싶어",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039755.1.13",
          "form": "골고루 먹어야지",
          "original_form": "골고루 먹어야지",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039755.1.14",
          "form": "시켰으니까 올거야 그럼 조리해줄게",
          "original_form": "시켰으니까 올거야 그럼 조리해줄게",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039755.1.15",
          "form": "다 로제로만 시킬걸ㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜ",
          "original_form": "다 로제로만 시킬걸ㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039755.1.16",
          "form": "납작당면 넣어서 해줄게",
          "original_form": "납작당면 넣어서 해줄게",
