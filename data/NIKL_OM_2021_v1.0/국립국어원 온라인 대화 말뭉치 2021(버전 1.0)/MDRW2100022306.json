      "utterance": [
        {
          "id": "MDRW2100022306.1.1",
          "form": "돼지고기 부위 잘 아세요?",
          "original_form": "돼지고기 부위 잘 아세요?",
          "speaker_id": "2",
          "time": "20210829 12:20"
        },
        {
          "id": "MDRW2100022306.1.2",
          "form": "헐 저랑은 반대 전 후라이드 좋아합니다",
          "original_form": "헐 저랑은 반대 전 후라이드 좋아합니다",
          "speaker_id": "1",
          "time": "20210829 12:20"
        },
        {
          "id": "MDRW2100022306.1.3",
          "form": "아니요 잘은 몰라요",
          "original_form": "아니요 잘은 몰라요",
          "speaker_id": "1",
          "time": "20210829 12:20"
        },
        {
          "id": "MDRW2100022306.1.4",
          "form": "어떤거 알아요?",
          "original_form": "어떤거 알아요?",
          "speaker_id": "2",
          "time": "20210829 12:20"
        },
        {
          "id": "MDRW2100022306.1.5",
          "form": "삼겹살 밖에 모르세용?",
          "original_form": "삼겹살 밖에 모르세용?",
          "speaker_id": "2",
          "time": "20210829 12:20"
        },
        {
          "id": "MDRW2100022306.1.6",
          "form": "검색하러 가면 반칙!!",
          "original_form": "검색하러 가면 반칙!!",
          "speaker_id": "2",
          "time": "20210829 12:20"
        },
        {
          "id": "MDRW2100022306.1.7",
          "form": "그냥 삼겹살 목살 앞다리 뒷다리 갈빗살 가브리살 항정살..",
          "original_form": "그냥 삼겹살 목살 앞다리 뒷다리 갈빗살 가브리살 항정살..",
          "speaker_id": "1",
          "time": "20210829 12:20"
        },
        {
          "id": "MDRW2100022306.1.8",
          "form": "ㅈㅅ",
          "original_form": "ㅈㅅ",
          "speaker_id": "2",
          "time": "20210829 12:20"
        },
        {
          "id": "MDRW2100022306.1.9",
          "form": "왜물으시죠",
          "original_form": "왜물으시죠",
          "speaker_id": "1",
          "time": "20210829 12:20"
        },
        {
          "id": "MDRW2100022306.1.10",
          "form": "되게 많이 아시는데요?ㅋㅋㅋ",
          "original_form": "되게 많이 아시는데요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:20"
        },
        {
          "id": "MDRW2100022306.1.11",
          "form": "갑자기 목살 스테이크 먹고싶어서요",
          "original_form": "갑자기 목살 스테이크 먹고싶어서요",
          "speaker_id": "2",
          "time": "20210829 12:21"
        },
        {
          "id": "MDRW2100022306.1.12",
          "form": "목살 스테이크는 서가앤쿡 맛있는데",
          "original_form": "목살 스테이크는 서가앤쿡 맛있는데",
          "speaker_id": "1",
          "time": "20210829 12:21"
        },
        {
          "id": "MDRW2100022306.1.13",
          "form": "어디가서ㅈ사드세요",
          "original_form": "어디가서ㅈ사드세요",
          "speaker_id": "1",
          "time": "20210829 12:21"
        },
        {
          "id": "MDRW2100022306.1.14",
          "form": "저 보통 집에서 해먹어요 ㅜㅜ",
          "original_form": "저 보통 집에서 해먹어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100022306.1.15",
          "form": "목살 바베큐용으로 두껍게 사가지구요",
          "original_form": "목살 바베큐용으로 두껍게 사가지구요",
          "speaker_id": "2",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100022306.1.16",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100022306.1.17",
          "form": "백종원 아저씨가 씨즈닝 알려줬어요",
          "original_form": "백종원 아저씨가 씨즈닝 알려줬어요",
          "speaker_id": "2",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100022306.1.18",
          "form": "익나요? 오래걸려서 집에서 먹기는 좀",
          "original_form": "익나요? 오래걸려서 집에서 먹기는 좀",
          "speaker_id": "1",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100022306.1.19",
          "form": "어렵단데",
          "original_form": "어렵단데",
          "speaker_id": "1",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100022306.1.20",
          "form": "잘 익어요 프라이팬에 기름 콸콸 부어서 ㅠ",
          "original_form": "잘 익어요 프라이팬에 기름 콸콸 부어서 ㅠ",
          "speaker_id": "2",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100022306.1.21",
          "form": "기름온도로 익혀요....",
          "original_form": "기름온도로 익혀요....",
          "speaker_id": "2",
          "time": "20210829 12:22"
        },
        {
          "id": "MDRW2100022306.1.22",
          "form": "오.. 뭔가 전문적",
          "original_form": "오.. 뭔가 전문적",
          "speaker_id": "1",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022306.1.23",
          "form": "ㅋㅋㅋㅋ 진짜 맛있어요 ㅜㅜㅜ",
          "original_form": "ㅋㅋㅋㅋ 진짜 맛있어요 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022306.1.24",
          "form": "뭐랑 같이드세요",
          "original_form": "뭐랑 같이드세요",
          "speaker_id": "1",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022306.1.25",
          "form": "아스파라거스?",
          "original_form": "아스파라거스?",
          "speaker_id": "1",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022306.1.26",
          "form": "아스파라거스랑 통마늘 같이 먹어요",
          "original_form": "아스파라거스랑 통마늘 같이 먹어요",
          "speaker_id": "2",
          "time": "20210829 12:23"
        }
      ]
    }
