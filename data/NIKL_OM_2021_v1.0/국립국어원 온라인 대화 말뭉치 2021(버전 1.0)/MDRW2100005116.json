      "utterance": [
        {
          "id": "MDRW2100005116.1.1",
          "form": "여행 못간지 한참됐어..ㅠ",
          "original_form": "여행 못간지 한참됐어..ㅠ",
          "speaker_id": "2",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005116.1.2",
          "form": "여행가고싶다ㅠㅠ",
          "original_form": "여행가고싶다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005116.1.3",
          "form": "그래서 요즘 워홀 브이로그 많이봐..",
          "original_form": "그래서 요즘 워홀 브이로그 많이봐..",
          "speaker_id": "2",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005116.1.4",
          "form": "맨날 유튜브로 여행 브이로그 보면서 대리만족 중..",
          "original_form": "맨날 유튜브로 여행 브이로그 보면서 대리만족 중..",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005116.1.5",
          "form": "가고싶은 여행지 있어?",
          "original_form": "가고싶은 여행지 있어?",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005116.1.6",
          "form": "그리스랑 도쿄",
          "original_form": "그리스랑 도쿄",
          "speaker_id": "2",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005116.1.7",
          "form": "와 좋다 나도 데려가",
          "original_form": "와 좋다 나도 데려가",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005116.1.8",
          "form": "그리스는 산토리니 마을 가보고싶고 도쿄는 너의 이름은 투어하고시퍼",
          "original_form": "그리스는 산토리니 마을 가보고싶고 도쿄는 너의 이름은 투어하고시퍼",
          "speaker_id": "2",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005116.1.9",
          "form": "둘 다 다른 매력이네",
          "original_form": "둘 다 다른 매력이네",
          "speaker_id": "1",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005116.1.10",
          "form": "나는 스위스 가보고싶어",
          "original_form": "나는 스위스 가보고싶어",
          "speaker_id": "1",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005116.1.11",
          "form": "스위스 왜?",
          "original_form": "스위스 왜?",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005116.1.12",
          "form": "스위스 사진을 봤는데 산하고 계곡 사이에 아기자기한 마을이 있는게 너무 예뻤어",
          "original_form": "스위스 사진을 봤는데 산하고 계곡 사이에 아기자기한 마을이 있는게 너무 예뻤어",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005116.1.13",
          "form": "내 버킷리스트",
          "original_form": "내 버킷리스트",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005116.1.14",
          "form": "가면 나도 데려가",
          "original_form": "가면 나도 데려가",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005116.1.15",
          "form": "우리 꼭 여행 같이 가자",
          "original_form": "우리 꼭 여행 같이 가자",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005116.1.16",
          "form": "대학생 되면 돈모아서 세계일주하기 꿈있었는데",
          "original_form": "대학생 되면 돈모아서 세계일주하기 꿈있었는데",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005116.1.17",
          "form": "나도나도ㅠㅠ 휴학하고 여행하려고 했는데",
          "original_form": "나도나도ㅠㅠ 휴학하고 여행하려고 했는데",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005116.1.18",
          "form": "직장인 돼서라두 같이가자",
          "original_form": "직장인 돼서라두 같이가자",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005116.1.19",
          "form": "좋아 휴가써!!!",
          "original_form": "좋아 휴가써!!!",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005116.1.20",
          "form": "볼사 여행 노래알지",
          "original_form": "볼사 여행 노래알지",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005116.1.21",
          "form": "응응 알지알지",
          "original_form": "응응 알지알지",
          "speaker_id": "1",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005116.1.22",
          "form": "난 고3때 대딩되면 이 노래 들으면서 여행다녀야지ㅠㅠ 했는데 .. 이러케됐다",
          "original_form": "난 고3때 대딩되면 이 노래 들으면서 여행다녀야지ㅠㅠ 했는데 .. 이러케됐다",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005116.1.23",
          "form": "진짜ㅠㅜㅜ 우리 진짜 꼭 여행 같이가자",
          "original_form": "진짜ㅠㅜㅜ 우리 진짜 꼭 여행 같이가자",
