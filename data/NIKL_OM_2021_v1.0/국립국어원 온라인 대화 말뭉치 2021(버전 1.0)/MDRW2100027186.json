      "utterance": [
        {
          "id": "MDRW2100027186.1.1",
          "form": "저도요 예고편보니 재밌을것같더라구요",
          "original_form": "저도요 예고편보니 재밌을것같더라구요",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027186.1.2",
          "form": "저는 사실 쫄보라",
          "original_form": "저는 사실 쫄보라",
          "speaker_id": "1",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027186.1.3",
          "form": "귀막고 봅니다",
          "original_form": "귀막고 봅니다",
          "speaker_id": "1",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027186.1.4",
          "form": "그러시군요 저는 그냥봐요 겁이없어서요",
          "original_form": "그러시군요 저는 그냥봐요 겁이없어서요",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027186.1.5",
          "form": "설마요",
          "original_form": "설마요",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027186.1.6",
          "form": "진짠가요",
          "original_form": "진짠가요",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027186.1.7",
          "form": "네 진짜에요 속고만 사셨나요",
          "original_form": "네 진짜에요 속고만 사셨나요",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027186.1.8",
          "form": "공포영화 개봉 많이 했으면 좋겠네요",
          "original_form": "공포영화 개봉 많이 했으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027186.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027186.1.10",
          "form": "큰화면으로 보는 공포영화가 짱",
          "original_form": "큰화면으로 보는 공포영화가 짱",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027186.1.11",
          "form": "맞아요 공포영화는 영화관에서봐야 제맛이죠 빨리 코로나없어져서 팝콘먹으면서 보고싶네요",
          "original_form": "맞아요 공포영화는 영화관에서봐야 제맛이죠 빨리 코로나없어져서 팝콘먹으면서 보고싶네요",
          "speaker_id": "2",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100027186.1.12",
          "form": "팝콘 어느 영화관꺼 좋아하세요",
          "original_form": "팝콘 어느 영화관꺼 좋아하세요",
          "speaker_id": "1",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100027186.1.13",
          "form": "저는 cgv요 양파맛 가루넣으니까 맛있더라구요",
          "original_form": "저는 cgv요 양파맛 가루넣으니까 맛있더라구요",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027186.1.14",
          "form": "오 저는 치즈맛 좋아해요",
          "original_form": "오 저는 치즈맛 좋아해요",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027186.1.15",
          "form": "역시 영화관 팝콘 짱",
          "original_form": "역시 영화관 팝콘 짱",
          "speaker_id": "1",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027186.1.16",
          "form": "맞아요 에이드도 너무 맛있어요",
          "original_form": "맞아요 에이드도 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027186.1.17",
          "form": "오 어떤맛이요",
          "original_form": "오 어떤맛이요",
          "speaker_id": "1",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100027186.1.18",
          "form": "자몽이요 새콤 달콤해서요",
          "original_form": "자몽이요 새콤 달콤해서요",
