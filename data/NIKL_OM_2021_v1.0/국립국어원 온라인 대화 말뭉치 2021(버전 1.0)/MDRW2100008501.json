      "utterance": [
        {
          "id": "MDRW2100008501.1.1",
          "form": "연애중 이신가?",
          "original_form": "연애중 이신가?",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008501.1.2",
          "form": "놉!",
          "original_form": "놉!",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008501.1.3",
          "form": "아님!",
          "original_form": "아님!",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008501.1.4",
          "form": "아 나 위너랑 연애중~_~",
          "original_form": "아 나 위너랑 연애중~_~",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008501.1.5",
          "form": "다들 군입대가고 없잖아",
          "original_form": "다들 군입대가고 없잖아",
          "speaker_id": "1",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008501.1.6",
          "form": "아냨ㅋㅋㅋ",
          "original_form": "아냨ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008501.1.7",
          "form": "송민호 강승윤 안갔으",
          "original_form": "송민호 강승윤 안갔으",
          "speaker_id": "2",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008501.1.8",
          "form": "둘이나 아직 있나",
          "original_form": "둘이나 아직 있나",
          "speaker_id": "1",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008501.1.9",
          "form": "웅ㅋㅋㅋ 연애는 굳이?",
          "original_form": "웅ㅋㅋㅋ 연애는 굳이?",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008501.1.10",
          "form": "바빠..",
          "original_form": "바빠..",
          "speaker_id": "2",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008501.1.11",
          "form": "바쁘면 할수가 없네",
          "original_form": "바쁘면 할수가 없네",
          "speaker_id": "1",
          "time": "20210822 16:40"
        },
        {
          "id": "MDRW2100008501.1.12",
          "form": "맞아.. ㅋㅋㅋ",
          "original_form": "맞아.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008501.1.13",
          "form": "바빠서 못하징",
          "original_form": "바빠서 못하징",
          "speaker_id": "2",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008501.1.14",
          "form": "주변친구들도  안하나",
          "original_form": "주변친구들도  안하나",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008501.1.15",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008501.1.16",
          "form": "아닠ㅋㅋㅋ 친구들은 연애 많이하더라",
          "original_form": "아닠ㅋㅋㅋ 친구들은 연애 많이하더라",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008501.1.17",
          "form": "쬐끔 부럽진 않아",
          "original_form": "쬐끔 부럽진 않아",
          "speaker_id": "1",
          "time": "20210822 17:47"
        },
        {
          "id": "MDRW2100008501.1.18",
          "form": "음 나는 별로 안부러웡",
          "original_form": "음 나는 별로 안부러웡",
          "speaker_id": "2",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100008501.1.19",
          "form": "넌 이상형이 누구니",
          "original_form": "넌 이상형이 누구니",
          "speaker_id": "1",
          "time": "20210830 07:04"
        },
        {
          "id": "MDRW2100008501.1.20",
