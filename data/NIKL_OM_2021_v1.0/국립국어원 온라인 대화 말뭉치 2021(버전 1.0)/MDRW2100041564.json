      "utterance": [
        {
          "id": "MDRW2100041564.1.1",
          "form": "출근하셨나요?",
          "original_form": "출근하셨나요?",
          "speaker_id": "1",
          "time": "20210906 10:57"
        },
        {
          "id": "MDRW2100041564.1.2",
          "form": "네~ 회사입니다",
          "original_form": "네~ 회사입니다",
          "speaker_id": "2",
          "time": "20210906 10:57"
        },
        {
          "id": "MDRW2100041564.1.3",
          "form": "월욜인데 업무량이 많으세요?",
          "original_form": "월욜인데 업무량이 많으세요?",
          "speaker_id": "1",
          "time": "20210906 10:58"
        },
        {
          "id": "MDRW2100041564.1.4",
          "form": "전 지금 한가한시기라서",
          "original_form": "전 지금 한가한시기라서",
          "speaker_id": "2",
          "time": "20210906 11:02"
        },
        {
          "id": "MDRW2100041564.1.5",
          "form": "여유롭습니다!",
          "original_form": "여유롭습니다!",
          "speaker_id": "2",
          "time": "20210906 11:02"
        },
        {
          "id": "MDRW2100041564.1.6",
          "form": "그래도 월요일은 싫지만ㅜㅜ",
          "original_form": "그래도 월요일은 싫지만ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 11:02"
        },
        {
          "id": "MDRW2100041564.1.7",
          "form": "오호 저는 월욜마다 바쁜데",
          "original_form": "오호 저는 월욜마다 바쁜데",
          "speaker_id": "1",
          "time": "20210906 11:02"
        },
        {
          "id": "MDRW2100041564.1.8",
          "form": "오늘따라 한가지네용",
          "original_form": "오늘따라 한가지네용",
          "speaker_id": "1",
          "time": "20210906 11:02"
        },
        {
          "id": "MDRW2100041564.1.9",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210906 11:03"
        },
        {
          "id": "MDRW2100041564.1.10",
          "form": "그쵸 바쁘시죠ㅜㅜ",
          "original_form": "그쵸 바쁘시죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 11:03"
        },
        {
          "id": "MDRW2100041564.1.11",
          "form": "덕분에 수다떨고있네여 ㅋㅋ",
          "original_form": "덕분에 수다떨고있네여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 11:03"
        },
        {
          "id": "MDRW2100041564.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:03"
        },
        {
          "id": "MDRW2100041564.1.13",
          "form": "으 월요일에 정말 메일함에 안읽은메일 쌓여있으면",
          "original_form": "으 월요일에 정말 메일함에 안읽은메일 쌓여있으면",
          "speaker_id": "2",
          "time": "20210906 11:04"
        },
        {
          "id": "MDRW2100041564.1.14",
          "form": "스트레스가ㅋㅋㅋ",
          "original_form": "스트레스가ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:04"
        },
        {
          "id": "MDRW2100041564.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ안읽은메일 뭔지알죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ안읽은메일 뭔지알죠",
          "speaker_id": "1",
          "time": "20210906 11:24"
        },
        {
          "id": "MDRW2100041564.1.16",
          "form": "점심식사는 이제 하시나요?",
          "original_form": "점심식사는 이제 하시나요?",
          "speaker_id": "2",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100041564.1.17",
          "form": "12시반부터요 님은 ?",
          "original_form": "12시반부터요 님은 ?",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041564.1.18",
          "form": "저는 먹고왔습니다!",
          "original_form": "저는 먹고왔습니다!",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041564.1.19",
          "form": "맛있게드세욥~",
          "original_form": "맛있게드세욥~",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041564.1.20",
          "form": "뭐드셨나용",
          "original_form": "뭐드셨나용",
          "speaker_id": "1",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041564.1.21",
          "form": "전 짬뽕밥 먹으러왔어용",
          "original_form": "전 짬뽕밥 먹으러왔어용",
          "speaker_id": "1",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041564.1.22",
          "form": "!!!!!",
          "original_form": "!!!!!",
          "speaker_id": "2",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041564.1.23",
          "form": "저도 짬뽕밥먹었는데ㅋㅋㅋㅋㅋ",
          "original_form": "저도 짬뽕밥먹었는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:24"
        },
        {
          "id": "MDRW2100041564.1.24",
