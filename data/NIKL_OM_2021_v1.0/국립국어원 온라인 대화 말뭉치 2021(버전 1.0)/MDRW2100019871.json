      "utterance": [
        {
          "id": "MDRW2100019871.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019871.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019871.1.3",
          "form": "전 오늘도 버블티를 먹었습니다",
          "original_form": "전 오늘도 버블티를 먹었습니다",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019871.1.4",
          "form": "아마스빈에서",
          "original_form": "아마스빈에서",
          "speaker_id": "2",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019871.1.5",
          "form": "아하! 버블티좋아하시나봐요?",
          "original_form": "아하! 버블티좋아하시나봐요?",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019871.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019871.1.7",
          "form": "name1님은 뭐 좋아하시나요?",
          "original_form": "&name1&님은 뭐 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019871.1.8",
          "form": "저는 디저트는 아이스크림좋아해요~",
          "original_form": "저는 디저트는 아이스크림좋아해요~",
          "speaker_id": "1",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019871.1.9",
          "form": "무슨 아이스크림이요?",
          "original_form": "무슨 아이스크림이요?",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019871.1.10",
          "form": "전 배라좋아해요~ 배라3픽여쭤봐도될까요?",
          "original_form": "전 배라좋아해요~ 배라3픽여쭤봐도될까요?",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019871.1.11",
          "form": "베라 저의 3픽은",
          "original_form": "베라 저의 3픽은",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019871.1.12",
          "form": "그린티와..",
          "original_form": "그린티와..",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019871.1.13",
          "form": "아니다 초코나무숲 이랑",
          "original_form": "아니다 초코나무숲 이랑",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019871.1.14",
          "form": "넼ㅋㅋ",
          "original_form": "넼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019871.1.15",
          "form": "북극곰폴라베어",
          "original_form": "북극곰폴라베어",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019871.1.16",
          "form": "나 민트초코랑",
          "original_form": "나 민트초코랑",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019871.1.17",
          "form": "사랑에 빠진 딸기요 ㅎㅎ",
          "original_form": "사랑에 빠진 딸기요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019871.1.18",
          "form": "힘들게 골랐네요~",
          "original_form": "힘들게 골랐네요~",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019871.1.19",
          "form": "앜ㅋㅋㅋ 애쓰셨어요~",
          "original_form": "앜ㅋㅋㅋ 애쓰셨어요~",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019871.1.20",
          "form": "그나저나 북극곰폴라베어아시다니 먹잘알!",
          "original_form": "그나저나 북극곰폴라베어아시다니 먹잘알!",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019871.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그런가요 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그런가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019871.1.22",
          "form": "name1님은 뭐좋아하세요? 3픽",
          "original_form": "&name1&님은 뭐좋아하세요? 3픽",
          "speaker_id": "2",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019871.1.23",
          "form": "제3픽은 피스타치오아몬드 쿠앤크 그리고 아몬드봉봉이요 ㅎㅎ",
          "original_form": "제3픽은 피스타치오아몬드 쿠앤크 그리고 아몬드봉봉이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019871.1.24",
          "form": "오 견과류 좋아하시네요",
          "original_form": "오 견과류 좋아하시네요",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019871.1.25",
          "form": "네맞아요~ 뭔가 들어간거좋아해요",
          "original_form": "네맞아요~ 뭔가 들어간거좋아해요",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019871.1.26",
          "form": "슈팅스타 같은건요?",
          "original_form": "슈팅스타 같은건요?",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019871.1.27",
          "form": "아 슈팅스타는 넘 심하게 달아서 패스요 ㅋㅋ",
          "original_form": "아 슈팅스타는 넘 심하게 달아서 패스요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019871.1.28",
          "form": "오 너무 단거는 안좋아하시는군요",
          "original_form": "오 너무 단거는 안좋아하시는군요",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019871.1.29",
          "form": "흠 나뚜루는 어때요",
          "original_form": "흠 나뚜루는 어때요",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019871.1.30",
          "form": "사실 전 편의점 나뚜루를 더 좋아한답니다",
          "original_form": "사실 전 편의점 나뚜루를 더 좋아한답니다",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019871.1.31",
          "form": "나뚜루 좋아해요~ 비싸지만않으면 하루한개씩 클리어가능이죠",
          "original_form": "나뚜루 좋아해요~ 비싸지만않으면 하루한개씩 클리어가능이죠",
          "speaker_id": "1",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100019871.1.32",
          "form": "저도 원쁠원할때 사먹는답니다",
          "original_form": "저도 원쁠원할때 사먹는답니다",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019871.1.33",
          "form": "그린티는 필수죠",
          "original_form": "그린티는 필수죠",
          "speaker_id": "2",
          "time": "20210827 23:04"
        }
      ]
    }
