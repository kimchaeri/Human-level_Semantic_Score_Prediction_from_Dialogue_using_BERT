      "utterance": [
        {
          "id": "MDRW2100039395.1.1",
          "form": "기사셤 봐야대 하기시러",
          "original_form": "기사셤 봐야대 하기시러",
          "speaker_id": "2",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100039395.1.2",
          "form": "회사에서 강의도 틀어놓는데 별로 안들어옴",
          "original_form": "회사에서 강의도 틀어놓는데 별로 안들어옴",
          "speaker_id": "2",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100039395.1.3",
          "form": "아 나두... 왜 주관식이지",
          "original_form": "아 나두... 왜 주관식이지",
          "speaker_id": "1",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100039395.1.4",
          "form": "객관식이면 통과했을텐데",
          "original_form": "객관식이면 통과했을텐데",
          "speaker_id": "1",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100039395.1.5",
          "form": "(ㅈㄴ",
          "original_form": "(ㅈㄴ",
          "speaker_id": "1",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100039395.1.6",
          "form": "ㅋㅋㅋㅋ개롭... 모든걸 알고있어야 풀수있는",
          "original_form": "ㅋㅋㅋㅋ개롭... 모든걸 알고있어야 풀수있는",
          "speaker_id": "2",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100039395.1.7",
          "form": "몬한다구요ㅠㅠ",
          "original_form": "몬한다구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100039395.1.8",
          "form": "일본서 시험도 접수해야하는디...",
          "original_form": "일본서 시험도 접수해야하는디...",
          "speaker_id": "1",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100039395.1.9",
          "form": "진짜 보는거임?ㅋㅋㅋㅋ",
          "original_form": "진짜 보는거임?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039395.1.10",
          "form": "웅 책도 받아왔다",
          "original_form": "웅 책도 받아왔다",
          "speaker_id": "1",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100039395.1.11",
          "form": "이제 애니메이션도 자막없이 보는거야?",
          "original_form": "이제 애니메이션도 자막없이 보는거야?",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100039395.1.12",
          "form": "그건 가능했음 좋겠다",
          "original_form": "그건 가능했음 좋겠다",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039395.1.13",
          "form": "그럼 드라마 씨디 알아 먹을 수 있는데",
          "original_form": "그럼 드라마 씨디 알아 먹을 수 있는데",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039395.1.14",
          "form": "유퀴즈에서 봉준호 통역사가 덕질이 제일 언어배우는데 좋댔는데",
          "original_form": "유퀴즈에서 봉준호 통역사가 덕질이 제일 언어배우는데 좋댔는데",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039395.1.15",
          "form": "그건... 맞다...",
          "original_form": "그건... 맞다...",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039395.1.16",
          "form": "그치만 노력 필요하다....",
          "original_form": "그치만 노력 필요하다....",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039395.1.17",
          "form": "경험담?",
          "original_form": "경험담?",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039395.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100039395.1.19",
          "form": "나는 노력 없어서 실력도 없다...",
          "original_form": "나는 노력 없어서 실력도 없다...",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039395.1.20",
          "form": "왜... 학원도 다녔자너",
          "original_form": "왜... 학원도 다녔자너",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
