      "utterance": [
        {
          "id": "MDRW2100034742.1.1",
          "form": "요즘 구매하신 물건이 있으신가요?",
          "original_form": "요즘 구매하신 물건이 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100034742.1.2",
          "form": "저는 트렌치코트요 ㅋㅋㅋ",
          "original_form": "저는 트렌치코트요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100034742.1.3",
          "form": "오호 가을맞이 아이템이네요",
          "original_form": "오호 가을맞이 아이템이네요",
          "speaker_id": "1",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100034742.1.4",
          "form": "마음에 드시나요?",
          "original_form": "마음에 드시나요?",
          "speaker_id": "1",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100034742.1.5",
          "form": "네 맘에들어요 ㅋㅋㅋ",
          "original_form": "네 맘에들어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100034742.1.6",
          "form": "쇼핑은 해도해도 끝이 없더라구요",
          "original_form": "쇼핑은 해도해도 끝이 없더라구요",
          "speaker_id": "2",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100034742.1.7",
          "form": "쇼핑 한번 시작하면 끝없죠",
          "original_form": "쇼핑 한번 시작하면 끝없죠",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034742.1.8",
          "form": "저는 최근에 가방을 2개 샀는데",
          "original_form": "저는 최근에 가방을 2개 샀는데",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034742.1.9",
          "form": "하나는 비건가죽 가방인데 포인트로 들기 좋고",
          "original_form": "하나는 비건가죽 가방인데 포인트로 들기 좋고",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034742.1.10",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034742.1.11",
          "form": "하나는 가죽백이 가벼워서 좋더라고요",
          "original_form": "하나는 가죽백이 가벼워서 좋더라고요",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034742.1.12",
          "form": "오 성공하셨네요",
          "original_form": "오 성공하셨네요",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034742.1.13",
          "form": "실패하면 골치에요",
          "original_form": "실패하면 골치에요",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034742.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034742.1.15",
          "form": "인터넷으로 사서 혹시 맘에 안들면 어쩌나 생각했는데",
          "original_form": "인터넷으로 사서 혹시 맘에 안들면 어쩌나 생각했는데",
          "speaker_id": "1",
          "time": "20210828 18:45"
        },
        {
          "id": "MDRW2100034742.1.16",
          "form": "넵 ㅋㅋㅋ",
          "original_form": "넵 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100034742.1.17",
          "form": "쇼핑 성공해서 다행이었어요",
          "original_form": "쇼핑 성공해서 다행이었어요",
          "speaker_id": "1",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100034742.1.18",
          "form": "실패하면 교환환불에 골치죠 ㅜㅜ",
          "original_form": "실패하면 교환환불에 골치죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100034742.1.19",
          "form": "네네 게다가 해외배송이면ㅠ",
          "original_form": "네네 게다가 해외배송이면ㅠ",
          "speaker_id": "1",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034742.1.20",
          "form": "하 생각만해도 골치네요 ㅠ",
          "original_form": "하 생각만해도 골치네요 ㅠ",
