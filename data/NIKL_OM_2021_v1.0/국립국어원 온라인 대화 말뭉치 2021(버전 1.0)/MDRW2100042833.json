      "utterance": [
        {
          "id": "MDRW2100042833.1.1",
          "form": "가산동에 맛집 되게 많은거같던데",
          "original_form": "가산동에 맛집 되게 많은거같던데",
          "speaker_id": "1",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042833.1.2",
          "form": "맞나요??",
          "original_form": "맞나요??",
          "speaker_id": "1",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042833.1.3",
          "form": "그런가요? 저 도시락파라ㅋㅋ",
          "original_form": "그런가요? 저 도시락파라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042833.1.4",
          "form": "디저트집은 몇개 알아요ㅋㅋ",
          "original_form": "디저트집은 몇개 알아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042833.1.5",
          "form": "ㅋㅋㅋ맛집뿌시기 이런거 잘 안하세여?ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ맛집뿌시기 이런거 잘 안하세여?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042833.1.6",
          "form": "오 디저트집 어떤 디저트인가요",
          "original_form": "오 디저트집 어떤 디저트인가요",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042833.1.7",
          "form": "회사내 도시락모임가지다 코로나로 각자ㅜ",
          "original_form": "회사내 도시락모임가지다 코로나로 각자ㅜ",
          "speaker_id": "2",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042833.1.8",
          "form": "마카롱집있어요!",
          "original_form": "마카롱집있어요!",
          "speaker_id": "2",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042833.1.9",
          "form": "회사내에서도 아예 따로 드세요?",
          "original_form": "회사내에서도 아예 따로 드세요?",
          "speaker_id": "1",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042833.1.10",
          "form": "굉장히 철저한 회사군요. 굳",
          "original_form": "굉장히 철저한 회사군요. 굳",
          "speaker_id": "1",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042833.1.11",
          "form": "마카롱! 뚱카롱인가요 클래식인가욤!",
          "original_form": "마카롱! 뚱카롱인가요 클래식인가욤!",
          "speaker_id": "1",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042833.1.12",
          "form": "ㅋㅋ마카롱 기본이요!",
          "original_form": "ㅋㅋ마카롱 기본이요!",
          "speaker_id": "2",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100042833.1.13",
          "form": "다른 구움과자도 다양하게 있어요",
          "original_form": "다른 구움과자도 다양하게 있어요",
          "speaker_id": "2",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100042833.1.14",
          "form": "플레잉디오븐",
          "original_form": "플레잉디오븐",
          "speaker_id": "2",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100042833.1.15",
          "form": "헉 구움과자 조아해요!!!",
          "original_form": "헉 구움과자 조아해요!!!",
          "speaker_id": "1",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100042833.1.16",
          "form": "플레잉디오븐..",
          "original_form": "플레잉디오븐..",
          "speaker_id": "1",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100042833.1.17",
          "form": "기억한다...",
          "original_form": "기억한다...",
          "speaker_id": "1",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100042833.1.18",
          "form": "디저트류 좋아하세요?",
          "original_form": "디저트류 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100042833.1.19",
          "form": "네ㅜ 끊어야하는데 회사에 있으니까 더 먹어요ㅜㅠ",
          "original_form": "네ㅜ 끊어야하는데 회사에 있으니까 더 먹어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100042833.1.20",
          "form": "회사에사는",
          "original_form": "회사에사는",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100042833.1.21",
          "form": "왜그렇게 당이 떨어질까요!!??",
          "original_form": "왜그렇게 당이 떨어질까요!!??",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100042833.1.22",
          "form": "근데 안먹는 사람은 아예 안먹더라고요",
          "original_form": "근데 안먹는 사람은 아예 안먹더라고요",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100042833.1.23",
          "form": "대단...",
          "original_form": "대단...",
          "speaker_id": "1",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100042833.1.24",
          "form": "저는 회사가 학동인데 진짜 먹을거 없어요 ㅋㅋㅋ",
          "original_form": "저는 회사가 학동인데 진짜 먹을거 없어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100042833.1.25",
          "form": "근데 사먹으면 식비 미1쳐가지고",
          "original_form": "근데 사먹으면 식비 미1쳐가지고",
          "speaker_id": "1",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100042833.1.26",
          "form": "그냥 싸갖고다녀요",
          "original_form": "그냥 싸갖고다녀요",
          "speaker_id": "1",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100042833.1.27",
          "form": "도시락이요?",
          "original_form": "도시락이요?",
          "speaker_id": "2",
          "time": "20210907 10:24"
        },
        {
          "id": "MDRW2100042833.1.28",
          "form": "주로 뭐싸세요? 전 오늘 고구마",
          "original_form": "주로 뭐싸세요? 전 오늘 고구마",
          "speaker_id": "2",
          "time": "20210907 10:24"
        },
        {
          "id": "MDRW2100042833.1.29",
          "form": "고구마아??? 다욧하세요???",
          "original_form": "고구마아??? 다욧하세요???",
          "speaker_id": "1",
          "time": "20210907 10:27"
        },
        {
          "id": "MDRW2100042833.1.30",
          "form": "저는 완전 밥순이라서 글고 ㅋㅋㅋㅋ 많이먹어야돼요 ㅠㅠ",
          "original_form": "저는 완전 밥순이라서 글고 ㅋㅋㅋㅋ 많이먹어야돼요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 10:27"
        },
        {
          "id": "MDRW2100042833.1.31",
          "form": "무조건 밥임다",
          "original_form": "무조건 밥임다",
          "speaker_id": "1",
          "time": "20210907 10:27"
        },
        {
          "id": "MDRW2100042833.1.32",
          "form": "냉동볶음밥이나 도시락 배달업체에서 시켜먹어요",
          "original_form": "냉동볶음밥이나 도시락 배달업체에서 시켜먹어요",
          "speaker_id": "1",
          "time": "20210907 10:28"
        },
        {
          "id": "MDRW2100042833.1.33",
          "form": "원래 밥싸오는데 아침에 귀찮아서ㅋㅋ",
          "original_form": "원래 밥싸오는데 아침에 귀찮아서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100042833.1.34",
          "form": "낙지냉동볶음밥도 있어요",
          "original_form": "낙지냉동볶음밥도 있어요",
          "speaker_id": "2",
          "time": "20210907 10:32"
        },
        {
