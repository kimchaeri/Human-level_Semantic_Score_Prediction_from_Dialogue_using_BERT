      "utterance": [
        {
          "id": "MDRW2100007310.1.1",
          "form": "오늘 비 엄청 오더라구요",
          "original_form": "오늘 비 엄청 오더라구요",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007310.1.2",
          "form": "네 맞아요 하늘에 구멍뚫린줄알았어요",
          "original_form": "네 맞아요 하늘에 구멍뚫린줄알았어요",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007310.1.3",
          "form": "온통 오전시간을 내리 밖에있었는데",
          "original_form": "온통 오전시간을 내리 밖에있었는데",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007310.1.4",
          "form": "실내 들어가자마자 화창해져서 좀 어이 없는 날씨였어요 ㅋㅋㅋㅋㅋ",
          "original_form": "실내 들어가자마자 화창해져서 좀 어이 없는 날씨였어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007310.1.5",
          "form": "하늘이 많이 아파보였어요ㅜㅠ",
          "original_form": "하늘이 많이 아파보였어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007310.1.6",
          "form": "이번주 내리",
          "original_form": "이번주 내리",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007310.1.7",
          "form": "비온다는 이야기도 있더라고요",
          "original_form": "비온다는 이야기도 있더라고요",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007310.1.8",
          "form": "하아...ㅜㅜ 장마가 다시오나봐요",
          "original_form": "하아...ㅜㅜ 장마가 다시오나봐요",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007310.1.9",
          "form": "이번엔 가을장마인가요ㅜㅜ",
          "original_form": "이번엔 가을장마인가요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007310.1.10",
          "form": "여름에 다른연도보다 비가 많이 오진 않았으니",
          "original_form": "여름에 다른연도보다 비가 많이 오진 않았으니",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007310.1.11",
          "form": "지금이라도 오는걸까요",
          "original_form": "지금이라도 오는걸까요",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007310.1.12",
          "form": "우산 꼭! 큰거 잘 쓰고 다니셔요",
          "original_form": "우산 꼭! 큰거 잘 쓰고 다니셔요",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007310.1.13",
          "form": "그래도 일주일 내내는 너무 자닌한거 같아요ㅜㅜ",
          "original_form": "그래도 일주일 내내는 너무 자닌한거 같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007310.1.14",
          "form": "저는 온통 운동화 안에 물이 찼답니다 ㅎ",
          "original_form": "저는 온통 운동화 안에 물이 찼답니다 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007310.1.15",
          "form": "악ㅋㅋㅋ쿠ㅜㅜㅜㅜ 운동화 말리기 너무 힘들겠어요ㅜ",
          "original_form": "악ㅋㅋㅋ쿠ㅜㅜㅜㅜ 운동화 말리기 너무 힘들겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007310.1.16",
          "form": "ㅋㅋㅋㅋㅋ 난간에 말려놓고있었는데",
          "original_form": "ㅋㅋㅋㅋㅋ 난간에 말려놓고있었는데",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007310.1.17",
          "form": "그 사이 낮에 비가 또와서",
          "original_form": "그 사이 낮에 비가 또와서",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007310.1.18",
          "form": "말리나 마나가 됐어요",
          "original_form": "말리나 마나가 됐어요",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007310.1.19",
          "form": "악ㅜㅜㅜ 그렇네요ㅜㅜ",
          "original_form": "악ㅜㅜㅜ 그렇네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007310.1.20",
          "form": "어쩐데요ㅜㅜ 비오는날 생각하면 장화가 있어야되는데",
          "original_form": "어쩐데요ㅜㅜ 비오는날 생각하면 장화가 있어야되는데",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007310.1.21",
          "form": "또 장화는 너무 무거워서 불편하고 ㅜ",
          "original_form": "또 장화는 너무 무거워서 불편하고 ㅜ",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007310.1.22",
          "form": "그냥 슬리퍼나 샌들이",
          "original_form": "그냥 슬리퍼나 샌들이",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007310.1.23",
          "form": "답인거 같아요 크록스 하나 장만해야할까 싶네요",
          "original_form": "답인거 같아요 크록스 하나 장만해야할까 싶네요",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007310.1.24",
          "form": "크록스 어렸을 때 신고 안신어봤는데 ㅋㅋㅋㅋ",
          "original_form": "크록스 어렸을 때 신고 안신어봤는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007310.1.25",
          "form": "그쵸ㅜㅜ 크록스 신으면 안에서 발이 퉁퉁불것 같아욬ㅋㅋㅋ",
          "original_form": "그쵸ㅜㅜ 크록스 신으면 안에서 발이 퉁퉁불것 같아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007310.1.26",
          "form": "왠지 그런이미지라며",
          "original_form": "왠지 그런이미지라며",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
