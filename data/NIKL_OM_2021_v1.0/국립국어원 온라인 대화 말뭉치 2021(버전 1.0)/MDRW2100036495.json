        },
        {
          "id": "MDRW2100036495.1.2",
          "form": "안녕하세요!! :>",
          "original_form": "안녕하세요!! :>",
          "speaker_id": "2",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100036495.1.3",
          "form": "혹시 이상형어떻게되세요",
          "original_form": "혹시 이상형어떻게되세요",
          "speaker_id": "1",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100036495.1.4",
          "form": "저는 키 큰 사람 좋아래요!",
          "original_form": "저는 키 큰 사람 좋아래요!",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036495.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 09:46"
        },
        {
          "id": "MDRW2100036495.1.6",
          "form": "전 베이글남이요",
          "original_form": "전 베이글남이요",
          "speaker_id": "1",
          "time": "20210831 13:38"
        },
        {
          "id": "MDRW2100036495.1.7",
          "form": "키큰사람이면 180이상이여?",
          "original_form": "키큰사람이면 180이상이여?",
          "speaker_id": "1",
          "time": "20210831 13:38"
        },
        {
          "id": "MDRW2100036495.1.8",
          "form": "네네!! 헐 저도 베이글남 좋아해요",
          "original_form": "네네!! 헐 저도 베이글남 좋아해요",
          "speaker_id": "2",
          "time": "20210831 14:13"
        },
        {
          "id": "MDRW2100036495.1.9",
          "form": "키 크고 흉통이 컸음 좋겠어요",
          "original_form": "키 크고 흉통이 컸음 좋겠어요",
          "speaker_id": "2",
          "time": "20210831 14:13"
        },
        {
          "id": "MDRW2100036495.1.10",
          "form": "대신 허리는 얇은 ㅎ.ㅎ",
          "original_form": "대신 허리는 얇은 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100036495.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅎㅎㅎㅎㅎ저도요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅎㅎㅎㅎㅎ저도요",
          "speaker_id": "1",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100036495.1.12",
          "form": "전 키는 저보다 한15센치만 더커도상관없는데 멸치는싫어여",
          "original_form": "전 키는 저보다 한15센치만 더커도상관없는데 멸치는싫어여",
          "speaker_id": "1",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100036495.1.13",
          "form": "그니까요! 저 예전에는 무조건 삐쩍 마른 사람이 좋았거든요?",
          "original_form": "그니까요! 저 예전에는 무조건 삐쩍 마른 사람이 좋았거든요?",
          "speaker_id": "2",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100036495.1.14",
          "form": "모델처럼??",
          "original_form": "모델처럼??",
          "speaker_id": "2",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100036495.1.15",
          "form": "근데 크니까 ^.^... 흉통의 멋을 알게 되더라고요",
          "original_form": "근데 크니까 ^.^... 흉통의 멋을 알게 되더라고요",
          "speaker_id": "2",
          "time": "20210831 14:16"
        },
        {
          "id": "MDRW2100036495.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그쳐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그쳐",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036495.1.17",
          "form": "두툼한 그 맛이있어여",
          "original_form": "두툼한 그 맛이있어여",
          "speaker_id": "1",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036495.1.18",
          "form": "그 막 연예인으로 치면",
          "original_form": "그 막 연예인으로 치면",
          "speaker_id": "2",
          "time": "20210831 14:34"
        },
        {
          "id": "MDRW2100036495.1.19",
          "form": "몬스타엑스 아세요?? 저 그런 몸이 너무 좋아요!",
          "original_form": "몬스타엑스 아세요?? 저 그런 몸이 너무 좋아요!",
          "speaker_id": "2",
          "time": "20210831 14:35"
        },
        {
          "id": "MDRW2100036495.1.20",
          "form": "요새 창균이가 아주 좋더라구여",
          "original_form": "요새 창균이가 아주 좋더라구여",
          "speaker_id": "1",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100036495.1.21",
          "form": "잘컸어요",
          "original_form": "잘컸어요",
          "speaker_id": "1",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100036495.1.22",
          "form": "헐 대박 제 최애멤버",
          "original_form": "헐 대박 제 최애멤버",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036495.1.23",
          "form": "어깨 넓어진 거 보고 기절할 뻔했어요",
          "original_form": "어깨 넓어진 거 보고 기절할 뻔했어요",
          "speaker_id": "2",
          "time": "20210831 14:41"
        },
        {
          "id": "MDRW2100036495.1.24",
          "form": "ㅋㅋㅋㅋ그니까여 어느순간 몸이 딱 벌어졌도라규여",
          "original_form": "ㅋㅋㅋㅋ그니까여 어느순간 몸이 딱 벌어졌도라규여",
          "speaker_id": "1",
