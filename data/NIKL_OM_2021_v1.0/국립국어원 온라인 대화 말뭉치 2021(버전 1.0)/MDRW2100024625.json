        },
        {
          "id": "MDRW2100024625.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 16:03"
        },
        {
          "id": "MDRW2100024625.1.3",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "1",
          "time": "20210831 16:03"
        },
        {
          "id": "MDRW2100024625.1.4",
          "form": "네네 카야토스트 먹었어요",
          "original_form": "네네 카야토스트 먹었어요",
          "speaker_id": "2",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100024625.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100024625.1.6",
          "form": "카야토스트 만드신거에요?",
          "original_form": "카야토스트 만드신거에요?",
          "speaker_id": "1",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100024625.1.7",
          "form": "전 짜장밥이요",
          "original_form": "전 짜장밥이요",
          "speaker_id": "1",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100024625.1.8",
          "form": "네 알바중이라 ㅋㅋ 파는 메뉴에요",
          "original_form": "네 알바중이라 ㅋㅋ 파는 메뉴에요",
          "speaker_id": "2",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100024625.1.9",
          "form": "짜장vs짬뽕 이면 짜장이신가요",
          "original_form": "짜장vs짬뽕 이면 짜장이신가요",
          "speaker_id": "2",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100024625.1.10",
          "form": "오 알바중이시구나",
          "original_form": "오 알바중이시구나",
          "speaker_id": "1",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100024625.1.11",
          "form": "전 그때 그때 다른데",
          "original_form": "전 그때 그때 다른데",
          "speaker_id": "1",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100024625.1.12",
          "form": "비오는날은 무조건짬뽕 그이외는 보통은 짜장이여 ㅎㅎ",
          "original_form": "비오는날은 무조건짬뽕 그이외는 보통은 짜장이여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100024625.1.13",
          "form": "아하 비오는날은 국물이 땡기죠",
          "original_form": "아하 비오는날은 국물이 땡기죠",
          "speaker_id": "2",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100024625.1.14",
          "form": "탕수육 부먹vs찍먹 이거는요",
          "original_form": "탕수육 부먹vs찍먹 이거는요",
          "speaker_id": "2",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100024625.1.15",
          "form": "핫 그것도어려운데 전반반파거든여",
          "original_form": "핫 그것도어려운데 전반반파거든여",
          "speaker_id": "1",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100024625.1.16",
          "form": "일단 찍먹상태로배달이오면",
          "original_form": "일단 찍먹상태로배달이오면",
          "speaker_id": "1",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100024625.1.17",
          "form": "반은 덜어서 찍어먹고 반은부어먹어요",
          "original_form": "반은 덜어서 찍어먹고 반은부어먹어요",
          "speaker_id": "1",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100024625.1.18",
          "form": "오호.. 뭔가 타협을 위해서? 아님 혼자 드실때도?",
          "original_form": "오호.. 뭔가 타협을 위해서? 아님 혼자 드실때도?",
          "speaker_id": "2",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100024625.1.19",
          "form": "네 무조건 그게 둘다 맛이 다르고 각각다맛있어요",
          "original_form": "네 무조건 그게 둘다 맛이 다르고 각각다맛있어요",
          "speaker_id": "1",
          "time": "20210831 16:08"
        },
        {
          "id": "MDRW2100024625.1.20",
          "form": "찍먹은 끝까지바삭하고 부먹은 소스가 잘 베여들고 바삭하면서 촉촉하거든여",
          "original_form": "찍먹은 끝까지바삭하고 부먹은 소스가 잘 베여들고 바삭하면서 촉촉하거든여",
          "speaker_id": "1",
          "time": "20210831 16:08"
        },
        {
          "id": "MDRW2100024625.1.21",
          "form": "오 약간 전문가같은ㅋㅋㅋ 저는 부먹이 좋아요",
          "original_form": "오 약간 전문가같은ㅋㅋㅋ 저는 부먹이 좋아요",
          "speaker_id": "2",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100024625.1.22",
          "form": "ㅋㅋㅋㅋㅋ먹는거 좋아해서요 ㅎㅎㄹㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ먹는거 좋아해서요 ㅎㅎㄹㅎ",
          "speaker_id": "1",
          "time": "20210831 16:34"
        }
      ]
