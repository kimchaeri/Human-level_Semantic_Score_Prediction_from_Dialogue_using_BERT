          "original_form": "혐",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016509.1.11",
          "form": "일듯",
          "original_form": "일듯",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016509.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016509.1.13",
          "form": "아 뭐",
          "original_form": "아 뭐",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016509.1.14",
          "form": "커뮤야 너무 몰입만 안 하면",
          "original_form": "커뮤야 너무 몰입만 안 하면",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016509.1.15",
          "form": "숨기시면되나요",
          "original_form": "숨기시면되나요",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016509.1.16",
          "form": "ㅋㅋㅋㅋ ㅋ ㅋ ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ ㅋ ㅋ ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016509.1.17",
          "form": "정보도 얻고 좋죠",
          "original_form": "정보도 얻고 좋죠",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016509.1.18",
          "form": "소식도 빨리 접하고",
          "original_form": "소식도 빨리 접하고",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016509.1.19",
          "form": "박씨 과몰입러같은데",
          "original_form": "박씨 과몰입러같은데",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016509.1.20",
          "form": "저번에보니까",
          "original_form": "저번에보니까",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016509.1.21",
          "form": "하나씩 다 해보신것같더라구요",
          "original_form": "하나씩 다 해보신것같더라구요",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016509.1.22",
          "form": "어쩜 그리 잘아는지",
          "original_form": "어쩜 그리 잘아는지",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016509.1.23",
          "form": "이씨",
          "original_form": "이씨",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016509.1.24",
          "form": "커뮤하시죠?",
          "original_form": "커뮤하시죠?",
          "speaker_id": "1",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016509.1.25",
          "form": "저 네이트판",
          "original_form": "저 네이트판",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016509.1.26",
          "form": "합니다",
          "original_form": "합니다",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016509.1.27",
          "form": "프로주작러에요",
          "original_form": "프로주작러에요",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016509.1.28",
