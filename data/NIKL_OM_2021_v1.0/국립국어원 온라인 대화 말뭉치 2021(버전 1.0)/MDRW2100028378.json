      "utterance": [
        {
          "id": "MDRW2100028378.1.1",
          "form": "마라샹궈도 맛있더라고용",
          "original_form": "마라샹궈도 맛있더라고용",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028378.1.2",
          "form": "오오 그건 뭔가 더 강렬하게 생겼는데 많이 매워요?",
          "original_form": "오오 그건 뭔가 더 강렬하게 생겼는데 많이 매워요?",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100028378.1.3",
          "form": "볶음이라 물이 없어서 그런지 더매웠어요..!!",
          "original_form": "볶음이라 물이 없어서 그런지 더매웠어요..!!",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100028378.1.4",
          "form": "오왕 스트레스 풀리겠어요!!",
          "original_form": "오왕 스트레스 풀리겠어요!!",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100028378.1.5",
          "form": "스트레스 받을때 주로 뭐 드세요??",
          "original_form": "스트레스 받을때 주로 뭐 드세요??",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100028378.1.6",
          "form": "저는 불닭에 맥주요ㅋㅋㅋㅋㅋ 조합이 적당해요",
          "original_form": "저는 불닭에 맥주요ㅋㅋㅋㅋㅋ 조합이 적당해요",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100028378.1.7",
          "form": "오오오 저도 불닭 먹어요ㅋㅋㅋㅋ",
          "original_form": "오오오 저도 불닭 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100028378.1.8",
          "form": "스트레스 받아도 위는 잘못이 없기 때문에,, 치즈 넣어 먹어요",
          "original_form": "스트레스 받아도 위는 잘못이 없기 때문에,, 치즈 넣어 먹어요",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100028378.1.9",
          "form": "치즈넣어도 맛있겠어요!",
          "original_form": "치즈넣어도 맛있겠어요!",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100028378.1.10",
          "form": "저는 맵찔이라 소스조금넣어요ㅋㅋㅋ",
          "original_form": "저는 맵찔이라 소스조금넣어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100028378.1.11",
          "form": "저두 좀 남겨요ㅋㅋㅋ 한 1/4는 남겨요,,",
          "original_form": "저두 좀 남겨요ㅋㅋㅋ 한 1/4는 남겨요,,",
          "speaker_id": "1",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100028378.1.12",
          "form": "진짜 불닭엔 체다치즈가 전 제일 맛있더라구요ㅜㅜ",
          "original_form": "진짜 불닭엔 체다치즈가 전 제일 맛있더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100028378.1.13",
          "form": "저는 1/3만 넣어요ㅋㅋㅋㅋ",
          "original_form": "저는 1/3만 넣어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100028378.1.14",
          "form": "체다치즈!",
          "original_form": "체다치즈!",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100028378.1.15",
          "form": "슬라이스로 된걸로 드시나용?",
          "original_form": "슬라이스로 된걸로 드시나용?",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100028378.1.16",
          "form": "그정도만 넣어도 강렬하죠! 충분히 매워요ㅜㅜ",
          "original_form": "그정도만 넣어도 강렬하죠! 충분히 매워요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100028378.1.17",
          "form": "맞아요ㅋㅋㅋ 불닭완전 이름값해요",
          "original_form": "맞아요ㅋㅋㅋ 불닭완전 이름값해요",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100028378.1.18",
          "form": "네네 하얀색 체다치즈 넣는데 노란치즈랑 다른지는 잘 모르겠어요ㅋㅋㅋ",
          "original_form": "네네 하얀색 체다치즈 넣는데 노란치즈랑 다른지는 잘 모르겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100028378.1.19",
          "form": "맞아요 불닭 시리즈 중에 뭘 제일 좋아하세요?",
          "original_form": "맞아요 불닭 시리즈 중에 뭘 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100028378.1.20",
          "form": "까르보불닭이요! 그나마 덜매워서요ㅋㅋㅋㅋ",
          "original_form": "까르보불닭이요! 그나마 덜매워서요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100028378.1.21",
          "form": "와 저도 한동안 그거에 빠져서 그것만 먹었었어요ㅋㅋㅋ",
          "original_form": "와 저도 한동안 그거에 빠져서 그것만 먹었었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100028378.1.22",
          "form": "부드럽기도 해서 너무 맛있어용ㅋㅋㅋㅋ",
          "original_form": "부드럽기도 해서 너무 맛있어용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100028378.1.23",
          "form": "까르보가 부드럽기도해서 제일좋아요ㅋㅋㅋㅋ",
          "original_form": "까르보가 부드럽기도해서 제일좋아요ㅋㅋㅋㅋ",
