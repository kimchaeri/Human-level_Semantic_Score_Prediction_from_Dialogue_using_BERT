        {
          "id": "MDRW2100025712.1.1",
          "form": "ㅇㅇ 빨리 치료제 나와서 걍 독감처럼 되길ㅜㅜㅠ",
          "original_form": "ㅇㅇ 빨리 치료제 나와서 걍 독감처럼 되길ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100025712.1.2",
          "form": "진짜지겨워",
          "original_form": "진짜지겨워",
          "speaker_id": "2",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100025712.1.3",
          "form": "마자 이제그만좀했으면",
          "original_form": "마자 이제그만좀했으면",
          "speaker_id": "1",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025712.1.4",
          "form": "넘지겨움",
          "original_form": "넘지겨움",
          "speaker_id": "1",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025712.1.5",
          "form": "2년이나 이렇게",
          "original_form": "2년이나 이렇게",
          "speaker_id": "1",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100025712.1.6",
          "form": "아무것다못할거라건",
          "original_form": "아무것다못할거라건",
          "speaker_id": "1",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100025712.1.7",
          "form": "생각다못한...",
          "original_form": "생각다못한...",
          "speaker_id": "1",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100025712.1.8",
          "form": "2년 지났는데 언제 끝날지 모른다는 사실이 진짜 지겨워",
          "original_form": "2년 지났는데 언제 끝날지 모른다는 사실이 진짜 지겨워",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100025712.1.9",
          "form": "ㅜㅜㅜㅜㅜ갈수록 더 암울해",
          "original_form": "ㅜㅜㅜㅜㅜ갈수록 더 암울해",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100025712.1.10",
          "form": "델타 또 다른 변이도 나왔다는데",
          "original_form": "델타 또 다른 변이도 나왔다는데",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100025712.1.11",
          "form": "아진짜.....?????",
          "original_form": "아진짜.....?????",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100025712.1.12",
          "form": "ㅜㅜㅜ아니 델타대체 뭘로변한거야",
          "original_form": "ㅜㅜㅜ아니 델타대체 뭘로변한거야",
          "speaker_id": "1",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025712.1.13",
          "form": "ㅇㅇ일본에서 뭐 또 다른 변이 나왔대",
          "original_form": "ㅇㅇ일본에서 뭐 또 다른 변이 나왔대",
