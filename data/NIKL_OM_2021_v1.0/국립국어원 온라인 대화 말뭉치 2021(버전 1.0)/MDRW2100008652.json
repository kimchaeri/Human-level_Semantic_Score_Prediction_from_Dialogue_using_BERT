      "utterance": [
        {
          "id": "MDRW2100008652.1.1",
          "form": "헐 저도 사실은 웹툰쪽으로 꿈꾸고 있어요",
          "original_form": "헐 저도 사실은 웹툰쪽으로 꿈꾸고 있어요",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008652.1.2",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008652.1.3",
          "form": "웹툰 작가는 아니지만",
          "original_form": "웹툰 작가는 아니지만",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008652.1.4",
          "form": "관련직종으로 어케든 가보려고",
          "original_form": "관련직종으로 어케든 가보려고",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008652.1.5",
          "form": "지금 아등바등 중인데",
          "original_form": "지금 아등바등 중인데",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008652.1.6",
          "form": "웹툰 작가를 꿈꾸시는 건가요?!",
          "original_form": "웹툰 작가를 꿈꾸시는 건가요?!",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008652.1.7",
          "form": "오 작가는 아니군요?",
          "original_form": "오 작가는 아니군요?",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008652.1.8",
          "form": "네!! 전 그림, 글 다 하고 싶어요",
          "original_form": "네!! 전 그림, 글 다 하고 싶어요",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008652.1.9",
          "form": "둘 다 부족해서 노력하는 중이에요",
          "original_form": "둘 다 부족해서 노력하는 중이에요",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008652.1.10",
          "form": "헐헐 너무 멋있어요",
          "original_form": "헐헐 너무 멋있어요",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008652.1.11",
          "form": "아 네네 저는 뭔가 작가는 못할것같아서",
          "original_form": "아 네네 저는 뭔가 작가는 못할것같아서",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008652.1.12",
          "form": "배경 어시나",
          "original_form": "배경 어시나",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008652.1.13",
          "form": "웹툰 PD 요 쪽으로 준비중이에요",
          "original_form": "웹툰 PD 요 쪽으로 준비중이에요",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008652.1.14",
          "form": "뭔가 작가님 서포트하는 쪽으로 가고싶어서ㅠㅠ",
          "original_form": "뭔가 작가님 서포트하는 쪽으로 가고싶어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008652.1.15",
          "form": "하 나중에 저랑 일해주세요. 제가 프로가 되어서",
          "original_form": "하 나중에 저랑 일해주세요. 제가 프로가 되어서",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008652.1.16",
          "form": "고료주는 곳에서 버티고 있을게요",
          "original_form": "고료주는 곳에서 버티고 있을게요",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008652.1.17",
          "form": "우와 너무 좋아요",
          "original_form": "우와 너무 좋아요",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008652.1.18",
          "form": "열심히 할게요!!!",
          "original_form": "열심히 할게요!!!",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008652.1.19",
          "form": "조아요 저도 열심히 할게요!!!!!",
          "original_form": "조아요 저도 열심히 할게요!!!!!",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008652.1.20",
          "form": "꿈이 비슷한 분을 만나다니 이런 행운이 있나요!",
          "original_form": "꿈이 비슷한 분을 만나다니 이런 행운이 있나요!",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008652.1.21",
          "form": "나랑 일해죠야해요(?)",
          "original_form": "나랑 일해죠야해요(?)",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008652.1.22",
          "form": "보통 네이버 노리시더라고요",
          "original_form": "보통 네이버 노리시더라고요",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008652.1.23",
          "form": "글 그림 둘 다 하시는 분들은",
          "original_form": "글 그림 둘 다 하시는 분들은",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008652.1.24",
          "form": "네이버가 최고죠",
          "original_form": "네이버가 최고죠",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008652.1.25",
          "form": "그쵸 아무래도 제일 크니까요...!",
          "original_form": "그쵸 아무래도 제일 크니까요...!",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008652.1.26",
          "form": "저도 웹툰은 네이버만 봐요 ㅋㅋ",
          "original_form": "저도 웹툰은 네이버만 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008652.1.27",
          "form": "그런데 제 경우 작가 데뷔 전에 회사생활도 해보고 싶더라고요",
          "original_form": "그런데 제 경우 작가 데뷔 전에 회사생활도 해보고 싶더라고요",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008652.1.28",
          "form": "맞아요 큰 도움이 되죠",
          "original_form": "맞아요 큰 도움이 되죠",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008652.1.29",
          "form": "회사를 스튜디오쪽으로 경험해보시는 분들도 있더라고요",
          "original_form": "회사를 스튜디오쪽으로 경험해보시는 분들도 있더라고요",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008652.1.30",
          "form": "요즘..하..이게 참 안타까운것이..  웹툰이 워낙에 고퀄이라 스튜디오들이 많이 생겨서",
          "original_form": "요즘..하..이게 참 안타까운것이..  웹툰이 워낙에 고퀄이라 스튜디오들이 많이 생겨서",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008652.1.31",
          "form": "혼자서는 진짜 못그리는 지경이어서 걱정이에요",
          "original_form": "혼자서는 진짜 못그리는 지경이어서 걱정이에요",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008652.1.32",
          "form": "스튜디오요? 정확히 어떤 곳이죠?",
          "original_form": "스튜디오요? 정확히 어떤 곳이죠?",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008652.1.33",
          "form": "선화 , 채색, 배경 다 따로 모집하는 곳이 있는데",
          "original_form": "선화 , 채색, 배경 다 따로 모집하는 곳이 있는데",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008652.1.34",
          "form": "고퀄리티 웹툰 만드는데 개인이 도저히 할 수 없는 분량이다보니",
          "original_form": "고퀄리티 웹툰 만드는데 개인이 도저히 할 수 없는 분량이다보니",
          "speaker_id": "1",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008652.1.35",
          "form": "아예 스튜디오에 맡기는 회사들이 많아졌거든요",
          "original_form": "아예 스튜디오에 맡기는 회사들이 많아졌거든요",
          "speaker_id": "1",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008652.1.36",
          "form": "웹툰 분업화해서 만들어지는 회사라고 생각하시면 될 것 같아요",
          "original_form": "웹툰 분업화해서 만들어지는 회사라고 생각하시면 될 것 같아요",
          "speaker_id": "1",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008652.1.37",
          "form": "레드아이스나 앤트 스튜디오같은 곳이 유명한 작품들이 많은 것 같아요",
          "original_form": "레드아이스나 앤트 스튜디오같은 곳이 유명한 작품들이 많은 것 같아요",
          "speaker_id": "1",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008652.1.38",
          "form": "아...! 꽤 들어본 것 같네요",
          "original_form": "아...! 꽤 들어본 것 같네요",
          "speaker_id": "2",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008652.1.39",
          "form": "진짜 회사랑 경쟁하려면 쉽지않겠어요",
          "original_form": "진짜 회사랑 경쟁하려면 쉽지않겠어요",
          "speaker_id": "2",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008652.1.40",
          "form": "맞아요 개인작가님들은 진짜",
          "original_form": "맞아요 개인작가님들은 진짜",
          "speaker_id": "1",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008652.1.41",
          "form": "바늘구멍 들어가기같아요",
          "original_form": "바늘구멍 들어가기같아요",
          "speaker_id": "1",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008652.1.42",
          "form": "근데 네이버는 그림에 엄청난거보다",
          "original_form": "근데 네이버는 그림에 엄청난거보다",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008652.1.43",
          "form": "스토리를 더 많이본다더라고요",
          "original_form": "스토리를 더 많이본다더라고요",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008652.1.44",
          "form": "화이팅하셔서 돈 많이버셨으면 좋겠어요",
          "original_form": "화이팅하셔서 돈 많이버셨으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008652.1.45",
          "form": "저도 그림보다는 스토리로 승부보고 싶어요!",
          "original_form": "저도 그림보다는 스토리로 승부보고 싶어요!",
          "speaker_id": "2",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008652.1.46",
          "form": "감사해요 ㅠㅠ",
          "original_form": "감사해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 17:37"
        }
      ]
