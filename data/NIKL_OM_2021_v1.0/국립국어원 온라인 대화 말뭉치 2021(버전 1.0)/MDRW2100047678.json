      "utterance": [
        {
          "id": "MDRW2100047678.1.1",
          "form": "저는 이 경우 후자요!ㅎㅎㅎ",
          "original_form": "저는 이 경우 후자요!ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:52"
        },
        {
          "id": "MDRW2100047678.1.2",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:53"
        },
        {
          "id": "MDRW2100047678.1.3",
          "form": "저는 사실 이쁜음식도 좋아한답니다",
          "original_form": "저는 사실 이쁜음식도 좋아한답니다",
          "speaker_id": "1",
          "time": "20210827 00:53"
        },
        {
          "id": "MDRW2100047678.1.4",
          "form": "예쁜 음식이 맛이 더 있게 느껴지죠 ㅎㅎ",
          "original_form": "예쁜 음식이 맛이 더 있게 느껴지죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100047678.1.5",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210827 01:07"
        },
        {
          "id": "MDRW2100047678.1.6",
          "form": "경치도좋으면",
          "original_form": "경치도좋으면",
          "speaker_id": "1",
          "time": "20210827 01:07"
        },
        {
          "id": "MDRW2100047678.1.7",
          "form": "더할나위없죠",
          "original_form": "더할나위없죠",
          "speaker_id": "1",
          "time": "20210827 01:07"
        },
        {
          "id": "MDRW2100047678.1.8",
          "form": "카페 가시면 사진 많이 찍으시나요?",
          "original_form": "카페 가시면 사진 많이 찍으시나요?",
          "speaker_id": "2",
          "time": "20210827 01:07"
        },
        {
          "id": "MDRW2100047678.1.9",
          "form": "ㅋㅋㅋㅋ 완전많이는아니고",
          "original_form": "ㅋㅋㅋㅋ 완전많이는아니고",
          "speaker_id": "1",
          "time": "20210827 01:12"
        },
        {
          "id": "MDRW2100047678.1.10",
          "form": "카페인테리어샷",
          "original_form": "카페인테리어샷",
          "speaker_id": "1",
          "time": "20210827 01:12"
        },
        {
          "id": "MDRW2100047678.1.11",
          "form": "음식샷",
          "original_form": "음식샷",
          "speaker_id": "1",
          "time": "20210827 01:12"
        },
        {
          "id": "MDRW2100047678.1.12",
          "form": "이렇게요ㅋㅋㅋ",
          "original_form": "이렇게요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:12"
        },
        {
          "id": "MDRW2100047678.1.13",
          "form": "ㅎㅎㅎ저도 음식샷은 꼭 찍어요",
          "original_form": "ㅎㅎㅎ저도 음식샷은 꼭 찍어요",
          "speaker_id": "2",
          "time": "20210827 01:12"
        },
        {
          "id": "MDRW2100047678.1.14",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:14"
        },
        {
          "id": "MDRW2100047678.1.15",
          "form": "전배달음식먹어도",
          "original_form": "전배달음식먹어도",
          "speaker_id": "1",
          "time": "20210827 01:15"
        },
        {
          "id": "MDRW2100047678.1.16",
          "form": "찍어요",
          "original_form": "찍어요",
          "speaker_id": "1",
          "time": "20210827 01:15"
        },
        {
          "id": "MDRW2100047678.1.17",
          "form": "ㅋㅋㅋㅋ나중에 보면 기억나고 재밌더라구요. 리뷰쓸때도 쓰고",
          "original_form": "ㅋㅋㅋㅋ나중에 보면 기억나고 재밌더라구요. 리뷰쓸때도 쓰고",
          "speaker_id": "2",
          "time": "20210827 01:17"
        },
        {
          "id": "MDRW2100047678.1.18",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100047678.1.19",
          "form": "앨범이거의다",
          "original_form": "앨범이거의다",
          "speaker_id": "1",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100047678.1.20",
          "form": "음식사진이예요",
          "original_form": "음식사진이예요",
          "speaker_id": "1",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100047678.1.21",
          "form": "ㅋㅋㅋㅋ저도 그런 거 같아요  역시 먹기위해 사는것",
          "original_form": "ㅋㅋㅋㅋ저도 그런 거 같아요  역시 먹기위해 사는것",
          "speaker_id": "2",
          "time": "20210827 01:23"
        },
        {
          "id": "MDRW2100047678.1.22",
          "form": "ㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋ저도요",
          "speaker_id": "1",
          "time": "20210827 01:30"
        },
        {
          "id": "MDRW2100047678.1.23",
