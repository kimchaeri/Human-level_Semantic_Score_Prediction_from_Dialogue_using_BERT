      "utterance": [
        {
          "id": "MDRW2100005110.1.1",
          "form": "요즘 사고싶은거 있나?",
          "original_form": "요즘 사고싶은거 있나?",
          "speaker_id": "1",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005110.1.2",
          "form": "사고싶은거 많이 있습니당",
          "original_form": "사고싶은거 많이 있습니당",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005110.1.3",
          "form": "뭐 말해봐",
          "original_form": "뭐 말해봐",
          "speaker_id": "1",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005110.1.4",
          "form": "나는 자동차나 좀 바꾸고싶은거 말고는 딱히",
          "original_form": "나는 자동차나 좀 바꾸고싶은거 말고는 딱히",
          "speaker_id": "1",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005110.1.5",
          "form": "옷, 신발, 화장품 등이요",
          "original_form": "옷, 신발, 화장품 등이요",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005110.1.6",
          "form": "과소비의 여왕이군",
          "original_form": "과소비의 여왕이군",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005110.1.7",
          "form": "자기는 피부 좋으니까 화장품 노필요",
          "original_form": "자기는 피부 좋으니까 화장품 노필요",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005110.1.8",
          "form": "옷과 신발은 배당나오면...",
          "original_form": "옷과 신발은 배당나오면...",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005110.1.9",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005110.1.10",
          "form": "애들 옷도 사야하고",
          "original_form": "애들 옷도 사야하고",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005110.1.11",
          "form": "소비는 미덕이아닌디",
          "original_form": "소비는 미덕이아닌디",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005110.1.12",
          "form": "허허",
          "original_form": "허허",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005110.1.13",
          "form": "포켓몬카드는 중고거래로 구입!",
          "original_form": "포켓몬카드는 중고거래로 구입!",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005110.1.14",
          "form": "그거 만이천원에 산댓는데답없음",
          "original_form": "그거 만이천원에 산댓는데답없음",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005110.1.15",
          "form": "그거로는 안팔려나 보네요",
          "original_form": "그거로는 안팔려나 보네요",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005110.1.16",
          "form": "만원도아까운디",
          "original_form": "만원도아까운디",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005110.1.17",
          "form": "아깝긴하죠ㅠ 비싸요",
          "original_form": "아깝긴하죠ㅠ 비싸요",
          "speaker_id": "2",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005110.1.18",
          "form": "그니까 그거 다 한 때 유행인디",
          "original_form": "그니까 그거 다 한 때 유행인디",
