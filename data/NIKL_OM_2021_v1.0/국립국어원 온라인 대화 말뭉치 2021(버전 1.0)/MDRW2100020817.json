      "utterance": [
        {
          "id": "MDRW2100020817.1.1",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020817.1.2",
          "form": "아직요. 다음달에 예약했어요",
          "original_form": "아직요. 다음달에 예약했어요",
          "speaker_id": "2",
          "time": "20210828 13:04"
        },
        {
          "id": "MDRW2100020817.1.3",
          "form": "저도 다음달에요",
          "original_form": "저도 다음달에요",
          "speaker_id": "1",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020817.1.4",
          "form": "다들 모더나 맞고싶다고 하더라구요",
          "original_form": "다들 모더나 맞고싶다고 하더라구요",
          "speaker_id": "1",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020817.1.5",
          "form": "코로나 질병이 쉽게 종식이 안되네요",
          "original_form": "코로나 질병이 쉽게 종식이 안되네요",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020817.1.6",
          "form": "근데 지금보니까 잔여백신으로 안올리고",
          "original_form": "근데 지금보니까 잔여백신으로 안올리고",
          "speaker_id": "1",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020817.1.7",
          "form": "지인들한테 모더나 맞춰주고 그러더라구요",
          "original_form": "지인들한테 모더나 맞춰주고 그러더라구요",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020817.1.8",
          "form": "회사언니도 아는 간호사가 구해줘서 신랑이랑 맞았다고 하고",
          "original_form": "회사언니도 아는 간호사가 구해줘서 신랑이랑 맞았다고 하고",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020817.1.9",
          "form": "병원관계자 알고 있으면 편리하겠네요",
          "original_form": "병원관계자 알고 있으면 편리하겠네요",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020817.1.10",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020817.1.11",
          "form": "저도 아는 지인이 치과의사뿐이라 ㅋ",
          "original_form": "저도 아는 지인이 치과의사뿐이라 ㅋ",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020817.1.12",
          "form": "좀걱정되요",
          "original_form": "좀걱정되요",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020817.1.13",
          "form": "백신 부작용 기사보면 걱정되요",
          "original_form": "백신 부작용 기사보면 걱정되요",
          "speaker_id": "2",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100020817.1.14",
          "form": "네 머리카락 다 빠진거 보셨어요?",
          "original_form": "네 머리카락 다 빠진거 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100020817.1.15",
          "form": "일본 사람이었던것 같은데 맞나요?",
          "original_form": "일본 사람이었던것 같은데 맞나요?",
          "speaker_id": "2",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020817.1.16",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020817.1.17",
          "form": "화이자맞고 뇌출헐인가로 죽은 아기엄마 35살",
          "original_form": "화이자맞고 뇌출헐인가로 죽은 아기엄마 35살",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020817.1.18",
          "form": "국민청원도 있고",
          "original_form": "국민청원도 있고",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020817.1.19",
          "form": "맞아요. 건강하고. 젎은 사람들이 쓰러진 기사 보면 무서워요",
          "original_form": "맞아요. 건강하고. 젎은 사람들이 쓰러진 기사 보면 무서워요",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020817.1.20",
          "form": "맘편히 먹고 맞아야쥬",
          "original_form": "맘편히 먹고 맞아야쥬",
          "speaker_id": "1",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020817.1.21",
          "form": "소수겠지",
          "original_form": "소수겠지",
          "speaker_id": "1",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020817.1.22",
          "form": "하면서 ㅎㅎ",
          "original_form": "하면서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020817.1.23",
          "form": "다른 질병에 비해 사망률도 낮다고 하는데..누구 말이 맞는건지",
          "original_form": "다른 질병에 비해 사망률도 낮다고 하는데..누구 말이 맞는건지",
          "speaker_id": "2",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020817.1.24",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020817.1.25",
          "form": "백신을 맞아야 덜아프다고 해서 맞는건데 ㅎ",
          "original_form": "백신을 맞아야 덜아프다고 해서 맞는건데 ㅎ",
          "speaker_id": "1",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020817.1.26",
          "form": "전염성이 높아서 확진자가 많을 뿐이지 사망자가 덜 많다고 하더라구요",
          "original_form": "전염성이 높아서 확진자가 많을 뿐이지 사망자가 덜 많다고 하더라구요",
          "speaker_id": "2",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100020817.1.27",
          "form": "코로나 쉽게 끝나지않겠죠? ㅜㅠ",
          "original_form": "코로나 쉽게 끝나지않겠죠? ㅜㅠ",
          "speaker_id": "1",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020817.1.28",
          "form": "변이 바이러스",
          "original_form": "변이 바이러스",
          "speaker_id": "2",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100020817.1.29",
          "form": "건강 지켜야 할것 같아요",
          "original_form": "건강 지켜야 할것 같아요",
          "speaker_id": "2",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100020817.1.30",
          "form": "자꾸자꾸 변이가 생기는거가타요",
          "original_form": "자꾸자꾸 변이가 생기는거가타요",
          "speaker_id": "1",
          "time": "20210828 13:30"
        }
      ]
    }
