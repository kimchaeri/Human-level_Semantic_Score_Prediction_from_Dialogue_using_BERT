      "utterance": [
        {
          "id": "MDRW2100013634.1.1",
          "form": "종교가 어떻게 되세요?",
          "original_form": "종교가 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100013634.1.2",
          "form": "전 무교입니다 ㅎ",
          "original_form": "전 무교입니다 ㅎ",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100013634.1.3",
          "form": "어떻게 되세여",
          "original_form": "어떻게 되세여",
          "speaker_id": "2",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100013634.1.4",
          "form": "전 불교요ㅎ",
          "original_form": "전 불교요ㅎ",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100013634.1.5",
          "form": "오 절 다니세요?",
          "original_form": "오 절 다니세요?",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100013634.1.6",
          "form": "네ㅋㅋ자주 가는 건 아니구 석가탄신일에 가거나 가끔 시간날 때 가요",
          "original_form": "네ㅋㅋ자주 가는 건 아니구 석가탄신일에 가거나 가끔 시간날 때 가요",
          "speaker_id": "1",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100013634.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100013634.1.8",
          "form": "전 예전에 산책할겸 간적 있어요ㅋㅋ",
          "original_form": "전 예전에 산책할겸 간적 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100013634.1.9",
          "form": "조용하니 좋죠?ㅋㅋㅋ",
          "original_form": "조용하니 좋죠?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100013634.1.10",
          "form": "전 불교 천주교 기독교 다 접해봤는데 \r\n혹시 학교에서 채플같은거 안해보셨나요",
          "original_form": "전 불교 천주교 기독교 다 접해봤는데 \r\n혹시 학교에서 채플같은거 안해보셨나요",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100013634.1.11",
          "form": "네 ㅋㅋ 좋았어요",
          "original_form": "네 ㅋㅋ 좋았어요",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100013634.1.12",
          "form": "저 채플이요 ㅠㅠㅠㅠㅠ",
          "original_form": "저 채플이요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100013634.1.13",
          "form": "저는 집안은 불교집안인데 학교들이 천주교 기독교였어서 꽤 공부가 되었다고해야할까요...ㅜ",
          "original_form": "저는 집안은 불교집안인데 학교들이 천주교 기독교였어서 꽤 공부가 되었다고해야할까요...ㅜ",
          "speaker_id": "1",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100013634.1.14",
          "form": "중고등학생 때는 학교에 수녀님이 계셨었고 미사도 주마다 했었어요ㅋㅋ",
          "original_form": "중고등학생 때는 학교에 수녀님이 계셨었고 미사도 주마다 했었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100013634.1.15",
          "form": "대학교때 기독교학교라 채플이랑 교양수업으로 기독교관련 수업들어야하고요ㅜ",
          "original_form": "대학교때 기독교학교라 채플이랑 교양수업으로 기독교관련 수업들어야하고요ㅜ",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100013634.1.16",
          "form": "전 힘들었어요.....",
          "original_form": "전 힘들었어요.....",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100013634.1.17",
          "form": "학점을 채워야 하는거라 더 오바였죠",
          "original_form": "학점을 채워야 하는거라 더 오바였죠",
          "speaker_id": "2",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100013634.1.18",
          "form": "ㅋㅋㅋㅋㅋ맞아요...",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요...",
          "speaker_id": "1",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100013634.1.19",
          "form": "아니 시험이...",
          "original_form": "아니 시험이...",
          "speaker_id": "1",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100013634.1.20",
          "form": "저흰 다행히",
          "original_form": "저흰 다행히",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100013634.1.21",
          "form": "시험은 없었는데",
          "original_form": "시험은 없었는데",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100013634.1.22",
          "form": "과제 폭탄쓰",
          "original_form": "과제 폭탄쓰",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100013634.1.23",
          "form": "저희는 과제도 있었고ㅠ 발표도 하고  b4용지에 시험쳤어요ㅋㅋㅋㅋㅋ",
          "original_form": "저희는 과제도 있었고ㅠ 발표도 하고  b4용지에 시험쳤어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100013634.1.24",
          "form": "질문은 한줄인데...",
          "original_form": "질문은 한줄인데...",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100013634.1.25",
          "form": "빈공간이 a4도 아니고 b4....",
          "original_form": "빈공간이 a4도 아니고 b4....",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100013634.1.26",
          "form": "진짜 너무해요",
          "original_form": "진짜 너무해요",
          "speaker_id": "2",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100013634.1.27",
          "form": "ㅋㅋㅋㅋㅋ헐 비포 양심 뭐에요 ㅠ",
