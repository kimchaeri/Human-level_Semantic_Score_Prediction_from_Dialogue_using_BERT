      "utterance": [
        {
          "id": "MDRW2100011792.1.1",
          "form": "이번에 도쿄 올림픽 폐막식 봤어?",
          "original_form": "이번에 도쿄 올림픽 폐막식 봤어?",
          "speaker_id": "1",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011792.1.2",
          "form": "어떤 언니가 춤추는거",
          "original_form": "어떤 언니가 춤추는거",
          "speaker_id": "1",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011792.1.3",
          "form": "이상한 표정으로...",
          "original_form": "이상한 표정으로...",
          "speaker_id": "1",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011792.1.4",
          "form": "아아아아",
          "original_form": "아아아아",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011792.1.5",
          "form": "그거 니가 영상으로보여주지않았나?",
          "original_form": "그거 니가 영상으로보여주지않았나?",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011792.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:07"
        },
        {
          "id": "MDRW2100011792.1.7",
          "form": "아 그랬나?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그랬나?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011792.1.8",
          "form": "아 아니다 다른애가보여준거같은데",
          "original_form": "아 아니다 다른애가보여준거같은데",
          "speaker_id": "2",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011792.1.9",
          "form": "쨋든봤음",
          "original_form": "쨋든봤음",
          "speaker_id": "2",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011792.1.10",
          "form": "혼자나와서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "혼자나와서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011792.1.11",
          "form": "근데 KBS에서 한 멘트 감동적이었어",
          "original_form": "근데 KBS에서 한 멘트 감동적이었어",
          "speaker_id": "1",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011792.1.12",
          "form": "맞아 초록 옷 입고 춤추는거...",
          "original_form": "맞아 초록 옷 입고 춤추는거...",
          "speaker_id": "1",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011792.1.13",
          "form": "무슨멘트??",
          "original_form": "무슨멘트??",
          "speaker_id": "2",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011792.1.14",
          "form": "근데 그 분 원래 엄청 유명하다며",
          "original_form": "근데 그 분 원래 엄청 유명하다며",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011792.1.15",
          "form": "아진짜??",
          "original_form": "아진짜??",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011792.1.16",
          "form": "잘 기억은 안나는데",
          "original_form": "잘 기억은 안나는데",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011792.1.17",
          "form": "하긴 유명하니까 올림픽에 혼자 나왔을거같긴하다",
          "original_form": "하긴 유명하니까 올림픽에 혼자 나왔을거같긴하다",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011792.1.18",
          "form": "나중에ㅜ한번 찾아 봐봐",
          "original_form": "나중에ㅜ한번 찾아 봐봐",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011792.1.19",
          "form": "막 명품 브랜드 모델하고",
          "original_form": "막 명품 브랜드 모델하고",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011792.1.20",
          "form": "그런 분이래",
          "original_form": "그런 분이래",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011792.1.21",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011792.1.22",
          "form": "몰랏음",
          "original_form": "몰랏음",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011792.1.23",
