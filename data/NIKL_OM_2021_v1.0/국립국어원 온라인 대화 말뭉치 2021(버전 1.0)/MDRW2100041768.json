      "utterance": [
        {
          "id": "MDRW2100041768.1.1",
          "form": "ㅇㅈㅇㅈ 걔가 손나은 업신여기는 장면도 있던데",
          "original_form": "ㅇㅈㅇㅈ 걔가 손나은 업신여기는 장면도 있던데",
          "speaker_id": "2",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100041768.1.2",
          "form": "꼴사나워서 더 못보겠어서 탈주함",
          "original_form": "꼴사나워서 더 못보겠어서 탈주함",
          "speaker_id": "2",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100041768.1.3",
          "form": "으",
          "original_form": "으",
          "speaker_id": "1",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100041768.1.4",
          "form": "지주제를알지",
          "original_form": "지주제를알지",
          "speaker_id": "1",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100041768.1.5",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100041768.1.6",
          "form": "대본이그렇게나와도",
          "original_form": "대본이그렇게나와도",
          "speaker_id": "1",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100041768.1.7",
          "form": "찔랴서못하겠다",
          "original_form": "찔랴서못하겠다",
          "speaker_id": "1",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100041768.1.8",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.9",
          "form": "양심이 잇어야",
          "original_form": "양심이 잇어야",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.10",
          "form": "지",
          "original_form": "지",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.11",
          "form": "그래서 또 인간실격이구나",
          "original_form": "그래서 또 인간실격이구나",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.12",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.14",
          "form": "그런듯",
          "original_form": "그런듯",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.15",
          "form": "아니그것도싫어",
          "original_form": "아니그것도싫어",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.16",
          "form": "남주는",
          "original_form": "남주는",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.17",
          "form": "류로",
          "original_form": "류로",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.18",
          "form": "그렇게못생겼는데",
          "original_form": "그렇게못생겼는데",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.19",
          "form": "여주들은",
          "original_form": "여주들은",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.20",
          "form": "손나은에전도연에",
          "original_form": "손나은에전도연에",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.21",
          "form": "아끔찍해",
          "original_form": "아끔찍해",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.22",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041768.1.23",
          "form": "황정음도 걔랑 했었잖아",
          "original_form": "황정음도 걔랑 했었잖아",
          "speaker_id": "2",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041768.1.24",
          "form": "걔한테 매달려야 하는 걸로",
          "original_form": "걔한테 매달려야 하는 걸로",
          "speaker_id": "2",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041768.1.25",
          "form": "그게진짜양심없었음",
          "original_form": "그게진짜양심없었음",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041768.1.26",
          "form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041768.1.27",
          "form": "진심 에바임",
          "original_form": "진심 에바임",
          "speaker_id": "2",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041768.1.28",
          "form": "작가는",
          "original_form": "작가는",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041768.1.29",
          "form": "아무생각이안들었을까",
          "original_form": "아무생각이안들었을까",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041768.1.30",
          "form": "본인이생각한그림이아니었을텐데",
          "original_form": "본인이생각한그림이아니었을텐데",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041768.1.31",
          "form": "자괴감 들었을 듯",
          "original_form": "자괴감 들었을 듯",
          "speaker_id": "2",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041768.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그니까",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041768.1.33",
          "form": "본인이얼추생각한",
          "original_form": "본인이얼추생각한",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041768.1.34",
          "form": "이미지가있었을텐데",
          "original_form": "이미지가있었을텐데",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041768.1.35",
          "form": "못해도 그정도는 아니었을 텐데",
          "original_form": "못해도 그정도는 아니었을 텐데",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041768.1.36",
          "form": "응팔때는",
          "original_form": "응팔때는",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041768.1.37",
          "form": "도대체왜",
          "original_form": "도대체왜",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100041768.1.38",
          "form": "걔가인기가있었을까?",
          "original_form": "걔가인기가있었을까?",
          "speaker_id": "1",
          "time": "20210906 15:39"
