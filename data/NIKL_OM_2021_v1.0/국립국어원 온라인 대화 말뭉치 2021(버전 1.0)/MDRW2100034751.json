        {
          "id": "MDRW2100034751.1.1",
          "form": "여기는 비가 엄청 와요",
          "original_form": "여기는 비가 엄청 와요",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100034751.1.2",
          "form": "name2님 계신곳도 비가 많이 오나요?",
          "original_form": "&name2&님 계신곳도 비가 많이 오나요?",
          "speaker_id": "1",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100034751.1.3",
          "form": "네 엄청 쏟아지넹ᆢㄷ",
          "original_form": "네 엄청 쏟아지넹ᆢㄷ",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100034751.1.4",
          "form": "호우주의보 문자도 왔어요",
          "original_form": "호우주의보 문자도 왔어요",
          "speaker_id": "2",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100034751.1.5",
          "form": "오 기상재난 문자까지 왔군요",
          "original_form": "오 기상재난 문자까지 왔군요",
          "speaker_id": "1",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100034751.1.6",
          "form": "엄청 쏟아져요",
          "original_form": "엄청 쏟아져요",
          "speaker_id": "1",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100034751.1.7",
          "form": "저도 방금 호우주의보 문자 받았어요",
          "original_form": "저도 방금 호우주의보 문자 받았어요",
          "speaker_id": "1",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100034751.1.8",
          "form": "주말에 잠깐 맑더니 또 비가ㅠ",
          "original_form": "주말에 잠깐 맑더니 또 비가ㅠ",
          "speaker_id": "2",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100034751.1.9",
          "form": "그러게나 말예요",
          "original_form": "그러게나 말예요",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100034751.1.10",
          "form": "장마 때보다 요즘 더 오는 것 같아요",
          "original_form": "장마 때보다 요즘 더 오는 것 같아요",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100034751.1.11",
          "form": "가을장마라는 표현이 딱이에요",
          "original_form": "가을장마라는 표현이 딱이에요",
          "speaker_id": "2",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100034751.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100034751.1.13",
          "form": "빨래하기 안 좋은 날들이네요ㅠ",
          "original_form": "빨래하기 안 좋은 날들이네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100034751.1.14",
          "form": "네네 습도가 높은 날들...",
          "original_form": "네네 습도가 높은 날들...",
          "speaker_id": "1",
          "time": "20210831 20:37"
        },
        {
          "id": "MDRW2100034751.1.15",
          "form": "내일 점심 무렵까지는 계속 비 올 것 같아요",
          "original_form": "내일 점심 무렵까지는 계속 비 올 것 같아요",
          "speaker_id": "2",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100034751.1.16",
          "form": "네 가을장마라네요",
          "original_form": "네 가을장마라네요",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100034751.1.17",
          "form": "어서 가을이 왔음 좋겠어요",
          "original_form": "어서 가을이 왔음 좋겠어요",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100034751.1.18",
          "form": "곧 가을이 올 것 같아요",
