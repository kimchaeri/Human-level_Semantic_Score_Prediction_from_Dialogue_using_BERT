      "utterance": [
        {
          "id": "MDRW2100013170.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013170.1.2",
          "form": "안녕하세요! name1님은 점심 식사 하셨나요?",
          "original_form": "안녕하세요! &name1&님은 점심 식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100013170.1.3",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013170.1.4",
          "form": "떡볶이 좋아하세요?",
          "original_form": "떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013170.1.5",
          "form": "네 떡볶이 없어서 못먹죠! 오늘 떡볶이 드셨나요?",
          "original_form": "네 떡볶이 없어서 못먹죠! 오늘 떡볶이 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013170.1.6",
          "form": "전 치킨먹었어요ㅋㅋㅋ",
          "original_form": "전 치킨먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013170.1.7",
          "form": "어떤브랜드 좋아하세요? 떡볶이!",
          "original_form": "어떤브랜드 좋아하세요? 떡볶이!",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013170.1.8",
          "form": "우와 치킨이 더 부럽네요 ㅋㅋㅋ\r\n전 신전떡볶이 좋아해요!",
          "original_form": "우와 치킨이 더 부럽네요 ㅋㅋㅋ\r\n전 신전떡볶이 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013170.1.9",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013170.1.10",
          "form": "전 요즘",
          "original_form": "전 요즘",
          "speaker_id": "1",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100013170.1.11",
          "form": "로제 너무좋아해요ㅋㅋㅋ",
          "original_form": "로제 너무좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100013170.1.12",
          "form": "전 아직 로제 안먹어봤어요ㅠㅠ 어느 브랜드 로제 즐겨 드시나요??",
          "original_form": "전 아직 로제 안먹어봤어요ㅠㅠ 어느 브랜드 로제 즐겨 드시나요??",
          "speaker_id": "2",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013170.1.13",
          "form": "떡군이네요ㅋㅋㅋㅋ",
          "original_form": "떡군이네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013170.1.14",
          "form": "스파게티면 추가필수요",
          "original_form": "스파게티면 추가필수요",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013170.1.15",
          "form": "기억해둘게요! 꿀팁 감사합니다ㅋㅋ",
          "original_form": "기억해둘게요! 꿀팁 감사합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013170.1.16",
          "form": "로제 파스타 맛이랑 비슷한가요?",
          "original_form": "로제 파스타 맛이랑 비슷한가요?",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013170.1.17",
          "form": "은근다르더라고요ㅋㅋㅋ",
          "original_form": "은근다르더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013170.1.18",
          "form": "튀김은 대게다리나 집게살 핫도그로하세요!",
          "original_form": "튀김은 대게다리나 집게살 핫도그로하세요!",
          "speaker_id": "1",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013170.1.19",
          "form": "오 정말 많이 드셔보신 전문가 느낌 팍팍나네요ㅋㅋㅋ",
          "original_form": "오 정말 많이 드셔보신 전문가 느낌 팍팍나네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013170.1.20",
          "form": "로제 떡볶이에도 치즈 추가해서 먹나요??",
          "original_form": "로제 떡볶이에도 치즈 추가해서 먹나요??",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013170.1.21",
          "form": "ㅋㅋㅋ 오우노우",
          "original_form": "ㅋㅋㅋ 오우노우",
          "speaker_id": "1",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100013170.1.22",
          "form": "그건안돼요!",
          "original_form": "그건안돼요!",
          "speaker_id": "1",
          "time": "20210825 14:42"
        },
