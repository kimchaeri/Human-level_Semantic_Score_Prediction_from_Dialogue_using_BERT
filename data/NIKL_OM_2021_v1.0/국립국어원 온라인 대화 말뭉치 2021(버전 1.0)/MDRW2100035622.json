      "utterance": [
        {
          "id": "MDRW2100035622.1.1",
          "form": "헉 그러면 독일어 이런 거 하셔야 하는...?",
          "original_form": "헉 그러면 독일어 이런 거 하셔야 하는...?",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100035622.1.2",
          "form": "헉 맞아요",
          "original_form": "헉 맞아요",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035622.1.3",
          "form": "독일어 노답이에요",
          "original_form": "독일어 노답이에요",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035622.1.4",
          "form": "전 독일어 안좋아하거든요 ㅋㅋㅋㅋㅋ 근데 겨울에 a2까진 가얗라거같아요..",
          "original_form": "전 독일어 안좋아하거든요 ㅋㅋㅋㅋㅋ 근데 겨울에 a2까진 가얗라거같아요..",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035622.1.5",
          "form": "으악 독일어 어렵기로 소문난 언어 아닌가요.ㅠㅠ a2면 어느정도인가요?",
          "original_form": "으악 독일어 어렵기로 소문난 언어 아닌가요.ㅠㅠ a2면 어느정도인가요?",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100035622.1.6",
          "form": "너무 어렵읍니다..",
          "original_form": "너무 어렵읍니다..",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035622.1.7",
          "form": "(초급) a1 a2 (중급) b1 b2 (고급) c1 이럴거에요",
          "original_form": "(초급) a1 a2 (중급) b1 b2 (고급) c1 이럴거에요",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035622.1.8",
          "form": "아 기본은 떼어야 하는거군요",
          "original_form": "아 기본은 떼어야 하는거군요",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035622.1.9",
          "form": "졸업할때 c1은 찍어야 할 것 같아요",
          "original_form": "졸업할때 c1은 찍어야 할 것 같아요",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035622.1.10",
          "form": "정관사가 15개... 이런 소문으로만 알고있슺니다 독일어ㅋㅋㅋㅠ",
          "original_form": "정관사가 15개... 이런 소문으로만 알고있슺니다 독일어ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035622.1.11",
          "form": "헥 진짜요?",
          "original_form": "헥 진짜요?",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035622.1.12",
          "form": "맞아요 관사놀이",
          "original_form": "맞아요 관사놀이",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035622.1.13",
          "form": "제정신이 아닌거같아요",
          "original_form": "제정신이 아닌거같아요",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035622.1.14",
          "form": "혹시 독일어과에 들어가신...? ㅠㅠ",
          "original_form": "혹시 독일어과에 들어가신...? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035622.1.15",
          "form": "철학인데 ㅋㅋㅋㅋ 원전읽어야해서요",
          "original_form": "철학인데 ㅋㅋㅋㅋ 원전읽어야해서요",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035622.1.16",
          "form": "박사과정도 생각해야하구",
          "original_form": "박사과정도 생각해야하구",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035622.1.17",
          "form": "전공도 아닌데... 그러니깐요...",
          "original_form": "전공도 아닌데... 그러니깐요...",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035622.1.18",
          "form": "전공공부만도 힘드실텐데 외국어까지...",
          "original_form": "전공공부만도 힘드실텐데 외국어까지...",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035622.1.19",
          "form": "윽ㅋㅋㅋㅋ 맞아요! 전공공부만 해도 벅찬데",
          "original_form": "윽ㅋㅋㅋㅋ 맞아요! 전공공부만 해도 벅찬데",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035622.1.20",
          "form": "글고 영어만 해도 바쁜데요 ㅋㅋㅋㅋ",
          "original_form": "글고 영어만 해도 바쁜데요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035622.1.21",
          "form": "그러니까요 영국인이 쓴 것도 원서로 읽고 독일인이 쓴 것도 원서로읽고",
          "original_form": "그러니까요 영국인이 쓴 것도 원서로 읽고 독일인이 쓴 것도 원서로읽고",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035622.1.22",
          "form": "바쁘다바빠...",
          "original_form": "바쁘다바빠...",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035622.1.23",
          "form": "ㅋㅋㅋㅋㅋ아 번역본도 못읽겟는데 원서라니",
          "original_form": "ㅋㅋㅋㅋㅋ아 번역본도 못읽겟는데 원서라니",
          "speaker_id": "1",
          "time": "20210830 12:26"
        }
      ]
