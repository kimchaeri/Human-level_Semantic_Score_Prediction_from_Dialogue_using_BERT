      "utterance": [
        {
          "id": "MDRW2100035063.1.1",
          "form": "오늘 안 좋은 일 있었니",
          "original_form": "오늘 안 좋은 일 있었니",
          "speaker_id": "2",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100035063.1.2",
          "form": "심각하게 나쁜 일이 있었어",
          "original_form": "심각하게 나쁜 일이 있었어",
          "speaker_id": "1",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100035063.1.3",
          "form": "어떤 거 길래",
          "original_form": "어떤 거 길래",
          "speaker_id": "2",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100035063.1.4",
          "form": "내가 비를 안 좋아하거든",
          "original_form": "내가 비를 안 좋아하거든",
          "speaker_id": "1",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100035063.1.5",
          "form": "알고 있지 그래서 우산을 꼭 가지고 다니잖아",
          "original_form": "알고 있지 그래서 우산을 꼭 가지고 다니잖아",
          "speaker_id": "2",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100035063.1.6",
          "form": "맞아 나는 꼭 우산을 가지고 다녀",
          "original_form": "맞아 나는 꼭 우산을 가지고 다녀",
          "speaker_id": "1",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100035063.1.7",
          "form": "오늘은  비를 맞게 된 이유가 뭐야",
          "original_form": "오늘은  비를 맞게 된 이유가 뭐야",
          "speaker_id": "2",
          "time": "20210829 01:32"
        },
        {
          "id": "MDRW2100035063.1.8",
          "form": "내가 하필 우산을 사물함에 두고 왔거든",
          "original_form": "내가 하필 우산을 사물함에 두고 왔거든",
          "speaker_id": "1",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100035063.1.9",
          "form": "일기예보를 확인했어야지",
          "original_form": "일기예보를 확인했어야지",
          "speaker_id": "2",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100035063.1.10",
          "form": "아침에 바빠서 정신이 없었단 말이야",
          "original_form": "아침에 바빠서 정신이 없었단 말이야",
          "speaker_id": "1",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100035063.1.11",
          "form": "비를 맞았다니 감기 조심해야겠다",
          "original_form": "비를 맞았다니 감기 조심해야겠다",
          "speaker_id": "2",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100035063.1.12",
          "form": "걱정해줘서 고마워",
          "original_form": "걱정해줘서 고마워",
          "speaker_id": "1",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100035063.1.13",
          "form": "날씨가 많이 쌀쌀해졌어",
          "original_form": "날씨가 많이 쌀쌀해졌어",
          "speaker_id": "2",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100035063.1.14",
          "form": "조심할게",
          "original_form": "조심할게",
          "speaker_id": "1",
          "time": "20210829 01:33"
        }
      ]
