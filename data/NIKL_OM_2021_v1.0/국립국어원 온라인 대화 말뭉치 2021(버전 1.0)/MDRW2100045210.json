      "utterance": [
        {
          "id": "MDRW2100045210.1.1",
          "form": "앗 마지막 뭐라고 보내셨나요",
          "original_form": "앗 마지막 뭐라고 보내셨나요",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045210.1.2",
          "form": "마트에서 자주 볼 수 있어요!",
          "original_form": "마트에서 자주 볼 수 있어요!",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045210.1.3",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045210.1.4",
          "form": "빨간 과일인가요?",
          "original_form": "빨간 과일인가요?",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045210.1.5",
          "form": "붉은 계열이긴 해요!",
          "original_form": "붉은 계열이긴 해요!",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045210.1.6",
          "form": "앗 그럼 주황색인가요?",
          "original_form": "앗 그럼 주황색인가요?",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045210.1.7",
          "form": "딱히 한가지는 아니고 알록달록한 편이에요!",
          "original_form": "딱히 한가지는 아니고 알록달록한 편이에요!",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045210.1.8",
          "form": "아아앗 어렵다",
          "original_form": "아아앗 어렵다",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045210.1.9",
          "form": "혹시 털이 있나요?",
          "original_form": "혹시 털이 있나요?",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045210.1.10",
          "form": "털은 없어요!",
          "original_form": "털은 없어요!",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045210.1.11",
          "form": "이런 복숭아가 아닌가봐요",
          "original_form": "이런 복숭아가 아닌가봐요",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045210.1.12",
          "form": "복숭아 아니죠?",
          "original_form": "복숭아 아니죠?",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045210.1.13",
          "form": "네 복숭아는 아니에요!",
          "original_form": "네 복숭아는 아니에요!",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045210.1.14",
          "form": "식감이 말랑한가요?",
          "original_form": "식감이 말랑한가요?",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045210.1.15",
          "form": "딱딱한 종도 있고",
          "original_form": "딱딱한 종도 있고",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045210.1.16",
          "form": "보통 말랑할수록 달아요!",
          "original_form": "보통 말랑할수록 달아요!",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045210.1.17",
          "form": "말랑한 거 딱딱한 거 둘 다 있나보군요!",
          "original_form": "말랑한 거 딱딱한 거 둘 다 있나보군요!",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045210.1.18",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045210.1.19",
          "form": "알록달록한 과일이 뭐가 있지...",
          "original_form": "알록달록한 과일이 뭐가 있지...",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045210.1.20",
          "form": "나무에서 열려요?",
          "original_form": "나무에서 열려요?",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045210.1.21",
          "form": "네 나무에서 열려요!",
          "original_form": "네 나무에서 열려요!",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045210.1.22",
          "form": "말려서 먹기도 하나요?",
          "original_form": "말려서 먹기도 하나요?",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045210.1.23",
          "form": "말려서 먹기도 하는 과일인가요?",
          "original_form": "말려서 먹기도 하는 과일인가요?",
          "speaker_id": "2",
          "time": "20210908 01:25"
        }
      ]
