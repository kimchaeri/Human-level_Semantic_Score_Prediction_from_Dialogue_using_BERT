      "utterance": [
        {
          "id": "MDRW2100046130.1.1",
          "form": "일반 짜장은 미리 짜장을 만들어 놓으니까",
          "original_form": "일반 짜장은 미리 짜장을 만들어 놓으니까",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046130.1.2",
          "form": "양파가 풀이 죽어있다면",
          "original_form": "양파가 풀이 죽어있다면",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046130.1.3",
          "form": "아아 그차이점이군요",
          "original_form": "아아 그차이점이군요",
          "speaker_id": "1",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046130.1.4",
          "form": "간짜장은 바로 볶아주니까",
          "original_form": "간짜장은 바로 볶아주니까",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046130.1.5",
          "form": "아삭하죠 ㅎㅎ",
          "original_form": "아삭하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046130.1.6",
          "form": "어쩐지 간짜장이 더 맛잇드라구요 ㅋㅋ",
          "original_form": "어쩐지 간짜장이 더 맛잇드라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046130.1.7",
          "form": "넵 해물이 먹고 싶다면",
          "original_form": "넵 해물이 먹고 싶다면",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046130.1.8",
          "form": "쟁반짜장이나 삼선짜장 드세요 ㅎㅎ",
          "original_form": "쟁반짜장이나 삼선짜장 드세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046130.1.9",
          "form": "해물 너무 좋아요 특히",
          "original_form": "해물 너무 좋아요 특히",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046130.1.10",
          "form": "새우가 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "새우가 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046130.1.11",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046130.1.12",
          "form": "새우 굉장히 좋아합니다",
          "original_form": "새우 굉장히 좋아합니다",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046130.1.13",
          "form": "도미노 쉬림프 피자 시키면",
          "original_form": "도미노 쉬림프 피자 시키면",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046130.1.14",
          "form": "새우 까는건 귀찮은데",
          "original_form": "새우 까는건 귀찮은데",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046130.1.15",
          "form": "원래는 새우가 다 제거였는데",
          "original_form": "원래는 새우가 다 제거였는데",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046130.1.16",
          "form": "오오 도미노 !!",
          "original_form": "오오 도미노 !!",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046130.1.17",
          "form": "이제는 저희 아들이",
          "original_form": "이제는 저희 아들이",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046130.1.18",
          "form": "저도 쉬림프로 시켜요 ㅋㅋㅋ",
          "original_form": "저도 쉬림프로 시켜요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046130.1.19",
          "form": "새우 맛을 알아서",
          "original_form": "새우 맛을 알아서",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046130.1.20",
          "form": "앜ㅋㅋ 어케 ㅠㅠ",
          "original_form": "앜ㅋㅋ 어케 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046130.1.21",
          "form": "피자에 있는 여덟 개 새우를 다 혼자 먹어요",
          "original_form": "피자에 있는 여덟 개 새우를 다 혼자 먹어요",
