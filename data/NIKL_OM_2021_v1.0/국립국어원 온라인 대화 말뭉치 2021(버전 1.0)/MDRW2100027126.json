      "utterance": [
        {
          "id": "MDRW2100027126.1.1",
          "form": "무슨재미가있나용??",
          "original_form": "무슨재미가있나용??",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100027126.1.2",
          "form": "약간 인생 대신 사는 느낌 같아요 ㅋㅋ",
          "original_form": "약간 인생 대신 사는 느낌 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100027126.1.3",
          "form": "ㅋㅋㅋㅋㅋ재미있긴하겠네요",
          "original_form": "ㅋㅋㅋㅋㅋ재미있긴하겠네요",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027126.1.4",
          "form": "또 다른게임하시나요??",
          "original_form": "또 다른게임하시나요??",
          "speaker_id": "2",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100027126.1.5",
          "form": "요즘",
          "original_form": "요즘",
          "speaker_id": "1",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100027126.1.6",
          "form": "좀비게임 유행하더라구요",
          "original_form": "좀비게임 유행하더라구요",
          "speaker_id": "1",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100027126.1.7",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "1",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100027126.1.8",
          "form": "너무 무섭네요",
          "original_form": "너무 무섭네요",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027126.1.9",
          "form": "그런게임은 무섭더라구요 ㅋㅋ",
          "original_form": "그런게임은 무섭더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027126.1.10",
          "form": "맞아요 생존게임인데",
          "original_form": "맞아요 생존게임인데",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027126.1.11",
          "form": "사람 뜯어먹으러 와요",
          "original_form": "사람 뜯어먹으러 와요",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027126.1.12",
          "form": "갑툭튀가 진짜",
          "original_form": "갑툭튀가 진짜",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027126.1.13",
          "form": "너무 싫어요 ㅠㅠ",
          "original_form": "너무 싫어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027126.1.14",
          "form": "근데 요즘 그게임이 유행이더라구요",
          "original_form": "근데 요즘 그게임이 유행이더라구요",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027126.1.15",
          "form": "저는 귀신만 아니면 괜찮은거 같아요",
          "original_form": "저는 귀신만 아니면 괜찮은거 같아요",
          "speaker_id": "1",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100027126.1.16",
          "form": "저는갑툭튀 ㅠㅠ",
          "original_form": "저는갑툭튀 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027126.1.17",
          "form": "맞아요 그런거 절대 못함",
          "original_form": "맞아요 그런거 절대 못함",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027126.1.18",
          "form": "배그도 무서운 수준...",
          "original_form": "배그도 무서운 수준...",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100027126.1.19",
          "form": "헐 전 그래도 배그는 마음먹고하는편이예요",
          "original_form": "헐 전 그래도 배그는 마음먹고하는편이예요",
          "speaker_id": "2",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027126.1.20",
          "form": "발자국소리가 들린다는데",
          "original_form": "발자국소리가 들린다는데",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027126.1.21",
          "form": "진짠가요 ㅋㅋ",
          "original_form": "진짠가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027126.1.22",
          "form": "맞아요 진짜 저벅저벅개무서워요 ㅠ",
          "original_form": "맞아요 진짜 저벅저벅개무서워요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100027126.1.23",
          "form": "저는 안해봐서",
          "original_form": "저는 안해봐서",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027126.1.24",
          "form": "뒤에서오는 소리 들어보고 싶네요 ㅋㅋ",
          "original_form": "뒤에서오는 소리 들어보고 싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:35"
        }
      ]
