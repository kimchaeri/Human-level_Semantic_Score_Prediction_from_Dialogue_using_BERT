      "utterance": [
        {
          "id": "MDRW2100027997.1.1",
          "form": "집에만 있으니까 병날거같아요 ㅠ",
          "original_form": "집에만 있으니까 병날거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027997.1.2",
          "form": "진짜요 코로나가 너무싫어요",
          "original_form": "진짜요 코로나가 너무싫어요",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100027997.1.3",
          "form": "백신 맞으세요??",
          "original_form": "백신 맞으세요??",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100027997.1.4",
          "form": "맞았어요",
          "original_form": "맞았어요",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100027997.1.5",
          "form": "오 열나거나 그러지 않으셨어요??",
          "original_form": "오 열나거나 그러지 않으셨어요??",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100027997.1.6",
          "form": "1차라서 갠춘했어용",
          "original_form": "1차라서 갠춘했어용",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100027997.1.7",
          "form": "1차는 대부분 별탈 없이 지나가나봐요 ㅠㅠㅠ",
          "original_form": "1차는 대부분 별탈 없이 지나가나봐요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100027997.1.8",
          "form": "저는 코로나 백신 담주에 맞아요 ㅠ",
          "original_form": "저는 코로나 백신 담주에 맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100027997.1.9",
          "form": "헐 홧팅입니댱",
          "original_form": "헐 홧팅입니댱",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100027997.1.10",
          "form": "코로나는 언제 끝날까요 ㅠㅠ",
          "original_form": "코로나는 언제 끝날까요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100027997.1.11",
          "form": "끝나긴할까요 ㅠㅠ",
          "original_form": "끝나긴할까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100027997.1.12",
          "form": "너무 무서워요 ㅠ 제주변엔 걸리신분은 없어서 실감이 안나기도해요",
          "original_form": "너무 무서워요 ㅠ 제주변엔 걸리신분은 없어서 실감이 안나기도해요",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100027997.1.13",
          "form": "맞아요 제즈변도 안걸린거보고 그저그래오...",
          "original_form": "맞아요 제즈변도 안걸린거보고 그저그래오...",
          "speaker_id": "2",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100027997.1.14",
          "form": "조심해야겟죠 ㅠ",
          "original_form": "조심해야겟죠 ㅠ",
          "speaker_id": "1",
