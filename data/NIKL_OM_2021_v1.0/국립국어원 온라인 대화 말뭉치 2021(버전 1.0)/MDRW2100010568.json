      "utterance": [
        {
          "id": "MDRW2100010568.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010568.1.2",
          "form": "좋아하는 게임 있으신가요?",
          "original_form": "좋아하는 게임 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010568.1.3",
          "form": "아뇨 게임은 잘 못해서 게임플레이 영상만 종종 봐요",
          "original_form": "아뇨 게임은 잘 못해서 게임플레이 영상만 종종 봐요",
          "speaker_id": "2",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010568.1.4",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010568.1.5",
          "form": "오! 저두요",
          "original_form": "오! 저두요",
          "speaker_id": "1",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010568.1.6",
          "form": "전 제가 하는 것보다 보는걸 더 좋아해요",
          "original_form": "전 제가 하는 것보다 보는걸 더 좋아해요",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010568.1.7",
          "form": "보는 게임 유튜버 있으세요?",
          "original_form": "보는 게임 유튜버 있으세요?",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010568.1.8",
          "form": "ㅋㅋㅋ저도예요",
          "original_form": "ㅋㅋㅋ저도예요",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010568.1.9",
          "form": "간단한 폰겜 같은건 빼구요",
          "original_form": "간단한 폰겜 같은건 빼구요",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010568.1.10",
          "form": "저는 수탉님 봐요",
          "original_form": "저는 수탉님 봐요",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010568.1.11",
          "form": "name2은 보는 스트리머나 유튜버 있으신가요?",
          "original_form": "&name2&은 보는 스트리머나 유튜버 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010568.1.12",
          "form": "자빱티비요",
          "original_form": "자빱티비요",
          "speaker_id": "2",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010568.1.13",
          "form": "오옹 주로 하시는 게임이나 게임 종류 있나요?",
          "original_form": "오옹 주로 하시는 게임이나 게임 종류 있나요?",
          "speaker_id": "1",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010568.1.14",
          "form": "자빱님이요? 자빱님은 마인크래프트 컨텐츠 주로 하시고 다른 게임은 데바데나 스듀 많이 하세요",
          "original_form": "자빱님이요? 자빱님은 마인크래프트 컨텐츠 주로 하시고 다른 게임은 데바데나 스듀 많이 하세요",
          "speaker_id": "2",
          "time": "20210823 19:47"
        },
        {
          "id": "MDRW2100010568.1.15",
          "form": "아하 왠지 알고리즘으로 떴었던 것같아요",
          "original_form": "아하 왠지 알고리즘으로 떴었던 것같아요",
          "speaker_id": "1",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010568.1.16",
          "form": "수탉님도 데바데 많이 하시는데 왠지 반갑네요ㅋㅋㅋ",
          "original_form": "수탉님도 데바데 많이 하시는데 왠지 반갑네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010568.1.17",
          "form": "전 게임 잘하는 사람 보면 신기해요 어렸을 때부터 게임 잘 못해서 그런지ㅋㅋ 그냥 스트리머 입담이 재밌어서 봐요",
          "original_form": "전 게임 잘하는 사람 보면 신기해요 어렸을 때부터 게임 잘 못해서 그런지ㅋㅋ 그냥 스트리머 입담이 재밌어서 봐요",
          "speaker_id": "2",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010568.1.18",
          "form": "맞아요ㅋㅋㅋㅋ잘하는 게임 플레이도 보고 싶지만 스트리머가 좋아서 보는 것도 있어요",
          "original_form": "맞아요ㅋㅋㅋㅋ잘하는 게임 플레이도 보고 싶지만 스트리머가 좋아서 보는 것도 있어요",
          "speaker_id": "1",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010568.1.19",
          "form": "예랑가랑 아세요? 전 이분들이 하시는 게임영상도 재밌게 봐요",
          "original_form": "예랑가랑 아세요? 전 이분들이 하시는 게임영상도 재밌게 봐요",
          "speaker_id": "2",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100010568.1.20",
          "form": "아아 그 혹시 미술 전공 졸업한ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아 그 혹시 미술 전공 졸업한ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100010568.1.21",
          "form": "고양이 병철이랑 누구였지 한마리 더 키우는!",
          "original_form": "고양이 병철이랑 누구였지 한마리 더 키우는!",
          "speaker_id": "1",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100010568.1.22",
          "form": "맞죠맞죠?",
          "original_form": "맞죠맞죠?",
          "speaker_id": "1",
