      "utterance": [
        {
          "id": "MDRW2100023640.1.1",
          "form": "반려동물 키우고 계세요?",
          "original_form": "반려동물 키우고 계세요?",
          "speaker_id": "2",
          "time": "20210830 14:00"
        },
        {
          "id": "MDRW2100023640.1.2",
          "form": "아뇨 전 못 키워봤어요ㅜ",
          "original_form": "아뇨 전 못 키워봤어요ㅜ",
          "speaker_id": "1",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100023640.1.3",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100023640.1.4",
          "form": "저는 강아지랑\r\n햄스터 키워봤어요:)",
          "original_form": "저는 강아지랑\r\n햄스터 키워봤어요:)",
          "speaker_id": "2",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023640.1.5",
          "form": "오 강아지 어떤 종이요?",
          "original_form": "오 강아지 어떤 종이요?",
          "speaker_id": "1",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100023640.1.6",
          "form": "전 햄스터 키워보고싶었는데 아직도 못키웠어요ㅋㅋㅠ",
          "original_form": "전 햄스터 키워보고싶었는데 아직도 못키웠어요ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100023640.1.7",
          "form": "믹스견이랑 미니핀이요:)",
          "original_form": "믹스견이랑 미니핀이요:)",
          "speaker_id": "2",
          "time": "20210901 16:22"
        },
        {
          "id": "MDRW2100023640.1.8",
          "form": "햄스터는\r\n너무 충격적이었어서\r\n다신 키우고싶지 않아요ㅜㅜ",
          "original_form": "햄스터는\r\n너무 충격적이었어서\r\n다신 키우고싶지 않아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100023640.1.9",
          "form": "헐 왜요??ㅠㅠ",
          "original_form": "헐 왜요??ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 16:45"
        },
        {
          "id": "MDRW2100023640.1.10",
          "form": "뭔일이 있었나요?",
          "original_form": "뭔일이 있었나요?",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100023640.1.11",
          "form": "하루는 아침에 일어나니\r\n햄스터 한 마리 한쪽 귀가\r\n사라져 있었어요...",
          "original_form": "하루는 아침에 일어나니\r\n햄스터 한 마리 한쪽 귀가\r\n사라져 있었어요...",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100023640.1.12",
          "form": "헐...혹시 다른 애가 뜯어먹은거에요??",
          "original_form": "헐...혹시 다른 애가 뜯어먹은거에요??",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100023640.1.13",
          "form": "충격....",
          "original_form": "충격....",
          "speaker_id": "1",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100023640.1.14",
          "form": "그때는 긴가민가 했었는데요...",
          "original_form": "그때는 긴가민가 했었는데요...",
          "speaker_id": "2",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100023640.1.15",
          "form": "몇일뒤 아침에는\r\n그 햄스터 목덜미가\r\n사라져 있었어요ㅠㅠ",
          "original_form": "몇일뒤 아침에는\r\n그 햄스터 목덜미가\r\n사라져 있었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100023640.1.16",
          "form": "아악 ㅠㅠㅜㅜㅜ잡아먹은것도 아니고",
          "original_form": "아악 ㅠㅠㅜㅜㅜ잡아먹은것도 아니고",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100023640.1.17",
          "form": "목덜미만ㅜㅠ",
          "original_form": "목덜미만ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100023640.1.18",
          "form": "어우 직접 그걸 봤으니",
          "original_form": "어우 직접 그걸 봤으니",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100023640.1.19",
          "form": "그렇게 한 마리가 가고나니\r\n남은 한 마리도 키울수가 없겠더라구요ㅠ",
          "original_form": "그렇게 한 마리가 가고나니\r\n남은 한 마리도 키울수가 없겠더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100023640.1.20",
          "form": "진짜 충격 받으실만도 하네요ㅠㅠ",
          "original_form": "진짜 충격 받으실만도 하네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100023640.1.21",
          "form": "그죠그죠ㅜㅜ",
          "original_form": "그죠그죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100023640.1.22",
          "form": "두마리만 있던거에요??",
          "original_form": "두마리만 있던거에요??",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100023640.1.23",
          "form": "네ㅠ 두마리 키웠었던건데\r\n남은 한마리는 다른분께\r\n보내드렸어요ㅜ",
          "original_form": "네ㅠ 두마리 키웠었던건데\r\n남은 한마리는 다른분께\r\n보내드렸어요ㅜ",
          "speaker_id": "2",
          "time": "20210901 17:00"
        }
      ]
    }
  ]
