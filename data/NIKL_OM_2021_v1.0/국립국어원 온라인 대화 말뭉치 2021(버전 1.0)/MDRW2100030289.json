        {
          "id": "MDRW2100030289.1.1",
          "form": "4학년때도 해야겠네",
          "original_form": "4학년때도 해야겠네",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030289.1.2",
          "form": "반띵해도 7.5",
          "original_form": "반띵해도 7.5",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030289.1.3",
          "form": "학기제한 그런건 없을거야님",
          "original_form": "학기제한 그런건 없을거야님",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030289.1.4",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030289.1.5",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030289.1.6",
          "form": "내년에도 ㄱㄱ",
          "original_form": "내년에도 ㄱㄱ",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030289.1.7",
          "form": "내년에도 하긴해야지",
          "original_form": "내년에도 하긴해야지",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030289.1.8",
          "form": "꿀인데",
          "original_form": "꿀인데",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030289.1.9",
          "form": "근데 이거하면 뭐 남는거있나",
          "original_form": "근데 이거하면 뭐 남는거있나",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030289.1.10",
          "form": "수료증이라도 줌?",
          "original_form": "수료증이라도 줌?",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030289.1.11",
          "form": "수료증",
          "original_form": "수료증",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030289.1.12",
          "form": "줄걸",
          "original_form": "줄걸",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030289.1.13",
          "form": "어디 쓸일 있을라나",
          "original_form": "어디 쓸일 있을라나",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030289.1.14",
          "form": "멘토는 무슨 교육받음?",
          "original_form": "멘토는 무슨 교육받음?",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030289.1.15",
          "form": "암것도안할걸",
          "original_form": "암것도안할걸",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030289.1.16",
          "form": "그냥 mbti랑 심리검사",
          "original_form": "그냥 mbti랑 심리검사",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030289.1.17",
          "form": "학교에서해주고",
          "original_form": "학교에서해주고",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030289.1.18",
          "form": "학교 mbti 1학년때 했었는데",
          "original_form": "학교 mbti 1학년때 했었는데",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030289.1.19",
          "form": "그때 대충 체크해서 그런지 이상한거나오던데",
          "original_form": "그때 대충 체크해서 그런지 이상한거나오던데",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030289.1.20",
          "form": "아마 이번에",
          "original_form": "아마 이번에",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030289.1.21",
          "form": "해야할걸",
