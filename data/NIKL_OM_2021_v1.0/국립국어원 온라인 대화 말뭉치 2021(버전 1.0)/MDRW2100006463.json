        },
        {
          "id": "MDRW2100006463.1.2",
          "form": "웅웅 내일 먹자 ㅋㅋ",
          "original_form": "웅웅 내일 먹자 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006463.1.3",
          "form": "아니면 배달시켜먹음된다",
          "original_form": "아니면 배달시켜먹음된다",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006463.1.4",
          "form": "지금도 하는데 있으려나",
          "original_form": "지금도 하는데 있으려나",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006463.1.5",
          "form": "있을수도",
          "original_form": "있을수도",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006463.1.6",
          "form": "찾아볼게 ㅋㅋ",
          "original_form": "찾아볼게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006463.1.7",
          "form": "먹고싶으면 먹어야지",
          "original_form": "먹고싶으면 먹어야지",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006463.1.8",
          "form": "웅웅 ㅎㅎ",
          "original_form": "웅웅 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100006463.1.9",
          "form": "순대국 고기 많이 ㅋㅋ",
          "original_form": "순대국 고기 많이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100006463.1.10",
          "form": "나도 고기많이 ㅎㅎ",
          "original_form": "나도 고기많이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100006463.1.11",
          "form": "순대는 배부르고",
          "original_form": "순대는 배부르고",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100006463.1.12",
          "form": "맞다 고기가 최고다ㅋㅋ",
          "original_form": "맞다 고기가 최고다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100006463.1.13",
          "form": "먹고싶네",
          "original_form": "먹고싶네",
          "speaker_id": "2",
