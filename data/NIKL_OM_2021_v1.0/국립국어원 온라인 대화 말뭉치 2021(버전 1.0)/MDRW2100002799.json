      "utterance": [
        {
          "id": "MDRW2100002799.1.1",
          "form": "너는 동호회 하는거 있어?",
          "original_form": "너는 동호회 하는거 있어?",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002799.1.2",
          "form": "동호회는 동아리랑 다른 건가?",
          "original_form": "동호회는 동아리랑 다른 건가?",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002799.1.3",
          "form": "나는 동호회 안하는데",
          "original_form": "나는 동호회 안하는데",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002799.1.4",
          "form": "하는 사람을 본 적 있어",
          "original_form": "하는 사람을 본 적 있어",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002799.1.5",
          "form": "동아리같은거지",
          "original_form": "동아리같은거지",
          "speaker_id": "1",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002799.1.6",
          "form": "근데 동아리는 학교고",
          "original_form": "근데 동아리는 학교고",
          "speaker_id": "1",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002799.1.7",
          "form": "동호회는 사회",
          "original_form": "동호회는 사회",
          "speaker_id": "1",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002799.1.8",
          "form": "동호회 하는사람 누구 봤는데?",
          "original_form": "동호회 하는사람 누구 봤는데?",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002799.1.9",
          "form": "아하 그렇구나",
          "original_form": "아하 그렇구나",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002799.1.10",
          "form": "보드게임 동호회 하는 거 봤어",
          "original_form": "보드게임 동호회 하는 거 봤어",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002799.1.11",
          "form": "보드게임 동호회 멋지다",
          "original_form": "보드게임 동호회 멋지다",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002799.1.12",
          "form": "재미있을거 같아",
          "original_form": "재미있을거 같아",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002799.1.13",
          "form": "나 한번 따라가봤는데",
          "original_form": "나 한번 따라가봤는데",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002799.1.14",
          "form": "정말 보드게임 종류가 다양하더라",
          "original_form": "정말 보드게임 종류가 다양하더라",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002799.1.15",
          "form": "10개 넘어?",
          "original_form": "10개 넘어?",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002799.1.16",
          "form": "한참 넘던데",
          "original_form": "한참 넘던데",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002799.1.17",
          "form": "신기하다 난 보드게임은 잘 몰라서",
          "original_form": "신기하다 난 보드게임은 잘 몰라서",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002799.1.18",
          "form": "나도 그날 처음 해본 게임들 많았는데",
          "original_form": "나도 그날 처음 해본 게임들 많았는데",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002799.1.19",
          "form": "신기하고 재밌었어",
          "original_form": "신기하고 재밌었어",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002799.1.20",
          "form": "그런거 같이 할 사람 있으면 재미있겠다",
          "original_form": "그런거 같이 할 사람 있으면 재미있겠다",
          "speaker_id": "1",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002799.1.21",
          "form": "맞아 새로 나온 게임 있으면",
          "original_form": "맞아 새로 나온 게임 있으면",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002799.1.22",
          "form": "같이 하고 재밌을 둣",
          "original_form": "같이 하고 재밌을 둣",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002799.1.23",
          "form": "듯",
          "original_form": "듯",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002799.1.24",
          "form": "나랑 다음에 같이 가줘",
          "original_form": "나랑 다음에 같이 가줘",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002799.1.25",
          "form": "나는 잘 모르니까",
          "original_form": "나는 잘 모르니까",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002799.1.26",
          "form": "그래 좋아",
          "original_form": "그래 좋아",
          "speaker_id": "2",
          "time": "20210820 15:56"
        }
      ]
    }
