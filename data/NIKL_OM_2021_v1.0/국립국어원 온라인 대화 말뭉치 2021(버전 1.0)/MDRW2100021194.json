        {
          "id": "MDRW2100021194.1.1",
          "form": "겜 조아하시나요",
          "original_form": "겜 조아하시나요",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021194.1.2",
          "form": "메이플하다 말았어요",
          "original_form": "메이플하다 말았어요",
          "speaker_id": "2",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021194.1.3",
          "form": "머하세요",
          "original_form": "머하세요",
          "speaker_id": "2",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021194.1.4",
          "form": "피파온라인이요",
          "original_form": "피파온라인이요",
          "speaker_id": "1",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021194.1.5",
          "form": "축구좋아하시나봐요",
          "original_form": "축구좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021194.1.6",
          "form": "네네 좋아해요 메이플만 하시나요?",
          "original_form": "네네 좋아해요 메이플만 하시나요?",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100021194.1.7",
          "form": "모바일 카트도 잠깐했었어요",
          "original_form": "모바일 카트도 잠깐했었어요",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021194.1.8",
          "form": "모바일카트도 유명하죠~",
          "original_form": "모바일카트도 유명하죠~",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021194.1.9",
          "form": "해보셧나요",
          "original_form": "해보셧나요",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021194.1.10",
          "form": "동생이 가끔하더라구요ㅎㅎ",
          "original_form": "동생이 가끔하더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021194.1.11",
          "form": "아 저도 동생따라 깔았어요ㅋㅋ",
          "original_form": "아 저도 동생따라 깔았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100021194.1.12",
          "form": "자주하시나요 요즘도?",
          "original_form": "자주하시나요 요즘도?",
          "speaker_id": "1",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021194.1.13",
          "form": "아뇨 조금하다 지웠어요",
          "original_form": "아뇨 조금하다 지웠어요",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021194.1.14",
          "form": "잘 질려해서",
          "original_form": "잘 질려해서",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021194.1.15",
          "form": "요즘도 잘나가나 모르겠네요",
          "original_form": "요즘도 잘나가나 모르겠네요",
