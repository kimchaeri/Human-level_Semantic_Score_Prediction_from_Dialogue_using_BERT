      "utterance": [
        {
          "id": "MDRW2100008352.1.1",
          "form": "여행 어디 가고싶으신데 있나요?",
          "original_form": "여행 어디 가고싶으신데 있나요?",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008352.1.2",
          "form": "저 제주도 가고싶어요 못간지 한참 됐어요 ..",
          "original_form": "저 제주도 가고싶어요 못간지 한참 됐어요 ..",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008352.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008352.1.4",
          "form": "아 저도 제주도 가고싶어요",
          "original_form": "아 저도 제주도 가고싶어요",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008352.1.5",
          "form": "바다의 향기가 그리워요 ㅠㅠ",
          "original_form": "바다의 향기가 그리워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008352.1.6",
          "form": "여행가면 뭐가 가장 하고싶으세요?",
          "original_form": "여행가면 뭐가 가장 하고싶으세요?",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008352.1.7",
          "form": "저는 해산물을 좋아해서",
          "original_form": "저는 해산물을 좋아해서",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008352.1.8",
          "form": "제주도가서 바다보며 해산물 먹고싶네요ㅠㅠ",
          "original_form": "제주도가서 바다보며 해산물 먹고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008352.1.9",
          "form": "와 저도 해산물 진짜 좋아해요 ㅠㅠ",
          "original_form": "와 저도 해산물 진짜 좋아해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008352.1.10",
          "form": "제일 좋아하시는게 뭐예요?",
          "original_form": "제일 좋아하시는게 뭐예요?",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008352.1.11",
          "form": "저는 해산물 거의 다 좋아하는데 젤 좋아하는건 방어나 조개류 좋아해요ㅎㅎ",
          "original_form": "저는 해산물 거의 다 좋아하는데 젤 좋아하는건 방어나 조개류 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008352.1.12",
          "form": "제주도에 고등어회도 맛있구요..",
          "original_form": "제주도에 고등어회도 맛있구요..",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008352.1.13",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008352.1.14",
          "form": "저는 새우가 너무 좋더라고요",
          "original_form": "저는 새우가 너무 좋더라고요",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100008352.1.15",
          "form": "통으로 튀겨먹으면 특히 너무 맛난거같아요",
          "original_form": "통으로 튀겨먹으면 특히 너무 맛난거같아요",
          "speaker_id": "1",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008352.1.16",
          "form": "제주도는 고등학생때 현장체험으로 가본게 다인데",
          "original_form": "제주도는 고등학생때 현장체험으로 가본게 다인데",
          "speaker_id": "1",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008352.1.17",
          "form": "너무 맛없는 음식 먹어서 다음에가면 고등어 회 먹어봐야갰어요 ㅠㅠ",
          "original_form": "너무 맛없는 음식 먹어서 다음에가면 고등어 회 먹어봐야갰어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008352.1.18",
          "form": "맞아요 ..코로나 끝나면 제주도 꼭 가봐야겠어요",
          "original_form": "맞아요 ..코로나 끝나면 제주도 꼭 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008352.1.19",
          "form": "요즘 제주도에 특산품..? 먹거리들이 되게 맛있는거 많이 나왔더라구요",
          "original_form": "요즘 제주도에 특산품..? 먹거리들이 되게 맛있는거 많이 나왔더라구요",
          "speaker_id": "2",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100008352.1.20",
          "form": "우와.. 너무 가고싶어요",
          "original_form": "우와.. 너무 가고싶어요",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008352.1.21",
          "form": "집애서 좀 나가고싶어요!!!",
          "original_form": "집애서 좀 나가고싶어요!!!",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008352.1.22",
          "form": "해외여행은 어디 가고싶으신곳 없나요?",
          "original_form": "해외여행은 어디 가고싶으신곳 없나요?",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008352.1.23",
          "form": "맞아요 어디든 여행떠나고 싶죠ㅠㅠ",
          "original_form": "맞아요 어디든 여행떠나고 싶죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008352.1.24",
          "form": "저는 동남아 갔을때 너무 좋았어서 동남아 다시 가보고싶어요..",
          "original_form": "저는 동남아 갔을때 너무 좋았어서 동남아 다시 가보고싶어요..",
          "speaker_id": "2",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008352.1.25",
          "form": "어떤 점이 좋았나요?!",
          "original_form": "어떤 점이 좋았나요?!",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008352.1.26",
          "form": "물가가 저렴하고 과일이 너무 맛있어요 ..!",
          "original_form": "물가가 저렴하고 과일이 너무 맛있어요 ..!",
          "speaker_id": "2",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008352.1.27",
          "form": "여행하는내내 망고 질릴정도로 먹고왔어요 정말 맛있더라구요",
          "original_form": "여행하는내내 망고 질릴정도로 먹고왔어요 정말 맛있더라구요",
          "speaker_id": "2",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008352.1.28",
          "form": "와 물가 저렴한건 진짜 좋네요",
          "original_form": "와 물가 저렴한건 진짜 좋네요",
          "speaker_id": "1",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100008352.1.29",
          "form": "제주도 가는 것보다 날거같기도 하고 ㅋㅋㅋㅋㅋ",
          "original_form": "제주도 가는 것보다 날거같기도 하고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008352.1.30",
          "form": "망고는 어때요 뭔가 달라요?",
          "original_form": "망고는 어때요 뭔가 달라요?",
          "speaker_id": "1",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008352.1.31",
          "form": "맞아요 제주도 너무 비싸긴해요 음식값이랑 숙소값생각하면은ㅠㅠ",
          "original_form": "맞아요 제주도 너무 비싸긴해요 음식값이랑 숙소값생각하면은ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008352.1.32",
          "form": "망고 진짜 달고 몇천원에 봉지가득 담아주세요!",
          "original_form": "망고 진짜 달고 몇천원에 봉지가득 담아주세요!",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008352.1.33",
          "form": "비싼 망고스틴도 저렴하게 먹을 수 있구요 열대과일좋아하시면 추천해요",
          "original_form": "비싼 망고스틴도 저렴하게 먹을 수 있구요 열대과일좋아하시면 추천해요",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008352.1.34",
          "form": "와 저도 꼭 가봐야겠어요",
          "original_form": "와 저도 꼭 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008352.1.35",
          "form": "해외는 사실 한번도 못나가봤거든요",
          "original_form": "해외는 사실 한번도 못나가봤거든요",
