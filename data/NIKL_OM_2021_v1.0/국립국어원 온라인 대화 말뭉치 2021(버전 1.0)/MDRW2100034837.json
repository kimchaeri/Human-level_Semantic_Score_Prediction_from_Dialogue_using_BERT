      "utterance": [
        {
          "id": "MDRW2100034837.1.1",
          "form": "좋아해요~",
          "original_form": "좋아해요~",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034837.1.2",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034837.1.3",
          "form": "저두 좋아하죠 ㅎㅎ 어떤 장르 좋아해요?",
          "original_form": "저두 좋아하죠 ㅎㅎ 어떤 장르 좋아해요?",
          "speaker_id": "1",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100034837.1.4",
          "form": "요즘에 인디 음악 많이 듣는 거 같아요!",
          "original_form": "요즘에 인디 음악 많이 듣는 거 같아요!",
          "speaker_id": "2",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100034837.1.5",
          "form": "인디 음악 좋아하시나요?",
          "original_form": "인디 음악 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100034837.1.6",
          "form": "네네 ! 최근에 좀 들었어요 !",
          "original_form": "네네 ! 최근에 좀 들었어요 !",
          "speaker_id": "1",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100034837.1.7",
          "form": "추천곡 있으신가요 ㅎㅎ",
          "original_form": "추천곡 있으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100034837.1.8",
          "form": "민수 님이랑 정우 님 노래 많이 들어요!",
          "original_form": "민수 님이랑 정우 님 노래 많이 들어요!",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100034837.1.9",
          "form": "들어보셨나요?",
          "original_form": "들어보셨나요?",
          "speaker_id": "2",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100034837.1.10",
          "form": "오 아니요 ! 첨 들어봐요",
          "original_form": "오 아니요 ! 첨 들어봐요",
          "speaker_id": "1",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100034837.1.11",
          "form": "아 민수님은 검색해보니까 알겠어요 !",
          "original_form": "아 민수님은 검색해보니까 알겠어요 !",
          "speaker_id": "1",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100034837.1.12",
          "form": "그럼 정우님 나에게서 당신에게라는 곡 추천드려요!",
          "original_form": "그럼 정우님 나에게서 당신에게라는 곡 추천드려요!",
          "speaker_id": "2",
          "time": "20210828 20:40"
        },
        {
          "id": "MDRW2100034837.1.13",
          "form": "오오 감사해요 ! 오늘 잘때 꼭 들을께요 ㅎㅎ",
          "original_form": "오오 감사해요 ! 오늘 잘때 꼭 들을께요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100034837.1.14",
          "form": "잔잔한가요?",
          "original_form": "잔잔한가요?",
          "speaker_id": "1",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100034837.1.15",
          "form": "네 잔잔해요!",
          "original_form": "네 잔잔해요!",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100034837.1.16",
          "form": "name1 님은 어떤 장르 좋아하세요?",
          "original_form": "&name1& 님은 어떤 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100034837.1.17",
          "form": "좋아요 ㅎㅎ 요즘 약간 잔잔한게 듣고싶더라구요!",
          "original_form": "좋아요 ㅎㅎ 요즘 약간 잔잔한게 듣고싶더라구요!",
          "speaker_id": "1",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100034837.1.18",
          "form": "잔잔한 노래 좋죠!",
          "original_form": "잔잔한 노래 좋죠!",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100034837.1.19",
          "form": "팝송도 들으세요?",
          "original_form": "팝송도 들으세요?",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100034837.1.20",
          "form": "팝송은 잘 안들어요 !",
          "original_form": "팝송은 잘 안들어요 !",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100034837.1.21",
          "form": "전 가사 위주로 노래를 들어서 ㅎㅎㅎ",
          "original_form": "전 가사 위주로 노래를 들어서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100034837.1.22",
          "form": "가사위주로 들으시는군요!",
          "original_form": "가사위주로 들으시는군요!",
          "speaker_id": "2",
          "time": "20210828 21:02"
        }
      ]
