      "utterance": [
        {
          "id": "MDRW2100042729.1.1",
          "form": "어제 애들 재우다가 잠들었음ㅋㅋ",
          "original_form": "어제 애들 재우다가 잠들었음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100042729.1.2",
          "form": "니 이제 거의끝났재?",
          "original_form": "니 이제 거의끝났재?",
          "speaker_id": "2",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042729.1.3",
          "form": "축하한다ㅋㅋ",
          "original_form": "축하한다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042729.1.4",
          "form": "첫째등교보냈어요. 등교준비는 끝났고ㅋㅋ",
          "original_form": "첫째등교보냈어요. 등교준비는 끝났고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042729.1.5",
          "form": "둘째등원준비중요ㅋ",
          "original_form": "둘째등원준비중요ㅋ",
          "speaker_id": "1",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042729.1.6",
          "form": "밥먹었어요?",
          "original_form": "밥먹었어요?",
          "speaker_id": "1",
          "time": "20210907 08:45"
        },
        {
          "id": "MDRW2100042729.1.7",
          "form": "오늘 브런치약속",
          "original_form": "오늘 브런치약속",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042729.1.8",
          "form": "룰루랄라",
          "original_form": "룰루랄라",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042729.1.9",
          "form": "오오 좋다요 ㅋ",
          "original_form": "오오 좋다요 ㅋ",
          "speaker_id": "1",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042729.1.10",
          "form": "앞산도착",
          "original_form": "앞산도착",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042729.1.11",
          "form": "브런치",
          "original_form": "브런치",
          "speaker_id": "1",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042729.1.12",
          "form": "어디가요?",
          "original_form": "어디가요?",
          "speaker_id": "1",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042729.1.13",
          "form": "아눅 이라는데",
          "original_form": "아눅 이라는데",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042729.1.14",
          "form": "지금도착했는데",
          "original_form": "지금도착했는데",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042729.1.15",
          "form": "10시오픈이래",
          "original_form": "10시오픈이래",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042729.1.16",
          "form": "차안이다ㅋ",
          "original_form": "차안이다ㅋ",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042729.1.17",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "1",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042729.1.18",
          "form": "나중에 사진으로 보내줘봐요 ㅋㅋ 궁금하네",
          "original_form": "나중에 사진으로 보내줘봐요 ㅋㅋ 궁금하네",
          "speaker_id": "1",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100042729.1.19",
          "form": "그래ㅋㅋㅋ",
          "original_form": "그래ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042729.1.20",
          "form": "이제들어가야긋다ㅋ",
          "original_form": "이제들어가야긋다ㅋ",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042729.1.21",
          "form": "언니 잘 먹고 와요 ㅋㅋ",
          "original_form": "언니 잘 먹고 와요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100042729.1.22",
          "form": "좋다 좋다",
          "original_form": "좋다 좋다",
          "speaker_id": "1",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100042729.1.23",
          "form": "그래 니 축하한닥",
          "original_form": "그래 니 축하한닥",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042729.1.24",
          "form": "여기 전쟁이네 자리전쟁ㅡㅡ",
          "original_form": "여기 전쟁이네 자리전쟁ㅡㅡ",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042729.1.25",
          "form": "몰랐음",
          "original_form": "몰랐음",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042729.1.26",
          "form": "일찍왔는데ㅡㅡ;;",
          "original_form": "일찍왔는데ㅡㅡ;;",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042729.1.27",
          "form": "ㅋㅋㅋㅋ와 사람많은가봐요 ㅠ",
          "original_form": "ㅋㅋㅋㅋ와 사람많은가봐요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042729.1.28",
          "form": "대박이네 ㅋㅋㅋ",
          "original_form": "대박이네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042729.1.29",
          "form": "핫플인듯",
          "original_form": "핫플인듯",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042729.1.30",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100042729.1.31",
          "form": "그런가봐요 ㅋㅋ",
          "original_form": "그런가봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:11"
        },
        {
          "id": "MDRW2100042729.1.32",
          "form": "조심해요 ㅋㅋ 코로나.....ㅠㅠ ㅋㅋ 조심해서 먹어요",
          "original_form": "조심해요 ㅋㅋ 코로나.....ㅠㅠ ㅋㅋ 조심해서 먹어요",
          "speaker_id": "1",
          "time": "20210907 10:11"
        },
        {
          "id": "MDRW2100042729.1.33",
          "form": "지금 야외비스무리",
          "original_form": "지금 야외비스무리",
          "speaker_id": "2",
          "time": "20210907 10:11"
        },
        {
          "id": "MDRW2100042729.1.34",
          "form": "여기가 명당인듯ㅡㅡ",
          "original_form": "여기가 명당인듯ㅡㅡ",
          "speaker_id": "2",
          "time": "20210907 10:12"
        },
        {
          "id": "MDRW2100042729.1.35",
          "form": "그러게요 ㅋㅋ 언니 잘 먹고 와요 ㅋ",
          "original_form": "그러게요 ㅋㅋ 언니 잘 먹고 와요 ㅋ",
