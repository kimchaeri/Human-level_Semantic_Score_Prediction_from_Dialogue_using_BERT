        },
        {
          "id": "MDRW2100046332.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211019 20:02"
        },
        {
          "id": "MDRW2100046332.1.3",
          "form": "안녕하세요...",
          "original_form": "안녕하세요...",
          "speaker_id": "1",
          "time": "20211019 20:02"
        },
        {
          "id": "MDRW2100046332.1.4",
          "form": "백신은 다 맞으셨나요?",
          "original_form": "백신은 다 맞으셨나요?",
          "speaker_id": "2",
          "time": "20211019 20:02"
        },
        {
          "id": "MDRW2100046332.1.5",
          "form": "네 다맞았습니다",
          "original_form": "네 다맞았습니다",
          "speaker_id": "1",
          "time": "20211019 20:02"
        },
        {
          "id": "MDRW2100046332.1.6",
          "form": "아프진 않으셨나요",
          "original_form": "아프진 않으셨나요",
          "speaker_id": "2",
          "time": "20211019 20:03"
        },
        {
          "id": "MDRW2100046332.1.7",
          "form": "네 안아팠습니다 괜찮습니다",
          "original_form": "네 안아팠습니다 괜찮습니다",
          "speaker_id": "1",
          "time": "20211019 20:03"
        },
        {
          "id": "MDRW2100046332.1.8",
          "form": "와 튼튼하신가봐요",
          "original_form": "와 튼튼하신가봐요",
          "speaker_id": "2",
          "time": "20211019 20:03"
        },
        {
          "id": "MDRW2100046332.1.9",
          "form": "전 1차 때도 이틀이나 누워있었는데",
          "original_form": "전 1차 때도 이틀이나 누워있었는데",
          "speaker_id": "2",
          "time": "20211019 20:03"
        },
        {
          "id": "MDRW2100046332.1.10",
          "form": "화이자라서 그런가봐요",
          "original_form": "화이자라서 그런가봐요",
          "speaker_id": "1",
          "time": "20211019 20:04"
        },
        {
          "id": "MDRW2100046332.1.11",
          "form": "화이자는 약하대요?",
          "original_form": "화이자는 약하대요?",
          "speaker_id": "2",
          "time": "20211019 20:04"
        },
        {
          "id": "MDRW2100046332.1.12",
          "form": "아픈사람이 덜하긴 하답니다",
          "original_form": "아픈사람이 덜하긴 하답니다",
          "speaker_id": "1",
          "time": "20211019 20:04"
        },
        {
          "id": "MDRW2100046332.1.13",
          "form": "그렇구나 저도 그거 맞았으면 나았으려나요",
          "original_form": "그렇구나 저도 그거 맞았으면 나았으려나요",
          "speaker_id": "2",
          "time": "20211019 20:04"
        },
        {
          "id": "MDRW2100046332.1.14",
          "form": "보통 2차가 더 아프다는데 전 1차도 너무 아파서 무섭네요 ㅠ",
          "original_form": "보통 2차가 더 아프다는데 전 1차도 너무 아파서 무섭네요 ㅠ",
          "speaker_id": "2",
          "time": "20211019 20:05"
        },
        {
          "id": "MDRW2100046332.1.15",
          "form": "아직 안맞으셧나오?",
          "original_form": "아직 안맞으셧나오?",
          "speaker_id": "1",
          "time": "20211019 20:05"
        },
        {
          "id": "MDRW2100046332.1.16",
          "form": "넹 다음주에요 ㅋㅋ",
          "original_form": "넹 다음주에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 20:05"
        },
        {
          "id": "MDRW2100046332.1.17",
          "form": "부스터샷 얘기 나오는데 그건 꿈도 못 꾸겠어요",
          "original_form": "부스터샷 얘기 나오는데 그건 꿈도 못 꾸겠어요",
          "speaker_id": "2",
          "time": "20211019 20:05"
        },
        {
          "id": "MDRW2100046332.1.18",
          "form": "부스터샷 맞으실 거예요?",
          "original_form": "부스터샷 맞으실 거예요?",
          "speaker_id": "2",
          "time": "20211019 20:06"
        },
        {
          "id": "MDRW2100046332.1.19",
          "form": "고민하고 있습니다",
          "original_form": "고민하고 있습니다",
          "speaker_id": "1",
          "time": "20211019 20:06"
        },
        {
          "id": "MDRW2100046332.1.20",
          "form": "같은 화이자면",
          "original_form": "같은 화이자면",
          "speaker_id": "1",
          "time": "20211019 20:06"
        },
        {
          "id": "MDRW2100046332.1.21",
          "form": "맞아볼려구요",
          "original_form": "맞아볼려구요",
          "speaker_id": "1",
          "time": "20211019 20:06"
        },
        {
          "id": "MDRW2100046332.1.22",
          "form": "그렇군요! 얼른 마스크 벗는 날 왔으면 좋겠어요 ㅎㅎ",
          "original_form": "그렇군요! 얼른 마스크 벗는 날 왔으면 좋겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 20:07"
        },
        {
          "id": "MDRW2100046332.1.23",
          "form": "네 2차 맞고 안아프셨으면 좋겠네요",
          "original_form": "네 2차 맞고 안아프셨으면 좋겠네요",
          "speaker_id": "1",
          "time": "20211019 20:08"
        },
        {
          "id": "MDRW2100046332.1.24",
          "form": "힘내세요!!",
          "original_form": "힘내세요!!",
