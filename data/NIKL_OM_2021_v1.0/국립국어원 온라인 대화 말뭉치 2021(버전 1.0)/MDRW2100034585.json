      "utterance": [
        {
          "id": "MDRW2100034585.1.1",
          "form": "좋아하던 과목 있으셨나요?",
          "original_form": "좋아하던 과목 있으셨나요?",
          "speaker_id": "1",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100034585.1.2",
          "form": "그럼 그때 이미 전공이 잇으셧겟네요",
          "original_form": "그럼 그때 이미 전공이 잇으셧겟네요",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034585.1.3",
          "form": "저는 국어 좋아햇어요 ㅋㅋ",
          "original_form": "저는 국어 좋아햇어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034585.1.4",
          "form": "오저두요 ㅋㅋㅋ",
          "original_form": "오저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100034585.1.5",
          "form": "네 저는 의료관련과였어요",
          "original_form": "네 저는 의료관련과였어요",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100034585.1.6",
          "form": "와 멋잇네요",
          "original_form": "와 멋잇네요",
          "speaker_id": "2",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100034585.1.7",
          "form": "말만 그렇지 별건 없어요 ㅋㅋㅋ",
          "original_form": "말만 그렇지 별건 없어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100034585.1.8",
          "form": "문과셨나요?",
          "original_form": "문과셨나요?",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100034585.1.9",
          "form": "네 문과엿어요 ㅋㅋㅋㅋ",
          "original_form": "네 문과엿어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100034585.1.10",
          "form": "ㅋㅋㅋㅋ 제 친구들도 보면 문과가 많더라구요",
          "original_form": "ㅋㅋㅋㅋ 제 친구들도 보면 문과가 많더라구요",
          "speaker_id": "1",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100034585.1.11",
          "form": "ㅋㅋㅋㅋㅋ다시 돌아가면 이과갈거예요...",
          "original_form": "ㅋㅋㅋㅋㅋ다시 돌아가면 이과갈거예요...",
          "speaker_id": "2",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100034585.1.12",
          "form": "헛 그래요?왜요?",
          "original_form": "헛 그래요?왜요?",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100034585.1.13",
          "form": "한번 이과과목이랑 멀어지고나면 이과쪽이 너무 어렵더라구요",
          "original_form": "한번 이과과목이랑 멀어지고나면 이과쪽이 너무 어렵더라구요",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100034585.1.14",
          "form": "이과 나온 친구들은 더러 문과쪽 전공 복수전공도 하는데 ㅋㅋㅋㅋ",
          "original_form": "이과 나온 친구들은 더러 문과쪽 전공 복수전공도 하는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100034585.1.15",
          "form": "하 맞아요",
          "original_form": "하 맞아요",
          "speaker_id": "1",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100034585.1.16",
          "form": "이과가 난이도가 더 높은가봐요 확실히..ㅋㅋㅋ",
          "original_form": "이과가 난이도가 더 높은가봐요 확실히..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100034585.1.17",
          "form": "숫자는 전 다 어렵더라구요",
          "original_form": "숫자는 전 다 어렵더라구요",
          "speaker_id": "1",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100034585.1.18",
          "form": "그래서 너무 후회햇어요 이과안간걸 ㅋㅋㅋㅋ",
          "original_form": "그래서 너무 후회햇어요 이과안간걸 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100034585.1.19",
          "form": "저는 과학쪽이 그렇게 싫엇어요 ㅋㅋㅋㅋ",
          "original_form": "저는 과학쪽이 그렇게 싫엇어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:53"
        },
        {
          "id": "MDRW2100034585.1.20",
          "form": "앜ㅋㅋ 과학도 어려워요",
          "original_form": "앜ㅋㅋ 과학도 어려워요",
