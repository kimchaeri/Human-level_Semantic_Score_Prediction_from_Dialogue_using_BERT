      "utterance": [
        {
          "id": "MDRW2100031786.1.1",
          "form": "엄마가 생선 손질 하셔서요",
          "original_form": "엄마가 생선 손질 하셔서요",
          "speaker_id": "2",
          "time": "20210905 00:28"
        },
        {
          "id": "MDRW2100031786.1.2",
          "form": "주방이 다 생선 비늘이에요",
          "original_form": "주방이 다 생선 비늘이에요",
          "speaker_id": "2",
          "time": "20210905 00:28"
        },
        {
          "id": "MDRW2100031786.1.3",
          "form": "식사 준비를 한다는 건 노동이지요",
          "original_form": "식사 준비를 한다는 건 노동이지요",
          "speaker_id": "2",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031786.1.4",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031786.1.5",
          "form": "아까많이받으셔시다했지요",
          "original_form": "아까많이받으셔시다했지요",
          "speaker_id": "1",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031786.1.6",
          "form": "집안일은 안 하면 티가 나지요",
          "original_form": "집안일은 안 하면 티가 나지요",
          "speaker_id": "2",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031786.1.7",
          "form": "저1분만요",
          "original_form": "저1분만요",
          "speaker_id": "1",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031786.1.8",
          "form": "네 손질이 아예 안 되어서",
          "original_form": "네 손질이 아예 안 되어서",
          "speaker_id": "2",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031786.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031786.1.10",
          "form": "미션 하고 오세요",
          "original_form": "미션 하고 오세요",
          "speaker_id": "2",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031786.1.11",
          "form": "왔습니다",
          "original_form": "왔습니다",
          "speaker_id": "1",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031786.1.12",
          "form": "미션이건어카7ㅣ아셨죠",
          "original_form": "미션이건어카7ㅣ아셨죠",
          "speaker_id": "1",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031786.1.13",
          "form": "일은 다 하셨나요",
          "original_form": "일은 다 하셨나요",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031786.1.14",
          "form": "안 봐도 뻔하지요",
          "original_form": "안 봐도 뻔하지요",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031786.1.15",
          "form": "네 맞아요 성공하고 왔습니다",
          "original_form": "네 맞아요 성공하고 왔습니다",
          "speaker_id": "1",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031786.1.16",
          "form": "중요한 일이지요",
          "original_form": "중요한 일이지요",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031786.1.17",
          "form": "짝짝짝",
          "original_form": "짝짝짝",
          "speaker_id": "2",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031786.1.18",
          "form": "맞아요 가사일은 안 하면은지가 너무 많이 나고 해야죠 티가 안 나죠",
          "original_form": "맞아요 가사일은 안 하면은지가 너무 많이 나고 해야죠 티가 안 나죠",
          "speaker_id": "1",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031786.1.19",
          "form": "저는 생선 내장까지 손질 못 해요",
          "original_form": "저는 생선 내장까지 손질 못 해요",
          "speaker_id": "2",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031786.1.20",
          "form": "비늘은 벗길 수 있어요",
          "original_form": "비늘은 벗길 수 있어요",
          "speaker_id": "2",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031786.1.21",
          "form": "저도 못 할 것 같아요 생선 손질 하면 손에 냄새가 오래 남지 않나요",
          "original_form": "저도 못 할 것 같아요 생선 손질 하면 손에 냄새가 오래 남지 않나요",
          "speaker_id": "1",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031786.1.22",
          "form": "전 생선 손질 해 본 적이 단 한 번도 없어요",
          "original_form": "전 생선 손질 해 본 적이 단 한 번도 없어요",
          "speaker_id": "1",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031786.1.23",
          "form": "비린내가 어마어마해요",
          "original_form": "비린내가 어마어마해요",
          "speaker_id": "2",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031786.1.24",
          "form": "제사 때 새우튀김 하면 새우 똥은 빼 본 적이 있네요",
          "original_form": "제사 때 새우튀김 하면 새우 똥은 빼 본 적이 있네요",
          "speaker_id": "1",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031786.1.25",
          "form": "비늘이 엄청 튀었어요",
          "original_form": "비늘이 엄청 튀었어요",
          "speaker_id": "2",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031786.1.26",
          "form": "비누로 씻어도 냄새가 오래 가겠지요",
          "original_form": "비누로 씻어도 냄새가 오래 가겠지요",
          "speaker_id": "1",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031786.1.27",
          "form": "정말 가사 일은 해도 해도 끝이 없네요 청소해 요리에",
          "original_form": "정말 가사 일은 해도 해도 끝이 없네요 청소해 요리에",
          "speaker_id": "1",
          "time": "20210905 00:34"
        },
        {
          "id": "MDRW2100031786.1.28",
          "form": "생선 구울 때 연기도나요",
          "original_form": "생선 구울 때 연기도나요",
          "speaker_id": "2",
          "time": "20210905 00:34"
        }
      ]
    }
