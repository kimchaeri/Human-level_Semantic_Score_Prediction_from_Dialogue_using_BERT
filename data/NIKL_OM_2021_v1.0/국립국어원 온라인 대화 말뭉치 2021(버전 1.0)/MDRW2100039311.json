        {
          "id": "MDRW2100039311.1.1",
          "form": "잘산템있우세요",
          "original_form": "잘산템있우세요",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100039311.1.2",
          "form": "전 먹을거 맛있는거요ㅎ",
          "original_form": "전 먹을거 맛있는거요ㅎ",
          "speaker_id": "1",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100039311.1.3",
          "form": "저도 복숭아 잘산거같아요",
          "original_form": "저도 복숭아 잘산거같아요",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039311.1.4",
          "form": "복숭아 맛나지요",
          "original_form": "복숭아 맛나지요",
          "speaker_id": "1",
          "time": "20210904 12:01"
        },
        {
          "id": "MDRW2100039311.1.5",
          "form": "요즘 사과도 괜찮아요",
          "original_form": "요즘 사과도 괜찮아요",
          "speaker_id": "1",
          "time": "20210904 12:01"
        },
        {
          "id": "MDRW2100039311.1.6",
          "form": "네네 사과가 벌써 맛이들었낭6",
          "original_form": "네네 사과가 벌써 맛이들었낭6",
          "speaker_id": "2",
          "time": "20210904 12:01"
        },
        {
          "id": "MDRW2100039311.1.7",
          "form": "네네 먹었더니 맛나더라구요",
          "original_form": "네네 먹었더니 맛나더라구요",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100039311.1.8",
          "form": "오 사과를 사야겠네요",
          "original_form": "오 사과를 사야겠네요",
          "speaker_id": "2",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100039311.1.9",
          "form": "네 초록색 사과도 맛나요",
          "original_form": "네 초록색 사과도 맛나요",
          "speaker_id": "1",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100039311.1.10",
          "form": "아오리사과요?",
