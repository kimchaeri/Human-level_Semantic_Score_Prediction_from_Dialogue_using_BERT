      "utterance": [
        {
          "id": "MDRW2100027075.1.1",
          "form": "헐 빨리나오면 좋겠네요",
          "original_form": "헐 빨리나오면 좋겠네요",
          "speaker_id": "2",
          "time": "20210902 09:02"
        },
        {
          "id": "MDRW2100027075.1.2",
          "form": "전 컴터사면 할려구요!",
          "original_form": "전 컴터사면 할려구요!",
          "speaker_id": "2",
          "time": "20210902 09:02"
        },
        {
          "id": "MDRW2100027075.1.3",
          "form": "무슨게임보세요??",
          "original_form": "무슨게임보세요??",
          "speaker_id": "2",
          "time": "20210902 09:02"
        },
        {
          "id": "MDRW2100027075.1.4",
          "form": "오 어떤컴퓨터 주문하시나요",
          "original_form": "오 어떤컴퓨터 주문하시나요",
          "speaker_id": "1",
          "time": "20210902 09:03"
        },
        {
          "id": "MDRW2100027075.1.5",
          "form": "게이밍 컴터입니당",
          "original_form": "게이밍 컴터입니당",
          "speaker_id": "2",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100027075.1.6",
          "form": "얼마정도 하나요?",
          "original_form": "얼마정도 하나요?",
          "speaker_id": "1",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100027075.1.7",
          "form": "저는 트위치 게임방송 많이 봐요",
          "original_form": "저는 트위치 게임방송 많이 봐요",
          "speaker_id": "1",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100027075.1.8",
          "form": "90정도 하는거같은데 다른장비들까지사려면",
          "original_form": "90정도 하는거같은데 다른장비들까지사려면",
          "speaker_id": "2",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100027075.1.9",
          "form": "한120.ㅎ",
          "original_form": "한120.ㅎ",
          "speaker_id": "2",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100027075.1.10",
          "form": "와 큰돈 쓰셨네요",
          "original_form": "와 큰돈 쓰셨네요",
          "speaker_id": "1",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100027075.1.11",
          "form": "맞아요 마음먹기가 ㅠㅠ",
          "original_form": "맞아요 마음먹기가 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 09:05"
        },
        {
          "id": "MDRW2100027075.1.12",
          "form": "그래서 유튭으로 많이 찾아보고있어요",
          "original_form": "그래서 유튭으로 많이 찾아보고있어요",
          "speaker_id": "2",
          "time": "20210902 09:06"
        },
        {
          "id": "MDRW2100027075.1.13",
          "form": "저도 게임 돌릴 컴퓨터 사고싶은데",
          "original_form": "저도 게임 돌릴 컴퓨터 사고싶은데",
          "speaker_id": "1",
          "time": "20210902 09:06"
        },
        {
          "id": "MDRW2100027075.1.14",
          "form": "한번사면",
          "original_form": "한번사면",
          "speaker_id": "2",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100027075.1.15",
          "form": "집에서 안나갈거같어욬ㅋㅋ",
          "original_form": "집에서 안나갈거같어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100027075.1.16",
          "form": "맞아요 시력도 난리나요",
          "original_form": "맞아요 시력도 난리나요",
          "speaker_id": "1",
          "time": "20210902 09:14"
        },
        {
          "id": "MDRW2100027075.1.17",
          "form": "맞아요...ㅎ 이상태에서 더 나빠지면 실명수준...",
          "original_form": "맞아요...ㅎ 이상태에서 더 나빠지면 실명수준...",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100027075.1.18",
          "form": "초등학교때 심즈1을 처음하고 그때부터 게임 중독돼써요",
          "original_form": "초등학교때 심즈1을 처음하고 그때부터 게임 중독돼써요",
          "speaker_id": "1",
          "time": "20210902 09:39"
        },
        {
          "id": "MDRW2100027075.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ심즈진짜오랜만이네요 저는 그거 보기만해봤어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ심즈진짜오랜만이네요 저는 그거 보기만해봤어요",
          "speaker_id": "2",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100027075.1.20",
          "form": "심즈 너무 중독이에요",
          "original_form": "심즈 너무 중독이에요",
