      "utterance": [
        {
          "id": "MDRW2100015767.1.1",
          "form": "요즘 여행 많이 못 가잖아요 ㅠ",
          "original_form": "요즘 여행 많이 못 가잖아요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015767.1.2",
          "form": "혹시 코로나 풀리면 가고 싶은 여행지 있나요",
          "original_form": "혹시 코로나 풀리면 가고 싶은 여행지 있나요",
          "speaker_id": "1",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015767.1.3",
          "form": "전 어디든 ㅋ 해외면  다좋을거같아요",
          "original_form": "전 어디든 ㅋ 해외면  다좋을거같아요",
          "speaker_id": "2",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015767.1.4",
          "form": "해외 저 중국밖에 안가봐서 궁금한데",
          "original_form": "해외 저 중국밖에 안가봐서 궁금한데",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015767.1.5",
          "form": "가장 좋으셨던 곳이 어딘가요?",
          "original_form": "가장 좋으셨던 곳이 어딘가요?",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015767.1.6",
          "form": "호주도  좋았고",
          "original_form": "호주도  좋았고",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100015767.1.7",
          "form": "캄보디아도 괜찮았어요",
          "original_form": "캄보디아도 괜찮았어요",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100015767.1.8",
          "form": "다 나라마다 느낌이 달라서",
          "original_form": "다 나라마다 느낌이 달라서",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100015767.1.9",
          "form": "동유럽도 좋고",
          "original_form": "동유럽도 좋고",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100015767.1.10",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100015767.1.11",
          "form": "여행 좋아하시나봐요",
          "original_form": "여행 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100015767.1.12",
          "form": "캄보디아 가면 뭘 꼭 먹어야 하나요?",
          "original_form": "캄보디아 가면 뭘 꼭 먹어야 하나요?",
          "speaker_id": "1",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100015767.1.13",
          "form": "유적지가 왜 나쁜말인지 ㅠ",
          "original_form": "유적지가 왜 나쁜말인지 ㅠ",
          "speaker_id": "2",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015767.1.14",
          "form": "앙코르와트보고 유적지주로 보는거라서요",
          "original_form": "앙코르와트보고 유적지주로 보는거라서요",
          "speaker_id": "2",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015767.1.15",
          "form": "음식맛집은 별로없어요",
          "original_form": "음식맛집은 별로없어요",
          "speaker_id": "2",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015767.1.16",
          "form": "아하 막 현지 음식 같은 것도 들어보진 못한 거 같기도 해요",
          "original_form": "아하 막 현지 음식 같은 것도 들어보진 못한 거 같기도 해요",
          "speaker_id": "1",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015767.1.17",
          "form": "앙코르와트  일출이 넘이뻐요",
          "original_form": "앙코르와트  일출이 넘이뻐요",
          "speaker_id": "2",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015767.1.18",
          "form": "물가도 싸서 1일1마사지도하고",
          "original_form": "물가도 싸서 1일1마사지도하고",
          "speaker_id": "2",
          "time": "20210826 01:19"
        },
        {
          "id": "MDRW2100015767.1.19",
          "form": "물가 싼 거 좋죠",
          "original_form": "물가 싼 거 좋죠",
          "speaker_id": "1",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015767.1.20",
          "form": "앙코르와트는 일출이 예쁘군요",
          "original_form": "앙코르와트는 일출이 예쁘군요",
          "speaker_id": "1",
          "time": "20210826 01:21"
        },
        {
          "id": "MDRW2100015767.1.21",
          "form": "캄보디아는 다른데  다가보시고",
          "original_form": "캄보디아는 다른데  다가보시고",
          "speaker_id": "2",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015767.1.22",
          "form": "기회될때 가보셔요",
          "original_form": "기회될때 가보셔요",
          "speaker_id": "2",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015767.1.23",
          "form": "더좋은데 많으니",
          "original_form": "더좋은데 많으니",
          "speaker_id": "2",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015767.1.24",
          "form": "동유럽이 가장 가고 싶네용",
          "original_form": "동유럽이 가장 가고 싶네용",
          "speaker_id": "1",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015767.1.25",
          "form": "동유럽좋아요",
          "original_form": "동유럽좋아요",
          "speaker_id": "2",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015767.1.26",
          "form": "서유럽은 안가봐서 비교는 못하지만",
          "original_form": "서유럽은 안가봐서 비교는 못하지만",
          "speaker_id": "2",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015767.1.27",
          "form": "유럽이 그 돌바닥이라서",
          "original_form": "유럽이 그 돌바닥이라서",
          "speaker_id": "1",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015767.1.28",
          "form": "느낌이 새로워요",
          "original_form": "느낌이 새로워요",
          "speaker_id": "1",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015767.1.29",
          "form": "분위기가 다르긴하죠^^",
          "original_form": "분위기가 다르긴하죠^^",
          "speaker_id": "2",
          "time": "20210826 01:28"
        }
      ]
    }
  ]
