      "utterance": [
        {
          "id": "MDRW2100021390.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021390.1.2",
          "form": "식빵이랑 복숭아먹고있어요 ㅎㅎ",
          "original_form": "식빵이랑 복숭아먹고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021390.1.3",
          "form": "복숭아 맛있겠다",
          "original_form": "복숭아 맛있겠다",
          "speaker_id": "1",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100021390.1.4",
          "form": "딱복인가요 말복인가요",
          "original_form": "딱복인가요 말복인가요",
          "speaker_id": "1",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100021390.1.5",
          "form": "말복이죠",
          "original_form": "말복이죠",
          "speaker_id": "2",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100021390.1.6",
          "form": "저녁 뭐드시나요?",
          "original_form": "저녁 뭐드시나요?",
          "speaker_id": "2",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100021390.1.7",
          "form": "저녁 모르겠어요ㅋㅋㅋㅋ",
          "original_form": "저녁 모르겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100021390.1.8",
          "form": "뭐 먹을거냐고 물었는데 답을 안하셔서",
          "original_form": "뭐 먹을거냐고 물었는데 답을 안하셔서",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100021390.1.9",
          "form": "음..  미역국이 아니지않을까 생각중이에요",
          "original_form": "음..  미역국이 아니지않을까 생각중이에요",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100021390.1.10",
          "form": "전 미역국 정말 좋아해요..",
          "original_form": "전 미역국 정말 좋아해요..",
          "speaker_id": "2",
          "time": "20210828 18:53"
        },
        {
          "id": "MDRW2100021390.1.11",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "1",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100021390.1.12",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100021390.1.13",
          "form": "소고기넣은거 좋아하시나요",
          "original_form": "소고기넣은거 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100021390.1.14",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100021390.1.15",
          "form": "아녀 소고기 노노",
          "original_form": "아녀 소고기 노노",
          "speaker_id": "2",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100021390.1.16",
          "form": "그럼 그냥 맑은 미역국?",
          "original_form": "그럼 그냥 맑은 미역국?",
          "speaker_id": "1",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100021390.1.17",
          "form": "고기안넣은거?",
          "original_form": "고기안넣은거?",
          "speaker_id": "1",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100021390.1.18",
          "form": "네! 맑은거 좋아하고",
          "original_form": "네! 맑은거 좋아하고",
          "speaker_id": "2",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100021390.1.19",
          "form": "그거 그거대로 매력이죠",
          "original_form": "그거 그거대로 매력이죠",
          "speaker_id": "1",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100021390.1.20",
          "form": "회사에서 점심으로 프랜차이즈 먹을땐 조개미역국같은거 좋아해요",
          "original_form": "회사에서 점심으로 프랜차이즈 먹을땐 조개미역국같은거 좋아해요",
          "speaker_id": "2",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100021390.1.21",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100021390.1.22",
          "form": "조개미역국 너무좋아",
          "original_form": "조개미역국 너무좋아",
          "speaker_id": "1",
          "time": "20210828 18:59"
        }
