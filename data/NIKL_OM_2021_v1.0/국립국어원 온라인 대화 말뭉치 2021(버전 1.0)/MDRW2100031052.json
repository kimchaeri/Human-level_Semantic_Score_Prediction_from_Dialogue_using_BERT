      "utterance": [
        {
          "id": "MDRW2100031052.1.1",
          "form": "열쇠가 어딨는지 알고있거든요",
          "original_form": "열쇠가 어딨는지 알고있거든요",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100031052.1.2",
          "form": "재택중에 법인카드 필요하면 그렇게 하는데",
          "original_form": "재택중에 법인카드 필요하면 그렇게 하는데",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100031052.1.3",
          "form": "쓰고 잘 잠궈야지ㅠㅠ",
          "original_form": "쓰고 잘 잠궈야지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100031052.1.4",
          "form": "아 그쵸 쓰고 잠궈두는게 예의인데",
          "original_form": "아 그쵸 쓰고 잠궈두는게 예의인데",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031052.1.5",
          "form": "구러게요ㅠㅠ 또 자긴 잠궜다고 할거같아여ㅠㅠ",
          "original_form": "구러게요ㅠㅠ 또 자긴 잠궜다고 할거같아여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100031052.1.6",
          "form": "아니면 안잠궜어? 미안",
          "original_form": "아니면 안잠궜어? 미안",
          "speaker_id": "1",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031052.1.7",
          "form": "이러면 열받을거 같슴다",
          "original_form": "이러면 열받을거 같슴다",
          "speaker_id": "1",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031052.1.8",
          "form": "그러게요 ㅋㅋ 잊어여죠 뭐",
          "original_form": "그러게요 ㅋㅋ 잊어여죠 뭐",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031052.1.9",
          "form": "그런 인간 하나로 뇌와 마음의 공간을 채우고싶디않아요",
          "original_form": "그런 인간 하나로 뇌와 마음의 공간을 채우고싶디않아요",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031052.1.10",
          "form": "이렇게 착한 바보가 되어갑니다",
          "original_form": "이렇게 착한 바보가 되어갑니다",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031052.1.11",
          "form": "겉과 속이 다른 ㅋㅋㅋ",
          "original_form": "겉과 속이 다른 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031052.1.12",
          "form": "맞아요 저도 취객 만나면",
          "original_form": "맞아요 저도 취객 만나면",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031052.1.13",
          "form": "아 개.가",
          "original_form": "아 개.가",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031052.1.14",
          "form": "또 짖는구나",
          "original_form": "또 짖는구나",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031052.1.15",
          "form": "합니다ㅋㅋ",
          "original_form": "합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031052.1.16",
          "form": "아 종말요 ㅋㅋ저도 그래야겠어요",
          "original_form": "아 종말요 ㅋㅋ저도 그래야겠어요",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031052.1.17",
          "form": "아 취객 종말 힘들것같습니다",
          "original_form": "아 취객 종말 힘들것같습니다",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031052.1.18",
          "form": "답 없으면 경찰 부르면 돼요 ㅋㅋ",
          "original_form": "답 없으면 경찰 부르면 돼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100031052.1.19",
          "form": "아직까진 맞은적은 없습니다 ㅋㅋㅋ",
          "original_form": "아직까진 맞은적은 없습니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100031052.1.20",
          "form": "한번 ㅋㅋㅋ",
          "original_form": "한번 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031052.1.21",
          "form": "아 항상 조심허셔요ㅠㅠ",
          "original_form": "아 항상 조심허셔요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100031052.1.22",
          "form": "세게는 아니고",
          "original_form": "세게는 아니고",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031052.1.23",
          "form": "긁힌정도 ㅋㅋ",
          "original_form": "긁힌정도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031052.1.24",
          "form": "에구ㅠㅠ 고생많으세요",
          "original_form": "에구ㅠㅠ 고생많으세요",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031052.1.25",
          "form": "ㅋㅋ 뭐 합의금도 몇번 받아봐서",
          "original_form": "ㅋㅋ 뭐 합의금도 몇번 받아봐서",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031052.1.26",
          "form": "개꿀입니다",
          "original_form": "개꿀입니다",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031052.1.27",
          "form": "합의금이요...?ㅠㅠ",
          "original_form": "합의금이요...?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031052.1.28",
          "form": "과격한가봅니다",
          "original_form": "과격한가봅니다",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031052.1.29",
          "form": "아 욕설이였어요",
          "original_form": "아 욕설이였어요",
          "speaker_id": "1",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031052.1.30",
          "form": "당시에는 화났지만",
          "original_form": "당시에는 화났지만",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031052.1.31",
          "form": "계좌보면 기분 좋아져서",
          "original_form": "계좌보면 기분 좋아져서",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031052.1.32",
          "form": "아 정말요 ㅋㅋ",
          "original_form": "아 정말요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031052.1.33",
          "form": "애구ㅠㅠ",
          "original_form": "애구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100031052.1.34",
          "form": "언제 또 안오나 하고 있습니다",
          "original_form": "언제 또 안오나 하고 있습니다",
          "speaker_id": "1",
          "time": "20210904 17:03"
        }
      ]
