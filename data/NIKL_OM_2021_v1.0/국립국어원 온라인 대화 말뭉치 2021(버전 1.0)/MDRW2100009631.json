      "utterance": [
        {
          "id": "MDRW2100009631.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009631.1.2",
          "form": "박사님이 키운건 어떻게 다른가요",
          "original_form": "박사님이 키운건 어떻게 다른가요",
          "speaker_id": "1",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009631.1.3",
          "form": "그거 듣고",
          "original_form": "그거 듣고",
          "speaker_id": "2",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009631.1.4",
          "form": "자퇴하고 내년에 다시 실과로 들어오고 싶었어요",
          "original_form": "자퇴하고 내년에 다시 실과로 들어오고 싶었어요",
          "speaker_id": "2",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009631.1.5",
          "form": "실하고요",
          "original_form": "실하고요",
          "speaker_id": "2",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009631.1.6",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009631.1.7",
          "form": "전 토마토 먹어봤거든요",
          "original_form": "전 토마토 먹어봤거든요",
          "speaker_id": "2",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009631.1.8",
          "form": "역시 좀 남다른게 있구나",
          "original_form": "역시 좀 남다른게 있구나",
          "speaker_id": "1",
          "time": "20210823 01:57"
        },
        {
          "id": "MDRW2100009631.1.9",
          "form": "교수님이 수업 끝날 때 나눠주셔서",
          "original_form": "교수님이 수업 끝날 때 나눠주셔서",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009631.1.10",
          "form": "싱싱하고",
          "original_form": "싱싱하고",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009631.1.11",
          "form": "맛있고",
          "original_form": "맛있고",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009631.1.12",
          "form": "새로운 종을 키우셨는지",
          "original_form": "새로운 종을 키우셨는지",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009631.1.13",
          "form": "검은 토마토 보셨어요?",
          "original_form": "검은 토마토 보셨어요?",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009631.1.14",
          "form": "그런 거였는데 진짜 맛있었어요",
          "original_form": "그런 거였는데 진짜 맛있었어요",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009631.1.15",
          "form": "대저토마토 말하는건가요",
          "original_form": "대저토마토 말하는건가요",
          "speaker_id": "1",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009631.1.16",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009631.1.17",
          "form": "약간 짭짤한거",
          "original_form": "약간 짭짤한거",
          "speaker_id": "1",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009631.1.18",
          "form": "그런 거 말고",
          "original_form": "그런 거 말고",
          "speaker_id": "2",
          "time": "20210823 01:58"
        },
        {
          "id": "MDRW2100009631.1.19",
          "form": "새로운 품종인데",
          "original_form": "새로운 품종인데",
          "speaker_id": "2",
          "time": "20210823 01:59"
        },
        {
          "id": "MDRW2100009631.1.20",
          "form": "항암에 효과 있고 어쩌구 하는 그런 토마토 있어요",
          "original_form": "항암에 효과 있고 어쩌구 하는 그런 토마토 있어요",
          "speaker_id": "2",
          "time": "20210823 01:59"
        },
        {
          "id": "MDRW2100009631.1.21",
          "form": "검은 토마토 치면 나와요",
          "original_form": "검은 토마토 치면 나와요",
          "speaker_id": "2",
          "time": "20210823 01:59"
        },
        {
          "id": "MDRW2100009631.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:59"
        },
        {
          "id": "MDRW2100009631.1.23",
          "form": "궁금하다 저도 심어보고싶어요",
          "original_form": "궁금하다 저도 심어보고싶어요",
          "speaker_id": "1",
          "time": "20210823 01:59"
        },
        {
          "id": "MDRW2100009631.1.24",
          "form": "검은토마토라니",
          "original_form": "검은토마토라니",
          "speaker_id": "1",
          "time": "20210823 01:59"
        },
        {
          "id": "MDRW2100009631.1.25",
          "form": "학교에서 그런 거 배웠어요",
          "original_form": "학교에서 그런 거 배웠어요",
          "speaker_id": "2",
          "time": "20210823 02:00"
        },
        {
          "id": "MDRW2100009631.1.26",
          "form": "실과 수업에서",
          "original_form": "실과 수업에서",
          "speaker_id": "2",
          "time": "20210823 02:00"
        },
        {
          "id": "MDRW2100009631.1.27",
          "form": "토마토에 생길 수 있는 병 이런 거 외우고",
          "original_form": "토마토에 생길 수 있는 병 이런 거 외우고",
          "speaker_id": "2",
          "time": "20210823 02:00"
        },
        {
          "id": "MDRW2100009631.1.28",
          "form": "농약 쓰는 법",
          "original_form": "농약 쓰는 법",
          "speaker_id": "2",
          "time": "20210823 02:00"
        },
        {
          "id": "MDRW2100009631.1.29",
          "form": "무의 바람들이 현상",
          "original_form": "무의 바람들이 현상",
          "speaker_id": "2",
          "time": "20210823 02:00"
        },
        {
          "id": "MDRW2100009631.1.30",
          "form": "되게 실용적이네요",
          "original_form": "되게 실용적이네요",
          "speaker_id": "1",
          "time": "20210823 02:00"
        },
        {
          "id": "MDRW2100009631.1.31",
          "form": "실과 시간이라서요",
          "original_form": "실과 시간이라서요",
          "speaker_id": "2",
          "time": "20210823 02:00"
        },
        {
          "id": "MDRW2100009631.1.32",
          "form": "그런거 배우는게 좋은거같아요",
          "original_form": "그런거 배우는게 좋은거같아요",
          "speaker_id": "1",
          "time": "20210823 02:01"
        },
        {
          "id": "MDRW2100009631.1.33",
          "form": "약간 중학교 다시 다니는 기분이에요",
          "original_form": "약간 중학교 다시 다니는 기분이에요",
          "speaker_id": "2",
          "time": "20210823 02:01"
        }
      ]
