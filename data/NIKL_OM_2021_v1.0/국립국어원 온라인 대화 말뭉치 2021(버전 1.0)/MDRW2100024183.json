      "utterance": [
        {
          "id": "MDRW2100024183.1.1",
          "form": "건강을 어떻게 챙기시나요",
          "original_form": "건강을 어떻게 챙기시나요",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024183.1.2",
          "form": "걷기 위주라...ㅋㅋ",
          "original_form": "걷기 위주라...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100024183.1.3",
          "form": "게으르네요ㅠ",
          "original_form": "게으르네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 10:01"
        },
        {
          "id": "MDRW2100024183.1.4",
          "form": "저도 매일 1시간 걷기 운동 합니다",
          "original_form": "저도 매일 1시간 걷기 운동 합니다",
          "speaker_id": "1",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100024183.1.5",
          "form": "오 1시간~~~",
          "original_form": "오 1시간~~~",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100024183.1.6",
          "form": "비조금 맞고 1시간 걷고 지금 들어왔어요",
          "original_form": "비조금 맞고 1시간 걷고 지금 들어왔어요",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100024183.1.7",
          "form": "오유 왜 비를ㅠㅠ",
          "original_form": "오유 왜 비를ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100024183.1.8",
          "form": "아주 조금 맞아어요\r\n시원하고 좋아요",
          "original_form": "아주 조금 맞아어요\r\n시원하고 좋아요",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100024183.1.9",
          "form": "ㅋㅋ그럼 다행이군요?",
          "original_form": "ㅋㅋ그럼 다행이군요?",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100024183.1.10",
          "form": "걷는것 말고 또 다른 운동하시나요",
          "original_form": "걷는것 말고 또 다른 운동하시나요",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100024183.1.11",
          "form": "아뇨 귀찮아서 ㅎㅎ",
          "original_form": "아뇨 귀찮아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100024183.1.12",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100024183.1.13",
          "form": "비올때나 걷기 안할때 자전거 타기를 30분정도 해요",
          "original_form": "비올때나 걷기 안할때 자전거 타기를 30분정도 해요",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100024183.1.14",
          "form": "부지런하시네요 와",
          "original_form": "부지런하시네요 와",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100024183.1.15",
          "form": "허리가 조금 아프면서 운동을 열심히 하게 되더라고요",
