      "utterance": [
        {
          "id": "MDRW2100022523.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210829 15:21"
        },
        {
          "id": "MDRW2100022523.1.2",
          "form": "아니요 반려동물 키우세요?",
          "original_form": "아니요 반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210829 15:21"
        },
        {
          "id": "MDRW2100022523.1.3",
          "form": "네 개랑 고양이 있어요",
          "original_form": "네 개랑 고양이 있어요",
          "speaker_id": "1",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100022523.1.4",
          "form": "어머 부지런하시네요",
          "original_form": "어머 부지런하시네요",
          "speaker_id": "2",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022523.1.5",
          "form": "서로 사이 좋은가요?",
          "original_form": "서로 사이 좋은가요?",
          "speaker_id": "2",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100022523.1.6",
          "form": "마당에서 키워요.\r\n개는 고양이한테 가서 친한척 하는데 고양이들이 무서워하고 싫어해요",
          "original_form": "마당에서 키워요.\r\n개는 고양이한테 가서 친한척 하는데 고양이들이 무서워하고 싫어해요",
          "speaker_id": "1",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100022523.1.7",
          "form": "그렇군요 마당이 있다니 동물들에게 참 좋은 환경이네요",
          "original_form": "그렇군요 마당이 있다니 동물들에게 참 좋은 환경이네요",
          "speaker_id": "2",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100022523.1.8",
          "form": "강아지랑 고양이 모습이 눈에 그려지는 듯합니다",
          "original_form": "강아지랑 고양이 모습이 눈에 그려지는 듯합니다",
          "speaker_id": "2",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100022523.1.9",
          "form": "강아지와 고양이의 색다른 매력을 둘 다 느낄 수 있으니 참 좋으시겠어요",
          "original_form": "강아지와 고양이의 색다른 매력을 둘 다 느낄 수 있으니 참 좋으시겠어요",
          "speaker_id": "2",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100022523.1.10",
          "form": "몇살이에요?",
          "original_form": "몇살이에요?",
          "speaker_id": "2",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100022523.1.11",
          "form": "개는 4살 3살\r\n고양이는 3살 2살이요",
          "original_form": "개는 4살 3살\r\n고양이는 3살 2살이요",
          "speaker_id": "1",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100022523.1.12",
          "form": "여러마리를 키우시는군요",
          "original_form": "여러마리를 키우시는군요",
          "speaker_id": "2",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022523.1.13",
          "form": "사료랑 간식이랑 장난감이랑 필요한게 많으시겠어요",
          "original_form": "사료랑 간식이랑 장난감이랑 필요한게 많으시겠어요",
          "speaker_id": "2",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100022523.1.14",
          "form": "네 조금 들긴하죠..\r\n그래도 이쁘게 크는거 보면 넘 좋아요",
          "original_form": "네 조금 들긴하죠..\r\n그래도 이쁘게 크는거 보면 넘 좋아요",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022523.1.15",
          "form": "집안에 웃음이 넘칠 것 같아요",
          "original_form": "집안에 웃음이 넘칠 것 같아요",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100022523.1.16",
          "form": "마당이 있으니 산책도 따로 필요없겠어요",
          "original_form": "마당이 있으니 산책도 따로 필요없겠어요",
          "speaker_id": "2",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100022523.1.17",
          "form": "네 동물을 좋아해서 너무 좋아요\r\n아이들도 좋아하구요",
          "original_form": "네 동물을 좋아해서 너무 좋아요\r\n아이들도 좋아하구요",
          "speaker_id": "1",
          "time": "20210829 17:55"
        },
        {
          "id": "MDRW2100022523.1.18",
          "form": "동물들에게도 사람에게도 ",
          "original_form": "동물들에게도 사람에게도 ",
          "speaker_id": "2",
          "time": "20210829 18:09"
        },
        {
          "id": "MDRW2100022523.1.19",
          "form": "강아지 고양이 이름 여쭤봐도 괜찮을까요?",
          "original_form": "강아지 고양이 이름 여쭤봐도 괜찮을까요?",
          "speaker_id": "2",
          "time": "20210829 18:10"
        },
        {
          "id": "MDRW2100022523.1.20",
          "form": "강아지  name3 name4\r\n고양이 name5 name6이요",
          "original_form": "강아지  &name3& &name4&\r\n고양이 &name5& &name6&이요",
          "speaker_id": "1",
