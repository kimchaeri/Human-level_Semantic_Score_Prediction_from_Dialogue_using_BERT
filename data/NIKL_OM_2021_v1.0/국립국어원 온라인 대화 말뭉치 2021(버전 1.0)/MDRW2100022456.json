        },
        {
          "id": "MDRW2100022456.1.2",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022456.1.3",
          "form": "간단하게 빵 먹었어요",
          "original_form": "간단하게 빵 먹었어요",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022456.1.4",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022456.1.5",
          "form": "전 간장계란밥 먹었어요",
          "original_form": "전 간장계란밥 먹었어요",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022456.1.6",
          "form": "맛있겠네요ㅋㅋ",
          "original_form": "맛있겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022456.1.7",
          "form": "간단하게 해먹기 좋잖아요",
          "original_form": "간단하게 해먹기 좋잖아요",
          "speaker_id": "2",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022456.1.8",
          "form": "ㅋㅋㅋ그렇죠 맛도 있고",
          "original_form": "ㅋㅋㅋ그렇죠 맛도 있고",
          "speaker_id": "1",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022456.1.9",
          "form": "배달 자주 드시나요?",
          "original_form": "배달 자주 드시나요?",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100022456.1.10",
          "form": "네 맨날 먹어요",
          "original_form": "네 맨날 먹어요",
          "speaker_id": "1",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100022456.1.11",
          "form": "자주 먹는 메뉴가 뭐에요?",
          "original_form": "자주 먹는 메뉴가 뭐에요?",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022456.1.12",
          "form": "치킨 아니면 김치찌개예요",
          "original_form": "치킨 아니면 김치찌개예요",
          "speaker_id": "1",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100022456.1.13",
          "form": "저두 치킨 자주 먹어요ㅋㅋ",
          "original_form": "저두 치킨 자주 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022456.1.14",
          "form": "좋아하는 브랜드 있나요?",
          "original_form": "좋아하는 브랜드 있나요?",
          "speaker_id": "2",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100022456.1.15",
          "form": "전 그냥 브랜드 없는 치킨먹어서 ㅋㅋ",
          "original_form": "전 그냥 브랜드 없는 치킨먹어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100022456.1.16",
          "form": "전 요새 노랑통닭 자주 시켜먹어요",
          "original_form": "전 요새 노랑통닭 자주 시켜먹어요",
          "speaker_id": "2",
          "time": "20210829 14:45"
        },
        {
          "id": "MDRW2100022456.1.17",
          "form": "후라이드 맛있더라구요",
          "original_form": "후라이드 맛있더라구요",
          "speaker_id": "2",
          "time": "20210829 14:46"
        },
        {
          "id": "MDRW2100022456.1.18",
