      "utterance": [
        {
          "id": "MDRW2100033989.1.1",
          "form": "꼭 운동화끈을 바꿔주세요ㅋㅋㅋㅋ",
          "original_form": "꼭 운동화끈을 바꿔주세요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100033989.1.2",
          "form": "네 뭘로요?",
          "original_form": "네 뭘로요?",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100033989.1.3",
          "form": "다른 운동화끈으로요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다른 운동화끈으로요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100033989.1.4",
          "form": "이유가있답니다",
          "original_form": "이유가있답니다",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100033989.1.5",
          "form": "아 그럴께요",
          "original_form": "아 그럴께요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100033989.1.6",
          "form": "저 멋진 엄마 되는거죠",
          "original_form": "저 멋진 엄마 되는거죠",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100033989.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 이유는요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 이유는요",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100033989.1.8",
          "form": "그 운동화가 신발끈이",
          "original_form": "그 운동화가 신발끈이",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100033989.1.9",
          "form": "너무잘풀린답니다",
          "original_form": "너무잘풀린답니다",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100033989.1.10",
          "form": "그래서 다른운동화끈으로",
          "original_form": "그래서 다른운동화끈으로",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100033989.1.11",
          "form": "바꿔주면좋아요",
          "original_form": "바꿔주면좋아요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100033989.1.12",
          "form": "아 그래요 빤질빤질 한가봐요",
          "original_form": "아 그래요 빤질빤질 한가봐요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100033989.1.13",
          "form": "꼭바꿀께요",
          "original_form": "꼭바꿀께요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100033989.1.14",
          "form": "ㅋㅋㅋ 진짜그거땜에",
          "original_form": "ㅋㅋㅋ 진짜그거땜에",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100033989.1.15",
          "form": "화병났잖아요",
          "original_form": "화병났잖아요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100033989.1.16",
          "form": "나중에 검색해보니 저같은사람이 한둘이아니더라고요",
          "original_form": "나중에 검색해보니 저같은사람이 한둘이아니더라고요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100033989.1.17",
          "form": "아 그건 너무 해요 ㅡㅡ",
          "original_form": "아 그건 너무 해요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100033989.1.18",
          "form": "ㅋㅋㅋ 걷다가 잘풀리니",
          "original_form": "ㅋㅋㅋ 걷다가 잘풀리니",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100033989.1.19",
          "form": "참고하셔용",
          "original_form": "참고하셔용",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100033989.1.20",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100033989.1.21",
          "form": "새상품이 참 성의 없네요 이뿌던데 끈도 신경쓰지",
          "original_form": "새상품이 참 성의 없네요 이뿌던데 끈도 신경쓰지",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100033989.1.22",
          "form": "이뿌고 짱짱한 끈으로 예쁘게 신으세요 ㅎㅎ",
          "original_form": "이뿌고 짱짱한 끈으로 예쁘게 신으세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100033989.1.23",
          "form": "ㅋㅋㅋ네네 감사합니다",
          "original_form": "ㅋㅋㅋ네네 감사합니다",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100033989.1.24",
          "form": "전 요즘",
          "original_form": "전 요즘",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100033989.1.25",
          "form": "전자기기도사고싶어요",
          "original_form": "전자기기도사고싶어요",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100033989.1.26",
          "form": "맞아요 버즈도 사고싶고 ㅜㅜ",
          "original_form": "맞아요 버즈도 사고싶고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100033989.1.27",
          "form": "딱꽂고 음악듣고 전화받고",
          "original_form": "딱꽂고 음악듣고 전화받고",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100033989.1.28",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100033989.1.29",
          "form": "버즈신제품나온거아시나요?",
          "original_form": "버즈신제품나온거아시나요?",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100033989.1.30",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100033989.1.31",
          "form": "지금 핫딜 카페 ㅋㅋ들러가 봤어요",
          "original_form": "지금 핫딜 카페 ㅋㅋ들러가 봤어요",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100033989.1.32",
          "form": "가끔 싸게 판대요",
          "original_form": "가끔 싸게 판대요",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100033989.1.33",
          "form": "좋나요?",
          "original_form": "좋나요?",
          "speaker_id": "2",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100033989.1.34",
          "form": "지금신제품이",
          "original_form": "지금신제품이",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100033989.1.35",
          "form": "진짜싸게나왔더라고요ㅋㅋㅋㅋ",
          "original_form": "진짜싸게나왔더라고요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100033989.1.36",
          "form": "버즈프로가 아니어서그런지",
          "original_form": "버즈프로가 아니어서그런지",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100033989.1.37",
          "form": "이유가 뭐래요?",
          "original_form": "이유가 뭐래요?",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100033989.1.38",
          "form": "모르겠어요ㄷㄷ",
          "original_form": "모르겠어요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100033989.1.39",
