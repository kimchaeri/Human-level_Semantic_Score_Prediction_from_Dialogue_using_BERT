      "utterance": [
        {
          "id": "MDRW2100042424.1.1",
          "form": "오늘도 비가 엄청오네요",
          "original_form": "오늘도 비가 엄청오네요",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042424.1.2",
          "form": "계신곳도 비오나요?",
          "original_form": "계신곳도 비오나요?",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042424.1.3",
          "form": "여긴 아침에 조금 오고 그쳤어요",
          "original_form": "여긴 아침에 조금 오고 그쳤어요",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042424.1.4",
          "form": "갑자기 가을이 된듯요",
          "original_form": "갑자기 가을이 된듯요",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042424.1.5",
          "form": "선선해졌군요 :)",
          "original_form": "선선해졌군요 :)",
          "speaker_id": "2",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042424.1.6",
          "form": "아침저녁으로 산책 가기 딱인것같아요",
          "original_form": "아침저녁으로 산책 가기 딱인것같아요",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042424.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042424.1.8",
          "form": "더워서 미뤄둔 운동 해야겠어요",
          "original_form": "더워서 미뤄둔 운동 해야겠어요",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042424.1.9",
          "form": "ㅎㅎㅎ아 네 그렇네요",
          "original_form": "ㅎㅎㅎ아 네 그렇네요",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042424.1.10",
          "form": "지금부터 운동하기 좋은 시즌이죠",
          "original_form": "지금부터 운동하기 좋은 시즌이죠",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042424.1.11",
          "form": "눈만 안오면?",
          "original_form": "눈만 안오면?",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042424.1.12",
          "form": "겨울되면 추워서 안하니 지금 열심히 해야돼요ㅋ",
          "original_form": "겨울되면 추워서 안하니 지금 열심히 해야돼요ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042424.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042424.1.14",
          "form": "겨울엔 관절 조심해야하니까..",
          "original_form": "겨울엔 관절 조심해야하니까..",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042424.1.15",
          "form": "ㅎㅎ보통 산책하시면 한시간씩 걸으시나요?",
          "original_form": "ㅎㅎ보통 산책하시면 한시간씩 걸으시나요?",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042424.1.16",
          "form": "네 동네 공원가서 걷고 동네도 걷고해요",
          "original_form": "네 동네 공원가서 걷고 동네도 걷고해요",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042424.1.17",
          "form": "내일 일기예보 보니 비오네요ㅠ",
          "original_form": "내일 일기예보 보니 비오네요ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042424.1.18",
          "form": "아 이런ㅜㅜ가을 장마라더니",
          "original_form": "아 이런ㅜㅜ가을 장마라더니",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042424.1.19",
          "form": "정말 그렇네요",
          "original_form": "정말 그렇네요",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042424.1.20",
          "form": "그러고보면 추석에 과일은 맛이 좀 덜하겠어요 ㅠㅠ",
          "original_form": "그러고보면 추석에 과일은 맛이 좀 덜하겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042424.1.21",
          "form": "그러겠네요ㅠㅠ",
          "original_form": "그러겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042424.1.22",
          "form": "비싸게 팔듯요",
          "original_form": "비싸게 팔듯요",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042424.1.23",
          "form": "비가 많이 와서",
          "original_form": "비가 많이 와서",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042424.1.24",
          "form": "맞아요ㅠㅠ올해 날씨도 너무 더웠어서..",
          "original_form": "맞아요ㅠㅠ올해 날씨도 너무 더웠어서..",
          "speaker_id": "2",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100042424.1.25",
          "form": "밤엔 쌀쌀하네요",
          "original_form": "밤엔 쌀쌀하네요",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100042424.1.26",
          "form": "그러네요 슬슬 감기 조심해야겠어요",
          "original_form": "그러네요 슬슬 감기 조심해야겠어요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100042424.1.27",
          "form": "일교차가 커지니까ㅠㅠ",
          "original_form": "일교차가 커지니까ㅠㅠ",
