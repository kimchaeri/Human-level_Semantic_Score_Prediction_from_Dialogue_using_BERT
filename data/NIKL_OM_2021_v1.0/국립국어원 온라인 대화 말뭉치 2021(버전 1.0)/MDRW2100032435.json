      "utterance": [
        {
          "id": "MDRW2100032435.1.1",
          "form": "저는 그때 그냥 할인 하는 걸로 아무거나 사요",
          "original_form": "저는 그때 그냥 할인 하는 걸로 아무거나 사요",
          "speaker_id": "2",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100032435.1.2",
          "form": "저는 청은차 치약을 씁니다 오래되었어요",
          "original_form": "저는 청은차 치약을 씁니다 오래되었어요",
          "speaker_id": "1",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100032435.1.3",
          "form": "그 치약 진짜 괜찮던데요 순하고",
          "original_form": "그 치약 진짜 괜찮던데요 순하고",
          "speaker_id": "2",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100032435.1.4",
          "form": "죽염같이 향이 강한 치약은 싫어요",
          "original_form": "죽염같이 향이 강한 치약은 싫어요",
          "speaker_id": "1",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100032435.1.5",
          "form": "센소다인도 괜찮아요",
          "original_form": "센소다인도 괜찮아요",
          "speaker_id": "2",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100032435.1.6",
          "form": "센소다인 치약은 한국과 외국것의 성분이 다르다고 하던데요",
          "original_form": "센소다인 치약은 한국과 외국것의 성분이 다르다고 하던데요",
          "speaker_id": "1",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100032435.1.7",
          "form": "그런가요 저는 몰랐어요",
          "original_form": "그런가요 저는 몰랐어요",
          "speaker_id": "2",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100032435.1.8",
          "form": "국내 치약중에서는 청은차가 괜찮아요",
          "original_form": "국내 치약중에서는 청은차가 괜찮아요",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100032435.1.9",
          "form": "저는 치아가 약해서 신경을 많이 씁니다",
          "original_form": "저는 치아가 약해서 신경을 많이 씁니다",
          "speaker_id": "2",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100032435.1.10",
          "form": "치실도 꼭 해야해요",
          "original_form": "치실도 꼭 해야해요",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100032435.1.11",
          "form": "네 워터픽 또 쓰고 있어요",
          "original_form": "네 워터픽 또 쓰고 있어요",
          "speaker_id": "2",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100032435.1.12",
          "form": "꼼꼼하시니 별 일 없을 거에요",
          "original_form": "꼼꼼하시니 별 일 없을 거에요",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100032435.1.13",
          "form": "아직은 치아가 깨끗합니다",
          "original_form": "아직은 치아가 깨끗합니다",
          "speaker_id": "2",
