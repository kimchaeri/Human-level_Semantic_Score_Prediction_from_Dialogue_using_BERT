      "utterance": [
        {
          "id": "MDRW2100027765.1.1",
          "form": "2차 맞으셨나요",
          "original_form": "2차 맞으셨나요",
          "speaker_id": "2",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027765.1.2",
          "form": "네 좀전에요 건강하려고 맞는 백신인데...백신이 무섭네요",
          "original_form": "네 좀전에요 건강하려고 맞는 백신인데...백신이 무섭네요",
          "speaker_id": "1",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027765.1.3",
          "form": "2차는 맞고나서 많이 아프다고 하던데요",
          "original_form": "2차는 맞고나서 많이 아프다고 하던데요",
          "speaker_id": "2",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100027765.1.4",
          "form": "음 아직은 모르겠네요 낼돼봐야알듯",
          "original_form": "음 아직은 모르겠네요 낼돼봐야알듯",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027765.1.5",
          "form": "백신 맞긴 싫은데 저도 맞아야 하네요",
          "original_form": "백신 맞긴 싫은데 저도 맞아야 하네요",
          "speaker_id": "2",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027765.1.6",
          "form": "음 맞아야죠 공익을 위해서요",
          "original_form": "음 맞아야죠 공익을 위해서요",
          "speaker_id": "1",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027765.1.7",
          "form": "넹 그래야죠",
          "original_form": "넹 그래야죠",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027765.1.8",
          "form": "운동은 하시나요",
          "original_form": "운동은 하시나요",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027765.1.9",
          "form": "잘 안해요 살만쪄요ㅜ",
          "original_form": "잘 안해요 살만쪄요ㅜ",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027765.1.10",
          "form": "전 오천보 걷기도 힘들어요",
          "original_form": "전 오천보 걷기도 힘들어요",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027765.1.11",
          "form": "부지런히 걸어야 건강해져요",
          "original_form": "부지런히 걸어야 건강해져요",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027765.1.12",
          "form": "네 산책을 좀 해보려구요",
          "original_form": "네 산책을 좀 해보려구요",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027765.1.13",
          "form": "산책은 정신건강에도 좋아요",
          "original_form": "산책은 정신건강에도 좋아요",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027765.1.14",
          "form": "정신건강에는 스트레스 안받는게 최고예요",
          "original_form": "정신건강에는 스트레스 안받는게 최고예요",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027765.1.15",
          "form": "ㅜㅜ 네 것도그러네요",
          "original_form": "ㅜㅜ 네 것도그러네요",
          "speaker_id": "1",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027765.1.16",
          "form": "패션의 완성은?",
