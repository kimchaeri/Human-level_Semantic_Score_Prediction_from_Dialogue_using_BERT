        {
          "id": "MDRW2100042982.1.6",
          "form": "스벅인가요",
          "original_form": "스벅인가요",
          "speaker_id": "1",
          "time": "20210907 14:16"
        },
        {
          "id": "MDRW2100042982.1.7",
          "form": "아뇨유ㅠ 스벅이너무멀어서 그냥 빠바에서 먹었어염 ㅠㅠ",
