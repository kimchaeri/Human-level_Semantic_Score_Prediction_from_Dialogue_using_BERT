      "utterance": [
        {
          "id": "MDRW2100006478.1.1",
          "form": "거기서 그럼 공짜로 공연 보여줘??",
          "original_form": "거기서 그럼 공짜로 공연 보여줘??",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006478.1.2",
          "form": "아니 원래 못 봐 ㅋㅋㅋㅋㅋ",
          "original_form": "아니 원래 못 봐 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006478.1.3",
          "form": "시설 관리직이었어 그냥",
          "original_form": "시설 관리직이었어 그냥",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006478.1.4",
          "form": "아 ?? 그랭? 본 연예인은 없어? 아니 뮤지컬인 ?",
          "original_form": "아 ?? 그랭? 본 연예인은 없어? 아니 뮤지컬인 ?",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100006478.1.5",
          "form": "아트센터에서는 뮤지컬 배우들만 봤지",
          "original_form": "아트센터에서는 뮤지컬 배우들만 봤지",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100006478.1.6",
          "form": "ㅇㅎㅇㅎ 나는 콘서트 전시회 공연장 알바 해보고 싶어",
          "original_form": "ㅇㅎㅇㅎ 나는 콘서트 전시회 공연장 알바 해보고 싶어",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100006478.1.7",
          "form": "근데 무료로 못본다면 안할래",
          "original_form": "근데 무료로 못본다면 안할래",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006478.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006478.1.9",
          "form": "티켓 예약 혜택이라도 없나",
          "original_form": "티켓 예약 혜택이라도 없나",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006478.1.10",
          "form": "ㅋㅋㅋ안 보여주지 공연장 측이랑 뮤지컬 제작사랑은 다르니까",
          "original_form": "ㅋㅋㅋ안 보여주지 공연장 측이랑 뮤지컬 제작사랑은 다르니까",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006478.1.11",
          "form": "뮤지컬 제작사 쪽으로 가봐",
          "original_form": "뮤지컬 제작사 쪽으로 가봐",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006478.1.12",
          "form": "아아아 그런가?",
          "original_form": "아아아 그런가?",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006478.1.13",
          "form": "진짜  나콘서트 갈때마다 그 관리직들이 부러웠어",
          "original_form": "진짜  나콘서트 갈때마다 그 관리직들이 부러웠어",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100006478.1.14",
          "form": "가까이서 연예인을 볼수있는 특권같아서",
          "original_form": "가까이서 연예인을 볼수있는 특권같아서",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100006478.1.15",
          "form": "위너 콘서트?",
          "original_form": "위너 콘서트?",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100006478.1.16",
          "form": "ㅋㅋㅋㅋ기억난다 올림픽공원이었지?",
          "original_form": "ㅋㅋㅋㅋ기억난다 올림픽공원이었지?",
