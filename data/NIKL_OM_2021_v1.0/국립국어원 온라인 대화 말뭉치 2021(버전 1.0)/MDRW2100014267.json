      "utterance": [
        {
          "id": "MDRW2100014267.1.1",
          "form": "오늘 뭐 먹었엉",
          "original_form": "오늘 뭐 먹었엉",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014267.1.2",
          "form": "오늘음 쭈꾸미볶음을 먹었지",
          "original_form": "오늘음 쭈꾸미볶음을 먹었지",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014267.1.3",
          "form": "너는 뭐먹었어?",
          "original_form": "너는 뭐먹었어?",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014267.1.4",
          "form": "헐 맛있겠다.... 밥비벼먹고싶네",
          "original_form": "헐 맛있겠다.... 밥비벼먹고싶네",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014267.1.5",
          "form": "나 초밥 배달시켜 먹었어",
          "original_form": "나 초밥 배달시켜 먹었어",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014267.1.6",
          "form": "초밥은 사랑이지",
          "original_form": "초밥은 사랑이지",
          "speaker_id": "2",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014267.1.7",
          "form": "주꾸미도 마찬가지여",
          "original_form": "주꾸미도 마찬가지여{emoji:♡}",
          "speaker_id": "1",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014267.1.8",
          "form": "슥슥 비벼서 깻잎에 싸먹고싶네",
          "original_form": "슥슥 비벼서 깻잎에 싸먹고싶네",
          "speaker_id": "1",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014267.1.9",
          "form": "ㅋㅋㅋㅋ 김에 싸먹었지 아주맛나더라고",
          "original_form": "ㅋㅋㅋㅋ 김에 싸먹었지 아주맛나더라고",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014267.1.10",
          "form": "매콤하니 짭짜롬하니",
          "original_form": "매콤하니 짭짜롬하니",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014267.1.11",
          "form": "아주 맛나!!",
          "original_form": "아주 맛나!!",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014267.1.12",
          "form": "아 약올라",
          "original_form": "아 약올라",
          "speaker_id": "1",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014267.1.13",
          "form": "다음에 너도 해먹어",
          "original_form": "다음에 너도 해먹어",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014267.1.14",
          "form": "어제 곱창먹는거 유툽으로 봤음",
          "original_form": "어제 곱창먹는거 유툽으로 봤음",
          "speaker_id": "2",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014267.1.15",
          "form": "아나 곱창.. 오늘 곱창도 얘기했는데 하 막창 곱창",
          "original_form": "아나 곱창.. 오늘 곱창도 얘기했는데 하 막창 곱창",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014267.1.16",
          "form": "너무 맛있어보였어",
          "original_form": "너무 맛있어보였어",
          "speaker_id": "2",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014267.1.17",
          "form": "먹고싶네.. 나중에먹자..꼭",
          "original_form": "먹고싶네.. 나중에먹자..꼭",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014267.1.18",
          "form": "그래 먹자 내가 어딘지도 알아뒀어",
          "original_form": "그래 먹자 내가 어딘지도 알아뒀어",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014267.1.19",
          "form": "아주 단단히 마음먹었구만",
          "original_form": "아주 단단히 마음먹었구만",
          "speaker_id": "1",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014267.1.20",
          "form": "ㅋㅋㅋ 맛있는곳은 주소검색이 필수지",
          "original_form": "ㅋㅋㅋ 맛있는곳은 주소검색이 필수지",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014267.1.21",
          "form": "맛집탐방도 하고싶고만",
          "original_form": "맛집탐방도 하고싶고만",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014267.1.22",
          "form": "세상에 맛있는곳이 너무많아",
          "original_form": "세상에 맛있는곳이 너무많아",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014267.1.23",
          "form": "샤브샤브도 먹고팡",
          "original_form": "샤브샤브도 먹고팡",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014267.1.24",
          "form": "해물샤브샤브 너무좋지 나도 먹고싶다아",
          "original_form": "해물샤브샤브 너무좋지 나도 먹고싶다아",
          "speaker_id": "2",
          "time": "20210825 18:13"
        }
      ]
