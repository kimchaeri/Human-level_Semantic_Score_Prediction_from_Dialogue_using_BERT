      "utterance": [
        {
          "id": "MDRW2100021353.1.1",
          "form": "오늘 저녁 뭐 드실거에요?",
          "original_form": "오늘 저녁 뭐 드실거에요?",
          "speaker_id": "1",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100021353.1.2",
          "form": "모르겠네",
          "original_form": "모르겠네",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021353.1.3",
          "form": "주는데로 먹지여..",
          "original_form": "주는데로 먹지여..",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021353.1.4",
          "form": "택푸는?",
          "original_form": "택푸는?",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100021353.1.5",
          "form": "나도 ㅋㅋㅋㅋ 어제 치킨먹었는데 또 먹고 싶다",
          "original_form": "나도 ㅋㅋㅋㅋ 어제 치킨먹었는데 또 먹고 싶다",
          "speaker_id": "1",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021353.1.6",
          "form": "꿀꺽",
          "original_form": "꿀꺽",
          "speaker_id": "1",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100021353.1.7",
          "form": "게임하니까 시간 빠르다",
          "original_form": "게임하니까 시간 빠르다",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021353.1.8",
          "form": "ㅋㅋㅋㅋ 게임 어떤거 했어?",
          "original_form": "ㅋㅋㅋㅋ 게임 어떤거 했어?",
          "speaker_id": "1",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100021353.1.9",
          "form": "아 식음료네 오늘 점심은 뭘 먹나?",
          "original_form": "아 식음료네 오늘 점심은 뭘 먹나?",
          "speaker_id": "1",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100021353.1.10",
          "form": "글쎄다",
          "original_form": "글쎄다",
          "speaker_id": "2",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100021353.1.11",
          "form": "폐기먹겠쥐..",
          "original_form": "폐기먹겠쥐..",
          "speaker_id": "2",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100021353.1.12",
          "form": "살짝 배고플락말악e",
          "original_form": "살짝 배고플락말악e",
          "speaker_id": "2",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100021353.1.13",
          "form": "아 맞다 편의점 가서 먹겠구나",
          "original_form": "아 맞다 편의점 가서 먹겠구나",
          "speaker_id": "1",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100021353.1.14",
          "form": "넌 뭐 먹냐??",
          "original_form": "넌 뭐 먹냐??",
          "speaker_id": "2",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100021353.1.15",
          "form": "모르겠네 오늘 앞에 마트가서 고기 조금 살까 생각 중",
          "original_form": "모르겠네 오늘 앞에 마트가서 고기 조금 살까 생각 중",
          "speaker_id": "1",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100021353.1.16",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100021353.1.17",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "2",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100021353.1.18",
          "form": "맛있겠다 고기..",
          "original_form": "맛있겠다 고기..",
          "speaker_id": "2",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100021353.1.19",
          "form": "츄베롭",
          "original_form": "츄베롭",
          "speaker_id": "2",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100021353.1.20",
          "form": "너 어제 먹었잖아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "너 어제 먹었잖아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:34"
        },
        {
          "id": "MDRW2100021353.1.21",
          "form": "맞아 고기는 항상 맛있어",
          "original_form": "맞아 고기는 항상 맛있어",
          "speaker_id": "1",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100021353.1.22",
          "form": "엉ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엉ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100021353.1.23",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100021353.1.24",
          "form": "얌긋임",
          "original_form": "얌긋임",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100021353.1.25",
          "form": "폐기 있었으면 좋겠다",
          "original_form": "폐기 있었으면 좋겠다",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100021353.1.26",
          "form": "삼각김밥?",
          "original_form": "삼각김밥?",
          "speaker_id": "1",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100021353.1.27",
          "form": "과자 있던데 그거 먹어야지",
          "original_form": "과자 있던데 그거 먹어야지",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100021353.1.28",
          "form": "오 무슨과자?",
          "original_form": "오 무슨과자?",
          "speaker_id": "1",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100021353.1.29",
          "form": "삼김이든 김밥이든",
          "original_form": "삼김이든 김밥이든",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100021353.1.30",
          "form": "쌀로별",
          "original_form": "쌀로별",
          "speaker_id": "2",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100021353.1.31",
          "form": "완전 얌긋인데",
          "original_form": "완전 얌긋인데",
          "speaker_id": "2",
          "time": "20210829 10:36"
        },
        {
          "id": "MDRW2100021353.1.32",
          "form": "와 오랜만이다 그 과자",
          "original_form": "와 오랜만이다 그 과자",
          "speaker_id": "1",
          "time": "20210829 10:36"
        }
      ]
    }
