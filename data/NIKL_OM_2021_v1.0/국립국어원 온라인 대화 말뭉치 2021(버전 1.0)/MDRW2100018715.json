      "utterance": [
        {
          "id": "MDRW2100018715.1.1",
          "form": "곧 점심시간이네요.",
          "original_form": "곧 점심시간이네요.",
          "speaker_id": "2",
          "time": "20210827 11:19"
        },
        {
          "id": "MDRW2100018715.1.2",
          "form": "점심 뭐 드실 생각이세요?",
          "original_form": "점심 뭐 드실 생각이세요?",
          "speaker_id": "2",
          "time": "20210827 11:19"
        },
        {
          "id": "MDRW2100018715.1.3",
          "form": "돈가스 먹을까  생각중이에요",
          "original_form": "돈가스 먹을까  생각중이에요",
          "speaker_id": "1",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100018715.1.4",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100018715.1.5",
          "form": "저는 콩국수 먹으려고요.",
          "original_form": "저는 콩국수 먹으려고요.",
          "speaker_id": "2",
          "time": "20210827 11:30"
        },
        {
          "id": "MDRW2100018715.1.6",
          "form": "여름 끝이라 곧 안 파실 것 같아서요!",
          "original_form": "여름 끝이라 곧 안 파실 것 같아서요!",
          "speaker_id": "2",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018715.1.7",
          "form": "콩국수 맛있죠ㅎ",
          "original_form": "콩국수 맛있죠ㅎ",
          "speaker_id": "1",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018715.1.8",
          "form": "회사 근처에 콩국수 맛집이 있어요",
          "original_form": "회사 근처에 콩국수 맛집이 있어요",
          "speaker_id": "1",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018715.1.9",
          "form": "얘기 들으니 혹하네요",
          "original_form": "얘기 들으니 혹하네요",
          "speaker_id": "1",
          "time": "20210827 11:31"
        },
        {
          "id": "MDRW2100018715.1.10",
          "form": "저도 돈까스 먹고 싶네요.",
          "original_form": "저도 돈까스 먹고 싶네요.",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018715.1.11",
          "form": "저녁에 시켜 먹어야겠어요.",
          "original_form": "저녁에 시켜 먹어야겠어요.",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018715.1.12",
          "form": "콩국수 설탕 넣어 드세요?",
          "original_form": "콩국수 설탕 넣어 드세요?",
          "speaker_id": "2",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018715.1.13",
          "form": "굳이 하면 소금이요",
          "original_form": "굳이 하면 소금이요",
          "speaker_id": "1",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018715.1.14",
          "form": "설탕 넣어 드시나요?",
          "original_form": "설탕 넣어 드시나요?",
          "speaker_id": "1",
          "time": "20210827 11:35"
        },
        {
          "id": "MDRW2100018715.1.15",
          "form": "이거  지역마다 다르죠?",
          "original_form": "이거  지역마다 다르죠?",
          "speaker_id": "1",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018715.1.16",
          "form": "네네 저는 설탕이요!",
          "original_form": "네네 저는 설탕이요!",
          "speaker_id": "2",
          "time": "20210827 11:38"
        },
        {
          "id": "MDRW2100018715.1.17",
          "form": "소금도 치긴 하는데 설탕맛을 극대화하려고 넣는 정도만 넣어요",
          "original_form": "소금도 치긴 하는데 설탕맛을 극대화하려고 넣는 정도만 넣어요",
          "speaker_id": "2",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018715.1.18",
          "form": "그렇게는 안먹어봐서 궁금해요",
          "original_form": "그렇게는 안먹어봐서 궁금해요",
          "speaker_id": "1",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018715.1.19",
          "form": "지역마다 다르다고 하더라고요.",
          "original_form": "지역마다 다르다고 하더라고요.",
          "speaker_id": "2",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018715.1.20",
          "form": "아 수박에 소금 치듯이요",
          "original_form": "아 수박에 소금 치듯이요",
          "speaker_id": "1",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018715.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018715.1.22",
          "form": "그럼 소금만 넣은 건 안 드셔봤겠네요",
          "original_form": "그럼 소금만 넣은 건 안 드셔봤겠네요",
          "speaker_id": "1",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018715.1.23",
          "form": "식사치곤 좀 많이 달긴 한데",
          "original_form": "식사치곤 좀 많이 달긴 한데",
          "speaker_id": "2",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018715.1.24",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018715.1.25",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018715.1.26",
          "form": "맨날 단 맛에 익숙해져서",
          "original_form": "맨날 단 맛에 익숙해져서",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018715.1.27",
          "form": "ㅎㅎ근처 식당에선 그렇게 안파니",
          "original_form": "ㅎㅎ근처 식당에선 그렇게 안파니",
          "speaker_id": "1",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018715.1.28",
          "form": "소금만 넣을 용기가 아직 없어요",
          "original_form": "소금만 넣을 용기가 아직 없어요",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018715.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그러고 보니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그러고 보니",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018715.1.30",
          "form": "그렇기도 하네요",
          "original_form": "그렇기도 하네요",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018715.1.31",
          "form": "콩물 사다 집에서 해먹어봐야겠네요 설탕으로!",
          "original_form": "콩물 사다 집에서 해먹어봐야겠네요 설탕으로!",
          "speaker_id": "1",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018715.1.32",
          "form": "아 혹시 차이나타운가보셨나요",
          "original_form": "아 혹시 차이나타운가보셨나요",
          "speaker_id": "1",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018715.1.33",
          "form": "아님 중국이나",
          "original_form": "아님 중국이나",
          "speaker_id": "1",
          "time": "20210827 11:41"
        }
      ]
    }
