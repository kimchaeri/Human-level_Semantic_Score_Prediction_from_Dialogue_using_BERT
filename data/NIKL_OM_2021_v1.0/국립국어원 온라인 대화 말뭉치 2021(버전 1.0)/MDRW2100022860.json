      "utterance": [
        {
          "id": "MDRW2100022860.1.1",
          "form": "아 애들 행동 반경이 제한당했네 진짜",
          "original_form": "아 애들 행동 반경이 제한당했네 진짜",
          "speaker_id": "2",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022860.1.2",
          "form": "나쁜 일이 있나",
          "original_form": "나쁜 일이 있나",
          "speaker_id": "2",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100022860.1.3",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "2",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100022860.1.4",
          "form": "??뭐가",
          "original_form": "??뭐가",
          "speaker_id": "1",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100022860.1.5",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100022860.1.6",
          "form": "지들은운동장넓게쓰고",
          "original_form": "지들은운동장넓게쓰고",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022860.1.7",
          "form": "하교길에",
          "original_form": "하교길에",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022860.1.8",
          "form": "겹치는갓도",
          "original_form": "겹치는갓도",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022860.1.9",
          "form": "짜증났는데",
          "original_form": "짜증났는데",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022860.1.10",
          "form": "그냥100m근처에",
          "original_form": "그냥100m근처에",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022860.1.11",
          "form": "ㅣ있다는거부터가",
          "original_form": "ㅣ있다는거부터가",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022860.1.12",
          "form": "짜증남",
          "original_form": "짜증남",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022860.1.13",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022860.1.14",
          "form": "그냥근처에아무것도없는",
          "original_form": "그냥근처에아무것도없는",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022860.1.15",
          "form": "운동장 적은거 진짜",
          "original_form": "운동장 적은거 진짜",
          "speaker_id": "2",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022860.1.16",
          "form": "그니가",
          "original_form": "그니가",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022860.1.17",
          "form": "안드래도",
          "original_form": "안드래도",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022860.1.18",
          "form": "작았는데",
          "original_form": "작았는데",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022860.1.19",
          "form": "남중중학교봤었음?",
          "original_form": "남중중학교봤었음?",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022860.1.20",
          "form": "엄청넓어",
          "original_form": "엄청넓어",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022860.1.21",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022860.1.22",
          "form": "그니가",
          "original_form": "그니가",
          "speaker_id": "2",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022860.1.23",
          "form": "일부러여자학교는다",
          "original_form": "일부러여자학교는다",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022860.1.24",
          "form": "뭐 여자 학생들은 운동 안하니?",
          "original_form": "뭐 여자 학생들은 운동 안하니?",
          "speaker_id": "2",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022860.1.25",
          "form": "운동장작게만다나보ㅓ",
          "original_form": "운동장작게만다나보ㅓ",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022860.1.26",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022860.1.27",
          "form": "다운동했는데",
          "original_form": "다운동했는데",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022860.1.28",
          "form": "애초에적아서불편하기도하고",
          "original_form": "애초에적아서불편하기도하고",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022860.1.29",
          "form": "인원도많았는데",
          "original_form": "인원도많았는데",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022860.1.30",
          "form": "아 ㅇㅈ",
          "original_form": "아 ㅇㅈ",
          "speaker_id": "2",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100022860.1.31",
