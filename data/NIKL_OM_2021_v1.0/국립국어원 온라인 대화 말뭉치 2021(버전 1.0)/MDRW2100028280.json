      "utterance": [
        {
          "id": "MDRW2100028280.1.1",
          "form": "아무튼 꿀잼입니다ㅋㅋ 강추합니다",
          "original_form": "아무튼 꿀잼입니다ㅋㅋ 강추합니다",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028280.1.2",
          "form": "댓글도 재밌다고 하네요 ㅋㅋ",
          "original_form": "댓글도 재밌다고 하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028280.1.3",
          "form": "그거는 보셨나요",
          "original_form": "그거는 보셨나요",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028280.1.4",
          "form": "빨강머리앤?",
          "original_form": "빨강머리앤?",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028280.1.5",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100028280.1.6",
          "form": "다음 시즌 엄청 기다리고 있었는데",
          "original_form": "다음 시즌 엄청 기다리고 있었는데",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028280.1.7",
          "form": "제작 종료했다고 들었어요ㅜㅜ",
          "original_form": "제작 종료했다고 들었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028280.1.8",
          "form": "길버트 배우가 어린데도 정말 잘 생겨서 앞으로가 기대됩니다",
          "original_form": "길버트 배우가 어린데도 정말 잘 생겨서 앞으로가 기대됩니다",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028280.1.9",
          "form": "헐 그럼 이야기가 갑자기 끝난건가요?",
          "original_form": "헐 그럼 이야기가 갑자기 끝난건가요?",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028280.1.10",
          "form": "볼까 했었는데",
          "original_form": "볼까 했었는데",
          "speaker_id": "1",
          "time": "20210902 18:53"
        },
        {
          "id": "MDRW2100028280.1.11",
          "form": "앤의 어린시절이 마무리된 정도에서 끝났어요 ㅜㅜ",
          "original_form": "앤의 어린시절이 마무리된 정도에서 끝났어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 18:54"
        },
        {
          "id": "MDRW2100028280.1.12",
          "form": "ㅠㅠ 늠해요 갑자기 끝나다니",
          "original_form": "ㅠㅠ 늠해요 갑자기 끝나다니",
          "speaker_id": "1",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100028280.1.13",
          "form": "ㅜㅜ 어른의 사정이 있겠지만 배우들 정말 잘해줬는데 아쉽습니다",
          "original_form": "ㅜㅜ 어른의 사정이 있겠지만 배우들 정말 잘해줬는데 아쉽습니다",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028280.1.14",
          "form": "진짜 여자주인공 맡은 배우는 앤 그 자체예요ㅋㅋ",
          "original_form": "진짜 여자주인공 맡은 배우는 앤 그 자체예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028280.1.15",
          "form": "어모 진짜 훈훈해요 어린데",
          "original_form": "어모 진짜 훈훈해요 어린데",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028280.1.16",
          "form": "길버트역 배우 ㅎㅎ",
          "original_form": "길버트역 배우 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028280.1.17",
          "form": "탄생년도 보고 이제 저도 이모팬이되는건가 싶었어요",
          "original_form": "탄생년도 보고 이제 저도 이모팬이되는건가 싶었어요",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028280.1.18",
          "form": "너무 어려서 잘생기면 오빠 드립도 못치겠어요",
          "original_form": "너무 어려서 잘생기면 오빠 드립도 못치겠어요",
          "speaker_id": "2",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028280.1.19",
          "form": "그러게요 ㅎㅎ 철컹철컹이라는",
          "original_form": "그러게요 ㅎㅎ 철컹철컹이라는",
          "speaker_id": "1",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100028280.1.20",
          "form": "그래도 참 보기 좋습니다",
          "original_form": "그래도 참 보기 좋습니다",
          "speaker_id": "2",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100028280.1.21",
          "form": "지금 티비에는 축구를 하네요",
          "original_form": "지금 티비에는 축구를 하네요",
          "speaker_id": "1",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100028280.1.22",
          "form": "아 벌써 경기시간인가요",
          "original_form": "아 벌써 경기시간인가요",
          "speaker_id": "2",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100028280.1.23",
          "form": "국가전이니 저는 나중에 봐야겠어요ㅋㅋ 축구경기 라이브로 보면 우리팀이 지는 징크스가 있어요",
          "original_form": "국가전이니 저는 나중에 봐야겠어요ㅋㅋ 축구경기 라이브로 보면 우리팀이 지는 징크스가 있어요",
          "speaker_id": "2",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100028280.1.24",
          "form": "ㄷㄷ 그럼 시청하기 금지!!",
          "original_form": "ㄷㄷ 그럼 시청하기 금지!!",
          "speaker_id": "1",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100028280.1.25",
          "form": "네ㅋㅋ 결과를 기다렸다가 하이라이트 보겠습니다",
          "original_form": "네ㅋㅋ 결과를 기다렸다가 하이라이트 보겠습니다",
          "speaker_id": "2",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028280.1.26",
          "form": "코로나라 관객석이 텅텅이네요 ㅎㅎ",
          "original_form": "코로나라 관객석이 텅텅이네요 ㅎㅎ",
