      "utterance": [
        {
          "id": "MDRW2100033116.1.1",
          "form": "저는 반려동물이 없지만",
          "original_form": "저는 반려동물이 없지만",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033116.1.2",
          "form": "재밌을것같기 때문에...",
          "original_form": "재밌을것같기 때문에...",
          "speaker_id": "1",
          "time": "20210906 16:47"
        },
        {
          "id": "MDRW2100033116.1.3",
          "form": "나나나 햄스터 키워본적 있어",
          "original_form": "나나나 햄스터 키워본적 있어",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033116.1.4",
          "form": "이름이",
          "original_form": "이름이",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033116.1.5",
          "form": "name3, name4, name5",
          "original_form": "&name3&, &name4&, &name5&",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033116.1.6",
          "form": "였어",
          "original_form": "였어",
          "speaker_id": "2",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100033116.1.7",
          "form": "우와!!!!",
          "original_form": "우와!!!!",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033116.1.8",
          "form": "이름 귀엽ㅋㅋ",
          "original_form": "이름 귀엽ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033116.1.9",
          "form": "지금은 다 햄찌별에 있지만",
          "original_form": "지금은 다 햄찌별에 있지만",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033116.1.10",
          "form": "다들 평균수명보다 오래 살았어",
          "original_form": "다들 평균수명보다 오래 살았어",
          "speaker_id": "2",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033116.1.11",
          "form": "ㅠㅠ name2이가 잘 키웠으니까",
          "original_form": "ㅠㅠ &name2&이가 잘 키웠으니까",
          "speaker_id": "1",
          "time": "20210906 16:50"
        },
        {
          "id": "MDRW2100033116.1.12",
          "form": "잘 살고 있을겁니다",
          "original_form": "잘 살고 있을겁니다",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033116.1.13",
          "form": "난 달팽이 키웠었는데",
          "original_form": "난 달팽이 키웠었는데",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033116.1.14",
          "form": "저도 그렇게 믿고있어요!!!!",
          "original_form": "저도 그렇게 믿고있어요!!!!",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033116.1.15",
          "form": "디따 컸어",
          "original_form": "디따 컸어",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033116.1.16",
          "form": "헉 달팽이?!",
          "original_form": "헉 달팽이?!",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033116.1.17",
          "form": "신기하다!!!!",
          "original_form": "신기하다!!!!",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100033116.1.18",
          "form": "넹!! 흑와달팽이라고 짱큰 달팽이가 있었는데",
          "original_form": "넹!! 흑와달팽이라고 짱큰 달팽이가 있었는데",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100033116.1.19",
          "form": "친구한테 받았단 말이죠.",
          "original_form": "친구한테 받았단 말이죠.",
          "speaker_id": "1",
          "time": "20210906 16:53"
        },
        {
          "id": "MDRW2100033116.1.20",
          "form": "처음엔 관리를 잘 해줬는데 제가 관리를 너무 소홀히 했어요...",
          "original_form": "처음엔 관리를 잘 해줬는데 제가 관리를 너무 소홀히 했어요...",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100033116.1.21",
          "form": "그 이후로는 책임감이 부담스러워서 절대 못 키움 ㄱㅠ",
          "original_form": "그 이후로는 책임감이 부담스러워서 절대 못 키움 ㄱㅠ",
          "speaker_id": "1",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100033116.1.22",
          "form": "반려동물이 책임감이 진짜 중요하긴하지........",
          "original_form": "반려동물이 책임감이 진짜 중요하긴하지........",
          "speaker_id": "2",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100033116.1.23",
          "form": "진짜 1빠따 책임감",
          "original_form": "진짜 1빠따 책임감",
          "speaker_id": "2",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100033116.1.24",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "1",
          "time": "20210906 16:55"
        },
        {
          "id": "MDRW2100033116.1.25",
          "form": "책임감이 없으면 아무리 좋은 용품이 있어도 안되는듯...",
          "original_form": "책임감이 없으면 아무리 좋은 용품이 있어도 안되는듯...",
          "speaker_id": "1",
          "time": "20210906 16:56"
        },
        {
          "id": "MDRW2100033116.1.26",
          "form": "모든 반려동물들이 책임감 있는 사람한테 가서 유기를 안 당햇으면 좋겠다는 마음입니다...",
          "original_form": "모든 반려동물들이 책임감 있는 사람한테 가서 유기를 안 당햇으면 좋겠다는 마음입니다...",
          "speaker_id": "2",
          "time": "20210906 16:57"
        },
        {
          "id": "MDRW2100033116.1.27",
          "form": "마자여ㅠ 책임감 없으면 키우면 안 됨!!!",
          "original_form": "마자여ㅠ 책임감 없으면 키우면 안 됨!!!",
          "speaker_id": "1",
          "time": "20210906 16:58"
        },
        {
          "id": "MDRW2100033116.1.28",
          "form": "모든 동물들이 행복하게 살아야 돼..",
          "original_form": "모든 동물들이 행복하게 살아야 돼..",
          "speaker_id": "1",
