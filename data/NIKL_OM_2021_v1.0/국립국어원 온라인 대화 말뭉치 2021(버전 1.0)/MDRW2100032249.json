      "utterance": [
        {
          "id": "MDRW2100032249.1.1",
          "form": "저는 군인을 사귀어 본 적 없는데",
          "original_form": "저는 군인을 사귀어 본 적 없는데",
          "speaker_id": "2",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032249.1.2",
          "form": "이제 제 사촌 동생 중 셋째가 군대 갈 예정입니다",
          "original_form": "이제 제 사촌 동생 중 셋째가 군대 갈 예정입니다",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032249.1.3",
          "form": "내년에 갈 것 같네요",
          "original_form": "내년에 갈 것 같네요",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100032249.1.4",
          "form": "예전에 위문편지 보낸 거 기억나요",
          "original_form": "예전에 위문편지 보낸 거 기억나요",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.5",
          "form": "군인은 사귀면 안 돼요",
          "original_form": "군인은 사귀면 안 돼요",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.6",
          "form": "지금 생각해 보면 왜 그랬나 모르겠어요",
          "original_form": "지금 생각해 보면 왜 그랬나 모르겠어요",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.7",
          "form": "맞아요 요즘에는 온라인 사이트에 쓰면 바로 이렇게 해 주기도 하지만",
          "original_form": "맞아요 요즘에는 온라인 사이트에 쓰면 바로 이렇게 해 주기도 하지만",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.8",
          "form": "위문 공연도 되게 웃기지 않나요",
          "original_form": "위문 공연도 되게 웃기지 않나요",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.9",
          "form": "카카오톡 되거든요 편지를게 따로 없어요",
          "original_form": "카카오톡 되거든요 편지를게 따로 없어요",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.10",
          "form": "여자들이 춤추고 그런 거",
          "original_form": "여자들이 춤추고 그런 거",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.11",
          "form": "그러니까 말이에요 저질 같아요",
          "original_form": "그러니까 말이에요 저질 같아요",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.12",
          "form": "이게 이상하고 쓸데없는 짓이죠",
          "original_form": "이게 이상하고 쓸데없는 짓이죠",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.13",
          "form": "여군도 있는데 남자들이 그러면 춤 안 쳐 주나",
          "original_form": "여군도 있는데 남자들이 그러면 춤 안 쳐 주나",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.14",
          "form": "요즘도 그런다고 합니다",
          "original_form": "요즘도 그런다고 합니다",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.15",
          "form": "여자가 왜 그래야 할까요",
          "original_form": "여자가 왜 그래야 할까요",
          "speaker_id": "2",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.16",
          "form": "이상한 거 배우는 문화를 만들고 있네요",
          "original_form": "이상한 거 배우는 문화를 만들고 있네요",
          "speaker_id": "1",
          "time": "20210905 14:29"
        },
        {
          "id": "MDRW2100032249.1.17",
          "form": "군통령이라는 말도 그래서 싫어요",
          "original_form": "군통령이라는 말도 그래서 싫어요",
