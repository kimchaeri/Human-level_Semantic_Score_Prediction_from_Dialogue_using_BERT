        },
        {
          "id": "MDRW2100022414.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022414.1.3",
          "form": "반려동물 있으신가요?",
          "original_form": "반려동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022414.1.4",
          "form": "네네 고양이 키워요",
          "original_form": "네네 고양이 키워요",
          "speaker_id": "1",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022414.1.5",
          "form": "아 고양이가 대세인가봐요",
          "original_form": "아 고양이가 대세인가봐요",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022414.1.6",
          "form": "전 없어요",
          "original_form": "전 없어요",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022414.1.7",
          "form": "전 고양이 키운지 10년은 넘어서",
          "original_form": "전 고양이 키운지 10년은 넘어서",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022414.1.8",
          "form": "와 정말 오래 키우셨네요",
          "original_form": "와 정말 오래 키우셨네요",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022414.1.9",
          "form": "네네 털만 안빠지면 참 좋은데 ㅜ",
          "original_form": "네네 털만 안빠지면 참 좋은데 ㅜ",
          "speaker_id": "1",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022414.1.10",
          "form": "털이 많이 빠지나요?",
          "original_form": "털이 많이 빠지나요?",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022414.1.11",
          "form": "진짜 털 많이빠져요",
          "original_form": "진짜 털 많이빠져요",
          "speaker_id": "1",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022414.1.12",
          "form": "주변에 개 키우는 사람이 대부분이라서요 고양이도 그렇군요",
          "original_form": "주변에 개 키우는 사람이 대부분이라서요 고양이도 그렇군요",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022414.1.13",
          "form": "저 작년에 반려동물 단속요원했었는데",
          "original_form": "저 작년에 반려동물 단속요원했었는데",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022414.1.14",
          "form": "반려동물 단속요원은 무슨 일을 하나요?",
          "original_form": "반려동물 단속요원은 무슨 일을 하나요?",
          "speaker_id": "2",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022414.1.15",
          "form": "그 강아지들 산책나올때 등록했는지 확인하는거랑 배변처리 목줄 맹견입마개 등등 단속해요",
          "original_form": "그 강아지들 산책나올때 등록했는지 확인하는거랑 배변처리 목줄 맹견입마개 등등 단속해요",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022414.1.16",
          "form": "오 꼭 필요한 일이네요  목줄 안한 개들 너무 많아요",
          "original_form": "오 꼭 필요한 일이네요  목줄 안한 개들 너무 많아요",
          "speaker_id": "2",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100022414.1.17",
          "form": "ㄹㅇ진짜 엄청 많더라구요 배변처리 안하시는 분들도 많고 진짜 그런분들 때문에 다른 사람들도 다 고통받고",
          "original_form": "ㄹㅇ진짜 엄청 많더라구요 배변처리 안하시는 분들도 많고 진짜 그런분들 때문에 다른 사람들도 다 고통받고",
          "speaker_id": "1",
          "time": "20210829 13:55"
        }
      ]
