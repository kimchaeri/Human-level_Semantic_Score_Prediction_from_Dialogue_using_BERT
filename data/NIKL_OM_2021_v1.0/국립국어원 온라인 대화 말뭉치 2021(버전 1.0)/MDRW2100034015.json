      "utterance": [
        {
          "id": "MDRW2100034015.1.1",
          "form": "게임 뭐 하시는거 있나요?",
          "original_form": "게임 뭐 하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100034015.1.2",
          "form": "지금 로드오브히어로즈 하고있어요 아시나요",
          "original_form": "지금 로드오브히어로즈 하고있어요 아시나요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100034015.1.3",
          "form": "첨 들어봤어요 pc게임인거죠?",
          "original_form": "첨 들어봤어요 pc게임인거죠?",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100034015.1.4",
          "form": "포켓몬고는 아시나요",
          "original_form": "포켓몬고는 아시나요",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100034015.1.5",
          "form": "네 예전에 동생이 하는거 옆에서 봤어요",
          "original_form": "네 예전에 동생이 하는거 옆에서 봤어요",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100034015.1.6",
          "form": "ㅋㅋ이게 참 붐이었는데 유행이 가버렸네요",
          "original_form": "ㅋㅋ이게 참 붐이었는데 유행이 가버렸네요",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100034015.1.7",
          "form": "그러게요 ㅋㅋㅋ 진짜 그 당시는 그거땜에 막 다치는 사람도 있고 그랬던 거 같은데..",
          "original_form": "그러게요 ㅋㅋㅋ 진짜 그 당시는 그거땜에 막 다치는 사람도 있고 그랬던 거 같은데..",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100034015.1.8",
          "form": "아 그게 앞을안보니까 그런거 같아요",
          "original_form": "아 그게 앞을안보니까 그런거 같아요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100034015.1.9",
          "form": "위험할수있어요",
          "original_form": "위험할수있어요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100034015.1.10",
          "form": "아 그쵸.. 한번도 안 가본데에도 가고 그러다보니..",
          "original_form": "아 그쵸.. 한번도 안 가본데에도 가고 그러다보니..",
          "speaker_id": "1",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100034015.1.11",
          "form": "나름 모으는재미가있는데 이제 귀찮네요ㅎㅎ",
          "original_form": "나름 모으는재미가있는데 이제 귀찮네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100034015.1.12",
          "form": "저는 모바일게임만 해서 ㅋㅋㅋ 꿈의 집 아시나요",
          "original_form": "저는 모바일게임만 해서 ㅋㅋㅋ 꿈의 집 아시나요",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100034015.1.13",
          "form": "광고본거같아요 퍼즐탈출겜 같은건가요?",
          "original_form": "광고본거같아요 퍼즐탈출겜 같은건가요?",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100034015.1.14",
          "form": "아 그 퍼즐탈출은 거의 낚시예요 ㅋㅋㅋ",
          "original_form": "아 그 퍼즐탈출은 거의 낚시예요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100034015.1.15",
          "form": "슈가크러쉬 게임 아시나요 거의 그런 게임이에요 그냥 판 깨는 ㅋㅋ",
          "original_form": "슈가크러쉬 게임 아시나요 거의 그런 게임이에요 그냥 판 깨는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100034015.1.16",
          "form": "ㅋㅋㅋ 그건 모르는겜인데 프랜즈팝콘처럼 판깨고 넘어가는건가요",
          "original_form": "ㅋㅋㅋ 그건 모르는겜인데 프랜즈팝콘처럼 판깨고 넘어가는건가요",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100034015.1.17",
          "form": "네 그냥 같은 그림끼리 막 이어서 판깨고 그러는..",
          "original_form": "네 그냥 같은 그림끼리 막 이어서 판깨고 그러는..",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100034015.1.18",
          "form": "저는 경쟁하는 게임은 못해요 ㅋㅋ",
          "original_form": "저는 경쟁하는 게임은 못해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100034015.1.19",
          "form": "ㄱㅋ 저도 순발력떨어져서ㅜㅜ",
          "original_form": "ㄱㅋ 저도 순발력떨어져서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100034015.1.20",
          "form": "개미처럼하는걸좋아해요",
          "original_form": "개미처럼하는걸좋아해요",
