      "utterance": [
        {
          "id": "MDRW2100040415.1.1",
          "form": "오 감사감사!",
          "original_form": "오 감사감사!",
          "speaker_id": "2",
          "time": "20210904 20:00"
        },
        {
          "id": "MDRW2100040415.1.2",
          "form": "백신맞은곳은어때요..!!!!!",
          "original_form": "백신맞은곳은어때요..!!!!!",
          "speaker_id": "2",
          "time": "20210904 20:00"
        },
        {
          "id": "MDRW2100040415.1.3",
          "form": "견딜만한가요오!!!!",
          "original_form": "견딜만한가요오!!!!",
          "speaker_id": "2",
          "time": "20210904 20:00"
        },
        {
          "id": "MDRW2100040415.1.4",
          "form": "이제 10시간 지낫는데",
          "original_form": "이제 10시간 지낫는데",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100040415.1.5",
          "form": "아직 괜찮은더같아요",
          "original_form": "아직 괜찮은더같아요",
          "speaker_id": "1",
          "time": "20210904 20:09"
        },
        {
          "id": "MDRW2100040415.1.6",
          "form": "다행이예요 약준비해놨어요?",
          "original_form": "다행이예요 약준비해놨어요?",
          "speaker_id": "2",
          "time": "20210904 20:10"
        },
        {
          "id": "MDRW2100040415.1.7",
          "form": "아까 하나 먹긴했어용",
          "original_form": "아까 하나 먹긴했어용",
          "speaker_id": "1",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100040415.1.8",
          "form": "9시간부터 증상 나타낫다해서 긴장햇는데",
          "original_form": "9시간부터 증상 나타낫다해서 긴장햇는데",
          "speaker_id": "1",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100040415.1.9",
          "form": "아직 괜찮네용",
          "original_form": "아직 괜찮네용",
          "speaker_id": "1",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100040415.1.10",
          "form": "무난하게 넘어갈건가봐요",
          "original_form": "무난하게 넘어갈건가봐요",
          "speaker_id": "2",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100040415.1.11",
          "form": "좋아요좋아",
          "original_form": "좋아요좋아",
          "speaker_id": "2",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100040415.1.12",
          "form": "대화쳐도돼요?무리하는거아닙니꽉",
          "original_form": "대화쳐도돼요?무리하는거아닙니꽉",
          "speaker_id": "2",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100040415.1.13",
          "form": "넹 구냥 오른쪽으로 쳐요",
          "original_form": "넹 구냥 오른쪽으로 쳐요",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100040415.1.14",
          "form": "ㄱㄲㄱㅋㅋㅋ",
          "original_form": "ㄱㄲㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100040415.1.15",
          "form": "오 한손스킬",
          "original_form": "오 한손스킬",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100040415.1.16",
          "form": "내일까지도 산책안하구쉬는게낫겟지?",
          "original_form": "내일까지도 산책안하구쉬는게낫겟지?",
          "speaker_id": "2",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100040415.1.17",
          "form": "백신맞음",
          "original_form": "백신맞음",
          "speaker_id": "2",
          "time": "20210904 20:42"
        },
        {
          "id": "MDRW2100040415.1.18",
          "form": "넹넹 아마 내일 더 아플거 같은",
          "original_form": "넹넹 아마 내일 더 아플거 같은",
          "speaker_id": "1",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100040415.1.19",
          "form": "아 ..  백신진짜맞기싫어지네요",
          "original_form": "아 ..  백신진짜맞기싫어지네요",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100040415.1.20",
          "form": "내일더아플거같다니",
          "original_form": "내일더아플거같다니",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100040415.1.21",
          "form": "ㅋㅋㅋㅋ근데 또 모르겟어요 쌤들중에 그냥 괜찮게 지나거시는 분들도 잇엇어여",
          "original_form": "ㅋㅋㅋㅋ근데 또 모르겟어요 쌤들중에 그냥 괜찮게 지나거시는 분들도 잇엇어여",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100040415.1.22",
          "form": "하나도안아프고싶어요  흑 주사자체도너무싫은데ㅜㅠ",
          "original_form": "하나도안아프고싶어요  흑 주사자체도너무싫은데ㅜㅠ",
