      "utterance": [
        {
          "id": "MDRW2100022265.1.1",
          "form": "그 다담에",
          "original_form": "그 다담에",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022265.1.2",
          "form": "다담 식자재?",
          "original_form": "다담 식자재?",
          "speaker_id": "1",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022265.1.3",
          "form": "찜닭소스있거든요",
          "original_form": "찜닭소스있거든요",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022265.1.4",
          "form": "어 본 것 같아요",
          "original_form": "어 본 것 같아요",
          "speaker_id": "1",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022265.1.5",
          "form": "통으로 든거요?",
          "original_form": "통으로 든거요?",
          "speaker_id": "1",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022265.1.6",
          "form": "씨제이에서 파는 소스",
          "original_form": "씨제이에서 파는 소스",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022265.1.7",
          "form": "그 파우치로된거",
          "original_form": "그 파우치로된거",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.8",
          "form": "아~ 씨제이에 그런 것도 팔구나",
          "original_form": "아~ 씨제이에 그런 것도 팔구나",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.9",
          "form": "그거 하나 사다가",
          "original_form": "그거 하나 사다가",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.10",
          "form": "그거 하나 사면 몇인분 만들 수 있어요?",
          "original_form": "그거 하나 사면 몇인분 만들 수 있어요?",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.11",
          "form": "설탕 때려붓고 졸여보세요",
          "original_form": "설탕 때려붓고 졸여보세요",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.12",
          "form": "그거 닭 1마리 용량 소스요",
          "original_form": "그거 닭 1마리 용량 소스요",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.13",
          "form": "아 설탕도 같이 넣어야해요? ㅠㅠ",
          "original_form": "아 설탕도 같이 넣어야해요? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.14",
          "form": "안넣어도 되긴하는데",
          "original_form": "안넣어도 되긴하는데",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.15",
          "form": "저는 좀 매워서",
          "original_form": "저는 좀 매워서",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.16",
          "form": "그럼 굽네에 그 뭐냐",
          "original_form": "그럼 굽네에 그 뭐냐",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.17",
          "form": "앗 저 매운거 잘못먹는데 ㅜㅜ",
          "original_form": "앗 저 매운거 잘못먹는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.18",
          "form": "갈비천왕맛나요",
          "original_form": "갈비천왕맛나요",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.19",
          "form": "헐 진짜요? 대박 ㅋㅋㅋㅋ",
          "original_form": "헐 진짜요? 대박 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.20",
          "form": "겁나 매운건아니고",
          "original_form": "겁나 매운건아니고",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022265.1.21",
          "form": "혹시 이 소스 겉에만 바르고 에어프라이기 넣으면",
          "original_form": "혹시 이 소스 겉에만 바르고 에어프라이기 넣으면",
          "speaker_id": "1",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022265.1.22",
          "form": "약간 매콤핫정도",
          "original_form": "약간 매콤핫정도",
          "speaker_id": "2",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022265.1.23",
          "form": "갈비천왕 될까요?",
          "original_form": "갈비천왕 될까요?",
          "speaker_id": "1",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022265.1.24",
          "form": "오 매콤하다니 기대가 되네요!",
          "original_form": "오 매콤하다니 기대가 되네요!",
          "speaker_id": "1",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022265.1.25",
          "form": "좀 짜지않을까요",
          "original_form": "좀 짜지않을까요",
          "speaker_id": "2",
          "time": "20210829 11:51"
        }
      ]
    }
