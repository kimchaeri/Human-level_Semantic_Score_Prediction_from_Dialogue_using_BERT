      "utterance": [
        {
          "id": "MDRW2100002231.1.1",
          "form": "커뮤니티 하는거 있어?",
          "original_form": "커뮤니티 하는거 있어?",
          "speaker_id": "2",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002231.1.2",
          "form": "나는 여러개 해",
          "original_form": "나는 여러개 해",
          "speaker_id": "2",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002231.1.3",
          "form": "어떤거 하는데?",
          "original_form": "어떤거 하는데?",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002231.1.4",
          "form": "말하기 부끄러워서",
          "original_form": "말하기 부끄러워서",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002231.1.5",
          "form": "말하긴 좀",
          "original_form": "말하긴 좀",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002231.1.6",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002231.1.7",
          "form": "나도 하긴 해",
          "original_form": "나도 하긴 해",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002231.1.8",
          "form": "ㅋㅋ 대신에 커뮤니티 주제는 알려줄 수 있어",
          "original_form": "ㅋㅋ 대신에 커뮤니티 주제는 알려줄 수 있어",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002231.1.9",
          "form": "보통 웹소설 커뮤니티 보고",
          "original_form": "보통 웹소설 커뮤니티 보고",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002231.1.10",
          "form": "뭔데?",
          "original_form": "뭔데?",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002231.1.11",
          "form": "그 뭐지",
          "original_form": "그 뭐지",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002231.1.12",
          "form": "부동산 커뮤니티도 봐",
          "original_form": "부동산 커뮤니티도 봐",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002231.1.13",
          "form": "너는 뭐 봐?",
          "original_form": "너는 뭐 봐?",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002231.1.14",
          "form": "나는 덕질...",
          "original_form": "나는 덕질...",
          "speaker_id": "1",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002231.1.15",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002231.1.16",
          "form": "덕질 좋지",
          "original_form": "덕질 좋지",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002231.1.17",
          "form": "응 정말 비교되네",
          "original_form": "응 정말 비교되네",
          "speaker_id": "1",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002231.1.18",
          "form": "같은 커뮤니티에서",
          "original_form": "같은 커뮤니티에서",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002231.1.19",
          "form": "덕질 친구들 만나는거",
          "original_form": "덕질 친구들 만나는거",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002231.1.20",
          "form": "참 즐거운듯",
          "original_form": "참 즐거운듯",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002231.1.21",
          "form": "맞아 그냥 구경만 해도 재밌더라",
          "original_form": "맞아 그냥 구경만 해도 재밌더라",
          "speaker_id": "1",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002231.1.22",
          "form": "같은 관심사니까",
          "original_form": "같은 관심사니까",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002231.1.23",
          "form": "다른 사람들 주접떠는거 보면",
          "original_form": "다른 사람들 주접떠는거 보면",
          "speaker_id": "2",
          "time": "20210820 01:00"
        },
        {
          "id": "MDRW2100002231.1.24",
          "form": "나도 웃게 돼",
          "original_form": "나도 웃게 돼",
          "speaker_id": "2",
          "time": "20210820 01:01"
        },
        {
          "id": "MDRW2100002231.1.25",
          "form": "맞아 정말 참신한 주접들 많더라",
          "original_form": "맞아 정말 참신한 주접들 많더라",
          "speaker_id": "1",
          "time": "20210820 01:01"
        }
