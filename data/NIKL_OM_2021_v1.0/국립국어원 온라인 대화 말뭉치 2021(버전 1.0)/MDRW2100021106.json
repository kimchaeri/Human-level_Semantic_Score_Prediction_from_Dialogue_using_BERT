      "utterance": [
        {
          "id": "MDRW2100021106.1.1",
          "form": "너희 동네도 오늘 비 왔니?",
          "original_form": "너희 동네도 오늘 비 왔니?",
          "speaker_id": "1",
          "time": "20210828 15:56"
        },
        {
          "id": "MDRW2100021106.1.2",
          "form": "아니 하루종일 흐리긴 한데 아직 안왔어 거긴?",
          "original_form": "아니 하루종일 흐리긴 한데 아직 안왔어 거긴?",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100021106.1.3",
          "form": "빗방울 좀 떨어졌는데 또 안오네 오늘 비가 오긴 하겠지??",
          "original_form": "빗방울 좀 떨어졌는데 또 안오네 오늘 비가 오긴 하겠지??",
          "speaker_id": "1",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100021106.1.4",
          "form": "요새 비가 많이오네 우산 항상 가지고다녀야겠어",
          "original_form": "요새 비가 많이오네 우산 항상 가지고다녀야겠어",
          "speaker_id": "2",
          "time": "20210901 14:36"
        },
        {
          "id": "MDRW2100021106.1.5",
          "form": "귀찮긴하지만 비 안맞으려면 ㅎㅎ",
          "original_form": "귀찮긴하지만 비 안맞으려면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100021106.1.6",
          "form": "그러게 어떤 색 우산 들고다니니",
          "original_form": "그러게 어떤 색 우산 들고다니니",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100021106.1.7",
          "form": "나는 요새 우양산 들고다녀 너는?",
          "original_form": "나는 요새 우양산 들고다녀 너는?",
          "speaker_id": "2",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100021106.1.8",
          "form": "흰색",
          "original_form": "흰색",
          "speaker_id": "2",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100021106.1.9",
          "form": "나는 그때그때 달라 비가 적게 오면 우양산이 편하지",
          "original_form": "나는 그때그때 달라 비가 적게 오면 우양산이 편하지",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100021106.1.10",
          "form": "무슨색 들고다녀?",
          "original_form": "무슨색 들고다녀?",
          "speaker_id": "2",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100021106.1.11",
          "form": "나는 검은 색 자주 들고 다녀",
          "original_form": "나는 검은 색 자주 들고 다녀",
          "speaker_id": "1",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100021106.1.12",
          "form": "음 검은색이 깔끔하지 근데 비올땐 밝은색이 좋대",
          "original_form": "음 검은색이 깔끔하지 근데 비올땐 밝은색이 좋대",
          "speaker_id": "2",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100021106.1.13",
          "form": "아 그래>? 이유가 뭐래?",
          "original_form": "아 그래>? 이유가 뭐래?",
          "speaker_id": "1",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100021106.1.14",
          "form": "어두우니까 잘 안보여서 운전자들이 잘 보려면 밝은색이 낫대 ㅋㅋ",
          "original_form": "어두우니까 잘 안보여서 운전자들이 잘 보려면 밝은색이 낫대 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100021106.1.15",
          "form": "좋운 정보 고마워~",
          "original_form": "좋운 정보 고마워~",
          "speaker_id": "1",
          "time": "20210901 14:47"
        }
