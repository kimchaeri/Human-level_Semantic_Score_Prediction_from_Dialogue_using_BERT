      "utterance": [
        {
          "id": "MDRW2100008288.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008288.1.2",
          "form": "아니요 학생이신가요?",
          "original_form": "아니요 학생이신가요?",
          "speaker_id": "2",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008288.1.3",
          "form": "졸업했어요!",
          "original_form": "졸업했어요!",
          "speaker_id": "1",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008288.1.4",
          "form": "전공은 어떤걸 하셨나요?",
          "original_form": "전공은 어떤걸 하셨나요?",
          "speaker_id": "2",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008288.1.5",
          "form": "생명공학쪽이였어요",
          "original_form": "생명공학쪽이였어요",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008288.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008288.1.7",
          "form": "생명공학이라니 멋진데요?",
          "original_form": "생명공학이라니 멋진데요?",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008288.1.8",
          "form": "저는 조리쪽이요",
          "original_form": "저는 조리쪽이요",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008288.1.9",
          "form": "생명공학은 어떤걸 배우나요?",
          "original_form": "생명공학은 어떤걸 배우나요?",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008288.1.10",
          "form": "생태 질환 병 치료법 같은걸 주로 배워요",
          "original_form": "생태 질환 병 치료법 같은걸 주로 배워요",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008288.1.11",
          "form": "dna을 주로 배웠구요",
          "original_form": "dna을 주로 배웠구요",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008288.1.12",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008288.1.13",
          "form": "조리쪽이시면 자격증따신다고 고생하셨겠다",
          "original_form": "조리쪽이시면 자격증따신다고 고생하셨겠다",
          "speaker_id": "1",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008288.1.14",
          "form": "약간 인간의 삶이랑 연관되어있네요",
          "original_form": "약간 인간의 삶이랑 연관되어있네요",
          "speaker_id": "2",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008288.1.15",
          "form": "자격증 맞아요..",
          "original_form": "자격증 맞아요..",
          "speaker_id": "2",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008288.1.16",
          "form": "힘들었어요ㅠㅠ",
          "original_form": "힘들었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008288.1.17",
          "form": "분야도 여러가지여서.. 매번 배울 때 마다 헷갈리더라구요",
          "original_form": "분야도 여러가지여서.. 매번 배울 때 마다 헷갈리더라구요",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008288.1.18",
          "form": "실습수업도 많이 하셨나요?",
          "original_form": "실습수업도 많이 하셨나요?",
          "speaker_id": "1",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008288.1.19",
          "form": "아무래도요? 요리를 할 줄 알아야하니까 불앞에 많이 있었어요ㅎㅎ",
          "original_form": "아무래도요? 요리를 할 줄 알아야하니까 불앞에 많이 있었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008288.1.20",
          "form": "코시국이라 실습이 더욱 힘들어지진 않았나요?",
          "original_form": "코시국이라 실습이 더욱 힘들어지진 않았나요?",
          "speaker_id": "1",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008288.1.21",
          "form": "마스크 껴야 하는게 힘들긴 했어요ㅠㅠ",
