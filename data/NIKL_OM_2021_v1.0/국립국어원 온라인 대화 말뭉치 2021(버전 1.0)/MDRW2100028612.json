        {
          "id": "MDRW2100028612.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028612.1.2",
          "form": "먹었어요! name1은요?",
          "original_form": "먹었어요! &name1&은요?",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028612.1.3",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210902 22:30"
        },
        {
          "id": "MDRW2100028612.1.4",
          "form": "저는 불고기 요",
          "original_form": "저는 불고기 요",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100028612.1.5",
          "form": "무슨 치킨 드셨나요?",
          "original_form": "무슨 치킨 드셨나요?",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100028612.1.6",
          "form": "네네요",
          "original_form": "네네요",
          "speaker_id": "1",
          "time": "20210903 07:22"
        },
        {
          "id": "MDRW2100028612.1.7",
          "form": "네네 치킨 뭔가 특징있나요?",
          "original_form": "네네 치킨 뭔가 특징있나요?",
          "speaker_id": "2",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100028612.1.8",
          "form": "후라이드 맛나오",
          "original_form": "후라이드 맛나오",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100028612.1.9",
          "form": "저는 구운 치킨을 좋아해서 굽네치킨을 주로 시켜먹어요 ㅎㅎ",
          "original_form": "저는 구운 치킨을 좋아해서 굽네치킨을 주로 시켜먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100028612.1.10",
          "form": "치킨 먹을때 콜라 사이다 둘중에 어떤 거 고르시나요?",
          "original_form": "치킨 먹을때 콜라 사이다 둘중에 어떤 거 고르시나요?",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100028612.1.11",
          "form": "사이다요",
          "original_form": "사이다요",
          "speaker_id": "1",
          "time": "20210904 08:36"
        },
        {
          "id": "MDRW2100028612.1.12",
          "form": "칠성사이다 스프라이트 둘중에는요?!",
          "original_form": "칠성사이다 스프라이트 둘중에는요?!",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100028612.1.13",
          "form": "칠성잉죠",
          "original_form": "칠성잉죠",
          "speaker_id": "1",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100028612.1.14",
          "form": "저는 아까 간식으로 라면 먹었어요",
          "original_form": "저는 아까 간식으로 라면 먹었어요",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100028612.1.15",
          "form": "무슨 라면 좋아하세요?",
          "original_form": "무슨 라면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 16:58"
