        },
        {
          "id": "MDRW2100036897.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100036897.1.3",
          "form": "여행 좋아하세요",
          "original_form": "여행 좋아하세요",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100036897.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100036897.1.5",
          "form": "네 완전좋아해요 지금은 못가지만ㅜ",
          "original_form": "네 완전좋아해요 지금은 못가지만ㅜ",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100036897.1.6",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100036897.1.7",
          "form": "저도 그렇답니다ㅠ",
          "original_form": "저도 그렇답니다ㅠ",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100036897.1.8",
          "form": "어디어디가셨어요",
          "original_form": "어디어디가셨어요",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100036897.1.9",
          "form": "코로나가 잠잠해지기만을 기다립니다ㅠ",
          "original_form": "코로나가 잠잠해지기만을 기다립니다ㅠ",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100036897.1.10",
          "form": "전국내는 거의 가 본 거 같아요~ 해외로 가고 싶네요ㅋㅋ",
          "original_form": "전국내는 거의 가 본 거 같아요~ 해외로 가고 싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100036897.1.11",
          "form": "오오 전국투어~다도셨군여",
          "original_form": "오오 전국투어~다도셨군여",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100036897.1.12",
          "form": "어디가 가장좋으셨어요",
          "original_form": "어디가 가장좋으셨어요",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100036897.1.13",
          "form": "전 거제도가 좋았던 것 같아요~",
          "original_form": "전 거제도가 좋았던 것 같아요~",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100036897.1.14",
          "form": "좋았던 여행지 있으세요?",
          "original_form": "좋았던 여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100036897.1.15",
          "form": "오오 저도 바다있는지역다좋아해요ㅜ",
          "original_form": "오오 저도 바다있는지역다좋아해요ㅜ",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100036897.1.16",
          "form": "국내는 제주도가 가장좋았어요",
          "original_form": "국내는 제주도가 가장좋았어요",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100036897.1.17",
          "form": "아 맞아요 제주도ㅋㅋㅋㅋ",
          "original_form": "아 맞아요 제주도ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100036897.1.18",
          "form": "제주도 정말 좋죠~",
          "original_form": "제주도 정말 좋죠~",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100036897.1.19",
          "form": "거제도 어디 글케좋으셨나여",
          "original_form": "거제도 어디 글케좋으셨나여",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100036897.1.20",
          "form": "전 그냥 외도 바람의  언덕~",
          "original_form": "전 그냥 외도 바람의  언덕~",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100036897.1.21",
          "form": "그 조용하고 차분한 분위기가 너무 좋았어요",
          "original_form": "그 조용하고 차분한 분위기가 너무 좋았어요",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100036897.1.22",
          "form": "그쳐 너무 북적거리지도않고",
          "original_form": "그쳐 너무 북적거리지도않고",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100036897.1.23",
          "form": "외도갈때 뱃길힘들던데",
          "original_form": "외도갈때 뱃길힘들던데",
          "speaker_id": "1",
          "time": "20210902 23:11"
        }
      ]
