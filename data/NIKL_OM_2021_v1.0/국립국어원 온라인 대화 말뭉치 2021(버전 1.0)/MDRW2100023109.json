      "utterance": [
        {
          "id": "MDRW2100023109.1.1",
          "form": "요가링 저도 쓸줄 몰라서 ㅋㅋㅋㅋ",
          "original_form": "요가링 저도 쓸줄 몰라서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023109.1.2",
          "form": "요가링 가장 짧은 지름이 한 4?센티 정도 되는 거 같아요",
          "original_form": "요가링 가장 짧은 지름이 한 4?센티 정도 되는 거 같아요",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023109.1.3",
          "form": "오 4센친면 되게",
          "original_form": "오 4센친면 되게",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023109.1.4",
          "form": "작을거 같은데",
          "original_form": "작을거 같은데",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023109.1.5",
          "form": "ㅋㅋㅋ여성분들은 다 잘맞을 것 같네용",
          "original_form": "ㅋㅋㅋ여성분들은 다 잘맞을 것 같네용",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023109.1.6",
          "form": "이런거 목같은데에다가도 끼고싶어요",
          "original_form": "이런거 목같은데에다가도 끼고싶어요",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023109.1.7",
          "form": "ㅌㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023109.1.8",
          "form": "누가 개발했찌 이런 훌륭한 아이템을..",
          "original_form": "누가 개발했찌 이런 훌륭한 아이템을..",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023109.1.9",
          "form": "폼롤러 하다가 전 그런 생각해요",
          "original_form": "폼롤러 하다가 전 그런 생각해요",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023109.1.10",
          "form": "이걸 왜 이제 샀지",
          "original_form": "이걸 왜 이제 샀지",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023109.1.11",
          "form": "ㅋㅋㅋㅋ 맞죠 폼블러 그거 허리 진짜 시원해요",
          "original_form": "ㅋㅋㅋㅋ 맞죠 폼블러 그거 허리 진짜 시원해요",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023109.1.12",
          "form": "네 그리고 허벅지 풀때도!!",
          "original_form": "네 그리고 허벅지 풀때도!!",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023109.1.13",
          "form": "근데 요가링 줄거면 두개를 주지",
          "original_form": "근데 요가링 줄거면 두개를 주지",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023109.1.14",
          "form": "이거 하나만 줘가지고 저 어떻게 쓰는 지도 몰랐어요",
          "original_form": "이거 하나만 줘가지고 저 어떻게 쓰는 지도 몰랐어요",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023109.1.15",
          "form": "엥 한개받았어요?ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엥 한개받았어요?ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023109.1.16",
          "form": "왜 세트로 안줬지 너무하네",
          "original_form": "왜 세트로 안줬지 너무하네",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023109.1.17",
          "form": "네... 두개 줘야하는거 아니에요 진짜?",
          "original_form": "네... 두개 줘야하는거 아니에요 진짜?",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023109.1.18",
          "form": "그러니까요 비대칭 되라는거야 머야",
          "original_form": "그러니까요 비대칭 되라는거야 머야",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023109.1.19",
          "form": "ㅠㅠ공짜라서 그냥 넘겼어요..",
          "original_form": "ㅠㅠ공짜라서 그냥 넘겼어요..",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023109.1.20",
          "form": "하나 더 사여ㅋㅋㅋㅋㅋ",
          "original_form": "하나 더 사여ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023109.1.21",
          "form": "짝짝이로 사도 돼요?",
          "original_form": "짝짝이로 사도 돼요?",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023109.1.22",
          "form": "같은 크기로 사는게 더 좋지않을까요",
          "original_form": "같은 크기로 사는게 더 좋지않을까요",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023109.1.23",
          "form": "후... 필라테스 학원 다니면 이런거 지원 해줘요?",
          "original_form": "후... 필라테스 학원 다니면 이런거 지원 해줘요?",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023109.1.24",
          "form": "다르게 끼고 있으먄 뭔가 이상할거 같은데ㅋㅋㅋㅋ",
          "original_form": "다르게 끼고 있으먄 뭔가 이상할거 같은데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023109.1.25",
          "form": "그냥 하나 번갈아가면서 쓸래요 ㅜㅜ",
          "original_form": "그냥 하나 번갈아가면서 쓸래요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023109.1.26",
          "form": "저희 학원엔 요가링없어욬ㅋㅋㅋㅋ",
          "original_form": "저희 학원엔 요가링없어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
