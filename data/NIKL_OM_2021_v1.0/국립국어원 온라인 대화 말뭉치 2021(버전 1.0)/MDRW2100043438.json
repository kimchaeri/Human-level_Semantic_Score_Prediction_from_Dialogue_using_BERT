      "utterance": [
        {
          "id": "MDRW2100043438.1.1",
          "form": "집안일 뭐하시나용",
          "original_form": "집안일 뭐하시나용",
          "speaker_id": "1",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043438.1.2",
          "form": "저는 빨래 주로 해요",
          "original_form": "저는 빨래 주로 해요",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043438.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043438.1.4",
          "form": "저는 거의 제 방 청소만 해요ㅋㅋ",
          "original_form": "저는 거의 제 방 청소만 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043438.1.5",
          "form": "방 청소는 깨끗하게 하시나요?",
          "original_form": "방 청소는 깨끗하게 하시나요?",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043438.1.6",
          "form": "그냥 적당히 해요 돌아다닐곳 보일정도만ㅋㅋ",
          "original_form": "그냥 적당히 해요 돌아다닐곳 보일정도만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043438.1.7",
          "form": "맞아요 사실 그게 제일 깔끔하고 내가 쓰기 좋은 방상태죠ㅋㅋㅋ",
          "original_form": "맞아요 사실 그게 제일 깔끔하고 내가 쓰기 좋은 방상태죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043438.1.8",
          "form": "ㅋㅋㅋㅋ저만 그런거 아닐거라고 믿어요",
          "original_form": "ㅋㅋㅋㅋ저만 그런거 아닐거라고 믿어요",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043438.1.9",
          "form": "대부분 다 그럴꺼예요ㅋㅋㅋㅋ",
          "original_form": "대부분 다 그럴꺼예요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043438.1.10",
          "form": "name1님은 기숙사 생활 해보신 적 있으신가요?",
          "original_form": "&name1&님은 기숙사 생활 해보신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043438.1.11",
          "form": "기숙사가 살기 불편하다그래서ㅠ",
          "original_form": "기숙사가 살기 불편하다그래서ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043438.1.12",
          "form": "그렇죠 그럼 지금 가족과 살고 계신가요?",
          "original_form": "그렇죠 그럼 지금 가족과 살고 계신가요?",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043438.1.13",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043438.1.14",
          "form": "가족 구성이 어떻게 되시나?",
          "original_form": "가족 구성이 어떻게 되시나?",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043438.1.15",
          "form": "나요?",
          "original_form": "나요?",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043438.1.16",
          "form": "저는 동생두명이랑 엄마랑 살아용",
