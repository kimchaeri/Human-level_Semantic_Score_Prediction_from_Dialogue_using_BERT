      "utterance": [
        {
          "id": "MDRW2100024234.1.1",
          "form": "ㅎㅎ 드레스 많이 사주셔요?ㅎ",
          "original_form": "ㅎㅎ 드레스 많이 사주셔요?ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100024234.1.2",
          "form": "전 남아라 드레스는 안사죠 ㅎㅎ",
          "original_form": "전 남아라 드레스는 안사죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024234.1.3",
          "form": "아아ㅎㅎ",
          "original_form": "아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024234.1.4",
          "form": "드레스 좋아한다고하길래ㅎㅎ",
          "original_form": "드레스 좋아한다고하길래ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024234.1.5",
          "form": "저희애 말구요 ㅋ",
          "original_form": "저희애 말구요 ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024234.1.6",
          "form": "어린이집 엄마들 아야기 하는거엿어여",
          "original_form": "어린이집 엄마들 아야기 하는거엿어여",
          "speaker_id": "1",
          "time": "20210830 23:31"
        },
        {
          "id": "MDRW2100024234.1.7",
          "form": "아아아아ㅎㅎ",
          "original_form": "아아아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024234.1.8",
          "form": "음 근데  아이가 이제 옷도 스스로 골라서 ㅎㅎ",
          "original_form": "음 근데  아이가 이제 옷도 스스로 골라서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024234.1.9",
          "form": "그런거엿군뇨ㅎㅎ",
          "original_form": "그런거엿군뇨ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024234.1.10",
          "form": "우왐ㅎ",
          "original_form": "우왐ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024234.1.11",
          "form": "다똑같네요ㅎ",
          "original_form": "다똑같네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024234.1.12",
          "form": "저희아이는  핑크 빵강옷 좋아해요",
          "original_form": "저희아이는  핑크 빵강옷 좋아해요",
          "speaker_id": "1",
          "time": "20210830 23:32"
        },
        {
          "id": "MDRW2100024234.1.13",
          "form": "저는 청멜빵 좋아해서 ㅎㅎ",
          "original_form": "저는 청멜빵 좋아해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024234.1.14",
          "form": "치마입고싶은데... 이러고ㅎㅎ",
          "original_form": "치마입고싶은데... 이러고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024234.1.15",
          "form": "청멜빵 자주 입히는거 같아요",
          "original_form": "청멜빵 자주 입히는거 같아요",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024234.1.16",
          "form": "ㅎㅎ 좋죠ㅎㅎ 어릴때아니면 언제 입어요ㅎㅎ",
          "original_form": "ㅎㅎ 좋죠ㅎㅎ 어릴때아니면 언제 입어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024234.1.17",
          "form": "ㅋ 애들도 취향이 잇엉요 ㅋ",
          "original_form": "ㅋ 애들도 취향이 잇엉요 ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024234.1.18",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024234.1.19",
          "form": "저는 모자 특히  ㅎㅎ",
          "original_form": "저는 모자 특히  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024234.1.20",
          "form": "근데 꼭 우리애는 내성적이라ㅎ",
          "original_form": "근데 꼭 우리애는 내성적이라ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100024234.1.21",
          "form": "...뭐뭐 하고 싶은데.. 이래요ㅎㅎ",
          "original_form": "...뭐뭐 하고 싶은데.. 이래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024234.1.22",
          "form": "그게 귀여워요ㅎ",
          "original_form": "그게 귀여워요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024234.1.23",
          "form": "모자요?",
          "original_form": "모자요?",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024234.1.24",
          "form": "모자만 한 겨울꺼가지 20개정도 되나봐요",
          "original_form": "모자만 한 겨울꺼가지 20개정도 되나봐요",
          "speaker_id": "1",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024234.1.25",
          "form": "우와ㅎㅎ",
          "original_form": "우와ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024234.1.26",
          "form": "우리애는 모자 싫어해요ㅎ",
          "original_form": "우리애는 모자 싫어해요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024234.1.27",
          "form": "2/3 은 타미꺼 ㅎㅎ",
          "original_form": "2/3 은 타미꺼 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024234.1.28",
          "form": "엄청 많네요ㅎㅎ",
          "original_form": "엄청 많네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024234.1.29",
          "form": "아ㅎㅎㅎㅎ 타미ㅎㅎ",
          "original_form": "아ㅎㅎㅎㅎ 타미ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024234.1.30",
          "form": "아이도 조아해요 색색별로 잇어서 옷입구 본인이 골라요 ㅎㅎ",
          "original_form": "아이도 조아해요 색색별로 잇어서 옷입구 본인이 골라요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100024234.1.31",
          "form": "이얌ㅎ 모자좋아하면 스타일 살아나겟네요ㅎ",
          "original_form": "이얌ㅎ 모자좋아하면 스타일 살아나겟네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024234.1.32",
          "form": "모자에 어울리는 옷도 많잖아요 ㅡㅎ",
          "original_form": "모자에 어울리는 옷도 많잖아요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024234.1.33",
          "form": "모자빨이 잇긴 해요 ㅎㅎ",
          "original_form": "모자빨이 잇긴 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
        {
          "id": "MDRW2100024234.1.34",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100024234.1.35",
          "form": "그래고 여아들은 꾸밀게 많아서 부럽요 ㅎㅎ",
          "original_form": "그래고 여아들은 꾸밀게 많아서 부럽요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100024234.1.36",
          "form": "컬러감이 있는걸좋아해요?",
          "original_form": "컬러감이 있는걸좋아해요?",
          "speaker_id": "2",
          "time": "20210830 23:36"
        },
        {
