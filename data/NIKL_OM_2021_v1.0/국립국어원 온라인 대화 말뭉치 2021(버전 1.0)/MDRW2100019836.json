      "utterance": [
        {
          "id": "MDRW2100019836.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019836.1.2",
          "form": "안녕하세요! 공연이나 전시회 좋아하세요?",
          "original_form": "안녕하세요! 공연이나 전시회 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019836.1.3",
          "form": "아뇨!ㅋㅋ영화 좋아해요",
          "original_form": "아뇨!ㅋㅋ영화 좋아해요",
          "speaker_id": "1",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100019836.1.4",
          "form": "영화관 가는거 좋아하시겠어요!",
          "original_form": "영화관 가는거 좋아하시겠어요!",
          "speaker_id": "2",
          "time": "20210828 01:13"
        },
        {
          "id": "MDRW2100019836.1.5",
          "form": "네 근디 코로나....",
          "original_form": "네 근디 코로나....",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100019836.1.6",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 01:15"
        },
        {
          "id": "MDRW2100019836.1.7",
          "form": "요즘은 그럼 ott 로 주로 보시나요?",
          "original_form": "요즘은 그럼 ott 로 주로 보시나요?",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100019836.1.8",
          "form": "네 ㅋㅋ 영화관은 진짜 가끔 가끔ㅠㅠ",
          "original_form": "네 ㅋㅋ 영화관은 진짜 가끔 가끔ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100019836.1.9",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100019836.1.10",
          "form": "한창보다가 요즘은 드라마봐요 보기는  많이봤어요 ㅋㅋ예전영화들",
          "original_form": "한창보다가 요즘은 드라마봐요 보기는  많이봤어요 ㅋㅋ예전영화들",
          "speaker_id": "2",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100019836.1.11",
          "form": "얼른 코로나가 끝나야 영화관을 마음 편하게 갈텐데ㅜㅜ",
          "original_form": "얼른 코로나가 끝나야 영화관을 마음 편하게 갈텐데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100019836.1.12",
          "form": "좋아하는 장르 있으신가요?",
          "original_form": "좋아하는 장르 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100019836.1.13",
          "form": "저는 액션인데 음 역사 기반 팩션 좋아해요",
          "original_form": "저는 액션인데 음 역사 기반 팩션 좋아해요",
          "speaker_id": "1",
          "time": "20210828 01:39"
        },
        {
          "id": "MDRW2100019836.1.14",
          "form": "예를들자면 뭐가있죠?",
          "original_form": "예를들자면 뭐가있죠?",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100019836.1.15",
          "form": "관상 광해 국제시장",
          "original_form": "관상 광해 국제시장",
          "speaker_id": "1",
          "time": "20210828 12:21"
        },
        {
          "id": "MDRW2100019836.1.16",
          "form": "음 또 모가디슈",
          "original_form": "음 또 모가디슈",
          "speaker_id": "1",
          "time": "20210828 12:22"
        },
        {
          "id": "MDRW2100019836.1.17",
          "form": "요런거?",
          "original_form": "요런거?",
          "speaker_id": "1",
          "time": "20210828 12:22"
        },
        {
          "id": "MDRW2100019836.1.18",
          "form": "아 모가디슈 아직 못봤어요! 말하신것중에 광해하나봤네요 ㅋㅋㅋㅋ",
          "original_form": "아 모가디슈 아직 못봤어요! 말하신것중에 광해하나봤네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:22"
        },
        {
          "id": "MDRW2100019836.1.19",
          "form": "관상을 안보시다닝",
          "original_form": "관상을 안보시다닝",
          "speaker_id": "1",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100019836.1.20",
          "form": "어이 관상가 양반 내가 왕이될상인가",
          "original_form": "어이 관상가 양반 내가 왕이될상인가",
          "speaker_id": "1",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100019836.1.21",
          "form": "영화 많이봤다고 생각하는데 남들이본건 또 많이 안봤더라구요 ㅋㅋㅋ",
          "original_form": "영화 많이봤다고 생각하는데 남들이본건 또 많이 안봤더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:23"
        }
      ]
