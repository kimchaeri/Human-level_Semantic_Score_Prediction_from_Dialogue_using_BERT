      "utterance": [
        {
          "id": "MDRW2100042428.1.1",
          "form": "낼 백신 맞나?\n1차?",
          "original_form": "낼 백신 맞나?\n1차?",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042428.1.2",
          "form": "오 좋겠다\n낼 바로 먹네",
          "original_form": "오 좋겠다\n낼 바로 먹네",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042428.1.3",
          "form": "먹을지안먹을진 낼 되어바야 알듯",
          "original_form": "먹을지안먹을진 낼 되어바야 알듯",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042428.1.4",
          "form": "저녁은 먹었나",
          "original_form": "저녁은 먹었나",
          "speaker_id": "1",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042428.1.5",
          "form": "응  ㅏㄹ탕ㅋ",
          "original_form": "응  ㅏㄹ탕ㅋ",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042428.1.6",
          "form": "알탕",
          "original_form": "알탕",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042428.1.7",
          "form": "저녁 야무지게 먹었어ㅋㅋ",
          "original_form": "저녁 야무지게 먹었어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042428.1.8",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "2",
          "time": "20210906 23:21"
        },
        {
          "id": "MDRW2100042428.1.9",
          "form": "난 고오추 참치랑 김이랑 그외반찬들 ㅋㅋ",
          "original_form": "난 고오추 참치랑 김이랑 그외반찬들 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:22"
        },
        {
          "id": "MDRW2100042428.1.10",
          "form": "ㅋㅋㄱ",
          "original_form": "ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 08:28"
        },
        {
          "id": "MDRW2100042428.1.11",
          "form": "음식이 좋아",
          "original_form": "음식이 좋아",
          "speaker_id": "2",
          "time": "20210907 08:54"
        },
        {
          "id": "MDRW2100042428.1.12",
          "form": "아침 먹었나 ㅋㅋ",
          "original_form": "아침 먹었나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:19"
        },
        {
          "id": "MDRW2100042428.1.13",
          "form": "지금ㅋ",
          "original_form": "지금ㅋ",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042428.1.14",
          "form": "까망베르치즈케잌",
          "original_form": "까망베르치즈케잌",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042428.1.15",
          "form": "약콩씨리얼",
          "original_form": "약콩씨리얼",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042428.1.16",
          "form": "우유 커피ㅋ",
          "original_form": "우유 커피ㅋ",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042428.1.17",
          "form": "넌? 아침 먹?",
          "original_form": "넌? 아침 먹?",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042428.1.18",
          "form": "나도 지금 먹는중 ㅋㅋ",
          "original_form": "나도 지금 먹는중 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:02"
        },
        {
          "id": "MDRW2100042428.1.19",
          "form": "열무김치에 비벼먹고있어 ㅋㅋ",
          "original_form": "열무김치에 비벼먹고있어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:02"
        },
        {
          "id": "MDRW2100042428.1.20",
          "form": "메뉴 다양ㅎㅎ맛있겠다ㅋ",
          "original_form": "메뉴 다양ㅎㅎ맛있겠다ㅋ",
          "speaker_id": "2",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100042428.1.21",
          "form": "ㅋㅋ 얼마전에 엄마가 열무김치 담아서",
          "original_form": "ㅋㅋ 얼마전에 엄마가 열무김치 담아서",
          "speaker_id": "1",
          "time": "20210907 10:36"
        },
        {
          "id": "MDRW2100042428.1.22",
          "form": "넌 아침 맛난거 먹었넹",
          "original_form": "넌 아침 맛난거 먹었넹",
          "speaker_id": "1",
          "time": "20210907 10:36"
        },
        {
          "id": "MDRW2100042428.1.23",
          "form": "오우 맛있겠어",
          "original_form": "오우 맛있겠어",
          "speaker_id": "2",
          "time": "20210907 11:43"
        },
        {
          "id": "MDRW2100042428.1.24",
          "form": "얼무국수 땡기네",
          "original_form": "얼무국수 땡기네",
          "speaker_id": "2",
          "time": "20210907 11:43"
        },
        {
          "id": "MDRW2100042428.1.25",
          "form": "난 지금 점심 먹으러옴\n메뉴는 새알미역국ㅋ",
          "original_form": "난 지금 점심 먹으러옴\n메뉴는 새알미역국ㅋ",
          "speaker_id": "2",
          "time": "20210907 11:43"
        },
        {
          "id": "MDRW2100042428.1.26",
          "form": "미역국 니가 좋아하는거네",
          "original_form": "미역국 니가 좋아하는거네",
          "speaker_id": "1",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100042428.1.27",
          "form": "전에 미역국 좋아한다 들은거같아",
          "original_form": "전에 미역국 좋아한다 들은거같아",
          "speaker_id": "1",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100042428.1.28",
          "form": "응 회사 미역국 맛있어\n진하고 짭쪼롬하게ㅎ",
          "original_form": "응 회사 미역국 맛있어\n진하고 짭쪼롬하게ㅎ",
          "speaker_id": "2",
          "time": "20210907 12:36"
        },
        {
          "id": "MDRW2100042428.1.29",
          "form": "미역국은 대용량 끓이는게 맛있는거 같더라",
          "original_form": "미역국은 대용량 끓이는게 맛있는거 같더라",
          "speaker_id": "2",
          "time": "20210907 12:36"
        },
        {
          "id": "MDRW2100042428.1.30",
          "form": "오 그렇구나 난 예전 학교급식이나 병원 미역국 노맛이라 집꺼가 더 맛있는줄",
          "original_form": "오 그렇구나 난 예전 학교급식이나 병원 미역국 노맛이라 집꺼가 더 맛있는줄",
          "speaker_id": "1",
          "time": "20210907 12:37"
        },
        {
          "id": "MDRW2100042428.1.31",
          "form": "엇 진짜? 의외다\n난 회사 식당 미역국은 항상 맛있어서ㅋ",
          "original_form": "엇 진짜? 의외다\n난 회사 식당 미역국은 항상 맛있어서ㅋ",
          "speaker_id": "2",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100042428.1.32",
          "form": "난 미역국 식당꺼도 별로더라",
          "original_form": "난 미역국 식당꺼도 별로더라",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100042428.1.33",
          "form": "너무 짜",
          "original_form": "너무 짜",
          "speaker_id": "1",
          "time": "20210907 13:06"
        }
      ]
