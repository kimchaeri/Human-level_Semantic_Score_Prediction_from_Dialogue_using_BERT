      "utterance": [
        {
          "id": "MDRW2100012237.1.1",
          "form": "이 신규취향지 노선이 이벤트를 많이 한다",
          "original_form": "이 신규취향지 노선이 이벤트를 많이 한다",
          "speaker_id": "1",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100012237.1.2",
          "form": "아~안알려져있으니까 말이지",
          "original_form": "아~안알려져있으니까 말이지",
          "speaker_id": "2",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100012237.1.3",
          "form": "정답~ 신규취항지가 솔직히 최고다 오픈빨이라",
          "original_form": "정답~ 신규취항지가 솔직히 최고다 오픈빨이라",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012237.1.4",
          "form": "맞네 오픈빨 ㅋㅋ",
          "original_form": "맞네 오픈빨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012237.1.5",
          "form": "요즘은 신규취향지도 엄청 많아서 이거 노려도 할만함",
          "original_form": "요즘은 신규취향지도 엄청 많아서 이거 노려도 할만함",
          "speaker_id": "1",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012237.1.6",
          "form": "여행팁이 엄청 많다",
          "original_form": "여행팁이 엄청 많다",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012237.1.7",
          "form": "그렇지 그냥 되느게 없다",
          "original_form": "그렇지 그냥 되느게 없다",
          "speaker_id": "1",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012237.1.8",
          "form": "들어보니 그렇네",
          "original_form": "들어보니 그렇네",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012237.1.9",
          "form": "거기다 그거 알제",
          "original_form": "거기다 그거 알제",
          "speaker_id": "1",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012237.1.10",
          "form": "뭐",
          "original_form": "뭐",
          "speaker_id": "2",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012237.1.11",
          "form": "직항보다 경유노선이 싼거",
          "original_form": "직항보다 경유노선이 싼거",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012237.1.12",
          "form": "피곤하잖아",
          "original_form": "피곤하잖아",
          "speaker_id": "2",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012237.1.13",
          "form": "그렇지 몸은 피곤해도 주머니는 빵빵 ㅋㅋ",
          "original_form": "그렇지 몸은 피곤해도 주머니는 빵빵 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012237.1.14",
          "form": "피곤타",
          "original_form": "피곤타",
          "speaker_id": "2",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012237.1.15",
          "form": "직항보다 엄청 저렴하기 때문에 일단 멀리가는거는 생활해볼만하다",
          "original_form": "직항보다 엄청 저렴하기 때문에 일단 멀리가는거는 생활해볼만하다",
          "speaker_id": "1",
          "time": "20210824 20:40"
        },
        {
          "id": "MDRW2100012237.1.16",
          "form": "멀리 가겠나?",
          "original_form": "멀리 가겠나?",
          "speaker_id": "2",
          "time": "20210824 20:40"
        },
        {
          "id": "MDRW2100012237.1.17",
          "form": "사람이 일이란게 모르지",
          "original_form": "사람이 일이란게 모르지",
          "speaker_id": "1",
          "time": "20210824 20:40"
        },
        {
          "id": "MDRW2100012237.1.18",
          "form": "일단 쇼핑을 할려면 많이 걸어야 하잖아",
          "original_form": "일단 쇼핑을 할려면 많이 걸어야 하잖아",
          "speaker_id": "1",
          "time": "20210824 20:41"
        }
      ]
