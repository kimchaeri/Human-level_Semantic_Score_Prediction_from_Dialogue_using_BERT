          "id": "MDRW2100021252.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021252.1.2",
          "form": "아뇨 키우시나요",
          "original_form": "아뇨 키우시나요",
          "speaker_id": "2",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021252.1.3",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "1",
          "time": "20210828 17:07"
        },
        {
          "id": "MDRW2100021252.1.4",
          "form": "키우고싶어요?",
          "original_form": "키우고싶어요?",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021252.1.5",
          "form": "네 여건이",
          "original_form": "네 여건이",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021252.1.6",
          "form": "저도 유튜브로 간접체험만..",
          "original_form": "저도 유튜브로 간접체험만..",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021252.1.7",
          "form": "보눈 것도 좋죠",
          "original_form": "보눈 것도 좋죠",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021252.1.8",
          "form": "고양이 좋아하세요?",
          "original_form": "고양이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100021252.1.9",
          "form": "털 때문에",
          "original_form": "털 때문에",
          "speaker_id": "1",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021252.1.10",
          "form": "그럼 키운다면 어떤걸??",
          "original_form": "그럼 키운다면 어떤걸??",
          "speaker_id": "2",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100021252.1.11",
          "form": "강아지죠",
          "original_form": "강아지죠",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100021252.1.12",
          "form": "털은 강아지도 날릴텐데요",
          "original_form": "털은 강아지도 날릴텐데요",
          "speaker_id": "2",
          "time": "20210828 19:55"
        },
        {
          "id": "MDRW2100021252.1.13",
          "form": "고양이보다는 낫죠",
