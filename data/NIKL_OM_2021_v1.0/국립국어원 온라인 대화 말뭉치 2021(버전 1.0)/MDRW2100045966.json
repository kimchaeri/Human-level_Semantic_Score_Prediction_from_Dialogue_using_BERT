        },
        {
          "id": "MDRW2100045966.1.2",
          "form": "최근 영화 재밌게 보신거 있으신가요?",
          "original_form": "최근 영화 재밌게 보신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045966.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045966.1.4",
          "form": "코로나때문에 최근 영화관을 못갔어요",
          "original_form": "코로나때문에 최근 영화관을 못갔어요",
          "speaker_id": "2",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045966.1.5",
          "form": "재밌게 보신거 있나요?",
          "original_form": "재밌게 보신거 있나요?",
          "speaker_id": "2",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045966.1.6",
          "form": "아이고 ㅠㅠ 저도 영화관은 코로나 이후 못갔어요",
          "original_form": "아이고 ㅠㅠ 저도 영화관은 코로나 이후 못갔어요",
          "speaker_id": "1",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045966.1.7",
          "form": "모가디슈 평 괜찮더라구요",
          "original_form": "모가디슈 평 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045966.1.8",
          "form": "아 맞아요 맞아요",
          "original_form": "아 맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045966.1.9",
          "form": "나중에 극장에서 내려오면 브이오디로 볼까싶어요",
          "original_form": "나중에 극장에서 내려오면 브이오디로 볼까싶어요",
          "speaker_id": "1",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045966.1.10",
          "form": "그쵸 저도 그거 기다리고 있어요",
          "original_form": "그쵸 저도 그거 기다리고 있어요",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045966.1.11",
          "form": "그리고 이번 디즈니 영화들도",
          "original_form": "그리고 이번 디즈니 영화들도",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045966.1.12",
          "form": "평이 좋더라구요",
          "original_form": "평이 좋더라구요",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045966.1.13",
          "form": "크루엘라 재밌다는데 아직 못봤어요",
          "original_form": "크루엘라 재밌다는데 아직 못봤어요",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045966.1.14",
          "form": "아 맞아요맞아요 ㅠㅠㅠㅠ",
          "original_form": "아 맞아요맞아요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045966.1.15",
          "form": "그것도 꼭 보고싶어요",
          "original_form": "그것도 꼭 보고싶어요",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045966.1.16",
          "form": "봐야되겠다고 킵해둔 영화는 여러개인데",
          "original_form": "봐야되겠다고 킵해둔 영화는 여러개인데",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045966.1.17",
          "form": "전 마블영화도 좋아해서",
          "original_form": "전 마블영화도 좋아해서",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045966.1.18",
          "form": "킵해두고 있어요 ㅠㅠ",
          "original_form": "킵해두고 있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045966.1.19",
          "form": "이번에 샹치도 평 괜찮더라구요",
          "original_form": "이번에 샹치도 평 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045966.1.20",
          "form": "맞아요 ㅠㅠㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045966.1.21",
          "form": "ㄷ ㅏ...챙겨봐야하는데",
          "original_form": "ㄷ ㅏ...챙겨봐야하는데",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045966.1.22",
          "form": "영화관을 안가고 미뤄두다보니까",
          "original_form": "영화관을 안가고 미뤄두다보니까",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045966.1.23",
          "form": "한도끝도없이 미루게되네요 ㅠㅠㅋㅋㅋ",
          "original_form": "한도끝도없이 미루게되네요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045966.1.24",
          "form": "ㅋㅋ큐ㅠㅠㅠㅠ 빨리 영화관 정상운영 했으면 좋겠네요",
          "original_form": "ㅋㅋ큐ㅠㅠㅠㅠ 빨리 영화관 정상운영 했으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045966.1.25",
          "form": "그러니까요 ㅠㅠㅠㅠ",
          "original_form": "그러니까요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045966.1.26",
          "form": "지금도 가려면 갈수있긴한데",
          "original_form": "지금도 가려면 갈수있긴한데",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045966.1.27",
          "form": "뭔가 찝찝해서",
          "original_form": "뭔가 찝찝해서",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045966.1.28",
          "form": "못가고있어요",
          "original_form": "못가고있어요",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045966.1.29",
          "form": "그쵸 ㅋㅋㅋㅋ 방역 한다고는 하는대",
          "original_form": "그쵸 ㅋㅋㅋㅋ 방역 한다고는 하는대",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045966.1.30",
          "form": "밀폐된 공간이니까요...",
          "original_form": "밀폐된 공간이니까요...",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045966.1.31",
          "form": "맞아요.... ㅠㅠㅠ",
          "original_form": "맞아요.... ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045966.1.32",
          "form": "영화관가서 큰 화면에 빵빵한 사운드로",
          "original_form": "영화관가서 큰 화면에 빵빵한 사운드로",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045966.1.33",
          "form": "봐야하는데 ...",
          "original_form": "봐야하는데 ...",
          "speaker_id": "1",
