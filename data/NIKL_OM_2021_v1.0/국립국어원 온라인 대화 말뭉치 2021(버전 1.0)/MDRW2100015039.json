      "utterance": [
        {
          "id": "MDRW2100015039.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100015039.1.2",
          "form": "반갑습니다",
          "original_form": "반갑습니다",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015039.1.3",
          "form": "강아지 vs 고양이 뭐더좋아하세요?",
          "original_form": "강아지 vs 고양이 뭐더좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015039.1.4",
          "form": "고양이가 더 좋아요",
          "original_form": "고양이가 더 좋아요",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015039.1.5",
          "form": "그쪽은 어느파이신지요",
          "original_form": "그쪽은 어느파이신지요",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015039.1.6",
          "form": "저는 둘다좋지만 강아지를 키우고잇어 강아지요",
          "original_form": "저는 둘다좋지만 강아지를 키우고잇어 강아지요",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015039.1.7",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015039.1.8",
          "form": "아니요 저는 알러지가 있어서 키우진 못하고",
          "original_form": "아니요 저는 알러지가 있어서 키우진 못하고",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015039.1.9",
          "form": "사진만 보고 좋아합니다",
          "original_form": "사진만 보고 좋아합니다",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015039.1.10",
          "form": "알러지 진짜 고생하더라구요\r\n제친구도 알러지잇는지모르고 키웠다가 약먹고잇더라는ㅜ",
          "original_form": "알러지 진짜 고생하더라구요\r\n제친구도 알러지잇는지모르고 키웠다가 약먹고잇더라는ㅜ",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015039.1.11",
          "form": "곤충같은건요? 어릴적 키워보셨나요?",
          "original_form": "곤충같은건요? 어릴적 키워보셨나요?",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015039.1.12",
          "form": "병아리를... 한번 키워봤었죠",
          "original_form": "병아리를... 한번 키워봤었죠",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015039.1.13",
          "form": "500원짜리 학교앞에서 팔던거...",
          "original_form": "500원짜리 학교앞에서 팔던거...",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015039.1.14",
          "form": "아시나요.....",
          "original_form": "아시나요.....",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015039.1.15",
          "form": "알죠 저랑 비슷한 연령대인가바요",
          "original_form": "알죠 저랑 비슷한 연령대인가바요",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015039.1.16",
          "form": "저희집은 아빠가 잘키워서 닭으로 자랐고",
          "original_form": "저희집은 아빠가 잘키워서 닭으로 자랐고",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015039.1.17",
          "form": "냠냠 먹었어요...삼계탕...ㅋㅋㄱㅋ",
          "original_form": "냠냠 먹었어요...삼계탕...ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015039.1.18",
          "form": "ㅋㅋㅋㅋ저는 그 병아리가 금방 돌아가셔서...",
          "original_form": "ㅋㅋㅋㅋ저는 그 병아리가 금방 돌아가셔서...",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015039.1.19",
          "form": "그걸 본 뒤로 반려동물을 못키우나봐요ㅠㅠ",
          "original_form": "그걸 본 뒤로 반려동물을 못키우나봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015039.1.20",
          "form": "아하 그럴수잇죠!\r\n병아리를 병든걸 갖다둔다하더라구요ㅜ\r\n그땐그게참이뻤는데!",
          "original_form": "아하 그럴수잇죠!\r\n병아리를 병든걸 갖다둔다하더라구요ㅜ\r\n그땐그게참이뻤는데!",
          "speaker_id": "2",
          "time": "20210826 09:33"
        },
        {
          "id": "MDRW2100015039.1.21",
          "form": "그땐 핑크도 있고 파랑도 있고 알록달록한 병아리들 보고 이쁘다고 생각했는데",
          "original_form": "그땐 핑크도 있고 파랑도 있고 알록달록한 병아리들 보고 이쁘다고 생각했는데",
          "speaker_id": "1",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100015039.1.22",
          "form": "그게 참 ㅜㅜㅜ 그러면 안되는건데 병아리들을 염색시켜가지고 ㅜㅜ",
          "original_form": "그게 참 ㅜㅜㅜ 그러면 안되는건데 병아리들을 염색시켜가지고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 10:00"
        },
        {
          "id": "MDRW2100015039.1.23",
          "form": "그니까요 요즘은 새우? 뭐그런거 판다하던데",
          "original_form": "그니까요 요즘은 새우? 뭐그런거 판다하던데",
          "speaker_id": "2",
          "time": "20210826 10:16"
        },
        {
          "id": "MDRW2100015039.1.24",
          "form": "키우고싶은동믄있으세요?",
          "original_form": "키우고싶은동믄있으세요?",
          "speaker_id": "2",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100015039.1.25",
          "form": "동물",
          "original_form": "동물",
          "speaker_id": "2",
          "time": "20210826 10:17"
        },
        {
          "id": "MDRW2100015039.1.26",
          "form": "고양이 한번 키워보고싶어요",
          "original_form": "고양이 한번 키워보고싶어요",
          "speaker_id": "1",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100015039.1.27",
          "form": "고양이 집사님 되면 진짜 재미날것같아요",
          "original_form": "고양이 집사님 되면 진짜 재미날것같아요",
          "speaker_id": "2",
          "time": "20210826 12:45"
        }
      ]
