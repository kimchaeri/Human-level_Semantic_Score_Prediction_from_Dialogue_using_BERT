      "utterance": [
        {
          "id": "MDRW2100008111.1.1",
          "form": "유산균 먹으니까 확실히 소화가 잘 되는 것 같아",
          "original_form": "유산균 먹으니까 확실히 소화가 잘 되는 것 같아",
          "speaker_id": "1",
          "time": "20210822 14:24"
        },
        {
          "id": "MDRW2100008111.1.2",
          "form": "꾸준히 먹으면 좋아",
          "original_form": "꾸준히 먹으면 좋아",
          "speaker_id": "2",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008111.1.3",
          "form": "배에서 꾸륵꾸륵 하던 소리도 잘 안나고",
          "original_form": "배에서 꾸륵꾸륵 하던 소리도 잘 안나고",
          "speaker_id": "1",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008111.1.4",
          "form": "배에 가스가 덜 차는 거지 ㅎㅎ",
          "original_form": "배에 가스가 덜 차는 거지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008111.1.5",
          "form": "ㅇㅇ 장운동이 활발해졌는지 더부룩한 느낌이 많이 줄었어",
          "original_form": "ㅇㅇ 장운동이 활발해졌는지 더부룩한 느낌이 많이 줄었어",
          "speaker_id": "1",
          "time": "20210822 14:25"
        },
        {
          "id": "MDRW2100008111.1.6",
          "form": "나도 유산균 먹고 나서부터는 속이 편해",
          "original_form": "나도 유산균 먹고 나서부터는 속이 편해",
          "speaker_id": "2",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008111.1.7",
          "form": "확실히 그렇지?",
          "original_form": "확실히 그렇지?",
          "speaker_id": "1",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008111.1.8",
          "form": "ㅇㅇ 효과 있는듯",
          "original_form": "ㅇㅇ 효과 있는듯",
          "speaker_id": "2",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008111.1.9",
          "form": "가격도 착한데 꾸준히 먹어야겠어",
          "original_form": "가격도 착한데 꾸준히 먹어야겠어",
          "speaker_id": "1",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008111.1.10",
          "form": "할인 기간만 잘 맞추면 국내에서 사는 것보다 싸니까",
          "original_form": "할인 기간만 잘 맞추면 국내에서 사는 것보다 싸니까",
          "speaker_id": "2",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008111.1.11",
          "form": "배송이 좀 오래 걸려서 그렇지 ㅎㅎ",
          "original_form": "배송이 좀 오래 걸려서 그렇지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:26"
        },
        {
          "id": "MDRW2100008111.1.12",
          "form": "미리미리 주문하면 되지 뭐",
          "original_form": "미리미리 주문하면 되지 뭐",
          "speaker_id": "2",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008111.1.13",
          "form": "ㅎㅎ 다 떨어지기 한달 전쯤에 주문해야겠다",
          "original_form": "ㅎㅎ 다 떨어지기 한달 전쯤에 주문해야겠다",
          "speaker_id": "1",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008111.1.14",
          "form": "그런데 비타민은 효과 잘 모르겠어",
          "original_form": "그런데 비타민은 효과 잘 모르겠어",
