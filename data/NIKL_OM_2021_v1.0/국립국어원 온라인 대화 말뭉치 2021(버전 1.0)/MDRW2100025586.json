      "utterance": [
        {
          "id": "MDRW2100025586.1.1",
          "form": "ㅋㅋㅋ캐릭터니까요",
          "original_form": "ㅋㅋㅋ캐릭터니까요",
          "speaker_id": "2",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025586.1.2",
          "form": "네 ㅎㅎ 그 캐릭터가 별로예요",
          "original_form": "네 ㅎㅎ 그 캐릭터가 별로예요",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025586.1.3",
          "form": "사석에서는 되게 소심하던데 ㅋㅋ",
          "original_form": "사석에서는 되게 소심하던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025586.1.4",
          "form": "정준하 캐릭터가 그나마 시트콤같고 나은거 같아요",
          "original_form": "정준하 캐릭터가 그나마 시트콤같고 나은거 같아요",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025586.1.5",
          "form": "물론 핑퐁 상태가 있어야 더 빛나겠지만",
          "original_form": "물론 핑퐁 상태가 있어야 더 빛나겠지만",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025586.1.6",
          "form": "서로 다 잘 어우러졌다고 봐요",
          "original_form": "서로 다 잘 어우러졌다고 봐요",
          "speaker_id": "2",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025586.1.7",
          "form": "콤비네이션 피자같이 ㅋㅋ",
          "original_form": "콤비네이션 피자같이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025586.1.8",
          "form": "저는 정형돈 나오길 바랬는데 아쉽게 된더 같더라고요",
          "original_form": "저는 정형돈 나오길 바랬는데 아쉽게 된더 같더라고요",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025586.1.9",
          "form": "그 공황장애인가 그거때문에 ㅠㅠ",
          "original_form": "그 공황장애인가 그거때문에 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100025586.1.10",
          "form": "그러니까요..",
          "original_form": "그러니까요..",
          "speaker_id": "1",
          "time": "20210901 14:22"
        },
        {
          "id": "MDRW2100025586.1.11",
          "form": "사실 다른멤버는 그렇고 제일보고싶은 멤번데 불발되서ㅠ",
          "original_form": "사실 다른멤버는 그렇고 제일보고싶은 멤번데 불발되서ㅠ",
          "speaker_id": "1",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100025586.1.12",
          "form": "형돈이 팬이셨군요?ㅋㅋ",
          "original_form": "형돈이 팬이셨군요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025586.1.13",
          "form": "정형돈 팬이라기보다 무도에선 제일 나아서요 ㅎㅎ",
          "original_form": "정형돈 팬이라기보다 무도에선 제일 나아서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025586.1.14",
          "form": "하하는 지금 나오니까 굳이 얘긴안했어요 ㅎ",
          "original_form": "하하는 지금 나오니까 굳이 얘긴안했어요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025586.1.15",
          "form": "하하는 뭐 케미 좋아서 ㅎㅎ",
          "original_form": "하하는 뭐 케미 좋아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100025586.1.16",
          "form": "그쵸\r\n런닝맨도 같이 해서 눈치도 빠르고 ㅎㅎ",
          "original_form": "그쵸\r\n런닝맨도 같이 해서 눈치도 빠르고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100025586.1.17",
          "form": "맞아요 ㅋㅋ 왠지 무도+런닝맨 될 느낌이긴해요",
          "original_form": "맞아요 ㅋㅋ 왠지 무도+런닝맨 될 느낌이긴해요",
          "speaker_id": "2",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025586.1.18",
          "form": "뭔가 정과장 이미지가 강해서 무도멤버 한명 꼽으라면 정준하랑 놀뭐했으면 좋겠어요 ㅋㅋ",
          "original_form": "뭔가 정과장 이미지가 강해서 무도멤버 한명 꼽으라면 정준하랑 놀뭐했으면 좋겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025586.1.19",
          "form": "지석진나오니까 런닝맨같더라구요",
          "original_form": "지석진나오니까 런닝맨같더라구요",
