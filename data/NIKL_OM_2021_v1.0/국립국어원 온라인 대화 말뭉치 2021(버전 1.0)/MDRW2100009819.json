      "utterance": [
        {
          "id": "MDRW2100009819.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 10:26"
        },
        {
          "id": "MDRW2100009819.1.2",
          "form": "혹시 일하고 계신가요?",
          "original_form": "혹시 일하고 계신가요?",
          "speaker_id": "1",
          "time": "20210823 10:26"
        },
        {
          "id": "MDRW2100009819.1.3",
          "form": "안녕하세요! 초등학교에서 돌봄교실 교사하고있습니다",
          "original_form": "안녕하세요! 초등학교에서 돌봄교실 교사하고있습니다",
          "speaker_id": "2",
          "time": "20210823 10:29"
        },
        {
          "id": "MDRW2100009819.1.4",
          "form": "어떤 일 하시나요?",
          "original_form": "어떤 일 하시나요?",
          "speaker_id": "2",
          "time": "20210823 10:30"
        },
        {
          "id": "MDRW2100009819.1.5",
          "form": "저는 it쪽이고 재택중입니다!",
          "original_form": "저는 it쪽이고 재택중입니다!",
          "speaker_id": "1",
          "time": "20210823 10:30"
        },
        {
          "id": "MDRW2100009819.1.6",
          "form": "초등학교는 방학인가요?",
          "original_form": "초등학교는 방학인가요?",
          "speaker_id": "1",
          "time": "20210823 10:30"
        },
        {
          "id": "MDRW2100009819.1.7",
          "form": "지금 개학해서 학교 나오고 있습니다..",
          "original_form": "지금 개학해서 학교 나오고 있습니다..",
          "speaker_id": "2",
          "time": "20210823 10:31"
        },
        {
          "id": "MDRW2100009819.1.8",
          "form": "그렇군요 코로나여도 등교하나요??",
          "original_form": "그렇군요 코로나여도 등교하나요??",
          "speaker_id": "1",
          "time": "20210823 10:32"
        },
        {
          "id": "MDRW2100009819.1.9",
          "form": "아직 교육청에서 안나왔는데",
          "original_form": "아직 교육청에서 안나왔는데",
          "speaker_id": "2",
          "time": "20210823 10:32"
        },
        {
          "id": "MDRW2100009819.1.10",
          "form": "조만간 전면등교 할거같아요 애들 격차가 너무벌어져서",
          "original_form": "조만간 전면등교 할거같아요 애들 격차가 너무벌어져서",
          "speaker_id": "2",
          "time": "20210823 10:32"
        },
        {
          "id": "MDRW2100009819.1.11",
          "form": "그렇겠네요 너무 길어지니...",
          "original_form": "그렇겠네요 너무 길어지니...",
          "speaker_id": "1",
          "time": "20210823 10:42"
        },
        {
          "id": "MDRW2100009819.1.12",
          "form": "일은 잘 맞나요??",
          "original_form": "일은 잘 맞나요??",
          "speaker_id": "1",
          "time": "20210823 10:42"
        },
        {
          "id": "MDRW2100009819.1.13",
          "form": "애들 사랑스러운데 저를 괴롭게합니다 ㅋㅋㅋㅋ",
          "original_form": "애들 사랑스러운데 저를 괴롭게합니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100009819.1.14",
          "form": "name1님은 어떠세요?",
          "original_form": "&name1&님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100009819.1.15",
          "form": "저는 할만한데 너무 발전이 없는 거 같아요ㅋㅋ",
          "original_form": "저는 할만한데 너무 발전이 없는 거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:22"
        },
        {
          "id": "MDRW2100009819.1.16",
          "form": "아 맨날 쳇바퀴 같은 느낌인가요?",
          "original_form": "아 맨날 쳇바퀴 같은 느낌인가요?",
          "speaker_id": "2",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009819.1.17",
          "form": "네 다른 회사 가서 써먹을 것도 별로 없는거 같고요..ㅋㅋ",
          "original_form": "네 다른 회사 가서 써먹을 것도 별로 없는거 같고요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:37"
        },
        {
          "id": "MDRW2100009819.1.18",
          "form": "돌봄교사는 어떤 일을 하나요??",
          "original_form": "돌봄교사는 어떤 일을 하나요??",
          "speaker_id": "1",
          "time": "20210823 12:37"
        },
        {
          "id": "MDRW2100009819.1.19",
          "form": "코로나여도 집에서 원격수업 못듣는애들이 학교나와서 수업듣게 돕고있어요!",
          "original_form": "코로나여도 집에서 원격수업 못듣는애들이 학교나와서 수업듣게 돕고있어요!",
          "speaker_id": "2",
          "time": "20210823 12:55"
        },
        {
          "id": "MDRW2100009819.1.20",
          "form": "아 원격수업 못 듣는 애들은 어떡하나 했는데",
          "original_form": "아 원격수업 못 듣는 애들은 어떡하나 했는데",
          "speaker_id": "1",
          "time": "20210823 13:03"
        },
        {
          "id": "MDRW2100009819.1.21",
          "form": "그런 방법이 있었군요!",
          "original_form": "그런 방법이 있었군요!",
