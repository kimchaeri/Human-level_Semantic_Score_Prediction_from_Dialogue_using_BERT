      "utterance": [
        {
          "id": "MDRW2100032875.1.1",
          "form": "동아리 뭐뭐 해봤어?",
          "original_form": "동아리 뭐뭐 해봤어?",
          "speaker_id": "1",
          "time": "20210906 03:06"
        },
        {
          "id": "MDRW2100032875.1.2",
          "form": "학교 다닐때 영어동아리?",
          "original_form": "학교 다닐때 영어동아리?",
          "speaker_id": "2",
          "time": "20210906 03:07"
        },
        {
          "id": "MDRW2100032875.1.3",
          "form": "나는 미술 코딩 3d프린터 봉사 크로키 헬스 페미 앱제작 해봤오",
          "original_form": "나는 미술 코딩 3d프린터 봉사 크로키 헬스 페미 앱제작 해봤오",
          "speaker_id": "1",
          "time": "20210906 03:07"
        },
        {
          "id": "MDRW2100032875.1.4",
          "form": "별로 하고 싶진 않았는데 선택지가 없었지...",
          "original_form": "별로 하고 싶진 않았는데 선택지가 없었지...",
          "speaker_id": "2",
          "time": "20210906 03:07"
        },
        {
          "id": "MDRW2100032875.1.5",
          "form": "헐 다른 건 없구?",
          "original_form": "헐 다른 건 없구?",
          "speaker_id": "1",
          "time": "20210906 03:07"
        },
        {
          "id": "MDRW2100032875.1.6",
          "form": "아...ㅋㅋㅋㅋ 학술이었구나",
          "original_form": "아...ㅋㅋㅋㅋ 학술이었구나",
          "speaker_id": "1",
          "time": "20210906 03:07"
        },
        {
          "id": "MDRW2100032875.1.7",
          "form": "봉사도 했던거 같고.. 다른건 기억이 안나네",
          "original_form": "봉사도 했던거 같고.. 다른건 기억이 안나네",
          "speaker_id": "2",
          "time": "20210906 03:08"
        },
        {
          "id": "MDRW2100032875.1.8",
          "form": "별로 재밌던 동아리는 아니었나봨ㅋㅋㅋ",
          "original_form": "별로 재밌던 동아리는 아니었나봨ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:08"
        },
        {
          "id": "MDRW2100032875.1.9",
          "form": "영어 동아리는 다른거 들어가려고 했는데 사람이 다 차서 어쩔 수 없이 둘어갔엌ㅋㅋ",
          "original_form": "영어 동아리는 다른거 들어가려고 했는데 사람이 다 차서 어쩔 수 없이 둘어갔엌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:08"
        },
        {
          "id": "MDRW2100032875.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 다 그냥그랬구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 다 그냥그랬구나",
          "speaker_id": "1",
          "time": "20210906 03:08"
        },
        {
          "id": "MDRW2100032875.1.11",
          "form": "아이고ㅠㅋㅋㅋ 영어동아리 경영동아리",
          "original_form": "아이고ㅠㅋㅋㅋ 영어동아리 경영동아리",
          "speaker_id": "1",
          "time": "20210906 03:08"
        },
        {
          "id": "MDRW2100032875.1.12",
          "form": "전부....눈물나네... 빡세게 살았구나",
          "original_form": "전부....눈물나네... 빡세게 살았구나",
          "speaker_id": "1",
          "time": "20210906 03:09"
        },
        {
          "id": "MDRW2100032875.1.13",
          "form": "아니얔ㅋㅋㅋ",
          "original_form": "아니얔ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:09"
        },
        {
          "id": "MDRW2100032875.1.14",
          "form": "동아리가서 그냥 멍때리다 왔엌ㅋㅋㅋ",
          "original_form": "동아리가서 그냥 멍때리다 왔엌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:10"
        },
        {
          "id": "MDRW2100032875.1.15",
          "form": "지금 생각해보니 시간낭비 오지넼ㅋㅋㅋ",
          "original_form": "지금 생각해보니 시간낭비 오지넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:10"
        },
        {
          "id": "MDRW2100032875.1.16",
          "form": "근데 3D프린터 동아리는 무슨 동아리야??",
          "original_form": "근데 3D프린터 동아리는 무슨 동아리야??",
          "speaker_id": "2",
          "time": "20210906 03:10"
        },
        {
          "id": "MDRW2100032875.1.17",
          "form": "ㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㅋ하기 싫으면 그럴 수 있지",
          "original_form": "ㅋㄱㅋㄱㅋㄱㄱㅋㄱㅋㅋ하기 싫으면 그럴 수 있지",
          "speaker_id": "1",
          "time": "20210906 03:11"
        },
        {
          "id": "MDRW2100032875.1.18",
          "form": "음 프로그램 배우고 만들고 싶은 거 만들어서 뽑아보고",
          "original_form": "음 프로그램 배우고 만들고 싶은 거 만들어서 뽑아보고",
          "speaker_id": "1",
          "time": "20210906 03:11"
        },
        {
          "id": "MDRW2100032875.1.19",
          "form": "근데 난 재미없어서 3D펜갖다가 미술동아리랑 협업해서 벽화그렸어",
          "original_form": "근데 난 재미없어서 3D펜갖다가 미술동아리랑 협업해서 벽화그렸어",
          "speaker_id": "1",
          "time": "20210906 03:12"
        },
        {
          "id": "MDRW2100032875.1.20",
          "form": "3D펜 재밌을 것같다ㅠㅠ",
          "original_form": "3D펜 재밌을 것같다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 03:14"
        },
        {
          "id": "MDRW2100032875.1.21",
          "form": "벽화도 그릴정도면 그림 되게 잘그리겠다ㅠㅠ",
          "original_form": "벽화도 그릴정도면 그림 되게 잘그리겠다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 03:14"
        },
        {
          "id": "MDRW2100032875.1.22",
          "form": "ㅋㅋㅋㅋ아 진짜 별거 아니구",
          "original_form": "ㅋㅋㅋㅋ아 진짜 별거 아니구",
          "speaker_id": "1",
          "time": "20210906 03:16"
        },
        {
          "id": "MDRW2100032875.1.23",
          "form": "나 그림 제일 잘그리는게 졸라맨이야...",
          "original_form": "나 그림 제일 잘그리는게 졸라맨이야...",
          "speaker_id": "2",
          "time": "20210906 03:16"
        },
        {
          "id": "MDRW2100032875.1.24",
          "form": "쓸디펜으로 꽃 만들어서 나뭇가지 그린 벽에 붙이고",
          "original_form": "쓸디펜으로 꽃 만들어서 나뭇가지 그린 벽에 붙이고",
          "speaker_id": "1",
          "time": "20210906 03:16"
        },
        {
          "id": "MDRW2100032875.1.25",
          "form": "이것보다 잘그리면 다 금손임...",
          "original_form": "이것보다 잘그리면 다 금손임...",
          "speaker_id": "2",
          "time": "20210906 03:16"
        },
        {
          "id": "MDRW2100032875.1.26",
          "form": "그냥 그런 거 했어",
          "original_form": "그냥 그런 거 했어",
          "speaker_id": "1",
          "time": "20210906 03:16"
        },
        {
          "id": "MDRW2100032875.1.27",
          "form": "아놬ㅋㅋㅋㅋㅋㅋ 금손됐네 고마오^.^",
          "original_form": "아놬ㅋㅋㅋㅋㅋㅋ 금손됐네 고마오^.^",
          "speaker_id": "1",
          "time": "20210906 03:16"
        },
        {
          "id": "MDRW2100032875.1.28",
          "form": "헐 완전 재밌었겠다ㅠㅠ",
          "original_form": "헐 완전 재밌었겠다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 03:17"
        },
        {
          "id": "MDRW2100032875.1.29",
          "form": "나때는 그런거 없어서 못했는데 금손님 부럽네요.ㅋㅋㅋㅋㅋ",
          "original_form": "나때는 그런거 없어서 못했는데 금손님 부럽네요.ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:17"
        },
        {
          "id": "MDRW2100032875.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아 재밌었어",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아 재밌었어",
          "speaker_id": "1",
          "time": "20210906 03:19"
        },
        {
          "id": "MDRW2100032875.1.31",
          "form": "자발적으로 9시까지 하다가고 그랬삼",
          "original_form": "자발적으로 9시까지 하다가고 그랬삼",
          "speaker_id": "1",
          "time": "20210906 03:19"
        },
        {
          "id": "MDRW2100032875.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나두...3학년에 새로 생긴 동아리였어...ㅜㅜ 얼마 못 함 재밌었는데 아쉽다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나두...3학년에 새로 생긴 동아리였어...ㅜㅜ 얼마 못 함 재밌었는데 아쉽다",
