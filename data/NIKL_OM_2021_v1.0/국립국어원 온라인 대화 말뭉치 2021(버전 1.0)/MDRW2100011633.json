      "utterance": [
        {
          "id": "MDRW2100011633.1.1",
          "form": "ㄴ난 반려동물을 많이 키운 편이지",
          "original_form": "ㄴ난 반려동물을 많이 키운 편이지",
          "speaker_id": "1",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011633.1.2",
          "form": "오그래?",
          "original_form": "오그래?",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.3",
          "form": "물고기 고슴도치 햄스터 강아지",
          "original_form": "물고기 고슴도치 햄스터 강아지",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.4",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.5",
          "form": "다 키워봄",
          "original_form": "다 키워봄",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.6",
          "form": "고슴도치가 새께 낳아서",
          "original_form": "고슴도치가 새께 낳아서",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.7",
          "form": "난 아직 북극곰밖에 안키워봤어",
          "original_form": "난 아직 북극곰밖에 안키워봤어",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.8",
          "form": "분양도 해봣닼ㅋㅋㅋ",
          "original_form": "분양도 해봣닼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.9",
          "form": "name1이?ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅂ",
          "original_form": "&name1&이?ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅂ",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.10",
          "form": "ㅋㅋㅋㅋㅋ아 초딩때 핸스터도 키워봤다",
          "original_form": "ㅋㅋㅋㅋㅋ아 초딩때 핸스터도 키워봤다",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.11",
          "form": "북극곰 보호해야하는ㄷ0",
          "original_form": "북극곰 보호해야하는ㄷ0",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.12",
          "form": "데",
          "original_form": "데",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.13",
          "form": "집에 데리고 와도 괜찮나?",
          "original_form": "집에 데리고 와도 괜찮나?",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011633.1.14",
          "form": "북극곰이랑 집 둘다 괜찮은거 맞나??",
          "original_form": "북극곰이랑 집 둘다 괜찮은거 맞나??",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011633.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ응 아직은 괜찮아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ응 아직은 괜찮아",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011633.1.16",
          "form": "16살까진 몸집이 그대로더라구",
          "original_form": "16살까진 몸집이 그대로더라구",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011633.1.17",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011633.1.18",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011633.1.19",
          "form": "앞으론 어떻게될지 모르겠네",
          "original_form": "앞으론 어떻게될지 모르겠네",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011633.1.20",
          "form": "16살",
          "original_form": "16살",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011633.1.21",
          "form": "내 동생이랑 동갑이네",
          "original_form": "내 동생이랑 동갑이네",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011633.1.22",
          "form": "중2병은 안왔니?",
          "original_form": "중2병은 안왔니?",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011633.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011633.1.24",
          "form": "집 다 부쉈을줄 알았는데",
          "original_form": "집 다 부쉈을줄 알았는데",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011633.1.25",
          "form": "잘모르겠어..",
          "original_form": "잘모르겠어..",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011633.1.26",
          "form": "심하게 온건 아니라 다행인듯 하네",
          "original_form": "심하게 온건 아니라 다행인듯 하네",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011633.1.27",
          "form": "그러게 착한애라서 다행이야",
          "original_form": "그러게 착한애라서 다행이야",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011633.1.28",
          "form": "고슴도치는 키우면",
          "original_form": "고슴도치는 키우면",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011633.1.29",
          "form": "그거 가시고ㅓㄴ리는 어떻게해?",
          "original_form": "그거 가시고ㅓㄴ리는 어떻게해?",
          "speaker_id": "2",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011633.1.30",
          "form": "그 처음에는",
          "original_form": "그 처음에는",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011633.1.31",
          "form": "친해지면 나한테는 가시 안세움",
          "original_form": "친해지면 나한테는 가시 안세움",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011633.1.32",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011633.1.33",
          "form": "감동이다",
          "original_form": "감동이다",
          "speaker_id": "2",
          "time": "20210824 15:21"
        }
      ]
