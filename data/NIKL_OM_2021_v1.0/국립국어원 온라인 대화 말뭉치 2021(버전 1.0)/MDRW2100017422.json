      "utterance": [
        {
          "id": "MDRW2100017422.1.1",
          "form": "안녕하세요 저녁 뭐 드셨어요?",
          "original_form": "안녕하세요 저녁 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017422.1.2",
          "form": "죽먹었어요ㅎㅎ",
          "original_form": "죽먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017422.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 19:59"
        },
        {
          "id": "MDRW2100017422.1.4",
          "form": "저 방금 어니언베이글에 감자튀김 먹었어요ㅎㅎ",
          "original_form": "저 방금 어니언베이글에 감자튀김 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017422.1.5",
          "form": "우와....맛있었겠어요ㅜㅜ",
          "original_form": "우와....맛있었겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017422.1.6",
          "form": "감튀먹고싶네요ㅜ",
          "original_form": "감튀먹고싶네요ㅜ",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017422.1.7",
          "form": "감튀 맛있죵!ㅎㅎ",
          "original_form": "감튀 맛있죵!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017422.1.8",
          "form": "차가운 밀크쉐이크에 찍어먹는거 좋아해요",
          "original_form": "차가운 밀크쉐이크에 찍어먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017422.1.9",
          "form": "밀크쉐이크에 감튀면 단짠단짠인가요?!",
          "original_form": "밀크쉐이크에 감튀면 단짠단짠인가요?!",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017422.1.10",
          "form": "오 맞아요ㅋㅋ 상상해보면 그렇게 궁합이 좋아보이진 않는데",
          "original_form": "오 맞아요ㅋㅋ 상상해보면 그렇게 궁합이 좋아보이진 않는데",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017422.1.11",
          "form": "먹어보면 맛있어요ㅋㅋ",
          "original_form": "먹어보면 맛있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017422.1.12",
          "form": "ㅋㅋㅋㅋ맛있을 거 같아요",
          "original_form": "ㅋㅋㅋㅋ맛있을 거 같아요",
          "speaker_id": "2",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017422.1.13",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017422.1.14",
          "form": "소울푸드같은",
          "original_form": "소울푸드같은",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017422.1.15",
          "form": "소울프드...평양냉면...?",
          "original_form": "소울프드...평양냉면...?",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017422.1.16",
          "form": "오호 평양냉면이 약간 슴슴한 맛이죠?",
          "original_form": "오호 평양냉면이 약간 슴슴한 맛이죠?",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017422.1.17",
          "form": "전 좋아하는 음식이 너무 많아서 ㅜㅜ 소울푸드 딱 고르기가 어렵네요ㅜ",
          "original_form": "전 좋아하는 음식이 너무 많아서 ㅜㅜ 소울푸드 딱 고르기가 어렵네요ㅜ",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017422.1.18",
          "form": "네ㅋㅋ슴슴해요",
          "original_form": "네ㅋㅋ슴슴해요",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017422.1.19",
          "form": "지금 딱 먹고싶어서 얘기했어요ㅋㅋㅋ",
          "original_form": "지금 딱 먹고싶어서 얘기했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017422.1.20",
          "form": "육수 맛있는곳 찾기가 힘든데",
          "original_form": "육수 맛있는곳 찾기가 힘든데",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017422.1.21",
          "form": "맛있는곳은 또 기가막혀요ㅋㅋㅋ",
          "original_form": "맛있는곳은 또 기가막혀요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017422.1.22",
          "form": "저는 약간 겨자를 많이 넣는 스타일이라...",
          "original_form": "저는 약간 겨자를 많이 넣는 스타일이라...",
          "speaker_id": "2",
          "time": "20210826 20:35"
        }
      ]
