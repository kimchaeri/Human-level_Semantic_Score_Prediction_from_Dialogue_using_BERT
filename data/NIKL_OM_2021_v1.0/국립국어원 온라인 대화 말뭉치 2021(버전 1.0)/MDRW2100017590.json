      "utterance": [
        {
          "id": "MDRW2100017590.1.1",
          "form": "곰표 맥주 좋아하세요?",
          "original_form": "곰표 맥주 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017590.1.2",
          "form": "네 함번 마셔봣어요",
          "original_form": "네 함번 마셔봣어요",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017590.1.3",
          "form": "곰표나쵸 좋아해요",
          "original_form": "곰표나쵸 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017590.1.4",
          "form": "곰표나쵸는 안먹어봣어요",
          "original_form": "곰표나쵸는 안먹어봣어요",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017590.1.5",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017590.1.6",
          "form": "맛있어요 곰표맥주두 말표맥주두",
          "original_form": "맛있어요 곰표맥주두 말표맥주두",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017590.1.7",
          "form": "말표맥주도 안마셔봣어요",
          "original_form": "말표맥주도 안마셔봣어요",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017590.1.8",
          "form": "곰표랑 맛이 비슷해요?",
          "original_form": "곰표랑 맛이 비슷해요?",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017590.1.9",
          "form": "말표는 흑맥주라서 좀 달라용",
          "original_form": "말표는 흑맥주라서 좀 달라용",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017590.1.10",
          "form": "편의점 가서 사올까봐유",
          "original_form": "편의점 가서 사올까봐유",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017590.1.11",
          "form": "아 말표는 흑맥주예요? 몰랏네요",
          "original_form": "아 말표는 흑맥주예요? 몰랏네요",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017590.1.12",
          "form": "맞아용 !",
          "original_form": "맞아용 !",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017590.1.13",
          "form": "곰표 젤리도있던데",
          "original_form": "곰표 젤리도있던데",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017590.1.14",
          "form": "열일하네요",
          "original_form": "열일하네요",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017590.1.15",
          "form": "와 곰표젤리도 잇엇어요??",
          "original_form": "와 곰표젤리도 잇엇어요??",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017590.1.16",
          "form": "콜라보 제품이 많이나왓네요",
          "original_form": "콜라보 제품이 많이나왓네요",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017590.1.17",
          "form": "넹넹 맞아용",
          "original_form": "넹넹 맞아용",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017590.1.18",
          "form": "근데 곰표 원래 밀가루회사 아니에요?",
          "original_form": "근데 곰표 원래 밀가루회사 아니에요?",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017590.1.19",
          "form": "네 그렇게 알고 잇어요",
          "original_form": "네 그렇게 알고 잇어요",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017590.1.20",
          "form": "그쵸 정말 열일한다",
          "original_form": "그쵸 정말 열일한다",
          "speaker_id": "1",
          "time": "20210826 21:26"
        }
