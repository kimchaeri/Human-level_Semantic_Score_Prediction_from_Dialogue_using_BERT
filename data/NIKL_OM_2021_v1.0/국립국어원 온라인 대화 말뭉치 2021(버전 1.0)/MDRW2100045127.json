        {
          "id": "MDRW2100045127.1.1",
          "form": "빵이 젤 아침떼우기 좋더라구요@",
          "original_form": "빵이 젤 아침떼우기 좋더라구요@",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045127.1.2",
          "form": "아 아무래도 빵이 제일 편하죠?",
          "original_form": "아 아무래도 빵이 제일 편하죠?",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045127.1.3",
          "form": "네 ~ ㅎㅎ 간편",
          "original_form": "네 ~ ㅎㅎ 간편",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045127.1.4",
          "form": "애는 밥줘도 ㅠ 저는 빵으로",
          "original_form": "애는 밥줘도 ㅠ 저는 빵으로",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045127.1.5",
          "form": "식빵 사시나요?",
          "original_form": "식빵 사시나요?",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045127.1.6",
          "form": "ㅋㅋ 어머니가 더 잘 챙겨드셔야해여",
          "original_form": "ㅋㅋ 어머니가 더 잘 챙겨드셔야해여",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045127.1.7",
          "form": "모카빵 먹을려구요",
          "original_form": "모카빵 먹을려구요",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045127.1.8",
          "form": "식빵도 있긴한데 ㅎ",
          "original_form": "식빵도 있긴한데 ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045127.1.9",
          "form": "ㅎㅎㅎ 모카빵도 맛있졍",
          "original_form": "ㅎㅎㅎ 모카빵도 맛있졍",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045127.1.10",
          "form": "빵 뭐 좋아하세요??",
          "original_form": "빵 뭐 좋아하세요??",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045127.1.11",
          "form": "땅콩크림빵, 커피번,",
          "original_form": "땅콩크림빵, 커피번,",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045127.1.12",
          "form": "사라다ㅠ",
          "original_form": "사라다ㅠ",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045127.1.13",
          "form": "커피번 맛있는데",
          "original_form": "커피번 맛있는데",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045127.1.14",
          "form": "어제 아침엔 그거 먹었어여",
          "original_form": "어제 아침엔 그거 먹었어여",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045127.1.15",
          "form": "로티보이 맛있었던걸로 기억해요",
          "original_form": "로티보이 맛있었던걸로 기억해요",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045127.1.16",
          "form": "와 맛있으셨겠다!!",
          "original_form": "와 맛있으셨겠다!!",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045127.1.17",
          "form": "맛잇져ㅠㅠㅠ 그치만 근처없어서 파바ㅠㅠ",
          "original_form": "맛잇져ㅠㅠㅠ 그치만 근처없어서 파바ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045127.1.18",
          "form": "지점도 많이 없어진거 같아요",
          "original_form": "지점도 많이 없어진거 같아요",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045127.1.19",
