      "utterance": [
        {
          "id": "MDRW2100038406.1.1",
          "form": "자주 가는 파스타집 있어요 ㅎㅎㅎ",
          "original_form": "자주 가는 파스타집 있어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:59"
        },
        {
          "id": "MDRW2100038406.1.2",
          "form": "집이 대학가 근처라 엄청 식당이 많은 편이라",
          "original_form": "집이 대학가 근처라 엄청 식당이 많은 편이라",
          "speaker_id": "2",
          "time": "20210902 12:59"
        },
        {
          "id": "MDRW2100038406.1.3",
          "form": "골라가는 재미가 있어요",
          "original_form": "골라가는 재미가 있어요",
          "speaker_id": "2",
          "time": "20210902 12:59"
        },
        {
          "id": "MDRW2100038406.1.4",
          "form": "우와 부러워요ㅜ",
          "original_form": "우와 부러워요ㅜ",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100038406.1.5",
          "form": "저는 주택가 살긴하는데",
          "original_form": "저는 주택가 살긴하는데",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100038406.1.6",
          "form": "먹자골목이라고 하지만",
          "original_form": "먹자골목이라고 하지만",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100038406.1.7",
          "form": "실제론 먹을만한 식당은 손꼽을정도...?",
          "original_form": "실제론 먹을만한 식당은 손꼽을정도...?",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100038406.1.8",
          "form": "먹자골목 뭔가 소문난 잔치집에 뭐가 없는 느낌이에요ㅋㅋㅋ",
          "original_form": "먹자골목 뭔가 소문난 잔치집에 뭐가 없는 느낌이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038406.1.9",
          "form": "근데 대학가도 엄청 가게가 자주 바껴서",
          "original_form": "근데 대학가도 엄청 가게가 자주 바껴서",
          "speaker_id": "2",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100038406.1.10",
          "form": "좋아하는 가게도 몇개월뒤엔 없어져있고 그래요",
          "original_form": "좋아하는 가게도 몇개월뒤엔 없어져있고 그래요",
          "speaker_id": "2",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100038406.1.11",
          "form": "ㅎㅎㅎㅎ저 지금 오랜만에",
          "original_form": "ㅎㅎㅎㅎ저 지금 오랜만에",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038406.1.12",
          "form": "동네 맛집에 안주 먹으러왓는데",
          "original_form": "동네 맛집에 안주 먹으러왓는데",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038406.1.13",
          "form": "여전히 맛있어요ㅜㅜㅜ",
          "original_form": "여전히 맛있어요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038406.1.14",
          "form": "친구도 맛있다고 해요",
          "original_form": "친구도 맛있다고 해요",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038406.1.15",
          "form": "오꼬노미야끼!!",
          "original_form": "오꼬노미야끼!!",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038406.1.16",
          "form": "거기두 10시까지 영업이에요?",
          "original_form": "거기두 10시까지 영업이에요?",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038406.1.17",
          "form": "하ㅎㅎㅎ오랸만에 술 마시니",
          "original_form": "하ㅎㅎㅎ오랸만에 술 마시니",
          "speaker_id": "1",
          "time": "20210903 06:59"
        },
        {
          "id": "MDRW2100038406.1.18",
          "form": "소주 한병에 끝났네요ㅋㅋㅋ",
          "original_form": "소주 한병에 끝났네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 06:59"
        },
        {
          "id": "MDRW2100038406.1.19",
          "form": "네네 저희지역은 10시까지요!",
          "original_form": "네네 저희지역은 10시까지요!",
          "speaker_id": "1",
          "time": "20210903 06:59"
        },
        {
          "id": "MDRW2100038406.1.20",
          "form": "칼같이 나왓습니다ㅜ",
          "original_form": "칼같이 나왓습니다ㅜ",
          "speaker_id": "1",
          "time": "20210903 06:59"
        },
        {
          "id": "MDRW2100038406.1.21",
          "form": "ㅋㅋㅋㅋㅋ둘이서 한병이면 딱 적당하게 드셨네용",
          "original_form": "ㅋㅋㅋㅋㅋ둘이서 한병이면 딱 적당하게 드셨네용",
          "speaker_id": "2",
          "time": "20210903 09:26"
        },
        {
