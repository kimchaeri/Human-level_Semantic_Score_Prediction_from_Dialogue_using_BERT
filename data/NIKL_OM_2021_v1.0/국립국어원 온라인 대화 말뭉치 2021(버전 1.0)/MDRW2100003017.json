      "utterance": [
        {
          "id": "MDRW2100003017.1.1",
          "form": "여행을 자주 가시나요",
          "original_form": "여행을 자주 가시나요",
          "speaker_id": "2",
          "time": "20210820 17:36"
        },
        {
          "id": "MDRW2100003017.1.2",
          "form": "코로나 이후에는 별로 가보지못했어요ㅠㅠ해외나가고싶네요",
          "original_form": "코로나 이후에는 별로 가보지못했어요ㅠㅠ해외나가고싶네요",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100003017.1.3",
          "form": "기억에남는여행지있으신가요?",
          "original_form": "기억에남는여행지있으신가요?",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100003017.1.4",
          "form": "여수에 자주가요",
          "original_form": "여수에 자주가요",
          "speaker_id": "2",
          "time": "20210820 17:39"
        },
        {
          "id": "MDRW2100003017.1.5",
          "form": "여수 근처에 살고있어서요",
          "original_form": "여수 근처에 살고있어서요",
          "speaker_id": "2",
          "time": "20210820 17:39"
        },
        {
          "id": "MDRW2100003017.1.6",
          "form": "여수에 가보셨나요",
          "original_form": "여수에 가보셨나요",
          "speaker_id": "2",
          "time": "20210820 17:39"
        },
        {
          "id": "MDRW2100003017.1.7",
          "form": "저 여수에 가봤었어요! 여수 음식이 넘 맛나더라구요! 케이블카도 재미있었어요!",
          "original_form": "저 여수에 가봤었어요! 여수 음식이 넘 맛나더라구요! 케이블카도 재미있었어요!",
          "speaker_id": "1",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003017.1.8",
          "form": "여수가까이에 살고계시다니 부러워용",
          "original_form": "여수가까이에 살고계시다니 부러워용",
          "speaker_id": "1",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003017.1.9",
          "form": "예 바다를 자주 볼 수 있어서 좋아요",
          "original_form": "예 바다를 자주 볼 수 있어서 좋아요",
          "speaker_id": "2",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100003017.1.10",
          "form": "어디로 여행 가셨던게 기억에 남나요?",
          "original_form": "어디로 여행 가셨던게 기억에 남나요?",
          "speaker_id": "2",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100003017.1.11",
          "form": "저는 해외는 치앙마이 국내는 제주도요!",
          "original_form": "저는 해외는 치앙마이 국내는 제주도요!",
          "speaker_id": "1",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100003017.1.12",
          "form": "베트남인가요",
          "original_form": "베트남인가요",
          "speaker_id": "2",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100003017.1.13",
          "form": "태국이요! 태국은 어딜가나 다좋더라구요",
          "original_form": "태국이요! 태국은 어딜가나 다좋더라구요",
          "speaker_id": "1",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100003017.1.14",
          "form": "태국 가보고싶어요 팟타이를 먹어보고싶네요",
          "original_form": "태국 가보고싶어요 팟타이를 먹어보고싶네요",
          "speaker_id": "2",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100003017.1.15",
          "form": "태국에서 무슨 음식 드셨나요",
          "original_form": "태국에서 무슨 음식 드셨나요",
          "speaker_id": "2",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100003017.1.16",
          "form": "저는 과일이 싸서 과일도 많이 먹고 팟타이 엄청 먹었던것같아요ㅎㅎ",
          "original_form": "저는 과일이 싸서 과일도 많이 먹고 팟타이 엄청 먹었던것같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003017.1.17",
          "form": "권호국님은 해외여행 어디가좋으셨어요?",
          "original_form": "권호국님은 해외여행 어디가좋으셨어요?",
          "speaker_id": "1",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003017.1.18",
          "form": "저는 스위스가 좋았어요",
          "original_form": "저는 스위스가 좋았어요",
          "speaker_id": "2",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003017.1.19",
          "form": "패러글라이딩하고 유람선탔습니다",
          "original_form": "패러글라이딩하고 유람선탔습니다",
          "speaker_id": "2",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003017.1.20",
          "form": "와 스위스 정말좋네요! 저도 가보고싶어요!",
          "original_form": "와 스위스 정말좋네요! 저도 가보고싶어요!",
          "speaker_id": "1",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003017.1.21",
          "form": "코로나 끝나면 꼭 가보세요",
          "original_form": "코로나 끝나면 꼭 가보세요",
          "speaker_id": "2",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003017.1.22",
          "form": "패러글라이딩과 유람선 말만들어도 재미있겠어용!",
          "original_form": "패러글라이딩과 유람선 말만들어도 재미있겠어용!",
          "speaker_id": "1",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003017.1.23",
          "form": "정말 아름다운 곳이예요",
          "original_form": "정말 아름다운 곳이예요",
          "speaker_id": "2",
          "time": "20210820 17:46"
        }
      ]
    }
  ]
