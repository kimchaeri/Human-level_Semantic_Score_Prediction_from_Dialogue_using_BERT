      "utterance": [
        {
          "id": "MDRW2100026425.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026425.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026425.1.3",
          "form": "혹시 종교 있으세요??",
          "original_form": "혹시 종교 있으세요??",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026425.1.4",
          "form": "저는 무교ㅎㅎ",
          "original_form": "저는 무교ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026425.1.5",
          "form": "앗 저도 무교예요ㅋㅋ",
          "original_form": "앗 저도 무교예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026425.1.6",
          "form": "앗 그러시구나ㅋㅋ",
          "original_form": "앗 그러시구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026425.1.7",
          "form": "저는 예전에 성당다녔는데 안맞더라구요ㅋㅋ",
          "original_form": "저는 예전에 성당다녔는데 안맞더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026425.1.8",
          "form": "어릴 때 선물준대서 친구 교회 따라가본게 다였던...",
          "original_form": "어릴 때 선물준대서 친구 교회 따라가본게 다였던...",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026425.1.9",
          "form": "저도 교회 가보고 그랬는데 안맞더라구용",
          "original_form": "저도 교회 가보고 그랬는데 안맞더라구용",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026425.1.10",
          "form": "저는 주변에 절 성당 교회 다 있어서 가보긴 다 가봤어요ㅋㅋ",
          "original_form": "저는 주변에 절 성당 교회 다 있어서 가보긴 다 가봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026425.1.11",
          "form": "아 친구가 사이비 빠져서 사이비도 가봄...",
          "original_form": "아 친구가 사이비 빠져서 사이비도 가봄...",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026425.1.12",
          "form": "ㄷㄷ사이비까지요???",
          "original_form": "ㄷㄷ사이비까지요???",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026425.1.13",
          "form": "그곳은.. 어땠나요",
          "original_form": "그곳은.. 어땠나요",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026425.1.14",
          "form": "자꾸.. 제사를 하자 그래요...",
          "original_form": "자꾸.. 제사를 하자 그래요...",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026425.1.15",
          "form": "돈이 없으면 만원도 괜찮다 근데 이왕이면 돈 들여서 크게해라.. 이ㅡ",
          "original_form": "돈이 없으면 만원도 괜찮다 근데 이왕이면 돈 들여서 크게해라.. 이ㅡ",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026425.1.16",
          "form": "이럽니다..",
          "original_form": "이럽니다..",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026425.1.17",
          "form": "헐... 진짜 무섭네요 그런 곳 한번 빠지면 거의 못나온다던데",
          "original_form": "헐... 진짜 무섭네요 그런 곳 한번 빠지면 거의 못나온다던데",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026425.1.18",
          "form": "어떻게 친구도 빠져나오긴 했어요ㅋㅋㅋ",
          "original_form": "어떻게 친구도 빠져나오긴 했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026425.1.19",
          "form": "다행이네요ㅠㅠ 엄청 위험했겠어요",
          "original_form": "다행이네요ㅠㅠ 엄청 위험했겠어요",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026425.1.20",
          "form": "근데 진심 그동안은 다른 친구랑 셋이하던 모임통장 다 폐기하고",
          "original_form": "근데 진심 그동안은 다른 친구랑 셋이하던 모임통장 다 폐기하고",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026425.1.21",
          "form": "우리 걍 연락 끊어야 한다 이러고..",
          "original_form": "우리 걍 연락 끊어야 한다 이러고..",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026425.1.22",
          "form": "그럴만도하죠...",
          "original_form": "그럴만도하죠...",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026425.1.23",
          "form": "진짜 무서운게 친구가 사이비된 이유가 그거래요",
          "original_form": "진짜 무서운게 친구가 사이비된 이유가 그거래요",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026425.1.24",
          "form": "처음엔 계속 거절했는데",
          "original_form": "처음엔 계속 거절했는데",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026425.1.25",
          "form": "몇달동안 계속 문 두드리면서 한번만 해보라고",
          "original_form": "몇달동안 계속 문 두드리면서 한번만 해보라고",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026425.1.26",
          "form": "괴롭혀서 그래 함 해본다 내가 그래도 안넘어가나보자 이랬는데 사이비됨...",
          "original_form": "괴롭혀서 그래 함 해본다 내가 그래도 안넘어가나보자 이랬는데 사이비됨...",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026425.1.27",
          "form": "헐..... 진짜 무섭다",
          "original_form": "헐..... 진짜 무섭다",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026425.1.28",
          "form": "어떻게 지금은 빠져나왔는데",
          "original_form": "어떻게 지금은 빠져나왔는데",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026425.1.29",
          "form": "그때는 진심 큰일 나는 줄 알았어요",
          "original_form": "그때는 진심 큰일 나는 줄 알았어요",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026425.1.30",
          "form": "빠져나와서 다행임",
          "original_form": "빠져나와서 다행임",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026425.1.31",
          "form": "저 아는 사람도 당할 뻔했는데 막 무슨 옷을 갈아입고 제사를 해야한다 이랬대요 돈도 달라고 하고",
          "original_form": "저 아는 사람도 당할 뻔했는데 막 무슨 옷을 갈아입고 제사를 해야한다 이랬대요 돈도 달라고 하고",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026425.1.32",
          "form": "헐 맞아요 저도 친구가 자기 집에서 샤워하고 제사하자고 했대요",
          "original_form": "헐 맞아요 저도 친구가 자기 집에서 샤워하고 제사하자고 했대요",
          "speaker_id": "1",
          "time": "20210901 22:19"
        }
      ]
