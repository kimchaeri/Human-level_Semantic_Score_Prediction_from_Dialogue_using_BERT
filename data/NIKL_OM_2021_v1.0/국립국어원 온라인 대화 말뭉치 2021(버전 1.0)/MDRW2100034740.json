      "utterance": [
        {
          "id": "MDRW2100034740.1.1",
          "form": "커피도 좋아하시나요",
          "original_form": "커피도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100034740.1.2",
          "form": "좋아하는데 잠을 못자서 디카페인으로 마셔요",
          "original_form": "좋아하는데 잠을 못자서 디카페인으로 마셔요",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100034740.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100034740.1.4",
          "form": "저두 저녁에 먹을땐 디카페인으로 마셔요",
          "original_form": "저두 저녁에 먹을땐 디카페인으로 마셔요",
          "speaker_id": "2",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100034740.1.5",
          "form": "주말에는 아예 안먹고ㅌㅋㅋ",
          "original_form": "주말에는 아예 안먹고ㅌㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100034740.1.6",
          "form": "정신차리거 싶을때나 커피 마셔요",
          "original_form": "정신차리거 싶을때나 커피 마셔요",
          "speaker_id": "2",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100034740.1.7",
          "form": "아 못주무세요?",
          "original_form": "아 못주무세요?",
          "speaker_id": "1",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100034740.1.8",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100034740.1.9",
          "form": "계속 꾸준히 먹으면 내성 생기는데",
          "original_form": "계속 꾸준히 먹으면 내성 생기는데",
          "speaker_id": "2",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100034740.1.10",
          "form": "잠깐 끊었더니만",
          "original_form": "잠깐 끊었더니만",
          "speaker_id": "2",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100034740.1.11",
          "form": "반응이 오더라구요 ㅋㅋㅋ",
          "original_form": "반응이 오더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100034740.1.12",
          "form": "오그래요?",
          "original_form": "오그래요?",
          "speaker_id": "1",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100034740.1.13",
          "form": "저도 꾸준히 먹어봐야할까봐요 ㅋㅋㅋ",
          "original_form": "저도 꾸준히 먹어봐야할까봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100034740.1.14",
          "form": "디카페인이 있으니 안전한 방향으로 갑시다",
          "original_form": "디카페인이 있으니 안전한 방향으로 갑시다",
          "speaker_id": "2",
          "time": "20210828 18:37"
        },
        {
          "id": "MDRW2100034740.1.15",
          "form": "하 근데 파는곳이 너무 적어요 ㅠ",
          "original_form": "하 근데 파는곳이 너무 적어요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 18:38"
        },
        {
          "id": "MDRW2100034740.1.16",
          "form": "마자요 스벅이나 가야 디카페인 시키거 ㅠ",
          "original_form": "마자요 스벅이나 가야 디카페인 시키거 ㅠ",
          "speaker_id": "2",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100034740.1.17",
          "form": "맞아요 ㅜㅜ",
          "original_form": "맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100034740.1.18",
          "form": "많이 팔면 좋겠어요",
          "original_form": "많이 팔면 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100034740.1.19",
          "form": "최근에 소개팅때도 막 저녁에 아아 마시다가 밤을 지새곤했죠...ㅎㅎ..",
          "original_form": "최근에 소개팅때도 막 저녁에 아아 마시다가 밤을 지새곤했죠...ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100034740.1.20",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100034740.1.21",
          "form": "눈물나네요 약간..",
          "original_form": "눈물나네요 약간..",
          "speaker_id": "1",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100034740.1.22",
          "form": "ㅋㅋㅋㅋ그냥 차마실것을...",
          "original_form": "ㅋㅋㅋㅋ그냥 차마실것을...",
          "speaker_id": "2",
          "time": "20210828 18:42"
        }
