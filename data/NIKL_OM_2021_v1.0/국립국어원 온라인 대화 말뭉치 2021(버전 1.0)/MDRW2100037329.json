      "utterance": [
        {
          "id": "MDRW2100037329.1.1",
          "form": "네이버 메인에 그한쪽구석에 막 썸네일 뜨잖아요",
          "original_form": "네이버 메인에 그한쪽구석에 막 썸네일 뜨잖아요",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037329.1.2",
          "form": "거기 홀린게아닐까요",
          "original_form": "거기 홀린게아닐까요",
          "speaker_id": "1",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037329.1.3",
          "form": "아 가끔 그렇게 홀릴 때가 있죠",
          "original_form": "아 가끔 그렇게 홀릴 때가 있죠",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037329.1.4",
          "form": "그래도 결제단계서 정신이 들던데ㅜ",
          "original_form": "그래도 결제단계서 정신이 들던데ㅜ",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100037329.1.5",
          "form": "10개란걸 생각못한건지",
          "original_form": "10개란걸 생각못한건지",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100037329.1.6",
          "form": "겨우 이틀치란걸 생각못한건지",
          "original_form": "겨우 이틀치란걸 생각못한건지",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037329.1.7",
          "form": "저같음 최소 2주나 한달치라 생각했을 듯요ㅎ",
          "original_form": "저같음 최소 2주나 한달치라 생각했을 듯요ㅎ",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037329.1.8",
          "form": "저도 받고나서 어찌먹어야하나 검색해보고 가격보고 ㅋㅋ 와 내동생 돈많네 ^_^",
          "original_form": "저도 받고나서 어찌먹어야하나 검색해보고 가격보고 ㅋㅋ 와 내동생 돈많네 ^_^",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037329.1.9",
          "form": "정말로 48시간짜리를 팔줄이얃ㄷㄷㄷ",
          "original_form": "정말로 48시간짜리를 팔줄이얃ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100037329.1.10",
          "form": "같은 느낌이긴했어요",
          "original_form": "같은 느낌이긴했어요",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037329.1.11",
          "form": "동생이 부르주아군요ㅎㅎ",
          "original_form": "동생이 부르주아군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037329.1.12",
          "form": "저거하루에한개먹음10일치긴해요 ㅋㄱ",
          "original_form": "저거하루에한개먹음10일치긴해요 ㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037329.1.13",
          "form": "아닠ㅋ 그냥 모닝음료잖아요  그럼",
          "original_form": "아닠ㅋ 그냥 모닝음료잖아요  그럼",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037329.1.14",
          "form": "근데 저거먹고 저녁대신할바에야..",
          "original_form": "근데 저거먹고 저녁대신할바에야..",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037329.1.15",
          "form": "그냥 두유를하나먹겠다 ㅋㅋㅋ",
          "original_form": "그냥 두유를하나먹겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100037329.1.16",
          "form": "ㅎㅎ 두유는 영양이라도 풍부하죠ㅎㅎ",
          "original_form": "ㅎㅎ 두유는 영양이라도 풍부하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100037329.1.17",
          "form": "저 근데 좀 궁금하긴했거든요",
          "original_form": "저 근데 좀 궁금하긴했거든요",
          "speaker_id": "1",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100037329.1.18",
          "form": "그 썸네일볼때마다",
          "original_form": "그 썸네일볼때마다",
          "speaker_id": "1",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100037329.1.19",
          "form": "문구도 막 그런식이잖아요",
          "original_form": "문구도 막 그런식이잖아요",
          "speaker_id": "1",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100037329.1.20",
          "form": "요거하나먹고다이어트끝? 뭐이런식의",
