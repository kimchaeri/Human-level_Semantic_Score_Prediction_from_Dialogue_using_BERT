      "utterance": [
        {
          "id": "MDRW2100034165.1.1",
          "form": "지금 먹고왔어요!",
          "original_form": "지금 먹고왔어요!",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100034165.1.2",
          "form": "오리고기 넘 맛나네요 ㅎㅎ",
          "original_form": "오리고기 넘 맛나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100034165.1.3",
          "form": "맛있는거 드셨나요?",
          "original_form": "맛있는거 드셨나요?",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100034165.1.4",
          "form": "남은 닭볶음탕 데워서 먹었어요!",
          "original_form": "남은 닭볶음탕 데워서 먹었어요!",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100034165.1.5",
          "form": "오리고기 드셨군요! 구워드신 건가요?",
          "original_form": "오리고기 드셨군요! 구워드신 건가요?",
          "speaker_id": "1",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100034165.1.6",
          "form": "네!! 전 구워먹는게 젤 좋더라고요 ㅎㅎ",
          "original_form": "네!! 전 구워먹는게 젤 좋더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100034165.1.7",
          "form": "소스는 뭐 찍어드세요?",
          "original_form": "소스는 뭐 찍어드세요?",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100034165.1.8",
          "form": "보통 소스ㄴ없이먹어요",
          "original_form": "보통 소스ㄴ없이먹어요",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100034165.1.9",
          "form": "머스타드있으면 그거 먹어도 되긴하는데",
          "original_form": "머스타드있으면 그거 먹어도 되긴하는데",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100034165.1.10",
          "form": "허니머스타드밖에 없어서 자제했죠 ㅎ..",
          "original_form": "허니머스타드밖에 없어서 자제했죠 ㅎ..",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100034165.1.11",
          "form": "저도 소스 없이 쌈무랑 먹는 걸 더 좋아해요",
          "original_form": "저도 소스 없이 쌈무랑 먹는 걸 더 좋아해요",
          "speaker_id": "1",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100034165.1.12",
          "form": "쌈무 좋아하세요?",
          "original_form": "쌈무 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100034165.1.13",
          "form": "쌈무 좋아해요?",
          "original_form": "쌈무 좋아해요?",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100034165.1.14",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100034165.1.15",
          "form": "상큼하고~ 시원하고",
          "original_form": "상큼하고~ 시원하고",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100034165.1.16",
          "form": "네 새콤달콤해서 좋아해요!",
          "original_form": "네 새콤달콤해서 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100034165.1.17",
          "form": "다른 고기에도 쌈무랑 자주 먹어요",
          "original_form": "다른 고기에도 쌈무랑 자주 먹어요",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100034165.1.18",
          "form": "무슨 고기 제일 좋아하세요?",
          "original_form": "무슨 고기 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100034165.1.19",
          "form": "고기는 닭고기를 제일 좋아하는 거 같아요",
          "original_form": "고기는 닭고기를 제일 좋아하는 거 같아요",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034165.1.20",
          "form": "정말 닭킬러시군요 ㅋㅋㅋㅋ",
          "original_form": "정말 닭킬러시군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100034165.1.21",
          "form": "닭.. 버릴곳이없죠",
          "original_form": "닭.. 버릴곳이없죠",
          "speaker_id": "2",
          "time": "20210828 01:16"
        },
        {
          "id": "MDRW2100034165.1.22",
          "form": "닭발도 좋아하새요?",
          "original_form": "닭발도 좋아하새요?",
