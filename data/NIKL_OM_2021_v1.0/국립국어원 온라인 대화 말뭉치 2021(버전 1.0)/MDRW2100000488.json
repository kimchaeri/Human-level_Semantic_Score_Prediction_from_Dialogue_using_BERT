        {
          "id": "MDRW2100000488.1.1",
          "form": "요즘 날씨 좋지않나요",
          "original_form": "요즘 날씨 좋지않나요",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000488.1.2",
          "form": "어제 비왓는데요?",
          "original_form": "어제 비왓는데요?",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000488.1.3",
          "form": "더운게 사라졌잖아요",
          "original_form": "더운게 사라졌잖아요",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000488.1.4",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000488.1.5",
          "form": "밥엔 춥던데",
          "original_form": "밥엔 춥던데",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000488.1.6",
          "form": "이제 바이크 웃옷 입고 타야겠네요",
          "original_form": "이제 바이크 웃옷 입고 타야겠네요",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000488.1.7",
          "form": "밥이요?",
          "original_form": "밥이요?",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000488.1.8",
          "form": "가죽자켓입고 타주세요",
          "original_form": "가죽자켓입고 타주세요",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000488.1.9",
          "form": "대답좀해주세요",
          "original_form": "대답좀해주세요",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000488.1.10",
          "form": "저만 기다리고 있나요??",
          "original_form": "저만 기다리고 있나요??",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000488.1.11",
          "form": "ㅋㅋㅋㅋㅋ바쁘잖아요",
          "original_form": "ㅋㅋㅋㅋㅋ바쁘잖아요",
          "speaker_id": "2",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000488.1.12",
          "form": "가죽자켓",
          "original_form": "가죽자켓",
          "speaker_id": "2",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000488.1.13",
          "form": "10월에 갑니다",
          "original_form": "10월에 갑니다",
          "speaker_id": "2",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000488.1.14",
          "form": "9월은 왜안돼요",
          "original_form": "9월은 왜안돼요",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000488.1.15",
          "form": "덜 춥잖아요",
          "original_form": "덜 춥잖아요",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
