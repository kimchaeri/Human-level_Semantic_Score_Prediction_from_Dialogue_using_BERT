          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017712.1.3",
          "form": "전 양념치킨이요",
          "original_form": "전 양념치킨이요",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017712.1.4",
          "form": "블랙알리오도 먹고싶네요",
          "original_form": "블랙알리오도 먹고싶네요",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017712.1.5",
          "form": "배고파요ㅜㅜ",
          "original_form": "배고파요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017712.1.6",
          "form": "저두요ㅜㅜ",
          "original_form": "저두요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017712.1.7",
          "form": "어떤 브랜드 좋아하세요?",
          "original_form": "어떤 브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017712.1.8",
          "form": "전 푸라닭이요",
          "original_form": "전 푸라닭이요",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017712.1.9",
          "form": "전 멕시카나 땡겨요",
          "original_form": "전 멕시카나 땡겨요",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017712.1.10",
          "form": "프라이드치킨은 비비큐죠",
          "original_form": "프라이드치킨은 비비큐죠",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017712.1.11",
          "form": "그쵸 아주커도 맛있어요",
          "original_form": "그쵸 아주커도 맛있어요",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017712.1.12",
          "form": "아 진짜 배고프네요",
          "original_form": "아 진짜 배고프네요",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017712.1.13",
          "form": "네 배고파요",
