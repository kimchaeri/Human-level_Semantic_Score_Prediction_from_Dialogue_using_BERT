      "utterance": [
        {
          "id": "MDRW2100030102.1.1",
          "form": "요즘 알바하고 있어요",
          "original_form": "요즘 알바하고 있어요",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100030102.1.2",
          "form": "저도요 쓰리잡뛰어요",
          "original_form": "저도요 쓰리잡뛰어요",
          "speaker_id": "2",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100030102.1.3",
          "form": "무척 활기찬 삶을 살고 계시네요",
          "original_form": "무척 활기찬 삶을 살고 계시네요",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100030102.1.4",
          "form": "네 많이 힘들어요ㅜ",
          "original_form": "네 많이 힘들어요ㅜ",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100030102.1.5",
          "form": "넹 그만큼 보람이 있겠죠ㅎ",
          "original_form": "넹 그만큼 보람이 있겠죠ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030102.1.6",
          "form": "음 경제적 자유를 누리고 싶네요",
          "original_form": "음 경제적 자유를 누리고 싶네요",
          "speaker_id": "2",
          "time": "20210904 07:41"
        },
        {
          "id": "MDRW2100030102.1.7",
          "form": "저도요 얼른 자유롭게",
          "original_form": "저도요 얼른 자유롭게",
          "speaker_id": "1",
          "time": "20210904 07:50"
        },
        {
          "id": "MDRW2100030102.1.8",
          "form": "그럴수록 일을 열심히해야죠",
          "original_form": "그럴수록 일을 열심히해야죠",
          "speaker_id": "2",
          "time": "20210904 08:47"
        },
        {
          "id": "MDRW2100030102.1.9",
          "form": "넹 직장보다 다른거에 관심 갖게 되네요",
          "original_form": "넹 직장보다 다른거에 관심 갖게 되네요",
          "speaker_id": "1",
          "time": "20210904 08:48"
        },
        {
          "id": "MDRW2100030102.1.10",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210904 08:48"
        },
        {
          "id": "MDRW2100030102.1.11",
          "form": "재테크요",
          "original_form": "재테크요",
          "speaker_id": "1",
          "time": "20210904 08:48"
        },
        {
          "id": "MDRW2100030102.1.12",
          "form": "재테크는 필수죠",
          "original_form": "재테크는 필수죠",
          "speaker_id": "2",
          "time": "20210904 08:49"
        },
        {
          "id": "MDRW2100030102.1.13",
          "form": "넹 이제 안정적 수입으로 투자 좀 해보려구요",
          "original_form": "넹 이제 안정적 수입으로 투자 좀 해보려구요",
          "speaker_id": "1",
          "time": "20210904 08:50"
        },
        {
          "id": "MDRW2100030102.1.14",
          "form": "수입은 안정적이신가요",
          "original_form": "수입은 안정적이신가요",
          "speaker_id": "2",
          "time": "20210904 08:50"
        },
        {
          "id": "MDRW2100030102.1.15",
          "form": "요즘 아르바이트가 스트레스네요",
          "original_form": "요즘 아르바이트가 스트레스네요",
