      "utterance": [
        {
          "id": "MDRW2100015622.1.1",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015622.1.2",
