          "speaker_id": "2",
          "time": "20210903 11:43"
        },
        {
          "id": "MDRW2100029691.1.2",
          "form": "전 소주파입니다",
          "original_form": "전 소주파입니다",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029691.1.3",
          "form": "소주만먹습니다 ㅋㅋ",
          "original_form": "소주만먹습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029691.1.4",
          "form": "이야기했나요? 저는 640미리만 사유",
          "original_form": "이야기했나요? 저는 640미리만 사유",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029691.1.5",
          "form": "640 미리 얘기하셨쥬ㅎㅎ",
          "original_form": "640 미리 얘기하셨쥬ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029691.1.6",
          "form": "혹시 담금주를 물처럼 마시지는 않남요?",
          "original_form": "혹시 담금주를 물처럼 마시지는 않남요?",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029691.1.7",
          "form": "담",
          "original_form": "담",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029691.1.8",
          "form": "금",
          "original_form": "금",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029691.1.9",
          "form": "주",
          "original_form": "주",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029691.1.10",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029691.1.11",
          "form": "회 쏘는 지금하나요",
          "original_form": "회 쏘는 지금하나요",
          "speaker_id": "2",
          "time": "20210903 12:07"
        },
        {
          "id": "MDRW2100029691.1.12",
          "form": "진진하게 고민중입니다",
          "original_form": "진진하게 고민중입니다",
          "speaker_id": "1",
          "time": "20210903 12:14"
        },
        {
          "id": "MDRW2100029691.1.13",
          "form": "광어 멍게 새우튀김 혼술세트냐",
          "original_form": "광어 멍게 새우튀김 혼술세트냐",
          "speaker_id": "1",
          "time": "20210903 12:14"
        },
        {
          "id": "MDRW2100029691.1.14",
          "form": "멍게 개불 새우 소라 해산물 세트냐",
          "original_form": "멍게 개불 새우 소라 해산물 세트냐",
          "speaker_id": "1",
          "time": "20210903 12:14"
        },
        {
          "id": "MDRW2100029691.1.15",
          "form": "전어냐",
          "original_form": "전어냐",
          "speaker_id": "1",
          "time": "20210903 12:14"
        },
        {
          "id": "MDRW2100029691.1.16",
          "form": "햄릿보다 더 어려운 고민에 직면했어요",
          "original_form": "햄릿보다 더 어려운 고민에 직면했어요",
          "speaker_id": "1",
          "time": "20210903 12:16"
        },
        {
          "id": "MDRW2100029691.1.17",
          "form": "저라면",
          "original_form": "저라면",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029691.1.18",
          "form": "멍게는 포기 안하겠습니다",
          "original_form": "멍게는 포기 안하겠습니다",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029691.1.19",
          "form": "지당하신 말씀ㅎ 맨위로 막 주문했습니다!",
          "original_form": "지당하신 말씀ㅎ 맨위로 막 주문했습니다!",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100029691.1.20",
          "form": "개불도 좋아해서 고민이 컸어요ㅎㅎ",
          "original_form": "개불도 좋아해서 고민이 컸어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100029691.1.21",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100029691.1.22",
          "form": "사나이 가슴에 불을 지피네요",
          "original_form": "사나이 가슴에 불을 지피네요",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100029691.1.23",
          "form": "너무 부릅습닏ㄴ",
          "original_form": "너무 부릅습닏ㄴ",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100029691.1.24",
          "form": "ㅋㅋ 진짜 벌써부터 위가 두근거려욪ㅎ",
          "original_form": "ㅋㅋ 진짜 벌써부터 위가 두근거려욪ㅎ",
          "speaker_id": "1",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100029691.1.25",
          "form": "님은 뭐 드셨나요!",
          "original_form": "님은 뭐 드셨나요!",
          "speaker_id": "1",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100029691.1.26",
          "form": "전 혀밑이",
          "original_form": "전 혀밑이",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029691.1.27",
          "form": "까슬까슬해지네요",
          "original_form": "까슬까슬해지네요",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029691.1.28",
          "form": "멍게생각하니까",
          "original_form": "멍게생각하니까",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029691.1.29",
          "form": "저는 참치야채비빔밥",
          "original_form": "저는 참치야채비빔밥",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029691.1.30",
          "form": "식당밥먹엇서요",
          "original_form": "식당밥먹엇서요",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029691.1.31",
          "form": "핵노맛",
          "original_form": "핵노맛",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029691.1.32",
          "form": "어...",
          "original_form": "어...",
          "speaker_id": "1",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029691.1.33",
          "form": "참치가 통조림은 아니죠?",
          "original_form": "참치가 통조림은 아니죠?",
          "speaker_id": "1",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029691.1.34",
          "form": "통조림입니다",
          "original_form": "통조림입니다",
          "speaker_id": "2",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100029691.1.35",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100029691.1.36",
          "form": "참치향비빔밥임",
          "original_form": "참치향비빔밥임",
          "speaker_id": "2",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100029691.1.37",
          "form": "아ㅎㅎㅎ",
          "original_form": "아ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100029691.1.38",
          "form": "잘 먹이지도 않고 일을 시키다니",
          "original_form": "잘 먹이지도 않고 일을 시키다니",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100029691.1.39",
          "form": "국가에 실망했습니다",
          "original_form": "국가에 실망했습니다",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100029691.1.40",
          "form": "전 오늘부터 무정부주의자입니다",
          "original_form": "전 오늘부터 무정부주의자입니다",
          "speaker_id": "1",
          "time": "20210903 12:59"
        }
