      "utterance": [
        {
          "id": "MDRW2100001679.1.1",
          "form": "다행이네 미리 말했으면 실망했을것 같은데",
          "original_form": "다행이네 미리 말했으면 실망했을것 같은데",
          "speaker_id": "2",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100001679.1.2",
          "form": "응 그래서 그냥 패쓰 대신 줄넘기 토요일에가",
          "original_form": "응 그래서 그냥 패쓰 대신 줄넘기 토요일에가",
          "speaker_id": "1",
          "time": "20210819 18:02"
        },
        {
          "id": "MDRW2100001679.1.3",
          "form": "근데 비안오면?",
          "original_form": "근데 비안오면?",
          "speaker_id": "2",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100001679.1.4",
          "form": "그래도 안갈거지?",
          "original_form": "그래도 안갈거지?",
          "speaker_id": "2",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100001679.1.5",
          "form": "응 집에 있을꺼야  애들 치킨 픽업해왔어",
          "original_form": "응 집에 있을꺼야  애들 치킨 픽업해왔어",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001679.1.6",
          "form": "응 맛나게 먹어",
          "original_form": "응 맛나게 먹어",
          "speaker_id": "2",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001679.1.7",
          "form": "도착했나?",
          "original_form": "도착했나?",
          "speaker_id": "1",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100001679.1.8",
          "form": "아직 퇴근하는중",
          "original_form": "아직 퇴근하는중",
          "speaker_id": "2",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100001679.1.9",
          "form": "주차하고 바로 병원가야지",
          "original_form": "주차하고 바로 병원가야지",
          "speaker_id": "2",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100001679.1.10",
          "form": "응 조심히 다녀와",
          "original_form": "응 조심히 다녀와",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001679.1.11",
          "form": "비  조금씩온다",
          "original_form": "비  조금씩온다",
          "speaker_id": "2",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100001679.1.12",
          "form": "한방울씩",
          "original_form": "한방울씩",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100001679.1.13",
          "form": "그래도 많이 안왔나보네",
          "original_form": "그래도 많이 안왔나보네",
          "speaker_id": "1",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100001679.1.14",
          "form": "저녁안 먹고 왔나보네",
          "original_form": "저녁안 먹고 왔나보네",
          "speaker_id": "1",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100001679.1.15",
          "form": "바뿌나 일찍자고 내일 연락해",
          "original_form": "바뿌나 일찍자고 내일 연락해",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100001679.1.16",
          "form": "오늘 늦어?",
          "original_form": "오늘 늦어?",
          "speaker_id": "1",
          "time": "20210823 14:37"
        },
        {
          "id": "MDRW2100001679.1.17",
          "form": "모르지",
          "original_form": "모르지",
          "speaker_id": "2",
          "time": "20210823 14:38"
        },
        {
          "id": "MDRW2100001679.1.18",
          "form": "5시에 선변연락없으면 일찍 가는거고",
          "original_form": "5시에 선변연락없으면 일찍 가는거고",
          "speaker_id": "2",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100001679.1.19",
          "form": "5시30분",
          "original_form": "5시30분",
          "speaker_id": "2",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100001679.1.20",
          "form": "내일 당직이야",
          "original_form": "내일 당직이야",
          "speaker_id": "2",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100001679.1.21",
          "form": "지금 다섯시 넘었는데 아직 모르나",
          "original_form": "지금 다섯시 넘었는데 아직 모르나",
          "speaker_id": "1",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100001679.1.22",
          "form": "일찍 퇴근할것 같은데",
          "original_form": "일찍 퇴근할것 같은데",
          "speaker_id": "2",
          "time": "20210823 17:45"
        }
