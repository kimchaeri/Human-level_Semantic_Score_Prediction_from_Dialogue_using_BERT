      "utterance": [
        {
          "id": "MDRW2100006167.1.1",
          "form": "와이프는",
          "original_form": "와이프는",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006167.1.2",
          "form": "2차 다 맞아서...",
          "original_form": "2차 다 맞아서...",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006167.1.3",
          "form": "일찍 맞으셨네요",
          "original_form": "일찍 맞으셨네요",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006167.1.4",
          "form": "서울시에서 갑자기 대중교통 종사자",
          "original_form": "서울시에서 갑자기 대중교통 종사자",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006167.1.5",
          "form": "백신줘서 ㅎㅎ",
          "original_form": "백신줘서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006167.1.6",
          "form": "무섭다고 예약 안 했다는 친구도 있는데.. 그냥 그렇구나 했어요ㅋㅋ",
          "original_form": "무섭다고 예약 안 했다는 친구도 있는데.. 그냥 그렇구나 했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006167.1.7",
          "form": "ㅋㅋ 에이 괜찮을거에요",
          "original_form": "ㅋㅋ 에이 괜찮을거에요",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006167.1.8",
          "form": "걱정이 많더라구요ㅋㅋ",
          "original_form": "걱정이 많더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100006167.1.9",
          "form": "돈워리",
          "original_form": "돈워리",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006167.1.10",
          "form": "코로나가 더 무섭죠",
          "original_form": "코로나가 더 무섭죠",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006167.1.11",
          "form": "거기다 대고 뭐라고 하기도 그래서 말았지만요ㅋㅋ",
          "original_form": "거기다 대고 뭐라고 하기도 그래서 말았지만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006167.1.12",
          "form": "네 그냥 두세요 ㅎㅎ",
          "original_form": "네 그냥 두세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100006167.1.13",
          "form": "저는 빨리 다 맞아버리고 싶네요ㅋㅋ",
          "original_form": "저는 빨리 다 맞아버리고 싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100006167.1.14",
          "form": "그게 마음 편하죠",
          "original_form": "그게 마음 편하죠",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006167.1.15",
          "form": "최대한 빠른 날로 한 건데도 거의 한달만에 맞네요",
          "original_form": "최대한 빠른 날로 한 건데도 거의 한달만에 맞네요",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006167.1.16",
          "form": "ㅎㅎ그래도 맞는게",
          "original_form": "ㅎㅎ그래도 맞는게",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100006167.1.17",
          "form": "다행이죠",
          "original_form": "다행이죠",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100006167.1.18",
