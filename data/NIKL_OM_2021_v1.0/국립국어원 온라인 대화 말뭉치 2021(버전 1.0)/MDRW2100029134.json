      "utterance": [
        {
          "id": "MDRW2100029134.1.1",
          "form": "사실 아닌듯 해요 ㅋㅋㅋ",
          "original_form": "사실 아닌듯 해요 ㅋㅋㅋ",
          "speaker_id": "1",
