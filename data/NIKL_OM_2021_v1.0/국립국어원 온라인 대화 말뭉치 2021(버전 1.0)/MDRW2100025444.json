        },
        {
          "id": "MDRW2100025444.1.2",
          "form": "메타버스인가요?",
          "original_form": "메타버스인가요?",
          "speaker_id": "2",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100025444.1.3",
          "form": "네 메타버스네요",
          "original_form": "네 메타버스네요",
          "speaker_id": "1",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100025444.1.4",
          "form": "vr이죠",
          "original_form": "vr이죠",
          "speaker_id": "1",
          "time": "20210901 11:31"
        },
        {
          "id": "MDRW2100025444.1.5",
          "form": "VR",
          "original_form": "VR",
          "speaker_id": "2",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025444.1.6",
          "form": "vr게임 다이소에서 팔던데",
          "original_form": "vr게임 다이소에서 팔던데",
          "speaker_id": "1",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025444.1.7",
          "form": "눈에 쓰는거ㅋㅋ",
          "original_form": "눈에 쓰는거ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025444.1.8",
          "form": "그게 바로 메타버스",
          "original_form": "그게 바로 메타버스",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025444.1.9",
          "form": "버디버디 아세요?",
          "original_form": "버디버디 아세요?",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025444.1.10",
          "form": "위메이드라는 회사에서 만든 건데",
          "original_form": "위메이드라는 회사에서 만든 건데",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025444.1.11",
          "form": "채팅이요",
          "original_form": "채팅이요",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025444.1.12",
          "form": "메신저 > 보이스채팅 > 메타버스로 발전 시켜 나갈 계획이라고 함",
          "original_form": "메신저 > 보이스채팅 > 메타버스로 발전 시켜 나갈 계획이라고 함",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025444.1.13",
          "form": "올해안에 재개한대요",
          "original_form": "올해안에 재개한대요",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025444.1.14",
          "form": "와 채팅이랑 메타버스요?",
          "original_form": "와 채팅이랑 메타버스요?",
          "speaker_id": "1",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025444.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025444.1.16",
          "form": "채팅이랑 메타버스 결합되면",
          "original_form": "채팅이랑 메타버스 결합되면",
          "speaker_id": "2",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025444.1.17",
          "form": "기존 메신저들의 파이를 뺏아올수 있을까요",
          "original_form": "기존 메신저들의 파이를 뺏아올수 있을까요",
          "speaker_id": "2",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025444.1.18",
          "form": "엄청나네요",
          "original_form": "엄청나네요",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100025444.1.19",
          "form": "가상현실에서 채팅하는건가요?",
          "original_form": "가상현실에서 채팅하는건가요?",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025444.1.20",
          "form": "그러게요 기대돼요",
          "original_form": "그러게요 기대돼요",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025444.1.21",
          "form": "대화배경을 바꾸는건가?",
          "original_form": "대화배경을 바꾸는건가?",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025444.1.22",
          "form": "카페",
          "original_form": "카페",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025444.1.23",
          "form": "백두산정상",
          "original_form": "백두산정상",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025444.1.24",
          "form": "이렇게?",
          "original_form": "이렇게?",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025444.1.25",
          "form": "오오 그런건가요",
          "original_form": "오오 그런건가요",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025444.1.26",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025444.1.27",
          "form": "그럼 진짜 좋을듯해요",
          "original_form": "그럼 진짜 좋을듯해요",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025444.1.28",
          "form": "다른 나라도 될까요?",
          "original_form": "다른 나라도 될까요?",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025444.1.29",
          "form": "코로나라서 여행도 못가는데",
          "original_form": "코로나라서 여행도 못가는데",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025444.1.30",
          "form": "메타버스로 여행하는 기분낼수 있겠네요",
          "original_form": "메타버스로 여행하는 기분낼수 있겠네요",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025444.1.31",
          "form": "음 괜찮은 사업아이템인거 같네요",
          "original_form": "음 괜찮은 사업아이템인거 같네요",
          "speaker_id": "1",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100025444.1.32",
          "form": "도쿄타워로 설정하고 대화",
          "original_form": "도쿄타워로 설정하고 대화",
          "speaker_id": "1",
          "time": "20210901 11:53"
        }
      ]
