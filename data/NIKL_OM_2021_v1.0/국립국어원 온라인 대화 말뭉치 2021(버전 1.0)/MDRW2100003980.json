      "utterance": [
        {
          "id": "MDRW2100003980.1.1",
          "form": "머하고 있었어?",
          "original_form": "머하고 있었어?",
          "speaker_id": "1",
          "time": "20210821 14:24"
        },
        {
          "id": "MDRW2100003980.1.2",
          "form": "밥먹고 커피마시고 있었어",
          "original_form": "밥먹고 커피마시고 있었어",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.3",
          "form": "혹시 학생이야?",
          "original_form": "혹시 학생이야?",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.4",
          "form": "오오 대박 토욜인데 좀 부지런하다",
          "original_form": "오오 대박 토욜인데 좀 부지런하다",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.5",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.6",
          "form": "근데 나는 이제 졸업해",
          "original_form": "근데 나는 이제 졸업해",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.7",
          "form": "너는?!",
          "original_form": "너는?!",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.8",
          "form": "나는...대학교 3학년이야",
          "original_form": "나는...대학교 3학년이야",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.10",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.11",
          "form": "비슷..할듯",
          "original_form": "비슷..할듯",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.12",
          "form": "혹시 과가 뭐야?",
          "original_form": "혹시 과가 뭐야?",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.13",
          "form": "나는 영상과야",
          "original_form": "나는 영상과야",
          "speaker_id": "2",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003980.1.14",
          "form": "너는 ?",
          "original_form": "너는 ?",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.15",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.16",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.17",
          "form": "나는 문화콘텐츠",
          "original_form": "나는 문화콘텐츠",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.18",
          "form": "아니 멋있어서",
          "original_form": "아니 멋있어서",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.19",
          "form": "오~~",
          "original_form": "오~~",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.20",
          "form": "CG쪽 배워?!",
          "original_form": "CG쪽 배워?!",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.21",
          "form": "약간 비슷하네",
          "original_form": "약간 비슷하네",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.22",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.23",
          "form": "광고쪽 준비중이야?",
          "original_form": "광고쪽 준비중이야?",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.24",
          "form": "배우긴하는데 잘 못해 ㅋㅋ",
          "original_form": "배우긴하는데 잘 못해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.25",
          "form": "약간 나 물음표 살인마같다 지금",
          "original_form": "약간 나 물음표 살인마같다 지금",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.26",
          "form": "아냐 잘할거같애",
          "original_form": "아냐 잘할거같애",
          "speaker_id": "1",
          "time": "20210821 14:26"
        },
        {
          "id": "MDRW2100003980.1.27",
          "form": "삘이와",
          "original_form": "삘이와",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003980.1.28",
          "form": "영상 안에서도 분야가 하도 많으니까",
          "original_form": "영상 안에서도 분야가 하도 많으니까",
          "speaker_id": "2",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003980.1.29",
          "form": "나는 2D 모션그래픽이나 애니메이션같은거 주로 하고있어 ㅋㅋ",
          "original_form": "나는 2D 모션그래픽이나 애니메이션같은거 주로 하고있어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003980.1.30",
          "form": "헐!!!",
          "original_form": "헐!!!",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003980.1.31",
          "form": "3D는 너무어렵다... ㅎㅎ",
          "original_form": "3D는 너무어렵다... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003980.1.32",
          "form": "햐 너무 멋지다",
          "original_form": "햐 너무 멋지다",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003980.1.33",
          "form": "2D도 넘 어렵지 않아?!",
          "original_form": "2D도 넘 어렵지 않아?!",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003980.1.34",
          "form": "너는 문화콘텐츠면 어떤거 배워?",
          "original_form": "너는 문화콘텐츠면 어떤거 배워?",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003980.1.35",
          "form": "맞아..사실 다 어려워ㅠ",
          "original_form": "맞아..사실 다 어려워ㅠ",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003980.1.36",
          "form": "나는 약간 음..국문학과랑 비슷한거같아",
          "original_form": "나는 약간 음..국문학과랑 비슷한거같아",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003980.1.37",
          "form": "뭔가 스토리텔링이랑",
          "original_form": "뭔가 스토리텔링이랑",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003980.1.38",
          "form": "걍 다 배우는 거 같아",
          "original_form": "걍 다 배우는 거 같아",
          "speaker_id": "1",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003980.1.39",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210821 14:28"
        },
        {
          "id": "MDRW2100003980.1.40",
          "form": "근데 그거 알지 애매하게 쫌쫌따리로 배워서",
          "original_form": "근데 그거 알지 애매하게 쫌쫌따리로 배워서",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003980.1.41",
          "form": "다 애매하게 못하는거",
          "original_form": "다 애매하게 못하는거",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003980.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003980.1.43",
          "form": "ㅋㅋㅋㅋㅋ나도그래ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ나도그래ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003980.1.44",
          "form": "영상 3학년이면 슬슬 진로 정했겠네?!",
          "original_form": "영상 3학년이면 슬슬 진로 정했겠네?!",
