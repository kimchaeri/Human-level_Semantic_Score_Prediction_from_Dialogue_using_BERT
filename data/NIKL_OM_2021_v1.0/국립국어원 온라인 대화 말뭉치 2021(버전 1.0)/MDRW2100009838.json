      "utterance": [
        {
          "id": "MDRW2100009838.1.1",
          "form": "중고거래 자주 하세요?",
          "original_form": "중고거래 자주 하세요?",
          "speaker_id": "1",
          "time": "20210823 10:45"
        },
        {
          "id": "MDRW2100009838.1.2",
          "form": "네 중고책 주로 팔아요",
          "original_form": "네 중고책 주로 팔아요",
          "speaker_id": "2",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100009838.1.3",
          "form": "오 책이 많으신가봐요",
          "original_form": "오 책이 많으신가봐요",
          "speaker_id": "1",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100009838.1.4",
          "form": "중고로 뭘 사진 않으세요?",
          "original_form": "중고로 뭘 사진 않으세요?",
          "speaker_id": "1",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100009838.1.5",
          "form": "음 책말곤 딱히 사는건 없는거 같은데",
          "original_form": "음 책말곤 딱히 사는건 없는거 같은데",
          "speaker_id": "2",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100009838.1.6",
          "form": "어떤걸 주로 사세요?",
          "original_form": "어떤걸 주로 사세요?",
          "speaker_id": "2",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100009838.1.7",
          "form": "저는 그때 마다 다른거 같아요",
          "original_form": "저는 그때 마다 다른거 같아요",
          "speaker_id": "1",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100009838.1.8",
          "form": "최근에 구매하신 물건이 있나요?",
          "original_form": "최근에 구매하신 물건이 있나요?",
          "speaker_id": "1",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100009838.1.9",
          "form": "구매한건 없어용 주로 어디서 거래하시나요?",
          "original_form": "구매한건 없어용 주로 어디서 거래하시나요?",
          "speaker_id": "2",
          "time": "20210823 10:52"
        },
        {
          "id": "MDRW2100009838.1.10",
          "form": "저는 제일 혜택 많이 받을수 있는 걸로 알아보고 구매해요 ㅎㅎ",
          "original_form": "저는 제일 혜택 많이 받을수 있는 걸로 알아보고 구매해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009838.1.11",
          "form": "사고싶은건 없으세요?",
          "original_form": "사고싶은건 없으세요?",
          "speaker_id": "1",
          "time": "20210823 10:54"
        },
        {
          "id": "MDRW2100009838.1.12",
          "form": "사고 싶은거라.. 요즘 베이킹에 관심이 있어서 오븐 사고싶긴해요",
          "original_form": "사고 싶은거라.. 요즘 베이킹에 관심이 있어서 오븐 사고싶긴해요",
          "speaker_id": "2",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009838.1.13",
          "form": "베이킹도 배우면 두고두고 쓰는거 같아요",
          "original_form": "베이킹도 배우면 두고두고 쓰는거 같아요",
          "speaker_id": "1",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009838.1.14",
          "form": "네 그렇죠",
          "original_form": "네 그렇죠",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009838.1.15",
          "form": "최근에 어떤걸 사셨나요?",
          "original_form": "최근에 어떤걸 사셨나요?",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009838.1.16",
          "form": "로션을 샀어요",
          "original_form": "로션을 샀어요",
          "speaker_id": "1",
          "time": "20210823 11:06"
        },
        {
          "id": "MDRW2100009838.1.17",
          "form": "얼굴타입에 맞는걸로 추천받아서!",
          "original_form": "얼굴타입에 맞는걸로 추천받아서!",
          "speaker_id": "1",
          "time": "20210823 11:06"
        },
        {
          "id": "MDRW2100009838.1.18",
          "form": "아 로션 저도 사야했는데!",
          "original_form": "아 로션 저도 사야했는데!",
          "speaker_id": "2",
          "time": "20210823 11:09"
        },
        {
          "id": "MDRW2100009838.1.19",
          "form": "어떤 브랜드꺼 쓰세요?",
          "original_form": "어떤 브랜드꺼 쓰세요?",
          "speaker_id": "2",
          "time": "20210823 11:09"
        },
        {
          "id": "MDRW2100009838.1.20",
          "form": "저는 이번에 비욘드꺼 샀어요!",
          "original_form": "저는 이번에 비욘드꺼 샀어요!",
          "speaker_id": "1",
          "time": "20210823 11:09"
        }
