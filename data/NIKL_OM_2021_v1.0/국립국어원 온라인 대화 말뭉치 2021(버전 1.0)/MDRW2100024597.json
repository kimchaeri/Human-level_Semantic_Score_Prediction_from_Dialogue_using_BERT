      "utterance": [
        {
          "id": "MDRW2100024597.1.1",
          "form": "그래도 잘하는게 하나는 있지 않을까?",
          "original_form": "그래도 잘하는게 하나는 있지 않을까?",
          "speaker_id": "2",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100024597.1.2",
          "form": "게임 중에서?",
          "original_form": "게임 중에서?",
          "speaker_id": "1",
          "time": "20210831 15:06"
        },
        {
          "id": "MDRW2100024597.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210831 15:06"
        },
        {
          "id": "MDRW2100024597.1.4",
          "form": "하지만 난 모든 종류의 게임을 못해",
          "original_form": "하지만 난 모든 종류의 게임을 못해",
          "speaker_id": "1",
          "time": "20210831 15:07"
        },
        {
          "id": "MDRW2100024597.1.5",
          "form": "카드 게임은 잘하지 않나?",
          "original_form": "카드 게임은 잘하지 않나?",
          "speaker_id": "2",
          "time": "20210831 15:07"
        },
        {
          "id": "MDRW2100024597.1.6",
          "form": "그 게임도 그렇게 그렇게 잘 하는 건 아니고 그래도 인터넷 게임 보단 훨씬 낫고",
          "original_form": "그 게임도 그렇게 그렇게 잘 하는 건 아니고 그래도 인터넷 게임 보단 훨씬 낫고",
          "speaker_id": "1",
          "time": "20210831 15:07"
        },
        {
          "id": "MDRW2100024597.1.7",
          "form": "뭐라도 잘하면 되지",
          "original_form": "뭐라도 잘하면 되지",
          "speaker_id": "2",
          "time": "20210831 15:08"
        },
        {
          "id": "MDRW2100024597.1.8",
          "form": "그래도 나도 뭐 배그나 롤이나 이런 게임 해보고 싶었는데",
          "original_form": "그래도 나도 뭐 배그나 롤이나 이런 게임 해보고 싶었는데",
          "speaker_id": "1",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100024597.1.9",
          "form": "롤이랑 배그는 넘 어렵더라",
          "original_form": "롤이랑 배그는 넘 어렵더라",
          "speaker_id": "2",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100024597.1.10",
          "form": "나는 아직도 그런 게임은 도대체 어떻게 하는 건지 모르겠어",
          "original_form": "나는 아직도 그런 게임은 도대체 어떻게 하는 건지 모르겠어",
          "speaker_id": "1",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100024597.1.11",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210831 15:10"
        },
        {
          "id": "MDRW2100024597.1.12",
          "form": "스타크래프트도 아직도 게임을 이해하지 못하고",
          "original_form": "스타크래프트도 아직도 게임을 이해하지 못하고",
          "speaker_id": "1",
          "time": "20210831 15:10"
        },
        {
          "id": "MDRW2100024597.1.13",
          "form": "스타도 난 안해봤어",
          "original_form": "스타도 난 안해봤어",
