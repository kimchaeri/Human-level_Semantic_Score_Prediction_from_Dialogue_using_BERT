      "utterance": [
        {
          "id": "MDRW2100026832.1.1",
          "form": "너 언제 결혼할거야",
          "original_form": "너 언제 결혼할거야",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026832.1.2",
          "form": "나 결혼 안할건데",
          "original_form": "나 결혼 안할건데",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026832.1.3",
          "form": "왜 안할건데",
          "original_form": "왜 안할건데",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026832.1.4",
          "form": "난 예민해서 나혼자 못살면 죽어",
          "original_form": "난 예민해서 나혼자 못살면 죽어",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026832.1.5",
          "form": "지금 같이 잘 살고있잖아",
          "original_form": "지금 같이 잘 살고있잖아",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026832.1.6",
          "form": "아냐 잘 살고있지 않아",
          "original_form": "아냐 잘 살고있지 않아",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026832.1.7",
          "form": "곧 독립할거야",
          "original_form": "곧 독립할거야",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026832.1.8",
          "form": "저번에 누구 좋다고했잖아",
          "original_form": "저번에 누구 좋다고했잖아",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100026832.1.9",
          "form": "걔는 연예인인데",
          "original_form": "걔는 연예인인데",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026832.1.10",
          "form": "걔만큼 잘생긴 사람있으면 결혼할게",
          "original_form": "걔만큼 잘생긴 사람있으면 결혼할게",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026832.1.11",
          "form": "넌 눈이 너무 높아",
          "original_form": "넌 눈이 너무 높아",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026832.1.12",
          "form": "아냐 엄마가 눈이 낮은거야",
          "original_form": "아냐 엄마가 눈이 낮은거야",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026832.1.13",
          "form": "엄마가 아무나 만나지 말라며!!",
          "original_form": "엄마가 아무나 만나지 말라며!!",
          "speaker_id": "1",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026832.1.14",
          "form": "그건 맞는데 이렇게 아무도 안만날줄은 몰랐지",
          "original_form": "그건 맞는데 이렇게 아무도 안만날줄은 몰랐지",
          "speaker_id": "2",
          "time": "20210902 00:44"
        },
        {
          "id": "MDRW2100026832.1.15",
          "form": "나 얼굴만 뜯어먹고 살고싶어",
          "original_form": "나 얼굴만 뜯어먹고 살고싶어",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026832.1.16",
          "form": "이상한 소리하지마",
          "original_form": "이상한 소리하지마",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026832.1.17",
          "form": "아냐 진심이야",
          "original_form": "아냐 진심이야",
          "speaker_id": "1",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100026832.1.18",
          "form": "얼굴만 보고 살순없어",
          "original_form": "얼굴만 보고 살순없어",
