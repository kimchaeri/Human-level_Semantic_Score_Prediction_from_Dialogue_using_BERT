      "utterance": [
        {
          "id": "MDRW2100031552.1.1",
          "form": "오늘 완전 바쁜하루였으",
          "original_form": "오늘 완전 바쁜하루였으",
          "speaker_id": "2",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100031552.1.2",
          "form": "왜 할 일 많았어?",
          "original_form": "왜 할 일 많았어?",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100031552.1.3",
          "form": "아니 뭐 그냥 날씨 좋길래 이불빨래하고",
          "original_form": "아니 뭐 그냥 날씨 좋길래 이불빨래하고",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100031552.1.4",
          "form": "어제 오늘 햇빛 쨍쨍하더라",
          "original_form": "어제 오늘 햇빛 쨍쨍하더라",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100031552.1.5",
          "form": "응 맞아ㅋㅋ 그리고 여름이불 이런날에는 금방 마르니까",
          "original_form": "응 맞아ㅋㅋ 그리고 여름이불 이런날에는 금방 마르니까",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100031552.1.6",
          "form": "좀 있으면 슬슬 두꺼운 이불로 교체해야할것같아 새벽에는 춥더라",
          "original_form": "좀 있으면 슬슬 두꺼운 이불로 교체해야할것같아 새벽에는 춥더라",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100031552.1.7",
          "form": "맞아 나 모시이불 덮고 자는데 너무 춥더라",
          "original_form": "맞아 나 모시이불 덮고 자는데 너무 춥더라",
          "speaker_id": "2",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031552.1.8",
          "form": "그건 내일 빨래해서 바째 말려서 넣어놔야지",
          "original_form": "그건 내일 빨래해서 바째 말려서 넣어놔야지",
          "speaker_id": "2",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100031552.1.9",
          "form": "모시이불은 너무 추운거아냐? 면이불로 바꿔 이제~",
          "original_form": "모시이불은 너무 추운거아냐? 면이불로 바꿔 이제~",
          "speaker_id": "1",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100031552.1.10",
          "form": "응응 그리고 아까 마트에서 장도 보고 오는길에 투썸에서 케익도 사옴ㅋㅋ",
          "original_form": "응응 그리고 아까 마트에서 장도 보고 오는길에 투썸에서 케익도 사옴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100031552.1.11",
          "form": "주말이라서 빨래랑 장보는거 몰아서 했구나",
          "original_form": "주말이라서 빨래랑 장보는거 몰아서 했구나",
          "speaker_id": "1",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100031552.1.12",
          "form": "응ㅋㅋ 낮에는 아직도 좀 덥더라ㅠ 땀흘림",
          "original_form": "응ㅋㅋ 낮에는 아직도 좀 덥더라ㅠ 땀흘림",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100031552.1.13",
          "form": "장 볼거 많았어? 같이 가자고 하지",
          "original_form": "장 볼거 많았어? 같이 가자고 하지",
          "speaker_id": "1",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100031552.1.14",
          "form": "아니 그냥 재료 조금 샀어!  그 툭툭누들타이에서 팟타이 밀키트 나왔길래 그것도 사와봤옹",
          "original_form": "아니 그냥 재료 조금 샀어!  그 툭툭누들타이에서 팟타이 밀키트 나왔길래 그것도 사와봤옹",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100031552.1.15",
          "form": "오 툭툭누들타이 연남동 맛집 거기아냐?",
          "original_form": "오 툭툭누들타이 연남동 맛집 거기아냐?",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100031552.1.16",
          "form": "응 거기서 파는거 밀키트로 만들었나봥",
          "original_form": "응 거기서 파는거 밀키트로 만들었나봥",
          "speaker_id": "2",
          "time": "20210904 22:48"
        }
      ]
