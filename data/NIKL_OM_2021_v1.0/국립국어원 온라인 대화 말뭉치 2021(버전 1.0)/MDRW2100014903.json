      "utterance": [
        {
          "id": "MDRW2100014903.1.1",
          "form": "동물보는게 좋으신데 키워보셨어요?",
          "original_form": "동물보는게 좋으신데 키워보셨어요?",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014903.1.2",
          "form": "고양이는 안키워보고 물고기 자라 새 정도만 키워봤어요",
          "original_form": "고양이는 안키워보고 물고기 자라 새 정도만 키워봤어요",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014903.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014903.1.4",
          "form": "구피 키워요",
          "original_form": "구피 키워요",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014903.1.5",
          "form": "문조도 키웠었네요",
          "original_form": "문조도 키웠었네요",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014903.1.6",
          "form": "저도 구피키웠었어요ㅋㅋ",
          "original_form": "저도 구피키웠었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014903.1.7",
          "form": "아기물고기..",
          "original_form": "아기물고기..",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014903.1.8",
          "form": "name2님 많이두 키우셨네요",
          "original_form": "&name2&님 많이두 키우셨네요",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014903.1.9",
          "form": "거북이 먹방도 귀여워요",
          "original_form": "거북이 먹방도 귀여워요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014903.1.10",
          "form": "근데 거의 어릴때만 키웠었어요",
          "original_form": "근데 거의 어릴때만 키웠었어요",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014903.1.11",
          "form": "죽는것도 많이보고ㅠ",
          "original_form": "죽는것도 많이보고ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014903.1.12",
          "form": "어릴때 정서에 좋지만은 않은거같아요ㅋㅋ",
          "original_form": "어릴때 정서에 좋지만은 않은거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014903.1.13",
          "form": "ㅜㅜㅜㅜㅜㅜㅜ안돼",
          "original_form": "ㅜㅜㅜㅜㅜㅜㅜ안돼",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014903.1.14",
          "form": "무덤만들어줬어요?ㅜ",
          "original_form": "무덤만들어줬어요?ㅜ",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014903.1.15",
          "form": "그거까진 잘 기억이 안나요ㅠㅠ",
          "original_form": "그거까진 잘 기억이 안나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014903.1.16",
          "form": "바로 없어지고..",
          "original_form": "바로 없어지고..",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014903.1.17",
          "form": "병아리 이런것도 잘 죽잖아요",
          "original_form": "병아리 이런것도 잘 죽잖아요",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014903.1.18",
          "form": "안좋긴요 상냥한 심성을 키우신건데",
          "original_form": "안좋긴요 상냥한 심성을 키우신건데",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014903.1.19",
          "form": "좋은거지욧ㅜ",
          "original_form": "좋은거지욧ㅜ",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014903.1.20",
          "form": "그래도 너무 불쌍했어요ㅜㅜ",
          "original_form": "그래도 너무 불쌍했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014903.1.21",
          "form": "요새 다른생명에 공감능력없는사람이 얼마나 많은데요",
          "original_form": "요새 다른생명에 공감능력없는사람이 얼마나 많은데요",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014903.1.22",
          "form": "아..너무 심한 사람들은 그냥 싸이코같아요..교정가능성이 없는",
          "original_form": "아..너무 심한 사람들은 그냥 싸이코같아요..교정가능성이 없는",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014903.1.23",
          "form": "짐승만도 못하다고 생각합니다",
          "original_form": "짐승만도 못하다고 생각합니다",
          "speaker_id": "1",
