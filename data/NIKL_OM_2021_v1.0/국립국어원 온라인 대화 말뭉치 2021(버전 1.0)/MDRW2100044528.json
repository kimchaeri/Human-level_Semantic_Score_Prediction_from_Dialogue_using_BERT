      "utterance": [
        {
          "id": "MDRW2100044528.1.1",
          "form": "다들 선택하고 쫄아있네",
          "original_form": "다들 선택하고 쫄아있네",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044528.1.2",
          "form": "헐 지목한 사람한테 할말 있냐고",
          "original_form": "헐 지목한 사람한테 할말 있냐고",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044528.1.3",
          "form": "홀리뱅 리더가 무섭나",
          "original_form": "홀리뱅 리더가 무섭나",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044528.1.4",
          "form": "엉 쌤들의 쌤",
          "original_form": "엉 쌤들의 쌤",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044528.1.5",
          "form": "뭐 그런느낌이지",
          "original_form": "뭐 그런느낌이지",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044528.1.6",
          "form": "메인댄서 그룹 기대된다",
          "original_form": "메인댄서 그룹 기대된다",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044528.1.7",
          "form": "왕선배님인가봐",
          "original_form": "왕선배님인가봐",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044528.1.8",
          "form": "그건 메인댄서그룹 아니고 리더그룹!",
          "original_form": "그건 메인댄서그룹 아니고 리더그룹!",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044528.1.9",
          "form": "아 리더그룹",
          "original_form": "아 리더그룹",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044528.1.10",
          "form": "리더끼리 하다니",
          "original_form": "리더끼리 하다니",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044528.1.11",
          "form": "이제 서브그룹하고 리더그룹이 젤 마지막",
          "original_form": "이제 서브그룹하고 리더그룹이 젤 마지막",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044528.1.12",
          "form": "인가보다",
          "original_form": "인가보다",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044528.1.13",
          "form": "기싸움들이..",
          "original_form": "기싸움들이..",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044528.1.14",
          "form": "ㅋㅋ그런걸 원하는 거 같지만",
          "original_form": "ㅋㅋ그런걸 원하는 거 같지만",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044528.1.15",
          "form": "근데 진짜 다들 잘 춘다",
          "original_form": "근데 진짜 다들 잘 춘다",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044528.1.16",
          "form": "다들 끝나면 인스타같은데서 친하고",
          "original_form": "다들 끝나면 인스타같은데서 친하고",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044528.1.17",
          "form": "덧글달고 그래",
          "original_form": "덧글달고 그래",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044528.1.18",
          "form": "그러고 다시 방송에선 으르렁",
          "original_form": "그러고 다시 방송에선 으르렁",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044528.1.19",
          "form": "ㅋㅋㅋ컨셉인가",
          "original_form": "ㅋㅋㅋ컨셉인가",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044528.1.20",
          "form": "그렇겠지?",
          "original_form": "그렇겠지?",
          "speaker_id": "2",
          "time": "20210907 23:06"
        }
      ]
    }
