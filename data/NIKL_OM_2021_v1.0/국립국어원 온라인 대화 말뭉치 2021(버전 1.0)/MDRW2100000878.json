      "utterance": [
        {
          "id": "MDRW2100000878.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000878.1.2",
          "form": "안녕하세요~ 요즘에 어디 나가지 못하고 집에만 있으니까 몸이 무거워지는 거 같아요ㅎ",
          "original_form": "안녕하세요~ 요즘에 어디 나가지 못하고 집에만 있으니까 몸이 무거워지는 거 같아요ㅎ",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000878.1.3",
          "form": "다이어트는 진행중이세요?",
          "original_form": "다이어트는 진행중이세요?",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000878.1.4",
          "form": "요즘에 기초대사량을 늘릴려고 근력운동 조금씩 하고 있어요~",
          "original_form": "요즘에 기초대사량을 늘릴려고 근력운동 조금씩 하고 있어요~",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000878.1.5",
          "form": "운동 중요해요",
          "original_form": "운동 중요해요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000878.1.6",
          "form": "운동안하면서 하면 금방 요요 오더라구요",
          "original_form": "운동안하면서 하면 금방 요요 오더라구요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000878.1.7",
          "form": "확실히 중요하더라구요",
          "original_form": "확실히 중요하더라구요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000878.1.8",
          "form": "맞아요~ 나이드니까 예전만큼 잘 빠지지도 않고",
          "original_form": "맞아요~ 나이드니까 예전만큼 잘 빠지지도 않고",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000878.1.9",
          "form": "근육도 잘 안 붙는 거 같고",
          "original_form": "근육도 잘 안 붙는 거 같고",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000878.1.10",
          "form": "진짜 살기위해서 운동하는 거 같아요~",
          "original_form": "진짜 살기위해서 운동하는 거 같아요~",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000878.1.11",
          "form": "운동해서 빼야 확실히",
          "original_form": "운동해서 빼야 확실히",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100000878.1.12",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100000878.1.13",
          "form": "그런거 같더라고요~",
          "original_form": "그런거 같더라고요~",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100000878.1.14",
          "form": "근력이 좀 붙어야 살도 덜찌는 거 같고~",
          "original_form": "근력이 좀 붙어야 살도 덜찌는 거 같고~",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100000878.1.15",
          "form": "그게 젤중요한거같아요",
          "original_form": "그게 젤중요한거같아요",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100000878.1.16",
          "form": "혹시 미용에 관심이 많으신가요",
          "original_form": "혹시 미용에 관심이 많으신가요",
          "speaker_id": "1",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100000878.1.17",
          "form": "네일아트 같은 거에 관심있어요~",
          "original_form": "네일아트 같은 거에 관심있어요~",
          "speaker_id": "2",
          "time": "20210820 12:03"
        },
        {
          "id": "MDRW2100000878.1.18",
          "form": "저도 좋아행6",
          "original_form": "저도 좋아행6",
          "speaker_id": "1",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100000878.1.19",
          "form": "해요",
          "original_form": "해요",
          "speaker_id": "1",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100000878.1.20",
          "form": "전 메이크업도 관심이 많아요",
          "original_form": "전 메이크업도 관심이 많아요",
          "speaker_id": "1",
          "time": "20210820 12:08"
        },
        {
          "id": "MDRW2100000878.1.21",
          "form": "오~ 그러면 뷰티유투버같은 거 많이 보시겠네요~",
          "original_form": "오~ 그러면 뷰티유투버같은 거 많이 보시겠네요~",
          "speaker_id": "2",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100000878.1.22",
          "form": "네 자주 보고 즐겨서 구독 해놔서",
          "original_form": "네 자주 보고 즐겨서 구독 해놔서",
