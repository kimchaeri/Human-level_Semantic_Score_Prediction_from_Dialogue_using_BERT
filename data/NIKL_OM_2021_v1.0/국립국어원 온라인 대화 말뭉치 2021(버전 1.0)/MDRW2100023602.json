      "utterance": [
        {
          "id": "MDRW2100023602.1.1",
          "form": "식사하셨나요??",
          "original_form": "식사하셨나요??",
          "speaker_id": "1",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100023602.1.2",
          "form": "아직이여 조금 뒤에 먹을 거에요",
          "original_form": "아직이여 조금 뒤에 먹을 거에요",
          "speaker_id": "2",
          "time": "20210830 12:36"
        },
        {
          "id": "MDRW2100023602.1.3",
          "form": "식사 하셨어요?",
          "original_form": "식사 하셨어요?",
          "speaker_id": "2",
          "time": "20210830 12:36"
        },
        {
          "id": "MDRW2100023602.1.4",
          "form": "네 전 구내식당 먹었어요 ㅎㅎ",
          "original_form": "네 전 구내식당 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:36"
        },
        {
          "id": "MDRW2100023602.1.5",
          "form": "저도요 곧 올라가서 먹어야죠",
          "original_form": "저도요 곧 올라가서 먹어야죠",
          "speaker_id": "2",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100023602.1.6",
          "form": "여긴 오늘 메뉴가 닭볶음탕이엇어요 ㅎㅎ",
          "original_form": "여긴 오늘 메뉴가 닭볶음탕이엇어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100023602.1.7",
          "form": "와 맛있었겠어요",
          "original_form": "와 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100023602.1.8",
          "form": "구내식당이 얼마전에 바껴서 맛이 좋아졌어요~!",
          "original_form": "구내식당이 얼마전에 바껴서 맛이 좋아졌어요~!",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100023602.1.9",
          "form": "좋네요 저희쪽도 바꿨으면 좋겠어요ㅠㅠ",
          "original_form": "좋네요 저희쪽도 바꿨으면 좋겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100023602.1.10",
          "form": "2500원인데 가성비 내려오고 넘 만족스러워요!",
          "original_form": "2500원인데 가성비 내려오고 넘 만족스러워요!",
          "speaker_id": "1",
          "time": "20210830 18:20"
        },
        {
          "id": "MDRW2100023602.1.11",
          "form": "진짜 싸네요!",
          "original_form": "진짜 싸네요!",
          "speaker_id": "2",
          "time": "20210830 18:31"
        },
        {
          "id": "MDRW2100023602.1.12",
          "form": "저녁은 뭐드시나요~?",
          "original_form": "저녁은 뭐드시나요~?",
          "speaker_id": "1",
          "time": "20210830 18:51"
        },
        {
          "id": "MDRW2100023602.1.13",
          "form": "쌀국수 먹었엉ㅅ",
          "original_form": "쌀국수 먹었엉ㅅ",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023602.1.14",
          "form": "헙 저도 쌀국수 좋아해뇨 ㅋㅋ",
          "original_form": "헙 저도 쌀국수 좋아해뇨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100023602.1.15",
          "form": "쌀국수에 땡초 넣고 먹는거 좋아해요",
          "original_form": "쌀국수에 땡초 넣고 먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023602.1.16",
          "form": "오늘은 볶음 국수로 먹었어요",
          "original_form": "오늘은 볶음 국수로 먹었어요",
          "speaker_id": "2",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100023602.1.17",
          "form": "국물 있는 걸 더 좋아하세요?",
          "original_form": "국물 있는 걸 더 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100023602.1.18",
          "form": "둘 다 좋아해요!!",
          "original_form": "둘 다 좋아해요!!",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100023602.1.19",
          "form": "고기랑 소스 같이 비벼먹는것도 좋아하고 다 좋아해뇨 ㅎㅍㅊ",
