      "utterance": [
        {
          "id": "MDRW2100032623.1.1",
          "form": "저녁 먹었는데 배고프면 어떻게 해야하죠?...",
          "original_form": "저녁 먹었는데 배고프면 어떻게 해야하죠?...",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032623.1.2",
          "form": "닭가슴살로 달래야죠...ㅠ",
          "original_form": "닭가슴살로 달래야죠...ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100032623.1.3",
          "form": "ㅜㅜ닭가슴살은 내일부터라며...",
          "original_form": "ㅜㅜ닭가슴살은 내일부터라며...",
          "speaker_id": "2",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100032623.1.4",
          "form": "그럼 마지막이니까 간식 ㄱ?",
          "original_form": "그럼 마지막이니까 간식 ㄱ?",
          "speaker_id": "1",
          "time": "20210905 21:42"
        },
        {
          "id": "MDRW2100032623.1.5",
          "form": "명랑핫도그...",
          "original_form": "명랑핫도그...",
          "speaker_id": "1",
          "time": "20210905 21:42"
        },
        {
          "id": "MDRW2100032623.1.6",
          "form": "ㅎ ㅏ ...배달각인가요 ㅋㅋㅋ",
          "original_form": "ㅎ ㅏ ...배달각인가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032623.1.7",
          "form": "모짜핫도그 먹곱네  맛잇겟당",
          "original_form": "모짜핫도그 먹곱네  맛잇겟당",
          "speaker_id": "2",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032623.1.8",
          "form": "운동겸 걸어가서 하나만 먹는거어때",
          "original_form": "운동겸 걸어가서 하나만 먹는거어때",
          "speaker_id": "1",
          "time": "20210905 21:43"
        },
        {
          "id": "MDRW2100032623.1.9",
          "form": "ㅋㅋ그 핫도그 집이...",
          "original_form": "ㅋㅋ그 핫도그 집이...",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032623.1.10",
          "form": "스카보다 먼데? ",
          "original_form": "스카보다 먼데? ",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032623.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ그러니까운듕이지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러니까운듕이지",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032623.1.12",
          "form": "모짜 맵 구마 맛잇어 ㅠㅠ",
          "original_form": "모짜 맵 구마 맛잇어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032623.1.13",
          "form": "ㅠㅠㅋㅋㅋ그럼 포기...",
          "original_form": "ㅠㅠㅋㅋㅋ그럼 포기...",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032623.1.14",
          "form": "그정도 의지는 아니다...ㅋㅋㅋ",
          "original_form": "그정도 의지는 아니다...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032623.1.15",
          "form": "놀랍게도 이미 문닫았네;;",
          "original_form": "놀랍게도 이미 문닫았네;;",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032623.1.16",
          "form": "담에 모 짜 맵구마 꼭 먹어바 ㅜ",
          "original_form": "담에 모 짜 맵구마 꼭 먹어바 ㅜ",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032623.1.17",
          "form": "매운데 맛남",
          "original_form": "매운데 맛남",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032623.1.18",
          "form": "오키 리스트업한다...",
          "original_form": "오키 리스트업한다...",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032623.1.19",
          "form": "갑자기 핫도그 땡기네..",
          "original_form": "갑자기 핫도그 땡기네..",
          "speaker_id": "1",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032623.1.20",
          "form": "명랑에 파는 떡볶이도 먹어봣니?",
          "original_form": "명랑에 파는 떡볶이도 먹어봣니?",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032623.1.21",
          "form": "명랑 떡볶이는 먹어봄??",
          "original_form": "명랑 떡볶이는 먹어봄??",
          "speaker_id": "1",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032623.1.22",
          "form": "ㄴㄴ아직 ㅋㅋㅋ",
          "original_form": "ㄴㄴ아직 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032623.1.23",
          "form": "궁금하긴 한데 좀...",
          "original_form": "궁금하긴 한데 좀...",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032623.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ아니 궁근함",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니 궁근함",
          "speaker_id": "1",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100032623.1.25",
          "form": "몬가 그냥 무난할거같애",
          "original_form": "몬가 그냥 무난할거같애",
