      "utterance": [
        {
          "id": "MDRW2100012252.1.1",
          "form": "혹시 반려동물 키우시나요?",
          "original_form": "혹시 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100012252.1.2",
          "form": "예전에 길렀어요",
          "original_form": "예전에 길렀어요",
          "speaker_id": "2",
          "time": "20210824 20:43"
        },
        {
          "id": "MDRW2100012252.1.3",
          "form": "그렇구나 저희는 고양이키워요",
          "original_form": "그렇구나 저희는 고양이키워요",
          "speaker_id": "1",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100012252.1.4",
          "form": "그렇군요 고양이 알레르기는 없나요",
          "original_form": "그렇군요 고양이 알레르기는 없나요",
          "speaker_id": "2",
          "time": "20210824 20:46"
        },
        {
          "id": "MDRW2100012252.1.5",
          "form": "다행이두요",
          "original_form": "다행이두요",
          "speaker_id": "1",
          "time": "20210824 20:47"
        },
        {
          "id": "MDRW2100012252.1.6",
          "form": "동물 알레르기는 없는것 같아요",
          "original_form": "동물 알레르기는 없는것 같아요",
          "speaker_id": "1",
          "time": "20210824 20:47"
        },
        {
          "id": "MDRW2100012252.1.7",
          "form": "정말 다행이네요",
          "original_form": "정말 다행이네요",
          "speaker_id": "2",
          "time": "20210824 20:47"
        },
        {
          "id": "MDRW2100012252.1.8",
          "form": "저희고양이 귀여운데",
          "original_form": "저희고양이 귀여운데",
          "speaker_id": "1",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012252.1.9",
          "form": "사진전송안되는게 아쉽네요",
          "original_form": "사진전송안되는게 아쉽네요",
          "speaker_id": "1",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012252.1.10",
          "form": "보고 싶었네요",
          "original_form": "보고 싶었네요",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012252.1.11",
          "form": "사람말을 알아듣는지 궁금해요",
          "original_form": "사람말을 알아듣는지 궁금해요",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012252.1.12",
          "form": "평소에 무슨생각을 하며 지내는지",
          "original_form": "평소에 무슨생각을 하며 지내는지",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012252.1.13",
          "form": "저도 궁금하네요",
          "original_form": "저도 궁금하네요",
          "speaker_id": "2",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012252.1.14",
          "form": "이름불러도 안오고",
          "original_form": "이름불러도 안오고",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012252.1.15",
          "form": "모른척해요",
          "original_form": "모른척해요",
          "speaker_id": "1",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100012252.1.16",
          "form": "간식줄때만 따라오고",
          "original_form": "간식줄때만 따라오고",
          "speaker_id": "1",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100012252.1.17",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100012252.1.18",
          "form": "정말 기르기 힘들겠네요",
          "original_form": "정말 기르기 힘들겠네요",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100012252.1.19",
          "form": "오잉 나쁜말 안했는데",
          "original_form": "오잉 나쁜말 안했는데",
          "speaker_id": "1",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100012252.1.20",
          "form": "독립적인성격이라 기르기엔 편해요",
          "original_form": "독립적인성격이라 기르기엔 편해요",
          "speaker_id": "1",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100012252.1.21",
          "form": "그래요 혹시 기르는데 불편은 없죠",
          "original_form": "그래요 혹시 기르는데 불편은 없죠",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012252.1.22",
          "form": "네네 딱히 손이안가서",
          "original_form": "네네 딱히 손이안가서",
          "speaker_id": "1",
          "time": "20210824 20:52"
        }
      ]
