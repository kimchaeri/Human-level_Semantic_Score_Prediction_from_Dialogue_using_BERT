      "utterance": [
        {
          "id": "MDRW2100004516.1.1",
          "form": "남주혁은 밥에다가 케찹 뿌린다",
          "original_form": "남주혁은 밥에다가 케찹 뿌린다",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004516.1.2",
          "form": "간장이 나을거 같은데",
          "original_form": "간장이 나을거 같은데",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004516.1.3",
          "form": "간장에다가 케찹까지 거기다 감자튀김 ㅋㅋ",
          "original_form": "간장에다가 케찹까지 거기다 감자튀김 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004516.1.4",
          "form": "간장과 케찹까지 맛 없겠다",
          "original_form": "간장과 케찹까지 맛 없겠다",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004516.1.5",
          "form": "비쥬얼이 너무 안좋다",
          "original_form": "비쥬얼이 너무 안좋다",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004516.1.6",
          "form": "맛없어 보일듯",
          "original_form": "맛없어 보일듯",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004516.1.7",
          "form": "엉 호준이 밥은 식용유 과다로 느끼",
          "original_form": "엉 호준이 밥은 식용유 과다로 느끼",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004516.1.8",
          "form": "호준이는 요리는 더 배워야겠다ㅋ",
          "original_form": "호준이는 요리는 더 배워야겠다ㅋ",
          "speaker_id": "1",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004516.1.9",
          "form": "주혁이는 케찹 추가로 느끼 달고 시큼이라고 자막에 나와서 웃었다",
          "original_form": "주혁이는 케찹 추가로 느끼 달고 시큼이라고 자막에 나와서 웃었다",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004516.1.10",
          "form": "확실히 차승원이 요리를 잘해",
          "original_form": "확실히 차승원이 요리를 잘해",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004516.1.11",
          "form": "손호준은 보조만 해야겠다",
          "original_form": "손호준은 보조만 해야겠다",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004516.1.12",
          "form": "엉 그래야 겠다 그래도 냄비밥은 잘하더라",
          "original_form": "엉 그래야 겠다 그래도 냄비밥은 잘하더라",
