        {
          "id": "MDRW2100009624.1.1",
          "form": "체육이",
          "original_form": "체육이",
          "speaker_id": "1",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009624.1.2",
          "form": "인기없는 이유가 있나요",
          "original_form": "인기없는 이유가 있나요",
          "speaker_id": "1",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009624.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009624.1.4",
          "form": "왜지? 왜 없어요",
          "original_form": "왜지? 왜 없어요",
          "speaker_id": "1",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009624.1.5",
          "form": "각 과마다 전통이나 분위기가 있는데",
          "original_form": "각 과마다 전통이나 분위기가 있는데",
          "speaker_id": "2",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009624.1.6",
          "form": "약간 빡센가",
          "original_form": "약간 빡센가",
          "speaker_id": "1",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009624.1.7",
          "form": "체육이 음",
          "original_form": "체육이 음",
          "speaker_id": "2",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009624.1.8",
          "form": "수직적이고",
          "original_form": "수직적이고",
          "speaker_id": "2",
          "time": "20210823 01:53"
        },
        {
          "id": "MDRW2100009624.1.9",
          "form": "아직도 음",
          "original_form": "아직도 음",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.10",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.11",
          "form": "술 강요하고 이런 게 좀 있어요",
          "original_form": "술 강요하고 이런 게 좀 있어요",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.12",
          "form": "군기잡는구나",
          "original_form": "군기잡는구나",
          "speaker_id": "1",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.13",
          "form": "이해완.",
          "original_form": "이해완.",
          "speaker_id": "1",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.14",
          "form": "저희가 지방의 고이고 고인",
          "original_form": "저희가 지방의 고이고 고인",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.15",
          "form": "조그마한 곳이라",
          "original_form": "조그마한 곳이라",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.16",
          "form": "여전히 그런데",
          "original_form": "여전히 그런데",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.17",
          "form": "그런곳이라면 더욱 더..",
          "original_form": "그런곳이라면 더욱 더..",
          "speaker_id": "1",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.18",
          "form": "체육이 특히 심해서요",
          "original_form": "체육이 특히 심해서요",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.19",
          "form": "사라지지않는",
          "original_form": "사라지지않는",
          "speaker_id": "1",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.20",
          "form": "그나마 교대라고",
          "original_form": "그나마 교대라고",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.21",
          "form": "나쁜 문화",
          "original_form": "나쁜 문화",
          "speaker_id": "1",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.22",
          "form": "주변 대학 중 가장 나아요",
          "original_form": "주변 대학 중 가장 나아요",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.23",
          "form": "놀랍게도",
          "original_form": "놀랍게도",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009624.1.25",
          "form": "흥미로운 전통이요?",
