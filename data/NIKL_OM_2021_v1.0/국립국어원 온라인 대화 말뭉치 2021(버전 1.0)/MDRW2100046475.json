        },
        {
          "id": "MDRW2100046475.1.2",
          "form": "안녕하세요 :)",
          "original_form": "안녕하세요 :)",
          "speaker_id": "2",
          "time": "20211025 14:39"
        },
        {
          "id": "MDRW2100046475.1.3",
          "form": "점심식사 하셨나요~?",
          "original_form": "점심식사 하셨나요~?",
          "speaker_id": "2",
          "time": "20211025 14:40"
        },
        {
          "id": "MDRW2100046475.1.4",
          "form": "네네 먹었습니다 ㅎ",
          "original_form": "네네 먹었습니다 ㅎ",
          "speaker_id": "1",
          "time": "20211025 14:40"
        },
        {
          "id": "MDRW2100046475.1.5",
          "form": "그렇군요 ㅎㅎ",
          "original_form": "그렇군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211025 14:42"
        },
        {
          "id": "MDRW2100046475.1.6",
          "form": "name1님은 취미가 어떻게 되시나요~?",
          "original_form": "&name1&님은 취미가 어떻게 되시나요~?",
          "speaker_id": "2",
          "time": "20211025 14:42"
        },
        {
          "id": "MDRW2100046475.1.7",
          "form": "아 취미는 유튜브 보는거나 게임입니다",
          "original_form": "아 취미는 유튜브 보는거나 게임입니다",
          "speaker_id": "1",
          "time": "20211025 14:44"
        },
        {
          "id": "MDRW2100046475.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20211025 14:45"
        },
        {
          "id": "MDRW2100046475.1.9",
          "form": "앗 저도 유튜브보는 것도 좋아하고 게임도 좋아해요 ㅎㅎ",
          "original_form": "앗 저도 유튜브보는 것도 좋아하고 게임도 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211025 14:46"
        },
        {
          "id": "MDRW2100046475.1.10",
          "form": "유튜브는 주로 어떤 거 보세요..?ㅎㅎㅎ",
          "original_form": "유튜브는 주로 어떤 거 보세요..?ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211025 14:46"
        },
        {
          "id": "MDRW2100046475.1.11",
          "form": "엇 ㅋㅋ",
          "original_form": "엇 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211025 14:46"
        },
        {
          "id": "MDRW2100046475.1.12",
          "form": "랑그릿사라는 게임을 하기에 관련 유튜브도 보고",
          "original_form": "랑그릿사라는 게임을 하기에 관련 유튜브도 보고",
          "speaker_id": "1",
          "time": "20211025 14:46"
        },
        {
          "id": "MDRW2100046475.1.13",
          "form": "주식투자를 하는중이라 요즘은 주식방송 많이 봅니다",
          "original_form": "주식투자를 하는중이라 요즘은 주식방송 많이 봅니다",
          "speaker_id": "1",
          "time": "20211025 14:46"
        },
        {
          "id": "MDRW2100046475.1.14",
          "form": "오오 그렇군요! 저 랑그릿사 예전에 조금 했었어요..!ㅋㅋㅋ",
          "original_form": "오오 그렇군요! 저 랑그릿사 예전에 조금 했었어요..!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211025 14:48"
        },
        {
          "id": "MDRW2100046475.1.15",
          "form": "물론 아주 예전이지만...",
          "original_form": "물론 아주 예전이지만...",
          "speaker_id": "2",
          "time": "20211025 14:48"
        },
        {
          "id": "MDRW2100046475.1.16",
          "form": "엥 ㄷ",
          "original_form": "엥 ㄷ",
          "speaker_id": "1",
          "time": "20211025 14:48"
        },
        {
          "id": "MDRW2100046475.1.17",
          "form": "얼마나 예전이시길래",
          "original_form": "얼마나 예전이시길래",
          "speaker_id": "1",
          "time": "20211025 14:48"
        },
        {
          "id": "MDRW2100046475.1.18",
          "form": "옛날 고전 랑그릿사 아니신가요 ㅋ",
          "original_form": "옛날 고전 랑그릿사 아니신가요 ㅋ",
          "speaker_id": "1",
          "time": "20211025 14:48"
        },
        {
          "id": "MDRW2100046475.1.19",
          "form": "음 거의 2년 전쯤인 것 같아요",
          "original_form": "음 거의 2년 전쯤인 것 같아요",
          "speaker_id": "2",
          "time": "20211025 14:48"
        },
        {
          "id": "MDRW2100046475.1.20",
          "form": "아아 그 옛날 그림체같은 게임이었는데...모바일 게임이요..! ㅎㅎ",
          "original_form": "아아 그 옛날 그림체같은 게임이었는데...모바일 게임이요..! ㅎㅎ",
          "speaker_id": "2",
          "time": "20211025 14:48"
        },
        {
          "id": "MDRW2100046475.1.21",
          "form": "제가 한건 모바일 랑그릿사입니다 ㅎ",
          "original_form": "제가 한건 모바일 랑그릿사입니다 ㅎ",
          "speaker_id": "1",
          "time": "20211025 14:48"
        },
        {
          "id": "MDRW2100046475.1.22",
          "form": "아 그러면 맞군요",
          "original_form": "아 그러면 맞군요",
          "speaker_id": "1",
          "time": "20211025 14:48"
        },
        {
          "id": "MDRW2100046475.1.23",
          "form": "name2님은 그럼 어떤 게임하시나요",
          "original_form": "&name2&님은 그럼 어떤 게임하시나요",
          "speaker_id": "1",
          "time": "20211025 14:49"
        },
        {
          "id": "MDRW2100046475.1.24",
          "form": "저는 지금은 안해가지고 요즘은 재밌는 게임이 뭔지 잘 모르겠어요 ㅠㅠ",
          "original_form": "저는 지금은 안해가지고 요즘은 재밌는 게임이 뭔지 잘 모르겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211025 14:49"
        },
        {
          "id": "MDRW2100046475.1.25",
          "form": "요즘은 모바일겜 중에 뭐가 유행하나요..?ㅎㅎ",
          "original_form": "요즘은 모바일겜 중에 뭐가 유행하나요..?ㅎㅎ",
          "speaker_id": "2",
          "time": "20211025 14:49"
        },
        {
          "id": "MDRW2100046475.1.26",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20211025 14:49"
        },
        {
          "id": "MDRW2100046475.1.27",
          "form": "흐음...",
          "original_form": "흐음...",
          "speaker_id": "1",
          "time": "20211025 14:49"
        },
        {
          "id": "MDRW2100046475.1.28",
          "form": "저희 나라 모바일 게임 순위보면",
          "original_form": "저희 나라 모바일 게임 순위보면",
          "speaker_id": "1",
          "time": "20211025 14:49"
        },
        {
          "id": "MDRW2100046475.1.29",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20211025 14:50"
        },
        {
          "id": "MDRW2100046475.1.30",
          "form": "오딘\r\n리니지m\r\n블레이드앤소울\r\n리니지2m 이런데",
          "original_form": "오딘\r\n리니지m\r\n블레이드앤소울\r\n리니지2m 이런데",
          "speaker_id": "1",
          "time": "20211025 14:50"
        },
        {
          "id": "MDRW2100046475.1.31",
          "form": "와.. 아직도 리니지가 강세이군요....",
          "original_form": "와.. 아직도 리니지가 강세이군요....",
          "speaker_id": "2",
          "time": "20211025 14:50"
        },
        {
          "id": "MDRW2100046475.1.32",
          "form": "너무 과금이 심해서..",
          "original_form": "너무 과금이 심해서..",
          "speaker_id": "1",
          "time": "20211025 14:50"
        },
        {
          "id": "MDRW2100046475.1.33",
          "form": "맞아요...그래서 난리도 한번 났던 걸로",
          "original_form": "맞아요...그래서 난리도 한번 났던 걸로",
          "speaker_id": "2",
          "time": "20211025 14:50"
        },
        {
          "id": "MDRW2100046475.1.34",
          "form": "기억하는데...",
          "original_form": "기억하는데...",
          "speaker_id": "2",
          "time": "20211025 14:50"
        },
        {
          "id": "MDRW2100046475.1.35",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20211025 14:50"
        },
        {
          "id": "MDRW2100046475.1.36",
          "form": "그런 게임에 비하면 랑그릿사는 꽤 보상도 괜찮고",
          "original_form": "그런 게임에 비하면 랑그릿사는 꽤 보상도 괜찮고",
          "speaker_id": "2",
          "time": "20211025 14:51"
        },
        {
          "id": "MDRW2100046475.1.37",
          "form": "ㄷㄷ 잘아시네요",
          "original_form": "ㄷㄷ 잘아시네요",
          "speaker_id": "1",
          "time": "20211025 14:51"
        },
        {
          "id": "MDRW2100046475.1.38",
          "form": "꽤 재밌었던 것 같아요 ㅎㅎㅎ",
          "original_form": "꽤 재밌었던 것 같아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211025 14:51"
        },
        {
          "id": "MDRW2100046475.1.39",
          "form": "유튭에서 한때 광고를 많이 해가지고",
          "original_form": "유튭에서 한때 광고를 많이 해가지고",
          "speaker_id": "2",
          "time": "20211025 14:51"
        },
        {
          "id": "MDRW2100046475.1.40",
          "form": "ㅋㅋ 광고 효과가 있었군요",
          "original_form": "ㅋㅋ 광고 효과가 있었군요",
          "speaker_id": "1",
          "time": "20211025 14:51"
        },
        {
          "id": "MDRW2100046475.1.41",
          "form": "저도 해봤었는데 요즘은",
          "original_form": "저도 해봤었는데 요즘은",
          "speaker_id": "2",
          "time": "20211025 14:51"
        },
        {
          "id": "MDRW2100046475.1.42",
          "form": "무슨 캐릭터가 강세인가요?ㅎㅎ",
          "original_form": "무슨 캐릭터가 강세인가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20211025 14:51"
        },
        {
          "id": "MDRW2100046475.1.43",
          "form": "랑그는 장점이",
          "original_form": "랑그는 장점이",
          "speaker_id": "1",
          "time": "20211025 14:51"
        },
        {
          "id": "MDRW2100046475.1.44",
          "form": "기존 게임처럼 신캐 나온다고 기존캐릭이 묻히는게 아니라",
          "original_form": "기존 게임처럼 신캐 나온다고 기존캐릭이 묻히는게 아니라",
          "speaker_id": "1",
          "time": "20211025 14:52"
        },
        {
          "id": "MDRW2100046475.1.45",
          "form": "기존캐릭을 리뉴얼해줘서\r\n\r\n엘윈이든 리아나든 레온이든 다 씁니다",
          "original_form": "기존캐릭을 리뉴얼해줘서\r\n\r\n엘윈이든 리아나든 레온이든 다 씁니다",
          "speaker_id": "1",
          "time": "20211025 14:52"
        },
        {
          "id": "MDRW2100046475.1.46",
          "form": "아아 그렇군요..! 헉 캐릭 이름 들으니까 좀 생각나는 것 같아요 ㅋㅋㅋ",
          "original_form": "아아 그렇군요..! 헉 캐릭 이름 들으니까 좀 생각나는 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211025 14:52"
        },
        {
          "id": "MDRW2100046475.1.47",
          "form": "엘윈... 리아나가 힐러였나.. 그랬고",
          "original_form": "엘윈... 리아나가 힐러였나.. 그랬고",
          "speaker_id": "2",
          "time": "20211025 14:53"
        },
        {
          "id": "MDRW2100046475.1.48",
          "form": "잘아시네요 역시",
          "original_form": "잘아시네요 역시",
          "speaker_id": "1",
          "time": "20211025 14:53"
        },
        {
          "id": "MDRW2100046475.1.49",
          "form": "ㅋㅋㅋㅋ겹치는 게임이 나오니까 대화가 너무 재밌었어요!",
          "original_form": "ㅋㅋㅋㅋ겹치는 게임이 나오니까 대화가 너무 재밌었어요!",
          "speaker_id": "2",
          "time": "20211025 14:53"
        }
