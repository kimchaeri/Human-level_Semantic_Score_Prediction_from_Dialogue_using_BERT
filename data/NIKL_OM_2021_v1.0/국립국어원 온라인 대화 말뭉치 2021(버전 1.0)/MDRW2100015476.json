      "utterance": [
        {
          "id": "MDRW2100015476.1.1",
          "form": "야식고파진다",
          "original_form": "야식고파진다",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015476.1.2",
          "form": "야식은 보쌈이지",
          "original_form": "야식은 보쌈이지",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015476.1.3",
          "form": "크",
          "original_form": "크",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015476.1.4",
          "form": "족발 보쌈",
          "original_form": "족발 보쌈",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015476.1.5",
          "form": "둘중에 고르라면 뭐 골라?",
          "original_form": "둘중에 고르라면 뭐 골라?",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015476.1.6",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015476.1.7",
          "form": "고민되네",
          "original_form": "고민되네",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015476.1.8",
          "form": "각각 땡기는 날이 달라서",
          "original_form": "각각 땡기는 날이 달라서",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015476.1.9",
          "form": "오늘은 보쌈",
          "original_form": "오늘은 보쌈",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015476.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ난 족발...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ난 족발...",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015476.1.11",
          "form": "우리동네에 족발맛집 있는데",
          "original_form": "우리동네에 족발맛집 있는데",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015476.1.12",
          "form": "핵존맛",
          "original_form": "핵존맛",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015476.1.13",
          "form": "잠깐만 불족발이면 이야기가 다르다",
          "original_form": "잠깐만 불족발이면 이야기가 다르다",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015476.1.14",
          "form": "아 불족발은 아니야",
          "original_form": "아 불족발은 아니야",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015476.1.15",
          "form": "냉채족발도 아니야?",
          "original_form": "냉채족발도 아니야?",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015476.1.16",
          "form": "ㅋㅋㅋㅋㅋ냉채족발도 아니야",
          "original_form": "ㅋㅋㅋㅋㅋ냉채족발도 아니야",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015476.1.17",
          "form": "그냥 일반 족발인데 짱맛있어",
          "original_form": "그냥 일반 족발인데 짱맛있어",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015476.1.18",
          "form": "address영양족발",
          "original_form": "&address&영양족발",
          "speaker_id": "2",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015476.1.19",
          "form": "오늘 점심 뭐먹어?",
          "original_form": "오늘 점심 뭐먹어?",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100015476.1.20",
          "form": "오늘은 서브웨이 먹었어",
          "original_form": "오늘은 서브웨이 먹었어",
          "speaker_id": "2",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100015476.1.21",
          "form": "샌드위치 좋아해?",
          "original_form": "샌드위치 좋아해?",
          "speaker_id": "2",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100015476.1.22",
          "form": "좋지",
          "original_form": "좋지",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100015476.1.23",
          "form": "에그드랍도 좋고",
          "original_form": "에그드랍도 좋고",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100015476.1.24",
          "form": "이삭토스트도 좋고",
          "original_form": "이삭토스트도 좋고",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100015476.1.25",
          "form": "오 난 아무리 뭐라해도 이삭토스트가 젤 좋다",
          "original_form": "오 난 아무리 뭐라해도 이삭토스트가 젤 좋다",
