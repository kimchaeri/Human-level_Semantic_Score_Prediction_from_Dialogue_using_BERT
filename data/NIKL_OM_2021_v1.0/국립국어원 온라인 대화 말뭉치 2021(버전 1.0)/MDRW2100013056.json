      "utterance": [
        {
          "id": "MDRW2100013056.1.1",
          "form": "오 미용실",
          "original_form": "오 미용실",
          "speaker_id": "2",
          "time": "20210825 11:44"
        },
        {
          "id": "MDRW2100013056.1.2",
          "form": "뿌염??",
          "original_form": "뿌염??",
          "speaker_id": "2",
          "time": "20210825 11:44"
        },
        {
          "id": "MDRW2100013056.1.3",
          "form": "뿌염도 해야되는데",
          "original_form": "뿌염도 해야되는데",
          "speaker_id": "1",
          "time": "20210825 11:44"
        },
        {
          "id": "MDRW2100013056.1.4",
          "form": "급한거는 머리가 너무 막 붕뜨고 곱슬해서ㅋㅋ",
          "original_form": "급한거는 머리가 너무 막 붕뜨고 곱슬해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:45"
        },
        {
          "id": "MDRW2100013056.1.5",
          "form": "매직펌하러옴. 2~3달전에",
          "original_form": "매직펌하러옴. 2~3달전에",
          "speaker_id": "1",
          "time": "20210825 11:45"
        },
        {
          "id": "MDRW2100013056.1.6",
          "form": "미용실에서 머리했는데 손상 제대로되가ㅜ",
          "original_form": "미용실에서 머리했는데 손상 제대로되가ㅜ",
          "speaker_id": "1",
          "time": "20210825 11:45"
        },
        {
          "id": "MDRW2100013056.1.7",
          "form": "미용실 어디 갔어??",
          "original_form": "미용실 어디 갔어??",
          "speaker_id": "2",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100013056.1.8",
          "form": "에헤이 손상돼서 어떡하노",
          "original_form": "에헤이 손상돼서 어떡하노",
          "speaker_id": "2",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100013056.1.9",
          "form": "나도 전에 싼 데서 머리했는데",
          "original_form": "나도 전에 싼 데서 머리했는데",
          "speaker_id": "2",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100013056.1.10",
          "form": "그 뒤로 머리 다 상해서",
          "original_form": "그 뒤로 머리 다 상해서",
          "speaker_id": "2",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100013056.1.11",
          "form": "아직도 머리 엉킴",
          "original_form": "아직도 머리 엉킴",
          "speaker_id": "2",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100013056.1.12",
          "form": "응 그러니깐",
          "original_form": "응 그러니깐",
          "speaker_id": "1",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100013056.1.13",
          "form": "지금은 유헤어왔는데",
          "original_form": "지금은 유헤어왔는데",
          "speaker_id": "1",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100013056.1.14",
          "form": "그전엔 가온",
          "original_form": "그전엔 가온",
          "speaker_id": "1",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100013056.1.15",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 11:49"
        },
        {
          "id": "MDRW2100013056.1.16",
          "form": "가온이 거기 오케이 마트에 있는 골목 맞제",
          "original_form": "가온이 거기 오케이 마트에 있는 골목 맞제",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013056.1.17",
          "form": "이마트 24 이층",
          "original_form": "이마트 24 이층",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013056.1.18",
          "form": "나도 가온에서 파마랑 염색하고 머릿결 썩었음",
          "original_form": "나도 가온에서 파마랑 염색하고 머릿결 썩었음",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013056.1.19",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "1",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100013056.1.20",
          "form": "아 언니도그랫구나ㅜ",
          "original_form": "아 언니도그랫구나ㅜ",
          "speaker_id": "1",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100013056.1.21",
          "form": "물어보고 갈걸ㅜ",
          "original_form": "물어보고 갈걸ㅜ",
          "speaker_id": "1",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100013056.1.22",
          "form": "거기 가격은 싼데 약이 정말 별로임",
          "original_form": "거기 가격은 싼데 약이 정말 별로임",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013056.1.23",
          "form": "싼 데는 다 이유가 있다",
          "original_form": "싼 데는 다 이유가 있다",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013056.1.24",
          "form": "거기 그때 오픈했을 때 갔는데",
          "original_form": "거기 그때 오픈했을 때 갔는데",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013056.1.25",
          "form": "아직도 머릿결이 엉망 임",
          "original_form": "아직도 머릿결이 엉망 임",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013056.1.26",
          "form": "그렇네 진짜",
          "original_form": "그렇네 진짜",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013056.1.27",
          "form": "그래두 6만원주고 파마했는데",
          "original_form": "그래두 6만원주고 파마했는데",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013056.1.28",
          "form": "정말 엉망이다",
          "original_form": "정말 엉망이다",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100013056.1.29",
          "form": "그래 진짜 짜증남",
          "original_form": "그래 진짜 짜증남",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013056.1.30",
          "form": "내 그때 파마하고 염색 같은날 했거든",
          "original_form": "내 그때 파마하고 염색 같은날 했거든",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013056.1.31",
          "form": "두개같이하면 머리결 상하잖아",
          "original_form": "두개같이하면 머리결 상하잖아",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013056.1.32",
          "form": "그래서 물어보고 영양도 같이 넣어달라했는데",
          "original_form": "그래서 물어보고 영양도 같이 넣어달라했는데",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013056.1.33",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "1",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013056.1.34",
          "form": "나두 두개같이 하려다가 그냥 하나만했거든ㅜ아휴",
          "original_form": "나두 두개같이 하려다가 그냥 하나만했거든ㅜ아휴",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013056.1.35",
          "form": "진짜 이럴줄이야",
          "original_form": "진짜 이럴줄이야",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013056.1.36",
          "form": "빗질이안된다",
          "original_form": "빗질이안된다",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013056.1.37",
          "form": "맞제?? 나는 염색약 바르자마자",
          "original_form": "맞제?? 나는 염색약 바르자마자",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013056.1.38",
          "form": "두피 타는거 같이 너무 따가워서",
          "original_form": "두피 타는거 같이 너무 따가워서",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013056.1.39",
          "form": "바로 씻겨달라했거든",
          "original_form": "바로 씻겨달라했거든",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013056.1.40",
          "form": "머리감고나서도 너무 따갑고 타는기분인거야",
          "original_form": "머리감고나서도 너무 따갑고 타는기분인거야",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013056.1.41",
          "form": "그뒤로 머리 빗질안되서",
          "original_form": "그뒤로 머리 빗질안되서",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013056.1.42",
          "form": "여태 중간중간 가위로 자름....",
          "original_form": "여태 중간중간 가위로 자름....",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013056.1.43",
          "form": "거기오픈행사때였는데",
          "original_form": "거기오픈행사때였는데",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013056.1.44",
          "form": "휴....",
          "original_form": "휴....",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013056.1.45",
          "form": "그라이 머리정리하고왔다",
          "original_form": "그라이 머리정리하고왔다",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013056.1.46",
          "form": "좋다ㅋ",
          "original_form": "좋다ㅋ",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013056.1.47",
          "form": "잘했네",
          "original_form": "잘했네",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013056.1.48",
          "form": "계속 잘라내야됨",
          "original_form": "계속 잘라내야됨",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013056.1.49",
          "form": "맞다. 맞다. ㅋㅋㅋ 이제 집에왔다 .점심먹으려구",
          "original_form": "맞다. 맞다. ㅋㅋㅋ 이제 집에왔다 .점심먹으려구",
          "speaker_id": "1",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100013056.1.50",
          "form": "언니 나도 ㅋㅋ",
          "original_form": "언니 나도 ㅋㅋ",
          "speaker_id": "1",
