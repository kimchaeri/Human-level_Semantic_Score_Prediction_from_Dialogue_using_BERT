      "utterance": [
        {
          "id": "MDRW2100045232.1.1",
          "form": "저는 샹치도 봤어요",
          "original_form": "저는 샹치도 봤어요",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045232.1.2",
          "form": "샹치 어떤 영화인가요",
          "original_form": "샹치 어떤 영화인가요",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045232.1.3",
          "form": "새로 나온 마블이에요",
          "original_form": "새로 나온 마블이에요",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045232.1.4",
          "form": "마블에 그런 게 있었나",
          "original_form": "마블에 그런 게 있었나",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045232.1.5",
          "form": "중국에서 만들었더라구요",
          "original_form": "중국에서 만들었더라구요",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045232.1.6",
          "form": "재미없을 거 같은데",
          "original_form": "재미없을 거 같은데",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045232.1.7",
          "form": "아니에요 완전 꿀잼",
          "original_form": "아니에요 완전 꿀잼",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045232.1.8",
          "form": "마블이라 평 타는 치나 봐요",
          "original_form": "마블이라 평 타는 치나 봐요",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045232.1.9",
          "form": "저도 기대 하나도 안 했는데",
          "original_form": "저도 기대 하나도 안 했는데",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045232.1.10",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045232.1.11",
          "form": "일단 양조위가 잘생김",
          "original_form": "일단 양조위가 잘생김",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045232.1.12",
          "form": "그 분은 레전드",
          "original_form": "그 분은 레전드",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045232.1.13",
          "form": "중국 배우도 액션 잘하더라구요",
          "original_form": "중국 배우도 액션 잘하더라구요",
