      "utterance": [
        {
          "id": "MDRW2100046917.1.1",
          "form": "반가워연",
          "original_form": "반가워연",
          "speaker_id": "1",
          "time": "20211008 17:25"
        },
        {
          "id": "MDRW2100046917.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20211008 17:31"
        },
        {
          "id": "MDRW2100046917.1.3",
          "form": "카카오 공화국이라는 말이 있었을 정도였죠 ㅎㅎ",
          "original_form": "카카오 공화국이라는 말이 있었을 정도였죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211008 17:31"
        },
        {
          "id": "MDRW2100046917.1.4",
          "form": "동의해요",
          "original_form": "동의해요",
          "speaker_id": "1",
          "time": "20211008 17:34"
        },
        {
          "id": "MDRW2100046917.1.5",
          "form": "지금 얘기아닌가요?",
          "original_form": "지금 얘기아닌가요?",
          "speaker_id": "1",
          "time": "20211008 17:34"
        },
        {
          "id": "MDRW2100046917.1.6",
          "form": "맞아요 친구들이랑 카카오에서 새로운 플랫폼 나올때마다 하는 얘기였는데",
          "original_form": "맞아요 친구들이랑 카카오에서 새로운 플랫폼 나올때마다 하는 얘기였는데",
          "speaker_id": "2",
          "time": "20211008 17:34"
        },
        {
          "id": "MDRW2100046917.1.7",
          "form": "이젠 정말 카카오공화국이 된것 같네요",
          "original_form": "이젠 정말 카카오공화국이 된것 같네요",
          "speaker_id": "2",
          "time": "20211008 17:34"
        },
        {
          "id": "MDRW2100046917.1.8",
          "form": "그게 플랫폼의 무서움이죠",
          "original_form": "그게 플랫폼의 무서움이죠",
          "speaker_id": "1",
          "time": "20211008 17:35"
        },
        {
          "id": "MDRW2100046917.1.9",
          "form": "카카오에서 톡 말고 가장 자주쓰는 플랫폼이 있으신가요??",
          "original_form": "카카오에서 톡 말고 가장 자주쓰는 플랫폼이 있으신가요??",
          "speaker_id": "2",
          "time": "20211008 17:36"
        },
        {
          "id": "MDRW2100046917.1.10",
          "form": "저는 음",
          "original_form": "저는 음",
          "speaker_id": "1",
          "time": "20211008 17:36"
        },
        {
          "id": "MDRW2100046917.1.11",
          "form": "카카오 맵?",
          "original_form": "카카오 맵?",
          "speaker_id": "1",
          "time": "20211008 17:36"
        },
        {
          "id": "MDRW2100046917.1.12",
          "form": "버스 시간도 나오고 가게 전화번호도",
          "original_form": "버스 시간도 나오고 가게 전화번호도",
          "speaker_id": "1",
          "time": "20211008 17:37"
        },
        {
          "id": "MDRW2100046917.1.13",
          "form": "나오고 편한거 같아요",
          "original_form": "나오고 편한거 같아요",
          "speaker_id": "1",
          "time": "20211008 17:37"
        },
        {
          "id": "MDRW2100046917.1.14",
          "form": "오 맞아요 저도 카카오 버스 자주 이용해요",
          "original_form": "오 맞아요 저도 카카오 버스 자주 이용해요",
          "speaker_id": "2",
          "time": "20211008 17:37"
        },
        {
          "id": "MDRW2100046917.1.15",
          "form": "저는 뱅크나 페이같은 금융 플랫폼도 많이 이용해요",
          "original_form": "저는 뱅크나 페이같은 금융 플랫폼도 많이 이용해요",
          "speaker_id": "2",
          "time": "20211008 17:37"
        },
        {
          "id": "MDRW2100046917.1.16",
          "form": "그래도 정부에서 플랫폼을 너무 막는거 같지는 않아서",
          "original_form": "그래도 정부에서 플랫폼을 너무 막는거 같지는 않아서",
          "speaker_id": "1",
          "time": "20211008 17:38"
        },
        {
          "id": "MDRW2100046917.1.17",
          "form": "음.. 대형 플랫폼들도 좀",
          "original_form": "음.. 대형 플랫폼들도 좀",
          "speaker_id": "1",
          "time": "20211008 17:38"
        },
        {
          "id": "MDRW2100046917.1.18",
          "form": "사회적으로 공익을 추구했으면 하네요 좀 더",
          "original_form": "사회적으로 공익을 추구했으면 하네요 좀 더",
          "speaker_id": "1",
          "time": "20211008 17:39"
        },
        {
          "id": "MDRW2100046917.1.19",
          "form": "어떤 공익이 있으면 좋을 것 같으세요??",
          "original_form": "어떤 공익이 있으면 좋을 것 같으세요??",
          "speaker_id": "2",
          "time": "20211008 17:41"
        },
        {
          "id": "MDRW2100046917.1.20",
          "form": "수수료를 적당하게 유지한다던디",
          "original_form": "수수료를 적당하게 유지한다던디",
          "speaker_id": "1",
          "time": "20211008 17:42"
        },
        {
          "id": "MDRW2100046917.1.21",
          "form": "던지",
          "original_form": "던지",
          "speaker_id": "1",
          "time": "20211008 17:42"
        },
        {
          "id": "MDRW2100046917.1.22",
          "form": "그리고 음",
          "original_form": "그리고 음",
          "speaker_id": "1",
          "time": "20211008 17:42"
        },
        {
          "id": "MDRW2100046917.1.23",
          "form": "개인정보의 보안성? 에 힘을 잘 쓴다던지",
          "original_form": "개인정보의 보안성? 에 힘을 잘 쓴다던지",
          "speaker_id": "1",
          "time": "20211008 17:43"
        },
        {
          "id": "MDRW2100046917.1.24",
          "form": "그런거에요",
          "original_form": "그런거에요",
          "speaker_id": "1",
          "time": "20211008 17:43"
        },
        {
          "id": "MDRW2100046917.1.25",
          "form": "오..그러게요 다 찬성하는 것들이에요!!",
          "original_form": "오..그러게요 다 찬성하는 것들이에요!!",
          "speaker_id": "2",
          "time": "20211008 17:43"
        },
        {
          "id": "MDRW2100046917.1.26",
          "form": "독점구조는 어떻게 막을 수 없을까요..",
          "original_form": "독점구조는 어떻게 막을 수 없을까요..",
          "speaker_id": "2",
          "time": "20211008 17:46"
        },
        {
          "id": "MDRW2100046917.1.27",
          "form": "다소 힘들거라고 봐요",
          "original_form": "다소 힘들거라고 봐요",
          "speaker_id": "1",
          "time": "20211008 17:54"
        },
        {
          "id": "MDRW2100046917.1.28",
          "form": "중소기업들은",
          "original_form": "중소기업들은",
          "speaker_id": "1",
          "time": "20211008 17:55"
        },
        {
          "id": "MDRW2100046917.1.29",
          "form": "이런 플랫폼 시장에서는 크기 힘들잖아요",
          "original_form": "이런 플랫폼 시장에서는 크기 힘들잖아요",
          "speaker_id": "1",
          "time": "20211008 17:55"
        },
        {
          "id": "MDRW2100046917.1.30",
          "form": "음식점 같은거면 몰라도",
          "original_form": "음식점 같은거면 몰라도",
          "speaker_id": "1",
          "time": "20211008 17:55"
        },
        {
          "id": "MDRW2100046917.1.31",
          "form": "아마 정부에서 규제를 한다고 해도",
          "original_form": "아마 정부에서 규제를 한다고 해도",
          "speaker_id": "1",
          "time": "20211008 17:55"
        },
        {
          "id": "MDRW2100046917.1.32",
          "form": "무리가 있을 수 있을거 같아요",
          "original_form": "무리가 있을 수 있을거 같아요",
          "speaker_id": "1",
          "time": "20211008 17:55"
        },
        {
          "id": "MDRW2100046917.1.33",
          "form": "그래서 정당히 막나가지 않을 만큼의 규제",
          "original_form": "그래서 정당히 막나가지 않을 만큼의 규제",
          "speaker_id": "1",
          "time": "20211008 17:56"
        },
        {
          "id": "MDRW2100046917.1.34",
          "form": "적당히",
          "original_form": "적당히",
          "speaker_id": "1",
          "time": "20211008 17:56"
        },
        {
          "id": "MDRW2100046917.1.35",
          "form": "를 정부하 하는게 답이라고 생각해요",
          "original_form": "를 정부하 하는게 답이라고 생각해요",
          "speaker_id": "1",
          "time": "20211008 17:56"
        }
      ]
