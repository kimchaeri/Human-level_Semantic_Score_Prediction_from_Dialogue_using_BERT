      "utterance": [
        {
          "id": "MDRW2100029755.1.1",
          "form": "이거 우리 만남부터가",
          "original_form": "이거 우리 만남부터가",
          "speaker_id": "1",
          "time": "20210903 14:08"
        },
        {
          "id": "MDRW2100029755.1.2",
          "form": "이거잖아요",
          "original_form": "이거잖아요",
          "speaker_id": "1",
          "time": "20210903 14:08"
        },
        {
          "id": "MDRW2100029755.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ 나는 입시떨어지고 name3누나랑 친해지려고 했지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 나는 입시떨어지고 &name3&누나랑 친해지려고 했지",
          "speaker_id": "2",
          "time": "20210903 14:09"
        },
        {
          "id": "MDRW2100029755.1.4",
          "form": "근데 지금 생각해도",
          "original_form": "근데 지금 생각해도",
          "speaker_id": "2",
          "time": "20210903 14:09"
        },
        {
          "id": "MDRW2100029755.1.5",
          "form": "내 캐릭터 와 그리됐지",
          "original_form": "내 캐릭터 와 그리됐지",
          "speaker_id": "2",
          "time": "20210903 14:09"
        },
        {
          "id": "MDRW2100029755.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ본인의 의지와 달라졌어요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ본인의 의지와 달라졌어요?",
          "speaker_id": "1",
          "time": "20210903 14:09"
        },
        {
          "id": "MDRW2100029755.1.7",
          "form": "처음에도 약간 고고한 애기할배아니었나",
          "original_form": "처음에도 약간 고고한 애기할배아니었나",
          "speaker_id": "1",
          "time": "20210903 14:09"
        },
        {
          "id": "MDRW2100029755.1.8",
          "form": "할배는 아니었...지만...",
          "original_form": "할배는 아니었...지만...",
          "speaker_id": "2",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100029755.1.9",
          "form": "늙었잖아요",
          "original_form": "늙었잖아요",
          "speaker_id": "1",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100029755.1.10",
          "form": "name3누나 캐릭터는 사실 커플이 있었는데",
          "original_form": "&name3&누나 캐릭터는 사실 커플이 있었는데",
          "speaker_id": "2",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100029755.1.11",
          "form": "가로채기 꼴이 됐잖아",
          "original_form": "가로채기 꼴이 됐잖아",
          "speaker_id": "2",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100029755.1.12",
          "form": "이얘기였음",
          "original_form": "이얘기였음",
          "speaker_id": "2",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100029755.1.13",
          "form": "본인의지는 아니었죠",
          "original_form": "본인의지는 아니었죠",
          "speaker_id": "1",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100029755.1.14",
          "form": "참고로 난 계속 그부분 뇌끈 조이고 있었어서",
          "original_form": "참고로 난 계속 그부분 뇌끈 조이고 있었어서",
          "speaker_id": "2",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100029755.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100029755.1.16",
          "form": "마음껏 즐기지조 못함ㅋㅋㅋㅋ",
          "original_form": "마음껏 즐기지조 못함ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100029755.1.17",
          "form": "구래도 친해진다는 목적은 달성했었지",
          "original_form": "구래도 친해진다는 목적은 달성했었지",
          "speaker_id": "1",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100029755.1.18",
          "form": "커뮤니티의 목적",
          "original_form": "커뮤니티의 목적",
          "speaker_id": "1",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100029755.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100029755.1.20",
          "form": "나",
          "original_form": "나",
          "speaker_id": "2",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100029755.1.21",
          "form": "커뮤할 때",
          "original_form": "커뮤할 때",
          "speaker_id": "2",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100029755.1.22",
          "form": "name3누나 앤캐 오너분한테",
          "original_form": "&name3&누나 앤캐 오너분한테",
          "speaker_id": "2",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100029755.1.23",
          "form": "견제도 당함ㅠ",
          "original_form": "견제도 당함ㅠ",
          "speaker_id": "2",
          "time": "20210903 14:15"
        },
        {
          "id": "MDRW2100029755.1.24",
          "form": "죄도 없는데 견제당하다니",
          "original_form": "죄도 없는데 견제당하다니",
          "speaker_id": "1",
          "time": "20210903 14:18"
        },
        {
          "id": "MDRW2100029755.1.25",
          "form": "아니 누가바도 호모엿으니까...",
          "original_form": "아니 누가바도 호모엿으니까...",
          "speaker_id": "2",
          "time": "20210903 14:23"
        },
        {
          "id": "MDRW2100029755.1.26",
          "form": "당신은 가만히 있었자나요",
          "original_form": "당신은 가만히 있었자나요",
          "speaker_id": "1",
          "time": "20210903 14:24"
        },
        {
          "id": "MDRW2100029755.1.27",
          "form": "안하진 않았지 .  근데 난... 토모로하고 싶엇어",
          "original_form": "안하진 않았지 .  근데 난... 토모로하고 싶엇어",
          "speaker_id": "2",
          "time": "20210903 14:24"
        },
        {
          "id": "MDRW2100029755.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:25"
        },
        {
          "id": "MDRW2100029755.1.29",
          "form": "근데 뭐.... 당해봤자죠. 지금 생각하면 ㅋㅋㅋㅋㅋㅋ 그 작은 커뮤에서...",
          "original_form": "근데 뭐.... 당해봤자죠. 지금 생각하면 ㅋㅋㅋㅋㅋㅋ 그 작은 커뮤에서...",
          "speaker_id": "1",
          "time": "20210903 14:26"
        },
        {
          "id": "MDRW2100029755.1.30",
          "form": "견제받아도 머....",
          "original_form": "견제받아도 머....",
          "speaker_id": "1",
          "time": "20210903 14:26"
        },
        {
          "id": "MDRW2100029755.1.31",
          "form": "음....ㅎㅎ",
          "original_form": "음....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 14:26"
        },
        {
          "id": "MDRW2100029755.1.32",
          "form": "노눈건데 뭐",
          "original_form": "노눈건데 뭐",
