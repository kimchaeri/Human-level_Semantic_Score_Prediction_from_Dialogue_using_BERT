      "utterance": [
        {
          "id": "MDRW2100014763.1.1",
          "form": "결혼함?",
          "original_form": "결혼함?",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014763.1.2",
          "form": "이거말고 다른거하잨ㅋㅋㅋㅋ",
          "original_form": "이거말고 다른거하잨ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014763.1.3",
          "form": "나 미혼.. ㅋㅋㅋ 만인의 여인으로 남고싶어..ㅋㅋ \r\nname2님은? 미혼?",
          "original_form": "나 미혼.. ㅋㅋㅋ 만인의 여인으로 남고싶어..ㅋㅋ \r\n&name2&님은? 미혼?",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100014763.1.4",
          "form": "미혼인데 난 결혼할거양",
          "original_form": "미혼인데 난 결혼할거양",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100014763.1.5",
          "form": "만인의연인보다 한곳에 정착해서 안정적으로 살고싶음",
          "original_form": "만인의연인보다 한곳에 정착해서 안정적으로 살고싶음",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100014763.1.6",
          "form": "그럴 상대는 있구? ㅋㅋㅋㅋ",
          "original_form": "그럴 상대는 있구? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100014763.1.7",
          "form": "웅 남자친구있음ㅎㅎ 넌 없구나 풉",
          "original_form": "웅 남자친구있음ㅎㅎ 넌 없구나 풉",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100014763.1.8",
          "form": "오랜 연예의 기본을 모라 생각해?",
          "original_form": "오랜 연예의 기본을 모라 생각해?",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100014763.1.9",
          "form": "바로 진실성이야.. ㅋㅋㅋ",
          "original_form": "바로 진실성이야.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100014763.1.10",
          "form": "거짓말하디마..",
          "original_form": "거짓말하디마..",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100014763.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 왜 안믿는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 왜 안믿는데",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100014763.1.12",
          "form": "난 개그코드잘맞는게 중요한거같음",
          "original_form": "난 개그코드잘맞는게 중요한거같음",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100014763.1.13",
          "form": "어떤 개그 좋아하는데?",
          "original_form": "어떤 개그 좋아하는데?",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100014763.1.14",
          "form": "화장실 유모아?",
          "original_form": "화장실 유모아?",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100014763.1.15",
          "form": "유모아는 무슨 언젯적 만득이시리즈같은 소리야 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "유모아는 무슨 언젯적 만득이시리즈같은 소리야 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100014763.1.16",
          "form": "만득이가 몬데요? ㅋㅋㅋㅋ",
          "original_form": "만득이가 몬데요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100014763.1.17",
          "form": "사랑싸움은 자주해?",
          "original_form": "사랑싸움은 자주해?",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100014763.1.18",
          "form": "처음엔 좀 별거아닌걸로 서운해하고 그랬는데 이젠 안그래ㅋㅋ",
          "original_form": "처음엔 좀 별거아닌걸로 서운해하고 그랬는데 이젠 안그래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100014763.1.19",
          "form": "넌 여자친구 있었던적 있어?",
          "original_form": "넌 여자친구 있었던적 있어?",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100014763.1.20",
          "form": "왜 아픈데  때려?",
          "original_form": "왜 아픈데  때려?",
          "speaker_id": "1",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100014763.1.21",
          "form": "연예에 별 관심이가 엄서... \r\n금방 질리는 성격이라..",
          "original_form": "연예에 별 관심이가 엄서... \r\n금방 질리는 성격이라..",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100014763.1.22",
          "form": "아픈데 때리는건 너 전문이잖아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아픈데 때리는건 너 전문이잖아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100014763.1.23",
          "form": "그치 금방질려해서 오래 못만나는애들 있더라",
          "original_form": "그치 금방질려해서 오래 못만나는애들 있더라",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100014763.1.24",
          "form": "ㅇㅇㅇ 너는 그런 연예 잘모르지?",
          "original_form": "ㅇㅇㅇ 너는 그런 연예 잘모르지?",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100014763.1.25",
          "form": "막 매달리는거 지겹지 않아? ㅋㅋㅋ",
          "original_form": "막 매달리는거 지겹지 않아? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100014763.1.26",
          "form": "움 막 매달리고 그런건 안해봐서 모르겠네",
          "original_form": "움 막 매달리고 그런건 안해봐서 모르겠네",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100014763.1.27",
          "form": "연애 관심없을수도있지 뭐",
          "original_form": "연애 관심없을수도있지 뭐",
