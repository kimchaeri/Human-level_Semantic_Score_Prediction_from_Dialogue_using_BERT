      "utterance": [
        {
          "id": "MDRW2100031872.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210905 06:47"
        },
        {
          "id": "MDRW2100031872.1.2",
          "form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "original_form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "speaker_id": "1",
          "time": "20210905 06:47"
        },
        {
          "id": "MDRW2100031872.1.3",
          "form": "가을이오면 어느 것이 가장 기대되시나요?",
          "original_form": "가을이오면 어느 것이 가장 기대되시나요?",
          "speaker_id": "1",
          "time": "20210905 06:47"
        },
        {
          "id": "MDRW2100031872.1.4",
          "form": "기대까지되는건없고 날씨가 좋아 기분이 좋네요",
          "original_form": "기대까지되는건없고 날씨가 좋아 기분이 좋네요",
          "speaker_id": "2",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100031872.1.5",
          "form": "화창하고 깨끗해서",
          "original_form": "화창하고 깨끗해서",
          "speaker_id": "2",
          "time": "20210905 07:29"
        },
        {
          "id": "MDRW2100031872.1.6",
          "form": "한동안 가을장마라고 2주정도 비가 오더니",
          "original_form": "한동안 가을장마라고 2주정도 비가 오더니",
          "speaker_id": "1",
          "time": "20210905 07:29"
        },
        {
          "id": "MDRW2100031872.1.7",
          "form": "주말부터 날씨가 좋아져서 정말 좋습니다",
          "original_form": "주말부터 날씨가 좋아져서 정말 좋습니다",
          "speaker_id": "1",
          "time": "20210905 07:29"
        },
        {
          "id": "MDRW2100031872.1.8",
          "form": "날씨요즘넘좋아요",
          "original_form": "날씨요즘넘좋아요",
          "speaker_id": "2",
          "time": "20210905 07:30"
        },
        {
          "id": "MDRW2100031872.1.9",
          "form": "이제 진짜 가을이 오려나 봅니다",
          "original_form": "이제 진짜 가을이 오려나 봅니다",
          "speaker_id": "1",
          "time": "20210905 07:31"
        },
        {
          "id": "MDRW2100031872.1.10",
          "form": "하늘 진짜예뻐요",
          "original_form": "하늘 진짜예뻐요",
          "speaker_id": "2",
          "time": "20210905 07:31"
        },
        {
          "id": "MDRW2100031872.1.11",
          "form": "좀전에 산책 다녀왔는데 좋더군요",
          "original_form": "좀전에 산책 다녀왔는데 좋더군요",
          "speaker_id": "1",
          "time": "20210905 07:31"
        },
        {
          "id": "MDRW2100031872.1.12",
          "form": "벌써요?",
          "original_form": "벌써요?",
          "speaker_id": "2",
          "time": "20210905 07:31"
        },
        {
          "id": "MDRW2100031872.1.13",
          "form": "일요일인데 부지런하시네요..",
          "original_form": "일요일인데 부지런하시네요..",
          "speaker_id": "2",
          "time": "20210905 07:32"
        },
        {
          "id": "MDRW2100031872.1.14",
          "form": "제가 아침에 일찍 일어납니다",
          "original_form": "제가 아침에 일찍 일어납니다",
          "speaker_id": "1",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031872.1.15",
          "form": "아침형인간이시군요..대부분일요일은 늦게일어나는데",
          "original_form": "아침형인간이시군요..대부분일요일은 늦게일어나는데",
          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031872.1.16",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210905 08:10"
        },
        {
          "id": "MDRW2100031872.1.17",
          "form": "항상 일찍 일어납니다",
          "original_form": "항상 일찍 일어납니다",
          "speaker_id": "1",
          "time": "20210905 08:10"
        },
        {
          "id": "MDRW2100031872.1.18",
          "form": "습관이에요",
          "original_form": "습관이에요",
          "speaker_id": "1",
          "time": "20210905 08:11"
        },
        {
          "id": "MDRW2100031872.1.19",
          "form": "날씨좋아서 산책하기좋으셨겠어요",
          "original_form": "날씨좋아서 산책하기좋으셨겠어요",
          "speaker_id": "2",
          "time": "20210905 08:11"
        },
        {
          "id": "MDRW2100031872.1.20",
          "form": "저도이제일어나려구요",
          "original_form": "저도이제일어나려구요",
          "speaker_id": "2",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100031872.1.21",
          "form": "산책하기 아주 좋은 아침이었지요",
          "original_form": "산책하기 아주 좋은 아침이었지요",
          "speaker_id": "1",
          "time": "20210905 08:12"
        },
        {
          "id": "MDRW2100031872.1.22",
          "form": "가을은 걷기에 좋은 계절입니다",
          "original_form": "가을은 걷기에 좋은 계절입니다",
          "speaker_id": "1",
          "time": "20210905 08:13"
        }
      ]
