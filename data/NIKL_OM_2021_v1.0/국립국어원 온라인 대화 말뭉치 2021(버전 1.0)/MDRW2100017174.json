        },
        {
          "id": "MDRW2100017174.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017174.1.4",
          "form": "그래서 다시 사고 싶어졌어",
          "original_form": "그래서 다시 사고 싶어졌어",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017174.1.5",
          "form": "필름카메라?",
          "original_form": "필름카메라?",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017174.1.6",
          "form": "아니 디지털!",
          "original_form": "아니 디지털!",
          "speaker_id": "2",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017174.1.7",
          "form": "필름카메라는 찍으면 바로 못보는게 좀 불편하더라고",
          "original_form": "필름카메라는 찍으면 바로 못보는게 좀 불편하더라고",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017174.1.8",
          "form": "필름 너무 비싸서",
          "original_form": "필름 너무 비싸서",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017174.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017174.1.10",
          "form": "필름 한 컷당",
          "original_form": "필름 한 컷당",
          "speaker_id": "2",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100017174.1.11",
          "form": "싸면 200원 비싸면 400원 가까이 되는 꼴이라서",
          "original_form": "싸면 200원 비싸면 400원 가까이 되는 꼴이라서",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017174.1.12",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017174.1.13",
          "form": "와 한장찍는데?",
          "original_form": "와 한장찍는데?",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017174.1.14",
          "form": "응...",
          "original_form": "응...",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017174.1.15",
          "form": "거의 그정도야",
          "original_form": "거의 그정도야",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017174.1.16",
          "form": "비싸네...",
          "original_form": "비싸네...",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017174.1.17",
          "form": "갑자기ㅜ엄청 비싸졌어",
          "original_form": "갑자기ㅜ엄청 비싸졌어",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017174.1.18",
          "form": "필름 카메라는 중고거래 많이 하는데",
          "original_form": "필름 카메라는 중고거래 많이 하는데",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017174.1.19",
          "form": "갑자기 비싸졋다고?왜??",
          "original_form": "갑자기 비싸졋다고?왜??",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017174.1.20",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017174.1.21",
          "form": "필름은 없더라고",
          "original_form": "필름은 없더라고",
          "speaker_id": "2",
          "time": "20210826 17:53"
