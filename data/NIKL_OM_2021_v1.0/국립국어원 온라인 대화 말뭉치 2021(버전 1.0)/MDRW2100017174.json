        {
          "id": "MDRW2100017174.1.22",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017174.1.23",
          "form": "모르겠어 갑자기 몇년 사이에",
          "original_form": "모르겠어 갑자기 몇년 사이에",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017174.1.24",
          "form": "몇천원씩 올랐대",
          "original_form": "몇천원씩 올랐대",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017174.1.25",
          "form": "덜덜",
          "original_form": "덜덜",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017174.1.26",
          "form": "아무래도 폰 카메라가 너무 좋아져서 그런것도 있는것 같고",
          "original_form": "아무래도 폰 카메라가 너무 좋아져서 그런것도 있는것 같고",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017174.1.27",
          "form": "그건맞음",
          "original_form": "그건맞음",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100017174.1.28",
          "form": "몇.년.사이에. 완전.좋아졌어",
