      "utterance": [
        {
          "id": "MDRW2100017174.1.1",
          "form": "좀 참다가",
          "original_form": "좀 참다가",
          "speaker_id": "2",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100017174.1.2",
          "form": "이번에 필름 감다가 찢었거든...",
          "original_form": "이번에 필름 감다가 찢었거든...",
          "speaker_id": "2",
