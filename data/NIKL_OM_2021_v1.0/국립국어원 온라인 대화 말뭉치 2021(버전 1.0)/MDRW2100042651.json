      "utterance": [
        {
          "id": "MDRW2100042651.1.1",
          "form": "혹시 당근마켓 해보셨나요",
          "original_form": "혹시 당근마켓 해보셨나요",
          "speaker_id": "1",
          "time": "20210907 00:24"
        },
        {
          "id": "MDRW2100042651.1.2",
          "form": "아 중고거래 맞죠 ?",
          "original_form": "아 중고거래 맞죠 ?",
          "speaker_id": "2",
          "time": "20210907 00:24"
        },
        {
          "id": "MDRW2100042651.1.3",
          "form": "네네 동네에서하는",
          "original_form": "네네 동네에서하는",
          "speaker_id": "1",
          "time": "20210907 00:24"
        },
        {
          "id": "MDRW2100042651.1.4",
          "form": "들어는 봤는데 해본적은 없어요",
          "original_form": "들어는 봤는데 해본적은 없어요",
          "speaker_id": "2",
          "time": "20210907 00:25"
        },
        {
          "id": "MDRW2100042651.1.5",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "2",
          "time": "20210907 00:25"
        },
        {
          "id": "MDRW2100042651.1.6",
          "form": "저는 몇번해봤어요",
          "original_form": "저는 몇번해봤어요",
          "speaker_id": "1",
          "time": "20210907 00:25"
        },
        {
          "id": "MDRW2100042651.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:25"
        },
        {
          "id": "MDRW2100042651.1.8",
          "form": "물건을 샀어요 ?",
          "original_form": "물건을 샀어요 ?",
          "speaker_id": "2",
          "time": "20210907 00:25"
        },
        {
          "id": "MDRW2100042651.1.9",
          "form": "아뇨!!",
          "original_form": "아뇨!!",
          "speaker_id": "1",
          "time": "20210907 00:26"
        },
        {
          "id": "MDRW2100042651.1.10",
          "form": "아 그럼 팔았어요 ?",
          "original_form": "아 그럼 팔았어요 ?",
          "speaker_id": "2",
          "time": "20210907 00:26"
        },
        {
          "id": "MDRW2100042651.1.11",
          "form": "네!  저는 팔기만ㅋㅋㅋ",
          "original_form": "네!  저는 팔기만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:26"
        },
        {
          "id": "MDRW2100042651.1.12",
          "form": "ㅎㅎ 뭐 팔았어요 ?",
          "original_form": "ㅎㅎ 뭐 팔았어요 ?",
          "speaker_id": "2",
          "time": "20210907 00:27"
        },
        {
          "id": "MDRW2100042651.1.13",
          "form": "ㅋㅋㅋㄱ안입는옷이랑",
          "original_form": "ㅋㅋㅋㄱ안입는옷이랑",
          "speaker_id": "1",
          "time": "20210907 00:27"
        },
        {
          "id": "MDRW2100042651.1.14",
          "form": "안쓰는 화장품들 팔았어요ㅋㅋ",
          "original_form": "안쓰는 화장품들 팔았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:27"
        },
        {
          "id": "MDRW2100042651.1.15",
          "form": "잘팔려요 ??",
          "original_form": "잘팔려요 ??",
          "speaker_id": "2",
          "time": "20210907 00:27"
        },
        {
          "id": "MDRW2100042651.1.16",
          "form": "아후",
          "original_form": "아후",
          "speaker_id": "1",
          "time": "20210907 00:28"
        },
        {
          "id": "MDRW2100042651.1.17",
          "form": "정말안팔려요",
          "original_form": "정말안팔려요",
          "speaker_id": "1",
          "time": "20210907 00:28"
        },
        {
          "id": "MDRW2100042651.1.18",
          "form": "완전헐값에 팔아야돼요",
          "original_form": "완전헐값에 팔아야돼요",
