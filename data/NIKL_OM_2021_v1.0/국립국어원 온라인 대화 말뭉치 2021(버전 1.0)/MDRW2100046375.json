      "utterance": [
        {
          "id": "MDRW2100046375.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20211021 00:17"
        },
        {
          "id": "MDRW2100046375.1.2",
          "form": "안녕하세요~ 점심시간인데 점심은 드셨나요?",
          "original_form": "안녕하세요~ 점심시간인데 점심은 드셨나요?",
          "speaker_id": "2",
          "time": "20211021 00:33"
        },
        {
          "id": "MDRW2100046375.1.3",
          "form": "네 맛점하셨나요",
          "original_form": "네 맛점하셨나요",
          "speaker_id": "1",
          "time": "20211021 00:34"
        },
        {
          "id": "MDRW2100046375.1.4",
          "form": "저는 마라샹궈에 꿔바로우 먹었어요 ㅎㅎ",
          "original_form": "저는 마라샹궈에 꿔바로우 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211021 00:34"
        },
        {
          "id": "MDRW2100046375.1.5",
          "form": "",
          "original_form": "{emoji}",
          "speaker_id": "1",
          "time": "20211021 00:34"
        },
        {
          "id": "MDRW2100046375.1.6",
          "form": "뭐 맛있는거 드셨나요?",
          "original_form": "뭐 맛있는거 드셨나요?",
          "speaker_id": "2",
          "time": "20211021 00:41"
        },
        {
          "id": "MDRW2100046375.1.7",
          "form": "저는 초밥먹었어요",
          "original_form": "저는 초밥먹었어요",
          "speaker_id": "1",
          "time": "20211021 00:42"
        },
        {
          "id": "MDRW2100046375.1.8",
          "form": "오 어떤 초밥 제일 좋아하시나요 저는 다 잘먹는데",
          "original_form": "오 어떤 초밥 제일 좋아하시나요 저는 다 잘먹는데",
          "speaker_id": "2",
          "time": "20211021 00:47"
        },
        {
          "id": "MDRW2100046375.1.9",
          "form": "연어가 제일 맛나요",
          "original_form": "연어가 제일 맛나요",
          "speaker_id": "2",
          "time": "20211021 00:47"
        },
        {
          "id": "MDRW2100046375.1.10",
          "form": "참치요",
          "original_form": "참치요",
          "speaker_id": "1",
          "time": "20211021 00:47"
        },
        {
          "id": "MDRW2100046375.1.11",
          "form": "다 잘먹긴해요",
          "original_form": "다 잘먹긴해요",
          "speaker_id": "1",
          "time": "20211021 00:47"
        },
        {
          "id": "MDRW2100046375.1.12",
          "form": "해물 종류를 다 잘드시나봐요!",
          "original_form": "해물 종류를 다 잘드시나봐요!",
          "speaker_id": "2",
          "time": "20211021 00:48"
        },
        {
          "id": "MDRW2100046375.1.13",
          "form": "해물을 좋아하진않는데 초밥은 좋아해요",
          "original_form": "해물을 좋아하진않는데 초밥은 좋아해요",
          "speaker_id": "1",
          "time": "20211021 00:49"
        },
        {
          "id": "MDRW2100046375.1.14",
          "form": "초밥이 먹기도 간단하고 ",
          "original_form": "초밥이 먹기도 간단하고 ",
          "speaker_id": "2",
          "time": "20211021 00:52"
        },
        {
          "id": "MDRW2100046375.1.15",
          "form": "도시락으로도 잘 나와서 점심으로 먹기 좋은 것 같아요",
          "original_form": "도시락으로도 잘 나와서 점심으로 먹기 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20211021 00:53"
        },
        {
          "id": "MDRW2100046375.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20211021 00:53"
        },
        {
          "id": "MDRW2100046375.1.17",
          "form": "저녁엔 뭐 드실 예정인가요?",
          "original_form": "저녁엔 뭐 드실 예정인가요?",
          "speaker_id": "2",
          "time": "20211021 00:54"
        },
        {
          "id": "MDRW2100046375.1.18",
          "form": "아직 미정이에요",
          "original_form": "아직 미정이에요",
          "speaker_id": "1",
          "time": "20211021 00:58"
        },
        {
          "id": "MDRW2100046375.1.19",
          "form": "점심만큼 맛있는 저녁 드시길 바라요",
          "original_form": "점심만큼 맛있는 저녁 드시길 바라요",
          "speaker_id": "2",
          "time": "20211020 13:04"
        }
      ]
