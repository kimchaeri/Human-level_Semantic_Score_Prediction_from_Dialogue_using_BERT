      "utterance": [
        {
          "id": "MDRW2100034274.1.1",
          "form": "저는 실내자전거 타요 ㅋㅋㅋ",
          "original_form": "저는 실내자전거 타요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100034274.1.2",
          "form": "실내자전거도 좋네요",
          "original_form": "실내자전거도 좋네요",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100034274.1.3",
          "form": "티비보면서 할수도있고",
          "original_form": "티비보면서 할수도있고",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100034274.1.4",
          "form": "밥먹고 바로타도",
          "original_form": "밥먹고 바로타도",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100034274.1.5",
          "form": "배가 안아파서 좋아요 ㅋㅋㅋ",
          "original_form": "배가 안아파서 좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100034274.1.6",
          "form": "저도 예전에 집에 있었는데 옷걸이로 쓰고",
          "original_form": "저도 예전에 집에 있었는데 옷걸이로 쓰고",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100034274.1.7",
          "form": "맞아요 폰보면서 할수도있고 ㅋㅋ",
          "original_form": "맞아요 폰보면서 할수도있고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100034274.1.8",
          "form": "버리게되었죠ㅋㅋ",
          "original_form": "버리게되었죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100034274.1.9",
          "form": "다들 그렇죠뭐 ㅋㅋㅋ",
          "original_form": "다들 그렇죠뭐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100034274.1.10",
          "form": "이거라도 안했으면 살 엄청 쪘을거에요 이미 쪘지만 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이거라도 안했으면 살 엄청 쪘을거에요 이미 쪘지만 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100034274.1.11",
          "form": "그러네요 밥먹고 바로해도 괜찮겠어요",
          "original_form": "그러네요 밥먹고 바로해도 괜찮겠어요",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100034274.1.12",
          "form": "특장점이에요 ㅋㅋ",
          "original_form": "특장점이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100034274.1.13",
          "form": "운동하긴 해야하는데 소화되는거 기다리다보면 하기싫어지거든요",
          "original_form": "운동하긴 해야하는데 소화되는거 기다리다보면 하기싫어지거든요",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100034274.1.14",
          "form": "아 맞아요 진짜 공감가요",
          "original_form": "아 맞아요 진짜 공감가요",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100034274.1.15",
          "form": "소화될때까지 기다리다가 눕게되고",
          "original_form": "소화될때까지 기다리다가 눕게되고",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100034274.1.16",
          "form": "맞아여 ㅋㅋㅋㅋ",
          "original_form": "맞아여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100034274.1.17",
          "form": "졸리더라구요ㅋㅋ",
          "original_form": "졸리더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100034274.1.18",
          "form": "하기싫단 생각이 들기도전에",
          "original_form": "하기싫단 생각이 들기도전에",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100034274.1.19",
          "form": "얼른 해치워버려야해요",
          "original_form": "얼른 해치워버려야해요",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100034274.1.20",
          "form": "ㅋㅋ마자요",
          "original_form": "ㅋㅋ마자요",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100034274.1.21",
          "form": "바로 실행에 옮겨야해요ㅋㅋ",
          "original_form": "바로 실행에 옮겨야해요ㅋㅋ",
