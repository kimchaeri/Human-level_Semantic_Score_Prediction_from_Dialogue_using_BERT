      "utterance": [
        {
          "id": "MDRW2100038197.1.1",
          "form": "와 어떤걸로 사셨어요??",
          "original_form": "와 어떤걸로 사셨어요??",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100038197.1.2",
          "form": "순살치킨이랑 고추바사삭? 치킨이 있더라구요",
          "original_form": "순살치킨이랑 고추바사삭? 치킨이 있더라구요",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100038197.1.3",
          "form": "고.추바사삭 맛있을거 같아요",
          "original_form": "고.추바사삭 맛있을거 같아요",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100038197.1.4",
          "form": "순살은 먹어봤는데 바사삭은 어떨지 궁금해요",
          "original_form": "순살은 먹어봤는데 바사삭은 어떨지 궁금해요",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100038197.1.5",
          "form": "네 그렇더라구요 ㅋ 저도 다시 썼어요",
          "original_form": "네 그렇더라구요 ㅋ 저도 다시 썼어요",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100038197.1.6",
          "form": "김만 사셨어요?",
          "original_form": "김만 사셨어요?",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100038197.1.7",
          "form": "여러가지 샀어요",
          "original_form": "여러가지 샀어요",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100038197.1.8",
          "form": "잼도 팔고 냉동식품도 팔길래",
          "original_form": "잼도 팔고 냉동식품도 팔길래",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100038197.1.9",
          "form": "아...잼도 필요한데",
          "original_form": "아...잼도 필요한데",
          "speaker_id": "1",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100038197.1.10",
          "form": "치킨너겟 하나 사고 용두동 쭈꾸미도 사고 냉동삼치도 사고 사과잼사고",
          "original_form": "치킨너겟 하나 사고 용두동 쭈꾸미도 사고 냉동삼치도 사고 사과잼사고",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100038197.1.11",
          "form": "거기도 냉동 실온 따로따로 배송비받아요?",
          "original_form": "거기도 냉동 실온 따로따로 배송비받아요?",
          "speaker_id": "1",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100038197.1.12",
          "form": "할인하니까 여러가지 골랐어요 네",
          "original_form": "할인하니까 여러가지 골랐어요 네",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100038197.1.13",
          "form": "그래서 다양하게 샀어요 ㅋㅋㅋ",
          "original_form": "그래서 다양하게 샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100038197.1.14",
          "form": "냉동삼치.. 맛이 궁금하네요",
          "original_form": "냉동삼치.. 맛이 궁금하네요",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038197.1.15",
          "form": "냉동자리가 없어서요",
          "original_form": "냉동자리가 없어서요",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038197.1.16",
          "form": "그냥 생선? ㅎㅎ",
          "original_form": "그냥 생선? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038197.1.17",
          "form": "냉동 삼치 괜찮을거에요 네 그냥 생선",
          "original_form": "냉동 삼치 괜찮을거에요 네 그냥 생선",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038197.1.18",
          "form": "저도 걱정이예요",
          "original_form": "저도 걱정이예요",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038197.1.19",
          "form": "가시 없는걸로 샀거든요",
          "original_form": "가시 없는걸로 샀거든요",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038197.1.20",
          "form": "배송오기전에 냉파 좀 해야할듯요",
          "original_form": "배송오기전에 냉파 좀 해야할듯요",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038197.1.21",
          "form": "아...가시도 없는거면 좋네요",
          "original_form": "아...가시도 없는거면 좋네요",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100038197.1.22",
          "form": "ㅋㅋㅋ 저는 오늘 좀 비웠어요",
          "original_form": "ㅋㅋㅋ 저는 오늘 좀 비웠어요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100038197.1.23",
          "form": "아이스크림 급히 먹었어요",
          "original_form": "아이스크림 급히 먹었어요",
          "speaker_id": "2",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100038197.1.24",
          "form": "저는 엊그제 베라도",
          "original_form": "저는 엊그제 베라도",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038197.1.25",
          "form": "넣어놨는데 ㅋㅋ",
          "original_form": "넣어놨는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038197.1.26",
          "form": "하프갤런 ㅋ",
          "original_form": "하프갤런 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038197.1.27",
          "form": "ㅋㅋㅋ 빨리 해치우세여 그거 한입꺼린데??",
          "original_form": "ㅋㅋㅋ 빨리 해치우세여 그거 한입꺼린데??",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038197.1.28",
          "form": "ㅋㅋㅋㅋ 아닌가여...",
          "original_form": "ㅋㅋㅋㅋ 아닌가여...",
          "speaker_id": "2",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038197.1.29",
          "form": "하프갤런이요?",
          "original_form": "하프갤런이요?",
          "speaker_id": "1",
          "time": "20210902 00:40"
        },
        {
          "id": "MDRW2100038197.1.30",
          "form": "이거 저 목표 아이스크림이에여",
          "original_form": "이거 저 목표 아이스크림이에여",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100038197.1.31",
          "form": "ㅋㅋ 아니 나눠서 여러번 드시면 한입...",
          "original_form": "ㅋㅋ 아니 나눠서 여러번 드시면 한입...",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100038197.1.32",
          "form": "31일 업그레이드행사 ㅋㅋ",
          "original_form": "31일 업그레이드행사 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100038197.1.33",
          "form": "처럼 먹을수 있다는 이야기였답니닼ㅋㅋ",
          "original_form": "처럼 먹을수 있다는 이야기였답니닼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100038197.1.34",
          "form": "아 놓쳤네여 ㅋㅋㅋ",
          "original_form": "아 놓쳤네여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100038197.1.35",
          "form": "싱글 깁콘이 많아서 질렀는데",
          "original_form": "싱글 깁콘이 많아서 질렀는데",
          "speaker_id": "1",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100038197.1.36",
          "form": "까묵고 있었다",
          "original_form": "까묵고 있었다",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100038197.1.37",
          "form": "또 씨제이가 지르게하네요 ㅎ",
          "original_form": "또 씨제이가 지르게하네요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100038197.1.38",
          "form": "ㅋㅋ 사놓으면 잘먹어서 괜찮아여",
          "original_form": "ㅋㅋ 사놓으면 잘먹어서 괜찮아여",
