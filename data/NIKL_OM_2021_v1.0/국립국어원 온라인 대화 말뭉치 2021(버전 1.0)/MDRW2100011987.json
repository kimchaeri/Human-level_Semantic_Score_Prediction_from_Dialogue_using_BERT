      "utterance": [
        {
          "id": "MDRW2100011987.1.1",
          "form": "오늘 날씨 어땠어요",
          "original_form": "오늘 날씨 어땠어요",
          "speaker_id": "1",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100011987.1.2",
          "form": "출근할 때는 비가 너무 와서",
          "original_form": "출근할 때는 비가 너무 와서",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011987.1.3",
          "form": "차에서 내릴때 엄청 불편했ㅇㆍ요",
          "original_form": "차에서 내릴때 엄청 불편했ㅇㆍ요",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011987.1.4",
          "form": "비 계속 왔죠ㅎㅎ",
          "original_form": "비 계속 왔죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100011987.1.5",
          "form": "아침 퇴근이라 ㅎㅎ",
          "original_form": "아침 퇴근이라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100011987.1.6",
          "form": "아침 퇴근이세어ㅡ?",
          "original_form": "아침 퇴근이세어ㅡ?",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100011987.1.7",
          "form": "8시에 출근할 때는 너무 많이와서 불편했는데",
          "original_form": "8시에 출근할 때는 너무 많이와서 불편했는데",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100011987.1.8",
          "form": "12시반쯤 되니 햇빛쪄서 뭐지?",
          "original_form": "12시반쯤 되니 햇빛쪄서 뭐지?",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100011987.1.9",
          "form": "2시반부터 다시비오더라고요",
          "original_form": "2시반부터 다시비오더라고요",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100011987.1.10",
          "form": "잠깐이라도 햇빛봐서 좋았어요ㅋㅋ",
          "original_form": "잠깐이라도 햇빛봐서 좋았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100011987.1.11",
          "form": "담주까지 비온데요ㅠㅠ",
          "original_form": "담주까지 비온데요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100011987.1.12",
          "form": "야간근무여서 ㅎㅎ",
          "original_form": "야간근무여서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100011987.1.13",
          "form": "네 지금은 조용하네요",
          "original_form": "네 지금은 조용하네요",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100011987.1.14",
          "form": "아이고 고생하시네요ㅠㅠ",
          "original_form": "아이고 고생하시네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100011987.1.15",
          "form": "비가 많이와서 합섬?으로 만든 무대장에 들어갔다가 바퀴빠져서 터져서 견인 부르고 난리..ㅠㅠ",
          "original_form": "비가 많이와서 합섬?으로 만든 무대장에 들어갔다가 바퀴빠져서 터져서 견인 부르고 난리..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100011987.1.16",
          "form": "비오니 별일이 다있네요",
          "original_form": "비오니 별일이 다있네요",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100011987.1.17",
          "form": "아 그렇게 많이 왔나요ㅠㅠ",
          "original_form": "아 그렇게 많이 왔나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100011987.1.18",
          "form": "네 물먹어서 땅이 꺼졌어요",
          "original_form": "네 물먹어서 땅이 꺼졌어요",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100011987.1.19",
          "form": "ㅋㅋㅋㅋㅋ살다살다 땅꺼져서 사고난거는 처음이네요",
          "original_form": "ㅋㅋㅋㅋㅋ살다살다 땅꺼져서 사고난거는 처음이네요",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100011987.1.20",
          "form": "지금도 근무중이세요?",
          "original_form": "지금도 근무중이세요?",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100011987.1.21",
          "form": "아니요 ㅎ 오전에 집와서 내일도 쉽니다",
          "original_form": "아니요 ㅎ 오전에 집와서 내일도 쉽니다",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100011987.1.22",
          "form": "내일은 비 없더라고요",
          "original_form": "내일은 비 없더라고요",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100011987.1.23",
          "form": "진짜요? 저희지역은 저번주부터 이번주 담주 목요일까지온데요ㅋㅋ",
          "original_form": "진짜요? 저희지역은 저번주부터 이번주 담주 목요일까지온데요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100011987.1.24",
          "form": "늦은 장마인가싶어요",
          "original_form": "늦은 장마인가싶어요",
          "speaker_id": "1",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100011987.1.25",
          "form": "남쪽이신가요ㅎㅎ비많이오네요",
          "original_form": "남쪽이신가요ㅎㅎ비많이오네요",
          "speaker_id": "2",
          "time": "20210825 00:31"
        },
        {
          "id": "MDRW2100011987.1.26",
          "form": "오늘은 어제보다는 약해졌어요",
          "original_form": "오늘은 어제보다는 약해졌어요",
          "speaker_id": "1",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100011987.1.27",
          "form": "비오니까 습해져서 더운거같네요",
          "original_form": "비오니까 습해져서 더운거같네요",
          "speaker_id": "1",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100011987.1.28",
          "form": "동남아 같아요",
          "original_form": "동남아 같아요",
          "speaker_id": "2",
          "time": "20210825 08:45"
        },
        {
          "id": "MDRW2100011987.1.29",
          "form": "그러게요ㅋㅋ 지금은 또 햇빛찌네요",
          "original_form": "그러게요ㅋㅋ 지금은 또 햇빛찌네요",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100011987.1.30",
          "form": "쨍쨍해요ㅋㅋ더워지려나",
          "original_form": "쨍쨍해요ㅋㅋ더워지려나",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100011987.1.31",
          "form": "습한데 햇빛찌니까 지옥이예요",
          "original_form": "습한데 햇빛찌니까 지옥이예요",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100011987.1.32",
          "form": "외근이 없어서 다행ㅎㅎ",
          "original_form": "외근이 없어서 다행ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100011987.1.33",
          "form": "ㅋㅋ전 오늘 휴무라서",
          "original_form": "ㅋㅋ전 오늘 휴무라서",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100011987.1.34",
          "form": "집이라 좋네요",
          "original_form": "집이라 좋네요",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100011987.1.35",
          "form": "좋겠어요 날씨가 동남아처럼 습해요",
          "original_form": "좋겠어요 날씨가 동남아처럼 습해요",
          "speaker_id": "1",
          "time": "20210825 14:08"
        }
      ]
