      "utterance": [
        {
          "id": "MDRW2100033048.1.1",
          "form": "집콕 어떠신가요",
          "original_form": "집콕 어떠신가요",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033048.1.2",
          "form": "집순이라 좋아해요 ㅋㅋㅋ",
          "original_form": "집순이라 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033048.1.3",
          "form": "아 전 힘들어요",
          "original_form": "아 전 힘들어요",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033048.1.4",
          "form": "외향적이라",
          "original_form": "외향적이라",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033048.1.5",
          "form": "보석십자수 절대 못합니다 !!!",
          "original_form": "보석십자수 절대 못합니다 !!!",
          "speaker_id": "1",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033048.1.6",
          "form": "아 밖으로 다니시는 스타일이군요",
          "original_form": "아 밖으로 다니시는 스타일이군요",
          "speaker_id": "2",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033048.1.7",
          "form": "ㅋㅋㅋㅋ 답답해서요? ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 답답해서요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:42"
        },
        {
          "id": "MDRW2100033048.1.8",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033048.1.9",
          "form": "하루 한번은 집밖으로 뛰쳐나가요",
          "original_form": "하루 한번은 집밖으로 뛰쳐나가요",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033048.1.10",
          "form": "ㅋㅋㅋ 그럼 여럿이서 하는 보드게임 이런건 어떠세요",
          "original_form": "ㅋㅋㅋ 그럼 여럿이서 하는 보드게임 이런건 어떠세요",
          "speaker_id": "2",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100033048.1.11",
          "form": "좋습니다 ㅋㅋㅋ",
          "original_form": "좋습니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100033048.1.12",
          "form": "부루마블",
          "original_form": "부루마블",
          "speaker_id": "1",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100033048.1.13",
          "form": "아 부루마블 진짜 재밌는데 은근 하기가 어려워요 ㅋㅋㅋ 기회가 안 생겨서",
          "original_form": "아 부루마블 진짜 재밌는데 은근 하기가 어려워요 ㅋㅋㅋ 기회가 안 생겨서",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033048.1.14",
          "form": "친구들이 집에 와야 ㅋㅋ 둘이서 하면 어색하던데요",
          "original_form": "친구들이 집에 와야 ㅋㅋ 둘이서 하면 어색하던데요",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033048.1.15",
          "form": "맞고마냥",
          "original_form": "맞고마냥",
          "speaker_id": "1",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033048.1.16",
          "form": "그쵸 ㅋㅋ 기본 한 4명은 되어야 재밌더라고요 ㅋㅋ 왁자지껄",
          "original_form": "그쵸 ㅋㅋ 기본 한 4명은 되어야 재밌더라고요 ㅋㅋ 왁자지껄",
          "speaker_id": "2",
          "time": "20210906 14:45"
        },
        {
          "id": "MDRW2100033048.1.17",
          "form": "반신욕은 좋아해요",
          "original_form": "반신욕은 좋아해요",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033048.1.18",
          "form": "오 반신욕.. 집에 욕조 있으신가요 저희집은 없어서 부럽네요",
          "original_form": "오 반신욕.. 집에 욕조 있으신가요 저희집은 없어서 부럽네요",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100033048.1.19",
          "form": "없어요 없어 ㅠㅜ",
          "original_form": "없어요 없어 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100033048.1.20",
          "form": "사고싶네요 휴대용 그런거 나오더라구요",
          "original_form": "사고싶네요 휴대용 그런거 나오더라구요",
