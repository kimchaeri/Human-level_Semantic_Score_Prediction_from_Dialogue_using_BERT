      "utterance": [
        {
          "id": "MDRW2100025422.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025422.1.2",
          "form": "저는 가리는게 거의 없어요!",
          "original_form": "저는 가리는게 거의 없어요!",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025422.1.3",
          "form": "name2님은요?!",
          "original_form": "&name2&님은요?!",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025422.1.4",
          "form": "저두요ㅋㅋ 홍어같은건 못먹지만요",
          "original_form": "저두요ㅋㅋ 홍어같은건 못먹지만요",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025422.1.5",
          "form": "드셔본적 있나요?",
          "original_form": "드셔본적 있나요?",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025422.1.6",
          "form": "아 홍어를 안먹어봤어요!",
          "original_form": "아 홍어를 안먹어봤어요!",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025422.1.7",
          "form": "삭힌 홍어는 못 먹겠더라구요",
          "original_form": "삭힌 홍어는 못 먹겠더라구요",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025422.1.8",
          "form": "그냥 무침같은것도 있는데 그건 괜찮아요",
          "original_form": "그냥 무침같은것도 있는데 그건 괜찮아요",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025422.1.9",
          "form": "오 회는 좋아하는데",
          "original_form": "오 회는 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025422.1.10",
          "form": "전혀다른거겠죠?",
          "original_form": "전혀다른거겠죠?",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025422.1.11",
          "form": "회무침이긴 한데 좀 더 꼬독꼬독해요",
          "original_form": "회무침이긴 한데 좀 더 꼬독꼬독해요",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025422.1.12",
          "form": "너무맛있겠네요,,,",
          "original_form": "너무맛있겠네요,,,",
          "speaker_id": "1",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025422.1.13",
          "form": "점심 뭐드실예정이세요?!",
          "original_form": "점심 뭐드실예정이세요?!",
          "speaker_id": "1",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025422.1.14",
          "form": "백반집 가려구요",
          "original_form": "백반집 가려구요",
          "speaker_id": "2",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025422.1.15",
          "form": "뭐 드실거에요?",
          "original_form": "뭐 드실거에요?",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025422.1.16",
          "form": "저 백반진짜좋아해요",
          "original_form": "저 백반진짜좋아해요",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025422.1.17",
          "form": "뭘먹어야할지 고민중입니다",
          "original_form": "뭘먹어야할지 고민중입니다",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025422.1.18",
          "form": "딱히없으면 라면먹을것같아요",
          "original_form": "딱히없으면 라면먹을것같아요",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025422.1.19",
          "form": "라면도 좋져. 전 달걀 휘휘 풀어서 먹는거 좋아해요",
          "original_form": "라면도 좋져. 전 달걀 휘휘 풀어서 먹는거 좋아해요",
          "speaker_id": "2",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025422.1.20",
          "form": "어떤라면좋아하세요?",
          "original_form": "어떤라면좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025422.1.21",
          "form": "저는 오동통면!",
          "original_form": "저는 오동통면!",
          "speaker_id": "1",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025422.1.22",
          "form": "저 요새 신라면볶음면에 빠졌어요",
          "original_form": "저 요새 신라면볶음면에 빠졌어요",
          "speaker_id": "2",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025422.1.23",
          "form": "달콤매콤해서 맛있더라구요",
          "original_form": "달콤매콤해서 맛있더라구요",
          "speaker_id": "2",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025422.1.24",
          "form": "저그거안먹어봤어요!",
          "original_form": "저그거안먹어봤어요!",
          "speaker_id": "1",
          "time": "20210901 11:18"
        }
      ]
    }
