      "utterance": [
        {
          "id": "MDRW2100012855.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 02:58"
        },
        {
          "id": "MDRW2100012855.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210825 02:58"
        },
        {
          "id": "MDRW2100012855.1.3",
          "form": "결혼하실 생각있으신가요?",
          "original_form": "결혼하실 생각있으신가요?",
          "speaker_id": "1",
          "time": "20210825 02:58"
        },
        {
          "id": "MDRW2100012855.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210825 02:59"
        },
        {
          "id": "MDRW2100012855.1.5",
          "form": "저는 비혼주의자입니다..",
          "original_form": "저는 비혼주의자입니다..",
          "speaker_id": "2",
          "time": "20210825 02:59"
        },
        {
          "id": "MDRW2100012855.1.6",
          "form": "연애에도 별로 관심이 없어요",
          "original_form": "연애에도 별로 관심이 없어요",
          "speaker_id": "2",
          "time": "20210825 02:59"
        },
        {
          "id": "MDRW2100012855.1.7",
          "form": "만약에 정말 좋은 남자를 만나서 결혼을 하더라도 애는 절대 안 낳고 싶어요",
          "original_form": "만약에 정말 좋은 남자를 만나서 결혼을 하더라도 애는 절대 안 낳고 싶어요",
          "speaker_id": "2",
          "time": "20210825 03:00"
        },
        {
          "id": "MDRW2100012855.1.8",
          "form": "저도 비혼 주의자입니다",
          "original_form": "저도 비혼 주의자입니다",
          "speaker_id": "1",
          "time": "20210825 03:00"
        },
        {
          "id": "MDRW2100012855.1.9",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "2",
          "time": "20210825 03:01"
        },
        {
          "id": "MDRW2100012855.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210825 03:01"
        },
        {
          "id": "MDRW2100012855.1.11",
          "form": "저 지금 이거 하면서 20대 여성분들 4명 정도랑 대화했는데",
          "original_form": "저 지금 이거 하면서 20대 여성분들 4명 정도랑 대화했는데",
          "speaker_id": "2",
          "time": "20210825 03:01"
        },
        {
          "id": "MDRW2100012855.1.12",
          "form": "다 비혼주의자셨어요ㅋㅋㅋㅋㅋ",
          "original_form": "다 비혼주의자셨어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 03:01"
        },
        {
          "id": "MDRW2100012855.1.13",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 03:01"
        },
        {
          "id": "MDRW2100012855.1.14",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 03:01"
        },
        {
          "id": "MDRW2100012855.1.15",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "2",
          "time": "20210825 03:01"
        },
        {
          "id": "MDRW2100012855.1.16",
          "form": "역시 다들 비혼비혼..",
          "original_form": "역시 다들 비혼비혼..",
          "speaker_id": "2",
          "time": "20210825 03:01"
        },
        {
          "id": "MDRW2100012855.1.17",
          "form": "세대가 교체되면서 결혼관도 바뀌네요",
          "original_form": "세대가 교체되면서 결혼관도 바뀌네요",
          "speaker_id": "1",
          "time": "20210825 03:02"
        },
        {
          "id": "MDRW2100012855.1.18",
          "form": "미팅이나 소개팅 해보셨어요?",
          "original_form": "미팅이나 소개팅 해보셨어요?",
          "speaker_id": "1",
          "time": "20210825 03:02"
        },
        {
          "id": "MDRW2100012855.1.19",
          "form": "저는 최악의 경험이었어요ㅋㅋ",
          "original_form": "저는 최악의 경험이었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 03:02"
        },
        {
          "id": "MDRW2100012855.1.20",
          "form": "아니요 한 번도 안해봤너요!!",
          "original_form": "아니요 한 번도 안해봤너요!!",
          "speaker_id": "2",
          "time": "20210825 03:02"
        },
        {
          "id": "MDRW2100012855.1.21",
          "form": "엑 진짜요?? 어떠셨어요?",
          "original_form": "엑 진짜요?? 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210825 03:02"
        },
        {
          "id": "MDRW2100012855.1.22",
          "form": "아주 옛날인데",
          "original_form": "아주 옛날인데",
          "speaker_id": "1",
          "time": "20210825 03:03"
        },
        {
          "id": "MDRW2100012855.1.23",
          "form": "남들 다하길래 저도 해봤거든요",
          "original_form": "남들 다하길래 저도 해봤거든요",
          "speaker_id": "1",
          "time": "20210825 03:03"
        },
        {
          "id": "MDRW2100012855.1.24",
          "form": "시간이 아까웠어요",
          "original_form": "시간이 아까웠어요",
          "speaker_id": "1",
          "time": "20210825 03:03"
        }
