      "utterance": [
        {
          "id": "MDRW2100002019.1.1",
          "form": "좋아하시는 배달음식있으신가요",
          "original_form": "좋아하시는 배달음식있으신가요",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002019.1.2",
          "form": "배달음식은 다사랑이죠ㅋㅋ",
          "original_form": "배달음식은 다사랑이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002019.1.3",
          "form": "치킨!!",
          "original_form": "치킨!!",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002019.1.4",
          "form": "오  저도 배달음식 좋아하는데",
          "original_form": "오  저도 배달음식 좋아하는데",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002019.1.5",
          "form": "치킨 어느브랜드 어떤치킨 좋아하세요 ?",
          "original_form": "치킨 어느브랜드 어떤치킨 좋아하세요 ?",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100002019.1.6",
          "form": "저는 푸라닭 맛있던데 애때문에 오늘도 네네를 먹었어요",
          "original_form": "저는 푸라닭 맛있던데 애때문에 오늘도 네네를 먹었어요",
          "speaker_id": "2",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100002019.1.7",
          "form": "염지가 맵더라구요ㅠ",
          "original_form": "염지가 맵더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100002019.1.8",
          "form": "아 애기있으니까 간센걸 못먹군여ㅜㅜ",
          "original_form": "아 애기있으니까 간센걸 못먹군여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100002019.1.9",
          "form": "네네 어쩐지 슴슴하거 퍽퍽하더니 애기들좋아하는닭이었군여",
          "original_form": "네네 어쩐지 슴슴하거 퍽퍽하더니 애기들좋아하는닭이었군여",
          "speaker_id": "1",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100002019.1.10",
          "form": "전 단짠단짠좋아해서 교촌 허니콤보 좋아해요",
          "original_form": "전 단짠단짠좋아해서 교촌 허니콤보 좋아해요",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100002019.1.11",
          "form": "맞아요ㅋ저도 네네가 애기들건지ㅋㅋ 최근에 알았답니다ㅎㅎ",
          "original_form": "맞아요ㅋ저도 네네가 애기들건지ㅋㅋ 최근에 알았답니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002019.1.12",
          "form": "앗 교촌 레드콤보 최애ㅠㅠ",
          "original_form": "앗 교촌 레드콤보 최애ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002019.1.13",
          "form": "크으 레콤 진짜 처음먹었을때 그맛잊지못해요",
          "original_form": "크으 레콤 진짜 처음먹었을때 그맛잊지못해요",
          "speaker_id": "1",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002019.1.14",
          "form": "요즘 딸기잼맛이 많이나던데 그래도 맛있더라구여",
          "original_form": "요즘 딸기잼맛이 많이나던데 그래도 맛있더라구여",
          "speaker_id": "1",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002019.1.15",
          "form": "켁 레콤 에서요?? 첨듣는소리네요ㅋㅋ",
          "original_form": "켁 레콤 에서요?? 첨듣는소리네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002019.1.16",
          "form": "네 레콤소스에 원래 딸기잼들어가요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 레콤소스에 원래 딸기잼들어가요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002019.1.17",
          "form": "근데 비율이 좀 더 많아진느낌이에요 옛날맛안나는..",
          "original_form": "근데 비율이 좀 더 많아진느낌이에요 옛날맛안나는..",
          "speaker_id": "1",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100002019.1.18",
          "form": "디핑소스시키면 성분에 딸기잼적혀있드라구여",
          "original_form": "디핑소스시키면 성분에 딸기잼적혀있드라구여",
          "speaker_id": "1",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100002019.1.19",
          "form": "와 처음 알았어요~ 딸기쨈ㅎㅎ",
          "original_form": "와 처음 알았어요~ 딸기쨈ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 11:48"
        },
        {
          "id": "MDRW2100002019.1.20",
          "form": "그쳐 저도 첨에 신기했어요 그거 알고나니까 맛에서 느껴지더라구여",
          "original_form": "그쳐 저도 첨에 신기했어요 그거 알고나니까 맛에서 느껴지더라구여",
          "speaker_id": "1",
          "time": "20210820 11:51"
        },
        {
          "id": "MDRW2100002019.1.21",
          "form": "오 다음에 먹어보면 딸기쨈. 맛을 찾아보겠어요ㅋ",
          "original_form": "오 다음에 먹어보면 딸기쨈. 맛을 찾아보겠어요ㅋ",
          "speaker_id": "2",
          "time": "20210820 11:52"
        },
        {
          "id": "MDRW2100002019.1.22",
          "form": "ㅋㅋㅋㅋㅋ네 알고먹으면 분명 느껴집니당",
          "original_form": "ㅋㅋㅋㅋㅋ네 알고먹으면 분명 느껴집니당",
          "speaker_id": "1",
          "time": "20210820 11:52"
        }
      ]
    }
