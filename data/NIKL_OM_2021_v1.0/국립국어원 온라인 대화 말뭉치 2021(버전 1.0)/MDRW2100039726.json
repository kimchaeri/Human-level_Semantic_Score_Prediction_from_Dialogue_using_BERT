      "utterance": [
        {
          "id": "MDRW2100039726.1.1",
          "form": "헉 어떤사람은 액정필름 2개 붙여서 줄이 쩍쩍갔데요",
          "original_form": "헉 어떤사람은 액정필름 2개 붙여서 줄이 쩍쩍갔데요",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039726.1.2",
          "form": "챙겨보는 프로 있으신가요?",
          "original_form": "챙겨보는 프로 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039726.1.3",
          "form": "요즘은 재밌는게 잘없더라구요",
          "original_form": "요즘은 재밌는게 잘없더라구요",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039726.1.4",
          "form": "런닝맨은 잘보긴해요",
          "original_form": "런닝맨은 잘보긴해요",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039726.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100039726.1.6",
          "form": "저도 런닝맨 챙겨봐요",
          "original_form": "저도 런닝맨 챙겨봐요",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039726.1.7",
          "form": "유제석 나오는건 다 챙겨보는 듯요",
          "original_form": "유제석 나오는건 다 챙겨보는 듯요",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039726.1.8",
          "form": "놀뭐 이번주 짱잼있을거같아요 ㅎㅎ",
          "original_form": "놀뭐 이번주 짱잼있을거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039726.1.9",
          "form": "그쵸?? ㅋㅋㅋㅋ 저도 기대중이예요",
          "original_form": "그쵸?? ㅋㅋㅋㅋ 저도 기대중이예요",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039726.1.10",
          "form": "지난주도 넘 재미있었어요",
          "original_form": "지난주도 넘 재미있었어요",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039726.1.11",
          "form": "앵커를 진짜 차분히 잘보시더라구요",
          "original_form": "앵커를 진짜 차분히 잘보시더라구요",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039726.1.12",
          "form": "유재석 경력 워낙 많으니까요",
          "original_form": "유재석 경력 워낙 많으니까요",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039726.1.13",
          "form": "나머지는 대략 말아먹는것 같던데요",
          "original_form": "나머지는 대략 말아먹는것 같던데요",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039726.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅌㅌㅌㅌ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅌㅌㅌㅌ",
          "speaker_id": "2",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100039726.1.15",
          "form": "꿀잼각이네요",
          "original_form": "꿀잼각이네요",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100039726.1.16",
          "form": "하하가 기대되지 않나요?",
          "original_form": "하하가 기대되지 않나요?",
          "speaker_id": "1",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100039726.1.17",
          "form": "눈도 잘안보인다는데",
          "original_form": "눈도 잘안보인다는데",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039726.1.18",
          "form": "자막 키워줬을런지 ㅋㅋ",
          "original_form": "자막 키워줬을런지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039726.1.19",
          "form": "알 있는 안경으로 바꿔 써야 할것 같은데요 ㅋㅋㅋ",
          "original_form": "알 있는 안경으로 바꿔 써야 할것 같은데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:08"
        }
