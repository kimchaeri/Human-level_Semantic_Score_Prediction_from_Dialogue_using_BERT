          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002797.1.4",
          "form": "그게 그대로 남아 있나 보네.",
          "original_form": "그게 그대로 남아 있나 보네.",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
