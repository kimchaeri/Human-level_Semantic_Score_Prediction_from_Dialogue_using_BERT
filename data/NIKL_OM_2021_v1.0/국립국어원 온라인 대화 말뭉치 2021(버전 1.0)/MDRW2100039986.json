      "utterance": [
        {
          "id": "MDRW2100039986.1.1",
          "form": "암튼 해외까지 쫓아가고 그랬네요",
          "original_form": "암튼 해외까지 쫓아가고 그랬네요",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100039986.1.2",
          "form": "저 애들 가르치는데 bap 인기 많았어요 ㅋㅋㅋ",
          "original_form": "저 애들 가르치는데 bap 인기 많았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100039986.1.3",
          "form": "우와 진짜요? ㅋㅋㅋㅋㅋ 찐이셨네요 ㅋㅋㅋ",
          "original_form": "우와 진짜요? ㅋㅋㅋㅋㅋ 찐이셨네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100039986.1.4",
          "form": "아 진짜요? ㅋㅋㅋㅋㅋ이야 선생님이시구나",
          "original_form": "아 진짜요? ㅋㅋㅋㅋㅋ이야 선생님이시구나",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039986.1.5",
          "form": "네 찐..이였죠",
          "original_form": "네 찐..이였죠",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039986.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039986.1.7",
          "form": "아니요 선생님은 아니고 학원강사 ㅋㅋㅋ영어가르쳐요 ㅋㅋㅋ",
          "original_form": "아니요 선생님은 아니고 학원강사 ㅋㅋㅋ영어가르쳐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039986.1.8",
          "form": "멤버들도 알아볼정도였으니",
          "original_form": "멤버들도 알아볼정도였으니",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039986.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039986.1.10",
          "form": "우와 ㅋㅋㅋ 초기부터 팬이셨나봐요 ㅋㅋㅋ",
          "original_form": "우와 ㅋㅋㅋ 초기부터 팬이셨나봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039986.1.11",
          "form": "지독하게 쫓아다닌",
          "original_form": "지독하게 쫓아다닌",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039986.1.12",
          "form": "어쩌다 마음이 식었나요 ㅜㅜ",
          "original_form": "어쩌다 마음이 식었나요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039986.1.13",
          "form": "그렇게 초기도 아니고 ㅎ 한 2-3집정도였나",
          "original_form": "그렇게 초기도 아니고 ㅎ 한 2-3집정도였나",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039986.1.14",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039986.1.15",
          "form": "활동도 뜸하고 다른 아이돌로 갈아탔죠 ㅎ",
          "original_form": "활동도 뜸하고 다른 아이돌로 갈아탔죠 ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039986.1.16",
          "form": "전 안식었는데 해체부터 해서 진짜 마상이었어요....",
          "original_form": "전 안식었는데 해체부터 해서 진짜 마상이었어요....",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039986.1.17",
          "form": "덕질에 진심이었던듯요",
          "original_form": "덕질에 진심이었던듯요",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039986.1.18",
          "form": "영어 가르치시구나 역시 ",
          "original_form": "영어 가르치시구나 역시 ",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039986.1.19",
          "form": "애초에 회사에서는 밀어줄 생각도 없었던거 같고...",
          "original_form": "애초에 회사에서는 밀어줄 생각도 없었던거 같고...",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039986.1.20",
          "form": "아ㅠㅠㅠㅠ 그럼 진짜 마상일듯요",
          "original_form": "아ㅠㅠㅠㅠ 그럼 진짜 마상일듯요",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039986.1.21",
          "form": "왜 역시인가요 ㅋㅋㅋㅋ 한국어를 잘 못하나요? ㅜㅜ ㅋㅋㅋㅋ 오타가 많아서인가요? ㅜㅜ ㅋㅋㅋ",
          "original_form": "왜 역시인가요 ㅋㅋㅋㅋ 한국어를 잘 못하나요? ㅜㅜ ㅋㅋㅋㅋ 오타가 많아서인가요? ㅜㅜ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039986.1.22",
          "form": "근데 애초에 멤들 사이도 좀... 그런거같았어요 ㅎㅎㅎ",
          "original_form": "근데 애초에 멤들 사이도 좀... 그런거같았어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039986.1.23",
          "form": "잘못갔네요ㅋㅋ대단하시다고 하려했는데",
          "original_form": "잘못갔네요ㅋㅋ대단하시다고 하려했는데",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039986.1.24",
          "form": "팬덤분위기는 ㄹㅇ 진짜 살벌했구요 ㅋㅋㅋㅋ",
          "original_form": "팬덤분위기는 ㄹㅇ 진짜 살벌했구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039986.1.25",
          "form": "영어 잘하시는분들 대단해요ㅈ부럽고",
