      "utterance": [
        {
          "id": "MDRW2100025421.1.1",
          "form": "아마 그때 무한상사 또나오지않을까",
          "original_form": "아마 그때 무한상사 또나오지않을까",
          "speaker_id": "1",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025421.1.2",
          "form": "형돈이도 나왔으면 좋겠는ㄷㅔ",
          "original_form": "형돈이도 나왔으면 좋겠는ㄷㅔ",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025421.1.3",
          "form": "그랬으면 좋겟다",
          "original_form": "그랬으면 좋겟다",
          "speaker_id": "2",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025421.1.4",
          "form": "저번에 정과장 어깨피라는거 존웃이었는데",
          "original_form": "저번에 정과장 어깨피라는거 존웃이었는데",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025421.1.5",
          "form": "가방열었더니 신문지나와서",
          "original_form": "가방열었더니 신문지나와서",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025421.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ유재석이 정준하 놀리는게 제일웃곀ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ유재석이 정준하 놀리는게 제일웃곀ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025421.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 마쟈 진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 마쟈 진짜",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025421.1.8",
          "form": "유재석 웃는거 ㄹㅇ 찐이었어",
          "original_form": "유재석 웃는거 ㄹㅇ 찐이었어",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025421.1.9",
          "form": "코먹고",
          "original_form": "코먹고",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025421.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ컹 거리는고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ컹 거리는고",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025421.1.11",
          "form": "찐웃음",
          "original_form": "찐웃음",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025421.1.12",
          "form": "유재석찐웃음 개웃김",
          "original_form": "유재석찐웃음 개웃김",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025421.1.13",
          "form": "찐이라고 하니까 요새 진우 너무기여워",
          "original_form": "찐이라고 하니까 요새 진우 너무기여워",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025421.1.14",
          "form": "아 찌누",
          "original_form": "아 찌누",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025421.1.15",
          "form": "찌누거누낭니",
          "original_form": "찌누거누낭니",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025421.1.16",
          "form": "우리 벤톨이랑 진우보는 낙",
          "original_form": "우리 벤톨이랑 진우보는 낙",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025421.1.17",
          "form": "마저 우리톨",
          "original_form": "마저 우리톨",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025421.1.18",
          "form": "우리지누",
          "original_form": "우리지누",
          "speaker_id": "1",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025421.1.19",
          "form": "진짜어쩜그리순해",
          "original_form": "진짜어쩜그리순해",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025421.1.20",
          "form": "근데완전거누미니미야",
          "original_form": "근데완전거누미니미야",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025421.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ머리 기니까 더 거누랑 똑같아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ머리 기니까 더 거누랑 똑같아",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025421.1.22",
          "form": "박주호는 좋겠다",
          "original_form": "박주호는 좋겠다",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025421.1.23",
          "form": "머리스타일도 거누",
          "original_form": "머리스타일도 거누",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025421.1.24",
          "form": "아 거누진짜",
          "original_form": "아 거누진짜",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025421.1.25",
          "form": "노래하고싶다고 우는거",
          "original_form": "노래하고싶다고 우는거",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025421.1.26",
          "form": "레전드",
          "original_form": "레전드",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025421.1.27",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ레전드진짜",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ레전드진짜",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025421.1.28",
          "form": "거누 갈수록 잘생겨지고",
          "original_form": "거누 갈수록 잘생겨지고",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025421.1.29",
          "form": "영어 진짜 잘하더러",
          "original_form": "영어 진짜 잘하더러",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025421.1.30",
          "form": "ㅋㅋㅋㅋ그거진짜 주기적으로 봐줘야댐",
          "original_form": "ㅋㅋㅋㅋ그거진짜 주기적으로 봐줘야댐",
          "speaker_id": "1",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025421.1.31",
          "form": "나보다 잘하는듯",
          "original_form": "나보다 잘하는듯",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025421.1.32",
          "form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025421.1.33",
