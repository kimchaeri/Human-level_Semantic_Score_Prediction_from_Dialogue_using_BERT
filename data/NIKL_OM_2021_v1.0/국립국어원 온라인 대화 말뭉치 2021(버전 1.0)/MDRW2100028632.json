      "utterance": [
        {
          "id": "MDRW2100028632.1.1",
          "form": "역시 남초가 엮이면 피곤해져....",
          "original_form": "역시 남초가 엮이면 피곤해져....",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028632.1.2",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028632.1.3",
          "form": "아 갑자기 남초하니까 생각났어",
          "original_form": "아 갑자기 남초하니까 생각났어",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028632.1.4",
          "form": "거기 영통한 여자팬들도 패고 다녔다머",
          "original_form": "거기 영통한 여자팬들도 패고 다녔다머",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028632.1.5",
          "form": "며",
          "original_form": "며",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028632.1.6",
          "form": "ㅋㅋㅋㅋ엥?",
          "original_form": "ㅋㅋㅋㅋ엥?",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028632.1.7",
          "form": "여팬을 왜패?",
          "original_form": "여팬을 왜패?",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028632.1.8",
          "form": "자기들이 모르던 영통 응모를 여팬들이 당첨되서",
          "original_form": "자기들이 모르던 영통 응모를 여팬들이 당첨되서",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028632.1.9",
          "form": "영통팬싸하고 트위터에 올리니깐ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "영통팬싸하고 트위터에 올리니깐ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028632.1.10",
          "form": "????디용",
          "original_form": "????디용",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028632.1.11",
          "form": "그걸로패고다님",
          "original_form": "그걸로패고다님",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028632.1.12",
          "form": "나둘아 첮아서 응모해야지",
          "original_form": "나둘아 첮아서 응모해야지",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028632.1.13",
          "form": "무슨 여팬들을 패고있어",
          "original_form": "무슨 여팬들을 패고있어",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028632.1.14",
          "form": "어이없을무",
          "original_form": "어이없을무",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028632.1.15",
          "form": "멍청이들이야",
          "original_form": "멍청이들이야",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028632.1.16",
          "form": "근데 무슨 이야기하려던거얌",
          "original_form": "근데 무슨 이야기하려던거얌",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028632.1.17",
          "form": "ㄹㅇ..........",
          "original_form": "ㄹㅇ..........",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028632.1.18",
          "form": "아 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028632.1.19",
          "form": "유투버중에 레나라고알아?",
          "original_form": "유투버중에 레나라고알아?",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028632.1.20",
          "form": "아닝 처음들어용",
          "original_form": "아닝 처음들어용",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028632.1.21",
          "form": "뷰투버였늗네",
          "original_form": "뷰투버였늗네",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028632.1.22",
          "form": "1베하는거 걸려서",
          "original_form": "1베하는거 걸려서",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028632.1.23",
          "form": "거의 쫒겨나다시피? 망한애있거든",
          "original_form": "거의 쫒겨나다시피? 망한애있거든",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100028632.1.24",
          "form": "지 뷰투버다신안한다더니 은근슬쩍 다시돌아와서 활동명바꾸고",
          "original_form": "지 뷰투버다신안한다더니 은근슬쩍 다시돌아와서 활동명바꾸고",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028632.1.25",
          "form": "다시활동하더니 오늘 웨이크메이크 모델되가지고 난리남",
          "original_form": "다시활동하더니 오늘 웨이크메이크 모델되가지고 난리남",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028632.1.26",
          "form": "잉?????\r\n뷰튜버면 여자아니야????",
          "original_form": "잉?????\r\n뷰튜버면 여자아니야????",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028632.1.27",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028632.1.28",
          "form": "뭐야....그 이상한 조합은",
          "original_form": "뭐야....그 이상한 조합은",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028632.1.29",
          "form": "진짜 쓴글보면 조금 찌든게아님",
          "original_form": "진짜 쓴글보면 조금 찌든게아님",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028632.1.30",
          "form": "진짜 심각한 찌꺼기도못한",
          "original_form": "진짜 심각한 찌꺼기도못한",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028632.1.31",
          "form": "뷰튜버면 대상도 주로 여잔데",
          "original_form": "뷰튜버면 대상도 주로 여잔데",
          "speaker_id": "2",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028632.1.32",
          "form": "ㅇㅇㅇㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅇㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028632.1.33",
          "form": "진짜 웃김 ㅋㅋㅋㅋㅋㅋㅋ 그래서 난리나고",
          "original_form": "진짜 웃김 ㅋㅋㅋㅋㅋㅋㅋ 그래서 난리나고",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028632.1.34",
          "form": "나름 백만가까이 구독자있었는데",
          "original_form": "나름 백만가까이 구독자있었는데",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028632.1.35",
          "form": "아니 근데 화장품 모델이라니",
          "original_form": "아니 근데 화장품 모델이라니",
          "speaker_id": "2",
          "time": "20210902 21:14"
        }
      ]
