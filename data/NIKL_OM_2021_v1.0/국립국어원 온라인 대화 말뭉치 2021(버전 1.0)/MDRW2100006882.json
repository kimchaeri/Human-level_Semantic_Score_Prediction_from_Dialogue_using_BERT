      "utterance": [
        {
          "id": "MDRW2100006882.1.1",
          "form": "고생은요",
          "original_form": "고생은요",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100006882.1.2",
          "form": "아니야고생했어",
          "original_form": "아니야고생했어",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100006882.1.3",
          "form": "그냥 시부모 모시는 건 며느리의 도리라고 생각하고 지냈어요",
          "original_form": "그냥 시부모 모시는 건 며느리의 도리라고 생각하고 지냈어요",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100006882.1.4",
          "form": "고생한건 맞아",
          "original_form": "고생한건 맞아",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006882.1.5",
          "form": "남들도 다 하는데요 뭘",
          "original_form": "남들도 다 하는데요 뭘",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006882.1.6",
          "form": "20년간 모시는게 쉽지 않지",
          "original_form": "20년간 모시는게 쉽지 않지",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006882.1.7",
          "form": "그래서 우리집이 복받은 ㄴ거 같아요",
          "original_form": "그래서 우리집이 복받은 ㄴ거 같아요",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100006882.1.8",
          "form": "경제적으로는 폈는데 당신이 암 걸린게 속상해",
          "original_form": "경제적으로는 폈는데 당신이 암 걸린게 속상해",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100006882.1.9",
          "form": "암은 꼭 그것때문이 아닐 수 있어요",
          "original_form": "암은 꼭 그것때문이 아닐 수 있어요",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100006882.1.10",
          "form": "그래도 알게 모르게",
          "original_form": "그래도 알게 모르게",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100006882.1.11",
          "form": "참고 살다가",
          "original_form": "참고 살다가",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100006882.1.12",
          "form": "병걸린걸 수도 있어",
          "original_form": "병걸린걸 수도 있어",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100006882.1.13",
          "form": "뭐 알 수 없죠",
          "original_form": "뭐 알 수 없죠",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100006882.1.14",
          "form": "하지만 저 아버님 모신거 후회 안해요",
          "original_form": "하지만 저 아버님 모신거 후회 안해요",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006882.1.15",
          "form": "항상 고마워",
          "original_form": "항상 고마워",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006882.1.16",
          "form": "저도 고마워요",
          "original_form": "저도 고마워요",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006882.1.17",
          "form": "애들만 다 결혼하면 끝날텐데",
          "original_form": "애들만 다 결혼하면 끝날텐데",
