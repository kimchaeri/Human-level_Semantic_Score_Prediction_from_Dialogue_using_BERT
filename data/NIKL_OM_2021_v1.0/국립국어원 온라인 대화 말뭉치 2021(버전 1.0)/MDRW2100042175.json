      "utterance": [
        {
          "id": "MDRW2100042175.1.1",
          "form": "여기도 추적추적 내리기 시작했어요",
          "original_form": "여기도 추적추적 내리기 시작했어요",
          "speaker_id": "2",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042175.1.2",
          "form": "월요일이라 그런지 모르겠지만 살짝 처지네요 ㅠ",
          "original_form": "월요일이라 그런지 모르겠지만 살짝 처지네요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 20:24"
        },
        {
          "id": "MDRW2100042175.1.3",
          "form": "네 월요일 퇴근길에 비오니",
          "original_form": "네 월요일 퇴근길에 비오니",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042175.1.4",
          "form": "좀 처지더라고요",
          "original_form": "좀 처지더라고요",
          "speaker_id": "1",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042175.1.5",
          "form": "내일도 비가 온데요",
          "original_form": "내일도 비가 온데요",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042175.1.6",
          "form": "덕분에 오늘 푹 잘 수 있을 것 같아요 ㅋㅋ",
          "original_form": "덕분에 오늘 푹 잘 수 있을 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042175.1.7",
          "form": "네 저희 동네도 내일 오전까지 비 예보가ㅜ",
          "original_form": "네 저희 동네도 내일 오전까지 비 예보가ㅜ",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042175.1.8",
          "form": "빗소리가 들리는건 아니지만",
          "original_form": "빗소리가 들리는건 아니지만",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042175.1.9",
          "form": "아 비 오는 날 숙면 취하시나요",
          "original_form": "아 비 오는 날 숙면 취하시나요",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042175.1.10",
          "form": "네 완전!!",
          "original_form": "네 완전!!",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042175.1.11",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042175.1.12",
          "form": "저는 요즘 선선해져서 밤에 푹 자용",
          "original_form": "저는 요즘 선선해져서 밤에 푹 자용",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042175.1.13",
          "form": "이불도 따순 걸로 바꿨더니",
          "original_form": "이불도 따순 걸로 바꿨더니",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042175.1.14",
          "form": "두꺼운 이불을 꺼내셨나요?",
          "original_form": "두꺼운 이불을 꺼내셨나요?",
          "speaker_id": "2",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042175.1.15",
          "form": "네 지난주부터 두꺼운 이불 꺼냈어요",
          "original_form": "네 지난주부터 두꺼운 이불 꺼냈어요",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042175.1.16",
          "form": "새벽엔 쌀쌀하게 느껴져서",
          "original_form": "새벽엔 쌀쌀하게 느껴져서",
          "speaker_id": "1",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042175.1.17",
          "form": "아주 좋은 선택이었던 것 같아요",
          "original_form": "아주 좋은 선택이었던 것 같아요",
          "speaker_id": "2",
          "time": "20210906 21:09"
        },
        {
          "id": "MDRW2100042175.1.18",
          "form": "얇은 이불 둘둘 말고 자다가 저도 오늘 꺼냈어요 ㅋㅋ",
          "original_form": "얇은 이불 둘둘 말고 자다가 저도 오늘 꺼냈어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042175.1.19",
          "form": "네 이젠 두꺼운 이불이 필요해요!!",
          "original_form": "네 이젠 두꺼운 이불이 필요해요!!",
