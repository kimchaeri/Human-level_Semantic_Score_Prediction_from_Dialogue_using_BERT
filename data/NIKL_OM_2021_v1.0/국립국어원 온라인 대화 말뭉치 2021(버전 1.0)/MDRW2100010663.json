        {
          "id": "MDRW2100010663.1.1",
          "form": "백신 2차는 언제맞능가",
          "original_form": "백신 2차는 언제맞능가",
          "speaker_id": "1",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100010663.1.2",
          "form": "오 나는 9월초",
          "original_form": "오 나는 9월초",
          "speaker_id": "2",
          "time": "20210823 20:40"
        },
        {
          "id": "MDRW2100010663.1.3",
          "form": "6일이요",
          "original_form": "6일이요",
          "speaker_id": "2",
