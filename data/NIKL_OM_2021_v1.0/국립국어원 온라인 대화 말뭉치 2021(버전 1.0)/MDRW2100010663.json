        },
        {
          "id": "MDRW2100010663.1.4",
          "form": "name1은,",
          "original_form": "&name1&은,",
          "speaker_id": "2",
          "time": "20210823 20:41"
        },
        {
          "id": "MDRW2100010663.1.5",
          "form": "난 이번주 금요일에",
          "original_form": "난 이번주 금요일에",
          "speaker_id": "1",
          "time": "20210823 20:43"
        },
        {
          "id": "MDRW2100010663.1.6",
          "form": "금요일 좋네요 휴유증 3일 있다는데",
          "original_form": "금요일 좋네요 휴유증 3일 있다는데",
          "speaker_id": "2",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100010663.1.7",
          "form": "오 1차도 금욜이었는데 꿀이네",
          "original_form": "오 1차도 금욜이었는데 꿀이네",
          "speaker_id": "1",
          "time": "20210823 20:50"
        },
        {
          "id": "MDRW2100010663.1.8",
          "form": "난 월요일이라서 조금 걱정된다는",
          "original_form": "난 월요일이라서 조금 걱정된다는",
          "speaker_id": "2",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010663.1.9",
          "form": "아 수업들어야하지 참",
          "original_form": "아 수업들어야하지 참",
          "speaker_id": "1",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010663.1.10",
          "form": "삼일은 휴유증 있다던데",
          "original_form": "삼일은 휴유증 있다던데",
          "speaker_id": "2",
          "time": "20210823 20:55"
        },
        {
          "id": "MDRW2100010663.1.11",
          "form": "name2 1차때 후유증 있었음?",
          "original_form": "&name2& 1차때 후유증 있었음?",
          "speaker_id": "1",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010663.1.12",
          "form": "딱히 있지는 않았는데",
          "original_form": "딱히 있지는 않았는데",
          "speaker_id": "2",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010663.1.13",
          "form": "알아서 몸을 사렸던 ㅋ",
          "original_form": "알아서 몸을 사렸던 ㅋ",
          "speaker_id": "2",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010663.1.14",
          "form": "현명하도다 ㅋㅋ",
          "original_form": "현명하도다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010663.1.15",
          "form": "2차가 더 부작용심하대서 걱정됨",
