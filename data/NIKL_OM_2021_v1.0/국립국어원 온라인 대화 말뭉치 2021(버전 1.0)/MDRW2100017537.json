      "utterance": [
        {
          "id": "MDRW2100017537.1.1",
          "form": "저녁머드셧어요",
          "original_form": "저녁머드셧어요",
          "speaker_id": "2",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017537.1.2",
          "form": "저 저녁 카레먹었어요.. 그리구 오늘아침은 닭볶음탕!!",
          "original_form": "저 저녁 카레먹었어요.. 그리구 오늘아침은 닭볶음탕!!",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100017537.1.3",
          "form": "name2도 맛있는거 드셨나요?",
          "original_form": "&name2&도 맛있는거 드셨나요?",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100017537.1.4",
          "form": "만두머것어요 방금",
          "original_form": "만두머것어요 방금",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100017537.1.5",
          "form": "넘마사서 소진해야해여",
          "original_form": "넘마사서 소진해야해여",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100017537.1.6",
          "form": "아참 주먹밥도 배달을 시켜서",
          "original_form": "아참 주먹밥도 배달을 시켜서",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100017537.1.7",
          "form": "얼른 비워야함",
          "original_form": "얼른 비워야함",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100017537.1.8",
          "form": "ㅋㅋㅋ우와 주먹밥도 시키셨군요!!! 저도 맨날 쟁여놓는 버릇이있어서 이제 2개남으면 시키려구요",
          "original_form": "ㅋㅋㅋ우와 주먹밥도 시키셨군요!!! 저도 맨날 쟁여놓는 버릇이있어서 이제 2개남으면 시키려구요",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100017537.1.9",
          "form": "안그러면 냉동실이 터져나가요 ㅠ",
          "original_form": "안그러면 냉동실이 터져나가요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100017537.1.10",
          "form": "진짜ㅠㅠㅠ",
          "original_form": "진짜ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100017537.1.11",
          "form": "닭볶음탕은 해드셧나요",
          "original_form": "닭볶음탕은 해드셧나요",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100017537.1.12",
          "form": "신라면 볶음면 드셔보셨나요? 궁금해서 오늘 장볼때 같이 시켜봣어요 ㅋㅋㅋ",
          "original_form": "신라면 볶음면 드셔보셨나요? 궁금해서 오늘 장볼때 같이 시켜봣어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100017537.1.13",
          "form": "넹 엄마가 해주셨어요 ㅋㅋㅋ",
          "original_form": "넹 엄마가 해주셨어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100017537.1.14",
          "form": "아하하 집이시구나! 마싯엇겟다",
          "original_form": "아하하 집이시구나! 마싯엇겟다",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100017537.1.15",
          "form": "아니요 안머거봣어요 궁금하네요",
          "original_form": "아니요 안머거봣어요 궁금하네요",
          "speaker_id": "2",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100017537.1.16",
          "form": "저도 궁금해요 집에 열흘가까이 머물 예정이라고",
          "original_form": "저도 궁금해요 집에 열흘가까이 머물 예정이라고",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100017537.1.17",
          "form": "미리 말해뒀더니 엄마가 먹을거를",
          "original_form": "미리 말해뒀더니 엄마가 먹을거를",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100017537.1.18",
          "form": "너무많이 준비하셨어용 ㅋㅋㅋ",
          "original_form": "너무많이 준비하셨어용 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100017537.1.19",
          "form": "지금은 간식으로 옥수수먹어요",
          "original_form": "지금은 간식으로 옥수수먹어요",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100017537.1.20",
          "form": "크 조타조타",
          "original_form": "크 조타조타",
          "speaker_id": "2",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100017537.1.21",
          "form": "휴양이네요 옥수수맛있겟다",
          "original_form": "휴양이네요 옥수수맛있겟다",
          "speaker_id": "2",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100017537.1.22",
          "form": "ㅋㅋㅋ가까이살면 옥수수 주고싶어요ㅠ 정말 많아요",
          "original_form": "ㅋㅋㅋ가까이살면 옥수수 주고싶어요ㅠ 정말 많아요",
          "speaker_id": "1",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100017537.1.23",
          "form": "name2 사과떡볶이 들어보셨나요?",
          "original_form": "&name2& 사과떡볶이 들어보셨나요?",
          "speaker_id": "1",
          "time": "20210827 12:45"
        },
