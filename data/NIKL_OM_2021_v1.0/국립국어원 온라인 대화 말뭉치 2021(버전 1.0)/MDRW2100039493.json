      "utterance": [
        {
          "id": "MDRW2100039493.1.1",
          "form": "그렇긴 하죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇긴 하죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039493.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그래도 이번에 디오 솔로도 좋던데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그래도 이번에 디오 솔로도 좋던데",
          "speaker_id": "1",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039493.1.3",
          "form": "들어보셨나요?ㅋㅋ",
          "original_form": "들어보셨나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039493.1.4",
          "form": "아하 저는 탈덕이후로 관심이 없어져서.. 노래를 안들어봤어요 ㅋㅋㅋㅋ",
          "original_form": "아하 저는 탈덕이후로 관심이 없어져서.. 노래를 안들어봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039493.1.5",
          "form": "유니버스가 아마 마지막이었던 것 같아요",
          "original_form": "유니버스가 아마 마지막이었던 것 같아요",
          "speaker_id": "2",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039493.1.6",
          "form": "덕질할때도 원래 노래 잘 안들었거든요 ㅋㅋㅋㅌㅌㅋㅋㅋ 귀찮아서",
          "original_form": "덕질할때도 원래 노래 잘 안들었거든요 ㅋㅋㅋㅌㅌㅋㅋㅋ 귀찮아서",
          "speaker_id": "2",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039493.1.7",
          "form": "ㅋㅋㅋ그러셨군요 ㅋㅋㅋ 겨울앨범 노래들 참 좋았는데...",
          "original_form": "ㅋㅋㅋ그러셨군요 ㅋㅋㅋ 겨울앨범 노래들 참 좋았는데...",
          "speaker_id": "1",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039493.1.8",
          "form": "노래 나왔다고 해도 바로바로 안 들었어요 ㅎ",
          "original_form": "노래 나왔다고 해도 바로바로 안 들었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039493.1.9",
          "form": "아 그러면 평소에 노래 잘 안들으세요?",
          "original_form": "아 그러면 평소에 노래 잘 안들으세요?",
          "speaker_id": "1",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039493.1.10",
          "form": "아니면 팝송위주로 들으시는거에요?",
          "original_form": "아니면 팝송위주로 들으시는거에요?",
          "speaker_id": "1",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039493.1.11",
          "form": "네네 팝송 많이 들어요",
          "original_form": "네네 팝송 많이 들어요",
          "speaker_id": "2",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039493.1.12",
          "form": "예전에 백색소음이랑",
          "original_form": "예전에 백색소음이랑",
          "speaker_id": "2",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039493.1.13",
          "form": "몬스터 좋아했는데,, 쩝",
          "original_form": "몬스터 좋아했는데,, 쩝",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039493.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ백색소음 진짜 노래 좋았죠....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ백색소음 진짜 노래 좋았죠....",
          "speaker_id": "1",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039493.1.15",
          "form": "전 엑소가 일단 노래가 좋아서 더 좋았거든요 ㅠㅠ",
          "original_form": "전 엑소가 일단 노래가 좋아서 더 좋았거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039493.1.16",
          "form": "슴 취향 귀라서 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "슴 취향 귀라서 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039493.1.17",
          "form": "아하 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039493.1.18",
          "form": "좋은 노래 많긴 했죠..",
          "original_form": "좋은 노래 많긴 했죠..",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039493.1.19",
          "form": "로또도 좋았고",
          "original_form": "로또도 좋았고",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039493.1.20",
          "form": "등등..",
          "original_form": "등등..",
          "speaker_id": "2",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100039493.1.21",
          "form": "ㅋㅋ맞아요 그리구 겨울마다 겨울앨범내주는게 너무좋았어요",
          "original_form": "ㅋㅋ맞아요 그리구 겨울마다 겨울앨범내주는게 너무좋았어요",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100039493.1.22",
          "form": "아 불공평해도 좋았는데",
          "original_form": "아 불공평해도 좋았는데",
          "speaker_id": "2",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100039493.1.23",
          "form": "타오는 요즘 뭐하고 살까요... 근데 무소식이 희소식인것같아요\r\n..",
          "original_form": "타오는 요즘 뭐하고 살까요... 근데 무소식이 희소식인것같아요\r\n..",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100039493.1.24",
          "form": "크리스 .......ㅋㅋ....ㅋ...ㅋㅋ",
          "original_form": "크리스 .......ㅋㅋ....ㅋ...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100039493.1.25",
          "form": "ㅋㅋㅋ 레전드 ㅎ",
          "original_form": "ㅋㅋㅋ 레전드 ㅎ",
          "speaker_id": "2",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039493.1.26",
          "form": "진짜....ㅋㅋㅋ....가지가지...",
          "original_form": "진짜....ㅋㅋㅋ....가지가지...",
          "speaker_id": "1",
