      "utterance": [
        {
          "id": "MDRW2100012845.1.1",
          "form": "네 가끔씩 보네요",
          "original_form": "네 가끔씩 보네요",
          "speaker_id": "2",
          "time": "20210825 02:38"
        },
        {
          "id": "MDRW2100012845.1.2",
          "form": "구독하는 계정 있으세요?",
          "original_form": "구독하는 계정 있으세요?",
          "speaker_id": "1",
          "time": "20210825 02:38"
        },
        {
          "id": "MDRW2100012845.1.3",
          "form": "별도로 구독하진 않네요",
          "original_form": "별도로 구독하진 않네요",
          "speaker_id": "2",
          "time": "20210825 02:38"
        },
        {
          "id": "MDRW2100012845.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 02:38"
        },
        {
          "id": "MDRW2100012845.1.5",
          "form": "저는 침착맨 유튜브랑",
          "original_form": "저는 침착맨 유튜브랑",
          "speaker_id": "1",
          "time": "20210825 02:38"
        },
        {
          "id": "MDRW2100012845.1.6",
          "form": "그알",
          "original_form": "그알",
          "speaker_id": "1",
          "time": "20210825 02:38"
        },
        {
          "id": "MDRW2100012845.1.7",
          "form": "공포유튜버 구독해요 ㅋㅋㅋ",
          "original_form": "공포유튜버 구독해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100012845.1.8",
          "form": "공포유튜버는 뭔가요?",
          "original_form": "공포유튜버는 뭔가요?",
          "speaker_id": "2",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100012845.1.9",
          "form": "무서운 얘기 읽어주고",
          "original_form": "무서운 얘기 읽어주고",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100012845.1.10",
          "form": "무당 관련된??",
          "original_form": "무당 관련된??",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100012845.1.11",
          "form": "컨텐츠들 올라오더라구요",
          "original_form": "컨텐츠들 올라오더라구요",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100012845.1.12",
          "form": "안 무서우세요?",
          "original_form": "안 무서우세요?",
          "speaker_id": "2",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100012845.1.13",
          "form": "가끔 무섭긴 한데",
          "original_form": "가끔 무섭긴 한데",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100012845.1.14",
          "form": "딴거하면서 틀어놓기 딱 좋은 것 같아요 ㅋㅋㅋ",
          "original_form": "딴거하면서 틀어놓기 딱 좋은 것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:39"
        },
        {
          "id": "MDRW2100012845.1.15",
          "form": "무서워서 딴 게 눈에 안들어올 것 같아여",
          "original_form": "무서워서 딴 게 눈에 안들어올 것 같아여",
          "speaker_id": "2",
          "time": "20210825 02:40"
        },
        {
          "id": "MDRW2100012845.1.16",
          "form": "네 그래서 적당히 무서운걸로 봐요 ㅋㅋ",
          "original_form": "네 그래서 적당히 무서운걸로 봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:40"
        },
        {
          "id": "MDRW2100012845.1.17",
          "form": "침착맨은 뭐에여",
          "original_form": "침착맨은 뭐에여",
          "speaker_id": "2",
          "time": "20210825 02:40"
        },
        {
          "id": "MDRW2100012845.1.18",
          "form": "이말년이라고",
          "original_form": "이말년이라고",
          "speaker_id": "1",
          "time": "20210825 02:40"
        },
        {
          "id": "MDRW2100012845.1.19",
          "form": "원래 만화가인데",
          "original_form": "원래 만화가인데",
          "speaker_id": "1",
          "time": "20210825 02:40"
        },
        {
          "id": "MDRW2100012845.1.20",
          "form": "이제 방송하는 사람 있어요",
          "original_form": "이제 방송하는 사람 있어요",
          "speaker_id": "1",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100012845.1.21",
          "form": "재밌어요 ㅋㅋ",
          "original_form": "재밌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100012845.1.22",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100012845.1.23",
          "form": "저는 잘 몰라요 ㅎㅎ",
          "original_form": "저는 잘 몰라요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100012845.1.24",
          "form": "그럴 수 있죠",
          "original_form": "그럴 수 있죠",
          "speaker_id": "1",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100012845.1.25",
          "form": "워낙 많으니까요",
          "original_form": "워낙 많으니까요",
          "speaker_id": "1",
          "time": "20210825 02:41"
        },
        {
          "id": "MDRW2100012845.1.26",
          "form": "저 오늘 무법변호사 거의 다 봤어요",
          "original_form": "저 오늘 무법변호사 거의 다 봤어요",
          "speaker_id": "1",
          "time": "20210825 02:46"
        }
      ]
    }
