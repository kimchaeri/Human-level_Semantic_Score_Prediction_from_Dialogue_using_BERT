      "utterance": [
        {
          "id": "MDRW2100002905.1.1",
          "form": "여행좋아해?",
          "original_form": "여행좋아해?",
          "speaker_id": "1",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002905.1.2",
          "form": "여행 좋아해?",
          "original_form": "여행 좋아해?",
          "speaker_id": "2",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002905.1.3",
          "form": "찌",
          "original_form": "찌",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002905.1.4",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002905.1.5",
          "form": "통했네",
          "original_form": "통했네",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002905.1.6",
          "form": "찌",
          "original_form": "찌",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002905.1.7",
          "form": "나는 20대때는 해외여행 좋아했는데",
          "original_form": "나는 20대때는 해외여행 좋아했는데",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002905.1.8",
          "form": "어느 순간부터 비행기 타는게 피곤해져서 국내 여행이 좋아",
          "original_form": "어느 순간부터 비행기 타는게 피곤해져서 국내 여행이 좋아",
          "speaker_id": "1",
          "time": "20210820 16:29"
        },
        {
          "id": "MDRW2100002905.1.9",
          "form": "우리 나라에도 좋은 곳 많더라",
          "original_form": "우리 나라에도 좋은 곳 많더라",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002905.1.10",
          "form": "안가본곳이 많아",
          "original_form": "안가본곳이 많아",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002905.1.11",
          "form": "응응 나도 그래",
          "original_form": "응응 나도 그래",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002905.1.12",
          "form": "가장 좋았던 여행지는 어디야?",
          "original_form": "가장 좋았던 여행지는 어디야?",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002905.1.13",
          "form": "사실 전붓 고창 여행 좋았거든 보리밭 /메밀밭",
          "original_form": "사실 전붓 고창 여행 좋았거든 보리밭 /메밀밭",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002905.1.14",
          "form": "이건 처녀때 감성이고",
          "original_form": "이건 처녀때 감성이고",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002905.1.15",
          "form": "애들이랑 어린이대공원 갔을때 ㅋㅋㅋㅋ",
          "original_form": "애들이랑 어린이대공원 갔을때 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002905.1.16",
          "form": "애들 반응 좋은 여행이...이제는 좋은 여행지 ㅋㅋ",
          "original_form": "애들 반응 좋은 여행이...이제는 좋은 여행지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002905.1.17",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002905.1.18",
          "form": "음 하나만 선택하기 어려운데",
          "original_form": "음 하나만 선택하기 어려운데",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002905.1.19",
          "form": "떠오르는건",
          "original_form": "떠오르는건",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002905.1.20",
          "form": "강원도 양양 가족여행 한 거랑",
          "original_form": "강원도 양양 가족여행 한 거랑",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002905.1.21",
          "form": "충북 제천 리솜포레스트 간 것",
          "original_form": "충북 제천 리솜포레스트 간 것",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002905.1.22",
          "form": "지금 생각나는건 이정도",
          "original_form": "지금 생각나는건 이정도",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002905.1.23",
          "form": "그리고 재작년에 후쿠오카 1박 2일로 다녀온적이 있는데 그것도 좋았어",
          "original_form": "그리고 재작년에 후쿠오카 1박 2일로 다녀온적이 있는데 그것도 좋았어",
          "speaker_id": "1",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002905.1.24",
          "form": "일본여행 처음이었는데 너무 좋았던 기억뿐이야",
          "original_form": "일본여행 처음이었는데 너무 좋았던 기억뿐이야",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002905.1.25",
          "form": "나도 일본여행 한번 갔는데 좋긴하더라",
          "original_form": "나도 일본여행 한번 갔는데 좋긴하더라",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002905.1.26",
          "form": "또 가고 싶었어",
          "original_form": "또 가고 싶었어",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002905.1.27",
          "form": "응 깔끔하고 디저트 맛있고",
          "original_form": "응 깔끔하고 디저트 맛있고",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002905.1.28",
          "form": "나는 무엇보다 도로가 깨끗한게 놀라웠어",
          "original_form": "나는 무엇보다 도로가 깨끗한게 놀라웠어",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002905.1.29",
          "form": "음 ..",
          "original_form": "음 ..",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002905.1.30",
          "form": "우리나라랑 좀 비교되더라",
          "original_form": "우리나라랑 좀 비교되더라",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002905.1.31",
          "form": "관광지도 정돈이 잘되어있고",
          "original_form": "관광지도 정돈이 잘되어있고",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002905.1.32",
          "form": "숙소가 ㄹㅛ ㅋㅏㄴ 이었는데",
          "original_form": "숙소가 ㄹㅛ ㅋㅏㄴ 이었는데",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002905.1.33",
          "form": "거기서 나온 가정식 식사가 정말 좋았어",
          "original_form": "거기서 나온 가정식 식사가 정말 좋았어",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002905.1.34",
          "form": "오오 나도 그런걸 꿈꿨는데",
          "original_form": "오오 나도 그런걸 꿈꿨는데",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002905.1.35",
          "form": "나도 도로 깔끔한거 놀랐어",
          "original_form": "나도 도로 깔끔한거 놀랐어",
          "speaker_id": "1",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002905.1.36",
          "form": "일본 드라마에서 보던 그런 가정식",
          "original_form": "일본 드라마에서 보던 그런 가정식",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002905.1.37",
          "form": "왜이렇게 깔끔해보이지 하고 생각해봤는데",
          "original_form": "왜이렇게 깔끔해보이지 하고 생각해봤는데",
          "speaker_id": "1",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002905.1.38",
          "form": "가변주차가 전혀없더라",
          "original_form": "가변주차가 전혀없더라",
          "speaker_id": "1",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002905.1.39",
          "form": "일본이 역사적으로 현재도 나쁜건 맞는데",
          "original_form": "일본이 역사적으로 현재도 나쁜건 맞는데",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002905.1.40",
          "form": "배워야할건 분명 잇더라",
          "original_form": "배워야할건 분명 잇더라",
          "speaker_id": "2",
          "time": "20210820 16:57"
        },
        {
          "id": "MDRW2100002905.1.41",
          "form": "맞아! 우리나라였음 주차난리였을텐데",
          "original_form": "맞아! 우리나라였음 주차난리였을텐데",
          "speaker_id": "2",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002905.1.42",
          "form": "경적도 함부로 안울리더라고",
          "original_form": "경적도 함부로 안울리더라고",
          "speaker_id": "2",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002905.1.43",
          "form": "응응 시민의식 좋은것 같아",
          "original_form": "응응 시민의식 좋은것 같아",
          "speaker_id": "1",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002905.1.44",
          "form": "또 갈수 잇을까? ㅎ",
          "original_form": "또 갈수 잇을까? ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002905.1.45",
          "form": "그리고 대학생일때",
          "original_form": "그리고 대학생일때",
          "speaker_id": "1",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002905.1.46",
          "form": "먹거리가 이젠 예전처럼 안심하고 먹을수 없을텐데 그것도 걱정이다",
          "original_form": "먹거리가 이젠 예전처럼 안심하고 먹을수 없을텐데 그것도 걱정이다",
          "speaker_id": "2",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002905.1.47",
          "form": "영국으로 어학연수 간 적 있었는데 그때 기억도 너무 소중하고 좋아",
          "original_form": "영국으로 어학연수 간 적 있었는데 그때 기억도 너무 소중하고 좋아",
          "speaker_id": "1",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002905.1.48",
          "form": "아 일본 식재료 불안한건 분명히 있지",
          "original_form": "아 일본 식재료 불안한건 분명히 있지",
          "speaker_id": "1",
          "time": "20210820 17:00"
        }
      ]
