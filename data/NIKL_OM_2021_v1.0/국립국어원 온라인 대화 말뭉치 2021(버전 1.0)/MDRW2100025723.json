      "utterance": [
        {
          "id": "MDRW2100025723.1.1",
          "form": "웅웅 맛있어?",
          "original_form": "웅웅 맛있어?",
          "speaker_id": "2",
          "time": "20210901 16:31"
        },
        {
          "id": "MDRW2100025723.1.2",
          "form": "완전 맛있어 ㅋㅋㅋ 흰앙금 호두과자도 해",
          "original_form": "완전 맛있어 ㅋㅋㅋ 흰앙금 호두과자도 해",
          "speaker_id": "1",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100025723.1.3",
          "form": "나는 강변이랑 성신여대에서 먹어봤는데 둘 다 별로였어ㅠ",
          "original_form": "나는 강변이랑 성신여대에서 먹어봤는데 둘 다 별로였어ㅠ",
          "speaker_id": "2",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100025723.1.4",
          "form": "아 진짜 왜?",
          "original_form": "아 진짜 왜?",
          "speaker_id": "1",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100025723.1.5",
          "form": "그냥 맛이없더라구ㅜㅜ",
          "original_form": "그냥 맛이없더라구ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100025723.1.6",
          "form": "동생집 근처는 바삭하고 맛있어 서울대입구 근처야",
          "original_form": "동생집 근처는 바삭하고 맛있어 서울대입구 근처야",
          "speaker_id": "1",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100025723.1.7",
          "form": "나중에 서울대 입구 쪽 가게되면 물어볼게",
          "original_form": "나중에 서울대 입구 쪽 가게되면 물어볼게",
          "speaker_id": "2",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025723.1.8",
          "form": "3번출구에 있어 ㅋㅋㅋㅋ근데 가는 시간에 따라 양이 달랔ㅋㅋ",
          "original_form": "3번출구에 있어 ㅋㅋㅋㅋ근데 가는 시간에 따라 양이 달랔ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025723.1.9",
          "form": "옼ㅋㅋㅋ 언제 가야 많아?",
          "original_form": "옼ㅋㅋㅋ 언제 가야 많아?",
          "speaker_id": "2",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025723.1.10",
          "form": "마감땤ㅋㅋㅋ",
          "original_form": "마감땤ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025723.1.11",
          "form": "낮에가면 현저히 적게줌..",
          "original_form": "낮에가면 현저히 적게줌..",
          "speaker_id": "1",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025723.1.12",
          "form": "앜ㅋㅋㅋ 떨이로 주는거야?",
          "original_form": "앜ㅋㅋㅋ 떨이로 주는거야?",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025723.1.13",
          "form": "그런 것 같아",
          "original_form": "그런 것 같아",
          "speaker_id": "1",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025723.1.14",
          "form": "약간 사장님 맘대로",
          "original_form": "약간 사장님 맘대로",
          "speaker_id": "1",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025723.1.15",
          "form": "사실 그런 곳은 마감때 가야 많이 주니깐ㅋㅋㅋㅋ",
          "original_form": "사실 그런 곳은 마감때 가야 많이 주니깐ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:35"
        }
