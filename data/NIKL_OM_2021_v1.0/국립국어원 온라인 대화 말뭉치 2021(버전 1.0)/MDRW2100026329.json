      "utterance": [
        {
          "id": "MDRW2100026329.1.1",
          "form": "안녕하세용 대학생이신가요?",
          "original_form": "안녕하세용 대학생이신가요?",
          "speaker_id": "1",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026329.1.2",
          "form": "안녕하세요~ 대학 졸업했습니다 허허",
          "original_form": "안녕하세요~ 대학 졸업했습니다 허허",
          "speaker_id": "2",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100026329.1.3",
          "form": "학창시절 기억에 남는 추억이 있으신가요?",
          "original_form": "학창시절 기억에 남는 추억이 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 17:47"
        },
        {
          "id": "MDRW2100026329.1.4",
          "form": "저 밤새면서 과제하는게 추억이네요 ㅎㅎ",
          "original_form": "저 밤새면서 과제하는게 추억이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100026329.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ과제의 추억...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ과제의 추억...",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100026329.1.6",
          "form": "진짜 아예 안 자고 새신 적도 있으세요?!",
          "original_form": "진짜 아예 안 자고 새신 적도 있으세요?!",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100026329.1.7",
          "form": "엄청 많아요 ㅋㅋㅋ",
          "original_form": "엄청 많아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:50"
        },
        {
          "id": "MDRW2100026329.1.8",
          "form": "밤새서 PPT 준비하고 발표한적두 많네요",
          "original_form": "밤새서 PPT 준비하고 발표한적두 많네요",
          "speaker_id": "1",
          "time": "20210907 17:50"
        },
        {
          "id": "MDRW2100026329.1.9",
          "form": "누가봐도 폐인인 얼굴로 ㅠㅠ",
          "original_form": "누가봐도 폐인인 얼굴로 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 17:50"
        },
        {
          "id": "MDRW2100026329.1.10",
          "form": "헉... 엄청 힘드셨겠어요 잠 안 자면 머리가 팽팽 돌던 기억이 나는데",
          "original_form": "헉... 엄청 힘드셨겠어요 잠 안 자면 머리가 팽팽 돌던 기억이 나는데",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100026329.1.11",
          "form": "과제량이 한가득이셨나봐요..!",
          "original_form": "과제량이 한가득이셨나봐요..!",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100026329.1.12",
          "form": "네 ㅠㅠ.. 교수들이 미웠죠 ㅋㅋㅋㅋ",
          "original_form": "네 ㅠㅠ.. 교수들이 미웠죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100026329.1.13",
          "form": "과 어디셨어요??",
          "original_form": "과 어디셨어요??",
          "speaker_id": "1",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100026329.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100026329.1.15",
          "form": "전 부동산과였는데 과제량이 그다지 많진 않았어요",
          "original_form": "전 부동산과였는데 과제량이 그다지 많진 않았어요",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100026329.1.16",
          "form": "추억 미화인가",
          "original_form": "추억 미화인가",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100026329.1.17",
          "form": "아무튼 적었어요..! 그랬을 거예요",
          "original_form": "아무튼 적었어요..! 그랬을 거예요",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100026329.1.18",
          "form": "와 부럽네요 ㅋㅋㅋ",
          "original_form": "와 부럽네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100026329.1.19",
          "form": "이과셨나요?! 폭풍과제를 하신 거 보니",
          "original_form": "이과셨나요?! 폭풍과제를 하신 거 보니",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100026329.1.20",
          "form": "저는 디자인과요 !!",
          "original_form": "저는 디자인과요 !!",
          "speaker_id": "1",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100026329.1.21",
          "form": "아 공포의 미대",
          "original_form": "아 공포의 미대",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100026329.1.22",
          "form": "예체능이요~!",
          "original_form": "예체능이요~!",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100026329.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100026329.1.24",
          "form": "미대는 시험 대신 과제로 틈틈이 평가되는건가요?!",
          "original_form": "미대는 시험 대신 과제로 틈틈이 평가되는건가요?!",
          "speaker_id": "2",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100026329.1.25",
          "form": "맞아요 시험대신 과제제출이랑 발표가 많았어요!",
          "original_form": "맞아요 시험대신 과제제출이랑 발표가 많았어요!",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100026329.1.26",
          "form": "시험없는건 좋았죠 ㅎㅎ",
          "original_form": "시험없는건 좋았죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100026329.1.27",
          "form": "그래서 밤을 자주 새셨구나 아이구",
          "original_form": "그래서 밤을 자주 새셨구나 아이구",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100026329.1.28",
          "form": "ㅋㅋㅋㅋ아 맞아요 저희쪽이 시험볼 때",
          "original_form": "ㅋㅋㅋㅋ아 맞아요 저희쪽이 시험볼 때",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100026329.1.29",
          "form": "예체능은 숨 돌리고 있던 것 같았어요",
          "original_form": "예체능은 숨 돌리고 있던 것 같았어요",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100026329.1.30",
          "form": "ㅋㅋㅋㅋ 시험만되면 과제물 발표하고 끝이라 부담은 덜었던고같아요",
          "original_form": "ㅋㅋㅋㅋ 시험만되면 과제물 발표하고 끝이라 부담은 덜었던고같아요",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100026329.1.31",
          "form": "저도 배우고싶었던 사이드 강의로 미대수업 들은 적 있는데",
          "original_form": "저도 배우고싶었던 사이드 강의로 미대수업 들은 적 있는데",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100026329.1.32",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100026329.1.33",
          "form": "고작 한두 전공만 들었는데도 평일에 과제때문에",
          "original_form": "고작 한두 전공만 들었는데도 평일에 과제때문에",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100026329.1.34",
          "form": "어떠셨어요!?",
          "original_form": "어떠셨어요!?",
          "speaker_id": "1",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100026329.1.35",
          "form": "일정에 쫒기더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "일정에 쫒기더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100026329.1.36",
          "form": "한두 전공도 이런데 아예 본전공이신 분들은",
          "original_form": "한두 전공도 이런데 아예 본전공이신 분들은",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100026329.1.37",
          "form": "아 ㅋㅋㅋ 힘들죠 ㅠㅜㅜ",
          "original_form": "아 ㅋㅋㅋ 힘들죠 ㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100026329.1.38",
          "form": "상상도 안 되네요 어우",
          "original_form": "상상도 안 되네요 어우",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100026329.1.39",
          "form": "대단하고 리스펙",
          "original_form": "대단하고 리스펙",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100026329.1.40",
          "form": "부전공으로 들으신분들도 보면 엄청 대단하신거같아요",
          "original_form": "부전공으로 들으신분들도 보면 엄청 대단하신거같아요",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100026329.1.41",
          "form": "역시 대단하구 리스펙",
          "original_form": "역시 대단하구 리스펙",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100026329.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋ아유 감사합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아유 감사합니다",
          "speaker_id": "2",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100026329.1.43",
          "form": "서로 좋군요",
          "original_form": "서로 좋군요",
          "speaker_id": "2",
