      "utterance": [
        {
          "id": "MDRW2100022386.1.1",
          "form": "빵 맛있게 식사하고 오세요",
          "original_form": "빵 맛있게 식사하고 오세요",
          "speaker_id": "2",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022386.1.2",
          "form": "저도 참치뽈찜 먹고 올게요~",
          "original_form": "저도 참치뽈찜 먹고 올게요~",
          "speaker_id": "2",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022386.1.3",
          "form": "네 식사 맛있게하세요",
          "original_form": "네 식사 맛있게하세요",
          "speaker_id": "1",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022386.1.4",
          "form": "엉아 저 왔어요",
          "original_form": "엉아 저 왔어요",
          "speaker_id": "2",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100022386.1.5",
          "form": "형 빵 다 먹었어요?",
          "original_form": "형 빵 다 먹었어요?",
          "speaker_id": "2",
          "time": "20210829 13:56"
        },
        {
          "id": "MDRW2100022386.1.6",
          "form": "아.. 네..",
          "original_form": "아.. 네..",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022386.1.7",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022386.1.8",
          "form": "ㅋㅋㅋ왜 이리 늦게 왔어요",
          "original_form": "ㅋㅋㅋ왜 이리 늦게 왔어요",
          "speaker_id": "2",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022386.1.9",
          "form": "일이 좀 있어서 ㅎ",
          "original_form": "일이 좀 있어서 ㅎ",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022386.1.10",
          "form": "형 생각에 저 후다닥 먹고 왔어요",
          "original_form": "형 생각에 저 후다닥 먹고 왔어요",
          "speaker_id": "2",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022386.1.11",
          "form": "어땠어요 뽈찜",
          "original_form": "어땠어요 뽈찜",
          "speaker_id": "1",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022386.1.12",
          "form": "기대했던 것보다 훨씬 맛있었어요",
          "original_form": "기대했던 것보다 훨씬 맛있었어요",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022386.1.13",
          "form": "오 식감은요",
          "original_form": "오 식감은요",
          "speaker_id": "1",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022386.1.14",
          "form": "안동찜닭에 무조림 합친 느낌이에요",
          "original_form": "안동찜닭에 무조림 합친 느낌이에요",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022386.1.15",
          "form": "생선 대가리 같이 먹을 게 없을 줄 알았는데",
          "original_form": "생선 대가리 같이 먹을 게 없을 줄 알았는데",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022386.1.16",
          "form": "살이 엄청 많던데요 ㄷㄷ",
          "original_form": "살이 엄청 많던데요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022386.1.17",
          "form": "원래 머리가 살이 많아요",
          "original_form": "원래 머리가 살이 많아요",
          "speaker_id": "1",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022386.1.18",
          "form": "근데 여긴 머리 아니였어요",
          "original_form": "근데 여긴 머리 아니였어요",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022386.1.19",
          "form": "다른 부위에요..",
          "original_form": "다른 부위에요..",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022386.1.20",
          "form": "선호하는 부위가 아니라그렇지",
          "original_form": "선호하는 부위가 아니라그렇지",
          "speaker_id": "1",
          "time": "20210829 14:15"
        }
      ]
    }
