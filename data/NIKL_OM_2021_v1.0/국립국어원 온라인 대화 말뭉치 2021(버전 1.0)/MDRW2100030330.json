      "utterance": [
        {
          "id": "MDRW2100030330.1.1",
          "form": "학교 생활에",
          "original_form": "학교 생활에",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030330.1.2",
          "form": "묘미는 급식이야..",
          "original_form": "묘미는 급식이야..",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030330.1.3",
          "form": "그리고 매점",
          "original_form": "그리고 매점",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030330.1.4",
          "form": "맞어 ㅋㅋ",
          "original_form": "맞어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030330.1.5",
          "form": "언니는 무슨 급식이 제일 좋아?",
          "original_form": "언니는 무슨 급식이 제일 좋아?",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030330.1.6",
          "form": "점심 석식 간식까지 먹어줘야지",
          "original_form": "점심 석식 간식까지 먹어줘야지",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030330.1.7",
          "form": "난 카레라이스 나오는 날이",
          "original_form": "난 카레라이스 나오는 날이",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.8",
          "form": "제일 좋앗엇어 ㅎㅎ",
          "original_form": "제일 좋앗엇어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.9",
          "form": "헐 나도!!!!",
          "original_form": "헐 나도!!!!",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.10",
          "form": "카레라이스는 보통",
          "original_form": "카레라이스는 보통",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.11",
          "form": "맛있는 반찬이랑 같이",
          "original_form": "맛있는 반찬이랑 같이",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.12",
          "form": "급식카레는 너무 맛있어.. 그리구 짜장밥두",
          "original_form": "급식카레는 너무 맛있어.. 그리구 짜장밥두",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.13",
          "form": "나오잖아 ㅋㅋ",
          "original_form": "나오잖아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.14",
          "form": "맞아!!",
          "original_form": "맞아!!",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.15",
          "form": "주로 수요일에 나오지",
          "original_form": "주로 수요일에 나오지",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.16",
          "form": "그럿지",
          "original_form": "그럿지",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.17",
          "form": "찹쌀탕수육같은거",
          "original_form": "찹쌀탕수육같은거",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.18",
          "form": "ㅋㅋ다 똑같군",
          "original_form": "ㅋㅋ다 똑같군",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.19",
          "form": "근데 가끔은 급식말구",
          "original_form": "근데 가끔은 급식말구",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.20",
          "form": "그러니까 ㅋㅋ",
          "original_form": "그러니까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.21",
          "form": "학교 밖으로 나가서 먹는것도",
          "original_form": "학교 밖으로 나가서 먹는것도",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.22",
          "form": "재밋고 좋아 ㅎㅎ",
          "original_form": "재밋고 좋아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030330.1.23",
          "form": "진짜? 하지만 너무 귀첞구 돈아까워",
          "original_form": "진짜? 하지만 너무 귀첞구 돈아까워",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030330.1.24",
          "form": "밥버거 같은거 먹으면 좋지",
          "original_form": "밥버거 같은거 먹으면 좋지",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030330.1.25",
          "form": "용돈 둿다 어따써!",
          "original_form": "용돈 둿다 어따써!",
          "speaker_id": "2",
          "time": "20210903 23:50"
        }
      ]
    }
