      "utterance": [
        {
          "id": "MDRW2100026468.1.1",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026468.1.2",
          "form": "갑자기 구름에서 가시나오고 하는거요",
          "original_form": "갑자기 구름에서 가시나오고 하는거요",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026468.1.3",
          "form": "실제로 하라고하면 못할거같아요...",
          "original_form": "실제로 하라고하면 못할거같아요...",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026468.1.4",
          "form": "보는건 재밌는데 ㅋㅋㅋㅋ",
          "original_form": "보는건 재밌는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026468.1.5",
          "form": "맞아요 그 게임하다가 마음에 화가 쌓일거 같은 게임...",
          "original_form": "맞아요 그 게임하다가 마음에 화가 쌓일거 같은 게임...",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026468.1.6",
          "form": "항아리게임도 너무 존경스럽다는",
          "original_form": "항아리게임도 너무 존경스럽다는",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026468.1.7",
          "form": "그게임 하다 보면 참을 인자 3번이 아니라 30번 그릴거 같아요",
          "original_form": "그게임 하다 보면 참을 인자 3번이 아니라 30번 그릴거 같아요",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026468.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아니 태초마을 갔는데도 잘 참으시더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아니 태초마을 갔는데도 잘 참으시더라구요",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026468.1.9",
          "form": "대단한 사람들",
          "original_form": "대단한 사람들",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100026468.1.10",
          "form": "화를 참고 플레이 하다니 저는 그래서 그런게임은 거의 보기만 해요ㅋㅋㅋ",
          "original_form": "화를 참고 플레이 하다니 저는 그래서 그런게임은 거의 보기만 해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026468.1.11",
          "form": "그 사슴시뮬레이션? 그것도 아세요?",
          "original_form": "그 사슴시뮬레이션? 그것도 아세요?",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026468.1.12",
          "form": "아맞아요",
          "original_form": "아맞아요",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026468.1.13",
          "form": "ㅋㅋㅋㅋㅋ그거 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ그거 맞아요",
          "speaker_id": "2",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026468.1.14",
          "form": "진짜 엄청난  사슴인 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 엄청난  사슴인 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026468.1.15",
          "form": "사슴이 두발로 달리는것 부터 심상치 않다는걸 알았어요ㅋㅋㅋ",
          "original_form": "사슴이 두발로 달리는것 부터 심상치 않다는걸 알았어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026468.1.16",
          "form": "어쩜 그런게임들 쏙쏙 찾아서 잘하는지",
          "original_form": "어쩜 그런게임들 쏙쏙 찾아서 잘하는지",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026468.1.17",
          "form": "넘욱겨요 ㅎㅎㅎ",
          "original_form": "넘욱겨요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026468.1.18",
          "form": "ㅋㅋㅋㅋ그런게임들 영상이 재미있더라고요",
          "original_form": "ㅋㅋㅋㅋ그런게임들 영상이 재미있더라고요",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026468.1.19",
          "form": "저는 마썸카 좋아해요",
          "original_form": "저는 마썸카 좋아해요",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026468.1.20",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026468.1.21",
          "form": "무슨 게임이에요?",
          "original_form": "무슨 게임이에요?",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026468.1.22",
          "form": "차를 고치는 게임인줄 알았지만 인생이 담긴 똥겜이에요 ㅋㅋㅋ",
          "original_form": "차를 고치는 게임인줄 알았지만 인생이 담긴 똥겜이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026468.1.23",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026468.1.24",
          "form": "그 병원 게임도 아세요? 이것도 꽤 유명한건데",
          "original_form": "그 병원 게임도 아세요? 이것도 꽤 유명한건데",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026468.1.25",
          "form": "오 호스피탈 인가 그거맞나요",
          "original_form": "오 호스피탈 인가 그거맞나요",
          "speaker_id": "1",
          "time": "20210901 23:08"
        }
