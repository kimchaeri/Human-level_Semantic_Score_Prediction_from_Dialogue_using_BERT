        },
        {
          "id": "MDRW2100028229.1.2",
          "form": "전 아직 물건 털린편 보고있다는 ㅎㅎ",
          "original_form": "전 아직 물건 털린편 보고있다는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028229.1.3",
          "form": "ㅋㅋ그 이집트편이",
          "original_form": "ㅋㅋ그 이집트편이",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028229.1.4",
          "form": "재밌었어요 전",
          "original_form": "재밌었어요 전",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028229.1.5",
          "form": "혹시 비둘기 였던가요?",
          "original_form": "혹시 비둘기 였던가요?",
          "speaker_id": "1",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100028229.1.6",
          "form": "네 거기",
          "original_form": "네 거기",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028229.1.7",
          "form": "그 호객꾼 오니까",
          "original_form": "그 호객꾼 오니까",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028229.1.8",
          "form": "비둘기집 올라가는거 보고 기절할뻔 ㅋㅋ",
          "original_form": "비둘기집 올라가는거 보고 기절할뻔 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:50"
        },
        {
          "id": "MDRW2100028229.1.9",
          "form": "소리 지르는거요 ㅋㅋ",
          "original_form": "소리 지르는거요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:50"
        },
        {
