      "utterance": [
        {
          "id": "MDRW2100035818.1.1",
          "form": "요즘 날이 제법 쌀쌀해서",
          "original_form": "요즘 날이 제법 쌀쌀해서",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100035818.1.2",
          "form": "가을옷을 조금만 꺼내볼까하는데",
          "original_form": "가을옷을 조금만 꺼내볼까하는데",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100035818.1.3",
          "form": "옷장정리는 하셨어요?",
          "original_form": "옷장정리는 하셨어요?",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035818.1.4",
          "form": "아뇨.. 이제 해야해요 ㅠㅜㅋㅋ 슬슬 간절해지고잇어요 ㅋㅋㅋ",
          "original_form": "아뇨.. 이제 해야해요 ㅠㅜㅋㅋ 슬슬 간절해지고잇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035818.1.5",
          "form": "여름이 참 빨리지나간것같네요",
          "original_form": "여름이 참 빨리지나간것같네요",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035818.1.6",
          "form": "맞아요ㅠㅠㅋㅋ근데 보통 추석전에 또 더워지는 시기가 와서",
          "original_form": "맞아요ㅠㅠㅋㅋ근데 보통 추석전에 또 더워지는 시기가 와서",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100035818.1.7",
          "form": "여름옷 다 넣어놨다가 다시 더워지면",
          "original_form": "여름옷 다 넣어놨다가 다시 더워지면",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035818.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035818.1.9",
          "form": "ㅋㅋㅋㅋ 그래서 저 그냥 여름이불 덮고자고잇어요",
          "original_form": "ㅋㅋㅋㅋ 그래서 저 그냥 여름이불 덮고자고잇어요",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035818.1.10",
          "form": "저두요 또 더워지겠지!",
          "original_form": "저두요 또 더워지겠지!",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035818.1.11",
          "form": "밤이라 좀 춥긴한데말이죠 ㅜ",
          "original_form": "밤이라 좀 춥긴한데말이죠 ㅜ",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035818.1.12",
          "form": "이러고 추워도",
          "original_form": "이러고 추워도",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035818.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035818.1.14",
          "form": "창문닫고",
          "original_form": "창문닫고",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035818.1.15",
          "form": "맞아요 ㅜ",
          "original_form": "맞아요 ㅜ",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035818.1.16",
          "form": "선풍기 끄고 버티고있어요 ㅋㅋㅋ",
          "original_form": "선풍기 끄고 버티고있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035818.1.17",
          "form": "닫으면 또 동생한테 욕먹어요 전 ㅠ 환기좀 하라고 ㅜ",
          "original_form": "닫으면 또 동생한테 욕먹어요 전 ㅠ 환기좀 하라고 ㅜ",
          "speaker_id": "2",
          "time": "20210830 03:31"
        },
        {
          "id": "MDRW2100035818.1.18",
          "form": "아아 저도 엄마한테 욕먹어욬ㅋㅋㅋㅋ",
          "original_form": "아아 저도 엄마한테 욕먹어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100035818.1.19",
          "form": "이번주는 내내 비온대요",
          "original_form": "이번주는 내내 비온대요",
          "speaker_id": "1",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100035818.1.20",
          "form": "헉.. 또 장마인가여?",
          "original_form": "헉.. 또 장마인가여?",
          "speaker_id": "2",
          "time": "20210830 03:32"
        },
        {
          "id": "MDRW2100035818.1.21",
          "form": "그런것같아요ㅋㅋㅋ",
          "original_form": "그런것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100035818.1.22",
          "form": "비가 주륵주륵...",
          "original_form": "비가 주륵주륵...",
          "speaker_id": "1",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100035818.1.23",
          "form": "아 ... 오늘 참 좋앗는데 말이져",
          "original_form": "아 ... 오늘 참 좋앗는데 말이져",
          "speaker_id": "2",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100035818.1.24",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100035818.1.25",
          "form": "비올때 맨날 밖에서 우산 사가지고",
          "original_form": "비올때 맨날 밖에서 우산 사가지고",
          "speaker_id": "1",
          "time": "20210830 03:33"
        },
        {
          "id": "MDRW2100035818.1.26",
          "form": "집에 우산이 넘 많아요",
          "original_form": "집에 우산이 넘 많아요",
          "speaker_id": "1",
          "time": "20210830 03:33"
        }
      ]
    }
  ]
