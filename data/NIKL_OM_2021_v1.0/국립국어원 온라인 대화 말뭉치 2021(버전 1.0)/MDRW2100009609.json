      "utterance": [
        {
          "id": "MDRW2100009609.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 01:39"
        },
        {
          "id": "MDRW2100009609.1.2",
          "form": "안녕하세요 요즘 장마라 많이 꿉꿉하죠ㅠㅠ",
          "original_form": "안녕하세요 요즘 장마라 많이 꿉꿉하죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 01:41"
        },
        {
          "id": "MDRW2100009609.1.3",
          "form": "맞아요 ㅠㅠ 어제도 비가 엄청 쏟아지더라구요",
          "original_form": "맞아요 ㅠㅠ 어제도 비가 엄청 쏟아지더라구요",
          "speaker_id": "1",
          "time": "20210823 01:43"
        },
        {
          "id": "MDRW2100009609.1.4",
          "form": "그러다가 오늘은 한참 덥고",
          "original_form": "그러다가 오늘은 한참 덥고",
          "speaker_id": "1",
          "time": "20210823 01:43"
        },
        {
          "id": "MDRW2100009609.1.5",
          "form": "헉 저희는 갑자기 비 몰아치더니 추워졌어요",
          "original_form": "헉 저희는 갑자기 비 몰아치더니 추워졌어요",
          "speaker_id": "2",
          "time": "20210823 01:44"
        },
        {
          "id": "MDRW2100009609.1.6",
          "form": "아니 옷을 어떻게 입어야할지 모르겠더라고요",
          "original_form": "아니 옷을 어떻게 입어야할지 모르겠더라고요",
          "speaker_id": "2",
          "time": "20210823 01:44"
        },
        {
          "id": "MDRW2100009609.1.7",
          "form": "정말요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "정말요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009609.1.8",
          "form": "내 덥다가 갑자기 춥고 이러니깐",
          "original_form": "내 덥다가 갑자기 춥고 이러니깐",
          "speaker_id": "1",
          "time": "20210823 01:45"
        },
        {
          "id": "MDRW2100009609.1.9",
          "form": "요즘 그래서 기상이변 심해졌다고 느껴욬ㅋㅋㅋ",
          "original_form": "요즘 그래서 기상이변 심해졌다고 느껴욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009609.1.10",
          "form": "더울땐 폭염인데 그게 또 길어진 것 같고",
          "original_form": "더울땐 폭염인데 그게 또 길어진 것 같고",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009609.1.11",
          "form": "ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009609.1.12",
          "form": "그쵸... 이러다가 9월에는 또 갑자기 더워지는 거 아닌가 싶어요",
          "original_form": "그쵸... 이러다가 9월에는 또 갑자기 더워지는 거 아닌가 싶어요",
          "speaker_id": "1",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009609.1.13",
          "form": "근데 사실 저는 비보다는 더운 게 나은 사람이라",
          "original_form": "근데 사실 저는 비보다는 더운 게 나은 사람이라",
          "speaker_id": "1",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009609.1.14",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009609.1.15",
          "form": "여름을 더 좋아하시는군요?!",
          "original_form": "여름을 더 좋아하시는군요?!",
          "speaker_id": "2",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009609.1.16",
          "form": "더위안타세요?!",
          "original_form": "더위안타세요?!",
          "speaker_id": "2",
          "time": "20210823 01:47"
        },
        {
          "id": "MDRW2100009609.1.17",
          "form": "저 더위 상당히 타는 편이기는 한데...!!",
          "original_form": "저 더위 상당히 타는 편이기는 한데...!!",
          "speaker_id": "1",
          "time": "20210823 01:49"
        },
        {
          "id": "MDRW2100009609.1.18",
          "form": "비오는 날에 울적해지는 편이라서",
          "original_form": "비오는 날에 울적해지는 편이라서",
          "speaker_id": "1",
          "time": "20210823 01:50"
        },
        {
          "id": "MDRW2100009609.1.19",
          "form": "차라리 맑은 날씨를 좋아해요",
          "original_form": "차라리 맑은 날씨를 좋아해요",
          "speaker_id": "1",
          "time": "20210823 01:50"
        },
        {
          "id": "MDRW2100009609.1.20",
          "form": "name2님은 날씨안타시나요?",
          "original_form": "&name2&님은 날씨안타시나요?",
          "speaker_id": "1",
          "time": "20210823 01:50"
        },
        {
          "id": "MDRW2100009609.1.21",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009609.1.22",
          "form": "아아 그쵸그쵸",
          "original_form": "아아 그쵸그쵸",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009609.1.23",
          "form": "어두워서",
          "original_form": "어두워서",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009609.1.24",
          "form": "저는 약간 비오는 날에",
          "original_form": "저는 약간 비오는 날에",
          "speaker_id": "2",
          "time": "20210823 01:51"
        },
        {
          "id": "MDRW2100009609.1.25",
          "form": "창가에서 노트북하면 기분 좋아져요",
          "original_form": "창가에서 노트북하면 기분 좋아져요",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009609.1.26",
          "form": "근데 밖에나가면 급 짜증나고 그러더라고요",
          "original_form": "근데 밖에나가면 급 짜증나고 그러더라고요",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009609.1.27",
          "form": "저는 더위 어어어엄청 타서",
          "original_form": "저는 더위 어어어엄청 타서",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009609.1.28",
          "form": "혹시 name1님 양산 쓰시나요?!",
          "original_form": "혹시 &name1&님 양산 쓰시나요?!",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009609.1.29",
          "form": "전 더위타서 양산쓰고 다닙니다",
          "original_form": "전 더위타서 양산쓰고 다닙니다",
          "speaker_id": "2",
          "time": "20210823 01:52"
        },
        {
          "id": "MDRW2100009609.1.30",
          "form": "저 양산!! 좋아합니다",
          "original_form": "저 양산!! 좋아합니다",
          "speaker_id": "1",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009609.1.31",
          "form": "학창시절에도 신경 안쓰고 들고 다녔던 거 같아요",
          "original_form": "학창시절에도 신경 안쓰고 들고 다녔던 거 같아요",
          "speaker_id": "1",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009609.1.32",
          "form": "오오오!!",
          "original_form": "오오오!!",
          "speaker_id": "2",
          "time": "20210823 01:54"
        },
        {
          "id": "MDRW2100009609.1.33",
          "form": "집에 골프우산이 있어서",
          "original_form": "집에 골프우산이 있어서",
          "speaker_id": "1",
          "time": "20210823 01:54"
