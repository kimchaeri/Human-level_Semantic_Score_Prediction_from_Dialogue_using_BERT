      "utterance": [
        {
          "id": "MDRW2100021952.1.1",
          "form": "좋아하는 예능프로그램있으세요?",
          "original_form": "좋아하는 예능프로그램있으세요?",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021952.1.2",
          "form": "일박이일 하고 이번에 시작한 스우파? 좀 흥미롭더라고요",
          "original_form": "일박이일 하고 이번에 시작한 스우파? 좀 흥미롭더라고요",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021952.1.3",
          "form": "어? 스우파요?",
          "original_form": "어? 스우파요?",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021952.1.4",
          "form": "누구누구나와요?",
          "original_form": "누구누구나와요?",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021952.1.5",
          "form": "스트릿댄서들 잔뜩?? ㅋㅋ",
          "original_form": "스트릿댄서들 잔뜩?? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021952.1.6",
          "form": "추천이 많길래 봤어요!",
          "original_form": "추천이 많길래 봤어요!",
          "speaker_id": "1",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021952.1.7",
          "form": "오ㅎㅎ",
          "original_form": "오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021952.1.8",
          "form": "한번봐봐야겟네요ㅎㅎ",
          "original_form": "한번봐봐야겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021952.1.9",
          "form": "예능이 요즘 대세긴해요ㅎ",
          "original_form": "예능이 요즘 대세긴해요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021952.1.10",
          "form": "예전에는 토크쇼나 이런데서 영화나 드라마 홍보했잖아요ㅎ",
          "original_form": "예전에는 토크쇼나 이런데서 영화나 드라마 홍보했잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021952.1.11",
          "form": "근데 요즘은 예능에서ㅎㅎ",
          "original_form": "근데 요즘은 예능에서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021952.1.12",
          "form": "요즘은 유퀴즈가 대세더라고요",
          "original_form": "요즘은 유퀴즈가 대세더라고요",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021952.1.13",
          "form": "거기가 유일한 토크쇼죠",
          "original_form": "거기가 유일한 토크쇼죠",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021952.1.14",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021952.1.15",
          "form": "유일한 토크쑈ㅎㅎ",
          "original_form": "유일한 토크쑈ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021952.1.16",
          "form": "유새석고 재석고라ㅎ 저도 잘보고있어요ㅎㅎ",
          "original_form": "유새석고 재석고라ㅎ 저도 잘보고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021952.1.17",
          "form": "ㅋㅋ 진짜 유재석 너무 대단해요 몇십년을",
          "original_form": "ㅋㅋ 진짜 유재석 너무 대단해요 몇십년을",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021952.1.18",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021952.1.19",
          "form": "유재석이 연예인들중에 제일 잘나가죠 ㅎㅎ",
          "original_form": "유재석이 연예인들중에 제일 잘나가죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021952.1.20",
          "form": "런닝맨 노라기리편 보셨어요?",
          "original_form": "런닝맨 노라기리편 보셨어요?",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021952.1.21",
          "form": "네ㅋㅋ 아 너무 웃겨요",
          "original_form": "네ㅋㅋ 아 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021952.1.22",
          "form": "저 아깐 유대감특집 봤어요!",
          "original_form": "저 아깐 유대감특집 봤어요!",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021952.1.23",
          "form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021952.1.24",
          "form": "엄청재밌아요ㅎㅎ",
          "original_form": "엄청재밌아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021952.1.25",
          "form": "유재석님은 남놀리는거는 타고났어요ㅎ",
          "original_form": "유재석님은 남놀리는거는 타고났어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021952.1.26",
          "form": "이광수표정도 웃기고ㅎㅎ",
          "original_form": "이광수표정도 웃기고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021952.1.27",
          "form": "ㅋㅋ그니까요 이광수 진짜 예능신의 아들인데!!!!!",
          "original_form": "ㅋㅋ그니까요 이광수 진짜 예능신의 아들인데!!!!!",
          "speaker_id": "1",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021952.1.28",
          "form": "요즘 미주나 전소민 편하게 대하는거같아요",
          "original_form": "요즘 미주나 전소민 편하게 대하는거같아요",
          "speaker_id": "1",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021952.1.29",
          "form": "맞아요ㅎㅎ 식스센스ㅎㅎ",
          "original_form": "맞아요ㅎㅎ 식스센스ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021952.1.30",
          "form": "잼있어요ㅎㅎ",
          "original_form": "잼있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021952.1.31",
          "form": "근데 전소민이랑 유재석이 잘어울린다는 생각은 너무 뭐라지? 완전 막장이죠?",
          "original_form": "근데 전소민이랑 유재석이 잘어울린다는 생각은 너무 뭐라지? 완전 막장이죠?",
          "speaker_id": "2",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021952.1.32",
          "form": "ㅋㅋ넵ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ넵ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021952.1.33",
          "form": "전소민 다 잘어울리더라고요",
          "original_form": "전소민 다 잘어울리더라고요",
          "speaker_id": "1",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021952.1.34",
          "form": "ㅎㅎ 아근데 정말 뭔기 느낌이 잘어울려서ㅎㅎ",
