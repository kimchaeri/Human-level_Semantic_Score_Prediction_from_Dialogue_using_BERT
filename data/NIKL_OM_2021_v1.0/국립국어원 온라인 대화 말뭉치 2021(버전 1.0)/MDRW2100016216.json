      "utterance": [
        {
          "id": "MDRW2100016216.1.1",
          "form": "유튭 보시나요",
          "original_form": "유튭 보시나요",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100016216.1.2",
          "form": "네 자주 봐요",
          "original_form": "네 자주 봐요",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016216.1.3",
          "form": "어떤거 보시나요",
          "original_form": "어떤거 보시나요",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016216.1.4",
          "form": "예능 클립이나 뮤직비디오 많이 보는것 같아요",
          "original_form": "예능 클립이나 뮤직비디오 많이 보는것 같아요",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016216.1.5",
          "form": "무한도전 오분순삭!",
          "original_form": "무한도전 오분순삭!",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016216.1.6",
          "form": "저두 봐요! 지금봐도 너무 재밌더라고요",
          "original_form": "저두 봐요! 지금봐도 너무 재밌더라고요",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016216.1.7",
          "form": "무한도전  시즌2했으면ㅠ",
          "original_form": "무한도전  시즌2했으면ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100016216.1.8",
          "form": "요새 멤버들 여기저기 나오는 것 같긴해요",
          "original_form": "요새 멤버들 여기저기 나오는 것 같긴해요",
          "speaker_id": "2",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016216.1.9",
          "form": "놀면뭐하니에 나오더라구요",
          "original_form": "놀면뭐하니에 나오더라구요",
          "speaker_id": "1",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016216.1.10",
          "form": "네 그거 봤어요. 재밌던데요ㅋㅋ",
          "original_form": "네 그거 봤어요. 재밌던데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:52"
        },
        {
          "id": "MDRW2100016216.1.11",
          "form": "옛날멤버들 나와서ㅠ",
          "original_form": "옛날멤버들 나와서ㅠ",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016216.1.12",
          "form": "그립기도 하고ㅠ 더 나올 수도 있다니까 기대되요",
          "original_form": "그립기도 하고ㅠ 더 나올 수도 있다니까 기대되요",
          "speaker_id": "2",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016216.1.13",
          "form": "다모였으면 좋을텐데",
          "original_form": "다모였으면 좋을텐데",
          "speaker_id": "1",
