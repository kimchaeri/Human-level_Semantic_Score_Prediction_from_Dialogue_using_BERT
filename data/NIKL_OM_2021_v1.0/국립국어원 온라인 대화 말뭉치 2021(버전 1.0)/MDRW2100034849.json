      "utterance": [
        {
          "id": "MDRW2100034849.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100034849.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100034849.1.3",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100034849.1.4",
          "form": "키우진 않지만 좋아해요ㅎ",
          "original_form": "키우진 않지만 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100034849.1.5",
          "form": "저는 고양이 한 마리 키우고 있어요 ㅎ",
          "original_form": "저는 고양이 한 마리 키우고 있어요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100034849.1.6",
          "form": "고양이 정말 귀엽겠네요ㅠㅠ 어떤 고양이인가요?",
          "original_form": "고양이 정말 귀엽겠네요ㅠㅠ 어떤 고양이인가요?",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100034849.1.7",
          "form": "삼색고양이에요 ㅎㅎ",
          "original_form": "삼색고양이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100034849.1.8",
          "form": "무릎에 앉아있는데 너무 덥습니다... ㅎㅎ",
          "original_form": "무릎에 앉아있는데 너무 덥습니다... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100034849.1.9",
          "form": "오 그럼 암컷인가요? 정말 부럽네요ㅜㅜ 더워도 좋으니 키우고 싶네요ㅠㅠ",
          "original_form": "오 그럼 암컷인가요? 정말 부럽네요ㅜㅜ 더워도 좋으니 키우고 싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100034849.1.10",
          "form": "저희 애는 수컷이에요!\n병원에서 암컷이래서 데려왔는데 키우고보니 수컷이네요 ㅎㅎ",
          "original_form": "저희 애는 수컷이에요!\n병원에서 암컷이래서 데려왔는데 키우고보니 수컷이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100034849.1.11",
          "form": "그럴수도 있나요?ㅋㅋㅋ 삼색이는 거의 암컷이라는 얘기를 본거같은데 수컷이라뇨 정말 신기하네요",
          "original_form": "그럴수도 있나요?ㅋㅋㅋ 삼색이는 거의 암컷이라는 얘기를 본거같은데 수컷이라뇨 정말 신기하네요",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100034849.1.12",
          "form": "눈도 못 뜰때 데려가서 티도 안나는데, 잠복고환이라 더 안보였대요 ㅎㅎ",
          "original_form": "눈도 못 뜰때 데려가서 티도 안나는데, 잠복고환이라 더 안보였대요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034849.1.13",
          "form": "삼색이라 암컷이겠거니 했던 것도 있다고 하셨어요ㅎㅎ",
          "original_form": "삼색이라 암컷이겠거니 했던 것도 있다고 하셨어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034849.1.14",
          "form": "그렇군요ㅎㅎ 수컷이 삼색이중 1퍼라는데 로또네요!",
          "original_form": "그렇군요ㅎㅎ 수컷이 삼색이중 1퍼라는데 로또네요!",
          "speaker_id": "2",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100034849.1.15",
          "form": "고양이는 입양하셨나요?",
          "original_form": "고양이는 입양하셨나요?",
          "speaker_id": "2",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100034849.1.16",
          "form": "아파트에 길냥이들이 많아서 밥 챙겨 주고 하다가",
          "original_form": "아파트에 길냥이들이 많아서 밥 챙겨 주고 하다가",
          "speaker_id": "1",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100034849.1.17",
          "form": "근데 어미가 아픈 애 둘을 버리고 떠나서 같이 돌봐주던 분이랑 한 마리씩 데려왔어요!",
          "original_form": "근데 어미가 아픈 애 둘을 버리고 떠나서 같이 돌봐주던 분이랑 한 마리씩 데려왔어요!",
          "speaker_id": "1",
          "time": "20210828 22:28"
        }
      ]
