      "utterance": [
        {
          "id": "MDRW2100029634.1.1",
          "form": "저는 낮은음이지요",
          "original_form": "저는 낮은음이지요",
          "speaker_id": "1",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029634.1.2",
          "form": "그래도 하는건 제손으로 합니다",
          "original_form": "그래도 하는건 제손으로 합니다",
          "speaker_id": "2",
          "time": "20210903 11:28"
        },
        {
          "id": "MDRW2100029634.1.3",
          "form": "대출처런 해줘서",
          "original_form": "대출처런 해줘서",
          "speaker_id": "2",
          "time": "20210903 11:30"
        },
        {
          "id": "MDRW2100029634.1.4",
          "form": "부평동 먹자",
          "original_form": "부평동 먹자",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100029634.1.5",
          "form": "야식골목 거기오",
          "original_form": "야식골목 거기오",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100029634.1.6",
          "form": "데이트할 때 항상 먹는 음식은",
          "original_form": "데이트할 때 항상 먹는 음식은",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100029634.1.7",
          "form": "언제쯤 만나요",
          "original_form": "언제쯤 만나요",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100029634.1.8",
          "form": "북촌 피만두에서 냉면과 만두를 시키는데",
          "original_form": "북촌 피만두에서 냉면과 만두를 시키는데",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100029634.1.9",
          "form": "남친이 요리도 잘 하나요",
          "original_form": "남친이 요리도 잘 하나요",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100029634.1.10",
          "form": "새우 튀김 만두가 정말 정말 좋은 맛이에요",
          "original_form": "새우 튀김 만두가 정말 정말 좋은 맛이에요",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100029634.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100029634.1.12",
          "form": "만나면 남친이 요리 다 해줍니다",
          "original_form": "만나면 남친이 요리 다 해줍니다",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100029634.1.13",
          "form": "튀김인가요 만두인가요",
          "original_form": "튀김인가요 만두인가요",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100029634.1.14",
          "form": "아직 백신 1차를 안 맞아서",
          "original_form": "아직 백신 1차를 안 맞아서",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100029634.1.15",
          "form": "남친이 1차 맞으면 그때 보려고요",
          "original_form": "남친이 1차 맞으면 그때 보려고요",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100029634.1.16",
          "form": "맞으면 만나나요",
          "original_form": "맞으면 만나나요",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100029634.1.17",
          "form": "튀긴 만두입니다",
          "original_form": "튀긴 만두입니다",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100029634.1.18",
          "form": "언제 만나",
          "original_form": "언제 만나",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100029634.1.19",
          "form": "주제 보세요",
          "original_form": "주제 보세요",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100029634.1.20",
          "form": "새우가 들어간 만두를 쪄서도 먹고 튀겨서도 먹는데 튀긴게 정말 맛있어요",
          "original_form": "새우가 들어간 만두를 쪄서도 먹고 튀겨서도 먹는데 튀긴게 정말 맛있어요",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100029634.1.21",
          "form": "게임이네요",
          "original_form": "게임이네요",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100029634.1.22",
          "form": "망했다",
          "original_form": "망했다",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100029634.1.23",
          "form": "남친이랑 게임을 자주 합니다",
          "original_form": "남친이랑 게임을 자주 합니다",
          "speaker_id": "2",
          "time": "20210904 00:10"
        }
      ]
