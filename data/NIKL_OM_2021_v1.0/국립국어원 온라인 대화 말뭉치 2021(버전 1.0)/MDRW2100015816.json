      "utterance": [
        {
          "id": "MDRW2100015816.1.1",
          "form": "요리좋아하세요?",
          "original_form": "요리좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015816.1.2",
          "form": "좋아하는편이에요!",
          "original_form": "좋아하는편이에요!",
          "speaker_id": "2",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015816.1.3",
          "form": "오 주로 무슨요리하세요?",
          "original_form": "오 주로 무슨요리하세요?",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015816.1.4",
          "form": "주로 파스타 많이해요! 다른것두 이것저것",
          "original_form": "주로 파스타 많이해요! 다른것두 이것저것",
          "speaker_id": "2",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015816.1.5",
          "form": "근데 좀 게으른편이라 아주가끔해요 ㅋㅋㅋ",
          "original_form": "근데 좀 게으른편이라 아주가끔해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015816.1.6",
          "form": "배달 음식 좋아하세요?",
          "original_form": "배달 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015816.1.7",
          "form": "시켜먹는거 좋아하는데 자주먹진않아요",
          "original_form": "시켜먹는거 좋아하는데 자주먹진않아요",
          "speaker_id": "1",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015816.1.8",
          "form": "그럼 주로 어떤거 시켜드세요?",
          "original_form": "그럼 주로 어떤거 시켜드세요?",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015816.1.9",
          "form": "혼자라 가끔치킨요",
          "original_form": "혼자라 가끔치킨요",
          "speaker_id": "1",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015816.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015816.1.11",
          "form": "저두 치킨이요!",
          "original_form": "저두 치킨이요!",
          "speaker_id": "2",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015816.1.12",
          "form": "어느 브랜드 좋아하세여?",
          "original_form": "어느 브랜드 좋아하세여?",
          "speaker_id": "2",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100015816.1.13",
          "form": "전 맛초킹ㅎ 교촌오리지날",
          "original_form": "전 맛초킹ㅎ 교촌오리지날",
          "speaker_id": "1",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100015816.1.14",
          "form": "간장베이스 다좋아해요",
          "original_form": "간장베이스 다좋아해요",
          "speaker_id": "1",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100015816.1.15",
          "form": "오리지날이 허니콤보말하능건가요?",
          "original_form": "오리지날이 허니콤보말하능건가요?",
          "speaker_id": "2",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100015816.1.16",
          "form": "오리지날은 기본이요",
          "original_form": "오리지날은 기본이요",
          "speaker_id": "1",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015816.1.17",
          "form": "허니콤보는 좀더달죠",
          "original_form": "허니콤보는 좀더달죠",
          "speaker_id": "1",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015816.1.18",
          "form": "오 그렇군요 오리지날은 한번도 안먹어본듯해요",
          "original_form": "오 그렇군요 오리지날은 한번도 안먹어본듯해요",
          "speaker_id": "2",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015816.1.19",
          "form": "오리지날에 꿀?넣은게 허니콤보일듯요",
          "original_form": "오리지날에 꿀?넣은게 허니콤보일듯요",
          "speaker_id": "1",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015816.1.20",
          "form": "아아 그렇군요 담에 먹어봐여겠어요!",
          "original_form": "아아 그렇군요 담에 먹어봐여겠어요!",
          "speaker_id": "2",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015816.1.21",
          "form": "매운거 좋아하세요?",
          "original_form": "매운거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015816.1.22",
          "form": "넵 ㅎ 맵고 짜고 달고 시고ㅎ",
          "original_form": "넵 ㅎ 맵고 짜고 달고 시고ㅎ",
          "speaker_id": "1",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015816.1.23",
          "form": "자극적인거 다좋아해요",
          "original_form": "자극적인거 다좋아해요",
          "speaker_id": "1",
          "time": "20210826 01:44"
        }
