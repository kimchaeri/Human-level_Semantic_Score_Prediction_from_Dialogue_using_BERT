      "utterance": [
        {
          "id": "MDRW2100041899.1.1",
          "form": "로제 스파게티 만들어줄까?",
          "original_form": "로제 스파게티 만들어줄까?",
          "speaker_id": "1",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041899.1.2",
          "form": "그게 뭐야",
          "original_form": "그게 뭐야",
          "speaker_id": "2",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041899.1.3",
          "form": "크림이랑 토마토 반반 섞인거~",
          "original_form": "크림이랑 토마토 반반 섞인거~",
          "speaker_id": "1",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041899.1.4",
          "form": "떡볶이 먹었던 거",
          "original_form": "떡볶이 먹었던 거",
          "speaker_id": "2",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041899.1.5",
          "form": "웅웅 요새 떡볶이 로제맛으로 많이 나오지 ㅋㅋ",
          "original_form": "웅웅 요새 떡볶이 로제맛으로 많이 나오지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041899.1.6",
          "form": "맛있겠네 해 봐라",
          "original_form": "맛있겠네 해 봐라",
          "speaker_id": "2",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041899.1.7",
          "form": "웅웅 생크림도 있으니까 가능함~",
          "original_form": "웅웅 생크림도 있으니까 가능함~",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041899.1.8",
          "form": "냉장고에 있는 재료 다 써라",
          "original_form": "냉장고에 있는 재료 다 써라",
          "speaker_id": "2",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041899.1.9",
          "form": "알았다 ㅎㅎ",
          "original_form": "알았다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041899.1.10",
          "form": "면은 많이 하지 말고",
          "original_form": "면은 많이 하지 말고",
          "speaker_id": "2",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041899.1.11",
          "form": "웅웅 살찌니까 적당히 ㅋ",
          "original_form": "웅웅 살찌니까 적당히 ㅋ",
          "speaker_id": "1",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041899.1.12",
          "form": "그래 지금 할 거야",
          "original_form": "그래 지금 할 거야",
          "speaker_id": "2",
          "time": "20210906 17:20"
        },
