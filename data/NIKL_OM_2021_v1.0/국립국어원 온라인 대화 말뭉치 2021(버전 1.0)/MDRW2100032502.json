      "utterance": [
        {
          "id": "MDRW2100032502.1.1",
          "form": "걍 엄마 눈 피해서",
          "original_form": "걍 엄마 눈 피해서",
          "speaker_id": "2",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032502.1.2",
          "form": "나 한달동안",
          "original_form": "나 한달동안",
          "speaker_id": "1",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032502.1.3",
          "form": "폰+ 배달음식",
          "original_form": "폰+ 배달음식",
          "speaker_id": "2",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032502.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032502.1.5",
          "form": "운동 ㄱ ㅐ빡시게 해야겟다",
          "original_form": "운동 ㄱ ㅐ빡시게 해야겟다",
          "speaker_id": "1",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032502.1.6",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032502.1.7",
          "form": "벼락치기로",
          "original_form": "벼락치기로",
          "speaker_id": "1",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032502.1.8",
          "form": "체력 길러봅니다",
          "original_form": "체력 길러봅니다",
          "speaker_id": "1",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032502.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄴ",
          "speaker_id": "2",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100032502.1.10",
          "form": "벼락치기 체력ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "벼락치기 체력ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032502.1.11",
          "form": "지금까지 입으로만운동햇는데",
          "original_form": "지금까지 입으로만운동햇는데",
          "speaker_id": "1",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032502.1.12",
          "form": "내가 또 목표생기먄",
          "original_form": "내가 또 목표생기먄",
          "speaker_id": "1",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032502.1.13",
          "form": "제대로하지 ㅎ",
          "original_form": "제대로하지 ㅎ",
          "speaker_id": "1",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032502.1.14",
          "form": "나 당장 런데이부터깐다",
          "original_form": "나 당장 런데이부터깐다",
          "speaker_id": "1",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032502.1.15",
          "form": "근데 런데이 뭐하는거임",
          "original_form": "근데 런데이 뭐하는거임",
          "speaker_id": "2",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032502.1.16",
          "form": "런닝하는거",
          "original_form": "런닝하는거",
          "speaker_id": "1",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032502.1.17",
          "form": "걍 삼성헬스 이런거랑 비슷항걸?",
          "original_form": "걍 삼성헬스 이런거랑 비슷항걸?",
          "speaker_id": "1",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032502.1.18",
          "form": "아 넌 달리기 어디서 해?",
          "original_form": "아 넌 달리기 어디서 해?",
          "speaker_id": "2",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.19",
          "form": "안하는데?",
          "original_form": "안하는데?",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.20",
          "form": "ㅇㅅㅇ",
          "original_form": "ㅇㅅㅇ",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.21",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ런데이 깔아서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ런데이 깔아서",
          "speaker_id": "2",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.23",
          "form": "어디서 하려고",
          "original_form": "어디서 하려고",
          "speaker_id": "2",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.25",
          "form": "그러게..",
          "original_form": "그러게..",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.26",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.27",
          "form": "황령산 쉼터라도 가야하나",
          "original_form": "황령산 쉼터라도 가야하나",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.28",
          "form": "문초가라",
          "original_form": "문초가라",
          "speaker_id": "2",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.29",
          "form": "지금 안여나?",
          "original_form": "지금 안여나?",
          "speaker_id": "2",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.30",
          "form": "아 문현초??",
          "original_form": "아 문현초??",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.31",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032502.1.32",
          "form": "운동장은 개방돼잇을걸?",
          "original_form": "운동장은 개방돼잇을걸?",
          "speaker_id": "1",
          "time": "20210905 19:46"
        }
      ]
