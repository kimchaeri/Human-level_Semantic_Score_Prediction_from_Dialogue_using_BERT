        },
        {
          "id": "MDRW2100025476.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025476.1.3",
          "form": "좋아하는 영화 있으세요?",
          "original_form": "좋아하는 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210901 12:05"
        },
        {
          "id": "MDRW2100025476.1.4",
          "form": "요번에 아메리칸 셰프 봤는데",
          "original_form": "요번에 아메리칸 셰프 봤는데",
          "speaker_id": "2",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100025476.1.5",
          "form": "재밌드라구용",
          "original_form": "재밌드라구용",
          "speaker_id": "2",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100025476.1.6",
          "form": "처음 들어봤는데 무슨 내용인가요?",
          "original_form": "처음 들어봤는데 무슨 내용인가요?",
          "speaker_id": "1",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100025476.1.7",
          "form": "유명 셰프가 아들하고 푸드트럭하는 내용안데",
          "original_form": "유명 셰프가 아들하고 푸드트럭하는 내용안데",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025476.1.8",
          "form": "힐링 영화같아요 ㅎㅎ",
          "original_form": "힐링 영화같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025476.1.9",
          "form": "함보세용",
          "original_form": "함보세용",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025476.1.10",
          "form": "넷플에이ㅛ드라구요",
          "original_form": "넷플에이ㅛ드라구요",
          "speaker_id": "2",
          "time": "20210901 13:29"
        },
        {
          "id": "MDRW2100025476.1.11",
          "form": "재밌겠네요",
          "original_form": "재밌겠네요",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025476.1.12",
          "form": "전 독립영화 좋아해요",
          "original_form": "전 독립영화 좋아해요",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025476.1.13",
          "form": "오 제일 좋아했던거 하나 추천해주세요",
          "original_form": "오 제일 좋아했던거 하나 추천해주세요",
          "speaker_id": "2",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025476.1.14",
          "form": "음 벌새랑 윤희에게요!",
          "original_form": "음 벌새랑 윤희에게요!",
          "speaker_id": "1",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100025476.1.15",
          "form": "윤희에게는 들었던거같은데",
          "original_form": "윤희에게는 들었던거같은데",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025476.1.16",
          "form": "한번봐야겠어용",
          "original_form": "한번봐야겠어용",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100025476.1.17",
          "form": "유튜브에 새소년이라는 밴드 노래에 영화 영상 합친 거 있는데 한번 보세요 영화 감성 그대로예요",
          "original_form": "유튜브에 새소년이라는 밴드 노래에 영화 영상 합친 거 있는데 한번 보세요 영화 감성 그대로예요",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025476.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025476.1.19",
          "form": "재밌을거같네여",
          "original_form": "재밌을거같네여",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025476.1.20",
          "form": "제일 최근에 본 영화 있으세요?",
          "original_form": "제일 최근에 본 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100025476.1.21",
          "form": "최근에 본게 아메리카 셰프요 ㅎㅎ",
          "original_form": "최근에 본게 아메리카 셰프요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100025476.1.22",
          "form": "그렇군요ㅋㅋ 그럼 자주 듣는 노래 있어요?",
          "original_form": "그렇군요ㅋㅋ 그럼 자주 듣는 노래 있어요?",
          "speaker_id": "1",
          "time": "20210902 07:58"
        },
        {
          "id": "MDRW2100025476.1.23",
          "form": "요즘은 악뮤 노래 자주들어요 ㅎㅎ",
          "original_form": "요즘은 악뮤 노래 자주들어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:14"
        },
        {
          "id": "MDRW2100025476.1.24",
          "form": "name1님은 자주듣는 노래있으세여?",
          "original_form": "&name1&님은 자주듣는 노래있으세여?",
          "speaker_id": "2",
          "time": "20210902 08:14"
        },
        {
          "id": "MDRW2100025476.1.25",
          "form": "전 요즘 레드벨벳 새 앨범 많이 들어요",
          "original_form": "전 요즘 레드벨벳 새 앨범 많이 들어요",
          "speaker_id": "1",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100025476.1.26",
          "form": "아~ 이번에 컴백했죠~??",
          "original_form": "아~ 이번에 컴백했죠~??",
          "speaker_id": "2",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100025476.1.27",
          "form": "무슨노래가 제일좋은가요?",
          "original_form": "무슨노래가 제일좋은가요?",
          "speaker_id": "2",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100025476.1.28",
          "form": "전 수록곡 중에 푸쉬 앤 풀린이 제일 좋더라고요",
          "original_form": "전 수록곡 중에 푸쉬 앤 풀린이 제일 좋더라고요",
          "speaker_id": "1",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100025476.1.29",
          "form": "수록곡까진 못들었는데 함들어봐야겠네영",
          "original_form": "수록곡까진 못들었는데 함들어봐야겠네영",
          "speaker_id": "2",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100025476.1.30",
          "form": "레드벨벳 좋아하세용??",
          "original_form": "레드벨벳 좋아하세용??",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100025476.1.31",
          "form": "네 노래 많이 들어요",
          "original_form": "네 노래 많이 들어요",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100025476.1.32",
          "form": "따로 좋아하는 가수도 있나요?",
          "original_form": "따로 좋아하는 가수도 있나요?",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100025476.1.33",
          "form": "새소년 좋아해요? 들어보셨나요?",
          "original_form": "새소년 좋아해요? 들어보셨나요?",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100025476.1.34",
          "form": "새소년 좋아해요 뒤에 물음표는 오타예요!!",
          "original_form": "새소년 좋아해요 뒤에 물음표는 오타예요!!",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100025476.1.35",
          "form": "새소년이용?",
          "original_form": "새소년이용?",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100025476.1.36",
          "form": "첨들어봐용",
          "original_form": "첨들어봐용",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100025476.1.37",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100025476.1.38",
          "form": "검색해보니 딱알거같아요 ㅋㅋㅋ",
          "original_form": "검색해보니 딱알거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100025476.1.39",
          "form": "여자분 유명하자나요",
          "original_form": "여자분 유명하자나요",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100025476.1.40",
          "form": "맞아요ㅋㅋ 원래 인디에 관심 없는 제 친구들도 황소윤은 조금 알더라고요",
          "original_form": "맞아요ㅋㅋ 원래 인디에 관심 없는 제 친구들도 황소윤은 조금 알더라고요",
          "speaker_id": "1",
          "time": "20210903 09:01"
        },
        {
          "id": "MDRW2100025476.1.41",
          "form": "맞아요 ㅋㅋㅋ 유트브에 유명해가지고 올라온걸봐가지고",
          "original_form": "맞아요 ㅋㅋㅋ 유트브에 유명해가지고 올라온걸봐가지고",
          "speaker_id": "2",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100025476.1.42",
          "form": "어떤 영상 보셨어요?",
          "original_form": "어떤 영상 보셨어요?",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100025476.1.43",
          "form": "넘예전에 봐서 기억은 잘안난다만..",
          "original_form": "넘예전에 봐서 기억은 잘안난다만..",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100025476.1.44",
          "form": "직캠같은건데...",
          "original_form": "직캠같은건데...",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100025476.1.45",
          "form": "그때 아마 청자켓입고 나온걸로 알고있는데",
          "original_form": "그때 아마 청자켓입고 나온걸로 알고있는데",
          "speaker_id": "2",
          "time": "20210903 09:55"
        },
        {
          "id": "MDRW2100025476.1.46",
          "form": "아 그 유명한 공연 영상 보신 거 같은데요?",
          "original_form": "아 그 유명한 공연 영상 보신 거 같은데요?",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100025476.1.47",
          "form": "그 영상 완전죽이죠...",
          "original_form": "그 영상 완전죽이죠...",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100025476.1.48",
          "form": "정말 멋있드라구요 ㅋㅋㅋㅋ",
          "original_form": "정말 멋있드라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100025476.1.49",
          "form": "여자들한테도 인기많은 이유를 알겠아요 ㅎㅎ",
          "original_form": "여자들한테도 인기많은 이유를 알겠아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:10"
        },
        {
          "id": "MDRW2100025476.1.50",
          "form": "그쵸ㅋㅋㅋ 여자들 휘어감는 특징은 다 가지고 있어요",
          "original_form": "그쵸ㅋㅋㅋ 여자들 휘어감는 특징은 다 가지고 있어요",
          "speaker_id": "1",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100025476.1.51",
          "form": "네 ㅋㅋ 진짜 멋있는거같아요",
          "original_form": "네 ㅋㅋ 진짜 멋있는거같아요",
          "speaker_id": "2",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100025476.1.52",
          "form": "제친구도 엄청 좋아하더라구욬ㅋㅋ",
          "original_form": "제친구도 엄청 좋아하더라구욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:58"
        }
      ]
