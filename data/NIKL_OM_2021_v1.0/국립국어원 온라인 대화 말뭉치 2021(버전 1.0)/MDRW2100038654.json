      "utterance": [
        {
          "id": "MDRW2100038654.1.1",
          "form": "왜 배탈이 나셨나요..",
          "original_form": "왜 배탈이 나셨나요..",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100038654.1.2",
          "form": "전 장이 약해서 여름에 한번씩 이래요..",
          "original_form": "전 장이 약해서 여름에 한번씩 이래요..",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100038654.1.3",
          "form": "튼튼하신가요? ㅎㅎ",
          "original_form": "튼튼하신가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100038654.1.4",
          "form": "글쿠나 ㅠㅠ 유산균 드시나요",
          "original_form": "글쿠나 ㅠㅠ 유산균 드시나요",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100038654.1.5",
          "form": "아뇨..저도 민감해요",
          "original_form": "아뇨..저도 민감해요",
          "speaker_id": "1",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100038654.1.6",
          "form": "네 유산균 먹는데.. 밀가루 이런거 좀 연달아 먹으면 탈나는거 같아요 ㅠㅠ",
          "original_form": "네 유산균 먹는데.. 밀가루 이런거 좀 연달아 먹으면 탈나는거 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100038654.1.7",
          "form": "ㅜㅜ  흑흑 밀가루 진짜그래요 ㅎ",
          "original_form": "ㅜㅜ  흑흑 밀가루 진짜그래요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100038654.1.8",
          "form": "하지만 못끊음",
          "original_form": "하지만 못끊음",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100038654.1.9",
          "form": "그래도 유산균 먹으면 좀 나아요",
          "original_form": "그래도 유산균 먹으면 좀 나아요",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100038654.1.10",
          "form": "맞아요 약간 달래는 느낌으로 먹어요 ㅋㅋㅋ",
          "original_form": "맞아요 약간 달래는 느낌으로 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100038654.1.11",
          "form": "매실차도 마시면 좀 위로돼요 ㅋㅋ",
          "original_form": "매실차도 마시면 좀 위로돼요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100038654.1.12",
          "form": "오 맞아요 소화되고",
          "original_form": "오 맞아요 소화되고",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100038654.1.13",
          "form": "다른 영양제드시나요",
          "original_form": "다른 영양제드시나요",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100038654.1.14",
          "form": "종합비타민이요!! 근데 루테인도 먹을까 생각중이에요 ㅋㅋ",
          "original_form": "종합비타민이요!! 근데 루테인도 먹을까 생각중이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100038654.1.15",
          "form": "눈이안조으신가여",
          "original_form": "눈이안조으신가여",
          "speaker_id": "1",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100038654.1.16",
          "form": "그렇진 않은데 주위보면 다들 챙겨먹더라고요 ㅋㅋ 눈건강이 중요하다고 ㅋㅋ",
          "original_form": "그렇진 않은데 주위보면 다들 챙겨먹더라고요 ㅋㅋ 눈건강이 중요하다고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100038654.1.17",
          "form": "뭐 챙겨드세요",
          "original_form": "뭐 챙겨드세요",
          "speaker_id": "2",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100038654.1.18",
          "form": "ㅋㅋㅋㅋ 저도 종비",
          "original_form": "ㅋㅋㅋㅋ 저도 종비",
          "speaker_id": "1",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100038654.1.19",
          "form": "유산균",
          "original_form": "유산균",
          "speaker_id": "1",
          "time": "20210902 18:27"
        },
        {
          "id": "MDRW2100038654.1.20",
          "form": "가끔 오메가...",
          "original_form": "가끔 오메가...",
          "speaker_id": "1",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100038654.1.21",
          "form": "맨날 빼먹어요",
          "original_form": "맨날 빼먹어요",
          "speaker_id": "1",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100038654.1.22",
          "form": "아 오메가 그거 약간 비리지 않나요 전 액상 먹었는데",
          "original_form": "아 오메가 그거 약간 비리지 않나요 전 액상 먹었는데",
          "speaker_id": "2",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100038654.1.23",
          "form": "전 갠찮더라구요 밥이랑 같이먹어그런가",
          "original_form": "전 갠찮더라구요 밥이랑 같이먹어그런가",
          "speaker_id": "1",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100038654.1.24",
          "form": "제품마다 좀 다른듯",
          "original_form": "제품마다 좀 다른듯",
          "speaker_id": "1",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100038654.1.25",
          "form": "심한것도잇고 ...",
          "original_form": "심한것도잇고 ...",
          "speaker_id": "1",
          "time": "20210902 18:28"
        },
        {
          "id": "MDRW2100038654.1.26",
          "form": "아하 하긴 밥이랑 같이 섭취하는 게 포인트 같아요..",
          "original_form": "아하 하긴 밥이랑 같이 섭취하는 게 포인트 같아요..",
          "speaker_id": "2",
          "time": "20210902 18:31"
        },
        {
