      "utterance": [
        {
          "id": "MDRW2100032772.1.1",
          "form": "저도 본진은 남돌이지만.... 여돌들 포카도 좀 구경하고...",
          "original_form": "저도 본진은 남돌이지만.... 여돌들 포카도 좀 구경하고...",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032772.1.2",
          "form": "신곡 나오면 와 개좋다 이러면서 듣고..",
          "original_form": "신곡 나오면 와 개좋다 이러면서 듣고..",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032772.1.3",
          "form": "뮤비보고 사랑에 빠지고 혼인신고서 쓰고(?)",
          "original_form": "뮤비보고 사랑에 빠지고 혼인신고서 쓰고(?)",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032772.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032772.1.5",
          "form": "아 너무 웃겨요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 너무 웃겨요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032772.1.6",
          "form": "해야죠 암요",
          "original_form": "해야죠 암요",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032772.1.7",
          "form": "혼인해야지...",
          "original_form": "혼인해야지...",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032772.1.8",
          "form": "저희 주파수가 너무 맞아가지궄ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저희 주파수가 너무 맞아가지궄ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032772.1.9",
          "form": "ㅋㅋㅋㅋㅋ아 너무 웃겨",
          "original_form": "ㅋㅋㅋㅋㅋ아 너무 웃겨",
          "speaker_id": "1",
          "time": "20210905 23:20"
        },
        {
          "id": "MDRW2100032772.1.10",
          "form": "진짜 케이팝은 못말려ㅠㅠ",
          "original_form": "진짜 케이팝은 못말려ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.11",
          "form": "저... 케이팝 진심녀라",
          "original_form": "저... 케이팝 진심녀라",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.12",
          "form": "근데 진짜 예쁜데 어떻게 안파요",
          "original_form": "근데 진짜 예쁜데 어떻게 안파요",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.13",
          "form": "문특 다시 그 컴눈명 해줬으면 좋겠어요",
          "original_form": "문특 다시 그 컴눈명 해줬으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.14",
          "form": "나와야 할 그룹이 수두룩",
          "original_form": "나와야 할 그룹이 수두룩",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.15",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.16",
          "form": "저도 쫌 진심인게 공방수니였어서ㅋㅋ",
          "original_form": "저도 쫌 진심인게 공방수니였어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.17",
          "form": "한번 덕후는 영원한 덕후",
          "original_form": "한번 덕후는 영원한 덕후",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.18",
          "form": "우와!!!!! 부러워욕 ㅠㅠㅠㅠ",
          "original_form": "우와!!!!! 부러워욕 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.19",
          "form": "근데 코로롱이라ㅠ 강제 안방수니예요",
          "original_form": "근데 코로롱이라ㅠ 강제 안방수니예요",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.20",
          "form": "그리고 팀이... 팀이 좋지 못한 곳을....",
          "original_form": "그리고 팀이... 팀이 좋지 못한 곳을....",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.21",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100032772.1.22",
          "form": "최애는 배우길 걸을거 같아염",
          "original_form": "최애는 배우길 걸을거 같아염",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032772.1.23",
          "form": "ㅠㅠㅠ....ㅜ.... 이해합니당....",
          "original_form": "ㅠㅠㅠ....ㅜ.... 이해합니당....",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032772.1.24",
          "form": "너라도... 살아남거라...",
          "original_form": "너라도... 살아남거라...",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032772.1.25",
          "form": "에휴 데습은진짜,,",
          "original_form": "에휴 데습은진짜,,",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032772.1.26",
          "form": "저 채경이 진짜루 응원하구있어요...ㅠㅠ",
          "original_form": "저 채경이 진짜루 응원하구있어요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032772.1.27",
          "form": "한명이라도... 살아야하지 않겠어여?",
          "original_form": "한명이라도... 살아야하지 않겠어여?",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032772.1.28",
          "form": "살사람은 살아야지...",
          "original_form": "살사람은 살아야지...",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032772.1.29",
          "form": "아 웃긴데.. 이게 웃어야할지..ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "아 웃긴데.. 이게 웃어야할지..ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032772.1.30",
          "form": "그쵸,,,,",
          "original_form": "그쵸,,,,",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032772.1.31",
          "form": "근데 진짜 엔시티 체제를 왜 수마니가 하려는지 알거 같은게",
          "original_form": "근데 진짜 엔시티 체제를 왜 수마니가 하려는지 알거 같은게",
          "speaker_id": "1",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100032772.1.32",
          "form": "데습 진짜,,,(험한말)",
          "original_form": "데습 진짜,,,(험한말)",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032772.1.33",
          "form": "살 애들은 살리고(?) 그게 넘 보여요",
          "original_form": "살 애들은 살리고(?) 그게 넘 보여요",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032772.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032772.1.35",
          "form": "라잌 슈퍼엠",
          "original_form": "라잌 슈퍼엠",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032772.1.36",
          "form": "솔직히 그렇쥬 ㅋㅋ큐ㅠㅠㅠㅠㅠㅠ",
          "original_form": "솔직히 그렇쥬 ㅋㅋ큐ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032772.1.37",
          "form": "증말 자본주의 끝판왕 같아여",
          "original_form": "증말 자본주의 끝판왕 같아여",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032772.1.38",
          "form": "마크 너무 고생하는게 보여서 눈물날 따름이에요,,",
          "original_form": "마크 너무 고생하는게 보여서 눈물날 따름이에요,,",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032772.1.39",
          "form": "근데 돌덕질은 쫌... 올팬기조로 가서 으쌰으쌰해야하는디...",
          "original_form": "근데 돌덕질은 쫌... 올팬기조로 가서 으쌰으쌰해야하는디...",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032772.1.40",
          "form": "할배 마크한테 보약 오백그릇 사다 바쳐라..",
          "original_form": "할배 마크한테 보약 오백그릇 사다 바쳐라..",
          "speaker_id": "2",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032772.1.41",
          "form": "그래서 이판이 좀.. 내부총질이 심하긴 해요 ㅠㅠ",
          "original_form": "그래서 이판이 좀.. 내부총질이 심하긴 해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100032772.1.42",
          "form": "마크는 진짜 엄청 바쁠거 같아요",
          "original_form": "마크는 진짜 엄청 바쁠거 같아요",
          "speaker_id": "1",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100032772.1.43",
          "form": "드림일이칠 다 파는 저한텐 좀 ..",
          "original_form": "드림일이칠 다 파는 저한텐 좀 ..",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100032772.1.44",
          "form": "스엠에 좋은 래퍼가 들어오면... 일어나는 일....",
          "original_form": "스엠에 좋은 래퍼가 들어오면... 일어나는 일....",
          "speaker_id": "1",
