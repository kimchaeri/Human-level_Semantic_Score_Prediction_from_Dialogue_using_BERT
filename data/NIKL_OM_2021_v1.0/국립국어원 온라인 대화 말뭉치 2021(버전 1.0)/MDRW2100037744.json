      "utterance": [
        {
          "id": "MDRW2100037744.1.1",
          "form": "씨씨티비에용",
          "original_form": "씨씨티비에용",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100037744.1.2",
          "form": "아 이름만 듣고 일반 카메라인줄 알았어요",
          "original_form": "아 이름만 듣고 일반 카메라인줄 알았어요",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100037744.1.3",
          "form": "집에 아이나 동물이 있으신가요?",
          "original_form": "집에 아이나 동물이 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100037744.1.4",
          "form": "펫캠이나 내니캠으로 많이 쓰시더라고요",
          "original_form": "펫캠이나 내니캠으로 많이 쓰시더라고요",
          "speaker_id": "1",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100037744.1.5",
          "form": "아 네네 강아지 펫캠이에요",
          "original_form": "아 네네 강아지 펫캠이에요",
          "speaker_id": "2",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100037744.1.6",
          "form": "잘산거같어요",
          "original_form": "잘산거같어요",
          "speaker_id": "2",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100037744.1.7",
          "form": "캠이 도움이 되나요?",
          "original_form": "캠이 도움이 되나요?",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100037744.1.8",
          "form": "강아지가 살짝 말썽을 피워두 캠으로 본들 바로 달려가진 못해서",
          "original_form": "강아지가 살짝 말썽을 피워두 캠으로 본들 바로 달려가진 못해서",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037744.1.9",
          "form": "도움이 별로 안될거라 생각했는데 그렇군요 하긴 좋으니 다들 사겠죠",
          "original_form": "도움이 별로 안될거라 생각했는데 그렇군요 하긴 좋으니 다들 사겠죠",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100037744.1.10",
          "form": "그냥 자는거 보는거만으로도 마음의 안정이와요 ㅋㅋ",
          "original_form": "그냥 자는거 보는거만으로도 마음의 안정이와요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037744.1.11",
          "form": "아 그럴수도 있겠네요 진짜 강아지가 가족이네요",
          "original_form": "아 그럴수도 있겠네요 진짜 강아지가 가족이네요",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100037744.1.12",
          "form": "부르면 놀라서 도망가서ㅠ 안불러요",
          "original_form": "부르면 놀라서 도망가서ㅠ 안불러요",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100037744.1.13",
          "form": "펫캠으로 보다가 부르면 개가 놀라긴 하겠어요 진자루,,",
          "original_form": "펫캠으로 보다가 부르면 개가 놀라긴 하겠어요 진자루,,",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037744.1.14",
          "form": "다이소에서 이제 캠도 판다니 완전 신기 ㅋㅋ",
          "original_form": "다이소에서 이제 캠도 판다니 완전 신기 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037744.1.15",
          "form": "오정말요 싱기",
          "original_form": "오정말요 싱기",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037744.1.16",
          "form": "다이소대단",
          "original_form": "다이소대단",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037744.1.17",
          "form": "다이소가 없음 어떻게 살았을지 ㅠ 이마트가서 다 살수도 없구요",
          "original_form": "다이소가 없음 어떻게 살았을지 ㅠ 이마트가서 다 살수도 없구요",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037744.1.18",
          "form": "맞아요 정말",
          "original_form": "맞아요 정말",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037744.1.19",
          "form": "비오네요 장화 사야하는데",
          "original_form": "비오네요 장화 사야하는데",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037744.1.20",
          "form": "장화는 어느 길이로 사실건가요?",
          "original_form": "장화는 어느 길이로 사실건가요?",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037744.1.21",
          "form": "발목? 종아리?/ 무릎?",
          "original_form": "발목? 종아리?/ 무릎?",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037744.1.22",
          "form": "미디움이용 종아리 ㅋㅋ",
          "original_form": "미디움이용 종아리 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037744.1.23",
          "form": "좋네요 전 무릎으로 샀다가,,,",
          "original_form": "좋네요 전 무릎으로 샀다가,,,",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037744.1.24",
          "form": "걸을때마다 장화가 무릎을 막 쳐서,,",
          "original_form": "걸을때마다 장화가 무릎을 막 쳐서,,",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037744.1.25",
          "form": "어떻게 해야할지 모르겟어요",
          "original_form": "어떻게 해야할지 모르겟어요",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037744.1.26",
          "form": "따흑",
          "original_form": "따흑",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037744.1.27",
          "form": "뭔지알것같아요",
          "original_form": "뭔지알것같아요",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037744.1.28",
          "form": "습기차면 또 안벗겨지죠?ㅠㅠ",
          "original_form": "습기차면 또 안벗겨지죠?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037744.1.29",
          "form": "벗겨지는 것두 문젠데,, 전 그것보다두 다리가 장화안에서 막 덕그럭거리며",
          "original_form": "벗겨지는 것두 문젠데,, 전 그것보다두 다리가 장화안에서 막 덕그럭거리며",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037744.1.30",
          "form": "걷거든요",
          "original_form": "걷거든요",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037744.1.31",
          "form": "다리보다 장화가 더 크잖아요 둘레가요",
          "original_form": "다리보다 장화가 더 크잖아요 둘레가요",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037744.1.32",
          "form": "그랬더니 2일 신었더니 물집이 잡혔어요 무릎에 1자로요",
          "original_form": "그랬더니 2일 신었더니 물집이 잡혔어요 무릎에 1자로요",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037744.1.33",
          "form": "ㅋㅋㅋㅋ황당 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ황당 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037744.1.34",
          "form": "헐 1자로요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐 1자로요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100037744.1.35",
          "form": "정말 황당",
          "original_form": "정말 황당",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100037744.1.36",
          "form": "헌터사셨어요?",
          "original_form": "헌터사셨어요?",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100037744.1.37",
          "form": "네,, 그래서 높이가 높았어요",
          "original_form": "네,, 그래서 높이가 높았어요",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100037744.1.38",
          "form": "저같은 ,, 키작은 사람은",
          "original_form": "저같은 ,, 키작은 사람은",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100037744.1.39",
          "form": "발목장화나 사야한다고점원이 알려줬어야되는데!",
          "original_form": "발목장화나 사야한다고점원이 알려줬어야되는데!",
          "speaker_id": "1",
          "time": "20210901 22:22"
        }
      ]
