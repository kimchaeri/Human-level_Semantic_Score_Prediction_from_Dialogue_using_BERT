        {
          "id": "MDRW2100013349.1.1",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013349.1.2",
          "form": "진짜 재밌을거같애",
          "original_form": "진짜 재밌을거같애",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013349.1.3",
          "form": "진하 name3도 같이~",
          "original_form": "진하 &name3&도 같이~",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013349.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.5",
          "form": "나중에 이모집 놀러가서",
          "original_form": "나중에 이모집 놀러가서",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.6",
          "form": "같이 눈사람 만들쟈",
          "original_form": "같이 눈사람 만들쟈",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.7",
          "form": "조아조아",
          "original_form": "조아조아",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.8",
          "form": "진짜 꿀잼이겠다",
          "original_form": "진짜 꿀잼이겠다",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.9",
          "form": "난 겨울이 좋다",
          "original_form": "난 겨울이 좋다",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.10",
          "form": "나두",
          "original_form": "나두",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.11",
          "form": "겨울엔 놀이도많고",
          "original_form": "겨울엔 놀이도많고",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.12",
          "form": "코코아도 있고",
          "original_form": "코코아도 있고",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.13",
          "form": "붕어빵이랑",
          "original_form": "붕어빵이랑",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.14",
          "form": "ㄱ타코야끼도팔아",
          "original_form": "ㄱ타코야끼도팔아",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.15",
          "form": "맞닥",
          "original_form": "맞닥",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.16",
          "form": "길거리 음식도 맛나쥬",
          "original_form": "길거리 음식도 맛나쥬",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.17",
          "form": "진짜좋아",
          "original_form": "진짜좋아",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.18",
          "form": "나도 겨울이 젤 좋아",
          "original_form": "나도 겨울이 젤 좋아",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.19",
          "form": "여름이 젤 싫어",
          "original_form": "여름이 젤 싫어",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.20",
          "form": "여음은",
          "original_form": "여음은",
          "speaker_id": "1",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013349.1.21",
          "form": "모기많고",
          "original_form": "모기많고",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.22",
          "form": "덥고 습하고 최악...",
          "original_form": "덥고 습하고 최악...",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.23",
          "form": "싫다시러",
          "original_form": "싫다시러",
          "speaker_id": "2",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.24",
          "form": "여름말고 겨울이 길었으면좋겟다리",
          "original_form": "여름말고 겨울이 길었으면좋겟다리",
          "speaker_id": "2",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.25",
          "form": "마쥬",
          "original_form": "마쥬",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.26",
          "form": "그냥 껴입고 다니면돼",
          "original_form": "그냥 껴입고 다니면돼",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.27",
          "form": "여름엔 더워도 걍 입고살아야해",
          "original_form": "여름엔 더워도 걍 입고살아야해",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.28",
          "form": "나도그게싫어",
          "original_form": "나도그게싫어",
          "speaker_id": "2",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.29",
          "form": "겨울엔 추우면 더입으면돼",
          "original_form": "겨울엔 추우면 더입으면돼",
          "speaker_id": "2",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.30",
          "form": "그니까!!!",
          "original_form": "그니까!!!",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.31",
          "form": "아 겨울돼서 호빵먹고싶다",
          "original_form": "아 겨울돼서 호빵먹고싶다",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.32",
          "form": "마잣",
          "original_form": "마잣",
          "speaker_id": "2",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013349.1.33",
          "form": "얼른 호빵묵자",
          "original_form": "얼른 호빵묵자",
