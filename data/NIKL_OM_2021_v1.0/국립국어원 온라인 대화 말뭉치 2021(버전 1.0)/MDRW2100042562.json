      "utterance": [
        {
          "id": "MDRW2100042562.1.1",
          "form": "좋아하는 프로 있으세요?",
          "original_form": "좋아하는 프로 있으세요?",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042562.1.2",
          "form": "넷플릭스나 티빙 구독하시나요?",
          "original_form": "넷플릭스나 티빙 구독하시나요?",
          "speaker_id": "2",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042562.1.3",
          "form": "전 꼬꼬무 재밌게 봤어요",
          "original_form": "전 꼬꼬무 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042562.1.4",
          "form": "꼬꼬무 이야기는 많이 들었어요",
          "original_form": "꼬꼬무 이야기는 많이 들었어요",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042562.1.5",
          "form": "전 당신이 혹하는사이???",
          "original_form": "전 당신이 혹하는사이???",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042562.1.6",
          "form": "이거 몇번 봤어요",
          "original_form": "이거 몇번 봤어요",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042562.1.7",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042562.1.8",
          "form": "전 그냥 채널 돌리다가 방영 하는 것만 봤어요",
          "original_form": "전 그냥 채널 돌리다가 방영 하는 것만 봤어요",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042562.1.9",
          "form": "어떤 프로인가요?",
          "original_form": "어떤 프로인가요?",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042562.1.10",
          "form": "음모론 파헤치는거 같더라고요",
          "original_form": "음모론 파헤치는거 같더라고요",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042562.1.11",
          "form": "꼬꼬무는 무서운 실화죠?",
          "original_form": "꼬꼬무는 무서운 실화죠?",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042562.1.12",
          "form": "네네 꼬꼬무는 약간",
          "original_form": "네네 꼬꼬무는 약간",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042562.1.13",
          "form": "유영철 옆집이야기???",
          "original_form": "유영철 옆집이야기???",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042562.1.14",
          "form": "그것이 알고싶다를 가볍게 풀어주는 느낌이에요",
          "original_form": "그것이 알고싶다를 가볍게 풀어주는 느낌이에요",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042562.1.15",
          "form": "그건 인터넷으로 영상 올라온거 봤는데",
          "original_form": "그건 인터넷으로 영상 올라온거 봤는데",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042562.1.16",
          "form": "아닌가요??",
          "original_form": "아닌가요??",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042562.1.17",
          "form": "그것도 있는진 모르겠어요",
          "original_form": "그것도 있는진 모르겠어요",
          "speaker_id": "2",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042562.1.18",
          "form": "연쇄살인범 이야기도 하고",
          "original_form": "연쇄살인범 이야기도 하고",
          "speaker_id": "2",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042562.1.19",
          "form": "정치사건들 얘기도 하고",
          "original_form": "정치사건들 얘기도 하고",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042562.1.20",
          "form": "삼풍백화점같은 참사? 얘기도 해요",
          "original_form": "삼풍백화점같은 참사? 얘기도 해요",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042562.1.21",
          "form": "그것도 캡쳐본거같아요",
          "original_form": "그것도 캡쳐본거같아요",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042562.1.22",
          "form": "한참 그때가 몇주기라 여러방송에서 다루더라고요",
          "original_form": "한참 그때가 몇주기라 여러방송에서 다루더라고요",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042562.1.23",
          "form": "유투브는 따로 안보시나요?",
          "original_form": "유투브는 따로 안보시나요?",
          "speaker_id": "1",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042562.1.24",
          "form": "유튜브는 보통 백그라운드로 음악 듣는데 써요",
          "original_form": "유튜브는 보통 백그라운드로 음악 듣는데 써요",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042562.1.25",
          "form": "보통 어떤 음악들으세요?",
          "original_form": "보통 어떤 음악들으세요?",
          "speaker_id": "1",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042562.1.26",
          "form": "요즘 방송에서 옛날 음악 많이 틀어줘서",
          "original_form": "요즘 방송에서 옛날 음악 많이 틀어줘서",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042562.1.27",
          "form": "00년대 음악들 다시 듣고있어요 ㅋㅋㅋ",
          "original_form": "00년대 음악들 다시 듣고있어요 ㅋㅋㅋ",
