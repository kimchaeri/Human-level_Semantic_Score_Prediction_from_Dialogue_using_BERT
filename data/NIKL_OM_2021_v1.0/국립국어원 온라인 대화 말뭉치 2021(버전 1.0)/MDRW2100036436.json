      "utterance": [
        {
          "id": "MDRW2100036436.1.1",
          "form": "타이머 콘센트...?",
          "original_form": "타이머 콘센트...?",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036436.1.2",
          "form": "이시간대에 그 말을 했던 거 같아",
          "original_form": "이시간대에 그 말을 했던 거 같아",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100036436.1.3",
          "form": "하루전일도 가물가물해",
          "original_form": "하루전일도 가물가물해",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100036436.1.4",
          "form": "나는 영양제를 쇼핑해야겠어",
          "original_form": "나는 영양제를 쇼핑해야겠어",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100036436.1.5",
          "form": "아니.. 너 혹시 타이머콘셈트 주식이라도 산거야?",
          "original_form": "아니.. 너 혹시 타이머콘셈트 주식이라도 산거야?",
          "speaker_id": "1",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100036436.1.6",
          "form": "나는 라벤더 오일이 필요했어",
          "original_form": "나는 라벤더 오일이 필요했어",
          "speaker_id": "1",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100036436.1.7",
          "form": "라벤더 오일을 어디에 쓰는지 맞혀봐!",
          "original_form": "라벤더 오일을 어디에 쓰는지 맞혀봐!",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.8",
          "form": "음 그건 섬유유연제 대신이야?",
          "original_form": "음 그건 섬유유연제 대신이야?",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.9",
          "form": "건조기에 넣을거야",
          "original_form": "건조기에 넣을거야",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.10",
          "form": "음 향을 내는 용도지",
          "original_form": "음 향을 내는 용도지",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.11",
          "form": "옷이 부드러워지진 않울 거 같아",
          "original_form": "옷이 부드러워지진 않울 거 같아",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.12",
          "form": "아하아하 너 좋은 향이 나겠구나",
          "original_form": "아하아하 너 좋은 향이 나겠구나",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.13",
          "form": "하지만 빨래를 갤 때, 그리고 옷을 입을 때 자연스럽고 향기롭단다",
          "original_form": "하지만 빨래를 갤 때, 그리고 옷을 입을 때 자연스럽고 향기롭단다",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.14",
          "form": "응. 추천해!",
          "original_form": "응. 추천해!",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.15",
          "form": "근데ㅜ가격이 비싸",
          "original_form": "근데ㅜ가격이 비싸",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.16",
          "form": "아로마 오일 나도 추천해줘!",
          "original_form": "아로마 오일 나도 추천해줘!",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.17",
          "form": "가격을 조심해",
          "original_form": "가격을 조심해",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.18",
          "form": "좋아!",
          "original_form": "좋아!",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.19",
          "form": "아니면 나눔해주는건 어때?",
          "original_form": "아니면 나눔해주는건 어때?",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100036436.1.20",
          "form": "우리 조만간 볼 때 다시 얘기하자",
          "original_form": "우리 조만간 볼 때 다시 얘기하자",
          "speaker_id": "1",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036436.1.21",
          "form": "그것에 관하여 토론을 해야겠어",
          "original_form": "그것에 관하여 토론을 해야겠어",
          "speaker_id": "1",
          "time": "20210830 23:08"
        },
        {
          "id": "MDRW2100036436.1.22",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210830 23:08"
        }
      ]
