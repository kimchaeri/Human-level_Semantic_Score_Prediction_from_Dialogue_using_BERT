      "utterance": [
        {
          "id": "MDRW2100010222.1.1",
          "form": "디저트39 오픈했더라",
          "original_form": "디저트39 오픈했더라",
          "speaker_id": "2",
          "time": "20210823 15:50"
        },
        {
          "id": "MDRW2100010222.1.2",
          "form": "나는 오늘 한정식집에 갔어.",
          "original_form": "나는 오늘 한정식집에 갔어.",
          "speaker_id": "2",
          "time": "20210823 15:50"
        },
        {
          "id": "MDRW2100010222.1.3",
          "form": "꽃등심을 먹고싶었는데 부모님이 마음대로 갈비찜으로 시키셨어.",
          "original_form": "꽃등심을 먹고싶었는데 부모님이 마음대로 갈비찜으로 시키셨어.",
          "speaker_id": "2",
          "time": "20210823 15:50"
        },
        {
          "id": "MDRW2100010222.1.4",
          "form": "부모님이 돈을 내니 어쩔 수 없는거겠지.",
          "original_form": "부모님이 돈을 내니 어쩔 수 없는거겠지.",
          "speaker_id": "2",
          "time": "20210823 15:50"
        },
        {
          "id": "MDRW2100010222.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ니가 돈내는게 아니니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ니가 돈내는게 아니니까",
          "speaker_id": "1",
          "time": "20210823 15:50"
        },
        {
          "id": "MDRW2100010222.1.6",
          "form": "어쩔수없지",
          "original_form": "어쩔수없지",
          "speaker_id": "1",
          "time": "20210823 15:50"
        },
        {
          "id": "MDRW2100010222.1.7",
          "form": "디저트39는\r\n어디에",
          "original_form": "디저트39는\r\n어디에",
          "speaker_id": "1",
          "time": "20210823 15:50"
        },
        {
          "id": "MDRW2100010222.1.8",
          "form": "대구 칠곡 3지구에 있단다.",
          "original_form": "대구 칠곡 3지구에 있단다.",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010222.1.9",
          "form": "근데 거기는 음료양이 너무 많아.",
          "original_form": "근데 거기는 음료양이 너무 많아.",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010222.1.10",
          "form": "나는 차라리 음료 양이 적당하고 가격이 더 쌌으면 좋겠어.",
          "original_form": "나는 차라리 음료 양이 적당하고 가격이 더 쌌으면 좋겠어.",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010222.1.11",
          "form": "그 커뮤니티에서 유명해졌지",
          "original_form": "그 커뮤니티에서 유명해졌지",
          "speaker_id": "1",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010222.1.12",
          "form": "그리고 요즘 카페들 단 음료들은 너무 달지 않니?",
          "original_form": "그리고 요즘 카페들 단 음료들은 너무 달지 않니?",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010222.1.13",
          "form": "그리고 디저트39 디저트는 비싸고 작아.",
          "original_form": "그리고 디저트39 디저트는 비싸고 작아.",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010222.1.14",
          "form": "맞아 나는 그래서 아이스아메리카노만 마셔",
          "original_form": "맞아 나는 그래서 아이스아메리카노만 마셔",
          "speaker_id": "1",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010222.1.15",
          "form": "크리스마스파티 케이크로 샀다가 경악을 했지.",
          "original_form": "크리스마스파티 케이크로 샀다가 경악을 했지.",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010222.1.16",
          "form": "나도 구래서 아아만 마셔",
          "original_form": "나도 구래서 아아만 마셔",
          "speaker_id": "2",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010222.1.17",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100010222.1.18",
          "form": "부자구아 거기서 디저트를 사다니",
          "original_form": "부자구아 거기서 디저트를 사다니",
          "speaker_id": "1",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100010222.1.19",
          "form": "근데 요즘 다시 잠이 잘 안와서 오늘 끊었다가 결국 오늘 다시 마셨어",
          "original_form": "근데 요즘 다시 잠이 잘 안와서 오늘 끊었다가 결국 오늘 다시 마셨어",
          "speaker_id": "2",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100010222.1.20",
          "form": "원래 부자친구들을 만나면 지출이 커지는 법이지.",
          "original_form": "원래 부자친구들을 만나면 지출이 커지는 법이지.",
          "speaker_id": "2",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100010222.1.21",
          "form": "카페인에 예민한편이니?",
          "original_form": "카페인에 예민한편이니?",
          "speaker_id": "1",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100010222.1.22",
          "form": "그렇지 않니?",
          "original_form": "그렇지 않니?",
          "speaker_id": "2",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100010222.1.23",
          "form": "맞아 나는 돈이 없늗데....",
          "original_form": "맞아 나는 돈이 없늗데....",
          "speaker_id": "1",
          "time": "20210823 15:52"
        },
        {
          "id": "MDRW2100010222.1.24",
          "form": "예민하지는 않지만 잠을 잘 못자는 거 같아.",
          "original_form": "예민하지는 않지만 잠을 잘 못자는 거 같아.",
          "speaker_id": "2",
          "time": "20210823 15:52"
        }
      ]
    }
