      "utterance": [
        {
          "id": "MDRW2100021389.1.1",
          "form": "보고서 써야하는데",
          "original_form": "보고서 써야하는데",
          "speaker_id": "2",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100021389.1.2",
          "form": "wow",
          "original_form": "wow",
          "speaker_id": "1",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100021389.1.3",
          "form": "귀찮다 귀찮아",
          "original_form": "귀찮다 귀찮아",
          "speaker_id": "2",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100021389.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100021389.1.5",
          "form": "그렇게 열심히하는 동아리",
          "original_form": "그렇게 열심히하는 동아리",
          "speaker_id": "1",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100021389.1.6",
          "form": "흔치않은데",
          "original_form": "흔치않은데",
          "speaker_id": "1",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100021389.1.7",
          "form": "진짜대단하다",
          "original_form": "진짜대단하다",
          "speaker_id": "1",
          "time": "20210828 18:29"
        },
        {
          "id": "MDRW2100021389.1.8",
          "form": "아 팀원들땜에",
          "original_form": "아 팀원들땜에",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.9",
          "form": "꾸역꾸역 하는거지",
          "original_form": "꾸역꾸역 하는거지",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.10",
          "form": "난 2학기때",
          "original_form": "난 2학기때",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.11",
          "form": "혼자하는 활동은 하나도 안함",
          "original_form": "혼자하는 활동은 하나도 안함",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.12",
          "form": "꼭 여자풋살동아리 들어가고싶었는데",
          "original_form": "꼭 여자풋살동아리 들어가고싶었는데",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.13",
          "form": "방학때 에타를 아예안봤더니",
          "original_form": "방학때 에타를 아예안봤더니",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.14",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.15",
          "form": "모집기간 놓쳤음ㅋㅋㅋㅋㅋㅋ",
          "original_form": "모집기간 놓쳤음ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.16",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.17",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "1",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.18",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.19",
          "form": "모집기간 끝남?",
          "original_form": "모집기간 끝남?",
          "speaker_id": "2",
          "time": "20210828 18:30"
        },
        {
          "id": "MDRW2100021389.1.20",
          "form": "위에 채팅안갔어?",
          "original_form": "위에 채팅안갔어?",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021389.1.21",
          "form": "ㅇㅇ방핫때 에타를 안봤더니",
          "original_form": "ㅇㅇ방핫때 에타를 안봤더니",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021389.1.22",
          "form": "아 늦게봤네",
          "original_form": "아 늦게봤네",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021389.1.23",
          "form": "모집기간끝난것도 모르고있었음ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "모집기간끝난것도 모르고있었음ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021389.1.24",
          "form": "이게 카톡만큼",
          "original_form": "이게 카톡만큼",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021389.1.25",
          "form": "바로 안가는거 같음",
          "original_form": "바로 안가는거 같음",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021389.1.26",
          "form": "맞아 그렇드라",
          "original_form": "맞아 그렇드라",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021389.1.27",
          "form": "일단 2학기때는",
          "original_form": "일단 2학기때는",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100021389.1.28",
          "form": "2학기땬 걍성적만 챙겨야지...ㅎ",
          "original_form": "2학기땬 걍성적만 챙겨야지...ㅎ",
