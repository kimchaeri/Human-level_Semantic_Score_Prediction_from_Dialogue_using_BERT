      "utterance": [
        {
          "id": "MDRW2100014585.1.1",
          "form": "일일드라마인가요!",
          "original_form": "일일드라마인가요!",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014585.1.2",
          "form": "네네 부녀지간이야기요",
          "original_form": "네네 부녀지간이야기요",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014585.1.3",
          "form": "둘이싸우는 내용이요",
          "original_form": "둘이싸우는 내용이요",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014585.1.4",
          "form": "일일드라마도 계속 비슷한 내용이 돌고 도네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "일일드라마도 계속 비슷한 내용이 돌고 도네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014585.1.5",
          "form": "엄마가 아주 지딸 죽이려고 ㅋㅋ",
          "original_form": "엄마가 아주 지딸 죽이려고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014585.1.6",
          "form": "친부모 맞아요?",
          "original_form": "친부모 맞아요?",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014585.1.7",
          "form": "네 친딸이죠",
          "original_form": "네 친딸이죠",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014585.1.8",
          "form": "이니..그런데 서로 왜....?",
          "original_form": "이니..그런데 서로 왜....?",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014585.1.9",
          "form": "딸버리고 갓죠 ㅋㅋ",
          "original_form": "딸버리고 갓죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014585.1.10",
          "form": "요즘 절정으로 싸워요 ㅋㅋ",
          "original_form": "요즘 절정으로 싸워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014585.1.11",
          "form": "아ㅋㅋㅋㅋㅋㅋ또 사연이 있구나",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ또 사연이 있구나",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014585.1.12",
          "form": "그래서 싸우는구나",
          "original_form": "그래서 싸우는구나",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014585.1.13",
          "form": "네  사연이 많아야 드라마라 재미죠",
          "original_form": "네  사연이 많아야 드라마라 재미죠",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014585.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014585.1.15",
          "form": "kBs",
          "original_form": "kBs",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014585.1.16",
          "form": "일일드라마는 또 그래야 인기를 끄니까!",
          "original_form": "일일드라마는 또 그래야 인기를 끄니까!",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014585.1.17",
