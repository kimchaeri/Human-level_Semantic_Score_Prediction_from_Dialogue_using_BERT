      "utterance": [
        {
          "id": "MDRW2100037832.1.1",
          "form": "아뇨 저 왓챠봐요",
          "original_form": "아뇨 저 왓챠봐요",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037832.1.2",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100037832.1.3",
          "form": "저는 넷플릭스랑 티빙봐요 ㅋㅋㅋ",
          "original_form": "저는 넷플릭스랑 티빙봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100037832.1.4",
          "form": "티빙으로 뭐보세요?",
          "original_form": "티빙으로 뭐보세요?",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100037832.1.5",
          "form": "저도 티빙 보거등요!",
          "original_form": "저도 티빙 보거등요!",
          "speaker_id": "2",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100037832.1.6",
          "form": "저는 요즘 환승연애재밌게보고잇어요 ㅋㅋㅋㅋㅋ보시나요?",
          "original_form": "저는 요즘 환승연애재밌게보고잇어요 ㅋㅋㅋㅋㅋ보시나요?",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037832.1.7",
          "form": "아뇨 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037832.1.8",
          "form": "근데 볼까 고민중이에요",
          "original_form": "근데 볼까 고민중이에요",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037832.1.9",
          "form": "재밌나요",
          "original_form": "재밌나요",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037832.1.10",
          "form": "네네 첨에 막장인줄알아서 욕햇는데 ㅋㅋㅋㅋ전혀 그렇지않더라구요",
          "original_form": "네네 첨에 막장인줄알아서 욕햇는데 ㅋㅋㅋㅋ전혀 그렇지않더라구요",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037832.1.11",
          "form": "추천드려요 재밋어요 ㅋㅋㅋ",
          "original_form": "추천드려요 재밋어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037832.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 봐야겠네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 봐야겠네요",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100037832.1.13",
          "form": "혹시 나는 솔로",
          "original_form": "혹시 나는 솔로",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100037832.1.14",
          "form": "이것도 보시낭6",
          "original_form": "이것도 보시낭6",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100037832.1.15",
          "form": "오 아니요 그건뭔가요??ㅋㅋㅋㅋㅋ",
          "original_form": "오 아니요 그건뭔가요??ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100037832.1.16",
          "form": "이것도 일반인 나와서",
          "original_form": "이것도 일반인 나와서",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100037832.1.17",
          "form": "커플되는 그런거에요ㅋㅋㅌ",
          "original_form": "커플되는 그런거에요ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100037832.1.18",
          "form": "오오 재밌나요 ?? 그런거 즐겨보는편이라 궁금하네요 ㅌㅋㅋㅋ",
          "original_form": "오오 재밌나요 ?? 그런거 즐겨보는편이라 궁금하네요 ㅌㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100037832.1.19",
          "form": "ㅎ 네ㅋㅋ 한번 봐보세요",
          "original_form": "ㅎ 네ㅋㅋ 한번 봐보세요",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100037832.1.20",
          "form": "시즌1이 재밌어요",
          "original_form": "시즌1이 재밌어요",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100037832.1.21",
          "form": "아 시즌이있나봐요 ㅋㅋㅋㅋ",
          "original_form": "아 시즌이있나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100037832.1.22",
          "form": "알겟어요 오늘 한번 찾아볼게옄ㅋㅋㅋ",
          "original_form": "알겟어요 오늘 한번 찾아볼게옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100037832.1.23",
