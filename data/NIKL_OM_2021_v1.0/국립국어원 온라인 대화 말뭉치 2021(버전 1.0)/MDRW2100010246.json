        {
          "id": "MDRW2100010246.1.13",
          "form": "저는 있어요!",
          "original_form": "저는 있어요!",
          "speaker_id": "1",
          "time": "20210824 11:49"
        },
        {
          "id": "MDRW2100010246.1.14",
          "form": "음... 한다고 해도 아이를 낳을 것 같진 않아요",
          "original_form": "음... 한다고 해도 아이를 낳을 것 같진 않아요",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
