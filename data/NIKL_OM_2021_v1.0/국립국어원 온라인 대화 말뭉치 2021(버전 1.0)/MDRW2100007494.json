      "utterance": [
        {
          "id": "MDRW2100007494.1.1",
          "form": "나도 더쿠 들어가고 싶어",
          "original_form": "나도 더쿠 들어가고 싶어",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007494.1.2",
          "form": "더쿠 가입 언제열리지?",
          "original_form": "더쿠 가입 언제열리지?",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007494.1.3",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007494.1.4",
          "form": "근데 가입 열려서 너 가입하면 나 거기다 글 못쓸듯",
          "original_form": "근데 가입 열려서 너 가입하면 나 거기다 글 못쓸듯",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007494.1.5",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007494.1.6",
          "form": "왠지 너같고 나같잖아",
          "original_form": "왠지 너같고 나같잖아",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007494.1.7",
          "form": "부끄러",
          "original_form": "부끄러",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007494.1.8",
          "form": "내가 볼까봥?",
          "original_form": "내가 볼까봥?",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007494.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007494.1.10",
          "form": "엉ㅎㅎ",
          "original_form": "엉ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007494.1.11",
          "form": "그렇지 않아",
          "original_form": "그렇지 않아",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007494.1.12",
          "form": "모른 척 해줄게",
          "original_form": "모른 척 해줄게",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007494.1.13",
          "form": "근데 재밌긴해",
          "original_form": "근데 재밌긴해",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007494.1.14",
          "form": "다른데도 더쿠같이 가입하나?",
          "original_form": "다른데도 더쿠같이 가입하나?",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007494.1.15",
          "form": "뭐 물어보면 다들 대답도 해주고 친철함",
          "original_form": "뭐 물어보면 다들 대답도 해주고 친철함",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007494.1.16",
          "form": "쥼 무섭긴한데 어.. 그래",
          "original_form": "쥼 무섭긴한데 어.. 그래",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007494.1.17",
          "form": "아닐껄",
          "original_form": "아닐껄",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007494.1.18",
          "form": "그럼 아무나 막 들어가는 거야?!?!",
          "original_form": "그럼 아무나 막 들어가는 거야?!?!",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007494.1.19",
          "form": "더쿠가 특이하게 있는거",
          "original_form": "더쿠가 특이하게 있는거",
          "speaker_id": "1",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007494.1.20",
          "form": "근데 다른 커뮤는 안해봐서",
          "original_form": "근데 다른 커뮤는 안해봐서",
          "speaker_id": "1",
