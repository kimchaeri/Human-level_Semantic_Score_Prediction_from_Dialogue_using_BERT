      "utterance": [
        {
          "id": "MDRW2100031184.1.1",
          "form": "비비큐 조아하세여?",
          "original_form": "비비큐 조아하세여?",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100031184.1.2",
          "form": "비비큐도 좋아해요~ 황올 최고",
          "original_form": "비비큐도 좋아해요~ 황올 최고",
          "speaker_id": "2",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031184.1.3",
          "form": "그죠 황올 넘 맛잇",
          "original_form": "그죠 황올 넘 맛잇",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031184.1.4",
          "form": "다른 브랜드도 추천할거 잇나요",
          "original_form": "다른 브랜드도 추천할거 잇나요",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031184.1.5",
          "form": "후참요!!! 후라이드참잘하는집 여기껏도 맛있어욤. 먹짱이님은 저녁 모드셔욤",
          "original_form": "후참요!!! 후라이드참잘하는집 여기껏도 맛있어욤. 먹짱이님은 저녁 모드셔욤",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100031184.1.6",
          "form": "오 ~",
          "original_form": "오 ~",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100031184.1.7",
          "form": "몰라요 집 가바야해요",
          "original_form": "몰라요 집 가바야해요",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100031184.1.8",
          "form": "ㅠ... 오늘도 랩실이셨어요?",
          "original_form": "ㅠ... 오늘도 랩실이셨어요?",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100031184.1.9",
          "form": "ㅋㅋㅋㅋㅋ 놀고 본가갑니다",
          "original_form": "ㅋㅋㅋㅋㅋ 놀고 본가갑니다",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100031184.1.10",
          "form": "아하!!!!! 집 가서 맛난거 드세요!!",
          "original_form": "아하!!!!! 집 가서 맛난거 드세요!!",
          "speaker_id": "2",
          "time": "20210904 18:06"
        },
        {
          "id": "MDRW2100031184.1.11",
          "form": "넹 커피 드시나요",
          "original_form": "넹 커피 드시나요",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031184.1.12",
          "form": "커피도 좋아해요~! 아아... 한정 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "커피도 좋아해요~! 아아... 한정 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100031184.1.13",
          "form": "아아만요?",
          "original_form": "아아만요?",
          "speaker_id": "1",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100031184.1.14",
          "form": "아이스 바닐라라떼도 좋아해욤",
          "original_form": "아이스 바닐라라떼도 좋아해욤",
          "speaker_id": "2",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100031184.1.15",
          "form": "스벅꺼중에선 자허블 좋아하구!",
          "original_form": "스벅꺼중에선 자허블 좋아하구!",
          "speaker_id": "2",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100031184.1.16",
          "form": "앗 아바라 마싯죠",
          "original_form": "앗 아바라 마싯죠",
          "speaker_id": "1",
          "time": "20210904 18:31"
        },
        {
          "id": "MDRW2100031184.1.17",
          "form": "자허블도 굳",
          "original_form": "자허블도 굳",
          "speaker_id": "1",
          "time": "20210904 18:31"
        },
        {
          "id": "MDRW2100031184.1.18",
          "form": "입맛 맞네요",
          "original_form": "입맛 맞네요",
          "speaker_id": "1",
          "time": "20210904 18:31"
        },
        {
          "id": "MDRW2100031184.1.19",
          "form": "ㅋ ㅋㅋㅋㅋㅋㅋ  진짜루요. 헉 저 아까 치킨시키려다가 그냥 족발먹고 싶어서 족발 시켰어요",
          "original_form": "ㅋ ㅋㅋㅋㅋㅋㅋ  진짜루요. 헉 저 아까 치킨시키려다가 그냥 족발먹고 싶어서 족발 시켰어요",
          "speaker_id": "2",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031184.1.20",
          "form": "막걸리 사와야겠서요...",
          "original_form": "막걸리 사와야겠서요...",
          "speaker_id": "2",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031184.1.21",
          "form": "아 ㅋㅋㅋㅋ 족발좋죠",
          "original_form": "아 ㅋㅋㅋㅋ 족발좋죠",
          "speaker_id": "1",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100031184.1.22",
          "form": "요기요에서 50% 할인쿠폰 받은게 있어서 냉큼 써버렸지 뭐여요",
          "original_form": "요기요에서 50% 할인쿠폰 받은게 있어서 냉큼 써버렸지 뭐여요",
          "speaker_id": "2",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100031184.1.23",
          "form": "전 삼겹살",
          "original_form": "전 삼겹살",
          "speaker_id": "1",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100031184.1.24",
          "form": "헉 어케받앗어요",
          "original_form": "헉 어케받앗어요",
          "speaker_id": "1",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100031184.1.25",
          "form": "그그 요기요 익스프레스 라구 있는데.. 그거 예상시간보다 10분 더 늦으면 할인쿠폰을 자동으로 줘요",
          "original_form": "그그 요기요 익스프레스 라구 있는데.. 그거 예상시간보다 10분 더 늦으면 할인쿠폰을 자동으로 줘요",
          "speaker_id": "2",
          "time": "20210904 18:34"
        },
        {
          "id": "MDRW2100031184.1.26",
          "form": "오 ㅋㅋㅋ 늦으면 좋겟다",
          "original_form": "오 ㅋㅋㅋ 늦으면 좋겟다",
          "speaker_id": "1",
          "time": "20210904 19:06"
        },
        {
          "id": "MDRW2100031184.1.27",
          "form": "꿀이네요",
          "original_form": "꿀이네요",
          "speaker_id": "1",
          "time": "20210904 19:06"
        },
        {
          "id": "MDRW2100031184.1.28",
          "form": "맞아용. 그래도 배달기사분들한테 페널티는 없어가지구",
          "original_form": "맞아용. 그래도 배달기사분들한테 페널티는 없어가지구",
          "speaker_id": "2",
          "time": "20210904 19:10"
        },
        {
          "id": "MDRW2100031184.1.29",
          "form": "그나마 다행!",
          "original_form": "그나마 다행!",
          "speaker_id": "2",
          "time": "20210904 19:10"
        },
        {
          "id": "MDRW2100031184.1.30",
          "form": "오호 족발이면 쟁반국수도?",
          "original_form": "오호 족발이면 쟁반국수도?",
          "speaker_id": "1",
          "time": "20210904 19:24"
        },
        {
          "id": "MDRW2100031184.1.31",
          "form": "헉 어뜨케 아셨지 쟁반국수도 하나 했지요~!!!",
          "original_form": "헉 어뜨케 아셨지 쟁반국수도 하나 했지요~!!!",
          "speaker_id": "2",
          "time": "20210904 19:25"
        },
        {
          "id": "MDRW2100031184.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ 아 국룰이죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아 국룰이죠",
          "speaker_id": "1",
          "time": "20210904 19:25"
        },
        {
          "id": "MDRW2100031184.1.33",
          "form": "삼겹살 맛있네요",
          "original_form": "삼겹살 맛있네요",
          "speaker_id": "1",
          "time": "20210904 19:25"
        }
      ]
    }
