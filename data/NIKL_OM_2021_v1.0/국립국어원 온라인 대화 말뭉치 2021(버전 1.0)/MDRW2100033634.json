      "utterance": [
        {
          "id": "MDRW2100033634.1.1",
          "form": "청소 매일 하시나용",
          "original_form": "청소 매일 하시나용",
          "speaker_id": "1",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100033634.1.2",
          "form": "저!! 더럽게살아요",
          "original_form": "저!! 더럽게살아요",
          "speaker_id": "2",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100033634.1.3",
          "form": "설거지만 성실하게 할 수 있어요",
          "original_form": "설거지만 성실하게 할 수 있어요",
          "speaker_id": "2",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100033634.1.4",
          "form": "청소 자주하세요?",
          "original_form": "청소 자주하세요?",
          "speaker_id": "2",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100033634.1.5",
          "form": "ㅋㅋㅋ ㅋㅋㅋㅋㅋㅋㅋㅋ 저도요",
          "original_form": "ㅋㅋㅋ ㅋㅋㅋㅋㅋㅋㅋㅋ 저도요",
          "speaker_id": "1",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100033634.1.6",
          "form": "청소 싫어래요",
          "original_form": "청소 싫어래요",
          "speaker_id": "1",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100033634.1.7",
          "form": "설거지는 안하면 냄새나니까 하는데",
          "original_form": "설거지는 안하면 냄새나니까 하는데",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100033634.1.8",
          "form": "청소는 못하겄어요 ㅋㅋㅋㅋ",
          "original_form": "청소는 못하겄어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100033634.1.9",
          "form": "건조기 있으신가요",
          "original_form": "건조기 있으신가요",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100033634.1.10",
          "form": "아니요 ㅠㅠㅠ",
          "original_form": "아니요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100033634.1.11",
          "form": "건조기 없어요",
          "original_form": "건조기 없어요",
          "speaker_id": "1",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100033634.1.12",
          "form": "있으신가요",
          "original_form": "있으신가요",
          "speaker_id": "1",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100033634.1.13",
          "form": "저 건조기 있는데",
          "original_form": "저 건조기 있는데",
          "speaker_id": "2",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100033634.1.14",
          "form": "요새 습해서 건조기없으면 어케 사나 싶어요",
          "original_form": "요새 습해서 건조기없으면 어케 사나 싶어요",
          "speaker_id": "2",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100033634.1.15",
          "form": "정말요",
          "original_form": "정말요",
          "speaker_id": "1",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100033634.1.16",
          "form": "비오면 빨래를 못해요",
          "original_form": "비오면 빨래를 못해요",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100033634.1.17",
          "form": "건조기가 삶의 질을 상승시켰어요",
          "original_form": "건조기가 삶의 질을 상승시켰어요",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100033634.1.18",
          "form": "이런날씨엔 빨래하면 안마르고 냄새나지않나요",
          "original_form": "이런날씨엔 빨래하면 안마르고 냄새나지않나요",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100033634.1.19",
          "form": "그래서 저희도",
          "original_form": "그래서 저희도",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100033634.1.20",
          "form": "구매할까 고민이에요!!!",
          "original_form": "구매할까 고민이에요!!!",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100033634.1.21",
          "form": "추천합니다 ㅋㅋㅋㅋㅋ 근데 옷이 좀 상할수도 잇대요",
          "original_form": "추천합니다 ㅋㅋㅋㅋㅋ 근데 옷이 좀 상할수도 잇대요",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100033634.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ알아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ알아요",
          "speaker_id": "1",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100033634.1.23",
          "form": "수건만이라도 돌릴려구요",
          "original_form": "수건만이라도 돌릴려구요",
          "speaker_id": "1",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100033634.1.24",
          "form": "수건만 돌려도 ㄹㅇ",
          "original_form": "수건만 돌려도 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210825 12:13"
        },
        {
          "id": "MDRW2100033634.1.25",
          "form": "이불빨래할때도 좋아요",
          "original_form": "이불빨래할때도 좋아요",
          "speaker_id": "2",
          "time": "20210825 12:13"
        },
        {
          "id": "MDRW2100033634.1.26",
          "form": "아 이불까지도 좋죠 !!!!!",
          "original_form": "아 이불까지도 좋죠 !!!!!",
          "speaker_id": "1",
          "time": "20210825 12:13"
