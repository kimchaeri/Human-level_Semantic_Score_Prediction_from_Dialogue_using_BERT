      "utterance": [
        {
          "id": "MDRW2100023946.1.1",
          "form": "옷 정리는 하셨어용?",
          "original_form": "옷 정리는 하셨어용?",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023946.1.2",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023946.1.3",
          "form": "낮은 더워서..ㅜ",
          "original_form": "낮은 더워서..ㅜ",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023946.1.4",
          "form": "계속 소나기가 오다보니 습해서 좀 더 있다가 해야할것 같아요",
          "original_form": "계속 소나기가 오다보니 습해서 좀 더 있다가 해야할것 같아요",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023946.1.5",
          "form": "정리벌써 하셨나요?",
          "original_form": "정리벌써 하셨나요?",
          "speaker_id": "2",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023946.1.6",
          "form": "저는 대~~충",
          "original_form": "저는 대~~충",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023946.1.7",
          "form": "가을옷 몇개만 꺼냈어용ㅋㅋㅋㅋ",
          "original_form": "가을옷 몇개만 꺼냈어용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023946.1.8",
          "form": "이걸로 이번주 돌려입다가",
          "original_form": "이걸로 이번주 돌려입다가",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023946.1.9",
          "form": "더워지면 다시 여름옷입을거에용ㅋㅋ",
          "original_form": "더워지면 다시 여름옷입을거에용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023946.1.10",
          "form": "그쵸 가디건이나 위에 걸칠것만 꺼내두면 될것 같네요",
          "original_form": "그쵸 가디건이나 위에 걸칠것만 꺼내두면 될것 같네요",
          "speaker_id": "2",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023946.1.11",
          "form": "넹넹ㅋㅋㅋ 추석전에 다시 더워진다에",
          "original_form": "넹넹ㅋㅋㅋ 추석전에 다시 더워진다에",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023946.1.12",
          "form": "한표 던집니다",
          "original_form": "한표 던집니다",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023946.1.13",
          "form": "어우 그 한표에 찬성합니다ㅎㅎㅎ",
          "original_form": "어우 그 한표에 찬성합니다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023946.1.14",
          "form": "가을옷은 참 코디하기가 번거롭더라구요",
          "original_form": "가을옷은 참 코디하기가 번거롭더라구요",
          "speaker_id": "2",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023946.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023946.1.16",
          "form": "일교차도 커서",
          "original_form": "일교차도 커서",
          "speaker_id": "1",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023946.1.17",
          "form": "아침에 맞는 옷이 있고",
          "original_form": "아침에 맞는 옷이 있고",
          "speaker_id": "1",
          "time": "20210830 20:33"
        },
        {
          "id": "MDRW2100023946.1.18",
          "form": "저녁에 맞는옷 다 달라요ㅋㅋㅋ",
          "original_form": "저녁에 맞는옷 다 달라요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023946.1.19",
          "form": "맞아요\r\n옷입기가 너무 변덕스러워서ㅎㅎ",
          "original_form": "맞아요\r\n옷입기가 너무 변덕스러워서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023946.1.20",
          "form": "온도에 맞춰 입으려면 밖에 나갈때 옷을 또 챙겨야해서 번거롭지만...",
          "original_form": "온도에 맞춰 입으려면 밖에 나갈때 옷을 또 챙겨야해서 번거롭지만...",
          "speaker_id": "1",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023946.1.21",
          "form": "그래도 익어가는 예쁜 낙엽들을 보면서ㅋㅋㅋㅋ",
          "original_form": "그래도 익어가는 예쁜 낙엽들을 보면서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100023946.1.22",
          "form": "참아요",
          "original_form": "참아요",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100023946.1.23",
          "form": "그쵸\r\n얼른 코로나없어지면 경주에 불국사가서 단풍구경 실컷하고 싶네요ㅎㅎ",
          "original_form": "그쵸\r\n얼른 코로나없어지면 경주에 불국사가서 단풍구경 실컷하고 싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100023946.1.24",
          "form": "경주불국사 단풍이 정말 멋지더라구요",
          "original_form": "경주불국사 단풍이 정말 멋지더라구요",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100023946.1.25",
          "form": "그 시기에 불국사 단풍 뉴스에",
          "original_form": "그 시기에 불국사 단풍 뉴스에",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100023946.1.26",
          "form": "많이 나오더라구요ㅋㅋㅋ",
          "original_form": "많이 나오더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100023946.1.27",
          "form": "단풍이 가장 예뻐서 그런가봐요",
          "original_form": "단풍이 가장 예뻐서 그런가봐요",
          "speaker_id": "1",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100023946.1.28",
          "form": "네 너무 예뻐요\r\n저도 뉴스보고 찾아갔었는데 다시금 가보고 싶네요",
          "original_form": "네 너무 예뻐요\r\n저도 뉴스보고 찾아갔었는데 다시금 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023946.1.29",
          "form": "아 뉴스보시고 가셨구나",
          "original_form": "아 뉴스보시고 가셨구나",
          "speaker_id": "1",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023946.1.30",
          "form": "저도 가고싶네용ㅎㅎ",
          "original_form": "저도 가고싶네용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023946.1.31",
          "form": "그 길따라 촤라락",
          "original_form": "그 길따라 촤라락",
          "speaker_id": "1",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023946.1.32",
          "form": "펼쳐진 단풍들이 넘 예쁘더라구요",
          "original_form": "펼쳐진 단풍들이 넘 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210830 20:38"
        }
      ]
    }
