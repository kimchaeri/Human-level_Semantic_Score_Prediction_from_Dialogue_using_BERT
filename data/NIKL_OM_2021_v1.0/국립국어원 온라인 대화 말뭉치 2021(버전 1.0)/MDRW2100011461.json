      "utterance": [
        {
          "id": "MDRW2100011461.1.1",
          "form": "최근에 사신것있나요?",
          "original_form": "최근에 사신것있나요?",
          "speaker_id": "1",
          "time": "20210824 12:53"
        },
        {
          "id": "MDRW2100011461.1.2",
          "form": "당근마켓으로 마우스사서 잘쓰고있어요",
          "original_form": "당근마켓으로 마우스사서 잘쓰고있어요",
          "speaker_id": "2",
          "time": "20210824 12:55"
        },
        {
          "id": "MDRW2100011461.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100011461.1.4",
          "form": "무선인가요?",
          "original_form": "무선인가요?",
          "speaker_id": "1",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100011461.1.5",
          "form": "유선?",
          "original_form": "유선?",
          "speaker_id": "1",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100011461.1.6",
          "form": "버티컬 무선마우스요",
          "original_form": "버티컬 무선마우스요",
          "speaker_id": "2",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100011461.1.7",
          "form": "누누님은 뭐사셨어요?",
          "original_form": "누누님은 뭐사셨어요?",
          "speaker_id": "2",
          "time": "20210824 13:00"
        },
        {
          "id": "MDRW2100011461.1.8",
          "form": "전 운동화요!",
          "original_form": "전 운동화요!",
          "speaker_id": "1",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100011461.1.9",
          "form": "발에 잘 맞나요",
          "original_form": "발에 잘 맞나요",
          "speaker_id": "2",
          "time": "20210824 13:03"
        },
        {
          "id": "MDRW2100011461.1.10",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100011461.1.11",
          "form": "사이즈가 딱이예요",
          "original_form": "사이즈가 딱이예요",
          "speaker_id": "1",
          "time": "20210824 13:05"
        },
        {
          "id": "MDRW2100011461.1.12",
          "form": "오 운동화 넘 비싸서 당근으로 잘구하셨네요",
          "original_form": "오 운동화 넘 비싸서 당근으로 잘구하셨네요",
          "speaker_id": "2",
          "time": "20210824 13:06"
        },
        {
          "id": "MDRW2100011461.1.13",
          "form": "엥 저늑",
          "original_form": "엥 저늑",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011461.1.14",
          "form": "인터넷에서 샀답니다ㅎㅎㅎ",
          "original_form": "인터넷에서 샀답니다ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:08"
        },
        {
          "id": "MDRW2100011461.1.15",
          "form": "아하! 글쿤요 저는 당근으로 운동화샀어서 누누님도 그러신줄 ㅋㅋ",
          "original_form": "아하! 글쿤요 저는 당근으로 운동화샀어서 누누님도 그러신줄 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:09"
        },
        {
          "id": "MDRW2100011461.1.16",
          "form": "네네ㅋㅋㅋ 다른거",
          "original_form": "네네ㅋㅋㅋ 다른거",
          "speaker_id": "1",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100011461.1.17",
          "form": "사시고싶으신거있나요?",
          "original_form": "사시고싶으신거있나요?",
          "speaker_id": "1",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100011461.1.18",
          "form": "집?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "집?ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:17"
        },
        {
          "id": "MDRW2100011461.1.19",
          "form": "손목이 아팠는데 버티컬마우스 넘 잘산거같아요",
          "original_form": "손목이 아팠는데 버티컬마우스 넘 잘산거같아요",
          "speaker_id": "2",
          "time": "20210824 13:17"
        }
