      "utterance": [
        {
          "id": "MDRW2100041353.1.1",
          "form": "name2님은 오늘 뭐 드셨어요?",
          "original_form": "&name2&님은 오늘 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100041353.1.2",
          "form": "저는 오늘 볶음밥 먹었어요",
          "original_form": "저는 오늘 볶음밥 먹었어요",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100041353.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100041353.1.4",
          "form": "저는 돼지불고기해서 먹었어요",
          "original_form": "저는 돼지불고기해서 먹었어요",
          "speaker_id": "1",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100041353.1.5",
          "form": "무슨 볶음밥이용?",
          "original_form": "무슨 볶음밥이용?",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100041353.1.6",
          "form": "김치볶음밥이요!",
          "original_form": "김치볶음밥이요!",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100041353.1.7",
          "form": "돼지불고기 생각만해도 맛있네요!!",
          "original_form": "돼지불고기 생각만해도 맛있네요!!",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100041353.1.8",
          "form": "으아 김볶밥 맛있겠어요 ㅠ",
          "original_form": "으아 김볶밥 맛있겠어요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100041353.1.9",
          "form": "돼지불고기도 맛있었어요 ㅎㅎ",
          "original_form": "돼지불고기도 맛있었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100041353.1.10",
          "form": "오랜만에 해먹어서 그런지 더 맛있었어요!",
          "original_form": "오랜만에 해먹어서 그런지 더 맛있었어요!",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100041353.1.11",
          "form": "일단 고기는...맛이 없을 수가 없어요ㅋㅋㅋ",
          "original_form": "일단 고기는...맛이 없을 수가 없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100041353.1.12",
          "form": "배불러도 고기는 계속 들어간다고요ㅋㅋㅋ",
          "original_form": "배불러도 고기는 계속 들어간다고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100041353.1.13",
          "form": "맞죠맞죠ㅋㅋㅋㅋ 고기는 원래 그런거죠? 후 저만 그런줄!",
          "original_form": "맞죠맞죠ㅋㅋㅋㅋ 고기는 원래 그런거죠? 후 저만 그런줄!",
          "speaker_id": "1",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100041353.1.14",
          "form": "김치볶음밥 할때 포인트는 뭔가요!",
          "original_form": "김치볶음밥 할때 포인트는 뭔가요!",
          "speaker_id": "1",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100041353.1.15",
          "form": "고기가 앞에 있으면 젓가락질을 멈출 수가 없어요ㅋㅋㅋ",
          "original_form": "고기가 앞에 있으면 젓가락질을 멈출 수가 없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100041353.1.16",
          "form": "저는 김치 많이 넣어요!",
          "original_form": "저는 김치 많이 넣어요!",
          "speaker_id": "2",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100041353.1.17",
          "form": "혹시 김치 사서 드세요?",
          "original_form": "혹시 김치 사서 드세요?",
          "speaker_id": "1",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100041353.1.18",
          "form": "네 사서 먹어용",
          "original_form": "네 사서 먹어용",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041353.1.19",
          "form": "오! 저 추천해주세요!",
          "original_form": "오! 저 추천해주세요!",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041353.1.20",
          "form": "저도 사서 먹으려구요",
          "original_form": "저도 사서 먹으려구요",
          "speaker_id": "1",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041353.1.21",
          "form": "엇 슬프게도...저는 랜덤으로 아무거나 짚이는것만 사먹는 바람에...ㅠㅠㅠㅠ",
          "original_form": "엇 슬프게도...저는 랜덤으로 아무거나 짚이는것만 사먹는 바람에...ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:47"
        },
        {
          "id": "MDRW2100041353.1.22",
