      "utterance": [
        {
          "id": "MDRW2100044838.1.1",
          "form": "어쩌다 정말 한번씩 가는 정도인데",
          "original_form": "어쩌다 정말 한번씩 가는 정도인데",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044838.1.2",
          "form": "신촌에도 솔직히 계속 가게되면 돌고돌아요ㅋㅋㅋ",
          "original_form": "신촌에도 솔직히 계속 가게되면 돌고돌아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044838.1.3",
          "form": "ㅋㅋㅋㅋ 그 특정 루트가 항상 생기는 느낌이에요",
          "original_form": "ㅋㅋㅋㅋ 그 특정 루트가 항상 생기는 느낌이에요",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044838.1.4",
          "form": "신촌에 대성이네라는 닭볶음탕집이 맛있는데 거기도 안간지 꽤 되었네요ㅜ",
          "original_form": "신촌에 대성이네라는 닭볶음탕집이 맛있는데 거기도 안간지 꽤 되었네요ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044838.1.5",
          "form": "닭볶음탕! 윽.. 진짜 안먹은지 오래된것 같아요",
          "original_form": "닭볶음탕! 윽.. 진짜 안먹은지 오래된것 같아요",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044838.1.6",
          "form": "전 조만간 대성이네 갈거에요ㅜㅜ",
          "original_form": "전 조만간 대성이네 갈거에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044838.1.7",
          "form": "아 배고파지는 것 같아요 ㅋㅋㅋ",
          "original_form": "아 배고파지는 것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044838.1.8",
          "form": "ㅋㅋㅋ전 이제 곧 자러가려구요ㅜㅜ 배고플 땐 자야해요...",
          "original_form": "ㅋㅋㅋ전 이제 곧 자러가려구요ㅜㅜ 배고플 땐 자야해요...",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044838.1.9",
          "form": "야식찾게되니까 안돼요...",
          "original_form": "야식찾게되니까 안돼요...",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044838.1.10",
          "form": "으앙 ㅠㅠ 그니까요 ㅠㅠ",
          "original_form": "으앙 ㅠㅠ 그니까요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044838.1.11",
          "form": "아 정말루요 ㅠㅠㅠ 홍대 지나가다 어느날 갑자기 만나게 될지도 몰라요 ㅠ",
          "original_form": "아 정말루요 ㅠㅠㅠ 홍대 지나가다 어느날 갑자기 만나게 될지도 몰라요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044838.1.12",
          "form": "ㅋㅋㅋㅋ스쳐지나가면서 혹싀...?",
          "original_form": "ㅋㅋㅋㅋ스쳐지나가면서 혹싀...?",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044838.1.13",
          "form": "ㅋㅋㅋㅋㅋ맛집에서 우연히 만날 수있길 바래요",
          "original_form": "ㅋㅋㅋㅋㅋ맛집에서 우연히 만날 수있길 바래요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044838.1.14",
          "form": "ㅋㅋㅋㅋ 꼭 꼭이요 모르고 지나칠 확률이 상당히 높지만",
          "original_form": "ㅋㅋㅋㅋ 꼭 꼭이요 모르고 지나칠 확률이 상당히 높지만",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044838.1.15",
          "form": "추천해주신 음식점들은 모두 다 적어뒀으니 가볼거에요!",
          "original_form": "추천해주신 음식점들은 모두 다 적어뒀으니 가볼거에요!",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044838.1.16",
          "form": "ㅋㅋㅋ저도 다 적어뒀어요! 맛집공유 감사해요!",
          "original_form": "ㅋㅋㅋ저도 다 적어뒀어요! 맛집공유 감사해요!",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100044838.1.17",
          "form": "저두요 받은게 더 많은 것 같아요 감사합니다!!!",
          "original_form": "저두요 받은게 더 많은 것 같아요 감사합니다!!!",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100044838.1.18",
          "form": "그럼 이만 안녕히!!",
          "original_form": "그럼 이만 안녕히!!",
          "speaker_id": "2",
          "time": "20210908 00:42"
        }
      ]
