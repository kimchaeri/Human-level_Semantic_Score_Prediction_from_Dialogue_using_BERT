      "utterance": [
        {
          "id": "MDRW2100027685.1.1",
          "form": "오늘 완전 가을 날씨더라구요",
          "original_form": "오늘 완전 가을 날씨더라구요",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027685.1.2",
          "form": "엊그제는 진짜 더웠는데 이렇게 갑자기",
          "original_form": "엊그제는 진짜 더웠는데 이렇게 갑자기",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027685.1.3",
          "form": "어젠가 얼마전에 비 오고 완전 선선해졌어",
          "original_form": "어젠가 얼마전에 비 오고 완전 선선해졌어",
          "speaker_id": "1",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027685.1.4",
          "form": "이제 슬슬 코트를 입을 수 있는 '그' 시기가 오는 건가",
          "original_form": "이제 슬슬 코트를 입을 수 있는 '그' 시기가 오는 건가",
          "speaker_id": "2",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027685.1.5",
          "form": "'그' 시기",
          "original_form": "'그' 시기",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027685.1.6",
          "form": "금방 호로록 사라질듯",
          "original_form": "금방 호로록 사라질듯",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027685.1.7",
          "form": "눈 감았다 뜨면 롱패딩 꺼내고 있을 듯",
          "original_form": "눈 감았다 뜨면 롱패딩 꺼내고 있을 듯",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027685.1.8",
          "form": "ㄹㅇ... 싫다 이런 기후...",
          "original_form": "ㄹㅇ... 싫다 이런 기후...",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027685.1.9",
          "form": "그래도 요즘 날씨는 좋아",
          "original_form": "그래도 요즘 날씨는 좋아",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027685.1.10",
          "form": "맞아 가을 날씨 짱",
          "original_form": "맞아 가을 날씨 짱",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100027685.1.11",
          "form": "일기예보에는 비 올 것처럼 나오네...",
          "original_form": "일기예보에는 비 올 것처럼 나오네...",
          "speaker_id": "1",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100027685.1.12",
          "form": "여긴 비 안 오는데",
          "original_form": "여긴 비 안 오는데",
          "speaker_id": "1",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100027685.1.13",
          "form": "뭐지 여기도 비 안 오는데",
          "original_form": "뭐지 여기도 비 안 오는데",
          "speaker_id": "2",
          "time": "20210902 14:56"
        },
        {
          "id": "MDRW2100027685.1.14",
          "form": "기상청 믿을 수 x",
          "original_form": "기상청 믿을 수 x",
          "speaker_id": "2",
          "time": "20210902 14:56"
        },
        {
          "id": "MDRW2100027685.1.15",
          "form": "저녁에 비 오려나",
          "original_form": "저녁에 비 오려나",
          "speaker_id": "1",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027685.1.16",
          "form": "근데 하루종일 우중충하긴 해서",
          "original_form": "근데 하루종일 우중충하긴 해서",
          "speaker_id": "1",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027685.1.17",
          "form": "오늘 밖에 안 나가봤는데 이따 나가볼까",
          "original_form": "오늘 밖에 안 나가봤는데 이따 나가볼까",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027685.1.18",
          "form": "집에만 있었더니 날씨를 모르겠네",
          "original_form": "집에만 있었더니 날씨를 모르겠네",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027685.1.19",
          "form": "오늘 짱 시원해요",
          "original_form": "오늘 짱 시원해요",
          "speaker_id": "1",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027685.1.20",
          "form": "비만 갑자기 안 쏟아지면 산책하기 딱 좋음",
          "original_form": "비만 갑자기 안 쏟아지면 산책하기 딱 좋음",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027685.1.21",
          "form": "비만 갑자기 안 쏟아지면",
          "original_form": "비만 갑자기 안 쏟아지면",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
