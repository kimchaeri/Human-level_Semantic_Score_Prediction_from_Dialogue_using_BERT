      "utterance": [
        {
          "id": "MDRW2100018809.1.1",
          "form": "안녕하세요~ 학생이신가요?",
          "original_form": "안녕하세요~ 학생이신가요?",
          "speaker_id": "1",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018809.1.2",
          "form": "안녕하세요 전 졸업했어요",
          "original_form": "안녕하세요 전 졸업했어요",
          "speaker_id": "2",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100018809.1.3",
          "form": "앗 저도 졸업생이에요ㅎㅎ",
          "original_form": "앗 저도 졸업생이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018809.1.4",
          "form": "몇학번이세요?",
          "original_form": "몇학번이세요?",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018809.1.5",
          "form": "ㅋㅋ졸업한지 오래오래됐어요",
          "original_form": "ㅋㅋ졸업한지 오래오래됐어요",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018809.1.6",
          "form": "저도 딱 십년됐네요ㅋㅋㅋ",
          "original_form": "저도 딱 십년됐네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018809.1.7",
          "form": "서로 오래됐네요ㅋㅋ",
          "original_form": "서로 오래됐네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018809.1.8",
          "form": "그쵸 이제 학교생활 잘 기억도 안나네요",
          "original_form": "그쵸 이제 학교생활 잘 기억도 안나네요",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018809.1.9",
          "form": "요즘 학생들은 줌으로 동아리모임도 한다고 해요ㅋㅋㅋ 신기",
          "original_form": "요즘 학생들은 줌으로 동아리모임도 한다고 해요ㅋㅋㅋ 신기",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018809.1.10",
          "form": "제동생은 휴학해버리던데요",
          "original_form": "제동생은 휴학해버리던데요",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018809.1.11",
          "form": "기숙사서 줌으로 수업한다구 등록금아깝대요",
          "original_form": "기숙사서 줌으로 수업한다구 등록금아깝대요",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018809.1.12",
          "form": "와 정말 그럴만하네요..",
          "original_form": "와 정말 그럴만하네요..",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018809.1.13",
          "form": "기숙사비는 기숙사비대로 들고",
          "original_form": "기숙사비는 기숙사비대로 들고",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018809.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018809.1.15",
          "form": "근데 휴학오래해도 손해라",
          "original_form": "근데 휴학오래해도 손해라",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018809.1.16",
          "form": "다시 준비하더라구요",
          "original_form": "다시 준비하더라구요",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018809.1.17",
          "form": "요즘 학생들도 힘들겠어요ㅋㅋㅋㅠㅠ",
          "original_form": "요즘 학생들도 힘들겠어요ㅋㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018809.1.18",
          "form": "무슨 과 나오셨어요?",
          "original_form": "무슨 과 나오셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018809.1.19",
          "form": "저는 디자인과요",
          "original_form": "저는 디자인과요",
          "speaker_id": "2",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018809.1.20",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018809.1.21",
          "form": "저는 무역학과요",
          "original_form": "저는 무역학과요",
          "speaker_id": "1",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018809.1.22",
          "form": "전공은 안살렸지만..^^ 다닐 땐 잼썼어요",
          "original_form": "전공은 안살렸지만..^^ 다닐 땐 잼썼어요",
          "speaker_id": "1",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018809.1.23",
          "form": "저는 쪼금 씩 써요 달리 할수있는게없어서 ㅎㅎ",
          "original_form": "저는 쪼금 씩 써요 달리 할수있는게없어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:48"
        },
        {
          "id": "MDRW2100018809.1.24",
          "form": "무역학은 머하나요?",
          "original_form": "무역학은 머하나요?",
          "speaker_id": "2",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018809.1.25",
          "form": "친구들보면 해외영업팀 들어가서 수출 수입도 하고",
          "original_form": "친구들보면 해외영업팀 들어가서 수출 수입도 하고",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018809.1.26",
          "form": "그냥 다방면으로 취업하더라구요ㅋㅋㅋ",
          "original_form": "그냥 다방면으로 취업하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018809.1.27",
          "form": "학교 다닐 때 정말 잡다하게 배워요",
          "original_form": "학교 다닐 때 정말 잡다하게 배워요",
          "speaker_id": "1",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018809.1.28",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018809.1.29",
          "form": "취업범위가 넓군요",
          "original_form": "취업범위가 넓군요",
          "speaker_id": "2",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018809.1.30",
          "form": "좋은과네요",
          "original_form": "좋은과네요",
          "speaker_id": "2",
