      "utterance": [
        {
          "id": "MDRW2100035687.1.1",
          "form": "저도여... 저 퀸덤보고 오마이걸 좋아하기 시작한거라서",
          "original_form": "저도여... 저 퀸덤보고 오마이걸 좋아하기 시작한거라서",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035687.1.2",
          "form": "아이들도 영상 많이 봤어요",
          "original_form": "아이들도 영상 많이 봤어요",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035687.1.3",
          "form": "으아 퀸덤 봤죠 !!",
          "original_form": "으아 퀸덤 봤죠 !!",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035687.1.4",
          "form": "전 원래 러블리즈 좋아해서 봤었거든요 ㅎㅎ",
          "original_form": "전 원래 러블리즈 좋아해서 봤었거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035687.1.5",
          "form": "근데 오마이걸이랑 아이들의 재발견 ㅠㅠ",
          "original_form": "근데 오마이걸이랑 아이들의 재발견 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035687.1.6",
          "form": "아 정말요??",
          "original_form": "아 정말요??",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035687.1.7",
          "form": "맞아요 오마이걸 진짜 대단해 천재야...",
          "original_form": "맞아요 오마이걸 진짜 대단해 천재야...",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035687.1.8",
          "form": "전 오마이걸이 본인들이 잘하는걸 잘 알고 있어서",
          "original_form": "전 오마이걸이 본인들이 잘하는걸 잘 알고 있어서",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035687.1.9",
          "form": "넘 좋았어요 !!!",
          "original_form": "넘 좋았어요 !!!",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035687.1.10",
          "form": "ㅋㅋㅋㅋ맞아요 자부심이 느껴지는...!",
          "original_form": "ㅋㅋㅋㅋ맞아요 자부심이 느껴지는...!",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100035687.1.11",
          "form": "아이들은 전소연씨의 리더십이 아주 멋지더라고요",
          "original_form": "아이들은 전소연씨의 리더십이 아주 멋지더라고요",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035687.1.12",
          "form": "와 진짜요 ㅎㅎ",
          "original_form": "와 진짜요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035687.1.13",
          "form": "글고 전소연도 천재에요 !!",
          "original_form": "글고 전소연도 천재에요 !!",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035687.1.14",
          "form": "노래 나오는거보면.. 항상 느낍니다 ㅋㅋ",
          "original_form": "노래 나오는거보면.. 항상 느낍니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035687.1.15",
          "form": "맞아요 진짜 초천재",
          "original_form": "맞아요 진짜 초천재",
          "speaker_id": "2",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035687.1.16",
