      "utterance": [
        {
          "id": "MDRW2100012238.1.1",
          "form": "나 카레랑 족발 먹었어",
          "original_form": "나 카레랑 족발 먹었어",
          "speaker_id": "2",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100012238.1.2",
          "form": "헐 마싱ㅆ는 거 먹었네",
          "original_form": "헐 마싱ㅆ는 거 먹었네",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012238.1.3",
          "form": "나는 돼지김치찌개 먹을라구",
          "original_form": "나는 돼지김치찌개 먹을라구",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100012238.1.4",
          "form": "김찌도 맛있지",
          "original_form": "김찌도 맛있지",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012238.1.5",
          "form": "나도 김찌에 다른 거보다 돼지고기 넣은 게 맛있더라고",
          "original_form": "나도 김찌에 다른 거보다 돼지고기 넣은 게 맛있더라고",
          "speaker_id": "2",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100012238.1.6",
          "form": "맞아 김찌는 돼지고기가 최고여",
          "original_form": "맞아 김찌는 돼지고기가 최고여",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012238.1.7",
          "form": "족발은 배달시킨 거야?",
          "original_form": "족발은 배달시킨 거야?",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100012238.1.8",
          "form": "아니아니 마트에서 산 포장제품 잘라먹었어",
          "original_form": "아니아니 마트에서 산 포장제품 잘라먹었어",
          "speaker_id": "2",
          "time": "20210824 20:45"
        },
        {
          "id": "MDRW2100012238.1.9",
          "form": "오 대박 족발 파는 음식점이랑 큰 차이 없어?",
          "original_form": "오 대박 족발 파는 음식점이랑 큰 차이 없어?",
          "speaker_id": "1",
          "time": "20210824 20:46"
        },
        {
          "id": "MDRW2100012238.1.10",
          "form": "족발맛집 같은 곳이랑은 비교 안되는데 그런 집 아니면 비슷한 거 같아",
          "original_form": "족발맛집 같은 곳이랑은 비교 안되는데 그런 집 아니면 비슷한 거 같아",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012238.1.11",
          "form": "오히려 마트에서 사는 게 가성비가 더 좋은 것 같아",
          "original_form": "오히려 마트에서 사는 게 가성비가 더 좋은 것 같아",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012238.1.12",
          "form": "오 나도 마트에서 함 사먹어봐야겠다",
          "original_form": "오 나도 마트에서 함 사먹어봐야겠다",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012238.1.13",
          "form": "배달은 자주 시켜?",
          "original_form": "배달은 자주 시켜?",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012238.1.14",
          "form": "배달 한달에 한두번 정도?",
          "original_form": "배달 한달에 한두번 정도?",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012238.1.15",
          "form": "많으면 네다섯번?",
          "original_form": "많으면 네다섯번?",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012238.1.16",
          "form": "오 진짜 적게 시키네",
          "original_form": "오 진짜 적게 시키네",
          "speaker_id": "1",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012238.1.17",
          "form": "보통뭐먹어",
          "original_form": "보통뭐먹어",
          "speaker_id": "1",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012238.1.18",
          "form": "떡볶이?",
          "original_form": "떡볶이?",
          "speaker_id": "1",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012238.1.19",
          "form": "떡볶이는 가끔? 먹고 보통 시키면 치킨 먹어",
          "original_form": "떡볶이는 가끔? 먹고 보통 시키면 치킨 먹어",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012238.1.20",
          "form": "집에서 배달음식보다 집밥을 더 좋아해서리..",
          "original_form": "집에서 배달음식보다 집밥을 더 좋아해서리..",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012238.1.21",
          "form": "좋은 습관이야",
          "original_form": "좋은 습관이야",
          "speaker_id": "1",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012238.1.22",
          "form": "나는 배달 한달에 열번 시킨 적도 있거든",
          "original_form": "나는 배달 한달에 열번 시킨 적도 있거든",
          "speaker_id": "1",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012238.1.23",
          "form": "헉 돈 많이 들지 않아???",
          "original_form": "헉 돈 많이 들지 않아???",
          "speaker_id": "2",
          "time": "20210824 22:18"
        },
        {
          "id": "MDRW2100012238.1.24",
          "form": "보통 뭐먹어?",
          "original_form": "보통 뭐먹어?",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012238.1.25",
          "form": "돈 많이 들지 그래서 자제중ㅠ",
          "original_form": "돈 많이 들지 그래서 자제중ㅠ",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012238.1.26",
          "form": "닭꼬치 먹어",
          "original_form": "닭꼬치 먹어",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012238.1.27",
          "form": "오늘 저녁 뭐 먹었어?",
          "original_form": "오늘 저녁 뭐 먹었어?",
          "speaker_id": "1",
          "time": "20210825 22:17"
        }
      ]
    }
