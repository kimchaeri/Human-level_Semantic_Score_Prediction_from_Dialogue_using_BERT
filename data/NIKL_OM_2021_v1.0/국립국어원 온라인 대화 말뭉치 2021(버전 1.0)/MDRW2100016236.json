        {
          "id": "MDRW2100016236.1.1",
          "form": "집안일 진짜 싫어요",
          "original_form": "집안일 진짜 싫어요",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016236.1.2",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016236.1.3",
          "form": "청소 빨래 너무 싫어요",
          "original_form": "청소 빨래 너무 싫어요",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016236.1.4",
          "form": "전 설거지요",
          "original_form": "전 설거지요",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016236.1.5",
          "form": "아..  진짜 너무 싫네요",
          "original_form": "아..  진짜 너무 싫네요",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016236.1.6",
          "form": "안하면티엄청나고",
          "original_form": "안하면티엄청나고",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016236.1.7",
          "form": "해도 현상유지고...",
          "original_form": "해도 현상유지고...",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016236.1.8",
          "form": "집안일 봇안나오나요",
          "original_form": "집안일 봇안나오나요",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016236.1.9",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016236.1.10",
          "form": "집안일 대신 해주는 기계 완전  원해요",
          "original_form": "집안일 대신 해주는 기계 완전  원해요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016236.1.11",
          "form": "엄청비싸겠죠?",
          "original_form": "엄청비싸겠죠?",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016236.1.12",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016236.1.13",
          "form": "후 스스로 해야죠 뭐",
          "original_form": "후 스스로 해야죠 뭐",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016236.1.14",
          "form": "돈없는 게 서럽세요",
          "original_form": "돈없는 게 서럽세요",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016236.1.15",
          "form": "혼자 사세요?",
          "original_form": "혼자 사세요?",
          "speaker_id": "2",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016236.1.16",
          "form": "네 자취생은 모든걸 혼자  해결중입니다",
          "original_form": "네 자취생은 모든걸 혼자  해결중입니다",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016236.1.17",
          "form": "저두 자취라...",
          "original_form": "저두 자취라...",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016236.1.18",
