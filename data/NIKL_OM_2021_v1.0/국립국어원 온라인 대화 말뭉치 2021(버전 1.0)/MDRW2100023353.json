      "utterance": [
        {
          "id": "MDRW2100023353.1.1",
          "form": "name1 반려동물 키우세요?ㅎ",
          "original_form": "&name1& 반려동물 키우세요?ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023353.1.2",
          "form": "키우고 싶은데.. 혼자라...",
          "original_form": "키우고 싶은데.. 혼자라...",
          "speaker_id": "1",
          "time": "20210830 00:42"
        },
        {
          "id": "MDRW2100023353.1.3",
          "form": "뭐 키우고 싶으신가요?",
          "original_form": "뭐 키우고 싶으신가요?",
          "speaker_id": "1",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023353.1.4",
          "form": "아.혼자사세용?",
          "original_form": "아.혼자사세용?",
          "speaker_id": "2",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023353.1.5",
          "form": "전 아이들이 알러지가있어서",
          "original_form": "전 아이들이 알러지가있어서",
          "speaker_id": "2",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023353.1.6",
          "form": "못키워요ㅜㅜ",
          "original_form": "못키워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023353.1.7",
          "form": "전강아지 아이들은 고양이를 너무 좋아하는데",
          "original_form": "전강아지 아이들은 고양이를 너무 좋아하는데",
          "speaker_id": "2",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023353.1.8",
          "form": "아이들이 알러지가 심해서요ㅠㅠㅠ",
          "original_form": "아이들이 알러지가 심해서요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:43"
        },
        {
          "id": "MDRW2100023353.1.9",
          "form": "동물 알러지 다 심해요? 아님 특정 동물?",
          "original_form": "동물 알러지 다 심해요? 아님 특정 동물?",
          "speaker_id": "1",
          "time": "20210830 00:46"
        },
        {
          "id": "MDRW2100023353.1.10",
          "form": "진드기란 진드기는",
          "original_form": "진드기란 진드기는",
          "speaker_id": "2",
          "time": "20210830 00:46"
        },
        {
          "id": "MDRW2100023353.1.11",
          "form": "죄다 걸려서요ㅜㅜ",
          "original_form": "죄다 걸려서요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 00:46"
        },
        {
          "id": "MDRW2100023353.1.12",
          "form": "풀밭에 옷두고 그옷만입어도 응급실가야할정도에요ㅠ",
          "original_form": "풀밭에 옷두고 그옷만입어도 응급실가야할정도에요ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:47"
        },
        {
          "id": "MDRW2100023353.1.13",
          "form": "아.. 그정도면 동물 어렵겠네요..",
          "original_form": "아.. 그정도면 동물 어렵겠네요..",
          "speaker_id": "1",
          "time": "20210830 00:48"
        },
        {
          "id": "MDRW2100023353.1.14",
          "form": "그럼 지나가던 강아지도 못 만져요?",
          "original_form": "그럼 지나가던 강아지도 못 만져요?",
          "speaker_id": "1",
          "time": "20210830 00:48"
        },
        {
          "id": "MDRW2100023353.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 00:52"
        },
        {
          "id": "MDRW2100023353.1.16",
          "form": "아기때만졌다가 난리났어용;",
          "original_form": "아기때만졌다가 난리났어용;",
          "speaker_id": "2",
          "time": "20210830 00:53"
        },
        {
          "id": "MDRW2100023353.1.17",
          "form": "그때도 병원행...ㅎㅎ;",
          "original_form": "그때도 병원행...ㅎㅎ;",
          "speaker_id": "2",
          "time": "20210830 00:53"
        },
        {
          "id": "MDRW2100023353.1.18",
          "form": "근데 슬프게도 애기스스로 그걸알아서  동물을 너무 좋아하는데 눈으로만 보네요",
          "original_form": "근데 슬프게도 애기스스로 그걸알아서  동물을 너무 좋아하는데 눈으로만 보네요",
          "speaker_id": "2",
          "time": "20210830 00:53"
        },
        {
          "id": "MDRW2100023353.1.19",
          "form": "친구들은 막 길고양이를 안아들고 (사람손을타서 안기는듯해요^^;)",
          "original_form": "친구들은 막 길고양이를 안아들고 (사람손을타서 안기는듯해요^^;)",
          "speaker_id": "2",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100023353.1.20",
          "form": "쓰다듬고 난린데  보고만 있더라구요",
          "original_form": "쓰다듬고 난린데  보고만 있더라구요",
          "speaker_id": "2",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100023353.1.21",
          "form": "아.. 동물 알러지 치료는 안되는거에요? ㅠㅠ",
          "original_form": "아.. 동물 알러지 치료는 안되는거에요? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100023353.1.22",
          "form": "네 피하는것밖에 없나봐요^^;",
          "original_form": "네 피하는것밖에 없나봐요^^;",
          "speaker_id": "2",
          "time": "20210830 00:58"
        },
        {
          "id": "MDRW2100023353.1.23",
          "form": "name1은 키워본 동물은 있으신가요?",
          "original_form": "&name1&은 키워본 동물은 있으신가요?",
          "speaker_id": "2",
          "time": "20210830 00:59"
        },
        {
          "id": "MDRW2100023353.1.24",
          "form": "전 결혼전에 강아지키우다왔는데 보고싶어요ㅠ  이젠  다 늙어서...ㅠ",
          "original_form": "전 결혼전에 강아지키우다왔는데 보고싶어요ㅠ  이젠  다 늙어서...ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:59"
        },
        {
          "id": "MDRW2100023353.1.25",
          "form": "친정에 강아디 있는거에여?",
          "original_form": "친정에 강아디 있는거에여?",
          "speaker_id": "1",
          "time": "20210830 01:03"
        },
        {
          "id": "MDRW2100023353.1.26",
          "form": "전 애기때 말티즈랑 요크셔테리 키워봤어요",
          "original_form": "전 애기때 말티즈랑 요크셔테리 키워봤어요",
          "speaker_id": "1",
          "time": "20210830 01:03"
        },
        {
          "id": "MDRW2100023353.1.27",
          "form": "네네 저희집 강아지 그냥 믹스견...ㅎㅎ",
          "original_form": "네네 저희집 강아지 그냥 믹스견...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023353.1.28",
          "form": "말티즈 요크 둘다 너무 너무 귀엽",
          "original_form": "말티즈 요크 둘다 너무 너무 귀엽{emoji:♥}",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023353.1.29",
          "form": "전 햄스터도 키워봤구요",
          "original_form": "전 햄스터도 키워봤구요",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023353.1.30",
          "form": "토끼도 키워봤어요",
          "original_form": "토끼도 키워봤어요",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023353.1.31",
          "form": "병아리도 키워봤구요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "병아리도 키워봤구요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023353.1.32",
          "form": "토끼 키워보고 싶네요.. ㅋㅋㅋ",
          "original_form": "토끼 키워보고 싶네요.. ㅋㅋㅋ",
