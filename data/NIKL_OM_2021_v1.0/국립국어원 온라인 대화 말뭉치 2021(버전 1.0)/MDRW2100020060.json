      "utterance": [
        {
          "id": "MDRW2100020060.1.1",
          "form": "근데 이사하려다보니ㅎㅎ",
          "original_form": "근데 이사하려다보니ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100020060.1.2",
          "form": "아까도 이야기했지만ㅎ 순천 광양 여수 이쪽이 제가 집을 샀을때보다 엄청 올랐어요ㅎ",
          "original_form": "아까도 이야기했지만ㅎ 순천 광양 여수 이쪽이 제가 집을 샀을때보다 엄청 올랐어요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020060.1.3",
          "form": "네네 부동산 요세 말도 못하죠",
          "original_form": "네네 부동산 요세 말도 못하죠",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020060.1.4",
          "form": "광양이 제일 안오른거고ㅎ 순천 여수는 미쳤었죠ㅎㅎ",
          "original_form": "광양이 제일 안오른거고ㅎ 순천 여수는 미쳤었죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020060.1.5",
          "form": "그러다보니 전세로 갈수 밖에없았어요ㅎ",
          "original_form": "그러다보니 전세로 갈수 밖에없았어요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020060.1.6",
          "form": "전세 2억8천ㅠ",
          "original_form": "전세 2억8천ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020060.1.7",
          "form": "쎄네욤 ㄷㄷㄷ",
          "original_form": "쎄네욤 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020060.1.8",
          "form": "저희쪽도 몇달도 안되서 5천 올랏어요",
          "original_form": "저희쪽도 몇달도 안되서 5천 올랏어요",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020060.1.9",
          "form": "지방살면서 매매가가 4~8억이 까지가늗데 없을꺼에요ㅎ",
          "original_form": "지방살면서 매매가가 4~8억이 까지가늗데 없을꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020060.1.10",
          "form": "어마무시하더라구요 집값이",
          "original_form": "어마무시하더라구요 집값이",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020060.1.11",
          "form": "저는 분양 알아보고 있는데 초보라 ㅋ",
          "original_form": "저는 분양 알아보고 있는데 초보라 ㅋ",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020060.1.12",
          "form": "어렵어렵 해요",
          "original_form": "어렵어렵 해요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020060.1.13",
          "form": "집에 이사오자마자 제집이 아니었고",
          "original_form": "집에 이사오자마자 제집이 아니었고",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020060.1.14",
          "form": "전세권 설정하면 5프로 올려줘야하니",
          "original_form": "전세권 설정하면 5프로 올려줘야하니",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020060.1.15",
          "form": "계산해보니 1400만원이 2년내에 필요하더라구요ㅎ",
          "original_form": "계산해보니 1400만원이 2년내에 필요하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020060.1.16",
          "form": "그때부터 앱테크 시작했어요ㅎㅎ 그게 제 1차목표였었네요 ㅎ",
          "original_form": "그때부터 앱테크 시작했어요ㅎㅎ 그게 제 1차목표였었네요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020060.1.17",
          "form": "수다쟁이라 말이 길어짐ㅠㅠ 이해해주세요ㅠㅠ",
          "original_form": "수다쟁이라 말이 길어짐ㅠㅠ 이해해주세요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020060.1.18",
          "form": "아항 그러시구나 ㅎㅎ",
          "original_form": "아항 그러시구나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020060.1.19",
          "form": "아뇨아뇨 ㅎㅎ",
          "original_form": "아뇨아뇨 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020060.1.20",
          "form": "2차목표는 name1 이야기듣고ㅎㅎㅎ",
          "original_form": "2차목표는 &name1& 이야기듣고ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020060.1.21",
          "form": "name1은 목표가?",
          "original_form": "&name1&은 목표가?",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020060.1.22",
          "form": "아 내집마련이죠ㅎㅎ",
          "original_form": "아 내집마련이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020060.1.23",
          "form": "저는 내집사는거요 간단해여 ㅋ",
          "original_form": "저는 내집사는거요 간단해여 ㅋ",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020060.1.24",
          "form": "근데 금액이있을꺼아니에요?",
          "original_form": "근데 금액이있을꺼아니에요?",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020060.1.25",
          "form": "구체적인 금액은 아직없으시고ㅡ",
          "original_form": "구체적인 금액은 아직없으시고ㅡ",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020060.1.26",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020060.1.27",
          "form": "청약만 되면  20평대 2억대긴 하더라구요",
          "original_form": "청약만 되면  20평대 2억대긴 하더라구요",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020060.1.28",
          "form": "저도 그래서 뭘해야할지모르겟어요",
          "original_form": "저도 그래서 뭘해야할지모르겟어요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020060.1.29",
          "form": "2군대 분양 예정이라 두군데 찍고  공부중이에요 ㅎㅎ",
          "original_form": "2군대 분양 예정이라 두군데 찍고  공부중이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020060.1.30",
          "form": "부동산 공부도 해야하는데... 맨날 시간이안된요",
          "original_form": "부동산 공부도 해야하는데... 맨날 시간이안된요",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020060.1.31",
          "form": "맨날 핑계ㅍㅍ",
          "original_form": "맨날 핑계ㅍㅍ",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020060.1.32",
          "form": "긍데.1도 모름 ㅋㅋㅋ",
          "original_form": "긍데.1도 모름 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020060.1.33",
          "form": "청약 순위는 좋은데 아마 중도금 대출을 좀 해야할거 같아요",
          "original_form": "청약 순위는 좋은데 아마 중도금 대출을 좀 해야할거 같아요",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020060.1.34",
          "form": "일단 청약되고 생각 ㅋ",
          "original_form": "일단 청약되고 생각 ㅋ",
