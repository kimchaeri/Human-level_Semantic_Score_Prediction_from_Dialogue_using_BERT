      "utterance": [
        {
          "id": "MDRW2100010587.1.1",
          "form": "갤탭은 구매하고 만족했음?",
          "original_form": "갤탭은 구매하고 만족했음?",
          "speaker_id": "1",
          "time": "20210823 19:52"
        },
        {
          "id": "MDRW2100010587.1.2",
          "form": "완전 조아요",
          "original_form": "완전 조아요",
          "speaker_id": "2",
          "time": "20210823 19:52"
        },
        {
          "id": "MDRW2100010587.1.3",
          "form": "게임기로 쓰고있음",
          "original_form": "게임기로 쓰고있음",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010587.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010587.1.5",
          "form": "아근데 폰게임을 갤탭으로 하니까",
          "original_form": "아근데 폰게임을 갤탭으로 하니까",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010587.1.6",
          "form": "폰이 자유로워서",
          "original_form": "폰이 자유로워서",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010587.1.7",
          "form": "그게 너무 좋아",
          "original_form": "그게 너무 좋아",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010587.1.8",
          "form": "그리고 그림그리는거 좋아해서",
          "original_form": "그리고 그림그리는거 좋아해서",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010587.1.9",
          "form": "가끔 낙서해요",
          "original_form": "가끔 낙서해요",
          "speaker_id": "2",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010587.1.10",
          "form": "ㅋㅋㅋㅋ나두 공기계 하나 사고싶다",
          "original_form": "ㅋㅋㅋㅋ나두 공기계 하나 사고싶다",
          "speaker_id": "1",
          "time": "20210823 19:53"
        },
        {
          "id": "MDRW2100010587.1.11",
          "form": "사치스런 소비를 희망함",
          "original_form": "사치스런 소비를 희망함",
          "speaker_id": "1",
          "time": "20210823 19:54"
        },
        {
          "id": "MDRW2100010587.1.12",
          "form": "플립사면",
          "original_form": "플립사면",
          "speaker_id": "2",
          "time": "20210823 19:55"
        },
        {
          "id": "MDRW2100010587.1.13",
          "form": "기존 폰 공기계로 ㄱㄱ",
          "original_form": "기존 폰 공기계로 ㄱㄱ",
          "speaker_id": "2",
          "time": "20210823 19:55"
        },
        {
          "id": "MDRW2100010587.1.14",
          "form": "이 폰 아직 2년정도라 또 사기 아까워",
          "original_form": "이 폰 아직 2년정도라 또 사기 아까워",
          "speaker_id": "1",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010587.1.15",
          "form": "아 그건 그렇다...",
          "original_form": "아 그건 그렇다...",
          "speaker_id": "2",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010587.1.16",
          "form": "근데 삼성은 예구하면",
          "original_form": "근데 삼성은 예구하면",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010587.1.17",
          "form": "가격 좀싸서 노려볼만 하지 않나",
          "original_form": "가격 좀싸서 노려볼만 하지 않나",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010587.1.18",
          "form": "폰은 잘 모르겠네",
          "original_form": "폰은 잘 모르겠네",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010587.1.19",
          "form": "좀 싸긴 함. 삼성카드도 있으니깐",
          "original_form": "좀 싸긴 함. 삼성카드도 있으니깐",
          "speaker_id": "1",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100010587.1.20",
          "form": "ㅇㅎ...그럼 다행인데",
          "original_form": "ㅇㅎ...그럼 다행인데",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010587.1.21",
          "form": "근데 이번예구 끝나서",
          "original_form": "근데 이번예구 끝나서",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010587.1.22",
          "form": "소셜커머스 예구 뜰때까지 기다려야겠네 최소",
          "original_form": "소셜커머스 예구 뜰때까지 기다려야겠네 최소",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010587.1.23",
          "form": "ㅠㅠ구매 타이밍 망해버렸고",
          "original_form": "ㅠㅠ구매 타이밍 망해버렸고",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100010587.1.24",
          "form": "그럴땐 정신승리",
          "original_form": "그럴땐 정신승리",
          "speaker_id": "2",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100010587.1.25",
          "form": "마케팅 영상 괜히 봐서 물욕만 생김",
          "original_form": "마케팅 영상 괜히 봐서 물욕만 생김",
          "speaker_id": "1",
          "time": "20210823 20:01"
        },
        {
          "id": "MDRW2100010587.1.26",
          "form": "원래 그런거보고 출근 의지를 잡는거지",
          "original_form": "원래 그런거보고 출근 의지를 잡는거지",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010587.1.27",
          "form": "일 없을때 쇼핑몰 보고있음 시간 잘 간다",
          "original_form": "일 없을때 쇼핑몰 보고있음 시간 잘 간다",
          "speaker_id": "1",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010587.1.28",
          "form": "전 백수라...",
          "original_form": "전 백수라...",
          "speaker_id": "2",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010587.1.29",
          "form": "사고싶은건 많은데 돈이 없네",
          "original_form": "사고싶은건 많은데 돈이 없네",
          "speaker_id": "2",
          "time": "20210823 20:05"
        }
      ]
    }
  ]
