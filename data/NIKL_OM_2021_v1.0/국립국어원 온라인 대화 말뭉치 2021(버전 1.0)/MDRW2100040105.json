      "utterance": [
        {
          "id": "MDRW2100040105.1.1",
          "form": "몰ㄹㅏ유",
          "original_form": "몰ㄹㅏ유",
          "speaker_id": "2",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100040105.1.2",
          "form": "ㅋㅋㅋㅋㅋ 올만이네요",
          "original_form": "ㅋㅋㅋㅋㅋ 올만이네요",
          "speaker_id": "2",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100040105.1.3",
          "form": "북촌이라..",
          "original_form": "북촌이라..",
          "speaker_id": "2",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100040105.1.4",
          "form": "추천잇을유?",
          "original_form": "추천잇을유?",
          "speaker_id": "2",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100040105.1.5",
          "form": "아니요 저도 잘 몰라요 ㅋㅋ",
          "original_form": "아니요 저도 잘 몰라요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:10"
        },
        {
          "id": "MDRW2100040105.1.6",
          "form": "예전에 북촌갔을때는 와플 먹었던것 같은데",
          "original_form": "예전에 북촌갔을때는 와플 먹었던것 같은데",
          "speaker_id": "1",
          "time": "20210904 10:10"
        },
        {
          "id": "MDRW2100040105.1.7",
          "form": "이거진짜 옛날이에요 ㅋㅋㅋ",
          "original_form": "이거진짜 옛날이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:10"
        },
        {
          "id": "MDRW2100040105.1.8",
          "form": "헉..와플",
          "original_form": "헉..와플",
          "speaker_id": "2",
          "time": "20210904 10:15"
        },
        {
          "id": "MDRW2100040105.1.9",
          "form": "어떤와플요 통통?",
          "original_form": "어떤와플요 통통?",
          "speaker_id": "2",
          "time": "20210904 10:15"
        },
        {
          "id": "MDRW2100040105.1.10",
          "form": "흐음 내 통통이요 ㅋㅋ 옛날엔 막 와플에 이거저거 올려먹었자나용 그런거요!!",
          "original_form": "흐음 내 통통이요 ㅋㅋ 옛날엔 막 와플에 이거저거 올려먹었자나용 그런거요!!",
          "speaker_id": "1",
          "time": "20210904 10:16"
        },
        {
          "id": "MDRW2100040105.1.11",
          "form": "엄머엄머 알조알죠",
          "original_form": "엄머엄머 알조알죠",
          "speaker_id": "2",
          "time": "20210904 10:17"
        },
        {
          "id": "MDRW2100040105.1.12",
          "form": "우리같은세대다",
          "original_form": "우리같은세대다",
          "speaker_id": "2",
          "time": "20210904 10:17"
        },
        {
          "id": "MDRW2100040105.1.13",
          "form": "허니브레드아심 ?",
          "original_form": "허니브레드아심 ?",
          "speaker_id": "2",
          "time": "20210904 10:17"
        },
        {
          "id": "MDRW2100040105.1.14",
          "form": "허어어어어얼 허니브레드 못참죠.....",
          "original_form": "허어어어어얼 허니브레드 못참죠.....",
          "speaker_id": "1",
          "time": "20210904 10:20"
        },
        {
          "id": "MDRW2100040105.1.15",
          "form": "미쳤었죠 전..",
          "original_form": "미쳤었죠 전..",
          "speaker_id": "1",
          "time": "20210904 10:20"
        },
        {
          "id": "MDRW2100040105.1.16",
          "form": "커핀 그루나루 아시나요?",
          "original_form": "커핀 그루나루 아시나요?",
          "speaker_id": "1",
          "time": "20210904 10:20"
        },
        {
          "id": "MDRW2100040105.1.17",
          "form": "알죠알죠 1인1허니브레드자나요 알죠",
          "original_form": "알죠알죠 1인1허니브레드자나요 알죠",
          "speaker_id": "2",
          "time": "20210904 10:21"
        },
        {
          "id": "MDRW2100040105.1.18",
          "form": "이제 찾기힘드러요ㅠ",
          "original_form": "이제 찾기힘드러요ㅠ",
          "speaker_id": "2",
          "time": "20210904 10:21"
        },
        {
          "id": "MDRW2100040105.1.19",
          "form": "맞아요 커핀그루나루 이제 거의없더라구요 ㅠ",
          "original_form": "맞아요 커핀그루나루 이제 거의없더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100040105.1.20",
          "form": "다시.허니브레드의 유행이 와야할텐데",
          "original_form": "다시.허니브레드의 유행이 와야할텐데",
          "speaker_id": "1",
          "time": "20210904 10:45"
        },
        {
          "id": "MDRW2100040105.1.21",
          "form": "긍까요 ....커핀그루나루가고싶다",
          "original_form": "긍까요 ....커핀그루나루가고싶다",
          "speaker_id": "2",
          "time": "20210904 10:48"
        },
        {
          "id": "MDRW2100040105.1.22",
          "form": "그래도 찾아보니까 서울엔 꽤많이 남아있네요!!",
          "original_form": "그래도 찾아보니까 서울엔 꽤많이 남아있네요!!",
          "speaker_id": "1",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100040105.1.23",
          "form": "언제함가야것어요",
          "original_form": "언제함가야것어요",
          "speaker_id": "2",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100040105.1.24",
          "form": "저두요 보라보라한 그 카페!!",
          "original_form": "저두요 보라보라한 그 카페!!",
          "speaker_id": "1",
          "time": "20210904 12:23"
        },
