      "utterance": [
        {
          "id": "MDRW2100034669.1.1",
          "form": "저는 스팀게임해요 ㅎ",
          "original_form": "저는 스팀게임해요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034669.1.2",
          "form": "스팀게임저도 하는데 어떤거하셔요?",
          "original_form": "스팀게임저도 하는데 어떤거하셔요?",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034669.1.3",
          "form": "저 돈스타브 열심히해요",
          "original_form": "저 돈스타브 열심히해요",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034669.1.4",
          "form": "너무 재밌더라고요",
          "original_form": "너무 재밌더라고요",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100034669.1.5",
          "form": "헉 돈스타브!",
          "original_form": "헉 돈스타브!",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034669.1.6",
          "form": "친구가 좋아해서 몇번했는데",
          "original_form": "친구가 좋아해서 몇번했는데",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034669.1.7",
          "form": "제가 잘몰라서 그런가 너무 잘죽어서 힘들었어요",
          "original_form": "제가 잘몰라서 그런가 너무 잘죽어서 힘들었어요",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100034669.1.8",
          "form": "너무 강하게 키웠내요",
          "original_form": "너무 강하게 키웠내요",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034669.1.9",
          "form": "저는 모드란모드는 다깔아서",
          "original_form": "저는 모드란모드는 다깔아서",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034669.1.10",
          "form": "부활만 20번씩하는데 ㅋㅋㅋㅋ",
          "original_form": "부활만 20번씩하는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034669.1.11",
          "form": "앜ㅋㅋㄲㅋ그런방법이 있군요",
          "original_form": "앜ㅋㅋㄲㅋ그런방법이 있군요",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034669.1.12",
          "form": "전 그래서 보다 평화로운 스타듀밸리해요",
          "original_form": "전 그래서 보다 평화로운 스타듀밸리해요",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034669.1.13",
          "form": "스듀도 재밌져 ㅎㅎ",
          "original_form": "스듀도 재밌져 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034669.1.14",
          "form": "좋아하는 캐릭터있어요?",
          "original_form": "좋아하는 캐릭터있어요?",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100034669.1.15",
          "form": "저는 페니랑 결혼했어요",
          "original_form": "저는 페니랑 결혼했어요",
          "speaker_id": "1",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100034669.1.16",
          "form": "누구 좋아하세요",
          "original_form": "누구 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100034669.1.17",
          "form": "한때는 쉐인 좋아했어요",
          "original_form": "한때는 쉐인 좋아했어요",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100034669.1.18",
          "form": "으이구.. 하면서 응원하개되는",
          "original_form": "으이구.. 하면서 응원하개되는",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100034669.1.19",
          "form": "결혼은 헤일리랑 ㅎㅎ",
          "original_form": "결혼은 헤일리랑 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100034669.1.20",
          "form": "쉐인 사람만드는 재미가 있어요 저는 절친만 해봤지만요",
          "original_form": "쉐인 사람만드는 재미가 있어요 저는 절친만 해봤지만요",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100034669.1.21",
          "form": "헤일리 좋죠",
          "original_form": "헤일리 좋죠",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100034669.1.22",
          "form": "초반에 많이 까칠해서 친해지는 재미가 있어요",
          "original_form": "초반에 많이 까칠해서 친해지는 재미가 있어요",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034669.1.23",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034669.1.24",
          "form": "차가운 사람이 녹는걸 보는개 참.ㅡ",
          "original_form": "차가운 사람이 녹는걸 보는개 참.ㅡ",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034669.1.25",
          "form": "즐겁죠",
          "original_form": "즐겁죠",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034669.1.26",
          "form": "취향 나타나네요",
          "original_form": "취향 나타나네요",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100034669.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100034669.1.28",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100034669.1.29",
          "form": "소설이나 드라마 볼때도 그런 사람보는게 제일 재밌었어요",
          "original_form": "소설이나 드라마 볼때도 그런 사람보는게 제일 재밌었어요",
          "speaker_id": "2",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100034669.1.30",
          "form": "ㅋㅋㄲㅋㅋㅋㅋ아 이분 왠지 친숙한 sns 하실거 같은 느낌이나는데...",
          "original_form": "ㅋㅋㄲㅋㅋㅋㅋ아 이분 왠지 친숙한 sns 하실거 같은 느낌이나는데...",
          "speaker_id": "1",
          "time": "20210828 21:54"
        },
        {
          "id": "MDRW2100034669.1.31",
          "form": "취향이 소나무시군요",
          "original_form": "취향이 소나무시군요",
          "speaker_id": "1",
