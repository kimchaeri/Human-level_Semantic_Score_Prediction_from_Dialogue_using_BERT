        {
          "id": "MDRW2100028542.1.1",
          "form": "런데이하는분들 많으신것같더라고요!",
          "original_form": "런데이하는분들 많으신것같더라고요!",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028542.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028542.1.3",
          "form": "저는 런데이 하루 해보고",
          "original_form": "저는 런데이 하루 해보고",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028542.1.4",
          "form": "저는체력이",
          "original_form": "저는체력이",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028542.1.5",
          "form": "바닥이라서 아직,,",
          "original_form": "바닥이라서 아직,,",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028542.1.6",
          "form": "아 나는 체력이",
          "original_form": "아 나는 체력이",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028542.1.7",
          "form": "없다",
          "original_form": "없다",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028542.1.8",
          "form": "앜ㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028542.1.9",
          "form": "ㅋㄱㄱㄱㄱㄱㄱㅋㄱㅋㄱㄱㅂㅋㄱ저희는 아직,,.",
          "original_form": "ㅋㄱㄱㄱㄱㄱㄱㅋㄱㅋㄱㄱㅂㅋㄱ저희는 아직,,.",
          "speaker_id": "1",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028542.1.10",
          "form": "그래서 걷습니다..",
          "original_form": "그래서 걷습니다..",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100028542.1.11",
          "form": "런데이까지 가지도못하는ㄱㄱㅋㄱㅋㅋㄱㄱㅋ",
          "original_form": "런데이까지 가지도못하는ㄱㄱㅋㄱㅋㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100028542.1.12",
          "form": "마스크써서 다행이에요 욕만 나오더라고요",
          "original_form": "마스크써서 다행이에요 욕만 나오더라고요",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100028542.1.13",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028542.1.14",
          "form": "사람 다 똑같네요",
          "original_form": "사람 다 똑같네요",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028542.1.15",
          "form": "모자쓰고뛰는데 후반엔 벗고 화내고있고,,",
          "original_form": "모자쓰고뛰는데 후반엔 벗고 화내고있고,,",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028542.1.16",
          "form": "요즘 날이 선선해서 경보하기 좋은것같아요",
          "original_form": "요즘 날이 선선해서 경보하기 좋은것같아요",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028542.1.17",
          "form": "앜ㅋㅋㅋㄱ 그죠 요즘이 딱 운동하기 좋은 날씨",
          "original_form": "앜ㅋㅋㅋㄱ 그죠 요즘이 딱 운동하기 좋은 날씨",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028542.1.18",
          "form": "오늘도 나가려고했는데",
          "original_form": "오늘도 나가려고했는데",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028542.1.19",
          "form": "컨디션 안좋아서 쉬게 됐어요",
          "original_form": "컨디션 안좋아서 쉬게 됐어요",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028542.1.20",
          "form": "쉬면서 회복해야죠! ㅎㅎㅎ",
          "original_form": "쉬면서 회복해야죠! ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028542.1.21",
