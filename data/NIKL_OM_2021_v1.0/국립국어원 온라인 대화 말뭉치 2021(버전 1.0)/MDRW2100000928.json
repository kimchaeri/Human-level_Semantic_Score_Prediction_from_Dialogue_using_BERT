      "utterance": [
        {
          "id": "MDRW2100000928.1.1",
          "form": "밤되니까 추워졌어요",
          "original_form": "밤되니까 추워졌어요",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100000928.1.2",
          "form": "그러게요~ 이제 에어컨 안틀어도 되네요 ㅎ",
          "original_form": "그러게요~ 이제 에어컨 안틀어도 되네요 ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100000928.1.3",
          "form": "이제 잠옷도 긴팔로 바꿨어요 잘때 춥더라구요ㅜㅜ",
          "original_form": "이제 잠옷도 긴팔로 바꿨어요 잘때 춥더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100000928.1.4",
          "form": "오~벌써 바꾸셨군요!!",
          "original_form": "오~벌써 바꾸셨군요!!",
          "speaker_id": "2",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100000928.1.5",
          "form": "근데 또 낮엔 더워서 외출복은 아직 그대로예요ㅎㅎ",
          "original_form": "근데 또 낮엔 더워서 외출복은 아직 그대로예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100000928.1.6",
          "form": "점심은 아직 햇볕이 뜨겁죠 ㅎ",
          "original_form": "점심은 아직 햇볕이 뜨겁죠 ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100000928.1.7",
          "form": "땀이 살짝 날정도?ㅎㅎ",
          "original_form": "땀이 살짝 날정도?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100000928.1.8",
          "form": "네~그래도 바람 불어서 좋더라고요",
          "original_form": "네~그래도 바람 불어서 좋더라고요",
          "speaker_id": "2",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100000928.1.9",
          "form": "해는 뜨거워도 바람은 참 시원해요ㅎㅎ",
          "original_form": "해는 뜨거워도 바람은 참 시원해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100000928.1.10",
          "form": "네~다시 더워지진 않겠죠?",
          "original_form": "네~다시 더워지진 않겠죠?",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100000928.1.11",
          "form": "헛 설마요ㅜㅜ 태풍 곧 온다는데..",
          "original_form": "헛 설마요ㅜㅜ 태풍 곧 온다는데..",
          "speaker_id": "1",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100000928.1.12",
          "form": "헐 태풍이요?ㅠㅠ",
          "original_form": "헐 태풍이요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100000928.1.13",
          "form": "지금이 딱좋은데 태풍안왔으면 좋겠어요!!",
          "original_form": "지금이 딱좋은데 태풍안왔으면 좋겠어요!!",
          "speaker_id": "1",
          "time": "20210819 21:43"
        },
        {
          "id": "MDRW2100000928.1.14",
          "form": "네 맞아요~태풍오면 피해도 많구ㅠㅠ",
          "original_form": "네 맞아요~태풍오면 피해도 많구ㅠㅠ",
