      "utterance": [
        {
          "id": "MDRW2100037295.1.1",
          "form": "어떤 고기 좋아해",
          "original_form": "어떤 고기 좋아해",
          "speaker_id": "2",
          "time": "20210831 22:17"
        },
        {
          "id": "MDRW2100037295.1.2",
          "form": "한우가 최고",
          "original_form": "한우가 최고",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037295.1.3",
          "form": "고기 맛있지",
          "original_form": "고기 맛있지",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037295.1.4",
          "form": "너도 좋아해?",
          "original_form": "너도 좋아해?",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037295.1.5",
          "form": "나도 소고기가 제일 좋아",
          "original_form": "나도 소고기가 제일 좋아",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037295.1.6",
          "form": "어떻게 먹는 게 좋아",
          "original_form": "어떻게 먹는 게 좋아",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037295.1.7",
          "form": "익혀 먹는 게 좋아",
          "original_form": "익혀 먹는 게 좋아",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037295.1.8",
          "form": "구워 먹는 게 맛있냐는 뜻이었어",
          "original_form": "구워 먹는 게 맛있냐는 뜻이었어",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037295.1.9",
          "form": "스테이크 맛있지",
          "original_form": "스테이크 맛있지",
          "speaker_id": "2",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100037295.1.10",
          "form": "고기에 익힘 정도는은 어느 정도",
          "original_form": "고기에 익힘 정도는은 어느 정도",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037295.1.11",
          "form": "나는 미디움",
          "original_form": "나는 미디움",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037295.1.12",
          "form": "나는 웰던",
          "original_form": "나는 웰던",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037295.1.13",
          "form": "익은 거 좋아하는구나",
          "original_form": "익은 거 좋아하는구나",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037295.1.14",
          "form": "피 기가 없어야 해",
          "original_form": "피 기가 없어야 해",
          "speaker_id": "1",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037295.1.15",
          "form": "그 등심이 좋아 안심이 좋아",
          "original_form": "그 등심이 좋아 안심이 좋아",
          "speaker_id": "2",
          "time": "20210831 22:20"
        },
        {
          "id": "MDRW2100037295.1.16",
          "form": "등심이 좋아",
          "original_form": "등심이 좋아",
          "speaker_id": "1",
          "time": "20210831 22:20"
        }
      ]
