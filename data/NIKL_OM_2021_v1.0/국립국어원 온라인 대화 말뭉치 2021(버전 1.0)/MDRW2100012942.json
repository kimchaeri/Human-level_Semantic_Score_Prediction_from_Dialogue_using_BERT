      "utterance": [
        {
          "id": "MDRW2100012942.1.1",
          "form": "키우고 계신 반려동물 있나요?",
          "original_form": "키우고 계신 반려동물 있나요?",
          "speaker_id": "1",
          "time": "20210825 08:53"
        },
        {
          "id": "MDRW2100012942.1.2",
          "form": "아니요 ㅎㅎ 님은?",
          "original_form": "아니요 ㅎㅎ 님은?",
          "speaker_id": "2",
          "time": "20210825 08:54"
        },
        {
          "id": "MDRW2100012942.1.3",
          "form": "저도 없어요:)",
          "original_form": "저도 없어요:)",
          "speaker_id": "1",
          "time": "20210825 08:55"
        },
        {
          "id": "MDRW2100012942.1.4",
          "form": "키워보고싶은 동물은 있으세요?",
          "original_form": "키워보고싶은 동물은 있으세요?",
          "speaker_id": "1",
          "time": "20210825 08:55"
        },
        {
          "id": "MDRW2100012942.1.5",
          "form": "저는 원숭이요",
          "original_form": "저는 원숭이요",
          "speaker_id": "2",
          "time": "20210825 09:08"
        },
        {
          "id": "MDRW2100012942.1.6",
          "form": "네? 원숭이요? ㅋㅋ",
          "original_form": "네? 원숭이요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:13"
        },
        {
          "id": "MDRW2100012942.1.7",
          "form": "이유도 여쭤봐도 될까요?",
          "original_form": "이유도 여쭤봐도 될까요?",
          "speaker_id": "1",
          "time": "20210825 09:13"
        },
        {
          "id": "MDRW2100012942.1.8",
          "form": "심부름 시키면",
          "original_form": "심부름 시키면",
          "speaker_id": "2",
          "time": "20210825 09:21"
        },
        {
          "id": "MDRW2100012942.1.9",
          "form": "알아들을거 같지않아요?ㅋㅋ",
          "original_form": "알아들을거 같지않아요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:21"
        },
        {
          "id": "MDRW2100012942.1.10",
          "form": "ㅋㅋ동물농장 같은거\r\n너무 많이 보신거 아니에요?ㅋㅋㅋ",
          "original_form": "ㅋㅋ동물농장 같은거\r\n너무 많이 보신거 아니에요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:22"
        },
        {
          "id": "MDRW2100012942.1.11",
          "form": "ㅋㅋ집 엄청 어지를거 같아요 사실",
          "original_form": "ㅋㅋ집 엄청 어지를거 같아요 사실",
          "speaker_id": "2",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012942.1.12",
          "form": "ㅋㅋ 저도 그럴거 같아서\r\n좀 당황했어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋ 저도 그럴거 같아서\r\n좀 당황했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:24"
        },
        {
          "id": "MDRW2100012942.1.13",
          "form": "원숭이 키우고 싶으시다길래ㅋㅋ",
          "original_form": "원숭이 키우고 싶으시다길래ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:24"
        },
        {
          "id": "MDRW2100012942.1.14",
          "form": "ㅋㅋ뭐 키우고 싶으세요",
          "original_form": "ㅋㅋ뭐 키우고 싶으세요",
          "speaker_id": "2",
          "time": "20210825 09:24"
        },
        {
          "id": "MDRW2100012942.1.15",
          "form": "저는... 아무것도 안 키우고 싶어요ㅋㅋ",
          "original_form": "저는... 아무것도 안 키우고 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:25"
        },
        {
          "id": "MDRW2100012942.1.16",
          "form": "어릴때 키웠던 햄스터도\r\n강아지도 다 상처와 충격으로 남아서ㅠ",
          "original_form": "어릴때 키웠던 햄스터도\r\n강아지도 다 상처와 충격으로 남아서ㅠ",
          "speaker_id": "1",
          "time": "20210825 09:25"
        },
        {
          "id": "MDRW2100012942.1.17",
          "form": "아이고ㅠ",
          "original_form": "아이고ㅠ",
          "speaker_id": "2",
          "time": "20210825 09:27"
        },
        {
          "id": "MDRW2100012942.1.18",
          "form": "그래도 보는건 좋아요:)",
