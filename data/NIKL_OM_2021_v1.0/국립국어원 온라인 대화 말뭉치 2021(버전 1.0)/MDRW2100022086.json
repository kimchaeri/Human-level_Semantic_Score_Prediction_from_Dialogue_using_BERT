      "utterance": [
        {
          "id": "MDRW2100022086.1.1",
          "form": "가지 쪄서, 마늘이랑 소금이랑 오일 넣고 드르륵 갈면 가지페스토에여",
          "original_form": "가지 쪄서, 마늘이랑 소금이랑 오일 넣고 드르륵 갈면 가지페스토에여",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100022086.1.2",
          "form": "조금 다른 느낌인데, 그것도 은은하고 맛있어요",
          "original_form": "조금 다른 느낌인데, 그것도 은은하고 맛있어요",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100022086.1.3",
          "form": "이건 재밌는게 오일을 올리브오일로 안 쓰면",
          "original_form": "이건 재밌는게 오일을 올리브오일로 안 쓰면",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100022086.1.4",
          "form": "ㅎㅎㅎ 전공이 뭐에요?",
          "original_form": "ㅎㅎㅎ 전공이 뭐에요?",
          "speaker_id": "1",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100022086.1.5",
          "form": "나중에 찌개에 넣어먹어도 되요 ㅋㅋ",
          "original_form": "나중에 찌개에 넣어먹어도 되요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:35"
        },
        {
          "id": "MDRW2100022086.1.6",
          "form": "에 =ㄱ=... 전공... 전혀 상관없는거요 ㅋㅋㅋㅋ",
          "original_form": "에 =ㄱ=... 전공... 전혀 상관없는거요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100022086.1.7",
          "form": "진짜 걍 먹는게 좋은 일반인......",
          "original_form": "진짜 걍 먹는게 좋은 일반인......",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100022086.1.8",
          "form": "직접다 연구하고 개발한거에요?",
          "original_form": "직접다 연구하고 개발한거에요?",
          "speaker_id": "1",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100022086.1.9",
          "form": "아녀 ㅋㅋㅋ 넓고 너른 인터넷 세상에서 검색해봤져 ㅋㅋㅋ",
          "original_form": "아녀 ㅋㅋㅋ 넓고 너른 인터넷 세상에서 검색해봤져 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100022086.1.10",
          "form": "식재료가 있는데 흔한 방법으로 먹기 싫을때 검색해보면",
          "original_form": "식재료가 있는데 흔한 방법으로 먹기 싫을때 검색해보면",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100022086.1.11",
          "form": "다른 방법이 또 나와요",
          "original_form": "다른 방법이 또 나와요",
          "speaker_id": "2",
          "time": "20210829 02:36"
        },
        {
          "id": "MDRW2100022086.1.12",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100022086.1.13",
          "form": "요리 좋아하세요?",
          "original_form": "요리 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100022086.1.14",
          "form": "가지페스토는 엄마네 밭에서 너무 많이 나와서 처치곤란일때 찾은거에요",
          "original_form": "가지페스토는 엄마네 밭에서 너무 많이 나와서 처치곤란일때 찾은거에요",
          "speaker_id": "2",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100022086.1.15",
          "form": "요리.. 를 좋아한다기보단 먹는걸 좋아해서 도전하는편이에요",
          "original_form": "요리.. 를 좋아한다기보단 먹는걸 좋아해서 도전하는편이에요",
          "speaker_id": "2",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100022086.1.16",
          "form": "자주하세요?",
          "original_form": "자주하세요?",
          "speaker_id": "1",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100022086.1.17",
          "form": "최근에 요리한 음식?",
          "original_form": "최근에 요리한 음식?",
          "speaker_id": "1",
          "time": "20210829 02:37"
        },
        {
          "id": "MDRW2100022086.1.18",
          "form": "어............ 최근에.........",
          "original_form": "어............ 최근에.........",
          "speaker_id": "2",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100022086.1.19",
          "form": "닭칼국수!",
          "original_form": "닭칼국수!",
          "speaker_id": "2",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100022086.1.20",
          "form": "어려울것 같은데",
          "original_form": "어려울것 같은데",
          "speaker_id": "1",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100022086.1.21",
          "form": "닭칼국수 진짜 의외로 쉬워요 짱",
          "original_form": "닭칼국수 진짜 의외로 쉬워요 짱",
          "speaker_id": "2",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100022086.1.22",
          "form": "혹시...밀키트?",
          "original_form": "혹시...밀키트?",
          "speaker_id": "1",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100022086.1.23",
          "form": "아주 푹 고은 맛은 안 나도 괜찮더라구요 아뇨 밀키트 말구",
          "original_form": "아주 푹 고은 맛은 안 나도 괜찮더라구요 아뇨 밀키트 말구",
          "speaker_id": "2",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100022086.1.24",
          "form": "인터넷에 냉동 닭가슴살 다리살 파는데,",
          "original_form": "인터넷에 냉동 닭가슴살 다리살 파는데,",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100022086.1.25",
          "form": "그거 사서 통마늘만 넣고 푹 고아도 닭백숙 되거든여",
          "original_form": "그거 사서 통마늘만 넣고 푹 고아도 닭백숙 되거든여",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100022086.1.26",
          "form": "그거 국물이랑 고기 남겨서 물 조금 더 넣고 팔팔 끓여서 칼국수면 삶아내면 끝!",
          "original_form": "그거 국물이랑 고기 남겨서 물 조금 더 넣고 팔팔 끓여서 칼국수면 삶아내면 끝!",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100022086.1.27",
          "form": "파 좀 넣어주고 소금후추간 하구요",
          "original_form": "파 좀 넣어주고 소금후추간 하구요",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100022086.1.28",
          "form": "들깨칼국수는 실패했는데 저건 맛있었어요",
          "original_form": "들깨칼국수는 실패했는데 저건 맛있었어요",
          "speaker_id": "2",
          "time": "20210829 02:40"
        }
      ]
    }
