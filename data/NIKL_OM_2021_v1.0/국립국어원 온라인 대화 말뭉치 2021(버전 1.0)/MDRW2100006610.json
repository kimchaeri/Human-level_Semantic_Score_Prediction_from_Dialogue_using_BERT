        {
          "id": "MDRW2100006610.1.1",
          "form": "음식은 뭐좋아하세요?",
          "original_form": "음식은 뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 03:14"
        },
        {
          "id": "MDRW2100006610.1.2",
          "form": "햄버거랑 피자요",
          "original_form": "햄버거랑 피자요",
          "speaker_id": "2",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100006610.1.3",
          "form": "ㅋㅋ 자주 드세요!",
          "original_form": "ㅋㅋ 자주 드세요!",
          "speaker_id": "1",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100006610.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100006610.1.5",
          "form": "아뇨 월에 한두번이요 ㅎ",
          "original_form": "아뇨 월에 한두번이요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006610.1.6",
          "form": "ㅋㅋ넘 적게ㅋㅋ",
          "original_form": "ㅋㅋ넘 적게ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006610.1.7",
          "form": "회사다니시죠?",
          "original_form": "회사다니시죠?",
          "speaker_id": "1",
          "time": "20210908 03:17"
        },
        {
          "id": "MDRW2100006610.1.8",
          "form": "네 그렇습니다",
          "original_form": "네 그렇습니다",
          "speaker_id": "2",
          "time": "20210908 03:17"
        },
        {
          "id": "MDRW2100006610.1.9",
          "form": "그럼 도시락드세요?",
          "original_form": "그럼 도시락드세요?",
          "speaker_id": "1",
          "time": "20210908 03:17"
        },
        {
          "id": "MDRW2100006610.1.10",
          "form": "회사 식당에서 먹습니다",
          "original_form": "회사 식당에서 먹습니다",
          "speaker_id": "2",
          "time": "20210908 05:31"
        },
        {
          "id": "MDRW2100006610.1.11",
          "form": "아!구내식당있음 편하겠어요!",
          "original_form": "아!구내식당있음 편하겠어요!",
          "speaker_id": "1",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100006610.1.12",
          "form": "네 고민할 필요가 없네요 ㅎㅎ",
          "original_form": "네 고민할 필요가 없네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100006610.1.13",
          "form": "고를수 있나요? 메뉴를??",
          "original_form": "고를수 있나요? 메뉴를??",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100006610.1.14",
          "form": "아니요..단일메뉴요..ㅠㅠ",
          "original_form": "아니요..단일메뉴요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100006610.1.15",
          "form": "그래도 공짜면...ㅜ",
          "original_form": "그래도 공짜면...ㅜ",
          "speaker_id": "1",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100006610.1.16",
          "form": "저흰 제공 안해요ㅋㅋ",
          "original_form": "저흰 제공 안해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100006610.1.17",
