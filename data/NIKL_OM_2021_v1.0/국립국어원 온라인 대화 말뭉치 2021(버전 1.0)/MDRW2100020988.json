      "utterance": [
        {
          "id": "MDRW2100020988.1.1",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020988.1.2",
          "form": "저 이상형 있었는데 만나던사람들이 다 그반대길래",
          "original_form": "저 이상형 있었는데 만나던사람들이 다 그반대길래",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020988.1.3",
          "form": "언젠가부턴 이상형도 없어지더라고요",
          "original_form": "언젠가부턴 이상형도 없어지더라고요",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020988.1.4",
          "form": "ㅋㅋ 만나면 그 사람이 이상형이 되는거죠ㅎㅎ",
          "original_form": "ㅋㅋ 만나면 그 사람이 이상형이 되는거죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020988.1.5",
          "form": "지금 연애중이신가요?",
          "original_form": "지금 연애중이신가요?",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020988.1.6",
          "form": "아뇨! 연애접었습니다",
          "original_form": "아뇨! 연애접었습니다",
          "speaker_id": "2",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100020988.1.7",
          "form": "이게왜나쁜말이람",
          "original_form": "이게왜나쁜말이람",
          "speaker_id": "2",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100020988.1.8",
          "form": "ㅋㅋ 혼자가 좋아요!!!",
          "original_form": "ㅋㅋ 혼자가 좋아요!!!",
          "speaker_id": "1",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020988.1.9",
          "form": "친구들 기혼이 다수인가요?",
          "original_form": "친구들 기혼이 다수인가요?",
          "speaker_id": "1",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020988.1.10",
          "form": "다수는아니고",
          "original_form": "다수는아니고",
          "speaker_id": "2",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020988.1.11",
          "form": "친구도하고",
          "original_form": "친구도하고",
          "speaker_id": "2",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020988.1.12",
          "form": "후배들이 결혼을 많이했어요 의외로",
          "original_form": "후배들이 결혼을 많이했어요 의외로",
          "speaker_id": "2",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020988.1.13",
          "form": "진짜요? 저흰 이제 미혼이 소수인데 대신 회사에서 다수가 미혼이라 다행이에요",
          "original_form": "진짜요? 저흰 이제 미혼이 소수인데 대신 회사에서 다수가 미혼이라 다행이에요",
          "speaker_id": "1",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100020988.1.14",
          "form": "결혼식 가기 싫어요",
          "original_form": "결혼식 가기 싫어요",
          "speaker_id": "2",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100020988.1.15",
          "form": "저는 안할건데",
          "original_form": "저는 안할건데",
          "speaker_id": "2",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100020988.1.16",
          "form": "비혼식이라도 해야 회수가될판,,,",
          "original_form": "비혼식이라도 해야 회수가될판,,,",
          "speaker_id": "2",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100020988.1.17",
          "form": "저도 비혼!!!",
          "original_form": "저도 비혼!!!",
          "speaker_id": "1",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100020988.1.18",
          "form": "아니 저도 나쁜 말ㅋㅋ 나왔어요ㅋㅋ",
          "original_form": "아니 저도 나쁜 말ㅋㅋ 나왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100020988.1.19",
          "form": "비혼들이 많아졌으면 합니다",
          "original_form": "비혼들이 많아졌으면 합니다",
          "speaker_id": "1",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100020988.1.20",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "2",
          "time": "20210828 14:55"
        },
        {
          "id": "MDRW2100020988.1.21",
          "form": "이성결혼하면",
          "original_form": "이성결혼하면",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100020988.1.22",
          "form": "대게 여자손해같아요ㅎ",
          "original_form": "대게 여자손해같아요ㅎ",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100020988.1.23",
          "form": "ㅋㅋ그러니까요 그 손해를 모두 감수하고도 사랑하면 모르겠지만ㅋㅋ 지금까지 그런 사랑 없었어요ㅋㅋ",
          "original_form": "ㅋㅋ그러니까요 그 손해를 모두 감수하고도 사랑하면 모르겠지만ㅋㅋ 지금까지 그런 사랑 없었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100020988.1.24",
          "form": "사랑은 호르몬의 장난",
          "original_form": "사랑은 호르몬의 장난",
          "speaker_id": "2",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100020988.1.25",
          "form": "ㅋㅋ 호르몬은 순간이죠ㅋㅋ",
          "original_form": "ㅋㅋ 호르몬은 순간이죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100020988.1.26",
          "form": "호르몬의장난이 600일인가900일간대요ㅋㅋ",
          "original_form": "호르몬의장난이 600일인가900일간대요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100020988.1.27",
          "form": "그 이후엔 제정신으로 돌아오죠ㅋㅋ",
          "original_form": "그 이후엔 제정신으로 돌아오죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100020988.1.28",
          "form": "후회만 남는~!",
          "original_form": "후회만 남는~!",
          "speaker_id": "2",
          "time": "20210828 15:59"
        }
      ]
