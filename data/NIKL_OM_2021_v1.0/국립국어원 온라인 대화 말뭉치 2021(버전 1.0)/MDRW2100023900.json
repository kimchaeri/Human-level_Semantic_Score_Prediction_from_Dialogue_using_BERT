      "utterance": [
        {
          "id": "MDRW2100023900.1.1",
          "form": "잔치국수 안먹은지 좀됐네",
          "original_form": "잔치국수 안먹은지 좀됐네",
          "speaker_id": "2",
          "time": "20210830 19:56"
        },
        {
          "id": "MDRW2100023900.1.2",
          "form": "맛있었게따 ㅋㅋ",
          "original_form": "맛있었게따 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:56"
        },
        {
          "id": "MDRW2100023900.1.3",
          "form": "국물은 짭쪼롬했는데 면이 간이 없어 괜찮더라",
          "original_form": "국물은 짭쪼롬했는데 면이 간이 없어 괜찮더라",
          "speaker_id": "1",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100023900.1.4",
          "form": "생김치도 간없어서 먹기좋더라ㅎ",
          "original_form": "생김치도 간없어서 먹기좋더라ㅎ",
          "speaker_id": "1",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100023900.1.5",
          "form": "오 생김치까지 제대로 잔치국수 한상이네",
          "original_form": "오 생김치까지 제대로 잔치국수 한상이네",
          "speaker_id": "2",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100023900.1.6",
          "form": "저녁먹을까 말까 생각했는데\n식당가서 조금먹어야지 했지만ㅋ 다먹는 폭식했지ㅎㅎ",
          "original_form": "저녁먹을까 말까 생각했는데\n식당가서 조금먹어야지 했지만ㅋ 다먹는 폭식했지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023900.1.7",
          "form": "넌 저녁 먹었어?",
          "original_form": "넌 저녁 먹었어?",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023900.1.8",
          "form": "나도 저녁안먹을랬는데 먹음",
          "original_form": "나도 저녁안먹을랬는데 먹음",
          "speaker_id": "2",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100023900.1.9",
          "form": "오늘은 콩나물이랑 스팸구이",
          "original_form": "오늘은 콩나물이랑 스팸구이",
          "speaker_id": "2",
          "time": "20210830 21:05"
        },
        {
          "id": "MDRW2100023900.1.10",
          "form": "맛나것다\n스팸 먹은지 오래ㅎ",
          "original_form": "맛나것다\n스팸 먹은지 오래ㅎ",
          "speaker_id": "1",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100023900.1.11",
          "form": "갖구운거 진짜 맛있자나",
          "original_form": "갖구운거 진짜 맛있자나",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023900.1.12",
          "form": "ㅇㅇ나도 엄청 싸길래 오랜만에 샀었어",
          "original_form": "ㅇㅇ나도 엄청 싸길래 오랜만에 샀었어",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023900.1.13",
          "form": "진짜 사면 살찌는데 넘 싸서 안살수가없었지",
          "original_form": "진짜 사면 살찌는데 넘 싸서 안살수가없었지",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023900.1.14",
          "form": "잘산거지 유통기한도 긴데ㅎ",
          "original_form": "잘산거지 유통기한도 긴데ㅎ",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100023900.1.15",
          "form": "ㅇㅇ유통기한 긴데 천천히 먹어야겠어",
          "original_form": "ㅇㅇ유통기한 긴데 천천히 먹어야겠어",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100023900.1.16",
          "form": "그니께 한번씩 먹음 더 맛나고 좋을거같아",
          "original_form": "그니께 한번씩 먹음 더 맛나고 좋을거같아",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100023900.1.17",
          "form": "나 과식햇나 배가 넘 부르다",
          "original_form": "나 과식햇나 배가 넘 부르다",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100023900.1.18",
          "form": "스팸 먹음 과식하게됨ㅜㅜ",
          "original_form": "스팸 먹음 과식하게됨ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100023900.1.19",
          "form": "나도 배불\n오늘은 퇴근 후 계속 움직이고 있는데도",
          "original_form": "나도 배불\n오늘은 퇴근 후 계속 움직이고 있는데도",
          "speaker_id": "1",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100023900.1.20",
          "form": "확실히 저녁은 거의안먹듯이가 좋은거아는데 안돼너ㅋ",
          "original_form": "확실히 저녁은 거의안먹듯이가 좋은거아는데 안돼너ㅋ",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100023900.1.21",
          "form": "마져 ㅋㅋ 그래도 좋은저녁이었다 낼 아침은 씨리얼 먹을까싶네",
          "original_form": "마져 ㅋㅋ 그래도 좋은저녁이었다 낼 아침은 씨리얼 먹을까싶네",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100023900.1.22",
          "form": "점심 먹었어?",
          "original_form": "점심 먹었어?",
          "speaker_id": "2",
          "time": "20210831 12:07"
        }
      ]
    }
  ]
