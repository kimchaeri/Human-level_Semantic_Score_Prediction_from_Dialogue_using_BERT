      "utterance": [
        {
          "id": "MDRW2100033441.1.1",
          "form": "아르바이트 해보셨나요?",
          "original_form": "아르바이트 해보셨나요?",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100033441.1.2",
          "form": "카페랑 시청알바 해봤어요ㅎㅎ",
          "original_form": "카페랑 시청알바 해봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100033441.1.3",
          "form": "시청알바 혹시 대학생 아르바이트 신청인가요?",
          "original_form": "시청알바 혹시 대학생 아르바이트 신청인가요?",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100033441.1.4",
          "form": "네 대학생때 했었어요ㅎㅎ",
          "original_form": "네 대학생때 했었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100033441.1.5",
          "form": "저는 한 번 도 당첨 못 해봤어요ㅠㅠ",
          "original_form": "저는 한 번 도 당첨 못 해봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100033441.1.6",
          "form": "맞아요 경쟁률이ㅋㅋㅋ저도 딱 한번 해봤어요ㅎㅎ",
          "original_form": "맞아요 경쟁률이ㅋㅋㅋ저도 딱 한번 해봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100033441.1.7",
          "form": "꿀 알바라고 해서 꼭 해보고 싶었는데ㅜ 기회가 좀처럼 안 오더라구요...대단하세요!",
          "original_form": "꿀 알바라고 해서 꼭 해보고 싶었는데ㅜ 기회가 좀처럼 안 오더라구요...대단하세요!",
          "speaker_id": "2",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100033441.1.8",
          "form": "근데 막상 할일은 없어서 직원분들이 애쓰시더라구요ㅋㅋ",
          "original_form": "근데 막상 할일은 없어서 직원분들이 애쓰시더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100033441.1.9",
          "form": "그래서 더 좋은 꿀알바가고 생각해요ㅋㅌㅋㅋ",
          "original_form": "그래서 더 좋은 꿀알바가고 생각해요ㅋㅌㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:57"
        },
        {
          "id": "MDRW2100033441.1.10",
          "form": "아 그렇죠ㅋㅋㅋ 근데 그래서 쓰레기만 줍고 눈치웠어요ㅜㅜㅋㅋㅋ",
          "original_form": "아 그렇죠ㅋㅋㅋ 근데 그래서 쓰레기만 줍고 눈치웠어요ㅜㅜㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:58"
        },
        {
          "id": "MDRW2100033441.1.11",
          "form": "카페 아르바이트는 업무 스트레스 많이 받지 않으셨나요ㅠ?",
          "original_form": "카페 아르바이트는 업무 스트레스 많이 받지 않으셨나요ㅠ?",
          "speaker_id": "2",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033441.1.12",
          "form": "네 사람대하는게 제일 힘들었어요ㅎㅎ",
          "original_form": "네 사람대하는게 제일 힘들었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100033441.1.13",
          "form": "진상 손님들이 스트레스였겠어요",
          "original_form": "진상 손님들이 스트레스였겠어요",
          "speaker_id": "2",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100033441.1.14",
          "form": "네맞아요ㅠㅠ",
          "original_form": "네맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 23:06"
        }
      ]
