      "utterance": [
        {
          "id": "MDRW2100009537.1.1",
          "form": "하도 앞에 캐릭터가",
          "original_form": "하도 앞에 캐릭터가",
          "speaker_id": "1",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009537.1.2",
          "form": "강했던지라ㅋㅋ",
          "original_form": "강했던지라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009537.1.3",
          "form": "유부녀",
          "original_form": "유부녀",
          "speaker_id": "1",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009537.1.4",
          "form": "그니까요ㅎ",
          "original_form": "그니까요ㅎ",
          "speaker_id": "2",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009537.1.5",
          "form": "주단태 일본 정신병원 들어간거죠?",
          "original_form": "주단태 일본 정신병원 들어간거죠?",
          "speaker_id": "2",
          "time": "20210823 00:19"
        },
        {
          "id": "MDRW2100009537.1.6",
          "form": "네ㅋㅋ 그 백준기라는이름으로",
          "original_form": "네ㅋㅋ 그 백준기라는이름으로",
          "speaker_id": "1",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100009537.1.7",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100009537.1.8",
          "form": "원래 자기의이름",
          "original_form": "원래 자기의이름",
          "speaker_id": "1",
          "time": "20210823 00:21"
        },
        {
          "id": "MDRW2100009537.1.9",
          "form": "주석경이 보낸거예요?",
          "original_form": "주석경이 보낸거예요?",
          "speaker_id": "2",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009537.1.10",
          "form": "아니요~ 로건이랑 심수련이요",
          "original_form": "아니요~ 로건이랑 심수련이요",
          "speaker_id": "1",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009537.1.11",
          "form": "주석경은 바다에 뛰어들어 따라왔더라구요",
          "original_form": "주석경은 바다에 뛰어들어 따라왔더라구요",
          "speaker_id": "1",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009537.1.12",
          "form": "주단태보려구요?",
          "original_form": "주단태보려구요?",
          "speaker_id": "2",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009537.1.13",
          "form": "일전에 아빠가 자길 가둔적이있어서",
          "original_form": "일전에 아빠가 자길 가둔적이있어서",
          "speaker_id": "1",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009537.1.14",
          "form": "한방 먹이려고",
          "original_form": "한방 먹이려고",
          "speaker_id": "1",
          "time": "20210823 00:22"
        },
        {
          "id": "MDRW2100009537.1.15",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009537.1.16",
          "form": "그배 출발하는거 브고",
          "original_form": "그배 출발하는거 브고",
          "speaker_id": "1",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009537.1.17",
          "form": "자기딸아니라서 폭력행사했나보네요",
          "original_form": "자기딸아니라서 폭력행사했나보네요",
          "speaker_id": "2",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009537.1.18",
          "form": "수영해서ㅋㅋ",
          "original_form": "수영해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009537.1.19",
          "form": "대단한데요ㅎㅎ",
          "original_form": "대단한데요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009537.1.20",
          "form": "네 그 유학보내준다고 거짓말치고",
          "original_form": "네 그 유학보내준다고 거짓말치고",
          "speaker_id": "1",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009537.1.21",
          "form": "그 고아들? 가둬놓고 기도원인가그런데보냈더라구요",
          "original_form": "그 고아들? 가둬놓고 기도원인가그런데보냈더라구요",
          "speaker_id": "1",
          "time": "20210823 00:23"
        },
        {
          "id": "MDRW2100009537.1.22",
          "form": "주단태는 진짜악마네요",
          "original_form": "주단태는 진짜악마네요",
          "speaker_id": "2",
          "time": "20210823 00:24"
        },
        {
          "id": "MDRW2100009537.1.23",
          "form": "단물다 빠지니",
          "original_form": "단물다 빠지니",
          "speaker_id": "1",
          "time": "20210823 00:24"
        },
        {
          "id": "MDRW2100009537.1.24",
          "form": "그장면은 좀 봤는데",
          "original_form": "그장면은 좀 봤는데",
          "speaker_id": "2",
          "time": "20210823 00:24"
        },
        {
          "id": "MDRW2100009537.1.25",
          "form": "그래서 복수",
          "original_form": "그래서 복수",
          "speaker_id": "1",
          "time": "20210823 00:24"
        }
      ]
    }
