      "utterance": [
        {
          "id": "MDRW2100024038.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024038.1.2",
          "form": "별로 안좋아하는데 요즘엔 가고싶기도하네요ㅋㅋ",
          "original_form": "별로 안좋아하는데 요즘엔 가고싶기도하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024038.1.3",
          "form": "요즘시기에 더간절한거같긴해요ㅠ",
          "original_form": "요즘시기에 더간절한거같긴해요ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024038.1.4",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024038.1.5",
          "form": "코로나 풀리면 가고싶은곳 있으세요?",
          "original_form": "코로나 풀리면 가고싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100024038.1.6",
          "form": "해외요!",
          "original_form": "해외요!",
          "speaker_id": "2",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100024038.1.7",
          "form": "일본ㅋㅋㅋ",
          "original_form": "일본ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100024038.1.8",
          "form": "오ㅋㅋㅋ 저도요ㅋㅋㅋ",
          "original_form": "오ㅋㅋㅋ 저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024038.1.9",
          "form": "어떤계절에 가고싶으세요?",
          "original_form": "어떤계절에 가고싶으세요?",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024038.1.10",
          "form": "늦가을쯤이요 덥지않게?",
          "original_form": "늦가을쯤이요 덥지않게?",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024038.1.11",
          "form": "아 좋죠 좋죠",
          "original_form": "아 좋죠 좋죠",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024038.1.12",
          "form": "저는 봄에 가보고 싶어요ㅋㅋ",
          "original_form": "저는 봄에 가보고 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024038.1.13",
          "form": "축제많을시기네요",
          "original_form": "축제많을시기네요",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024038.1.14",
          "form": "청춘들의 벚꽃을 보고 싶어용ㅋㅋ",
          "original_form": "청춘들의 벚꽃을 보고 싶어용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024038.1.15",
          "form": "오 좋죠",
          "original_form": "오 좋죠",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024038.1.16",
          "form": "그맘때 벚꽃주를 판다고해서",
          "original_form": "그맘때 벚꽃주를 판다고해서",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024038.1.17",
          "form": "엿보고싶은..ㅋㅋㅋ",
          "original_form": "엿보고싶은..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024038.1.18",
          "form": "저도 가서 사보고싶네요",
          "original_form": "저도 가서 사보고싶네요",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024038.1.19",
          "form": "아 들었어요!",
          "original_form": "아 들었어요!",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024038.1.20",
          "form": "앗..ㅋㅋㅋ",
          "original_form": "앗..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024038.1.21",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024038.1.22",
          "form": "저도용",
          "original_form": "저도용",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024038.1.23",
          "form": "근데 관상용이라던데ㅋㅋ",
          "original_form": "근데 관상용이라던데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024038.1.24",
          "form": "음...그건맞아요ㅋㅋㅋ",
          "original_form": "음...그건맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024038.1.25",
          "form": "축제도 가고싶어요ㅋㅋ",
          "original_form": "축제도 가고싶어요ㅋㅋ",
          "speaker_id": "1",
