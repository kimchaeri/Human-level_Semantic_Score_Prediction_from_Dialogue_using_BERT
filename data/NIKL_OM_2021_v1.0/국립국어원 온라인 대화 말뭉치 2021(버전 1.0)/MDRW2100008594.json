      "utterance": [
        {
          "id": "MDRW2100008594.1.1",
          "form": "버거킹앱할인이 많이돼요?",
          "original_form": "버거킹앱할인이 많이돼요?",
          "speaker_id": "1",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008594.1.2",
          "form": "1+1 있어서 요긴했어요",
          "original_form": "1+1 있어서 요긴했어요",
          "speaker_id": "2",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008594.1.3",
          "form": "특히 감자튀김 라지가 1+1",
          "original_form": "특히 감자튀김 라지가 1+1",
          "speaker_id": "2",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008594.1.4",
          "form": "오 앱에서 쿠폰줘요?",
          "original_form": "오 앱에서 쿠폰줘요?",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008594.1.5",
          "form": "쿠폰아니라 그냥 앱깔면 행사 상품으로 떠요",
          "original_form": "쿠폰아니라 그냥 앱깔면 행사 상품으로 떠요",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008594.1.6",
          "form": "아 카톡 플친에서 광고오는거 그런거요?",
          "original_form": "아 카톡 플친에서 광고오는거 그런거요?",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008594.1.7",
          "form": "버거킹은 쿠폰 많이줘서 제값주고 못먹겠어요",
          "original_form": "버거킹은 쿠폰 많이줘서 제값주고 못먹겠어요",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008594.1.8",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008594.1.9",
          "form": "핫딜도 많이뜨고",
          "original_form": "핫딜도 많이뜨고",
          "speaker_id": "2",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008594.1.10",
          "form": "4900원세트도있고요",
          "original_form": "4900원세트도있고요",
          "speaker_id": "2",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008594.1.11",
          "form": "롯데리아나 맥도날드는 그런쿠폰은 없어서",
          "original_form": "롯데리아나 맥도날드는 그런쿠폰은 없어서",
          "speaker_id": "1",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008594.1.12",
          "form": "맥도날드는 올데이 해요?",
          "original_form": "맥도날드는 올데이 해요?",
          "speaker_id": "1",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008594.1.13",
          "form": "하긴하는거같은데",
          "original_form": "하긴하는거같은데",
          "speaker_id": "2",
          "time": "20210822 17:05"
        },
        {
          "id": "MDRW2100008594.1.14",
          "form": "버거킹보다 못한거같아요",
          "original_form": "버거킹보다 못한거같아요",
          "speaker_id": "2",
          "time": "20210822 17:05"
        },
        {
          "id": "MDRW2100008594.1.15",
          "form": "최근 산 물건중 자랑하고 싶으신 것 있으세요?",
          "original_form": "최근 산 물건중 자랑하고 싶으신 것 있으세요?",
          "speaker_id": "2",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008594.1.16",
          "form": "요즘 먹을것만 사서ㅎㅎ",
          "original_form": "요즘 먹을것만 사서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008594.1.17",
          "form": "아 아까 집업후드하나 잤어요",
          "original_form": "아 아까 집업후드하나 잤어요",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008594.1.18",
          "form": "쿠폰쓰고 적립금쓰고 아주싸게ㅋ",
          "original_form": "쿠폰쓰고 적립금쓰고 아주싸게ㅋ",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008594.1.19",
          "form": "쇼핑좋아하세요?",
          "original_form": "쇼핑좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008594.1.20",
          "form": "저는 좀 그 과정을 즐기는편이에요",
          "original_form": "저는 좀 그 과정을 즐기는편이에요",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008594.1.21",
          "form": "당근마켓 깔아놓고 키워드 뜨면 구경하는?",
          "original_form": "당근마켓 깔아놓고 키워드 뜨면 구경하는?",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008594.1.22",
          "form": "예전엔 구경하는것도 좋아했는데 나이먹으니 귀찮아서ㅋ",
          "original_form": "예전엔 구경하는것도 좋아했는데 나이먹으니 귀찮아서ㅋ",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008594.1.23",
          "form": "아 중고거래로도 사시나보네요",
          "original_form": "아 중고거래로도 사시나보네요",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008594.1.24",
          "form": "당근에서는 몇개 팔아보고 토익책 사봤어요 ㅎㅎ",
          "original_form": "당근에서는 몇개 팔아보고 토익책 사봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008594.1.25",
          "form": "보통 어디사이트에서 쇼핑하세요?",
          "original_form": "보통 어디사이트에서 쇼핑하세요?",
          "speaker_id": "2",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008594.1.26",
          "form": "엇 토익책 전 팔아봤는데ㅋ",
          "original_form": "엇 토익책 전 팔아봤는데ㅋ",
          "speaker_id": "1",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008594.1.27",
          "form": "저도 당근마켓도 하구 주로사는건 지마켓이나 11번가",
          "original_form": "저도 당근마켓도 하구 주로사는건 지마켓이나 11번가",
          "speaker_id": "1",
          "time": "20210822 17:14"
        },
        {
          "id": "MDRW2100008594.1.28",
          "form": "최근 구매 물건으로는 아 손목시계샀어요",
          "original_form": "최근 구매 물건으로는 아 손목시계샀어요",
          "speaker_id": "2",
          "time": "20210822 17:17"
        },
        {
          "id": "MDRW2100008594.1.29",
          "form": "메탈시계요?",
          "original_form": "메탈시계요?",
          "speaker_id": "1",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008594.1.30",
          "form": "아뇨그냥 이걸뭐라하지",
          "original_form": "아뇨그냥 이걸뭐라하지",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008594.1.31",
          "form": "플라스틱은 아니고",
          "original_form": "플라스틱은 아니고",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008594.1.32",
          "form": "우레탄 밴드요",
          "original_form": "우레탄 밴드요",
          "speaker_id": "2",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008594.1.33",
          "form": "아 ㅎ 시계하나 저도 사고싶은데",
          "original_form": "아 ㅎ 시계하나 저도 사고싶은데",
          "speaker_id": "1",
          "time": "20210822 17:21"
        }
