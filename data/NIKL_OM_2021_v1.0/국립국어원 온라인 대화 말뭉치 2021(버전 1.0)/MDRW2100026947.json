      "utterance": [
        {
          "id": "MDRW2100026947.1.1",
          "form": "안녕하세요 좋은 아침입니다ㅎㅎ",
          "original_form": "안녕하세요 좋은 아침입니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 09:04"
        },
        {
          "id": "MDRW2100026947.1.2",
          "form": "반가워요 전 방금 일어났네여",
          "original_form": "반가워요 전 방금 일어났네여",
          "speaker_id": "1",
          "time": "20210903 09:05"
        },
        {
          "id": "MDRW2100026947.1.3",
          "form": "아하 저는 오늘 새벽예배 가느라 5시에 일어났어요",
          "original_form": "아하 저는 오늘 새벽예배 가느라 5시에 일어났어요",
          "speaker_id": "2",
          "time": "20210903 09:05"
        },
        {
          "id": "MDRW2100026947.1.4",
          "form": "헉 부지런하시세요",
          "original_form": "헉 부지런하시세요",
          "speaker_id": "1",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100026947.1.5",
          "form": "교회 다니시는구나 ㅎㅎ 저도 교회 다녀요",
          "original_form": "교회 다니시는구나 ㅎㅎ 저도 교회 다녀요",
          "speaker_id": "1",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100026947.1.6",
          "form": "오오 반가워요~",
          "original_form": "오오 반가워요~",
          "speaker_id": "2",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100026947.1.7",
          "form": "이렇게 교인분을 만나네용 ㅎㅎ",
          "original_form": "이렇게 교인분을 만나네용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100026947.1.8",
          "form": "새벽예배는 매일 가시는 거에요?",
          "original_form": "새벽예배는 매일 가시는 거에요?",
          "speaker_id": "1",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100026947.1.9",
          "form": "아니요 매주 금요일만 갑니다",
          "original_form": "아니요 매주 금요일만 갑니다",
          "speaker_id": "2",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100026947.1.10",
          "form": "아하 그렇구나 전 월요일 저녁 예배랑 주일 예배 가요",
          "original_form": "아하 그렇구나 전 월요일 저녁 예배랑 주일 예배 가요",
          "speaker_id": "1",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100026947.1.11",
          "form": "주일에 예배 끝나고 다른 성도들이랑 교제도 하고용",
          "original_form": "주일에 예배 끝나고 다른 성도들이랑 교제도 하고용",
          "speaker_id": "1",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100026947.1.12",
          "form": "아 저도요ㅋㅋㅋㅋㅋ 예배 끝나고 다른 지체분들이랑 교류하는 것도 즐겁죠",
          "original_form": "아 저도요ㅋㅋㅋㅋㅋ 예배 끝나고 다른 지체분들이랑 교류하는 것도 즐겁죠",
          "speaker_id": "2",
          "time": "20210903 09:08"
        },
        {
          "id": "MDRW2100026947.1.13",
          "form": "맞아요 교회 식구들과도 좋은 관계 유지하면서 서로 기도해주고 하는 게 너무 좋아요",
          "original_form": "맞아요 교회 식구들과도 좋은 관계 유지하면서 서로 기도해주고 하는 게 너무 좋아요",
          "speaker_id": "1",
          "time": "20210903 09:09"
        },
        {
          "id": "MDRW2100026947.1.14",
          "form": "공감해요 서로 기도해주는게 의지가 많이 돼요",
          "original_form": "공감해요 서로 기도해주는게 의지가 많이 돼요",
          "speaker_id": "2",
          "time": "20210903 09:10"
        },
        {
          "id": "MDRW2100026947.1.15",
          "form": "아 그리고 저는 청소년부 교사라서 다다음주 수련회 준비하고 있어요",
          "original_form": "아 그리고 저는 청소년부 교사라서 다다음주 수련회 준비하고 있어요",
          "speaker_id": "2",
          "time": "20210903 09:10"
        },
        {
          "id": "MDRW2100026947.1.16",
          "form": "아 청소년부 쌤이시구나 고생이 많으시네요",
          "original_form": "아 청소년부 쌤이시구나 고생이 많으시네요",
          "speaker_id": "1",
          "time": "20210903 09:11"
        },
        {
          "id": "MDRW2100026947.1.17",
          "form": "수련회는 온라인으로 드리나요?",
          "original_form": "수련회는 온라인으로 드리나요?",
          "speaker_id": "1",
          "time": "20210903 09:11"
        },
        {
          "id": "MDRW2100026947.1.18",
          "form": "네 온라인으로 드려요",
          "original_form": "네 온라인으로 드려요",
          "speaker_id": "2",
          "time": "20210903 09:11"
        }
      ]
