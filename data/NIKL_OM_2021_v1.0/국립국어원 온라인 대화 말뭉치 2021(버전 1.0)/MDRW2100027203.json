        },
        {
          "id": "MDRW2100027203.1.2",
          "form": "하죠",
          "original_form": "하죠",
          "speaker_id": "2",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027203.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027203.1.4",
          "form": "청소는 귀찮죠 특히 욕실",
          "original_form": "청소는 귀찮죠 특히 욕실",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027203.1.5",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027203.1.6",
          "form": "네 가급적 깨끗이 써요",
          "original_form": "네 가급적 깨끗이 써요",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027203.1.7",
          "form": "그래도 곰팡이걱정은요",
          "original_form": "그래도 곰팡이걱정은요",
          "speaker_id": "1",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100027203.1.8",
          "form": "쓰고 나서 바로 물기닦아요\n그래도 시간지나면 생기긴 하더라고요",
          "original_form": "쓰고 나서 바로 물기닦아요\n그래도 시간지나면 생기긴 하더라고요",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027203.1.9",
          "form": "락스써요",
          "original_form": "락스써요",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
