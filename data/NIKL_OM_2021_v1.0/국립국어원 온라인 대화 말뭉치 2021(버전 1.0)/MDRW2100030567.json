      "utterance": [
        {
          "id": "MDRW2100030567.1.1",
          "form": "날씨좋네요",
          "original_form": "날씨좋네요",
          "speaker_id": "1",
          "time": "20210904 09:01"
        },
        {
          "id": "MDRW2100030567.1.2",
          "form": "화창하고 구름예뻐요",
          "original_form": "화창하고 구름예뻐요",
          "speaker_id": "2",
          "time": "20210904 09:01"
        },
        {
          "id": "MDRW2100030567.1.3",
          "form": "날씨가 놀러가기딱이죠",
          "original_form": "날씨가 놀러가기딱이죠",
          "speaker_id": "1",
          "time": "20210904 09:01"
        },
        {
          "id": "MDRW2100030567.1.4",
          "form": "맞아요.캠핑의계절이왔네요",
          "original_form": "맞아요.캠핑의계절이왔네요",
          "speaker_id": "2",
          "time": "20210904 09:01"
        },
        {
          "id": "MDRW2100030567.1.5",
          "form": "캠핑좋아하세요?",
          "original_form": "캠핑좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100030567.1.6",
          "form": "네..완전좋아하죠",
          "original_form": "네..완전좋아하죠",
          "speaker_id": "1",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100030567.1.7",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100030567.1.8",
          "form": "아이들이제일좋아해요",
          "original_form": "아이들이제일좋아해요",
          "speaker_id": "1",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100030567.1.9",
          "form": "이런날씨에 딱이죠",
          "original_form": "이런날씨에 딱이죠",
          "speaker_id": "2",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100030567.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100030567.1.11",
          "form": "겨울은 너무춥고 여름은덥고",
          "original_form": "겨울은 너무춥고 여름은덥고",
          "speaker_id": "1",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100030567.1.12",
          "form": "저도빨리가고싶네요",
          "original_form": "저도빨리가고싶네요",
          "speaker_id": "2",
          "time": "20210904 09:03"
        },
        {
          "id": "MDRW2100030567.1.13",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210904 09:03"
        },
        {
          "id": "MDRW2100030567.1.14",
          "form": "어디가좋은가요?",
          "original_form": "어디가좋은가요?",
          "speaker_id": "1",
          "time": "20210904 09:03"
        },
        {
          "id": "MDRW2100030567.1.15",
          "form": "저는 가까운 논산상상플러스마당추천요",
          "original_form": "저는 가까운 논산상상플러스마당추천요",
          "speaker_id": "2",
          "time": "20210904 09:03"
        },
        {
          "id": "MDRW2100030567.1.16",
          "form": "좋아요?",
          "original_form": "좋아요?",
          "speaker_id": "1",
          "time": "20210904 09:10"
        },
        {
          "id": "MDRW2100030567.1.17",
          "form": "깨끗하고 화장실도 좋고  넘좋더군요",
          "original_form": "깨끗하고 화장실도 좋고  넘좋더군요",
          "speaker_id": "2",
          "time": "20210904 09:10"
        },
        {
          "id": "MDRW2100030567.1.18",
          "form": "저녁되니 춥네요..",
          "original_form": "저녁되니 춥네요..",
          "speaker_id": "1",
