      "utterance": [
        {
          "id": "MDRW2100004512.1.1",
          "form": "안녕하세요 하시는 모임 있으신가요?",
          "original_form": "안녕하세요 하시는 모임 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004512.1.2",
          "form": "저는 운동 모임 하고 있습니다!",
          "original_form": "저는 운동 모임 하고 있습니다!",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004512.1.3",
          "form": "저도 산악회 하고 있어요!",
          "original_form": "저도 산악회 하고 있어요!",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004512.1.4",
          "form": "긍정적인 영향을 줘서 좋더라고요",
          "original_form": "긍정적인 영향을 줘서 좋더라고요",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004512.1.5",
          "form": "어떤 운동 하시나요?",
          "original_form": "어떤 운동 하시나요?",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004512.1.6",
          "form": "오 산악회 멋지네요",
          "original_form": "오 산악회 멋지네요",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004512.1.7",
          "form": "저는 축구 합니다!",
          "original_form": "저는 축구 합니다!",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004512.1.8",
          "form": "서툴지만 재밌어요",
          "original_form": "서툴지만 재밌어요",
          "speaker_id": "2",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004512.1.9",
          "form": "산악회 하시면 산 많이 타셨겠네요",
          "original_form": "산악회 하시면 산 많이 타셨겠네요",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004512.1.10",
          "form": "축구! 저 요새 골때리는 그녀들 보는데 정말 멋있더라구요",
          "original_form": "축구! 저 요새 골때리는 그녀들 보는데 정말 멋있더라구요",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004512.1.11",
          "form": "몇대 몇으로 하세요?",
          "original_form": "몇대 몇으로 하세요?",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004512.1.12",
          "form": "저 코로나때문에 산에 많이는 못 갔는데 얼마전에 한라산을 다녀왔어요!",
          "original_form": "저 코로나때문에 산에 많이는 못 갔는데 얼마전에 한라산을 다녀왔어요!",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004512.1.13",
          "form": "와 골때리는그녀 보시는군요!",
          "original_form": "와 골때리는그녀 보시는군요!",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004512.1.14",
          "form": "저 그거보고 축구 시작했어요 ㅋㅋ",
          "original_form": "저 그거보고 축구 시작했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004512.1.15",
          "form": "와 한라산 대단하신데요? 한번도 올라가볼 생각도 못한...",
          "original_form": "와 한라산 대단하신데요? 한번도 올라가볼 생각도 못한...",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004512.1.16",
          "form": "엌ㅋㅋㅋ 저도 그거 지금 되게 열심히보고있어요 개벤져스 너무 아쉬워요 ㅜㅜ",
          "original_form": "엌ㅋㅋㅋ 저도 그거 지금 되게 열심히보고있어요 개벤져스 너무 아쉬워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004512.1.17",
          "form": "산악회 아니었으면 한라산 혼자서는 못 갔을 것 같아요 ㅎㅎ 모임의 힘이 큰 것 같아요",
          "original_form": "산악회 아니었으면 한라산 혼자서는 못 갔을 것 같아요 ㅎㅎ 모임의 힘이 큰 것 같아요",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004512.1.18",
          "form": "맞아요 모임 없었으면 평생 축구해볼 생각도 못했을 것 같아요",
          "original_form": "맞아요 모임 없었으면 평생 축구해볼 생각도 못했을 것 같아요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004512.1.19",
          "form": "모임 있는 게 너무 감사하더라고요ㅋㅋ",
          "original_form": "모임 있는 게 너무 감사하더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004512.1.20",
          "form": "관심있는 다른 동호회 있으신가요? 저는 생활영어 모임 좀 가져보고 싶네요 ㅎ",
          "original_form": "관심있는 다른 동호회 있으신가요? 저는 생활영어 모임 좀 가져보고 싶네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004512.1.21",
          "form": "영어도 되게 생산적이고 좋은거같아요!",
          "original_form": "영어도 되게 생산적이고 좋은거같아요!",
          "speaker_id": "1",
          "time": "20210821 20:08"
        },
        {
          "id": "MDRW2100004512.1.22",
          "form": "특히 회화는 혼자 하면 제대로 못하니까",
          "original_form": "특히 회화는 혼자 하면 제대로 못하니까",
          "speaker_id": "1",
          "time": "20210821 20:08"
        },
        {
          "id": "MDRW2100004512.1.23",
          "form": "영어회화는 평생의 꿈이라 빨리 찾아봐야겠어요 ㅎㅎ",
          "original_form": "영어회화는 평생의 꿈이라 빨리 찾아봐야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100004512.1.24",
          "form": "대체 언제쯤 유창한 대화 가능한즤,,",
          "original_form": "대체 언제쯤 유창한 대화 가능한즤,,",
          "speaker_id": "2",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004512.1.25",
          "form": "영어 회화는 잘하면 진짜 쓸모가 많은거같아요",
          "original_form": "영어 회화는 잘하면 진짜 쓸모가 많은거같아요",
          "speaker_id": "1",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100004512.1.26",
          "form": "구직도 요새 영어 필수인 것 같더라구요",
          "original_form": "구직도 요새 영어 필수인 것 같더라구요",
          "speaker_id": "1",
          "time": "20210821 20:11"
        }
      ]
