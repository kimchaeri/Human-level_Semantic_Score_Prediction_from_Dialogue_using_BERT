        },
        {
          "id": "MDRW2100030017.1.2",
          "form": "두꺼비가 귀엽네요ㅎㅎㅎ",
          "original_form": "두꺼비가 귀엽네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100030017.1.3",
          "form": "진로 두꺼비 귀엽더라고요 ㅎㅎ",
          "original_form": "진로 두꺼비 귀엽더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100030017.1.4",
          "form": "카카오나 라인 캐릭터는 좋아하세요?",
          "original_form": "카카오나 라인 캐릭터는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100030017.1.5",
          "form": "어피치 좋아해용ㅋㅋㅋ",
          "original_form": "어피치 좋아해용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:47"
        },
        {
          "id": "MDRW2100030017.1.6",
          "form": "어피치도 인기많죠 카카오 이번에 미니골드랑 콜라보 했더라구요",
          "original_form": "어피치도 인기많죠 카카오 이번에 미니골드랑 콜라보 했더라구요",
          "speaker_id": "1",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100030017.1.7",
          "form": "앗 맞아요 너무귀엽더라구요ㅋㅋㅋ",
          "original_form": "앗 맞아요 너무귀엽더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030017.1.8",
          "form": "콜라보 제품 잘 사시나요?",
          "original_form": "콜라보 제품 잘 사시나요?",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100030017.1.9",
          "form": "저는 캐릭터 상품 많이 사요!!",
          "original_form": "저는 캐릭터 상품 많이 사요!!",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100030017.1.10",
          "form": "마블이나 디즈니요!!",
          "original_form": "마블이나 디즈니요!!",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100030017.1.11",
          "form": "오 전 라인이랑 카카오요",
          "original_form": "오 전 라인이랑 카카오요",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100030017.1.12",
          "form": "최근에 콜라보 상품 구매한 적 있나요?",
          "original_form": "최근에 콜라보 상품 구매한 적 있나요?",
          "speaker_id": "1",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030017.1.13",
          "form": "음 최근은 아니지맘",
          "original_form": "음 최근은 아니지맘",
          "speaker_id": "2",
          "time": "20210904 19:00"
        },
        {
          "id": "MDRW2100030017.1.14",
          "form": "생각나는 콜라보 상품은",
          "original_form": "생각나는 콜라보 상품은",
          "speaker_id": "2",
          "time": "20210904 19:00"
        },
        {
          "id": "MDRW2100030017.1.15",
          "form": "마블 표지로 나온 쎈이요...ㅋㅋㅋㅋㅋ",
          "original_form": "마블 표지로 나온 쎈이요...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:00"
        },
        {
          "id": "MDRW2100030017.1.16",
          "form": "그거 열심히 풀었던 기억이 나네요ㅋㅋ",
          "original_form": "그거 열심히 풀었던 기억이 나네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:01"
        },
        {
          "id": "MDRW2100030017.1.17",
          "form": "오 표지 보면 기분 좋았겠네요",
          "original_form": "오 표지 보면 기분 좋았겠네요",
          "speaker_id": "1",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100030017.1.18",
          "form": "ㅎㅎ표지만 뜯어서 붙여놨었어요ㅋㅋㅋ",
          "original_form": "ㅎㅎ표지만 뜯어서 붙여놨었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:51"
