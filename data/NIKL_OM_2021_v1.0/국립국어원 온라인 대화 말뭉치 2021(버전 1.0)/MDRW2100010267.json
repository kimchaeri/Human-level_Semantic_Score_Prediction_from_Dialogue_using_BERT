      "utterance": [
        {
          "id": "MDRW2100010267.1.1",
          "form": "요새 신 군복이 세련됐다던데요",
          "original_form": "요새 신 군복이 세련됐다던데요",
          "speaker_id": "1",
          "time": "20210823 16:18"
        },
        {
          "id": "MDRW2100010267.1.2",
          "form": "점점 발전하나보네요ㅎㅎ",
          "original_form": "점점 발전하나보네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:18"
        },
        {
          "id": "MDRW2100010267.1.3",
          "form": "네 제 남동생 군대갔다왔는데 휴대폰쓰더라구요",
          "original_form": "네 제 남동생 군대갔다왔는데 휴대폰쓰더라구요",
          "speaker_id": "1",
          "time": "20210823 16:19"
        },
        {
          "id": "MDRW2100010267.1.4",
          "form": "새로워요",
          "original_form": "새로워요",
          "speaker_id": "1",
          "time": "20210823 16:19"
        },
        {
          "id": "MDRW2100010267.1.5",
          "form": "아 맞아요 그렇다하더라구요ㅎㅎ 제동생은 공익이라 뭐가 다른지 잘모르겠어요ㅠㅠㅋㅋ",
          "original_form": "아 맞아요 그렇다하더라구요ㅎㅎ 제동생은 공익이라 뭐가 다른지 잘모르겠어요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:20"
        },
        {
          "id": "MDRW2100010267.1.6",
          "form": "ㅋㅋ 그렇군요",
          "original_form": "ㅋㅋ 그렇군요",
          "speaker_id": "1",
          "time": "20210823 16:20"
        },
        {
          "id": "MDRW2100010267.1.7",
          "form": "밴드방만들어서 어머니들 보시도록 하던데요",
          "original_form": "밴드방만들어서 어머니들 보시도록 하던데요",
          "speaker_id": "1",
          "time": "20210823 16:21"
        },
        {
          "id": "MDRW2100010267.1.8",
          "form": "생활사진올라오고 ㅋㅋ",
          "original_form": "생활사진올라오고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:21"
        },
        {
          "id": "MDRW2100010267.1.9",
          "form": "걱정하실텐데 가족은 맘이 그래도 놓일것같아요ㅎㅎ",
          "original_form": "걱정하실텐데 가족은 맘이 그래도 놓일것같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:21"
        },
        {
          "id": "MDRW2100010267.1.10",
          "form": "네 심리안정이 커졌대요 덕분에",
          "original_form": "네 심리안정이 커졌대요 덕분에",
          "speaker_id": "1",
          "time": "20210823 16:22"
        },
        {
          "id": "MDRW2100010267.1.11",
          "form": "시대가바뀌면 군대도 따라바뀌나봐요",
          "original_form": "시대가바뀌면 군대도 따라바뀌나봐요",
          "speaker_id": "1",
          "time": "20210823 16:23"
        },
        {
          "id": "MDRW2100010267.1.12",
          "form": "그렇죠ㅎㅎ다른것보다 부모님들이..참 맘고생하시니",
          "original_form": "그렇죠ㅎㅎ다른것보다 부모님들이..참 맘고생하시니",
          "speaker_id": "2",
          "time": "20210823 16:24"
        },
        {
          "id": "MDRW2100010267.1.13",
          "form": "한국군복이 좋아서",
          "original_form": "한국군복이 좋아서",
          "speaker_id": "1",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100010267.1.14",
          "form": "탈레반무장단체에 팔더라구요",
          "original_form": "탈레반무장단체에 팔더라구요",
          "speaker_id": "1",
          "time": "20210823 16:25"
        },
        {
          "id": "MDRW2100010267.1.15",
          "form": "탈레반 단체사진보면 셋은 한국군복입고있어요",
          "original_form": "탈레반 단체사진보면 셋은 한국군복입고있어요",
          "speaker_id": "1",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010267.1.16",
          "form": "세상에...별일이 다있네요",
          "original_form": "세상에...별일이 다있네요",
          "speaker_id": "2",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010267.1.17",
          "form": "뭐지..재질이 좋나요...ㅎㅎ",
          "original_form": "뭐지..재질이 좋나요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010267.1.18",
          "form": "그러게요..재활용이 좋긴한데ㅋ 저건아닌거같아요..ㅜ",
          "original_form": "그러게요..재활용이 좋긴한데ㅋ 저건아닌거같아요..ㅜ",
          "speaker_id": "1",
          "time": "20210823 16:26"
        },
        {
          "id": "MDRW2100010267.1.19",
          "form": "ㅠㅠ이상해요ㅋㅋㅋ",
          "original_form": "ㅠㅠ이상해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010267.1.20",
          "form": "산악지형이라 울나라랑 비슷하대요",
          "original_form": "산악지형이라 울나라랑 비슷하대요",
          "speaker_id": "1",
          "time": "20210823 16:27"
        }
      ]
