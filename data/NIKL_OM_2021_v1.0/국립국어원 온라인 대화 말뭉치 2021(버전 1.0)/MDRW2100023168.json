      "utterance": [
        {
          "id": "MDRW2100023168.1.1",
          "form": "전문의를 찾아가는게 좋지읺을까요",
          "original_form": "전문의를 찾아가는게 좋지읺을까요",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023168.1.2",
          "form": "공장형보다는 ㅋㅋㅋ",
          "original_form": "공장형보다는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023168.1.3",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023168.1.4",
          "form": "저 갑자기 생각 났는데",
          "original_form": "저 갑자기 생각 났는데",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023168.1.5",
          "form": "저 프락셀 받았어요!!",
          "original_form": "저 프락셀 받았어요!!",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023168.1.6",
          "form": "공장형은 뭔가 싸게 시술받을때 제모할때 좋다는데",
          "original_form": "공장형은 뭔가 싸게 시술받을때 제모할때 좋다는데",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023168.1.7",
          "form": "헐 프락셀",
          "original_form": "헐 프락셀",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023168.1.8",
          "form": "엄청 고통이지 않나요",
          "original_form": "엄청 고통이지 않나요",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023168.1.9",
          "form": "프락셀 원래 빨리 끝나요?",
          "original_form": "프락셀 원래 빨리 끝나요?",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023168.1.10",
          "form": "몰라요 ㅋㅋㅋㅋ",
          "original_form": "몰라요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023168.1.11",
          "form": "한 1초? 정도만 찍!! 하고 끝나던데요",
          "original_form": "한 1초? 정도만 찍!! 하고 끝나던데요",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023168.1.12",
          "form": "막 그 촘촘한 바늘로",
          "original_form": "막 그 촘촘한 바늘로",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023168.1.13",
          "form": "한번 자극주고 끝인가보네여",
          "original_form": "한번 자극주고 끝인가보네여",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023168.1.14",
          "form": "ㅠㅠ 네.. 그래서 뭐지 싶었어요",
          "original_form": "ㅠㅠ 네.. 그래서 뭐지 싶었어요",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023168.1.15",
          "form": "그거 한번 하니까 바로 딱지 생겼어요",
          "original_form": "그거 한번 하니까 바로 딱지 생겼어요",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023168.1.16",
          "form": "딱지가 다시 피부 회복의 과정이니깐",
          "original_form": "딱지가 다시 피부 회복의 과정이니깐",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023168.1.17",
          "form": "맞는 시술인가봐여 ㅋㅋㅋㅋ",
          "original_form": "맞는 시술인가봐여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023168.1.18",
          "form": "흉터조각술이라는 영상 봤는데 이거 핵좋네요",
          "original_form": "흉터조각술이라는 영상 봤는데 이거 핵좋네요",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023168.1.19",
          "form": "저도 받아보고싶어졌어요",
          "original_form": "저도 받아보고싶어졌어요",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023168.1.20",
          "form": "그건 머에여",
          "original_form": "그건 머에여",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023168.1.21",
