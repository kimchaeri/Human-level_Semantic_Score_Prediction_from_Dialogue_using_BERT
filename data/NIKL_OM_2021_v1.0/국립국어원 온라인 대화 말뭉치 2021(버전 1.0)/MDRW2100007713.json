      "utterance": [
        {
          "id": "MDRW2100007713.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007713.1.2",
          "form": "앗 저는 종교도 없습니다",
          "original_form": "앗 저는 종교도 없습니다",
          "speaker_id": "2",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007713.1.3",
          "form": "종교 있으신가요",
          "original_form": "종교 있으신가요",
          "speaker_id": "2",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007713.1.4",
          "form": "아뇨 저도 없어요",
          "original_form": "아뇨 저도 없어요",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007713.1.5",
          "form": "그래서 좀 이해 못하는 부분도 있는거 같아요",
          "original_form": "그래서 좀 이해 못하는 부분도 있는거 같아요",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007713.1.6",
          "form": "어떤부분이 그런가요?",
          "original_form": "어떤부분이 그런가요?",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007713.1.7",
          "form": "저는 약간 맹목적 믿음..",
          "original_form": "저는 약간 맹목적 믿음..",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007713.1.8",
          "form": "지금 시국에 대면으로 많이 모인다던지ㅎㅎ",
          "original_form": "지금 시국에 대면으로 많이 모인다던지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007713.1.9",
          "form": "아.. 알 것 같아요",
          "original_form": "아.. 알 것 같아요",
          "speaker_id": "2",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007713.1.10",
          "form": "맞아요 맹목적 믿음도..",
          "original_form": "맞아요 맹목적 믿음도..",
          "speaker_id": "1",
          "time": "20210822 10:19"
        },
        {
          "id": "MDRW2100007713.1.11",
          "form": "음 그럼 교회나 절 같은 곳 한번도 안가보셨나요?",
          "original_form": "음 그럼 교회나 절 같은 곳 한번도 안가보셨나요?",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007713.1.12",
          "form": "어릴때 친구따라 교회는 좀 갔었어요",
          "original_form": "어릴때 친구따라 교회는 좀 갔었어요",
          "speaker_id": "1",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007713.1.13",
          "form": "저는 대부분의 사람들이 그렇듯이.. 친구따라 맛있는거 준대서 한 두번 갔던거 같아요",
          "original_form": "저는 대부분의 사람들이 그렇듯이.. 친구따라 맛있는거 준대서 한 두번 갔던거 같아요",
          "speaker_id": "2",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007713.1.14",
          "form": "어 name1님도요? 저두요ㅎ",
          "original_form": "어 &name1&님도요? 저두요ㅎ",
          "speaker_id": "2",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007713.1.15",
          "form": "완전 공감이에요ㅋㅋ",
          "original_form": "완전 공감이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007713.1.16",
          "form": "약간 가면 좀 지루하고",
          "original_form": "약간 가면 좀 지루하고",
          "speaker_id": "2",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007713.1.17",
          "form": "맞아요 노래 부르고",
          "original_form": "맞아요 노래 부르고",
          "speaker_id": "1",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007713.1.18",
          "form": "앉아있는데 이게 뭐라고 날 이렇게 까지 전도하는거지?",
          "original_form": "앉아있는데 이게 뭐라고 날 이렇게 까지 전도하는거지?",
          "speaker_id": "2",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007713.1.19",
          "form": "친구따라 절에 간적도 있는데",
          "original_form": "친구따라 절에 간적도 있는데",
          "speaker_id": "1",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007713.1.20",
          "form": "어떤 느낌인지 아시죠ㅋㅋ",
          "original_form": "어떤 느낌인지 아시죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007713.1.21",
          "form": "절은 어때요",
          "original_form": "절은 어때요",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007713.1.22",
          "form": "교회보다는 절이 나았어요",
          "original_form": "교회보다는 절이 나았어요",
          "speaker_id": "1",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007713.1.23",
          "form": "절은 가본적이 없어서",
          "original_form": "절은 가본적이 없어서",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007713.1.24",
          "form": "그 전도하는게 없어서..ㅎㅎ",
          "original_form": "그 전도하는게 없어서..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007713.1.25",
          "form": "그건 그래요ㅎ",
