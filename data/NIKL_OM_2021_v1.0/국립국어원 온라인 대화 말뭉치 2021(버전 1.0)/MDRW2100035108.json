      "utterance": [
        {
          "id": "MDRW2100035108.1.1",
          "form": "엄청 건강하더라구요ㅋㅋㅋ",
          "original_form": "엄청 건강하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100035108.1.2",
          "form": "그런 강아지 처음봤어요",
          "original_form": "그런 강아지 처음봤어요",
          "speaker_id": "1",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100035108.1.3",
          "form": "할머니 사랑을 듬뿍 받았군요",
          "original_form": "할머니 사랑을 듬뿍 받았군요",
          "speaker_id": "2",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100035108.1.4",
          "form": "이빨도 다 빠졌는데 완전 활동적이고",
          "original_form": "이빨도 다 빠졌는데 완전 활동적이고",
          "speaker_id": "1",
          "time": "20210829 02:49"
        },
        {
          "id": "MDRW2100035108.1.5",
          "form": "쌩쌩해요ㅋㅋㅋ",
          "original_form": "쌩쌩해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100035108.1.6",
          "form": "저는 거북이 키워보고싳어용",
          "original_form": "저는 거북이 키워보고싳어용",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100035108.1.7",
          "form": "에고 진짜 사람이랑 같아요ㅠ",
          "original_form": "에고 진짜 사람이랑 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100035108.1.8",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100035108.1.9",
          "form": "눈도 멀고 귀도멀고.. ㅠㅠ",
          "original_form": "눈도 멀고 귀도멀고.. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100035108.1.10",
          "form": "아아ㅠㅠ 슬퍼요 얼마전에",
          "original_form": "아아ㅠㅠ 슬퍼요 얼마전에",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100035108.1.11",
          "form": "아 이름이 기억이 안나는데",
          "original_form": "아 이름이 기억이 안나는데",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100035108.1.12",
          "form": "강아지가 계속 환생해서",
          "original_form": "강아지가 계속 환생해서",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100035108.1.13",
          "form": "주인 찾는 그런 영화였는데",
          "original_form": "주인 찾는 그런 영화였는데",
          "speaker_id": "1",
          "time": "20210829 02:50"
        },
        {
          "id": "MDRW2100035108.1.14",
          "form": "진짜 주인에 대한 댓가없는 사랑은 반려동물 아니면 없어요ㅠ",
          "original_form": "진짜 주인에 대한 댓가없는 사랑은 반려동물 아니면 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 02:51"
        },
        {
          "id": "MDRW2100035108.1.15",
          "form": "와ㅜㅜ",
          "original_form": "와ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 02:51"
        },
        {
          "id": "MDRW2100035108.1.16",
          "form": "스토리가 넘짠하겠는데요ㅠㅠ",
          "original_form": "스토리가 넘짠하겠는데요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:51"
        },
        {
          "id": "MDRW2100035108.1.17",
          "form": "네네ㅠㅠ",
          "original_form": "네네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:51"
        },
        {
          "id": "MDRW2100035108.1.18",
          "form": "진짜 인간만 나쁜존재네요ㅠㅠ",
          "original_form": "진짜 인간만 나쁜존재네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:51"
        },
        {
          "id": "MDRW2100035108.1.19",
          "form": "그리고 인스타에서 본건데",
          "original_form": "그리고 인스타에서 본건데",
          "speaker_id": "1",
          "time": "20210829 02:51"
        },
        {
          "id": "MDRW2100035108.1.20",
          "form": "버려지는동물들보면 ㅠㅠ 진짜 무책임해여",
          "original_form": "버려지는동물들보면 ㅠㅠ 진짜 무책임해여",
          "speaker_id": "2",
          "time": "20210829 02:51"
        },
        {
          "id": "MDRW2100035108.1.21",
          "form": "10년만인가 길에서 잃어버려서 헤어졌다가",
          "original_form": "10년만인가 길에서 잃어버려서 헤어졌다가",
          "speaker_id": "1",
          "time": "20210829 02:51"
        },
        {
          "id": "MDRW2100035108.1.22",
          "form": "주인 만났는데 애가 넘 좋아서 울더라구요ㅠㅠ",
          "original_form": "주인 만났는데 애가 넘 좋아서 울더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:51"
        },
        {
          "id": "MDRW2100035108.1.23",
          "form": "주인 보자마자ㅠㅠ",
          "original_form": "주인 보자마자ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:51"
        },
        {
          "id": "MDRW2100035108.1.24",
          "form": "만나면 서로 알아보는건가요ㅠ",
          "original_form": "만나면 서로 알아보는건가요ㅠ",
          "speaker_id": "2",
          "time": "20210829 02:52"
        },
        {
          "id": "MDRW2100035108.1.25",
          "form": "그런것같아요ㅠㅠㅠ",
          "original_form": "그런것같아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:52"
        },
        {
          "id": "MDRW2100035108.1.26",
          "form": "와 그럼 다 기억한다는거잖아여ㅠㅠ",
          "original_form": "와 그럼 다 기억한다는거잖아여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:52"
        },
        {
          "id": "MDRW2100035108.1.27",
          "form": "네네...",
          "original_form": "네네...",
          "speaker_id": "1",
          "time": "20210829 02:52"
        },
        {
          "id": "MDRW2100035108.1.28",
          "form": "에고ㅠㅠ진짜..못살게구는사람들없으면 좋겠네요ㅠㅠ",
          "original_form": "에고ㅠㅠ진짜..못살게구는사람들없으면 좋겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:52"
        },
        {
          "id": "MDRW2100035108.1.29",
          "form": "맞아요 동물 사랑하는 사람들이 많아졌음 좋겠어요ㅠㅠ",
          "original_form": "맞아요 동물 사랑하는 사람들이 많아졌음 좋겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:52"
        }
      ]
