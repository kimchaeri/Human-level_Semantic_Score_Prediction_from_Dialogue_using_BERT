      "utterance": [
        {
          "id": "MDRW2100034071.1.1",
          "form": "오 대구에?",
          "original_form": "오 대구에?",
          "speaker_id": "2",
          "time": "20210827 14:19"
        },
        {
          "id": "MDRW2100034071.1.2",
          "form": "맛있겠당",
          "original_form": "맛있겠당",
          "speaker_id": "2",
          "time": "20210827 14:19"
        },
        {
          "id": "MDRW2100034071.1.3",
          "form": "무조건 다 1000원이라는데",
          "original_form": "무조건 다 1000원이라는데",
          "speaker_id": "1",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100034071.1.4",
          "form": "요새 떡 시세를 몰라서리",
          "original_form": "요새 떡 시세를 몰라서리",
          "speaker_id": "1",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100034071.1.5",
          "form": "싼거갇은데",
          "original_form": "싼거갇은데",
          "speaker_id": "2",
          "time": "20210827 14:20"
        },
        {
          "id": "MDRW2100034071.1.6",
          "form": "나도 떡잘안사먹어서 몰랑ㅜ",
          "original_form": "나도 떡잘안사먹어서 몰랑ㅜ",
          "speaker_id": "2",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100034071.1.7",
          "form": "근데 딸기설기는 존맛이었음",
          "original_form": "근데 딸기설기는 존맛이었음",
          "speaker_id": "1",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100034071.1.8",
          "form": "달아 ㅋㅋ",
          "original_form": "달아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100034071.1.9",
          "form": "오오 설탕마니들갔겧다",
          "original_form": "오오 설탕마니들갔겧다",
          "speaker_id": "2",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100034071.1.10",
          "form": "안에 살짝 딸기잼이 들어가있음",
          "original_form": "안에 살짝 딸기잼이 들어가있음",
          "speaker_id": "1",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100034071.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100034071.1.12",
          "form": "먹어보고싶네",
          "original_form": "먹어보고싶네",
          "speaker_id": "2",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100034071.1.13",
          "form": "근데 확실히 떡은 잘하는집꺼 먹어야 맛있음 ㅋㅋ",
          "original_form": "근데 확실히 떡은 잘하는집꺼 먹어야 맛있음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100034071.1.14",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100034071.1.15",
          "form": "광주쪽이 딱맛있게 하드라",
          "original_form": "광주쪽이 딱맛있게 하드라",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100034071.1.16",
          "form": "아 갑자기 베트남 쌀국수 먹고싶다",
          "original_form": "아 갑자기 베트남 쌀국수 먹고싶다",
          "speaker_id": "1",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100034071.1.17",
          "form": "아님 반미인가 그거도 궁금하던데",
          "original_form": "아님 반미인가 그거도 궁금하던데",
          "speaker_id": "1",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100034071.1.18",
          "form": "아 마싯지",
          "original_form": "아 마싯지",
          "speaker_id": "2",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100034071.1.19",
          "form": "쌀국수는 먹고나서 몸에 부담이 없어 조아",
          "original_form": "쌀국수는 먹고나서 몸에 부담이 없어 조아",
          "speaker_id": "2",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100034071.1.20",
          "form": "반미는 간식같아서 늘 안무봤네 가서 국수먹음ㅋㅋ",
          "original_form": "반미는 간식같아서 늘 안무봤네 가서 국수먹음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:24"
        },
        {
          "id": "MDRW2100034071.1.21",
          "form": "으어 배고프고 나는 짜증이나면서 우울해지는중ㅜ",
          "original_form": "으어 배고프고 나는 짜증이나면서 우울해지는중ㅜ",
          "speaker_id": "1",
          "time": "20210827 17:25"
        },
        {
          "id": "MDRW2100034071.1.22",
          "form": "나도 배고프면화나",
          "original_form": "나도 배고프면화나",
          "speaker_id": "2",
          "time": "20210827 17:25"
        },
        {
          "id": "MDRW2100034071.1.23",
          "form": "진짜 배가 안고파지고싶다",
          "original_form": "진짜 배가 안고파지고싶다",
          "speaker_id": "1",
          "time": "20210827 17:25"
        }
      ]
