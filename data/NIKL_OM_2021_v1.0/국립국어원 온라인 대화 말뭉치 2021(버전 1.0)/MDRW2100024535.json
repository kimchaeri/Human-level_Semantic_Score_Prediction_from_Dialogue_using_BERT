      "utterance": [
        {
          "id": "MDRW2100024535.1.1",
          "form": "구구콘 세일해서 10개 겟했습니다",
          "original_form": "구구콘 세일해서 10개 겟했습니다",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024535.1.2",
          "form": "10개 5처넌",
          "original_form": "10개 5처넌",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024535.1.3",
          "form": "ㅋㅋㅋ득템하셧네요",
          "original_form": "ㅋㅋㅋ득템하셧네요",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100024535.1.4",
          "form": "저는 오늘 일찍퇴근하는 날이라서",
          "original_form": "저는 오늘 일찍퇴근하는 날이라서",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100024535.1.5",
          "form": "라면먹고 쉬어요",
          "original_form": "라면먹고 쉬어요",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100024535.1.6",
          "form": "라면 맜있게 드셨나요? 무슨라면 드셨어요?ㅎㅎ",
          "original_form": "라면 맜있게 드셨나요? 무슨라면 드셨어요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100024535.1.7",
          "form": "요즘은 오동통너구리에 빠졋어요",
          "original_form": "요즘은 오동통너구리에 빠졋어요",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100024535.1.8",
          "form": "너구리 가끔 먹으니 좋더라구요 ㅋㅋ",
          "original_form": "너구리 가끔 먹으니 좋더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100024535.1.9",
          "form": "저도 한봉지 있는데 약간 물 좀 더 넣어서 끓여요",
          "original_form": "저도 한봉지 있는데 약간 물 좀 더 넣어서 끓여요",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100024535.1.10",
          "form": "오호 물좀더넣음 맛잇나요?",
          "original_form": "오호 물좀더넣음 맛잇나요?",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100024535.1.11",
          "form": "약간 더 익은거 좋아해서 30초 더 끓입니다 ㅋㅋ",
          "original_form": "약간 더 익은거 좋아해서 30초 더 끓입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100024535.1.12",
          "form": "크 저두요",
          "original_form": "크 저두요",
          "speaker_id": "1",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100024535.1.13",
          "form": "꼬들보다 더익은게좋아요",
          "original_form": "꼬들보다 더익은게좋아요",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100024535.1.14",
          "form": "오 저도",
          "original_form": "오 저도",
          "speaker_id": "2",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100024535.1.15",
          "form": "익은게 좋습니다",
          "original_form": "익은게 좋습니다",
          "speaker_id": "2",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100024535.1.16",
          "form": "아침은드셧나용",
          "original_form": "아침은드셧나용",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100024535.1.17",
          "form": "아침에 미숫가루 한잔 먹고왔습니다",
          "original_form": "아침에 미숫가루 한잔 먹고왔습니다",
          "speaker_id": "2",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100024535.1.18",
          "form": "카스테라하구요",
          "original_form": "카스테라하구요",
          "speaker_id": "2",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100024535.1.19",
          "form": "읍천리 아시나용\n어제 그거 시켜놓고\n오늘 읍천리 미숫가루 먹엇어요",
          "original_form": "읍천리 아시나용\n어제 그거 시켜놓고\n오늘 읍천리 미숫가루 먹엇어요",
          "speaker_id": "1",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100024535.1.20",
          "form": "카스테라 궁합도좋고 야무지게잘드셧네욤",
          "original_form": "카스테라 궁합도좋고 야무지게잘드셧네욤",
