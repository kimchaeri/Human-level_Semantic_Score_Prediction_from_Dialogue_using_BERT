      "utterance": [
        {
          "id": "MDRW2100022917.1.1",
          "form": "스포츠 보시나요?",
          "original_form": "스포츠 보시나요?",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022917.1.2",
          "form": "잘 안봐요 올림픽이나 아시안 게임 월드컵 정도 봐요",
          "original_form": "잘 안봐요 올림픽이나 아시안 게임 월드컵 정도 봐요",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022917.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 19:12"
        },
        {
          "id": "MDRW2100022917.1.4",
          "form": "저는 잘 안봐요",
          "original_form": "저는 잘 안봐요",
          "speaker_id": "2",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022917.1.5",
          "form": "가끔 당구는 봐요!",
          "original_form": "가끔 당구는 봐요!",
          "speaker_id": "2",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022917.1.6",
          "form": "오 당구 좋아하세요?",
          "original_form": "오 당구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022917.1.7",
          "form": "저는 스포츠와 거리가 먼 몸이라서요",
          "original_form": "저는 스포츠와 거리가 먼 몸이라서요",
          "speaker_id": "1",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022917.1.8",
          "form": "잘하는 사람 하고있는걸 보면 집중하게 되더라구요",
          "original_form": "잘하는 사람 하고있는걸 보면 집중하게 되더라구요",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022917.1.9",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022917.1.10",
          "form": "몸이 안따라주더라구요",
          "original_form": "몸이 안따라주더라구요",
          "speaker_id": "2",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022917.1.11",
          "form": "맞아요 몸이 말을 안들어요 ㅠ",
          "original_form": "맞아요 몸이 말을 안들어요 ㅠ",
          "speaker_id": "1",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022917.1.12",
          "form": "잘하면 스포츠 엄청 좋아했을지도 몰라요",
          "original_form": "잘하면 스포츠 엄청 좋아했을지도 몰라요",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022917.1.13",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100022917.1.14",
          "form": "체육할때도 발야구 한다하면 한숨부터 나왔어요",
          "original_form": "체육할때도 발야구 한다하면 한숨부터 나왔어요",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022917.1.15",
          "form": "전 체육대회도 부담이...",
          "original_form": "전 체육대회도 부담이...",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022917.1.16",
          "form": "잘하는 애들은 못하는 게 거의 없더라고요",
          "original_form": "잘하는 애들은 못하는 게 거의 없더라고요",
          "speaker_id": "1",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022917.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 19:30"
        },
        {
          "id": "MDRW2100022917.1.18",
          "form": "부러웠어요",
          "original_form": "부러웠어요",
          "speaker_id": "2",
          "time": "20210829 19:30"
        },
        {
          "id": "MDRW2100022917.1.19",
          "form": "그래도 배드민턴은 좀 재밌어요ㅎㅎ 잘은 못해도요",
          "original_form": "그래도 배드민턴은 좀 재밌어요ㅎㅎ 잘은 못해도요",
          "speaker_id": "1",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100022917.1.20",
          "form": "오~ 저는 훌라후프!",
          "original_form": "오~ 저는 훌라후프!",
          "speaker_id": "2",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100022917.1.21",
          "form": "훌라후프 어릴때 한번씩은 다하죠",
          "original_form": "훌라후프 어릴때 한번씩은 다하죠",
          "speaker_id": "1",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100022917.1.22",
          "form": "크면 다이어트용으로 변하고요",
          "original_form": "크면 다이어트용으로 변하고요",
          "speaker_id": "1",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100022917.1.23",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022917.1.24",
          "form": "크고 무거운게 많이 나왔죠",
          "original_form": "크고 무거운게 많이 나왔죠",
          "speaker_id": "2",
          "time": "20210829 19:47"
        }
      ]
    }
