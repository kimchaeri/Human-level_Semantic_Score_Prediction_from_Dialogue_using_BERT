      "utterance": [
        {
          "id": "MDRW2100000664.1.1",
          "form": "탄산음료 먹으면 뱃살 주범~",
          "original_form": "탄산음료 먹으면 뱃살 주범~",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000664.1.2",
          "form": "난 별로 안 먹는데",
          "original_form": "난 별로 안 먹는데",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100000664.1.3",
          "form": "그짓말~",
          "original_form": "그짓말~",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100000664.1.4",
          "form": "좋아하자나유",
          "original_form": "좋아하자나유",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100000664.1.5",
          "form": "좋아하는 하는데 잘 안 먹지",
          "original_form": "좋아하는 하는데 잘 안 먹지",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100000664.1.6",
          "form": "없어서못먹는거 아니고?ㅋㅋ",
          "original_form": "없어서못먹는거 아니고?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100000664.1.7",
          "form": "암튼 액상과당은 안좋은데! 먹지말장",
          "original_form": "암튼 액상과당은 안좋은데! 먹지말장",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100000664.1.8",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100000664.1.9",
          "form": "안 먹을게",
          "original_form": "안 먹을게",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100000664.1.10",
          "form": "구래 나두 끊었당 ㅎㅎ",
          "original_form": "구래 나두 끊었당 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100000664.1.11",
          "form": "그래 뱃살 좀 빼야겠다",
          "original_form": "그래 뱃살 좀 빼야겠다",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100000664.1.12",
          "form": "옹옹 ㅋㅋ 음료수 대신 물을 많이 마시자~",
          "original_form": "옹옹 ㅋㅋ 음료수 대신 물을 많이 마시자~",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100000664.1.13",
          "form": "알겠다 알겠어",
          "original_form": "알겠다 알겠어",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100000664.1.14",
          "form": "굿~ ^^좋아유",
          "original_form": "굿~ ^^좋아유",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100000664.1.15",
          "form": "딸래미 말 들어야지",
          "original_form": "딸래미 말 들어야지",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100000664.1.16",
          "form": "구래 딸래미 말을 잘들어야된당",
