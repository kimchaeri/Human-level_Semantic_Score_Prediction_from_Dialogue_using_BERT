      "utterance": [
        {
          "id": "MDRW2100022329.1.1",
          "form": "name1은 점심 드셨나요?",
          "original_form": "&name1&은 점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022329.1.2",
          "form": "저는 이제 먹으려구용 ㅎㅎ",
          "original_form": "저는 이제 먹으려구용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:04"
        },
        {
          "id": "MDRW2100022329.1.3",
          "form": "늣게 드시네요 뭐 드셨어요?",
          "original_form": "늣게 드시네요 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 15:16"
        },
        {
          "id": "MDRW2100022329.1.4",
          "form": "저 김치찌개 먹었어요!",
          "original_form": "저 김치찌개 먹었어요!",
          "speaker_id": "1",
          "time": "20210830 15:21"
        },
        {
          "id": "MDRW2100022329.1.5",
          "form": "맛있었겠어요!!",
          "original_form": "맛있었겠어요!!",
          "speaker_id": "2",
          "time": "20210830 16:10"
        },
        {
          "id": "MDRW2100022329.1.6",
          "form": "평소에 어떤 음식 좋아하세요?",
          "original_form": "평소에 어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100022329.1.7",
          "form": "저는 평소에 인스턴트 식품을 좋아하는 것 같아요 ㅎㅎ",
          "original_form": "저는 평소에 인스턴트 식품을 좋아하는 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100022329.1.8",
          "form": "만두나 햄버거같은거요!",
          "original_form": "만두나 햄버거같은거요!",
          "speaker_id": "1",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100022329.1.9",
          "form": "저는 떡볶이 같은 분식 좋아해요ㅋㅋ",
          "original_form": "저는 떡볶이 같은 분식 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100022329.1.10",
          "form": "떡볶이 맛있죠 ㅋㅋㅋ",
          "original_form": "떡볶이 맛있죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100022329.1.11",
          "form": "로제떡볶이 드셔보셨어요?",
          "original_form": "로제떡볶이 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100022329.1.12",
          "form": "네 엄청 좋아해요 신전이랑 달토끼에서 먹어봤어요",
          "original_form": "네 엄청 좋아해요 신전이랑 달토끼에서 먹어봤어요",
          "speaker_id": "2",
          "time": "20210901 11:53"
        },
        {
          "id": "MDRW2100022329.1.13",
          "form": "오오 둘중에 어디가 더 맛있어요?",
          "original_form": "오오 둘중에 어디가 더 맛있어요?",
          "speaker_id": "1",
          "time": "20210901 12:10"
        },
        {
          "id": "MDRW2100022329.1.14",
          "form": "둘이 맛이 많이 다른데 신전은 좀 더 가볍고 달토끼는 크림맛이 진해요",
          "original_form": "둘이 맛이 많이 다른데 신전은 좀 더 가볍고 달토끼는 크림맛이 진해요",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100022329.1.15",
          "form": "취향차이일 것 같아요",
          "original_form": "취향차이일 것 같아요",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100022329.1.16",
          "form": "가볍게 먹기에는 신전이 더 좋을 것 같네요 ㅋㅋ",
          "original_form": "가볍게 먹기에는 신전이 더 좋을 것 같네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100022329.1.17",
          "form": "달토끼는 처음들어봐요!",
          "original_form": "달토끼는 처음들어봐요!",
          "speaker_id": "1",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100022329.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 07:56"
        },
        {
          "id": "MDRW2100022329.1.19",
          "form": "저도 달토끼 친구 때문에 알게 됐는데 되게 맛있었어요",
          "original_form": "저도 달토끼 친구 때문에 알게 됐는데 되게 맛있었어요",
          "speaker_id": "2",
          "time": "20210902 07:57"
        },
        {
          "id": "MDRW2100022329.1.20",
          "form": "오오 궁금하네요 ㅎㅎㅎ",
          "original_form": "오오 궁금하네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100022329.1.21",
          "form": "다음에 꼭 먹어볼게요",
          "original_form": "다음에 꼭 먹어볼게요",
          "speaker_id": "1",
