        {
          "id": "MDRW2100023850.1.1",
          "form": "여행가보셨어요?",
          "original_form": "여행가보셨어요?",
          "speaker_id": "1",
          "time": "20210830 18:58"
        },
        {
          "id": "MDRW2100023850.1.2",
          "form": "안녕하세요 요샌 통 못가봤네요",
          "original_form": "안녕하세요 요샌 통 못가봤네요",
          "speaker_id": "2",
