        },
        {
          "id": "MDRW2100023850.1.3",
          "form": "name1는요 영도가 훌륭한 여행지죠",
          "original_form": "&name1&는요 영도가 훌륭한 여행지죠",
          "speaker_id": "2",
          "time": "20210830 18:59"
        },
        {
          "id": "MDRW2100023850.1.4",
          "form": "네 요새는 새로 카페도 많이 지어져서 너무 여행하기 좋아요",
          "original_form": "네 요새는 새로 카페도 많이 지어져서 너무 여행하기 좋아요",
          "speaker_id": "1",
          "time": "20210830 19:03"
        },
        {
          "id": "MDRW2100023850.1.5",
          "form": "오션뷰가 멋지지요",
          "original_form": "오션뷰가 멋지지요",
          "speaker_id": "2",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100023850.1.6",
          "form": "name2은 어디에 사시나요?",
          "original_form": "&name2&은 어디에 사시나요?",
          "speaker_id": "1",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100023850.1.7",
          "form": "저는 드니드니타워",
          "original_form": "저는 드니드니타워",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023850.1.8",
          "form": "살아요",
          "original_form": "살아요",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023850.1.9",
          "form": "거긴 어딘가요?",
          "original_form": "거긴 어딘가요?",
          "speaker_id": "1",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023850.1.10",
          "form": "한국인가요?",
          "original_form": "한국인가요?",
          "speaker_id": "1",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100023850.1.11",
          "form": "그럼요 요새 외국함부러못나가거든요",
          "original_form": "그럼요 요새 외국함부러못나가거든요",
          "speaker_id": "2",
          "time": "20210830 19:31"
        },
        {
          "id": "MDRW2100023850.1.12",
          "form": "아  타워라길래 도쿄타워가 생각났네요",
          "original_form": "아  타워라길래 도쿄타워가 생각났네요",
          "speaker_id": "1",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100023850.1.13",
          "form": "아 도쿄타워 가보셨나요? 전 못가봤어요",
          "original_form": "아 도쿄타워 가보셨나요? 전 못가봤어요",
          "speaker_id": "2",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100023850.1.14",
          "form": "전 영화로 봤어요",
          "original_form": "전 영화로 봤어요",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100023850.1.15",
          "form": "어느 타워사시나요?",
          "original_form": "어느 타워사시나요?",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100023850.1.16",
          "form": "드니드니타워요",
          "original_form": "드니드니타워요",
          "speaker_id": "2",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100023850.1.17",
