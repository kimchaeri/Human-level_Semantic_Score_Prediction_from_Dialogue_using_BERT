      "utterance": [
        {
          "id": "MDRW2100024707.1.1",
          "form": "종교잇으신가요",
          "original_form": "종교잇으신가요",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024707.1.2",
          "form": "종교는 없어요",
          "original_form": "종교는 없어요",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024707.1.3",
          "form": "저는 불교쪽",
          "original_form": "저는 불교쪽",
          "speaker_id": "1",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024707.1.4",
          "form": "불교에 더 마음이 가긴 해요",
          "original_form": "불교에 더 마음이 가긴 해요",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024707.1.5",
          "form": "음 그냥 학문으로 불교선호 무교입니다",
          "original_form": "음 그냥 학문으로 불교선호 무교입니다",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024707.1.6",
          "form": "성당이 멋있어서 한번 가보고 싶어요",
          "original_form": "성당이 멋있어서 한번 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024707.1.7",
          "form": "종교학을 배우고싶네요",
          "original_form": "종교학을 배우고싶네요",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024707.1.8",
          "form": "수녀님들은 더운데 고생이세요 옷이",
          "original_form": "수녀님들은 더운데 고생이세요 옷이",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024707.1.9",
          "form": "수녀옷 통풍 잘 되요",
          "original_form": "수녀옷 통풍 잘 되요",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024707.1.10",
          "form": "어렸을때 간식 먹으러 교회에 간적 있어요",
          "original_form": "어렸을때 간식 먹으러 교회에 간적 있어요",
          "speaker_id": "2",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024707.1.11",
          "form": "저도요 찬송가 부르면 과자줬어요",
          "original_form": "저도요 찬송가 부르면 과자줬어요",
          "speaker_id": "1",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024707.1.12",
          "form": "성경책 읽기가 싫어서 관뒀어요",
          "original_form": "성경책 읽기가 싫어서 관뒀어요",
          "speaker_id": "2",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024707.1.13",
          "form": "성경은 관심없어요",
          "original_form": "성경은 관심없어요",
          "speaker_id": "1",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024707.1.14",
