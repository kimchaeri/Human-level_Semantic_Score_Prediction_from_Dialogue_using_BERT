      "utterance": [
        {
          "id": "MDRW2100004931.1.1",
          "form": "다이어트 얘기할까?",
          "original_form": "다이어트 얘기할까?",
          "speaker_id": "2",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004931.1.2",
          "form": "짬뽕 먹고 싶어요",
          "original_form": "짬뽕 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004931.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004931.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004931.1.5",
          "form": "다이어트 해야 되는데",
          "original_form": "다이어트 해야 되는데",
          "speaker_id": "1",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004931.1.6",
          "form": "계속 뭐가 먹고 싶어",
          "original_form": "계속 뭐가 먹고 싶어",
          "speaker_id": "1",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004931.1.7",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "2",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004931.1.8",
          "form": "그리고 뭔가 속세 음식 한 번 먹으면",
          "original_form": "그리고 뭔가 속세 음식 한 번 먹으면",
          "speaker_id": "2",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004931.1.9",
          "form": "식단 다시는 하고 싶지 않음",
          "original_form": "식단 다시는 하고 싶지 않음",
          "speaker_id": "2",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004931.1.10",
          "form": "속세음식이",
          "original_form": "속세음식이",
          "speaker_id": "1",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004931.1.11",
          "form": "찐이야",
          "original_form": "찐이야",
          "speaker_id": "1",
          "time": "20210821 19:41"
        },
        {
          "id": "MDRW2100004931.1.12",
          "form": "식단이랑 운동 둘 다 그냥 조금씩 꾸준히 하는 게 나을 거 같아",
          "original_form": "식단이랑 운동 둘 다 그냥 조금씩 꾸준히 하는 게 나을 거 같아",
          "speaker_id": "2",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004931.1.13",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004931.1.14",
          "form": "답은 운동이다...",
          "original_form": "답은 운동이다...",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004931.1.15",
          "form": "근데 운동하기 시름",
          "original_form": "근데 운동하기 시름",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004931.1.16",
          "form": "허허",
          "original_form": "허허",
          "speaker_id": "1",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004931.1.17",
          "form": "맞아 몸이 그렇게 무거울 수가 없음",
          "original_form": "맞아 몸이 그렇게 무거울 수가 없음",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004931.1.18",
          "form": "운동하기 직전에 천근만근",
          "original_form": "운동하기 직전에 천근만근",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004931.1.19",
          "form": "인정...",
          "original_form": "인정...",
          "speaker_id": "1",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004931.1.20",
          "form": "추욱 쳐짐",
          "original_form": "추욱 쳐짐",
          "speaker_id": "1",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004931.1.21",
          "form": "운동한 것마냥",
          "original_form": "운동한 것마냥",
          "speaker_id": "1",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004931.1.22",
          "form": "운동을 안 하면 안 빠지니까 하긴 해야 하는데",
          "original_form": "운동을 안 하면 안 빠지니까 하긴 해야 하는데",
          "speaker_id": "2",
          "time": "20210821 19:48"
        },
        {
          "id": "MDRW2100004931.1.23",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "1",
          "time": "20210821 19:48"
        }
