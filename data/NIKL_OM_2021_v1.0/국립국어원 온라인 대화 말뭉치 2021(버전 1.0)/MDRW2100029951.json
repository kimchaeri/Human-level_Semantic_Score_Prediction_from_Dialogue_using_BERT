      "utterance": [
        {
          "id": "MDRW2100029951.1.1",
          "form": "ㅋㅋ 개불과 절친이셨냐는 말이었쥬ㅎㅎ",
          "original_form": "ㅋㅋ 개불과 절친이셨냐는 말이었쥬ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100029951.1.2",
          "form": "담에는 멍게 개불 소라 새우 세트로 먹어야겠어요",
          "original_form": "담에는 멍게 개불 소라 새우 세트로 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100029951.1.3",
          "form": "요기서 새우가 좀 약해서 고민했는데",
          "original_form": "요기서 새우가 좀 약해서 고민했는데",
          "speaker_id": "1",
          "time": "20210903 17:24"
        },
        {
          "id": "MDRW2100029951.1.4",
          "form": "아 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029951.1.5",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029951.1.6",
          "form": "탁월한선택",
          "original_form": "탁월한선택",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029951.1.7",
          "form": "아 회먹고싶당",
          "original_form": "아 회먹고싶당",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029951.1.8",
          "form": "참치도 땡기고",
          "original_form": "참치도 땡기고",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029951.1.9",
          "form": "아이고 ㅎㅎ",
          "original_form": "아이고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029951.1.10",
          "form": "ㅎㅎ  천사는 해산물 영 별로인가요",
          "original_form": "ㅎㅎ  천사는 해산물 영 별로인가요",
          "speaker_id": "1",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100029951.1.11",
          "form": "음 즐기지는 않아요 ㅋㅋ",
          "original_form": "음 즐기지는 않아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029951.1.12",
          "form": "아쉽습니다",
          "original_form": "아쉽습니다",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029951.1.13",
          "form": "우리집도 가족들이 해산물은 쏘쏘라 아쉽ㅜ",
          "original_form": "우리집도 가족들이 해산물은 쏘쏘라 아쉽ㅜ",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100029951.1.14",
          "form": "그나마 육지고기는 다들 좋아해서 다행입니다",
          "original_form": "그나마 육지고기는 다들 좋아해서 다행입니다",
          "speaker_id": "1",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100029951.1.15",
          "form": "소고기 좋아합니다",
          "original_form": "소고기 좋아합니다",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100029951.1.16",
          "form": "믿어 독 싫어해요",
          "original_form": "믿어 독 싫어해요",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100029951.1.17",
          "form": "미더덕회 싫어해요",
          "original_form": "미더덕회 싫어해요",
          "speaker_id": "2",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100029951.1.18",
          "form": "미더덕회 십중 팔구 불호라 확신합니다ㅜ",
          "original_form": "미더덕회 십중 팔구 불호라 확신합니다ㅜ",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100029951.1.19",
          "form": "맞습니다 맞고요",
          "original_form": "맞습니다 맞고요",
          "speaker_id": "2",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100029951.1.20",
          "form": "제 몇 안되는 회 메이트도 미더덕회 시키자니 갸우뚱ㅜ",
          "original_form": "제 몇 안되는 회 메이트도 미더덕회 시키자니 갸우뚱ㅜ",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100029951.1.21",
          "form": "사실 매운탕속 미더덕조차 안먹는 사람들이 많아서요",
          "original_form": "사실 매운탕속 미더덕조차 안먹는 사람들이 많아서요",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100029951.1.22",
          "form": "천4 도 극혐하네요",
          "original_form": "천4 도 극혐하네요",
          "speaker_id": "2",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100029951.1.23",
          "form": "그거 안에 있는 물만 드시나요?",
          "original_form": "그거 안에 있는 물만 드시나요?",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100029951.1.24",
          "form": "물도 먹지 않습니다",
          "original_form": "물도 먹지 않습니다",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100029951.1.25",
          "form": "사지 않습니다",
          "original_form": "사지 않습니다",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100029951.1.26",
          "form": "쳐다보지도 않습니다",
          "original_form": "쳐다보지도 않습니다",
          "speaker_id": "2",
          "time": "20210903 19:16"
        },
        {
          "id": "MDRW2100029951.1.27",
          "form": "노재팬도 아니고 노미더덕입니까",
          "original_form": "노재팬도 아니고 노미더덕입니까",
