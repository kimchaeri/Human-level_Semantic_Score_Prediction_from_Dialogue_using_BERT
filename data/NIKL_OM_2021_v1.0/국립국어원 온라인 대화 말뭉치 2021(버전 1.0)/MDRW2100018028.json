      "utterance": [
        {
          "id": "MDRW2100018028.1.1",
          "form": "책 자주읽어.?",
          "original_form": "책 자주읽어.?",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100018028.1.2",
          "form": "읽고 싶긴한데 잘 안 읽게되네",
          "original_form": "읽고 싶긴한데 잘 안 읽게되네",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100018028.1.3",
          "form": "도서관 가서 빌리면 좋을텐데 코로나라서 아쉽네",
          "original_form": "도서관 가서 빌리면 좋을텐데 코로나라서 아쉽네",
          "speaker_id": "2",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018028.1.4",
          "form": "책 사는 값이 비싸긴 해",
          "original_form": "책 사는 값이 비싸긴 해",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018028.1.5",
          "form": "맞아 책도 할인하면 더 자주 구매할텐데",
          "original_form": "맞아 책도 할인하면 더 자주 구매할텐데",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018028.1.6",
          "form": "나는 그래서 밀리의 서재로 책을 읽어",
          "original_form": "나는 그래서 밀리의 서재로 책을 읽어",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018028.1.7",
          "form": "오 나도 이북으로 책 읽어본적 있어",
          "original_form": "오 나도 이북으로 책 읽어본적 있어",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018028.1.8",
          "form": "구독권으로 한달동안 책을 빌려 읽을 수 있어",
          "original_form": "구독권으로 한달동안 책을 빌려 읽을 수 있어",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018028.1.9",
          "form": "출퇴근길에 가볍게 책 읽을수 있어서 좋겠다",
          "original_form": "출퇴근길에 가볍게 책 읽을수 있어서 좋겠다",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018028.1.10",
          "form": "맞아 그리고 인기 순위의 책을 알 수 있어",
          "original_form": "맞아 그리고 인기 순위의 책을 알 수 있어",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018028.1.11",
          "form": "베스트셀러 읽는거 좋아해?",
          "original_form": "베스트셀러 읽는거 좋아해?",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018028.1.12",
          "form": "베스트셀러들은 대부분 재밌더라",
          "original_form": "베스트셀러들은 대부분 재밌더라",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018028.1.13",
          "form": "맞아 보장된 재밌는 책들이지",
          "original_form": "맞아 보장된 재밌는 책들이지",
          "speaker_id": "2",
          "time": "20210826 23:49"
        }
