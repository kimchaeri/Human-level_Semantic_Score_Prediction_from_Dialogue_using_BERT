          "speaker_id": "1",
          "time": "20210902 19:47"
        },
        {
          "id": "MDRW2100028412.1.3",
          "form": "비가 종일온다",
          "original_form": "비가 종일온다",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028412.1.4",
          "form": "나갈수가 없어",
          "original_form": "나갈수가 없어",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028412.1.5",
          "form": "내일도 비가 올까",
          "original_form": "내일도 비가 올까",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028412.1.6",
          "form": "태풍인지",
          "original_form": "태풍인지",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028412.1.7",
          "form": "스콜성 비가 며칠째 내려",
          "original_form": "스콜성 비가 며칠째 내려",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028412.1.8",
          "form": "동남아 날씨같아",
          "original_form": "동남아 날씨같아",
          "speaker_id": "1",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028412.1.9",
          "form": "우기와 건기로 나뉘려나",
          "original_form": "우기와 건기로 나뉘려나",
          "speaker_id": "2",
          "time": "20210902 19:48"
        },
        {
          "id": "MDRW2100028412.1.10",
          "form": "집에서 빗소리 듣는건 좋아",
          "original_form": "집에서 빗소리 듣는건 좋아",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028412.1.11",
          "form": "하지만 비때문에 외출을 못하지",
          "original_form": "하지만 비때문에 외출을 못하지",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028412.1.12",
          "form": "나는 우산쓰고도 나갈 수 있다",
          "original_form": "나는 우산쓰고도 나갈 수 있다",
          "speaker_id": "2",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028412.1.13",
          "form": "굳이 빗속을 뚫고 갈 이유는 없지",
          "original_form": "굳이 빗속을 뚫고 갈 이유는 없지",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100028412.1.14",
          "form": "비 내려도 나갈거야?",
          "original_form": "비 내려도 나갈거야?",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100028412.1.15",
          "form": "아니 집에 있어야지",
          "original_form": "아니 집에 있어야지",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100028412.1.16",
          "form": "눈 와도 나갈거야",
          "original_form": "눈 와도 나갈거야",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100028412.1.17",
          "form": "첫눈오면 나가야지",
          "original_form": "첫눈오면 나가야지",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100028412.1.18",
          "form": "첫눈은 희망이지",
          "original_form": "첫눈은 희망이지",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100028412.1.19",
          "form": "제주도는 눈이 많이 내려",
          "original_form": "제주도는 눈이 많이 내려",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028412.1.20",
          "form": "부산날씨는 어때",
          "original_form": "부산날씨는 어때",
          "speaker_id": "1",
          "time": "20210902 19:51"
        }
