      "utterance": [
        {
          "id": "MDRW2100008553.1.1",
          "form": "흠 씻고나서 욕실청소??",
          "original_form": "흠 씻고나서 욕실청소??",
          "speaker_id": "2",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008553.1.2",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008553.1.3",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008553.1.4",
          "form": "욕실청소 저도 귀찮아요",
          "original_form": "욕실청소 저도 귀찮아요",
          "speaker_id": "1",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008553.1.5",
          "form": "특히 바닥이랑 벽닦는거 너무싫어",
          "original_form": "특히 바닥이랑 벽닦는거 너무싫어",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008553.1.6",
          "form": "맞아요ㅠ맞아요",
          "original_form": "맞아요ㅠ맞아요",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008553.1.7",
          "form": "물기 닦는거 너무 귀찮아요...",
          "original_form": "물기 닦는거 너무 귀찮아요...",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008553.1.8",
          "form": "약간 안닦아주면 물 때 끼는데",
          "original_form": "약간 안닦아주면 물 때 끼는데",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008553.1.9",
          "form": "안닦기엔 찝찝하고",
          "original_form": "안닦기엔 찝찝하고",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008553.1.10",
          "form": "그거 나중에 새집 이사가실때",
          "original_form": "그거 나중에 새집 이사가실때",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008553.1.11",
          "form": "들어가기 전에 욕실에 싹 코팅하면",
          "original_form": "들어가기 전에 욕실에 싹 코팅하면",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008553.1.12",
          "form": "안닦아도 된대요",
          "original_form": "안닦아도 된대요",
          "speaker_id": "2",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008553.1.13",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210822 16:38"
        },
        {
          "id": "MDRW2100008553.1.14",
          "form": "그걸 나중에 알아서 이미 늦어버린 저",
          "original_form": "그걸 나중에 알아서 이미 늦어버린 저",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008553.1.15",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008553.1.16",
          "form": "아 이사갈 돈 부터 마련을 해야겠군",
          "original_form": "아 이사갈 돈 부터 마련을 해야겠군",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008553.1.17",
          "form": "병원이나 다른 시설에선",
          "original_form": "병원이나 다른 시설에선",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008553.1.18",
          "form": "그렇게 한대요 ㅋㅋㅋㅋ",
          "original_form": "그렇게 한대요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008553.1.19",
          "form": "이사...언젠가 가게되면... 꼭... ㅠㅠ",
          "original_form": "이사...언젠가 가게되면... 꼭... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008553.1.20",
          "form": "그래요? 그럼 우리 부자되서 꼭 새집가요",
          "original_form": "그래요? 그럼 우리 부자되서 꼭 새집가요",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008553.1.21",
          "form": "꼭 부자가되어야지",
          "original_form": "꼭 부자가되어야지",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008553.1.22",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008553.1.23",
          "form": "그래서 욕실청소 하지않을테야",
          "original_form": "그래서 욕실청소 하지않을테야",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008553.1.24",
          "form": "코팅 꼭 해야지",
          "original_form": "코팅 꼭 해야지",
          "speaker_id": "1",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008553.1.25",
          "form": "맞아요맞아욬ㅋㅋㅋ큐ㅠ",
          "original_form": "맞아요맞아욬ㅋㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210822 16:40"
        }
      ]
