      "utterance": [
        {
          "id": "MDRW2100022190.1.1",
          "form": "최근 여행 간곳은 어디입니까?",
          "original_form": "최근 여행 간곳은 어디입니까?",
          "speaker_id": "1",
          "time": "20210829 10:45"
        },
        {
          "id": "MDRW2100022190.1.2",
          "form": "마지막 여행이 스페인입니다",
          "original_form": "마지막 여행이 스페인입니다",
          "speaker_id": "2",
          "time": "20210829 10:45"
        },
        {
          "id": "MDRW2100022190.1.3",
          "form": "여행을 좋아해서 자주 다녔습니다",
          "original_form": "여행을 좋아해서 자주 다녔습니다",
          "speaker_id": "2",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022190.1.4",
          "form": "오~스페인 ~!!",
          "original_form": "오~스페인 ~!!",
          "speaker_id": "1",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022190.1.5",
          "form": "탱코는 추셨습니까??",
          "original_form": "탱코는 추셨습니까??",
          "speaker_id": "1",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022190.1.6",
          "form": "코로나때문에 다닐수가 없겠군요ㅠ",
          "original_form": "코로나때문에 다닐수가 없겠군요ㅠ",
          "speaker_id": "1",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022190.1.7",
          "form": "파트너가 없어서 못추겠더라구요",
          "original_form": "파트너가 없어서 못추겠더라구요",
          "speaker_id": "2",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022190.1.8",
          "form": "그리고 몸치입니다",
          "original_form": "그리고 몸치입니다",
          "speaker_id": "2",
          "time": "20210829 10:46"
        },
        {
          "id": "MDRW2100022190.1.9",
          "form": "몸이 흥을 못따라가는 타입이죠",
          "original_form": "몸이 흥을 못따라가는 타입이죠",
          "speaker_id": "2",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100022190.1.10",
          "form": "파트너와 함께 가셨을 텐데요??ㅋㅋㅋ",
          "original_form": "파트너와 함께 가셨을 텐데요??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100022190.1.11",
          "form": "그럼 잠시 쉬어주는것도 좋습니다 ㅋㅋㅋㅋ",
          "original_form": "그럼 잠시 쉬어주는것도 좋습니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100022190.1.12",
          "form": "운동은 좋아하는데 춤은 어렵더라구요 ㅎㅎ",
          "original_form": "운동은 좋아하는데 춤은 어렵더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 10:47"
        },
        {
          "id": "MDRW2100022190.1.13",
          "form": "같이 가서 손만 잡고 다녓네요 ㅎ",
          "original_form": "같이 가서 손만 잡고 다녓네요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 10:48"
        },
        {
          "id": "MDRW2100022190.1.14",
          "form": "최근 여행\r\n간곳 어딘가요?",
          "original_form": "최근 여행\r\n간곳 어딘가요?",
          "speaker_id": "2",
          "time": "20210829 10:48"
        },
        {
          "id": "MDRW2100022190.1.15",
          "form": "경주 다녀왔습죠~",
          "original_form": "경주 다녀왔습죠~",
          "speaker_id": "1",
          "time": "20210829 10:48"
        },
        {
          "id": "MDRW2100022190.1.16",
          "form": "아 경주 좋습니다",
          "original_form": "아 경주 좋습니다",
          "speaker_id": "2",
          "time": "20210829 10:48"
        },
        {
          "id": "MDRW2100022190.1.17",
          "form": "다음에 애가 크거든 같이 가 봅시다",
          "original_form": "다음에 애가 크거든 같이 가 봅시다",
          "speaker_id": "1",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100022190.1.18",
          "form": "아 너무 기대됩니다",
          "original_form": "아 너무 기대됩니다",
          "speaker_id": "2",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100022190.1.19",
          "form": "애들이 놀기에 너무 좋았던 것 같습니다",
          "original_form": "애들이 놀기에 너무 좋았던 것 같습니다",
          "speaker_id": "1",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100022190.1.20",
          "form": "제가 가이드할게요",
          "original_form": "제가 가이드할게요",
          "speaker_id": "2",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100022190.1.21",
          "form": "제주도도 함께 가고",
          "original_form": "제주도도 함께 가고",
          "speaker_id": "1",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100022190.1.22",
          "form": "어디라도 갑시다",
          "original_form": "어디라도 갑시다",
          "speaker_id": "1",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100022190.1.23",
          "form": "역사공부해서 애기들한테 가이드해주고 싶네요",
          "original_form": "역사공부해서 애기들한테 가이드해주고 싶네요",
          "speaker_id": "2",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100022190.1.24",
          "form": "재미있게 이야기 풀어가면 역사도 알고 흥미도 생길것같아요",
          "original_form": "재미있게 이야기 풀어가면 역사도 알고 흥미도 생길것같아요",
          "speaker_id": "2",
          "time": "20210829 10:49"
        },
        {
          "id": "MDRW2100022190.1.25",
          "form": "열정적인 아빠 군요~",
          "original_form": "열정적인 아빠 군요~",
          "speaker_id": "1",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100022190.1.26",
          "form": "꼭 재미있게 이야기해 합니다",
          "original_form": "꼭 재미있게 이야기해 합니다",
          "speaker_id": "1",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100022190.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100022190.1.28",
          "form": "네 한번 경험해봣습니다",
          "original_form": "네 한번 경험해봣습니다",
          "speaker_id": "2",
          "time": "20210829 10:50"
        }
      ]
    }
