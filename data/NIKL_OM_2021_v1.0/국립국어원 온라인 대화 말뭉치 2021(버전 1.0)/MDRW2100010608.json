      "utterance": [
        {
          "id": "MDRW2100010608.1.1",
          "form": "떡볶이 맛나죠ㅎㅎ 직접해드셨어요?",
          "original_form": "떡볶이 맛나죠ㅎㅎ 직접해드셨어요?",
          "speaker_id": "1",
          "time": "20210823 19:59"
        },
        {
          "id": "MDRW2100010608.1.2",
          "form": "아뇨 로제떡볶이 시켰어요",
          "original_form": "아뇨 로제떡볶이 시켰어요",
          "speaker_id": "2",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100010608.1.3",
          "form": "요리를 잘 못해서ㅋㅋㅋ ㅠㅠ",
          "original_form": "요리를 잘 못해서ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100010608.1.4",
          "form": "오 로제떡볶이 맛있다면서요",
          "original_form": "오 로제떡볶이 맛있다면서요",
          "speaker_id": "1",
          "time": "20210823 20:01"
        },
        {
          "id": "MDRW2100010608.1.5",
          "form": "배떡이 맛있다던데 어디꺼드셨나요?",
          "original_form": "배떡이 맛있다던데 어디꺼드셨나요?",
          "speaker_id": "1",
          "time": "20210823 20:01"
        },
        {
          "id": "MDRW2100010608.1.6",
          "form": "ㅈ저는 배떡은 갠적으로 별로라서",
          "original_form": "ㅈ저는 배떡은 갠적으로 별로라서",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010608.1.7",
          "form": "한번 멈ㄱ고 안 먹어봤어요",
          "original_form": "한번 멈ㄱ고 안 먹어봤어요",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010608.1.8",
          "form": "브랜드는 아닌 거 같은 데서 시켰어요",
          "original_form": "브랜드는 아닌 거 같은 데서 시켰어요",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010608.1.9",
          "form": "전 아직 못먹어봐서 궁금하네요",
          "original_form": "전 아직 못먹어봐서 궁금하네요",
          "speaker_id": "1",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010608.1.10",
          "form": "브랜드는 엽떡 신전 배떡 순으로 괜찮더라고요",
          "original_form": "브랜드는 엽떡 신전 배떡 순으로 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100010608.1.11",
          "form": "요즘 로제가 유행인가봐요",
          "original_form": "요즘 로제가 유행인가봐요",
          "speaker_id": "1",
          "time": "20210823 20:03"
        },
        {
          "id": "MDRW2100010608.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 20:05"
        },
        {
          "id": "MDRW2100010608.1.13",
          "form": "밀키트만 사서 먹어봤는데",
          "original_form": "밀키트만 사서 먹어봤는데",
          "speaker_id": "1",
          "time": "20210823 20:06"
        },
        {
          "id": "MDRW2100010608.1.14",
          "form": "그것도 괜찮더라구요",
          "original_form": "그것도 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210823 20:06"
        },
        {
          "id": "MDRW2100010608.1.15",
          "form": "오 밀키트 좋죠",
          "original_form": "오 밀키트 좋죠",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010608.1.16",
          "form": "심플리쿡인가 프레시몰서 샀는데 맛있었어요",
          "original_form": "심플리쿡인가 프레시몰서 샀는데 맛있었어요",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010608.1.17",
          "form": "요즘 밀키트가 확실히 유행인가 봐요",
          "original_form": "요즘 밀키트가 확실히 유행인가 봐요",
          "speaker_id": "2",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010608.1.18",
          "form": "편하고 맛도 괜찮고하니 많이 사먹더라구요",
          "original_form": "편하고 맛도 괜찮고하니 많이 사먹더라구요",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010608.1.19",
          "form": "재료손질필요도 없고",
          "original_form": "재료손질필요도 없고",
          "speaker_id": "1",
          "time": "20210823 20:15"
        }
      ]
