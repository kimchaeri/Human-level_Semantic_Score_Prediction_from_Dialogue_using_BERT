      "utterance": [
        {
          "id": "MDRW2100003432.1.1",
          "form": "오늘 치유 데리고 산에 갔다왔다",
          "original_form": "오늘 치유 데리고 산에 갔다왔다",
          "speaker_id": "2",
          "time": "20210820 23:02"
        },
        {
          "id": "MDRW2100003432.1.2",
          "form": "재밌었겠네",
          "original_form": "재밌었겠네",
          "speaker_id": "1",
          "time": "20210820 23:02"
        },
        {
          "id": "MDRW2100003432.1.3",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210820 23:02"
        },
        {
          "id": "MDRW2100003432.1.4",
          "form": "좋아하더라",
          "original_form": "좋아하더라",
          "speaker_id": "2",
          "time": "20210820 23:02"
        },
        {
          "id": "MDRW2100003432.1.5",
          "form": "울 멍멍이 신났겠네",
          "original_form": "울 멍멍이 신났겠네",
          "speaker_id": "1",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003432.1.6",
          "form": "공원에 있는 산에 갔다왔어?",
          "original_form": "공원에 있는 산에 갔다왔어?",
          "speaker_id": "1",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003432.1.7",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003432.1.8",
          "form": "거기가 젤 좋다",
          "original_form": "거기가 젤 좋다",
          "speaker_id": "2",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003432.1.9",
          "form": "글쿠만 ㅋㅋ 여기 집 뒤에 있는 산도 괜찮았는데",
          "original_form": "글쿠만 ㅋㅋ 여기 집 뒤에 있는 산도 괜찮았는데",
          "speaker_id": "1",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003432.1.10",
          "form": "거긴 좀 낮아서 별로",
          "original_form": "거긴 좀 낮아서 별로",
          "speaker_id": "2",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003432.1.11",
          "form": "넘 높으면 힘들당",
          "original_form": "넘 높으면 힘들당",
          "speaker_id": "1",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003432.1.12",
          "form": "운동이 되야지",
          "original_form": "운동이 되야지",
          "speaker_id": "2",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003432.1.13",
          "form": "ㅠㅠ 등산은 넘힘들엉",
          "original_form": "ㅠㅠ 등산은 넘힘들엉",
          "speaker_id": "1",
          "time": "20210820 23:04"
        },
        {
          "id": "MDRW2100003432.1.14",
          "form": "그냥 산책 하는 정도가 좋다 ㅎㅎ",
          "original_form": "그냥 산책 하는 정도가 좋다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 23:04"
        },
        {
          "id": "MDRW2100003432.1.15",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210820 23:04"
        },
        {
          "id": "MDRW2100003432.1.16",
          "form": "넘 빡세게하면 무릎에 안좋다",
          "original_form": "넘 빡세게하면 무릎에 안좋다",
          "speaker_id": "2",
          "time": "20210820 23:04"
        },
        {
          "id": "MDRW2100003432.1.17",
          "form": "그렇쥬ㅋㅋ 뭐든 적당히",
          "original_form": "그렇쥬ㅋㅋ 뭐든 적당히",
          "speaker_id": "1",
          "time": "20210820 23:04"
        },
        {
          "id": "MDRW2100003432.1.18",
          "form": "우리 멍멍이들도 관절 좋아야하니까",
          "original_form": "우리 멍멍이들도 관절 좋아야하니까",
          "speaker_id": "1",
          "time": "20210820 23:04"
        },
        {
          "id": "MDRW2100003432.1.19",
          "form": "조심해야한다",
          "original_form": "조심해야한다",
          "speaker_id": "1",
          "time": "20210820 23:04"
        },
        {
          "id": "MDRW2100003432.1.20",
          "form": "그래 아직 튼튼하지만 조심해야지",
          "original_form": "그래 아직 튼튼하지만 조심해야지",
          "speaker_id": "2",
          "time": "20210820 23:05"
        }
      ]
