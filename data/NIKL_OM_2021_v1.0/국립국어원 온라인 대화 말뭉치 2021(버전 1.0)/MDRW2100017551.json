      "utterance": [
        {
          "id": "MDRW2100017551.1.1",
          "form": "난 집안일 살림 똥손이야",
          "original_form": "난 집안일 살림 똥손이야",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017551.1.2",
          "form": "주방은 남편 영역",
          "original_form": "주방은 남편 영역",
          "speaker_id": "2",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017551.1.3",
          "form": "와 좋은 남편이다",
          "original_form": "와 좋은 남편이다",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017551.1.4",
          "form": "둘중에 잘하는사람이 하능거쥐",
          "original_form": "둘중에 잘하는사람이 하능거쥐",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017551.1.5",
          "form": "난 요리하는거 좋아해",
          "original_form": "난 요리하는거 좋아해",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017551.1.6",
          "form": "근데 요즘은 다 귀차나",
          "original_form": "근데 요즘은 다 귀차나",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017551.1.7",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017551.1.8",
          "form": "남편이 가정적인가보네",
          "original_form": "남편이 가정적인가보네",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017551.1.9",
          "form": "엄청 ㅎㅎ",
          "original_form": "엄청 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017551.1.10",
          "form": "너무너무 좋아",
          "original_form": "너무너무 좋아",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017551.1.11",
          "form": "난 10월달까지 일하고 그만두고 놀거야",
          "original_form": "난 10월달까지 일하고 그만두고 놀거야",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017551.1.12",
          "form": "크 ㅠㅠ",
          "original_form": "크 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017551.1.13",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017551.1.14",
          "form": "그게 최고지 가정적인거",
          "original_form": "그게 최고지 가정적인거",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017551.1.15",
          "form": "미리축하해",
          "original_form": "미리축하해",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017551.1.16",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017551.1.17",
          "form": "11월에 아기 태어남",
          "original_form": "11월에 아기 태어남",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017551.1.18",
          "form": "육아로 종목변경이지",
          "original_form": "육아로 종목변경이지",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017551.1.19",
          "form": "헉ㅠㅠ 대박",
          "original_form": "헉ㅠㅠ 대박",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017551.1.20",
          "form": "축하해ㅜㅜㅜㅜㅜ",
          "original_form": "축하해ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017551.1.21",
          "form": "무서워 출산",
          "original_form": "무서워 출산",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017551.1.22",
          "form": "나는 아직 생각없어",
          "original_form": "나는 아직 생각없어",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017551.1.23",
          "form": "무서워",
          "original_form": "무서워",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017551.1.24",
          "form": "나도 생각없었는데 속도위반 해버림",
          "original_form": "나도 생각없었는데 속도위반 해버림",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017551.1.25",
          "form": "딩크하고싶은데",
          "original_form": "딩크하고싶은데",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017551.1.26",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017551.1.27",
          "form": "딩크가 최곤듯",
          "original_form": "딩크가 최곤듯",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017551.1.28",
          "form": "근데 자식있는것도 괜찮을거같긴해",
          "original_form": "근데 자식있는것도 괜찮을거같긴해",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017551.1.29",
          "form": "난 좀더 놀고 미루다가",
          "original_form": "난 좀더 놀고 미루다가",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017551.1.30",
          "form": "임신과 출산은 생각보다 많이 힘든 일이야",
          "original_form": "임신과 출산은 생각보다 많이 힘든 일이야",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017551.1.31",
          "form": "하나 낳기로 햇어",
          "original_form": "하나 낳기로 햇어",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017551.1.32",
          "form": "응 하나낳아서 잘 키움 장땡",
          "original_form": "응 하나낳아서 잘 키움 장땡",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017551.1.33",
          "form": "더 내려놓고 더 놀고ㅜㅜ",
          "original_form": "더 내려놓고 더 놀고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017551.1.34",
          "form": "35살 전에만 낳으믄 갠츈",
          "original_form": "35살 전에만 낳으믄 갠츈",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017551.1.35",
          "form": "그 이후로는 노산이라",
          "original_form": "그 이후로는 노산이라",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017551.1.36",
          "form": "지금이 넘좋아ㅠㅠ",
          "original_form": "지금이 넘좋아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017551.1.37",
          "form": "놀고싶다 술마시고",
          "original_form": "놀고싶다 술마시고",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017551.1.38",
          "form": "조카랑 놀때마다 딩크생각듬",
          "original_form": "조카랑 놀때마다 딩크생각듬",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017551.1.39",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017551.1.40",
          "form": "조카몇살임?",
          "original_form": "조카몇살임?",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017551.1.41",
          "form": "네살",
          "original_form": "네살",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017551.1.42",
          "form": "일주일에 하루 놀아주는데",
          "original_form": "일주일에 하루 놀아주는데",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017551.1.43",
          "form": "넘넘 힘드러ㅜㅜ",
          "original_form": "넘넘 힘드러ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 20:36"
        }
      ]
    }
