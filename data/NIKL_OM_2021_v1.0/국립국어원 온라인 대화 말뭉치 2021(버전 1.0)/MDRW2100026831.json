      "utterance": [
        {
          "id": "MDRW2100026831.1.1",
          "form": "암튼 문제네요ㅎ",
          "original_form": "암튼 문제네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026831.1.2",
          "form": "나라면 그렇게 생각안했을껀데ㅎ",
          "original_form": "나라면 그렇게 생각안했을껀데ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100026831.1.3",
          "form": "뭐 따로 생각이있을꺼에요ㅎㅎ",
          "original_form": "뭐 따로 생각이있을꺼에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026831.1.4",
          "form": "음 사람은 다 생각이 다르니",
          "original_form": "음 사람은 다 생각이 다르니",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026831.1.5",
          "form": "맨날 name1한테 아내욕만하네요ㅎ",
          "original_form": "맨날 &name1&한테 아내욕만하네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026831.1.6",
          "form": "일단 존중해주셔욮ㅎㅎ",
          "original_form": "일단 존중해주셔욮ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026831.1.7",
          "form": "필요에 의해서 샀을꺼니깐요ㅎ",
          "original_form": "필요에 의해서 샀을꺼니깐요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026831.1.8",
          "form": "존중해요ㅎ 항상 생각하고ㅎ",
          "original_form": "존중해요ㅎ 항상 생각하고ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100026831.1.9",
          "form": "근데 오늘같은날은 이해가안되서ㅎ",
          "original_form": "근데 오늘같은날은 이해가안되서ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026831.1.10",
          "form": "아이때문에 차를 사는건 당연히 맞고ㅎ 그래야한다고생각해요ㅎ",
          "original_form": "아이때문에 차를 사는건 당연히 맞고ㅎ 그래야한다고생각해요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026831.1.11",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026831.1.12",
          "form": "그러면 그냥 기존에 쓰던 차를 그냥 쓰면되지",
          "original_form": "그러면 그냥 기존에 쓰던 차를 그냥 쓰면되지",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026831.1.13",
          "form": "그리고 나중에 어느정도 돈모으고 바꾸는게 옳은 방법아닌가 싶은데",
          "original_form": "그리고 나중에 어느정도 돈모으고 바꾸는게 옳은 방법아닌가 싶은데",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100026831.1.14",
          "form": "아 차가 있는데 다시 사는거에요?",
          "original_form": "아 차가 있는데 다시 사는거에요?",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026831.1.15",
          "form": "아무래도 큰차를 그냥 사고싶은것같애요 작은차는 불편하니",
          "original_form": "아무래도 큰차를 그냥 사고싶은것같애요 작은차는 불편하니",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100026831.1.16",
          "form": "재산분할합의할때 차는 필요없다거하더라구요",
          "original_form": "재산분할합의할때 차는 필요없다거하더라구요",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026831.1.17",
          "form": "아하   에훔",
          "original_form": "아하   에훔",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026831.1.18",
          "form": "아반떼인데ㅎ 아직은 쓸만하고",
          "original_form": "아반떼인데ㅎ 아직은 쓸만하고",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026831.1.19",
          "form": "3만이제 된차에요ㅎ",
          "original_form": "3만이제 된차에요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100026831.1.20",
          "form": "음 그럼 안사셔도 될듯한데",
          "original_form": "음 그럼 안사셔도 될듯한데",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026831.1.21",
          "form": "암튼 제 생각하고 많이달라요ㅎ",
          "original_form": "암튼 제 생각하고 많이달라요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026831.1.22",
          "form": "새로운 마음으로 타고 싶나ㅜ해야죠 뭐",
          "original_form": "새로운 마음으로 타고 싶나ㅜ해야죠 뭐",
          "speaker_id": "1",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026831.1.23",
          "form": "예전에는 아우디이야기했는데요뭐ㅎ",
          "original_form": "예전에는 아우디이야기했는데요뭐ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026831.1.24",
          "form": "암튼 보기에는 좋아보이는차를 사고싶나봐요",
          "original_form": "암튼 보기에는 좋아보이는차를 사고싶나봐요",
          "speaker_id": "2",
          "time": "20210902 00:35"
        },
        {
          "id": "MDRW2100026831.1.25",
          "form": "전 마티즈를타도 내가 떳떳하면 괜찮다고 생각하는데 아내는 그런건 아니에요ㅎㅎ",
          "original_form": "전 마티즈를타도 내가 떳떳하면 괜찮다고 생각하는데 아내는 그런건 아니에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026831.1.26",
          "form": "헐 제가 아는 누궁가가생각나네여 ㅎㅎ",
          "original_form": "헐 제가 아는 누궁가가생각나네여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:36"
        },
        {
          "id": "MDRW2100026831.1.27",
          "form": "아 친구분이요?ㅎ",
          "original_form": "아 친구분이요?ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:37"
        },
        {
          "id": "MDRW2100026831.1.28",
          "form": "그친구분이 제 아내는 아니겟죠?ㅎㅎㅎ",
          "original_form": "그친구분이 제 아내는 아니겟죠?ㅎㅎㅎ",
