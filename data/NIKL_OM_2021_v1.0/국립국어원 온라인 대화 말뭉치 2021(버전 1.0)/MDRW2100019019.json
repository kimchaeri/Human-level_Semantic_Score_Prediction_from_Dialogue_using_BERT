        },
        {
          "id": "MDRW2100019019.1.2",
          "form": "점심은 먹었뉘",
          "original_form": "점심은 먹었뉘",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019019.1.3",
          "form": "넹...빵조금 먹었어욤...ㅎ",
          "original_form": "넹...빵조금 먹었어욤...ㅎ",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019019.1.4",
          "form": "불금인데\r\n저녁 메뉴 정하셨어여?",
          "original_form": "불금인데\r\n저녁 메뉴 정하셨어여?",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019019.1.5",
          "form": "저 집에",
          "original_form": "저 집에",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019019.1.6",
          "form": "대창",
          "original_form": "대창",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019019.1.7",
          "form": "사둔게이ㅛ어서",
          "original_form": "사둔게이ㅛ어서",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019019.1.8",
          "form": "그거먹을라꼬요 ㅋㅋㅋ",
          "original_form": "그거먹을라꼬요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019019.1.9",
          "form": "대창? 꾸워먹나요? ㅋㅋ",
          "original_form": "대창? 꾸워먹나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019019.1.10",
          "form": "네 구워먹죵 ㅋㅋㅋ",
          "original_form": "네 구워먹죵 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019019.1.11",
          "form": "곱창도 맛닜는데",
          "original_form": "곱창도 맛닜는데",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019019.1.12",
          "form": "대창도맛있어욮",
          "original_form": "대창도맛있어욮",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019019.1.13",
          "form": "대창 어디서 사나염ㅋㅋㅋ",
          "original_form": "대창 어디서 사나염ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019019.1.14",
          "form": "저 이음카드 쓰는데",
          "original_form": "저 이음카드 쓰는데",
          "speaker_id": "2",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019019.1.15",
          "form": "거기서 세일해서",
          "original_form": "거기서 세일해서",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019019.1.16",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019019.1.17",
          "form": "대창이 어디서 팔죠?",
          "original_form": "대창이 어디서 팔죠?",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019019.1.18",
          "form": "정육점? ㅋㅋ 대창집?",
          "original_form": "정육점? ㅋㅋ 대창집?",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019019.1.19",
          "form": "서구몰이라고",
          "original_form": "서구몰이라고",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019019.1.20",
          "form": "온라인 주문으로했엉..",
          "original_form": "온라인 주문으로했엉..",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019019.1.21",
          "form": "언니도 거기...지역화폐???",
          "original_form": "언니도 거기...지역화폐???",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019019.1.22",
          "form": "있으?",
          "original_form": "있으?",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019019.1.23",
          "form": "그럼요:)\r\n경기 무시하세요?\r\n저희 착한페이 있습니다ㅋㅋ\r\n저희도 그걸로 배달도 시켜먹고\r\n장도 보고 다합니다 ㅋㅋ",
          "original_form": "그럼요:)\r\n경기 무시하세요?\r\n저희 착한페이 있습니다ㅋㅋ\r\n저희도 그걸로 배달도 시켜먹고\r\n장도 보고 다합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100019019.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그쵸그쵸 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그쵸그쵸 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:20"
        },
        {
          "id": "MDRW2100019019.1.25",
          "form": "무시한건아니고 ..ㅎㅎ...",
          "original_form": "무시한건아니고 ..ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210827 17:20"
