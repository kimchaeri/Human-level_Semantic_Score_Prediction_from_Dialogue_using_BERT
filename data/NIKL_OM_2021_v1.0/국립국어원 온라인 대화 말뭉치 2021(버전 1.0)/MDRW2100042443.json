      "utterance": [
        {
          "id": "MDRW2100042443.1.1",
          "form": "저도욬ㅋㅋㅋ 빵이 더 좋아요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도욬ㅋㅋㅋ 빵이 더 좋아요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042443.1.2",
          "form": "좋아요 ㅎㅎㅎㅎ",
          "original_form": "좋아요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042443.1.3",
          "form": "맛있는 레몬케이크 이길 빕니다 ㅎㅎ",
          "original_form": "맛있는 레몬케이크 이길 빕니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042443.1.4",
          "form": "안해보고 또 해보고 싶다 생각할테니까 만들어보고 후회하는게 나을거 같아요 ㅋㅋㅋㅋ",
          "original_form": "안해보고 또 해보고 싶다 생각할테니까 만들어보고 후회하는게 나을거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042443.1.5",
          "form": "그료그쵸",
          "original_form": "그료그쵸",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042443.1.6",
          "form": "한번 사는인생인데",
          "original_form": "한번 사는인생인데",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042443.1.7",
          "form": "넹 레몬을 사고 싶은 그날",
          "original_form": "넹 레몬을 사고 싶은 그날",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042443.1.8",
          "form": "그냥 해버리는게 ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 해버리는게 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042443.1.9",
          "form": "물건도 사버리고",
          "original_form": "물건도 사버리고",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042443.1.10",
          "form": "ㅋㅋㅋㅋ 맞아여",
          "original_form": "ㅋㅋㅋㅋ 맞아여",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042443.1.11",
          "form": "ㅌㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042443.1.12",
          "form": "과일도 그래서 비싼걸로 사먹고 있어요ㅋㅋㅋㅋㅋ",
          "original_form": "과일도 그래서 비싼걸로 사먹고 있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042443.1.13",
          "form": "언제 또 먹을지 몰라 이러면서",
          "original_form": "언제 또 먹을지 몰라 이러면서",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042443.1.14",
          "form": "과일은 확실히 비싼게 너무 맛있어라구요",
          "original_form": "과일은 확실히 비싼게 너무 맛있어라구요",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042443.1.15",
          "form": "저 올해 사먹은 수박이 제일 맛있었어요",
          "original_form": "저 올해 사먹은 수박이 제일 맛있었어요",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042443.1.16",
          "form": "강고집 수박이라고 내년에 기회되면 드셔보세요",
          "original_form": "강고집 수박이라고 내년에 기회되면 드셔보세요",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042443.1.17",
          "form": "거의 업체 사람처럼 심심이에서 추천하는거 같은데요ㅋㅋㅋㅋㅋ",
          "original_form": "거의 업체 사람처럼 심심이에서 추천하는거 같은데요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042443.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042443.1.19",
          "form": "껍데기 얇고 껍데기 근처까지 맛있어서 설탕 들어갔나 의심했습니다",
          "original_form": "껍데기 얇고 껍데기 근처까지 맛있어서 설탕 들어갔나 의심했습니다",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042443.1.20",
          "form": "네 ㅋㅋㅋㅋ 강고집 수박 기록해봅니다",
          "original_form": "네 ㅋㅋㅋㅋ 강고집 수박 기록해봅니다",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042443.1.21",
          "form": "우와 ㅋㅋㅋ 궁금해욬ㅋㅋㅋㅋㅋ",
          "original_form": "우와 ㅋㅋㅋ 궁금해욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042443.1.22",
          "form": "ㅋㅋㅋㅋ 근데 다른 수박 사먹어보니 아 강고집수박 맛있는걸 괜히 의심했구나",
          "original_form": "ㅋㅋㅋㅋ 근데 다른 수박 사먹어보니 아 강고집수박 맛있는걸 괜히 의심했구나",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042443.1.23",
          "form": "그랬어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그랬어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042443.1.24",
          "form": "오홍ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오홍ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042443.1.25",
          "form": "정말 궁금하네욬ㅋㅋㅋ 여름이 다가서 아쉽 ㅠㅜ",
          "original_form": "정말 궁금하네욬ㅋㅋㅋ 여름이 다가서 아쉽 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042443.1.26",
          "form": "현대식품관에서 팔길래 사먹어봤는데 진짜 맛있었어요",
          "original_form": "현대식품관에서 팔길래 사먹어봤는데 진짜 맛있었어요",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042443.1.27",
          "form": "초여름에 파니까 늦지 않게 드셔야해요",
          "original_form": "초여름에 파니까 늦지 않게 드셔야해요",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042443.1.28",
          "form": "으아 현대식품관ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "으아 현대식품관ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042443.1.29",
          "form": "인터넷으로 드신게아니구나",
          "original_form": "인터넷으로 드신게아니구나",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042443.1.30",
          "form": "얼마정도 하나요?",
          "original_form": "얼마정도 하나요?",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042443.1.31",
          "form": "현대식품관 몰에서 샀어여!!",
          "original_form": "현대식품관 몰에서 샀어여!!",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042443.1.32",
          "form": "오만원 이상 사면 만원 할인해주더라고요",
          "original_form": "오만원 이상 사면 만원 할인해주더라고요",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042443.1.33",
          "form": "수박이 삼만오천원에서 사만원 사이였어요",
          "original_form": "수박이 삼만오천원에서 사만원 사이였어요",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042443.1.34",
          "form": "오오오 역시 비싼게맛있러요",
          "original_form": "오오오 역시 비싼게맛있러요",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042443.1.35",
          "form": "맛없으면 억울한 가격인데요??",
          "original_form": "맛없으면 억울한 가격인데요??",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042443.1.36",
          "form": "그러니까요 시킬때는 과연 맛있을까 했는데",
          "original_form": "그러니까요 시킬때는 과연 맛있을까 했는데",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042443.1.37",
          "form": "먹어보고 이마트에서 세번 시킬 수박 한번만 먹자 했어요ㅋㅋㅋㅋㅋ",
          "original_form": "먹어보고 이마트에서 세번 시킬 수박 한번만 먹자 했어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042443.1.38",
          "form": "그러니가욬ㅋㅋ그정도면 세번란먹고 한번먹어도될꺼같아요",
          "original_form": "그러니가욬ㅋㅋ그정도면 세번란먹고 한번먹어도될꺼같아요",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042443.1.39",
          "form": "레이니어래옄ㅋㅋㅋ",
          "original_form": "레이니어래옄ㅋㅋㅋ",
