      "utterance": [
        {
          "id": "MDRW2100018185.1.1",
          "form": "일단 무조건 서울 중상위는 가야해서",
          "original_form": "일단 무조건 서울 중상위는 가야해서",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018185.1.2",
          "form": "지금 전공은 문예창작이요",
          "original_form": "지금 전공은 문예창작이요",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018185.1.3",
          "form": "그럼 휴학하시는동안",
          "original_form": "그럼 휴학하시는동안",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018185.1.4",
          "form": "학사하나따서",
          "original_form": "학사하나따서",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018185.1.5",
          "form": "학사편입으로 하셔요",
          "original_form": "학사편입으로 하셔요",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018185.1.6",
          "form": "진짜 경쟁률도 다르고 경쟁하는 사람들 자체도",
          "original_form": "진짜 경쟁률도 다르고 경쟁하는 사람들 자체도",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.7",
          "form": "학사로 가셨어요?",
          "original_form": "학사로 가셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.8",
          "form": "다른것같아요",
          "original_form": "다른것같아요",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.9",
          "form": "네 일반 실패하고",
          "original_form": "네 일반 실패하고",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.10",
          "form": "학사편입했어요 ㅠㅠ",
          "original_form": "학사편입했어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.11",
          "form": "근데 지금 너무 늦은 거 같아서..  ㅜㅜ",
          "original_form": "근데 지금 너무 늦은 거 같아서..  ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.12",
          "form": "26살에 졸업했어요 ㅋㅋ",
          "original_form": "26살에 졸업했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.13",
          "form": "저도 딱 올해 성공해서 들어가면",
          "original_form": "저도 딱 올해 성공해서 들어가면",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.14",
          "form": "24살에 3학년으로 편입하고",
          "original_form": "24살에 3학년으로 편입하고",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.15",
          "form": "그때 졸업이에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그때 졸업이에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.17",
          "form": "근데 막상 학교가면",
          "original_form": "근데 막상 학교가면",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018185.1.18",
          "form": "막내일듯",
          "original_form": "막내일듯",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018185.1.19",
          "form": "편입동기중에 그때 제가 어린편이였어요",
          "original_form": "편입동기중에 그때 제가 어린편이였어요",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018185.1.20",
          "form": "아 진짜요? 근데 그럼 좀 높은 곳으로 가셔서",
          "original_form": "아 진짜요? 근데 그럼 좀 높은 곳으로 가셔서",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018185.1.21",
          "form": "그런 거 같은데 원래 상위권 학교일수록",
          "original_form": "그런 거 같은데 원래 상위권 학교일수록",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018185.1.22",
          "form": "아뇨........ㅋㅋㅋ저 하위권  ㅠㅠ흑흑",
          "original_form": "아뇨........ㅋㅋㅋ저 하위권  ㅠㅠ흑흑",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018185.1.23",
          "form": "나이대가 높아져서",
          "original_form": "나이대가 높아져서",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018185.1.24",
          "form": "그래도 학벌세탁은 잘햇다고봐요ㅠㅠ제기준은",
          "original_form": "그래도 학벌세탁은 잘햇다고봐요ㅠㅠ제기준은",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018185.1.25",
          "form": "전적대가 전문대였거든요",
          "original_form": "전적대가 전문대였거든요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018185.1.26",
          "form": "아 편입 성공하면 상위에요 상위! ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 편입 성공하면 상위에요 상위! ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018185.1.27",
          "form": "편입얘기하니 말이 술술나오네요",
          "original_form": "편입얘기하니 말이 술술나오네요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018185.1.28",
          "form": "공부하는법도 잘 모르는데 영어공부하느라",
          "original_form": "공부하는법도 잘 모르는데 영어공부하느라",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018185.1.29",
          "form": "무지 힘들었어요 ㅠ_ㅠ",
          "original_form": "무지 힘들었어요 ㅠ_ㅠ",
          "speaker_id": "1",
          "time": "20210826 23:10"
        }
      ]
