      "utterance": [
        {
          "id": "MDRW2100040535.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "2",
          "time": "20210904 22:37"
        },
        {
          "id": "MDRW2100040535.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040535.1.3",
          "form": "name2님은 키우시나요?",
          "original_form": "&name2&님은 키우시나요?",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040535.1.4",
          "form": "아뇨 키우고싶다는 생각만요ㅜ",
          "original_form": "아뇨 키우고싶다는 생각만요ㅜ",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040535.1.5",
          "form": "어떤 동물 키우고 싶으신가요??",
          "original_form": "어떤 동물 키우고 싶으신가요??",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100040535.1.6",
          "form": "저는 고양이요",
          "original_form": "저는 고양이요",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100040535.1.7",
          "form": "강아지가 좋으세요 고양이가 좋으세요?",
          "original_form": "강아지가 좋으세요 고양이가 좋으세요?",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100040535.1.8",
          "form": "저도 고양이요",
          "original_form": "저도 고양이요",
          "speaker_id": "1",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040535.1.9",
          "form": "키우시고 싶은 종이 있으신가요?",
          "original_form": "키우시고 싶은 종이 있으신가요?",
          "speaker_id": "1",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040535.1.10",
          "form": "전 폴드요",
          "original_form": "전 폴드요",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040535.1.11",
          "form": "유전적으로 안좋은건아는데",
          "original_form": "유전적으로 안좋은건아는데",
          "speaker_id": "2",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100040535.1.12",
          "form": "너무 귀여워요",
          "original_form": "너무 귀여워요",
          "speaker_id": "2",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100040535.1.13",
          "form": "name1님은 어떤종이 좋으세요?",
          "original_form": "&name1&님은 어떤종이 좋으세요?",
          "speaker_id": "2",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100040535.1.14",
          "form": "저는 그냥 종 상관없이 하얀고양이요",
          "original_form": "저는 그냥 종 상관없이 하얀고양이요",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040535.1.15",
          "form": "그럼 반려동물을 키워보신 적도 없으신가요?",
          "original_form": "그럼 반려동물을 키워보신 적도 없으신가요?",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040535.1.16",
          "form": "네 가족이 싫어해서 ㅜㅜ",
          "original_form": "네 가족이 싫어해서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040535.1.17",
          "form": "키우신적있으세요?",
          "original_form": "키우신적있으세요?",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040535.1.18",
          "form": "저도요ㅠㅠㅠ",
          "original_form": "저도요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040535.1.19",
          "form": "그럼 혼자사시면 키우실 생각이 있으신건가요?",
          "original_form": "그럼 혼자사시면 키우실 생각이 있으신건가요?",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040535.1.20",
          "form": "동지시네요",
          "original_form": "동지시네요",
          "speaker_id": "2",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040535.1.21",
          "form": "엄청 고민할거 같긴해요",
          "original_form": "엄청 고민할거 같긴해요",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040535.1.22",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040535.1.23",
          "form": "혼자둬야하고 돈드는것도 무시 못하니까요",
          "original_form": "혼자둬야하고 돈드는것도 무시 못하니까요",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040535.1.24",
          "form": "맞아요 그런 고충이 또 있죠ㅠㅠ",
          "original_form": "맞아요 그런 고충이 또 있죠ㅠㅠ",
