        {
          "id": "MDRW2100015830.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015830.1.2",
          "form": "보통이요?",
          "original_form": "보통이요?",
          "speaker_id": "1",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015830.1.3",
          "form": "!",
          "original_form": "!",
          "speaker_id": "1",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015830.1.4",
          "form": "어떤 공연 좋아하세요??",
          "original_form": "어떤 공연 좋아하세요??",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015830.1.5",
          "form": "전 밴드공연 좋아해요ㅋㅋ",
          "original_form": "전 밴드공연 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015830.1.6",
          "form": "무슨 밴드 좋아하세요??",
          "original_form": "무슨 밴드 좋아하세요??",
          "speaker_id": "1",
          "time": "20210826 01:40"
        },
        {
          "id": "MDRW2100015830.1.7",
          "form": "국내인디밴드요 두루두루 좋아해요",
          "original_form": "국내인디밴드요 두루두루 좋아해요",
          "speaker_id": "2",
          "time": "20210826 01:40"
        },
        {
          "id": "MDRW2100015830.1.8",
          "form": "님은 좋아하는 밴드 있어요??",
          "original_form": "님은 좋아하는 밴드 있어요??",
          "speaker_id": "2",
          "time": "20210826 01:41"
        },
        {
          "id": "MDRW2100015830.1.9",
          "form": "혹시 바이바이배드맨 아세요?",
          "original_form": "혹시 바이바이배드맨 아세요?",
          "speaker_id": "1",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015830.1.10",
          "form": "알아요ㅋㅋ",
          "original_form": "알아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015830.1.11",
          "form": "페스티벌 가서 봤었어요ㅋㅋ",
          "original_form": "페스티벌 가서 봤었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015830.1.12",
          "form": "오 꽤 오래전인데",
          "original_form": "오 꽤 오래전인데",
          "speaker_id": "1",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015830.1.13",
          "form": "어떤 밴드 좋아하세요?",
          "original_form": "어떤 밴드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100015830.1.14",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:45"
        },
        {
          "id": "MDRW2100015830.1.15",
          "form": "음 엄청 많아요ㅋㅋㅋ",
          "original_form": "음 엄청 많아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:45"
        },
        {
          "id": "MDRW2100015830.1.16",
          "form": "페스티벌 라인업 거의 다 좋아했었어요ㅋㅋㅋㅋ",
          "original_form": "페스티벌 라인업 거의 다 좋아했었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:45"
        },
        {
          "id": "MDRW2100015830.1.17",
          "form": "그렇군요ㅎㅎ",
          "original_form": "그렇군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:45"
        },
        {
          "id": "MDRW2100015830.1.18",
          "form": "하나를 못고르겠어요ㅠㅋㅋㅋ",
          "original_form": "하나를 못고르겠어요ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100015830.1.19",
          "form": "코로나 돌고는 공연 안가본지 오래됐네요ㅠ",
          "original_form": "코로나 돌고는 공연 안가본지 오래됐네요ㅠ",
