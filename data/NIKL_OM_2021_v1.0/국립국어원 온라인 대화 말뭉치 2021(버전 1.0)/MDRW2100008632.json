        },
        {
          "id": "MDRW2100008632.1.2",
          "form": "아뇨 잘 몰라요",
          "original_form": "아뇨 잘 몰라요",
          "speaker_id": "2",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008632.1.3",
          "form": "배구는요?",
          "original_form": "배구는요?",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008632.1.4",
          "form": "이번 올림픽때만 봤어요",
          "original_form": "이번 올림픽때만 봤어요",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008632.1.5",
          "form": "김연경",
          "original_form": "김연경",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008632.1.6",
          "form": "저도요 ㅎㅎ",
          "original_form": "저도요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008632.1.7",
          "form": "야구는??",
          "original_form": "야구는??",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008632.1.8",
          "form": "잘 몰라요 스포츠는 ㅎㅎ",
          "original_form": "잘 몰라요 스포츠는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008632.1.9",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008632.1.10",
          "form": "저도 사실 그래요 ㅎㅎ",
          "original_form": "저도 사실 그래요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008632.1.11",
          "form": "야구장 한번만 가본 적 있네요",
          "original_form": "야구장 한번만 가본 적 있네요",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008632.1.12",
          "form": "저도 딱 한번 가봤어요",
          "original_form": "저도 딱 한번 가봤어요",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100008632.1.13",
          "form": "어느 팀 보셨어요?",
          "original_form": "어느 팀 보셨어요?",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
