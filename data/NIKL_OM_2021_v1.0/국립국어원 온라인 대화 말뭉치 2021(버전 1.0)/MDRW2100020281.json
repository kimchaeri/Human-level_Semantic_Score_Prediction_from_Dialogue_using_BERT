        },
        {
          "id": "MDRW2100020281.1.2",
          "form": "오 화장실텅소",
          "original_form": "오 화장실텅소",
          "speaker_id": "1",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.3",
          "form": "뭐더라그거?",
          "original_form": "뭐더라그거?",
          "speaker_id": "2",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.4",
          "form": "개수구..?",
          "original_form": "개수구..?",
          "speaker_id": "2",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.5",
          "form": "정리가 제일 싫음",
          "original_form": "정리가 제일 싫음",
          "speaker_id": "2",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.6",
          "form": "ㅇㅎ 그거요즘",
          "original_form": "ㅇㅎ 그거요즘",
          "speaker_id": "1",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.7",
          "form": "음식물을 좀 혐오하는 경향이 잇음",
          "original_form": "음식물을 좀 혐오하는 경향이 잇음",
          "speaker_id": "2",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.8",
          "form": "정리용품",
          "original_form": "정리용품",
          "speaker_id": "1",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.9",
          "form": "되게잘나와서",
          "original_form": "되게잘나와서",
          "speaker_id": "1",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.10",
          "form": "하라면 하지만 역겨워하는 경우가 다수 있음",
          "original_form": "하라면 하지만 역겨워하는 경우가 다수 있음",
          "speaker_id": "2",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.11",
          "form": "오 다행이다 그건",
          "original_form": "오 다행이다 그건",
          "speaker_id": "2",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.12",
          "form": "그리고 청소기 유선일시 좀 싫어함",
          "original_form": "그리고 청소기 유선일시 좀 싫어함",
          "speaker_id": "2",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.13",
          "form": "쏙빼서 바로 버리는게 잇더라고요",
          "original_form": "쏙빼서 바로 버리는게 잇더라고요",
          "speaker_id": "1",
          "time": "20210828 00:58"
        },
        {
          "id": "MDRW2100020281.1.14",
          "form": "아오 욕이래개속",
          "original_form": "아오 욕이래개속",
          "speaker_id": "1",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020281.1.15",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020281.1.16",
          "form": "나도 그거 만",
          "original_form": "나도 그거 만",
          "speaker_id": "1",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020281.1.17",
          "form": "지는 거 싫어해서",
          "original_form": "지는 거 싫어해서",
          "speaker_id": "1",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020281.1.18",
          "form": "독립하면 꼭 그걸 달아놓ㄱㅎ싶다",
          "original_form": "독립하면 꼭 그걸 달아놓ㄱㅎ싶다",
          "speaker_id": "1",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020281.1.19",
          "form": "오 대박이다 나도",
          "original_form": "오 대박이다 나도",
          "speaker_id": "2",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020281.1.20",
          "form": "그리고 음악기술 다수 보유",
          "original_form": "그리고 음악기술 다수 보유",
          "speaker_id": "2",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020281.1.21",
          "form": "저는 화장실관련",
          "original_form": "저는 화장실관련",
          "speaker_id": "1",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020281.1.22",
          "form": "싫은거잇음",
          "original_form": "싫은거잇음",
          "speaker_id": "1",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020281.1.23",
          "form": "뭔데요",
          "original_form": "뭔데요",
          "speaker_id": "2",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020281.1.24",
          "form": "변기에 샤워하다 뭉튀기면",
          "original_form": "변기에 샤워하다 뭉튀기면",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.25",
          "form": "튀긴사람 알아서 닦기",
          "original_form": "튀긴사람 알아서 닦기",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.26",
          "form": "오 쉽지 ㅋ",
          "original_form": "오 쉽지 ㅋ",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.27",
          "form": "근데 커버 닫고",
          "original_form": "근데 커버 닫고",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.28",
          "form": "휴지 다쓴사람이 걸어놓기",
          "original_form": "휴지 다쓴사람이 걸어놓기",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.29",
          "form": "씻으면 되잲아",
          "original_form": "씻으면 되잲아",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.30",
          "form": "하 그거 기본인데",
          "original_form": "하 그거 기본인데",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.31",
          "form": "우리 집 남자들",
          "original_form": "우리 집 남자들",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.32",
          "form": "개 상 안지킴",
          "original_form": "개 상 안지킴",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.33",
          "form": "난 집에서 맨날",
          "original_form": "난 집에서 맨날",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.34",
          "form": "휴ㅣㅈ",
          "original_form": "휴ㅣㅈ",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.35",
          "form": "그 수건보돤함에",
          "original_form": "그 수건보돤함에",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.36",
          "form": "내가 쳐해야해서",
          "original_form": "내가 쳐해야해서",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.37",
          "form": "두개씩 채눠두는데",
          "original_form": "두개씩 채눠두는데",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.38",
          "form": "그거도",
          "original_form": "그거도",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.39",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.40",
          "form": "꼭 안채우더래?",
          "original_form": "꼭 안채우더래?",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.41",
          "form": "떵어지면",
          "original_form": "떵어지면",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.42",
          "form": "본사람이 채우래고",
          "original_form": "본사람이 채우래고",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.43",
          "form": "글고 휴지",
          "original_form": "글고 휴지",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.44",
          "form": "떨어졋아면 갈아야지",
          "original_form": "떨어졋아면 갈아야지",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.45",
          "form": "걍 휴지김 그대로 놓고",
          "original_form": "걍 휴지김 그대로 놓고",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.46",
          "form": "나오더라",
          "original_form": "나오더라",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.47",
          "form": "드질라고",
          "original_form": "드질라고",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020281.1.48",
          "form": "저는 또 할머니랑 살아서",
          "original_form": "저는 또 할머니랑 살아서",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.49",
          "form": "기본적인건 거의 내가 다해여해",
          "original_form": "기본적인건 거의 내가 다해여해",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.50",
          "form": "열어두면 세균쩐대뇨",
          "original_form": "열어두면 세균쩐대뇨",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.51",
          "form": "글쿠나 도랏네",
          "original_form": "글쿠나 도랏네",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.52",
          "form": "난 열어놓는거 싫어",
          "original_form": "난 열어놓는거 싫어",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.53",
          "form": "글고 냄새도",
          "original_form": "글고 냄새도",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.54",
          "form": "더난대요",
          "original_form": "더난대요",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.55",
          "form": "닫아야 덜난대",
          "original_form": "닫아야 덜난대",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.56",
          "form": "너무 당연하지맘",
          "original_form": "너무 당연하지맘",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.57",
          "form": "글고 세면대나 바닥에",
          "original_form": "글고 세면대나 바닥에",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.58",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.59",
          "form": "이물질 잇어도 싫어나중에",
          "original_form": "이물질 잇어도 싫어나중에",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020281.1.60",
          "form": "배수구 정리할대 토하더라도",
          "original_form": "배수구 정리할대 토하더라도",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.61",
          "form": "이ㅜㄹ질 전부",
          "original_form": "이ㅜㄹ질 전부",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.62",
          "form": "물로 쓸려보내야해",
          "original_form": "물로 쓸려보내야해",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.63",
          "form": "그거 샤워한사람이 치우기 해여함",
          "original_form": "그거 샤워한사람이 치우기 해여함",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.64",
          "form": "그니간",
          "original_form": "그니간",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.65",
          "form": "근데그런거는",
          "original_form": "근데그런거는",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.66",
          "form": "내가 좀 잘지키면될듯 ㅋ",
          "original_form": "내가 좀 잘지키면될듯 ㅋ",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.67",
          "form": "ㅋ ㄱㅇㄱ",
          "original_form": "ㅋ ㄱㅇㄱ",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.68",
          "form": "근데 나 머리카락 저일 이런거",
          "original_form": "근데 나 머리카락 저일 이런거",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.69",
          "form": "잘 안해요",
          "original_form": "잘 안해요",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.70",
          "form": "나중에 몰아서",
          "original_form": "나중에 몰아서",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.71",
          "form": "빼내가지고...",
          "original_form": "빼내가지고...",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.72",
          "form": "글고 우리집",
          "original_form": "글고 우리집",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020281.1.73",
          "form": "그니까 나도",
          "original_form": "그니까 나도",
          "speaker_id": "1",
          "time": "20210828 01:03"
        },
