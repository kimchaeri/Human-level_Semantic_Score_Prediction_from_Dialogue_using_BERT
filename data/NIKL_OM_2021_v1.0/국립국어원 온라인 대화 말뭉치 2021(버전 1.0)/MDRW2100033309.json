      "utterance": [
        {
          "id": "MDRW2100033309.1.1",
          "form": "반려동물얘기하자",
          "original_form": "반려동물얘기하자",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100033309.1.2",
          "form": "고양이는 정말 귀여운 생물이예요",
          "original_form": "고양이는 정말 귀여운 생물이예요",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100033309.1.3",
          "form": "맛습니다.고양이.귀여운.생물이죠.^^",
          "original_form": "맛습니다.고양이.귀여운.생물이죠.^^",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100033309.1.4",
          "form": "님은. 무슨 동물 좋아하시나요^^",
          "original_form": "님은. 무슨 동물 좋아하시나요^^",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100033309.1.5",
          "form": "동물.털.알러지가.잇어서.아가리로만.키웁이다.",
          "original_form": "동물.털.알러지가.잇어서.아가리로만.키웁이다.",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100033309.1.6",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100033309.1.7",
          "form": "전 물고기 키웠다.가",
          "original_form": "전 물고기 키웠다.가",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100033309.1.8",
          "form": "다 죽엇지뭐예요",
          "original_form": "다 죽엇지뭐예요",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100033309.1.9",
          "form": "당신.아사히나.마후유.입미까?",
          "original_form": "당신.아사히나.마후유.입미까?",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100033309.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100033309.1.11",
          "form": "제 방에는 수조가 없어요",
          "original_form": "제 방에는 수조가 없어요",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100033309.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033309.1.13",
          "form": "그건 ㅇㅈ이죠",
          "original_form": "그건 ㅇㅈ이죠",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100033309.1.14",
          "form": "우우",
          "original_form": "우우",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100033309.1.15",
          "form": "님 사료 뭐드세요",
          "original_form": "님 사료 뭐드세요",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100033309.1.16",
          "form": "전 로얄캐닌먹어요",
          "original_form": "전 로얄캐닌먹어요",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100033309.1.17",
          "form": "요즘.관절이.않좋아서.래비앙독.먹습미다.",
          "original_form": "요즘.관절이.않좋아서.래비앙독.먹습미다.",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100033309.1.18",
          "form": "뼈가아주.튼튼해지고.좋습미다.^^",
          "original_form": "뼈가아주.튼튼해지고.좋습미다.^^",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100033309.1.19",
          "form": "하.. 간식은 뭐드시나요",
          "original_form": "하.. 간식은 뭐드시나요",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100033309.1.20",
          "form": "전 요새 고구마간식이 좋더라구요",
          "original_form": "전 요새 고구마간식이 좋더라구요",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100033309.1.21",
          "form": "요즈음.북어채도.맛이.좋습니다.^^ 고구마간식도.맛잇고.돼지가.될것.같습니다.^^",
          "original_form": "요즈음.북어채도.맛이.좋습니다.^^ 고구마간식도.맛잇고.돼지가.될것.같습니다.^^",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100033309.1.22",
          "form": "주인이. 다이어트. 시킬것입니다.",
          "original_form": "주인이. 다이어트. 시킬것입니다.",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100033309.1.23",
          "form": "님 참치캔 드세요?",
          "original_form": "님 참치캔 드세요?",
