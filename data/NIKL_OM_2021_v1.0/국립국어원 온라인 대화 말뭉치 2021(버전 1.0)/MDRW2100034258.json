      "utterance": [
        {
          "id": "MDRW2100034258.1.1",
          "form": "혹시 중간에 무뽑듯이 사람들 뽑히는거도 보셨어요?",
          "original_form": "혹시 중간에 무뽑듯이 사람들 뽑히는거도 보셨어요?",
          "speaker_id": "1",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100034258.1.2",
          "form": "중간에 나가시는분들 이해가 가더라구요",
          "original_form": "중간에 나가시는분들 이해가 가더라구요",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034258.1.3",
          "form": "마자요 봤어요ㅠ",
          "original_form": "마자요 봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034258.1.4",
          "form": "맞아요 너무 힘들어요",
          "original_form": "맞아요 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034258.1.5",
          "form": "저도 그분들보면서",
          "original_form": "저도 그분들보면서",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034258.1.6",
          "form": "근데 다 끝나고나면 ...",
          "original_form": "근데 다 끝나고나면 ...",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034258.1.7",
          "form": "나도 나간다할까 싶더라구요",
          "original_form": "나도 나간다할까 싶더라구요",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034258.1.8",
          "form": "다음에 또 간답니다 ㅋㅋㅋㅋㅋ",
          "original_form": "다음에 또 간답니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034258.1.9",
          "form": "마자요ㅋㅋㅋㅋ",
          "original_form": "마자요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034258.1.10",
          "form": "따라나가시는 분들도 많이 계시죠",
          "original_form": "따라나가시는 분들도 많이 계시죠",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034258.1.11",
          "form": "그럴만해요ㅠ",
          "original_form": "그럴만해요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034258.1.12",
          "form": "마마무도 가보셨다고 했었나요??",
          "original_form": "마마무도 가보셨다고 했었나요??",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034258.1.13",
          "form": "저는 그래서 다시는 스탠딩 못가요ㅠ",
          "original_form": "저는 그래서 다시는 스탠딩 못가요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034258.1.14",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034258.1.15",
          "form": "여자아이돌 콘서트는 못가봐서 그러는데",
          "original_form": "여자아이돌 콘서트는 못가봐서 그러는데",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034258.1.16",
          "form": "여자아이돌 스탠딩도 그렇게 미나요? ㅠㅠ",
          "original_form": "여자아이돌 스탠딩도 그렇게 미나요? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034258.1.17",
          "form": "그후로는 다 좌석이었어서",
          "original_form": "그후로는 다 좌석이었어서",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100034258.1.18",
          "form": "근데 콘서트하면서",
          "original_form": "근데 콘서트하면서",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100034258.1.19",
          "form": "스탠딩분들한테 가수들이 조심하라고하거나 밀지말아달라고하는데",
          "original_form": "스탠딩분들한테 가수들이 조심하라고하거나 밀지말아달라고하는데",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100034258.1.20",
          "form": "마마무때는 안그랬던거같긴해요",
          "original_form": "마마무때는 안그랬던거같긴해요",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034258.1.21",
          "form": "아그래요?",
          "original_form": "아그래요?",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034258.1.22",
          "form": "좀 덜하긴한가봐요",
          "original_form": "좀 덜하긴한가봐요",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034258.1.23",
          "form": "저희는 무뽑혀가는거 가수가 다 쳐다보고 난리나요",
          "original_form": "저희는 무뽑혀가는거 가수가 다 쳐다보고 난리나요",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034258.1.24",
          "form": "진짜 난리도 아니죠ㅠ",
          "original_form": "진짜 난리도 아니죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100034258.1.25",
          "form": "팬들끼리 밀지 말라고",
          "original_form": "팬들끼리 밀지 말라고",
          "speaker_id": "2",
          "time": "20210827 22:07"
        }
      ]
    }
  ]
