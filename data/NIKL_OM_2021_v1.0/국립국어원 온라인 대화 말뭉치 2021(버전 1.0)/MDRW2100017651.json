      "utterance": [
        {
          "id": "MDRW2100017651.1.1",
          "form": "수강신청 성공함??",
          "original_form": "수강신청 성공함??",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017651.1.2",
          "form": "엉야 1순위 올클",
          "original_form": "엉야 1순위 올클",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017651.1.3",
          "form": "오올",
          "original_form": "오올",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017651.1.4",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017651.1.5",
          "form": "꿀교양은 없지만 강의평 좋은 걸로 담았으",
          "original_form": "꿀교양은 없지만 강의평 좋은 걸로 담았으",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017651.1.6",
          "form": "난 겨우 성공",
          "original_form": "난 겨우 성공",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017651.1.7",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017651.1.8",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017651.1.9",
          "form": "서버가 안 좋아서 힘들었지ㅠ",
          "original_form": "서버가 안 좋아서 힘들었지ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017651.1.10",
          "form": "넘 걱정했으",
          "original_form": "넘 걱정했으",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017651.1.11",
          "form": "튕겼다길래",
          "original_form": "튕겼다길래",
          "speaker_id": "1",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017651.1.12",
          "form": "40분동안 수강신청",
          "original_form": "40분동안 수강신청",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017651.1.13",
          "form": "실화냐고",
          "original_form": "실화냐고",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017651.1.14",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017651.1.15",
          "form": "계속 튕기고... 막 어휴...",
          "original_form": "계속 튕기고... 막 어휴...",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017651.1.16",
          "form": "우리는 자체 프로그램 만들어서 서버 터질 일은 없는듯",
          "original_form": "우리는 자체 프로그램 만들어서 서버 터질 일은 없는듯",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017651.1.17",
          "form": "와... 부럽다",
          "original_form": "와... 부럽다",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017651.1.18",
          "form": "그리고 학년 별로 다른 날에 해서 더 그런 거 같기도",
          "original_form": "그리고 학년 별로 다른 날에 해서 더 그런 거 같기도",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017651.1.19",
          "form": "우린 다 같이 함",
          "original_form": "우린 다 같이 함",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017651.1.20",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017651.1.21",
          "form": "그러니까 터지지",
          "original_form": "그러니까 터지지",
