      "utterance": [
        {
          "id": "MDRW2100033330.1.1",
          "form": "빗소리가 크게 들리네",
          "original_form": "빗소리가 크게 들리네",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033330.1.2",
          "form": "오후 내내 비오는구나",
          "original_form": "오후 내내 비오는구나",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033330.1.3",
          "form": "비내리니까 확실히 춥다",
          "original_form": "비내리니까 확실히 춥다",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033330.1.4",
          "form": "반팔 입으니까 좀 춥지?",
          "original_form": "반팔 입으니까 좀 춥지?",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033330.1.5",
          "form": "ㅇㅇ 긴팔 입어야겠어",
          "original_form": "ㅇㅇ 긴팔 입어야겠어",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033330.1.6",
          "form": "비오니까 기온이 훅 떨어지네",
          "original_form": "비오니까 기온이 훅 떨어지네",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033330.1.7",
          "form": "내일 애들도 긴팔 입혀야겠다",
          "original_form": "내일 애들도 긴팔 입혀야겠다",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033330.1.8",
          "form": "ㅇㅇ 바람막이도 같이 보내면 좋을듯",
          "original_form": "ㅇㅇ 바람막이도 같이 보내면 좋을듯",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033330.1.9",
          "form": "비가 많이 계속 내리네",
          "original_form": "비가 많이 계속 내리네",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033330.1.10",
          "form": "올해 진짜 징하게 내리지? ㅎㅎ",
          "original_form": "올해 진짜 징하게 내리지? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100033330.1.11",
          "form": "내일도 하루종일 내릴 생각하니 벌써 피곤하다 ㅋ",
          "original_form": "내일도 하루종일 내릴 생각하니 벌써 피곤하다 ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100033330.1.12",
          "form": "비내리는 날은 기분 때문인지 유독 피곤한 것  같아",
          "original_form": "비내리는 날은 기분 때문인지 유독 피곤한 것  같아",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100033330.1.13",
          "form": "나도 그래.. 괜히 축 쳐지는 기분",
          "original_form": "나도 그래.. 괜히 축 쳐지는 기분",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100033330.1.14",
          "form": "진짜로 내일까지만 오는 거였으면 좋겠다",
          "original_form": "진짜로 내일까지만 오는 거였으면 좋겠다",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100033330.1.15",
          "form": "그러게 말야.. 해뜬 하늘 보고 싶어",
          "original_form": "그러게 말야.. 해뜬 하늘 보고 싶어",
