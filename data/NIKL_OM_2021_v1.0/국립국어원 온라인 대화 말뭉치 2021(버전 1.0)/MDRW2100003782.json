        },
        {
          "id": "MDRW2100003782.1.2",
          "form": "없으니까 사지",
          "original_form": "없으니까 사지",
          "speaker_id": "1",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003782.1.3",
          "form": "옷 많은데",
          "original_form": "옷 많은데",
          "speaker_id": "2",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003782.1.4",
          "form": "없다니까",
          "original_form": "없다니까",
          "speaker_id": "1",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003782.1.5",
          "form": "내 거랑 옷 같이 입자",
          "original_form": "내 거랑 옷 같이 입자",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003782.1.6",
          "form": "엄마옷은 커서 커서 못입는다",
          "original_form": "엄마옷은 커서 커서 못입는다",
          "speaker_id": "1",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003782.1.7",
          "form": "별로 안 큰데",
          "original_form": "별로 안 큰데",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003782.1.8",
          "form": "크다! 암튼 새로 옷 살거야",
          "original_form": "크다! 암튼 새로 옷 살거야",
          "speaker_id": "1",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003782.1.9",
          "form": "튼튼한 걸로 사라",
          "original_form": "튼튼한 걸로 사라",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003782.1.10",
          "form": "당연하지~",
          "original_form": "당연하지~",
          "speaker_id": "1",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003782.1.11",
          "form": "너무 싸구려 사지 말고",
          "original_form": "너무 싸구려 사지 말고",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003782.1.12",
          "form": "알겠어용~",
          "original_form": "알겠어용~",
          "speaker_id": "1",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003782.1.13",
