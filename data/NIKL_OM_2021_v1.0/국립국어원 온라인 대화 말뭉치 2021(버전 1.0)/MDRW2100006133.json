      "utterance": [
        {
          "id": "MDRW2100006133.1.1",
          "form": "날씨 어떤가요",
          "original_form": "날씨 어떤가요",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100006133.1.2",
          "form": "계속 비오네요",
          "original_form": "계속 비오네요",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100006133.1.3",
          "form": "여긴 흐렸다 갰다 변덕이예요",
          "original_form": "여긴 흐렸다 갰다 변덕이예요",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006133.1.4",
          "form": "여긴 좀 가늘게 내리다가 쏟아지고 그래요",
          "original_form": "여긴 좀 가늘게 내리다가 쏟아지고 그래요",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006133.1.5",
          "form": "우산 계속 들고 다니고 있어요ㅋㅋ",
          "original_form": "우산 계속 들고 다니고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006133.1.6",
          "form": "맞아요 가벼운 우산 필수예요...요즘",
          "original_form": "맞아요 가벼운 우산 필수예요...요즘",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006133.1.7",
          "form": "하루종일 우산 들고 다니다가 집에서 말리고.. 반복이에요",
          "original_form": "하루종일 우산 들고 다니다가 집에서 말리고.. 반복이에요",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100006133.1.8",
          "form": "지겹죠 비오는 날씨..",
          "original_form": "지겹죠 비오는 날씨..",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100006133.1.9",
          "form": "내일은 비 안올거라는데ㅋㅋ 아직 좀 내려서 불안하네요",
          "original_form": "내일은 비 안올거라는데ㅋㅋ 아직 좀 내려서 불안하네요",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006133.1.10",
          "form": "맑으면 좋겠네요 바람이 엄청 차요",
          "original_form": "맑으면 좋겠네요 바람이 엄청 차요",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006133.1.11",
          "form": "금방 추워지겠네요ㅠㅠ",
          "original_form": "금방 추워지겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006133.1.12",
          "form": "감기 걸리기 쉬운 날씨 죠",
          "original_form": "감기 걸리기 쉬운 날씨 죠",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006133.1.13",
          "form": "맞아요ㅋㅋ 이젠 긴팔입고 다녀야겠어요",
          "original_form": "맞아요ㅋㅋ 이젠 긴팔입고 다녀야겠어요",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006133.1.14",
          "form": "네 외투를 잘 챙겨야하겠어요",
          "original_form": "네 외투를 잘 챙겨야하겠어요",
