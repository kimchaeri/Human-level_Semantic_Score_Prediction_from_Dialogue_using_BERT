      "utterance": [
        {
          "id": "MDRW2100020905.1.1",
          "form": "집안일 자주해요?",
          "original_form": "집안일 자주해요?",
          "speaker_id": "1",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020905.1.2",
          "form": "자주 하는 편은 아니지만",
          "original_form": "자주 하는 편은 아니지만",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020905.1.3",
          "form": "주말엔 제가 좀 하는 편입니다 ㅎㅎ",
          "original_form": "주말엔 제가 좀 하는 편입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020905.1.4",
          "form": "오늘은 방 청소랑 요리를 해볼까 해요!",
          "original_form": "오늘은 방 청소랑 요리를 해볼까 해요!",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020905.1.5",
          "form": "무슨 요리를 계획했어요?",
          "original_form": "무슨 요리를 계획했어요?",
          "speaker_id": "1",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100020905.1.6",
          "form": "호박전이요!",
          "original_form": "호박전이요!",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020905.1.7",
          "form": "name1님은 집안일 자주 하시나요?",
          "original_form": "&name1&님은 집안일 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020905.1.8",
          "form": "저는 자취해서 매일매일해요ㅠㅠ",
          "original_form": "저는 자취해서 매일매일해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020905.1.9",
          "form": "오늘 날씨가 좋아서 세탁기 돌렸습니다",
          "original_form": "오늘 날씨가 좋아서 세탁기 돌렸습니다",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020905.1.10",
          "form": "맞아요 날씨 좋죠!",
          "original_form": "맞아요 날씨 좋죠!",
          "speaker_id": "2",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020905.1.11",
          "form": "이불빨래 해도 괜찮을 날씨예요 ㅎㅎ",
          "original_form": "이불빨래 해도 괜찮을 날씨예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020905.1.12",
          "form": "자취하시면 집안일 혼자 다 하시겠군요!!",
          "original_form": "자취하시면 집안일 혼자 다 하시겠군요!!",
          "speaker_id": "2",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020905.1.13",
          "form": "네 독박이에요 ㅜㅜ",
          "original_form": "네 독박이에요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100020905.1.14",
          "form": "지난 번에 빨래 널어둔 것도 아직 안갰는데..",
          "original_form": "지난 번에 빨래 널어둔 것도 아직 안갰는데..",
          "speaker_id": "1",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100020905.1.15",
          "form": "독박 ㅋㅋㅋㅋㅋ",
          "original_form": "독박 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100020905.1.16",
          "form": "자기가 자기꺼 하는거니까",
          "original_form": "자기가 자기꺼 하는거니까",
          "speaker_id": "2",
          "time": "20210828 15:01"
        },
        {
          "id": "MDRW2100020905.1.17",
          "form": "어쩔 수 없죠!!",
          "original_form": "어쩔 수 없죠!!",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100020905.1.18",
          "form": "맞아요 이불은",
          "original_form": "맞아요 이불은",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100020905.1.19",
          "form": "큰맘먹고 만져야하는 물건이죠...",
          "original_form": "큰맘먹고 만져야하는 물건이죠...",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100020905.1.20",
          "form": "ㅋㅋㅋ이불은 드럼세타기에 들어가지도 않아요 ㅠ",
          "original_form": "ㅋㅋㅋ이불은 드럼세타기에 들어가지도 않아요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100020905.1.21",
          "form": "그래서 근처 빨래방에서 세탁과 건조까지 한번에 끝내야해요",
          "original_form": "그래서 근처 빨래방에서 세탁과 건조까지 한번에 끝내야해요",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100020905.1.22",
          "form": "빨래방!!! 저 한 번도 안가봤는데",
          "original_form": "빨래방!!! 저 한 번도 안가봤는데",
          "speaker_id": "2",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100020905.1.23",
          "form": "보통 이불빨래할 때 얼마나 드나요???",
          "original_form": "보통 이불빨래할 때 얼마나 드나요???",
          "speaker_id": "2",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100020905.1.24",
          "form": "세탁-건조까지 합쳐서 8천원정도요!",
          "original_form": "세탁-건조까지 합쳐서 8천원정도요!",
          "speaker_id": "1",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100020905.1.25",
          "form": "우와 생각보다 값이 많이 나가네요?!",
          "original_form": "우와 생각보다 값이 많이 나가네요?!",
          "speaker_id": "2",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100020905.1.26",
          "form": "역시 자취하면 다 돈이네요... 그쵸?",
          "original_form": "역시 자취하면 다 돈이네요... 그쵸?",
          "speaker_id": "2",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100020905.1.27",
          "form": "음식도 1인분으로 사야하니까",
          "original_form": "음식도 1인분으로 사야하니까",
          "speaker_id": "2",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100020905.1.28",
          "form": "재료비도 더 많이 든다고들 하더라구요!",
          "original_form": "재료비도 더 많이 든다고들 하더라구요!",
          "speaker_id": "2",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100020905.1.29",
          "form": "그래서 음식을 4~6인분치 시장을 봐와요 ㅋㅋㅋ",
          "original_form": "그래서 음식을 4~6인분치 시장을 봐와요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100020905.1.30",
          "form": "매일 같은 음식 먹기 ㅠㅠㅠ",
          "original_form": "매일 같은 음식 먹기 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 15:54"
        }
      ]
