      "utterance": [
        {
          "id": "MDRW2100016781.1.1",
          "form": "다음에 빌려줘",
          "original_form": "다음에 빌려줘",
          "speaker_id": "2",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100016781.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ그래",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그래",
          "speaker_id": "1",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016781.1.3",
          "form": "읽어보고 싶다",
          "original_form": "읽어보고 싶다",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016781.1.4",
          "form": "이렇게 인물들끼리 이야기 이어지는거는",
          "original_form": "이렇게 인물들끼리 이야기 이어지는거는",
          "speaker_id": "1",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016781.1.5",
          "form": "읽는거도 힘들긴한데",
          "original_form": "읽는거도 힘들긴한데",
          "speaker_id": "1",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016781.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016781.1.7",
          "form": "쓰는 작가가 ㄴ진짜 대단한거같음",
          "original_form": "쓰는 작가가 ㄴ진짜 대단한거같음",
          "speaker_id": "1",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016781.1.8",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016781.1.9",
          "form": "그리고 그런책은",
          "original_form": "그리고 그런책은",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016781.1.10",
          "form": "좀 후루룩?",
          "original_form": "좀 후루룩?",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016781.1.11",
          "form": "읽어야해",
          "original_form": "읽어야해",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016781.1.12",
          "form": "ㅋㅋㅋ맞아 흐름타고 한번에 쭉읽어야함",
          "original_form": "ㅋㅋㅋ맞아 흐름타고 한번에 쭉읽어야함",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016781.1.13",
          "form": "그 내가 말했던",
          "original_form": "그 내가 말했던",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016781.1.14",
          "form": "소크라테스 익스프레스 있잖아",
          "original_form": "소크라테스 익스프레스 있잖아",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016781.1.15",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016781.1.16",
          "form": "이거 생윤 배우기 전에 읽었으면 진짜 도움 됐을것 같아",
          "original_form": "이거 생윤 배우기 전에 읽었으면 진짜 도움 됐을것 같아",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016781.1.17",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016781.1.18",
          "form": "배웠던내용 많이 나와?",
          "original_form": "배웠던내용 많이 나와?",
          "speaker_id": "1",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016781.1.19",
          "form": "학자들이 왜 그런 주장했는지 좀 이해가 간다해야 하나",
          "original_form": "학자들이 왜 그런 주장했는지 좀 이해가 간다해야 하나",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016781.1.20",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016781.1.21",
          "form": "그 철학자들이 이래서 이런 주장 했겠구나",
          "original_form": "그 철학자들이 이래서 이런 주장 했겠구나",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016781.1.22",
          "form": "이게 딱 와닿음",
          "original_form": "이게 딱 와닿음",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016781.1.23",
          "form": "좋은책이네",
          "original_form": "좋은책이네",
          "speaker_id": "1",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016781.1.24",
          "form": "진짜 주입식 교육의 문제점을 또 느꼈다...",
          "original_form": "진짜 주입식 교육의 문제점을 또 느꼈다...",
          "speaker_id": "2",
          "time": "20210826 15:23"
        },
        {
          "id": "MDRW2100016781.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ이런...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이런...",
          "speaker_id": "1",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016781.1.26",
          "form": "로설",
          "original_form": "로설",
          "speaker_id": "1",
          "time": "20210827 17:39"
        }
      ]
    }
