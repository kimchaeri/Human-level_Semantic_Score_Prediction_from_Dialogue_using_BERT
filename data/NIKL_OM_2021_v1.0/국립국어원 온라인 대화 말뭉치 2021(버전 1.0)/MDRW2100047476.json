      "utterance": [
        {
          "id": "MDRW2100047476.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100047476.1.2",
          "form": "안녕하세요 혹시 명동교자 가 보셨어요?",
          "original_form": "안녕하세요 혹시 명동교자 가 보셨어요?",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100047476.1.3",
          "form": "아니요ㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100047476.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100047476.1.5",
          "form": "ㅋㅋ 전 좋아하는데 못간지 몇년 됐네요. 최근에 티비 나왔는데 생각나더라구요",
          "original_form": "ㅋㅋ 전 좋아하는데 못간지 몇년 됐네요. 최근에 티비 나왔는데 생각나더라구요",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100047476.1.6",
          "form": "추천 맛집 있으신가요?",
          "original_form": "추천 맛집 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100047476.1.7",
          "form": "맛집이라기보단",
          "original_form": "맛집이라기보단",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100047476.1.8",
          "form": "저 거기가고싶어요ㅋㅋㅋ",
          "original_form": "저 거기가고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100047476.1.9",
          "form": "바이킹스워프",
          "original_form": "바이킹스워프",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100047476.1.10",
          "form": "뭐 파는 곳이에요?",
          "original_form": "뭐 파는 곳이에요?",
          "speaker_id": "2",
          "time": "20210826 17:58"
        },
        {
          "id": "MDRW2100047476.1.11",
          "form": "랍스타뷔페요ㅋㅋㅋㅋ",
          "original_form": "랍스타뷔페요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100047476.1.12",
          "form": "오 궁금하네요 찾아봐야겠어요",
          "original_form": "오 궁금하네요 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100047476.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100047476.1.14",
          "form": "비싸요ㅠ",
          "original_form": "비싸요ㅠ",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100047476.1.15",
          "form": "랍스타라 그런지 비싸긴 하네요ㅋㅋ",
          "original_form": "랍스타라 그런지 비싸긴 하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100047476.1.16",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100047476.1.17",
          "form": "근데맛잇대요",
          "original_form": "근데맛잇대요",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100047476.1.18",
          "form": "가보고싶네요",
          "original_form": "가보고싶네요",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100047476.1.19",
          "form": "찾아보니 시간제한이 있나봐요",
          "original_form": "찾아보니 시간제한이 있나봐요",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100047476.1.20",
          "form": "네네 뷔페라서요ㅋㅋㅋ",
          "original_form": "네네 뷔페라서요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100047476.1.21",
          "form": "고기도맛있대요",
          "original_form": "고기도맛있대요",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100047476.1.22",
          "form": "뷔페 간지도 오래됐네요ㅠ",
          "original_form": "뷔페 간지도 오래됐네요ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:56"
        }
      ]
