      "utterance": [
        {
          "id": "MDRW2100037995.1.1",
          "form": "음 누룽지 끓여서 죽이된걸로 200그램이면 보식괜찮을까요",
          "original_form": "음 누룽지 끓여서 죽이된걸로 200그램이면 보식괜찮을까요",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037995.1.2",
          "form": "어; 솔직히 보식으론 많이 부족해 보이지만ㅜ",
          "original_form": "어; 솔직히 보식으론 많이 부족해 보이지만ㅜ",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037995.1.3",
          "form": "그 정도만 드시는게 나을것 같아요",
          "original_form": "그 정도만 드시는게 나을것 같아요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037995.1.4",
          "form": "안 그럼 이틀 굶은게 억울해질까봐ㅜ",
          "original_form": "안 그럼 이틀 굶은게 억울해질까봐ㅜ",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037995.1.5",
          "form": "누룽지는 뭐랑 드실 건가요",
          "original_form": "누룽지는 뭐랑 드실 건가요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037995.1.6",
          "form": "내일 주방저울 재서 먹을거에요ㅜ",
          "original_form": "내일 주방저울 재서 먹을거에요ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037995.1.7",
          "form": "저는 제 눈대중믿을수없다며ㅡㅜ",
          "original_form": "저는 제 눈대중믿을수없다며ㅡㅜ",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037995.1.8",
          "form": "간자이랑 먹어야겧죠?",
          "original_form": "간자이랑 먹어야겧죠?",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100037995.1.9",
          "form": "좋은 자세입니다!!",
          "original_form": "좋은 자세입니다!!",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100037995.1.10",
          "form": "간장이요...",
          "original_form": "간장이요...",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100037995.1.11",
          "form": "깨간장?",
          "original_form": "깨간장?",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100037995.1.12",
          "form": "딴반찬 아예 없이요?",
          "original_form": "딴반찬 아예 없이요?",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100037995.1.13",
          "form": "간장이랑 김치정도는 괜찮지않을까",
          "original_form": "간장이랑 김치정도는 괜찮지않을까",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100037995.1.14",
          "form": "타협중이에요 ㅋㅋ",
          "original_form": "타협중이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100037995.1.15",
          "form": "간 아주 약하게 한 반찬 정도는 같이 드세요ㅜ",
          "original_form": "간 아주 약하게 한 반찬 정도는 같이 드세요ㅜ",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100037995.1.16",
          "form": "너무 영양이 없게 드셔서;",
          "original_form": "너무 영양이 없게 드셔서;",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100037995.1.17",
          "form": "아님 달걀삶은거를같이 먹어도되려나",
          "original_form": "아님 달걀삶은거를같이 먹어도되려나",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100037995.1.18",
          "form": "달걀한개랑 현미누룽지 200g",
          "original_form": "달걀한개랑 현미누룽지 200g",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100037995.1.19",
          "form": "프라이 아니면 괜찮을 것 같아요1",
          "original_form": "프라이 아니면 괜찮을 것 같아요1",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100037995.1.20",
          "form": "아님 기름 없이 수란으로 해도 좋을 것 같고요",
          "original_form": "아님 기름 없이 수란으로 해도 좋을 것 같고요",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100037995.1.21",
          "form": "달걀 2개는 안되겠죠?",
          "original_form": "달걀 2개는 안되겠죠?",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100037995.1.22",
          "form": "ㅎㅎ 저는.. 될것 같은데ㅎ",
          "original_form": "ㅎㅎ 저는.. 될것 같은데ㅎ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100037995.1.23",
          "form": "좀 그러면 밥을 쪼끔 줄이면요?",
          "original_form": "좀 그러면 밥을 쪼끔 줄이면요?",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100037995.1.24",
          "form": "ㅋㅋㅋ저 몸무게 살짝 올라가봤습니다",
          "original_form": "ㅋㅋㅋ저 몸무게 살짝 올라가봤습니다",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100037995.1.25",
          "form": "방금막요 ㅋㅋ",
          "original_form": "방금막요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100037995.1.26",
          "form": "어때요?",
          "original_form": "어때요?",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100037995.1.27",
          "form": "ㅋㅋㅋ저좀 울고싶네요",
          "original_form": "ㅋㅋㅋ저좀 울고싶네요",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100037995.1.28",
          "form": "0.6줄음ㅜ",
          "original_form": "0.6줄음ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100037995.1.29",
          "form": "물도 아껴마셨는데!",
          "original_form": "물도 아껴마셨는데!",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100037995.1.30",
