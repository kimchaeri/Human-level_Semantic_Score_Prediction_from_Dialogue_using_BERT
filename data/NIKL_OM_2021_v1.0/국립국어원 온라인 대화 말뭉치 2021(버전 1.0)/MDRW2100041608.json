      "utterance": [
        {
          "id": "MDRW2100041608.1.1",
          "form": "핸드워시요",
          "original_form": "핸드워시요",
          "speaker_id": "2",
          "time": "20210906 13:02"
        },
        {
          "id": "MDRW2100041608.1.2",
          "form": "핸드워시는 어떤거 구매하셨나요??",
          "original_form": "핸드워시는 어떤거 구매하셨나요??",
          "speaker_id": "1",
          "time": "20210906 13:23"
        },
        {
          "id": "MDRW2100041608.1.3",
          "form": "저는 닥터브루너??엿나요 거기서 나온거 쓰고싶더라구요",
          "original_form": "저는 닥터브루너??엿나요 거기서 나온거 쓰고싶더라구요",
          "speaker_id": "1",
          "time": "20210906 13:23"
        },
        {
          "id": "MDRW2100041608.1.4",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 13:23"
        },
        {
          "id": "MDRW2100041608.1.5",
          "form": "크리넥스꺼요\n그건뭔가요",
          "original_form": "크리넥스꺼요\n그건뭔가요",
          "speaker_id": "2",
          "time": "20210906 13:24"
        },
        {
          "id": "MDRW2100041608.1.6",
          "form": "시원한제품이라서 구매하고 싶은더. 이름이 생각이 안나네요 ㅎㅎ",
          "original_form": "시원한제품이라서 구매하고 싶은더. 이름이 생각이 안나네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041608.1.7",
          "form": "종류가 워낙 많아서 고르기도 힘들어요ㅋ",
          "original_form": "종류가 워낙 많아서 고르기도 힘들어요ㅋ",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041608.1.8",
          "form": "맞아요 정말 많아요 ㅋㅋㅋ",
          "original_form": "맞아요 정말 많아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041608.1.9",
          "form": "그만큼 필요하기도 하지만 저렴하고 좋은제품으로 사봐야 겠네요",
          "original_form": "그만큼 필요하기도 하지만 저렴하고 좋은제품으로 사봐야 겠네요",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041608.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041608.1.11",
          "form": "그래서 모하나 고르려면 며칠걸리져",
          "original_form": "그래서 모하나 고르려면 며칠걸리져",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041608.1.12",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041608.1.13",
          "form": "쇼핑하는 재미와 귀찮음이 동시에 오네요",
          "original_form": "쇼핑하는 재미와 귀찮음이 동시에 오네요",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041608.1.14",
          "form": "저랑 똑같으시네요\n그래도 싸게잘사면 뿌듯함이 잇어요",
          "original_form": "저랑 똑같으시네요\n그래도 싸게잘사면 뿌듯함이 잇어요",
          "speaker_id": "2",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100041608.1.15",
          "form": "맞아요 ㅎㅎ 그럼 진짜 뿌듯하죠",
          "original_form": "맞아요 ㅎㅎ 그럼 진짜 뿌듯하죠",
          "speaker_id": "1",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100041608.1.16",
          "form": "그맛에 쇼핑을 끊을수없져ㅋ",
          "original_form": "그맛에 쇼핑을 끊을수없져ㅋ",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100041608.1.17",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100041608.1.18",
          "form": "최저가 알아볼 수 있는거 정말 편리한 것 같아요",
