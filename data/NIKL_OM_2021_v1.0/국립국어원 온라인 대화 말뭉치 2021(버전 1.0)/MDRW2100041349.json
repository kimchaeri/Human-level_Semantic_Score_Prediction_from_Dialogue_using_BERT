        {
          "id": "MDRW2100041349.1.1",
          "form": "공연좋아하세요?",
          "original_form": "공연좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100041349.1.2",
          "form": "네name1님도 좋아하시나요?",
          "original_form": "네&name1&님도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100041349.1.3",
          "form": "앗ㅎㅎ영화는요?",
          "original_form": "앗ㅎㅎ영화는요?",
          "speaker_id": "1",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100041349.1.4",
          "form": "제가 영화를 거의 안봐요ㅠㅠ",
          "original_form": "제가 영화를 거의 안봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100041349.1.5",
          "form": "영화 좋아하시나요?",
          "original_form": "영화 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100041349.1.6",
          "form": "앗ㅎㅎ저도잘안보지만",
          "original_form": "앗ㅎㅎ저도잘안보지만",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100041349.1.7",
          "form": "마블은좋아해요",
          "original_form": "마블은좋아해요",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100041349.1.8",
          "form": "그럼공연은어떤거좋아하세요?",
          "original_form": "그럼공연은어떤거좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100041349.1.9",
          "form": "저는 콘서트는 좀 가요",
          "original_form": "저는 콘서트는 좀 가요",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100041349.1.10",
          "form": "콘서트 가보셨나요?",
          "original_form": "콘서트 가보셨나요?",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100041349.1.11",
          "form": "코로나전에 이적 가봤어요",
          "original_form": "코로나전에 이적 가봤어요",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100041349.1.12",
          "form": "직접 티켓팅 하신건가요?",
          "original_form": "직접 티켓팅 하신건가요?",
          "speaker_id": "2",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100041349.1.13",
          "form": "음 같이갔던친구가해줬어요",
          "original_form": "음 같이갔던친구가해줬어요",
          "speaker_id": "1",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100041349.1.14",
          "form": "기억에남는공연있으신가요?",
          "original_form": "기억에남는공연있으신가요?",
          "speaker_id": "1",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100041349.1.15",
          "form": "저는 아이유콘서트요",
          "original_form": "저는 아이유콘서트요",
          "speaker_id": "2",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100041349.1.16",
          "form": "와 티켓팅능력자시네요",
          "original_form": "와 티켓팅능력자시네요",
          "speaker_id": "1",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100041349.1.17",
          "form": "입성이 목표여서 면봉크기였어요ㅋㅋㅋㅋ",
          "original_form": "입성이 목표여서 면봉크기였어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100041349.1.18",
          "form": "ㅎㅎㅎ그래도귀가호강했겠어요",
          "original_form": "ㅎㅎㅎ그래도귀가호강했겠어요",
