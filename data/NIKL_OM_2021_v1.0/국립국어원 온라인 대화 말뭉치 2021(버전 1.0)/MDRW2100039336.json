      "utterance": [
        {
          "id": "MDRW2100039336.1.1",
          "form": "드릴펑이..먼지 궁금한 나",
          "original_form": "드릴펑이..먼지 궁금한 나",
          "speaker_id": "1",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100039336.1.2",
          "form": "그 락스냄새나는 앤데",
          "original_form": "그 락스냄새나는 앤데",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100039336.1.3",
          "form": "아하 크린펑 이런거",
          "original_form": "아하 크린펑 이런거",
          "speaker_id": "1",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100039336.1.4",
          "form": "아 마자 크린펑인가보다",
          "original_form": "아 마자 크린펑인가보다",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100039336.1.5",
          "form": "세면대에서 머리감음?",
          "original_form": "세면대에서 머리감음?",
          "speaker_id": "1",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100039336.1.6",
          "form": "드릴펑....이미지 비슷하지않니",
          "original_form": "드릴펑....이미지 비슷하지않니",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100039336.1.7",
          "form": "드릴펑개웃겨 기곈줄알앗어",
          "original_form": "드릴펑개웃겨 기곈줄알앗어",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100039336.1.8",
          "form": "하수관청소라는기계이런거인줄",
          "original_form": "하수관청소라는기계이런거인줄",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100039336.1.9",
          "form": "ㅋㅋㅋㅋ아주 속 시원하게 내려줘요",
          "original_form": "ㅋㅋㅋㅋ아주 속 시원하게 내려줘요",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100039336.1.10",
          "form": "머리 세면대에 감는건 아닌데 화장실이 좁다보니",
          "original_form": "머리 세면대에 감는건 아닌데 화장실이 좁다보니",
          "speaker_id": "2",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100039336.1.11",
          "form": "머리 말리다보면 머리카락 빠진게 자꾸 세면대 하수구에 들어가더라구",
          "original_form": "머리 말리다보면 머리카락 빠진게 자꾸 세면대 하수구에 들어가더라구",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039336.1.12",
          "form": "그게 쌓이고 쌓이다보니...",
          "original_form": "그게 쌓이고 쌓이다보니...",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039336.1.13",
          "form": "슬픈 일이야",
          "original_form": "슬픈 일이야",
          "speaker_id": "2",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100039336.1.14",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100039336.1.15",
          "form": "그거갖구 그렇게 자주막힐정돈가",
          "original_form": "그거갖구 그렇게 자주막힐정돈가",
          "speaker_id": "1",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100039336.1.16",
          "form": "나중에 여유생김 기사 함 불러봐",
          "original_form": "나중에 여유생김 기사 함 불러봐",
          "speaker_id": "1",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100039336.1.17",
          "form": "나 자취방 화장실 변기가 맨날 처막혀가지구",
          "original_form": "나 자취방 화장실 변기가 맨날 처막혀가지구",
          "speaker_id": "1",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100039336.1.18",
          "form": "하수관에 동전들어잇고 카드들어잇고 이랫음",
          "original_form": "하수관에 동전들어잇고 카드들어잇고 이랫음",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100039336.1.19",
          "form": "그런게 걸려잇어서 이물질 쫌만 들어가도 막혓던거랫어",
          "original_form": "그런게 걸려잇어서 이물질 쫌만 들어가도 막혓던거랫어",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100039336.1.20",
          "form": "아 세상에 변기에 그런걸 대체 왜 넣는거야",
          "original_form": "아 세상에 변기에 그런걸 대체 왜 넣는거야",
          "speaker_id": "2",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100039336.1.21",
          "form": "아직 변기는 막혀본적이 없어서 모르겠는데 진짜 변기는 제발 안막혔으면 좋겠어",
          "original_form": "아직 변기는 막혀본적이 없어서 모르겠는데 진짜 변기는 제발 안막혔으면 좋겠어",
          "speaker_id": "2",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100039336.1.22",
          "form": "대학가니까...",
          "original_form": "대학가니까...",
          "speaker_id": "1",
          "time": "20210903 17:09"
        },
        {
          "id": "MDRW2100039336.1.23",
          "form": "딱 우리전에 신입생남자3명이 같이살앗다더라구...",
          "original_form": "딱 우리전에 신입생남자3명이 같이살앗다더라구...",
          "speaker_id": "1",
          "time": "20210903 17:09"
        },
        {
          "id": "MDRW2100039336.1.24",
          "form": "그땐 뭣도모를때지..",
          "original_form": "그땐 뭣도모를때지..",
          "speaker_id": "1",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100039336.1.25",
          "form": "청소도할줄모르고..아무것도모를때긴하지..",
          "original_form": "청소도할줄모르고..아무것도모를때긴하지..",
          "speaker_id": "1",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100039336.1.26",
          "form": "제발 다들 기본 교양과목으로 살림을 배워야한다고 생각해",
          "original_form": "제발 다들 기본 교양과목으로 살림을 배워야한다고 생각해",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039336.1.27",
          "form": "이건 생존과도 관련된 일이라구",
          "original_form": "이건 생존과도 관련된 일이라구",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039336.1.28",
          "form": "나도 깨끗해게 사는건 아니지만 진짜 살림살이 하나도 못하는 사람들 보면 너무하잖아요",
          "original_form": "나도 깨끗해게 사는건 아니지만 진짜 살림살이 하나도 못하는 사람들 보면 너무하잖아요",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039336.1.29",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100039336.1.30",
          "form": "오늘도 패륜아가 되면 안된다를 마음속으로 삼십번 외치고 나왓어",
          "original_form": "오늘도 패륜아가 되면 안된다를 마음속으로 삼십번 외치고 나왓어",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100039336.1.31",
          "form": "나이육십을 넘겨서도 밥하나 제대로 못차려먹는 사람이 집에잇지..",
          "original_form": "나이육십을 넘겨서도 밥하나 제대로 못차려먹는 사람이 집에잇지..",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100039336.1.32",
          "form": "아 우리집에도 있다",
          "original_form": "아 우리집에도 있다",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039336.1.33",
          "form": "그냥 있는 반찬 꺼내만이라도 드셔요 제발",
          "original_form": "그냥 있는 반찬 꺼내만이라도 드셔요 제발",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039336.1.34",
          "form": "아이구 나도 얼른 나가살아야지",
          "original_form": "아이구 나도 얼른 나가살아야지",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039336.1.35",
          "form": "화장실청소할때가 젤빡쳐증말...",
          "original_form": "화장실청소할때가 젤빡쳐증말...",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039336.1.36",
          "form": "아니 이게 대체 왜 나쁜말이야",
          "original_form": "아니 이게 대체 왜 나쁜말이야",
          "speaker_id": "2",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039336.1.37",
          "form": "기준을 모르겠네",
          "original_form": "기준을 모르겠네",
          "speaker_id": "2",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039336.1.38",
          "form": "휴 화장실 청소",
          "original_form": "휴 화장실 청소",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039336.1.39",
          "form": "하는 사람만 하는 화장실 청소",
          "original_form": "하는 사람만 하는 화장실 청소",
          "speaker_id": "2",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100039336.1.40",
          "form": "ㅋㅋㅋ아 그치 나쁜말기준이해x",
          "original_form": "ㅋㅋㅋ아 그치 나쁜말기준이해x",
          "speaker_id": "1",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039336.1.41",
          "form": "화장실청소할때마다 생각함",
          "original_form": "화장실청소할때마다 생각함",
          "speaker_id": "1",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039336.1.42",
          "form": "남자랑...안살아",
          "original_form": "남자랑...안살아",
          "speaker_id": "1",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039336.1.43",
          "form": "남자랑살기싫어..",
          "original_form": "남자랑살기싫어..",
          "speaker_id": "1",
          "time": "20210903 18:04"
        },
        {
          "id": "MDRW2100039336.1.44",
          "form": "나 화장실청소 나름 좋아하거든.. 깨끗해지면 기분좋자너",
          "original_form": "나 화장실청소 나름 좋아하거든.. 깨끗해지면 기분좋자너",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039336.1.45",
          "form": "근데 남이 지지해놓은걸 닦는건 기분좋지않드라구",
          "original_form": "근데 남이 지지해놓은걸 닦는건 기분좋지않드라구",
          "speaker_id": "1",
          "time": "20210903 18:05"
        },
        {
          "id": "MDRW2100039336.1.46",
          "form": "마자마자 내가 지지한건 내가 지지했으니 치웅수있어",
          "original_form": "마자마자 내가 지지한건 내가 지지했으니 치웅수있어",
          "speaker_id": "2",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100039336.1.47",
          "form": "역시 잉간이 혼자 살아야해",
          "original_form": "역시 잉간이 혼자 살아야해",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039336.1.48",
          "form": "설거지해야하는데 고기기름땜에 그릇이 너무 미끌미끌해",
          "original_form": "설거지해야하는데 고기기름땜에 그릇이 너무 미끌미끌해",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039336.1.49",
          "form": "뜨거운물로해",
          "original_form": "뜨거운물로해",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039336.1.50",
          "form": "보일러 켜야하누",
          "original_form": "보일러 켜야하누",
          "speaker_id": "2",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039336.1.51",
          "form": "비린내나는 해산물쓴거아니면",
          "original_form": "비린내나는 해산물쓴거아니면",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039336.1.52",
          "form": "설거지할때 미지근~따끈물 쓰는게좋음",
          "original_form": "설거지할때 미지근~따끈물 쓰는게좋음",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100039336.1.53",
          "form": "그게 잔류세제도 덜남는대",
          "original_form": "그게 잔류세제도 덜남는대",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039336.1.54",
          "form": "아 해산물은 찬물이야???",
          "original_form": "아 해산물은 찬물이야???",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039336.1.55",
          "form": "웅 해산물은 뜨신물로 그릇씻으면",
          "original_form": "웅 해산물은 뜨신물로 그릇씻으면",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039336.1.56",
          "form": "그릇에 비린내 뱀",
          "original_form": "그릇에 비린내 뱀",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
