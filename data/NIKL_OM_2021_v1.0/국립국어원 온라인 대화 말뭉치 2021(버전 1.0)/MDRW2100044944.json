      "utterance": [
        {
          "id": "MDRW2100044944.1.1",
          "form": "라면이요",
          "original_form": "라면이요",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044944.1.2",
          "form": "열라면이요",
          "original_form": "열라면이요",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044944.1.3",
          "form": "매운거 잘먹어요?",
          "original_form": "매운거 잘먹어요?",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044944.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044944.1.5",
          "form": "좋겠다 전 잘 못먹음",
          "original_form": "좋겠다 전 잘 못먹음",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044944.1.6",
          "form": "먹다보면 익숙해져요",
          "original_form": "먹다보면 익숙해져요",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044944.1.7",
          "form": "그런가요? ㅜ",
          "original_form": "그런가요? ㅜ",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044944.1.8",
          "form": "덜매운거 드셔도 되고요!",
          "original_form": "덜매운거 드셔도 되고요!",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044944.1.9",
          "form": "전 하면은 참깨좋아해요",
          "original_form": "전 하면은 참깨좋아해요",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044944.1.10",
          "form": "덜매운? 고소한맛",
          "original_form": "덜매운? 고소한맛",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044944.1.11",
          "form": "참깨도 맛있죠",
          "original_form": "참깨도 맛있죠",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044944.1.12",
          "form": "계란블록 좋아요 ㅋㅋ",
          "original_form": "계란블록 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044944.1.13",
          "form": "전 열라면 더 좋아하지만 ㅋㅋ",
          "original_form": "전 열라면 더 좋아하지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044944.1.14",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044944.1.15",
          "form": "저도 열라면 가끔 먹긴해요",
          "original_form": "저도 열라면 가끔 먹긴해요",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044944.1.16",
          "form": "컵라면이 좀 덜매울껄요",
          "original_form": "컵라면이 좀 덜매울껄요",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044944.1.17",
          "form": "저 오늘 케익먹음",
          "original_form": "저 오늘 케익먹음",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
