      "utterance": [
        {
          "id": "MDRW2100031781.1.1",
          "form": "대학교 다닐 때 개 달았어요",
          "original_form": "대학교 다닐 때 개 달았어요",
          "speaker_id": "1",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100031781.1.2",
          "form": "깨달았어요",
          "original_form": "깨달았어요",
          "speaker_id": "1",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100031781.1.3",
          "form": "암기가 쉬운 과목이 없다는 것을요",
          "original_form": "암기가 쉬운 과목이 없다는 것을요",
          "speaker_id": "1",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100031781.1.4",
          "form": "제일 좋아하는 수업이 뭐예요",
          "original_form": "제일 좋아하는 수업이 뭐예요",
          "speaker_id": "2",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100031781.1.5",
          "form": "글쎄요 화 하기 굉장히 많이 있었거든요 화 화 까여서",
          "original_form": "글쎄요 화 하기 굉장히 많이 있었거든요 화 화 까여서",
          "speaker_id": "1",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031781.1.6",
          "form": "암기 진짜 어렵죠 머리아파여",
          "original_form": "암기 진짜 어렵죠 머리아파여",
          "speaker_id": "2",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031781.1.7",
          "form": "물리화학 생물화학 유기화학 무기화학",
          "original_form": "물리화학 생물화학 유기화학 무기화학",
          "speaker_id": "1",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031781.1.8",
          "form": "그래서 저는 다른 과목이 재미있었어요 온라인으로 듣는 화장품 만들기 같은 수업 이요",
          "original_form": "그래서 저는 다른 과목이 재미있었어요 온라인으로 듣는 화장품 만들기 같은 수업 이요",
          "speaker_id": "1",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031781.1.9",
          "form": "다 알아들었어요",
          "original_form": "다 알아들었어요",
          "speaker_id": "2",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031781.1.10",
          "form": "그때 온라인 강의를 보고 직접 만들어 보면서 꽤나 재미있게 활용했던 기억이 나네요",
          "original_form": "그때 온라인 강의를 보고 직접 만들어 보면서 꽤나 재미있게 활용했던 기억이 나네요",
          "speaker_id": "1",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031781.1.11",
          "form": "화학생각만 해도 절레절레",
          "original_form": "화학생각만 해도 절레절레",
          "speaker_id": "2",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031781.1.12",
          "form": "화장품 만들기 수업이 제일 실전에 강해야 코 재미있었고 활용도가 높았어요",
          "original_form": "화장품 만들기 수업이 제일 실전에 강해야 코 재미있었고 활용도가 높았어요",
          "speaker_id": "1",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031781.1.13",
          "form": "대학 수업은 낭비가 많았죠",
          "original_form": "대학 수업은 낭비가 많았죠",
          "speaker_id": "2",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031781.1.14",
          "form": "요즘에는 심지어 온라인 수업하면서 온라인 수업이 절반 이상이면서",
          "original_form": "요즘에는 심지어 온라인 수업하면서 온라인 수업이 절반 이상이면서",
          "speaker_id": "1",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031781.1.15",
          "form": "대학 과정에서 화장품이 있었군요 실용적이네요",
          "original_form": "대학 과정에서 화장품이 있었군요 실용적이네요",
          "speaker_id": "2",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031781.1.16",
          "form": "등록금은 그대로 받더라고요 돈이 너무 아까운 것 같아요",
          "original_form": "등록금은 그대로 받더라고요 돈이 너무 아까운 것 같아요",
          "speaker_id": "1",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031781.1.17",
          "form": "저는 애니메이션 보기 애니메이션 그리기",
          "original_form": "저는 애니메이션 보기 애니메이션 그리기",
          "speaker_id": "2",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031781.1.18",
          "form": "정말 재미있었겠네요",
          "original_form": "정말 재미있었겠네요",
          "speaker_id": "1",
          "time": "20210905 00:25"
        }
      ]
