      "utterance": [
        {
          "id": "MDRW2100015092.1.1",
          "form": "아무래도 윙어로 뛰겠죠",
          "original_form": "아무래도 윙어로 뛰겠죠",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015092.1.2",
          "form": "사실 젤잘어울리는 옷이라고생각하긴해요",
          "original_form": "사실 젤잘어울리는 옷이라고생각하긴해요",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015092.1.3",
          "form": "다만...",
          "original_form": "다만...",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015092.1.4",
          "form": "손흥민의 선수생활을 토트넘에서 마감할텐데",
          "original_form": "손흥민의 선수생활을 토트넘에서 마감할텐데",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015092.1.5",
          "form": "케인위주의 팀이라는건아쉽네요ㅠ",
          "original_form": "케인위주의 팀이라는건아쉽네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015092.1.6",
          "form": "잉글랜드 출신 첵",
          "original_form": "잉글랜드 출신 첵",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015092.1.7",
          "form": "쳇",
          "original_form": "쳇",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015092.1.8",
          "form": "또다시",
          "original_form": "또다시",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015092.1.9",
          "form": "케인팀이 되겠어요ㅋㅋㅋ 어휴",
          "original_form": "케인팀이 되겠어요ㅋㅋㅋ 어휴",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015092.1.10",
          "form": "전진짜 아쉽네요",
          "original_form": "전진짜 아쉽네요",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015092.1.11",
          "form": "재작년이었던가요 챔스 준결에 빠졌다가 결승나와서 말아먹은...",
          "original_form": "재작년이었던가요 챔스 준결에 빠졌다가 결승나와서 말아먹은...",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015092.1.12",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015092.1.13",
          "form": "손흥민이",
          "original_form": "손흥민이",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015092.1.14",
          "form": "나이만 어렸어도",
          "original_form": "나이만 어렸어도",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015092.1.15",
          "form": "이적에 용기를 냈을텐데",
          "original_form": "이적에 용기를 냈을텐데",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015092.1.16",
          "form": "나이가 애매해서",
          "original_form": "나이가 애매해서",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015092.1.17",
          "form": "토트넘에서 장기계약하는게 제일 베스트였는지도ㅠㅠ",
          "original_form": "토트넘에서 장기계약하는게 제일 베스트였는지도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015092.1.18",
          "form": "5년재계약했잖아요ㅠ",
          "original_form": "5년재계약했잖아요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015092.1.19",
          "form": "사실 빅클럽에서 뛰는것도보고싶은데",
          "original_form": "사실 빅클럽에서 뛰는것도보고싶은데",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015092.1.20",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015092.1.21",
          "form": "기왕 이렇게 된김에 컵하나 들고 레전드로 남자ㅠㅠ",
          "original_form": "기왕 이렇게 된김에 컵하나 들고 레전드로 남자ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015092.1.22",
          "form": "과연...",
          "original_form": "과연...",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015092.1.23",
          "form": "컵을..들..수..........",
          "original_form": "컵을..들..수..........",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015092.1.24",
          "form": "있....",
          "original_form": "있....",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015092.1.25",
          "form": "ㅎ 없다고봅니다",
          "original_form": "ㅎ 없다고봅니다",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015092.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015092.1.27",
          "form": "찐사랑이시네요 내팀내잘알",
          "original_form": "찐사랑이시네요 내팀내잘알",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015092.1.28",
          "form": "그래서 아쉬운겁니다ㅠ",
          "original_form": "그래서 아쉬운겁니다ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015092.1.29",
          "form": "이왕갈거면 좀더젊어서 딴데가거나",
          "original_form": "이왕갈거면 좀더젊어서 딴데가거나",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015092.1.30",
          "form": "근데 안젊으니ㅎ..좀",
          "original_form": "근데 안젊으니ㅎ..좀",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015092.1.31",
          "form": "케인위주의 팀이란건아쉽죠",
          "original_form": "케인위주의 팀이란건아쉽죠",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015092.1.32",
          "form": "어느새 흥미니도 낡은이 대열로 가나요...",
          "original_form": "어느새 흥미니도 낡은이 대열로 가나요...",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015092.1.33",
          "form": "축구선수중엔 어린나이는아니죠...ㅠ",
          "original_form": "축구선수중엔 어린나이는아니죠...ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015092.1.34",
          "form": "아마 오퍼도",
          "original_form": "아마 오퍼도",
          "speaker_id": "2",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015092.1.35",
          "form": "그닥안들어왔을것같네요",
          "original_form": "그닥안들어왔을것같네요",
          "speaker_id": "2",
          "time": "20210825 22:54"
        }
      ]
