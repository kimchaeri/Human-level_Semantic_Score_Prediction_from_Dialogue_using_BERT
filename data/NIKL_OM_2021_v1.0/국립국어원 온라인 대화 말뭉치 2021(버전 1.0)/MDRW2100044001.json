      "utterance": [
        {
          "id": "MDRW2100044001.1.1",
          "form": "말씀하시는데 끊었네요ㅜ",
          "original_form": "말씀하시는데 끊었네요ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100044001.1.2",
          "form": "숙대근처 케이크집",
          "original_form": "숙대근처 케이크집",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100044001.1.3",
          "form": "아뇨아뇨 괜찮아요!",
          "original_form": "아뇨아뇨 괜찮아요!",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044001.1.4",
          "form": "비나 케이크 라는 곳인데",
          "original_form": "비나 케이크 라는 곳인데",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044001.1.5",
          "form": "홀케이크 위주로 팔긴 하지만",
          "original_form": "홀케이크 위주로 팔긴 하지만",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044001.1.6",
          "form": "재료도 좋고 정말 깔끔하게 먹은 좋은 케이크였어요!",
          "original_form": "재료도 좋고 정말 깔끔하게 먹은 좋은 케이크였어요!",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044001.1.7",
          "form": "오 한번 찾아봐야겠어요",
          "original_form": "오 한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044001.1.8",
          "form": "아레아레아는 조각이나 작은 케이크를 주로 팔아요",
          "original_form": "아레아레아는 조각이나 작은 케이크를 주로 팔아요",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044001.1.9",
          "form": "특별한 날 홀케이크 미리 주문해서 먹는데",
          "original_form": "특별한 날 홀케이크 미리 주문해서 먹는데",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044001.1.10",
          "form": "행복해요",
          "original_form": "행복해요",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044001.1.11",
          "form": "카페 가는 것도 좋아해서 작은 조각 케이크쪽도 좋아요",
          "original_form": "카페 가는 것도 좋아해서 작은 조각 케이크쪽도 좋아요{emoji:♡}",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044001.1.12",
          "form": "오.. 홀케이크도 파는군요!",
          "original_form": "오.. 홀케이크도 파는군요!",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044001.1.13",
          "form": "네 미리 예약하시면요",
          "original_form": "네 미리 예약하시면요",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044001.1.14",
          "form": "어떤 케이크 제일 좋아하세요?",
          "original_form": "어떤 케이크 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044001.1.15",
          "form": "저는 다 좋아하느뉘ㅣ",
          "original_form": "저는 다 좋아하느뉘ㅣ",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044001.1.16",
          "form": "는데",
          "original_form": "는데",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044001.1.17",
          "form": "저는 홍대 근처가면 르쁘띠푸 라는 가게 자주 갔었어요!",
          "original_form": "저는 홍대 근처가면 르쁘띠푸 라는 가게 자주 갔었어요!",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044001.1.18",
          "form": "우유맛 진한 생크림케이크나 프레지에 좋아해요",
          "original_form": "우유맛 진한 생크림케이크나 프레지에 좋아해요",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044001.1.19",
          "form": "그러면 아마 비나케이크 마음에 드실 것 같아요!",
          "original_form": "그러면 아마 비나케이크 마음에 드실 것 같아요!",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044001.1.20",
          "form": "아아 그런가요",
          "original_form": "아아 그런가요",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044001.1.21",
          "form": "한번 찾아봐야지ㅋㅋ",
          "original_form": "한번 찾아봐야지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044001.1.22",
          "form": "name1님 혹시 터키 음식인데 카이막이라고 들어보셨나요?",
          "original_form": "&name1&님 혹시 터키 음식인데 카이막이라고 들어보셨나요?",
          "speaker_id": "2",
