      "utterance": [
        {
          "id": "MDRW2100019710.1.1",
          "form": "최근 읽으신 책 있나요",
          "original_form": "최근 읽으신 책 있나요",
          "speaker_id": "1",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100019710.1.2",
          "form": "한강 채식주의자를 최근에 다시 읽엇어요",
          "original_form": "한강 채식주의자를 최근에 다시 읽엇어요",
          "speaker_id": "2",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019710.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019710.1.4",
          "form": "헐 저도 얼마전에",
          "original_form": "헐 저도 얼마전에",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019710.1.5",
          "form": "읽었어요",
          "original_form": "읽었어요",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019710.1.6",
          "form": "오 통햇네요!",
          "original_form": "오 통햇네요!",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019710.1.7",
          "form": "평이나뉘어서 안읽고팠는데",
          "original_form": "평이나뉘어서 안읽고팠는데",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019710.1.8",
          "form": "읽어보길 잘했어요",
          "original_form": "읽어보길 잘했어요",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019710.1.9",
          "form": "맞아요 생각이 많아지더라구요",
          "original_form": "맞아요 생각이 많아지더라구요",
          "speaker_id": "2",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019710.1.10",
          "form": "폭력적인 이야기 기피하다가",
          "original_form": "폭력적인 이야기 기피하다가",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019710.1.11",
          "form": "너무 유명하고 상받아서 읽어보고팠는데 외면하다가",
          "original_form": "너무 유명하고 상받아서 읽어보고팠는데 외면하다가",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019710.1.12",
          "form": "과제로 쓰기 좋을 것 같아서 읽어봤어요ㅠㅠ",
          "original_form": "과제로 쓰기 좋을 것 같아서 읽어봤어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019710.1.13",
          "form": "과제가 뭔지 여쭤봐도 될까요?",
          "original_form": "과제가 뭔지 여쭤봐도 될까요?",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019710.1.14",
          "form": "현대소설 비평과제였어요",
          "original_form": "현대소설 비평과제였어요",
          "speaker_id": "1",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019710.1.15",
          "form": "아 전공이 비평쪽이세요? 멋잇네요",
          "original_form": "아 전공이 비평쪽이세요? 멋잇네요",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019710.1.16",
          "form": "아아 아니에요 국어교육 전공이에요",
          "original_form": "아아 아니에요 국어교육 전공이에요",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019710.1.17",
          "form": "그 관련 수업을 들었어요 ㅎㄹ",
          "original_form": "그 관련 수업을 들었어요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019710.1.18",
          "form": "국어교육 전공이시구나",
          "original_form": "국어교육 전공이시구나",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019710.1.19",
          "form": "그런데도 책을 잘 안봐요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런데도 책을 잘 안봐요 ㅋㅋㅋㅋㅋㅋㅋ",
