        },
        {
          "id": "MDRW2100026284.1.3",
          "form": "아 왠지",
          "original_form": "아 왠지",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026284.1.4",
          "form": "name3 교수님 분반에서",
          "original_form": "&name3& 교수님 분반에서",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026284.1.5",
          "form": "name4 교수님으로 바뀐 게",
          "original_form": "&name4& 교수님으로 바뀐 게",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026284.1.6",
          "form": "고생의 서막이었나 싶기도 하고....",
          "original_form": "고생의 서막이었나 싶기도 하고....",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026284.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 약간 그런 느낌이 있긴한데 ,,,",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 약간 그런 느낌이 있긴한데 ,,,",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026284.1.8",
          "form": "어쩌겠어유ㅠ 지금 조교님이 교수님 맘에 들게 일도 더 잘하실거같아욬ㅋㅋㅋㅋ",
          "original_form": "어쩌겠어유ㅠ 지금 조교님이 교수님 맘에 들게 일도 더 잘하실거같아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100026284.1.9",
          "form": "저는 그냥 졸업요건이니까 ㅎㅎㅎ 무탈히 끝내는데 집중하려고요 ㅋㅋㅋㅋㅋㅋ 그친구 걱정이네요... 언니가 말해주신 그친구...",
          "original_form": "저는 그냥 졸업요건이니까 ㅎㅎㅎ 무탈히 끝내는데 집중하려고요 ㅋㅋㅋㅋㅋㅋ 그친구 걱정이네요... 언니가 말해주신 그친구...",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026284.1.10",
          "form": "아 그쵸 name5씨",
          "original_form": "아 그쵸 &name5&씨",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026284.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026284.1.12",
          "form": "22명이면",
          "original_form": "22명이면",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026284.1.13",
          "form": "빌런이 있을 확률도 더 높아지는데...",
          "original_form": "빌런이 있을 확률도 더 높아지는데...",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026284.1.14",
          "form": "진짜 지나가기만 하면 돼요",
          "original_form": "진짜 지나가기만 하면 돼요",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026284.1.15",
          "form": "이건 name2님 성적에 들어가는 것도 아니니까요!!!",
          "original_form": "이건 &name2&님 성적에 들어가는 것도 아니니까요!!!",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026284.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 벌써 실험 신청하겠다는 사람도 있는거 같더라고욬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 벌써 실험 신청하겠다는 사람도 있는거 같더라고욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026284.1.17",
          "form": "네...?!!",
          "original_form": "네...?!!",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026284.1.18",
          "form": "그래... 22명중에 한명도 안올리가...없겠죠...",
          "original_form": "그래... 22명중에 한명도 안올리가...없겠죠...",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026284.1.19",
          "form": "누군지 name2님은 아직 모르는거예요? ㅠ",
          "original_form": "누군지 &name2&님은 아직 모르는거예요? ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026284.1.20",
          "form": "실험 신청하면",
          "original_form": "실험 신청하면",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026284.1.21",
          "form": "교수님이 성적을 더 잘 줄지도 모른다는",
          "original_form": "교수님이 성적을 더 잘 줄지도 모른다는",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026284.1.22",
          "form": "기대감 때문에 그랬으려나....",
          "original_form": "기대감 때문에 그랬으려나....",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026284.1.23",
          "form": "네! 근데 그건 어차피 교수님이 하신댔으니까ㅜㅜ 믿어야죠ㅠㅠㅠ",
          "original_form": "네! 근데 그건 어차피 교수님이 하신댔으니까ㅜㅜ 믿어야죠ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026284.1.24",
          "form": "그런거라면 걱정안햐도 되는대",
          "original_form": "그런거라면 걱정안햐도 되는대",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026284.1.25",
          "form": "저도 약간 그런 생각 했었어서 뭐라고 못하겠네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 약간 그런 생각 했었어서 뭐라고 못하겠네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026284.1.26",
          "form": "하.... 그러게요 그건 그나마 다행이네요",
          "original_form": "하.... 그러게요 그건 그나마 다행이네요",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026284.1.27",
          "form": "name2씨가 걱정할 것은 이제",
          "original_form": "&name2&씨가 걱정할 것은 이제",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026284.1.28",
          "form": "엄청난....레포트의 양인거죠?!",
          "original_form": "엄청난....레포트의 양인거죠?!",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026284.1.29",
          "form": "네ㅜ 그거랑 채점기준... 자료는 어디까지 줄지... 언제까지 내라고 할지... 아직 하나도 못정하긴 했어요 ㅋㅋㅋㅋ",
          "original_form": "네ㅜ 그거랑 채점기준... 자료는 어디까지 줄지... 언제까지 내라고 할지... 아직 하나도 못정하긴 했어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026284.1.30",
          "form": "문재도 내야하는데 허허",
          "original_form": "문재도 내야하는데 허허",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026284.1.31",
          "form": "내일부터 열심히 해야죠",
