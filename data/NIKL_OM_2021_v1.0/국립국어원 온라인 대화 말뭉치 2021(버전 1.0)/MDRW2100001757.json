      "utterance": [
        {
          "id": "MDRW2100001757.1.1",
          "form": "룸메이트 있어요?",
          "original_form": "룸메이트 있어요?",
          "speaker_id": "1",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001757.1.2",
          "form": "아니요 가족과 함께 지내고 있습니다!",
          "original_form": "아니요 가족과 함께 지내고 있습니다!",
          "speaker_id": "2",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001757.1.3",
          "form": "볶음밥 님은 룸메이트 있으신가요?",
          "original_form": "볶음밥 님은 룸메이트 있으신가요?",
          "speaker_id": "2",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001757.1.4",
          "form": "아뇨 저도 가족이랑 지내고 있어요!",
          "original_form": "아뇨 저도 가족이랑 지내고 있어요!",
          "speaker_id": "1",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100001757.1.5",
          "form": "룸메이트 있었던 적은 있어요",
          "original_form": "룸메이트 있었던 적은 있어요",
          "speaker_id": "1",
          "time": "20210819 18:41"
        },
        {
          "id": "MDRW2100001757.1.6",
          "form": "저도 예전에 친구랑 같이 살았던 적이 있어요!",
          "original_form": "저도 예전에 친구랑 같이 살았던 적이 있어요!",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100001757.1.7",
          "form": "집안일 자주 하는 편이신가요ㅡ",
          "original_form": "집안일 자주 하는 편이신가요ㅡ",
          "speaker_id": "2",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100001757.1.8",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100001757.1.9",
          "form": "오 저는 기숙사 살았어서 외국인이랑도 같이 살아봤어요ㅎㅎ",
          "original_form": "오 저는 기숙사 살았어서 외국인이랑도 같이 살아봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 19:34"
        },
        {
          "id": "MDRW2100001757.1.10",
          "form": "집안일 필요할땐 하는 편인데.. 쉽지 않네요",
          "original_form": "집안일 필요할땐 하는 편인데.. 쉽지 않네요",
          "speaker_id": "1",
          "time": "20210819 19:35"
        },
        {
          "id": "MDRW2100001757.1.11",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210819 19:37"
        },
        {
          "id": "MDRW2100001757.1.12",
          "form": "안할수가 없는데 하기가 참 어렵더라구요ㅠㅠ",
          "original_form": "안할수가 없는데 하기가 참 어렵더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 19:37"
        },
        {
          "id": "MDRW2100001757.1.13",
          "form": "그래서 제 방은 항상 더러운가봐요...ㅎㅎ",
          "original_form": "그래서 제 방은 항상 더러운가봐요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 19:37"
        },
        {
          "id": "MDRW2100001757.1.14",
          "form": "저는 청소가 진짜 어려워요ㅠㅠ",
          "original_form": "저는 청소가 진짜 어려워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 19:38"
        },
        {
          "id": "MDRW2100001757.1.15",
          "form": "차라리 설거지나 이런건 하라면 하겠는데",
          "original_form": "차라리 설거지나 이런건 하라면 하겠는데",
          "speaker_id": "1",
          "time": "20210819 19:38"
        },
        {
          "id": "MDRW2100001757.1.16",
          "form": "저도요 저도요!!!",
          "original_form": "저도요 저도요!!!",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100001757.1.17",
          "form": "빨래나 설거지는 괜찮은데",
          "original_form": "빨래나 설거지는 괜찮은데",
          "speaker_id": "2",
          "time": "20210819 19:42"
        },
        {
          "id": "MDRW2100001757.1.18",
          "form": "방청소, 화장실청소 너무 힘들어요...",
          "original_form": "방청소, 화장실청소 너무 힘들어요...",
          "speaker_id": "2",
          "time": "20210819 19:43"
        },
        {
          "id": "MDRW2100001757.1.19",
          "form": "그쵸ㅠㅠㅠ방청소가 진짜ㅋㅋㅋ전 물건이 너무 많아서",
          "original_form": "그쵸ㅠㅠㅠ방청소가 진짜ㅋㅋㅋ전 물건이 너무 많아서",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100001757.1.20",
          "form": "그리고 버리는걸 진짜 못해요!",
          "original_form": "그리고 버리는걸 진짜 못해요!",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100001757.1.21",
          "form": "그래서 매번 물건이 가득가득",
          "original_form": "그래서 매번 물건이 가득가득",
          "speaker_id": "1",
          "time": "20210819 19:58"
        },
        {
          "id": "MDRW2100001757.1.22",
          "form": "저도요!!!!!",
          "original_form": "저도요!!!!!",
          "speaker_id": "2",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100001757.1.23",
          "form": "다 언젠간 쓰겠지...하고 버리질 못해서 방이 더 더러운가봐요...",
          "original_form": "다 언젠간 쓰겠지...하고 버리질 못해서 방이 더 더러운가봐요...",
          "speaker_id": "2",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100001757.1.24",
          "form": "최근에 진짜 안치우다가 책상 정리하면서 안쓰는것들 버렸는데 개운하더라구요ㅎㅎ",
          "original_form": "최근에 진짜 안치우다가 책상 정리하면서 안쓰는것들 버렸는데 개운하더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100001757.1.25",
          "form": "우와 대단해요",
          "original_form": "우와 대단해요",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100001757.1.26",
          "form": "저 진짜 안쓰눈거 버리려고 하면",
          "original_form": "저 진짜 안쓰눈거 버리려고 하면",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100001757.1.27",
          "form": "큰 결심해야 가능해요ㅠ",
          "original_form": "큰 결심해야 가능해요ㅠ",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100001757.1.28",
          "form": "1년이상 안쓰고 방치하는건 버려도 좋을거같아요ㅎㅎ",
          "original_form": "1년이상 안쓰고 방치하는건 버려도 좋을거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100001757.1.29",
          "form": "저는 머리론 생각하지만 못버리는게 한트럭이지만....",
          "original_form": "저는 머리론 생각하지만 못버리는게 한트럭이지만....",
          "speaker_id": "2",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100001757.1.30",
          "form": "그쵸 아예 하루 날 잡아서",
          "original_form": "그쵸 아예 하루 날 잡아서",
          "speaker_id": "1",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100001757.1.31",
          "form": "안 쓰는거 치워야 될까봐요ㅠㅠ",
          "original_form": "안 쓰는거 치워야 될까봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 21:12"
        }
      ]
