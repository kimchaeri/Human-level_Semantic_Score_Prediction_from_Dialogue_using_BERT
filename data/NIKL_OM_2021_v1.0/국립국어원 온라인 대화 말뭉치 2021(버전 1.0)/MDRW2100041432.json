      "utterance": [
        {
          "id": "MDRW2100041432.1.1",
          "form": "좋아요 19일 저도 좋아요",
          "original_form": "좋아요 19일 저도 좋아요",
          "speaker_id": "2",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100041432.1.2",
          "form": "18일 아닌가요 19일인가요",
          "original_form": "18일 아닌가요 19일인가요",
          "speaker_id": "2",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100041432.1.3",
          "form": "18일은 금요일인데요",
          "original_form": "18일은 금요일인데요",
          "speaker_id": "1",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100041432.1.4",
          "form": "토요일이네요",
          "original_form": "토요일이네요",
          "speaker_id": "1",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041432.1.5",
          "form": "파일로 18일 ㅋㅋㅋㅋ",
          "original_form": "파일로 18일 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041432.1.6",
          "form": "지금이 9월 아닌가요 18일이 토요일인데",
          "original_form": "지금이 9월 아닌가요 18일이 토요일인데",
          "speaker_id": "2",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041432.1.7",
          "form": "바람 때문에 달력이 한참 넘어가 있었네요",
          "original_form": "바람 때문에 달력이 한참 넘어가 있었네요",
          "speaker_id": "1",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041432.1.8",
          "form": "깜짝 놀랬네요 허허허",
          "original_form": "깜짝 놀랬네요 허허허",
          "speaker_id": "2",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041432.1.9",
          "form": "영도는 지금 태풍이 불어요",
          "original_form": "영도는 지금 태풍이 불어요",
          "speaker_id": "1",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041432.1.10",
          "form": "헉 어떡해요",
          "original_form": "헉 어떡해요",
          "speaker_id": "2",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041432.1.11",
          "form": "바람이 엄청나게 분답니다",
          "original_form": "바람이 엄청나게 분답니다",
          "speaker_id": "1",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041432.1.12",
          "form": "어제도 그랬고 그제도 그랬고 구월 들어 계속 그렇네요",
          "original_form": "어제도 그랬고 그제도 그랬고 구월 들어 계속 그렇네요",
          "speaker_id": "1",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041432.1.13",
          "form": "조심하세요 신문지 꼭 끼워 놓으시고요",
          "original_form": "조심하세요 신문지 꼭 끼워 놓으시고요",
          "speaker_id": "2",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041432.1.14",
          "form": "너무 추워요",
          "original_form": "너무 추워요",
          "speaker_id": "1",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041432.1.15",
          "form": "바람이 많이 부는 날에는 따뜻한 음료를 먹으면 참 좋은데 말이죠",
          "original_form": "바람이 많이 부는 날에는 따뜻한 음료를 먹으면 참 좋은데 말이죠",
          "speaker_id": "2",
          "time": "20210905 23:33"
        },
        {
          "id": "MDRW2100041432.1.16",
          "form": "그 정도는 아니에요",
          "original_form": "그 정도는 아니에요",
          "speaker_id": "1",
          "time": "20210905 23:33"
        },
        {
          "id": "MDRW2100041432.1.17",
          "form": "맞아요 그래서 지금 차 마시면서 대화중이에요",
          "original_form": "맞아요 그래서 지금 차 마시면서 대화중이에요",
          "speaker_id": "1",
          "time": "20210905 23:33"
        },
        {
          "id": "MDRW2100041432.1.18",
          "form": "요즘 시력이 나빠지기도 했고 알러지성 비염 때문에 눈에 좋은 차를 한번 구매해서 마셔 볼까 해요",
          "original_form": "요즘 시력이 나빠지기도 했고 알러지성 비염 때문에 눈에 좋은 차를 한번 구매해서 마셔 볼까 해요",
          "speaker_id": "2",
          "time": "20210905 23:33"
        },
        {
          "id": "MDRW2100041432.1.19",
          "form": "아까 사과 얘기 하셔서 기억나네요",
          "original_form": "아까 사과 얘기 하셔서 기억나네요",
          "speaker_id": "1",
          "time": "20210905 23:39"
        },
        {
          "id": "MDRW2100041432.1.20",
          "form": "작년에 사과가 너무 맛이 없어서 사과 어 저가 쨈을 만들어 먹었어요",
          "original_form": "작년에 사과가 너무 맛이 없어서 사과 어 저가 쨈을 만들어 먹었어요",
          "speaker_id": "1",
          "time": "20210905 23:39"
        }
      ]
