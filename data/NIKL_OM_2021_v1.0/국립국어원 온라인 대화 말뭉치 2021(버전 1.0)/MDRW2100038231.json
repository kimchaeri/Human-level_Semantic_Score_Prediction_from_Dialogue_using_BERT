      "utterance": [
        {
          "id": "MDRW2100038231.1.1",
          "form": "근데 카즈하는",
          "original_form": "근데 카즈하는",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038231.1.2",
          "form": "밸런스를 무게로 맞추더라고",
          "original_form": "밸런스를 무게로 맞추더라고",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038231.1.3",
          "form": "몸이 들어가잖아",
          "original_form": "몸이 들어가잖아",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038231.1.4",
          "form": "몸?",
          "original_form": "몸?",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038231.1.5",
          "form": "벤티는 어느 한 점에 모는건데",
          "original_form": "벤티는 어느 한 점에 모는건데",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038231.1.6",
          "form": "카즈하는 자기가 있던 자리에",
          "original_form": "카즈하는 자기가 있던 자리에",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038231.1.7",
          "form": "생기는거 아니야?",
          "original_form": "생기는거 아니야?",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038231.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038231.1.9",
          "form": "정확하게는",
          "original_form": "정확하게는",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038231.1.10",
          "form": "벤티는 원거리공격이라",
          "original_form": "벤티는 원거리공격이라",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038231.1.11",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038231.1.12",
          "form": "원거리 허공으로 끌어올리는거고",
          "original_form": "원거리 허공으로 끌어올리는거고",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038231.1.13",
          "form": "카즈하는 근접딜러라",
          "original_form": "카즈하는 근접딜러라",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038231.1.14",
          "form": "근거리 지상에 모으는거",
          "original_form": "근거리 지상에 모으는거",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038231.1.15",
          "form": "몹에 둘러싸이진 않아",
          "original_form": "몹에 둘러싸이진 않아",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038231.1.16",
          "form": "카즈하는 궁이 뭐야?",
          "original_form": "카즈하는 궁이 뭐야?",
          "speaker_id": "2",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038231.1.17",
          "form": "일정 범위 안애",
          "original_form": "일정 범위 안애",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038231.1.18",
          "form": "내가 직전에 가했던 공격이",
          "original_form": "내가 직전에 가했던 공격이",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038231.1.19",
          "form": "반복적으로 되풀이됨",
          "original_form": "반복적으로 되풀이됨",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038231.1.20",
          "form": "복잡하네",
          "original_form": "복잡하네",
          "speaker_id": "2",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038231.1.21",
          "form": "직후에 가한건가?",
          "original_form": "직후에 가한건가?",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038231.1.22",
          "form": "카즈하 궁쓰고",
          "original_form": "카즈하 궁쓰고",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038231.1.23",
          "form": "다이루크 스킬 쓰면",
          "original_form": "다이루크 스킬 쓰면",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038231.1.24",
          "form": "한 15초?정도",
          "original_form": "한 15초?정도",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038231.1.25",
          "form": "화염공격이 자동으로 돌아다님",
          "original_form": "화염공격이 자동으로 돌아다님",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038231.1.26",
          "form": "상대방 공격 복사하는 느낌",
          "original_form": "상대방 공격 복사하는 느낌",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038231.1.27",
          "form": "오 딜보조 스킬이구나",
          "original_form": "오 딜보조 스킬이구나",
          "speaker_id": "2",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100038231.1.28",
          "form": "ㅇㅇ 그런 느낌이야",
          "original_form": "ㅇㅇ 그런 느낌이야",
          "speaker_id": "1",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100038231.1.29",
          "form": "그래서 다이루크 쓰고 주인공 스킬로 번개 붙임",
          "original_form": "그래서 다이루크 쓰고 주인공 스킬로 번개 붙임",
          "speaker_id": "1",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100038231.1.30",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100038231.1.31",
          "form": "난 라이덴 다 키우면",
          "original_form": "난 라이덴 다 키우면",
          "speaker_id": "2",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100038231.1.32",
          "form": "파티",
          "original_form": "파티",
          "speaker_id": "2",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100038231.1.33",
          "form": "를 어떡할까",
          "original_form": "를 어떡할까",
          "speaker_id": "2",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100038231.1.34",
          "form": "고민중이야",
          "original_form": "고민중이야",
          "speaker_id": "2",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100038231.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 난 또",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 난 또",
          "speaker_id": "1",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100038231.1.36",
          "form": "파티 어떻게 변경한다는줄 ㅋㅋㅋㅋ",
          "original_form": "파티 어떻게 변경한다는줄 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:22"
        },
        {
          "id": "MDRW2100038231.1.37",
          "form": "아니 얘가 또",
          "original_form": "아니 얘가 또",
          "speaker_id": "2",
          "time": "20210902 01:22"
        }
      ]
