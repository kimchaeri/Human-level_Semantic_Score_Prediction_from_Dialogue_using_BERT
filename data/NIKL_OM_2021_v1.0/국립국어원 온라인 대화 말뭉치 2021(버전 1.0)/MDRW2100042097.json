      "utterance": [
        {
          "id": "MDRW2100042097.1.1",
          "form": "타타라 모노가타리",
          "original_form": "타타라 모노가타리",
          "speaker_id": "2",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042097.1.2",
          "form": "다 해도",
          "original_form": "다 해도",
          "speaker_id": "2",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042097.1.3",
          "form": "걍 엥간해선 멘스 밀거나 퍼즐 풀면 업ㄱ어지던데",
          "original_form": "걍 엥간해선 멘스 밀거나 퍼즐 풀면 업ㄱ어지던데",
          "speaker_id": "1",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042097.1.4",
          "form": "장판 안사라져?",
          "original_form": "장판 안사라져?",
          "speaker_id": "2",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042097.1.5",
          "form": "ㅇㅇ 걘 변함이 없어서...",
          "original_form": "ㅇㅇ 걘 변함이 없어서...",
          "speaker_id": "1",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042097.1.6",
          "form": "난 그게 없애는거라고",
          "original_form": "난 그게 없애는거라고",
          "speaker_id": "2",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042097.1.7",
          "form": "생각했는데",
          "original_form": "생각했는데",
          "speaker_id": "2",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042097.1.8",
          "form": "헐....",
          "original_form": "헐....",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.9",
          "form": "그건 타타라쪽 장판만 없애는거고",
          "original_form": "그건 타타라쪽 장판만 없애는거고",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.10",
          "form": "그 무상의 협곡?말하는거 아냐?",
          "original_form": "그 무상의 협곡?말하는거 아냐?",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.11",
          "form": "ㅇㅎ....그 흐르는 강은",
          "original_form": "ㅇㅎ....그 흐르는 강은",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.12",
          "form": "그대로구나",
          "original_form": "그대로구나",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.13",
          "form": "무상의 일태도 흔적",
          "original_form": "무상의 일태도 흔적",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.14",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.15",
          "form": "난 거기까지 없어질줄......",
          "original_form": "난 거기까지 없어질줄......",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.16",
          "form": "나도 거기 없어질줄 알앗는데",
          "original_form": "나도 거기 없어질줄 알앗는데",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.17",
          "form": "그대로더라고...",
          "original_form": "그대로더라고...",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.18",
          "form": "너무 도둑놈 심보인가",
          "original_form": "너무 도둑놈 심보인가",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.19",
          "form": "미카게 용광로쪽에서",
          "original_form": "미카게 용광로쪽에서",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.20",
          "form": "흐르는거라고",
          "original_form": "흐르는거라고",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.21",
          "form": "생각했는데",
          "original_form": "생각했는데",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042097.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 멘스 밀었음 사라져야지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 멘스 밀었음 사라져야지",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042097.1.23",
          "form": "그닉가",
          "original_form": "그닉가",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042097.1.24",
          "form": "이제 안수령도 안한다는데 뭔 전기장판이냐\r\n...!",
          "original_form": "이제 안수령도 안한다는데 뭔 전기장판이냐\r\n...!",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042097.1.25",
          "form": "귀찮아 진짜",
          "original_form": "귀찮아 진짜",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042097.1.26",
          "form": "여튼 울림풀 은근 구하기 힘들어",
          "original_form": "여튼 울림풀 은근 구하기 힘들어",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042097.1.27",
          "form": "ㅇㅇ 근데 또 특산물이라고 여기저기서 요구함",
          "original_form": "ㅇㅇ 근데 또 특산물이라고 여기저기서 요구함",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042097.1.28",
          "form": "총 48개 필요하다는데",
          "original_form": "총 48개 필요하다는데",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042097.1.29",
          "form": "그 퀘스트에",
          "original_form": "그 퀘스트에",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042097.1.30",
          "form": "헐 진짜???",
          "original_form": "헐 진짜???",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042097.1.31",
          "form": "그럼 한번 더 내야하네",
          "original_form": "그럼 한번 더 내야하네",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042097.1.32",
          "form": "젠장",
          "original_form": "젠장",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042097.1.33",
          "form": "아마 만나서 12개 주는건 했지?",
          "original_form": "아마 만나서 12개 주는건 했지?",
          "speaker_id": "2",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100042097.1.34",
          "form": "빨리 끝내버리고 싶은데....",
          "original_form": "빨리 끝내버리고 싶은데....",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100042097.1.35",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100042097.1.36",
          "form": "그럼 36개일듯",
          "original_form": "그럼 36개일듯",
          "speaker_id": "2",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100042097.1.37",
