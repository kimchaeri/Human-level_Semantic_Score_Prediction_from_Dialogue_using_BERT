      "utterance": [
        {
          "id": "MDRW2100020167.1.1",
          "form": "게임 좋아하시나요",
          "original_form": "게임 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020167.1.2",
          "form": "어릴때 많이했었어요",
          "original_form": "어릴때 많이했었어요",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020167.1.3",
          "form": "요즘은 안하시는군요",
          "original_form": "요즘은 안하시는군요",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020167.1.4",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020167.1.5",
          "form": "요즘은 딱히 재밌는게임이 없네요",
          "original_form": "요즘은 딱히 재밌는게임이 없네요",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020167.1.6",
          "form": "스팀게임은 재미납니다",
          "original_form": "스팀게임은 재미납니다",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020167.1.7",
          "form": "전 온라인게임은 안좋아해요",
          "original_form": "전 온라인게임은 안좋아해요",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020167.1.8",
          "form": "스팀게임이 뭐에요?",
          "original_form": "스팀게임이 뭐에요?",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020167.1.9",
          "form": "전 주로 온라인게임 했었어요 ㅎㅎ",
          "original_form": "전 주로 온라인게임 했었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020167.1.10",
          "form": "스팀은 게임유통사라고 해야할까요",
          "original_form": "스팀은 게임유통사라고 해야할까요",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020167.1.11",
          "form": "온라인게임 어떤거요?",
          "original_form": "온라인게임 어떤거요?",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020167.1.12",
          "form": "게임유통사? ㅜ모르겠네요",
          "original_form": "게임유통사? ㅜ모르겠네요",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020167.1.13",
          "form": "어릴땐 큐플레이 알투비트",
          "original_form": "어릴땐 큐플레이 알투비트",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020167.1.14",
          "form": "크레이지아케이드",
          "original_form": "크레이지아케이드",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020167.1.15",
          "form": "이런게임 했었어요",
          "original_form": "이런게임 했었어요",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020167.1.16",
          "form": "너무 예전같은데",
          "original_form": "너무 예전같은데",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020167.1.17",
          "form": "옛날..ㅋㅋㅋㅋ",
          "original_form": "옛날..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020167.1.18",
          "form": "이마트 같은거라 보심되요",
          "original_form": "이마트 같은거라 보심되요",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020167.1.19",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020167.1.20",
          "form": "테트리스랑 맞고도 좋아했는데",
          "original_form": "테트리스랑 맞고도 좋아했는데",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020167.1.21",
          "form": "한때 저도 테트리스 열심히했었어요 근데 늘 방식이 비슷비슷해서 쉽게질리더라고요",
          "original_form": "한때 저도 테트리스 열심히했었어요 근데 늘 방식이 비슷비슷해서 쉽게질리더라고요",
          "speaker_id": "1",
