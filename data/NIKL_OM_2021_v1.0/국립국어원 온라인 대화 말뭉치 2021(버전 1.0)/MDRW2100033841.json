        },
        {
          "id": "MDRW2100033841.1.2",
          "form": "커피는 좋아하세요?\r\n아메리카노vs푸라푸치노?",
          "original_form": "커피는 좋아하세요?\r\n아메리카노vs푸라푸치노?",
          "speaker_id": "1",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100033841.1.3",
          "form": "아메리 카노요",
          "original_form": "아메리 카노요",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100033841.1.4",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 12:13"
        },
        {
          "id": "MDRW2100033841.1.5",
          "form": "전 푸라푸치노요ㅋㅋㅋ",
          "original_form": "전 푸라푸치노요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:17"
        },
        {
          "id": "MDRW2100033841.1.6",
          "form": "아메를 못마신답니다ㅠ",
          "original_form": "아메를 못마신답니다ㅠ",
          "speaker_id": "1",
          "time": "20210825 12:17"
        },
        {
          "id": "MDRW2100033841.1.7",
          "form": "아 그러세요",
          "original_form": "아 그러세요",
          "speaker_id": "2",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100033841.1.8",
          "form": "그럴수 있러요 ㅎㅎ제친구도 그래요",
          "original_form": "그럴수 있러요 ㅎㅎ제친구도 그래요",
          "speaker_id": "2",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100033841.1.9",
          "form": "네네ㅋㅋㅋ 아예요",
          "original_form": "네네ㅋㅋㅋ 아예요",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100033841.1.10",
          "form": "녹차vs홍차는 어떠세요?",
          "original_form": "녹차vs홍차는 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100033841.1.11",
          "form": "홍차요",
          "original_form": "홍차요",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100033841.1.12",
          "form": "밀크딩 달달하게 잘마셔요",
          "original_form": "밀크딩 달달하게 잘마셔요",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100033841.1.13",
          "form": "흑당 ㅋㅋ중독자 였어요",
          "original_form": "흑당 ㅋㅋ중독자 였어요",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100033841.1.14",
          "form": "흑당은요?",
          "original_form": "흑당은요?",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100033841.1.15",
          "form": "ㅋㅋㅋ 저는 이상하게 흑당잘못먹겠더라고요",
          "original_form": "ㅋㅋㅋ 저는 이상하게 흑당잘못먹겠더라고요",
          "speaker_id": "1",
          "time": "20210825 12:29"
        },
        {
          "id": "MDRW2100033841.1.16",
          "form": "밀크티도요",
          "original_form": "밀크티도요",
          "speaker_id": "1",
          "time": "20210825 12:29"
        },
        {
          "id": "MDRW2100033841.1.17",
          "form": "아 그게 호불호 갈려요",
          "original_form": "아 그게 호불호 갈려요",
          "speaker_id": "2",
          "time": "20210825 12:29"
        },
        {
          "id": "MDRW2100033841.1.18",
          "form": "저도 사실 대만에선 못먹었어요 ㅋㅋ",
          "original_form": "저도 사실 대만에선 못먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:29"
        },
        {
          "id": "MDRW2100033841.1.19",
          "form": "몇번 먹다 보니 맛있더라구요",
          "original_form": "몇번 먹다 보니 맛있더라구요",
          "speaker_id": "2",
          "time": "20210825 12:29"
        },
        {
          "id": "MDRW2100033841.1.20",
          "form": "한창유행했었죠ㅋㅋㅋ",
          "original_form": "한창유행했었죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:36"
        },
        {
          "id": "MDRW2100033841.1.21",
          "form": "대만 음식이 한국입맛에 잘맞더라구요",
          "original_form": "대만 음식이 한국입맛에 잘맞더라구요",
          "speaker_id": "1",
          "time": "20210825 12:36"
        },
        {
          "id": "MDRW2100033841.1.22",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100033841.1.23",
          "form": "전 여행에서 잘먹고왔어요 ㅎㅎ",
          "original_form": "전 여행에서 잘먹고왔어요 ㅎㅎ",
