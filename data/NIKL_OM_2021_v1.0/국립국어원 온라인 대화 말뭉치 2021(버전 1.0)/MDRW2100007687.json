      "utterance": [
        {
          "id": "MDRW2100007687.1.1",
          "form": "안녕하세요 비와서 날씨가 선선하네요ㅎㅎ",
          "original_form": "안녕하세요 비와서 날씨가 선선하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100007687.1.2",
          "form": "근데 넘 습해서 ㅜㅜ",
          "original_form": "근데 넘 습해서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007687.1.3",
          "form": "힘드네요ㅜㅜ",
          "original_form": "힘드네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007687.1.4",
          "form": "네 제습기 돌리고 있어요 ㅠㅠ빨래사가 안말라서ㅎㅎ",
          "original_form": "네 제습기 돌리고 있어요 ㅠㅠ빨래사가 안말라서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007687.1.5",
          "form": "앗 저도 어제 신발 젖어서 제습기돌렸어요",
          "original_form": "앗 저도 어제 신발 젖어서 제습기돌렸어요",
          "speaker_id": "2",
          "time": "20210822 09:43"
        },
        {
          "id": "MDRW2100007687.1.6",
          "form": "비가 갑자기 ㅎㅎ태풍도 온다구 하고 이상해요ㅎㅎ",
          "original_form": "비가 갑자기 ㅎㅎ태풍도 온다구 하고 이상해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100007687.1.7",
          "form": "제습기 말리니까 신발도 하루만에 마르긴했네요 다행히 ㅋㅋ",
          "original_form": "제습기 말리니까 신발도 하루만에 마르긴했네요 다행히 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100007687.1.8",
          "form": "저도 어제 운동화 신었다가ㅠㅠ감당이 안되서 빨았어요ㅎㅎ",
          "original_form": "저도 어제 운동화 신었다가ㅠㅠ감당이 안되서 빨았어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007687.1.9",
          "form": "운동화 빨때 뭘로 빠셨어요?",
          "original_form": "운동화 빨때 뭘로 빠셨어요?",
          "speaker_id": "2",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007687.1.10",
          "form": "저는 샴푸썼는데 망한거같아요 ㅋㅋㅋ",
          "original_form": "저는 샴푸썼는데 망한거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007687.1.11",
          "form": "아 중성세제ㅋㅋㅋ저는 바디워시ㅠㅠㅋㅋ비올줄 알았으면 샌들신는건데ㅠㅠ",
          "original_form": "아 중성세제ㅋㅋㅋ저는 바디워시ㅠㅠㅋㅋ비올줄 알았으면 샌들신는건데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007687.1.12",
          "form": "저도 아쿠아슈즈샀는데",
          "original_form": "저도 아쿠아슈즈샀는데",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007687.1.13",
          "form": "어제는 왜 운동화 신었나몰라요",
          "original_form": "어제는 왜 운동화 신었나몰라요",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007687.1.14",
          "form": "아쿠아슈즈 좋은데 음 별로기도하고 ㅋㅋ",
          "original_form": "아쿠아슈즈 좋은데 음 별로기도하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007687.1.15",
          "form": "저도 아쿠아슈즈 있는데 별로 신지도 못하고 여름이 가버리고 있어요ㅎㅎ",
          "original_form": "저도 아쿠아슈즈 있는데 별로 신지도 못하고 여름이 가버리고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007687.1.16",
          "form": "오 아쿠아슈즈 어디 거세요",
          "original_form": "오 아쿠아슈즈 어디 거세요",
          "speaker_id": "2",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007687.1.17",
          "form": "저는 벨롭이유명하대서 샀는데",
          "original_form": "저는 벨롭이유명하대서 샀는데",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007687.1.18",
          "form": "저는 그냥 보세요",
          "original_form": "저는 그냥 보세요",
          "speaker_id": "1",
          "time": "20210822 09:54"
        }
      ]
