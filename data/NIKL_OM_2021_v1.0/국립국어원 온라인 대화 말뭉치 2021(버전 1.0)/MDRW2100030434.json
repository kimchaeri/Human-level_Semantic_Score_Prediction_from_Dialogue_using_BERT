      "utterance": [
        {
          "id": "MDRW2100030434.1.1",
          "form": "요즘엔 이렇게 하이브리드 된 걸로 게임 할 수 있게 나오네요",
          "original_form": "요즘엔 이렇게 하이브리드 된 걸로 게임 할 수 있게 나오네요",
          "speaker_id": "1",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100030434.1.2",
          "form": "눈도 빨간 거 같기도 해요 자세히 보면",
          "original_form": "눈도 빨간 거 같기도 해요 자세히 보면",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100030434.1.3",
          "form": "잘 몰라서 그냥 구워 먹으려구요",
          "original_form": "잘 몰라서 그냥 구워 먹으려구요",
          "speaker_id": "2",
          "time": "20210904 23:08"
        },
        {
          "id": "MDRW2100030434.1.4",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100030434.1.5",
          "form": "시도해 보는 것이 굉장히 용감한 일이라고 생각합니다",
          "original_form": "시도해 보는 것이 굉장히 용감한 일이라고 생각합니다",
          "speaker_id": "1",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100030434.1.6",
          "form": "새로 시작하는 기분이에요",
          "original_form": "새로 시작하는 기분이에요",
          "speaker_id": "2",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100030434.1.7",
          "form": "다시 한국에서 살지 몰랐거든요",
          "original_form": "다시 한국에서 살지 몰랐거든요",
          "speaker_id": "2",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100030434.1.8",
          "form": "맞아요 시행착오를 많이 겪어야 될 것 같아요",
          "original_form": "맞아요 시행착오를 많이 겪어야 될 것 같아요",
          "speaker_id": "1",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100030434.1.9",
          "form": "그러나 때문에 일을 하는 것도 그렇고 생활 전반이 많이 바뀌네요",
          "original_form": "그러나 때문에 일을 하는 것도 그렇고 생활 전반이 많이 바뀌네요",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.10",
          "form": "품앗이 하면서 농사를 어깨너머로 보려고 해요",
          "original_form": "품앗이 하면서 농사를 어깨너머로 보려고 해요",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.11",
          "form": "코로나",
          "original_form": "코로나",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.12",
          "form": "항상 새로운 도전을 많이 해요",
          "original_form": "항상 새로운 도전을 많이 해요",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.13",
          "form": "좀 쉬엄쉬엄 놀며 살고 싶네요",
          "original_form": "좀 쉬엄쉬엄 놀며 살고 싶네요",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.14",
          "form": "맞아요 항상 도전적인 것 같아요",
          "original_form": "맞아요 항상 도전적인 것 같아요",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.15",
          "form": "일 복이 많은 것이겠죠",
          "original_form": "일 복이 많은 것이겠죠",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.16",
          "form": "딸기가 아니라 다른 농사를 할 수도 있는데",
          "original_form": "딸기가 아니라 다른 농사를 할 수도 있는데",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.17",
          "form": "농사를 하고 싶어요",
          "original_form": "농사를 하고 싶어요",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.18",
          "form": "딸기 수학하면 나눔도 할게요",
          "original_form": "딸기 수학하면 나눔도 할게요",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.19",
          "form": "잘하실 거예요 꼼꼼한 성격 이시다 보니 체크도 잘 하고 배운 걸 잘 활용할 것 같아요",
          "original_form": "잘하실 거예요 꼼꼼한 성격 이시다 보니 체크도 잘 하고 배운 걸 잘 활용할 것 같아요",
          "speaker_id": "1",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.20",
          "form": "딸기 따러 오실 거죠",
          "original_form": "딸기 따러 오실 거죠",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100030434.1.21",
          "form": "주소 미리 불러 드릴까요",
          "original_form": "주소 미리 불러 드릴까요",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100030434.1.22",
          "form": "일 손이 필요해요",
          "original_form": "일 손이 필요해요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100030434.1.23",
          "form": "주말에 갈게요 평일에는 힘들어요",
          "original_form": "주말에 갈게요 평일에는 힘들어요",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100030434.1.24",
          "form": "가면 고기 주시나요",
          "original_form": "가면 고기 주시나요",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100030434.1.25",
          "form": "와서 열 바켓 주고 한 바켓 가져가요",
          "original_form": "와서 열 바켓 주고 한 바켓 가져가요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100030434.1.26",
          "form": "저는 배가 빨리 꺼져요",
          "original_form": "저는 배가 빨리 꺼져요",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100030434.1.27",
          "form": "산청에 흑돼지가 있지요",
          "original_form": "산청에 흑돼지가 있지요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100030434.1.28",
          "form": "산청 돼지 유명하답니다",
          "original_form": "산청 돼지 유명하답니다",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100030434.1.29",
          "form": "딸기 좋아해서 그 자리에서 다 먹을 듯",
          "original_form": "딸기 좋아해서 그 자리에서 다 먹을 듯",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100030434.1.30",
          "form": "일당을 돼지로 드릴게요",
          "original_form": "일당을 돼지로 드릴게요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100030434.1.31",
          "form": "흑돼지 맛있죠",
          "original_form": "흑돼지 맛있죠",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100030434.1.32",
          "form": "블루베리도 많이 키우 더라구요",
          "original_form": "블루베리도 많이 키우 더라구요",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100030434.1.33",
          "form": "2잡하게 되는 건가요",
          "original_form": "2잡하게 되는 건가요",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100030434.1.34",
          "form": "약초도 많이 키우고요",
          "original_form": "약초도 많이 키우고요",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100030434.1.35",
          "form": "괜찮네요 그것도",
          "original_form": "괜찮네요 그것도",
          "speaker_id": "2",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100030434.1.36",
          "form": "블루베리도 개인적으로 매우 좋아합니다",
          "original_form": "블루베리도 개인적으로 매우 좋아합니다",
