      },
      "utterance": [
        {
          "id": "MDRW2100003426.1.1",
          "form": "자는 듯",
          "original_form": "자는 듯",
          "speaker_id": "2",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003426.1.2",
          "form": "산책 갔다 와서 피곤한가 보다",
          "original_form": "산책 갔다 와서 피곤한가 보다",
          "speaker_id": "2",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003426.1.3",
          "form": "글쿠만",
          "original_form": "글쿠만",
          "speaker_id": "1",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003426.1.4",
          "form": "요새 많이 자는것같아",
          "original_form": "요새 많이 자는것같아",
          "speaker_id": "1",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003426.1.5",
          "form": "이제 나이가",
          "original_form": "이제 나이가",
          "speaker_id": "2",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003426.1.6",
          "form": "좀 많으니까 그렇지",
          "original_form": "좀 많으니까 그렇지",
          "speaker_id": "2",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003426.1.7",
          "form": "글쿠만 울 강아지들도 이제 8살씩 됐으니 ㅠㅎ",
          "original_form": "글쿠만 울 강아지들도 이제 8살씩 됐으니 ㅠㅎ",
          "speaker_id": "1",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003426.1.8",
          "form": "그래도 아직 한창이다",
          "original_form": "그래도 아직 한창이다",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003426.1.9",
          "form": "산책하면 날아다닌다",
          "original_form": "산책하면 날아다닌다",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003426.1.10",
          "form": "글쿤 ㅋㅋ",
          "original_form": "글쿤 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003426.1.11",
          "form": "그래 20살넘께 살아야지 울 강아지들은!",
          "original_form": "그래 20살넘께 살아야지 울 강아지들은!",
          "speaker_id": "1",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003426.1.12",
          "form": "튼튼해서 오래오래 살거얌",
          "original_form": "튼튼해서 오래오래 살거얌",
          "speaker_id": "1",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003426.1.13",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003426.1.14",
          "form": "저녁에 양치 시켜야 하는데",
          "original_form": "저녁에 양치 시켜야 하는데",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003426.1.15",
          "form": "구래 엄마가 양치좀 시켜줘라",
          "original_form": "구래 엄마가 양치좀 시켜줘라",
          "speaker_id": "1",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003426.1.16",
          "form": "울 강아지들 이빨 깨끗해지게",
          "original_form": "울 강아지들 이빨 깨끗해지게",
          "speaker_id": "1",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003426.1.17",
          "form": "딸내미가 좀 해라",
          "original_form": "딸내미가 좀 해라",
          "speaker_id": "2",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003426.1.18",
          "form": "알았다 ㅎ",
          "original_form": "알았다 ㅎ",
          "speaker_id": "1",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003426.1.19",
          "form": "나도 같이 해야겠다",
          "original_form": "나도 같이 해야겠다",
          "speaker_id": "1",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003426.1.20",
          "form": "그래 양치하고 씻고 자라",
          "original_form": "그래 양치하고 씻고 자라",
          "speaker_id": "2",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003426.1.21",
          "form": "알았어용",
          "original_form": "알았어용",
          "speaker_id": "1",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003426.1.22",
          "form": "강아지 안고 자라",
          "original_form": "강아지 안고 자라",
