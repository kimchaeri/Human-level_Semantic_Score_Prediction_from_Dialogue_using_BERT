      "utterance": [
        {
          "id": "MDRW2100008385.1.1",
          "form": "가족 하 요새",
          "original_form": "가족 하 요새",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008385.1.2",
          "form": "화장실청소땜에스트레스받음",
          "original_form": "화장실청소땜에스트레스받음",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008385.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008385.1.4",
          "form": "그것은 절대 사소한문제가 아님",
          "original_form": "그것은 절대 사소한문제가 아님",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008385.1.5",
          "form": "스트레스요인이 된다궁",
          "original_form": "스트레스요인이 된다궁",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008385.1.6",
          "form": "화장실 곰팡이는 어떻게해야 사라지는건가요",
          "original_form": "화장실 곰팡이는 어떻게해야 사라지는건가요",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008385.1.7",
          "form": "자잘한 집안일 진짜 스트레스임",
          "original_form": "자잘한 집안일 진짜 스트레스임",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008385.1.8",
          "form": "곰팡이 제거 세제 뿌려놓고",
          "original_form": "곰팡이 제거 세제 뿌려놓고",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008385.1.9",
          "form": "두세시간뒤에 청소하문 안대나",
          "original_form": "두세시간뒤에 청소하문 안대나",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100008385.1.10",
          "form": "여름옷 손빨래라던가",
          "original_form": "여름옷 손빨래라던가",
          "speaker_id": "2",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100008385.1.11",
          "form": "손빨래",
          "original_form": "손빨래",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008385.1.12",
          "form": "손빨래 진짜 하기싫은데",
          "original_form": "손빨래 진짜 하기싫은데",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008385.1.13",
          "form": "여름옷이 좋아",
          "original_form": "여름옷이 좋아",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008385.1.14",
          "form": "엄마는 또 꼭 여름옷은 손빨래해야한다 주의심",
          "original_form": "엄마는 또 꼭 여름옷은 손빨래해야한다 주의심",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008385.1.15",
          "form": "겨울옷은 손빨래힘들오ㅜ",
          "original_form": "겨울옷은 손빨래힘들오ㅜ",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008385.1.16",
          "form": "옷이 얇아서 상한다고",
          "original_form": "옷이 얇아서 상한다고",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008385.1.17",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008385.1.18",
          "form": "일리가 있다",
          "original_form": "일리가 있다",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008385.1.19",
          "form": "힘들뿐",
          "original_form": "힘들뿐",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008385.1.20",
          "form": "겨울옷은 시도안하는데",
          "original_form": "겨울옷은 시도안하는데",
