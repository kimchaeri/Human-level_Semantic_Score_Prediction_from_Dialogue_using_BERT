      "utterance": [
        {
          "id": "MDRW2100009444.1.1",
          "form": "좋아하는 메뉴있으신가요?",
          "original_form": "좋아하는 메뉴있으신가요?",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009444.1.2",
          "form": "커피요? ㅋ",
          "original_form": "커피요? ㅋ",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009444.1.3",
          "form": "넹넹 ㅎㅎ 좋아하는 커피는 어떤거에요?",
          "original_form": "넹넹 ㅎㅎ 좋아하는 커피는 어떤거에요?",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009444.1.4",
          "form": "집에선 맥심, 나가면 스벅요 ㅎ 뭐 좋아하세요?",
          "original_form": "집에선 맥심, 나가면 스벅요 ㅎ 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009444.1.5",
          "form": "저는 아이스아메리카노요 ㅎㅎ",
          "original_form": "저는 아이스아메리카노요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009444.1.6",
          "form": "스벅에서 뭐 주로드세요?",
          "original_form": "스벅에서 뭐 주로드세요?",
          "speaker_id": "1",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009444.1.7",
          "form": "전 달달한 돌체라떼요 ㅎ",
          "original_form": "전 달달한 돌체라떼요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009444.1.8",
          "form": "돌체라떼맛나던데 ㅎㅎㅎ",
          "original_form": "돌체라떼맛나던데 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009444.1.9",
          "form": "디저트좋아하시는거있나요",
          "original_form": "디저트좋아하시는거있나요",
          "speaker_id": "1",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009444.1.10",
          "form": "저 마카롱 좋아해요~ 비싸서 가끔 먹지만요 ㅎ",
          "original_form": "저 마카롱 좋아해요~ 비싸서 가끔 먹지만요 ㅎ",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100009444.1.11",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100009444.1.12",
          "form": "샌드위치도 좋아하시나요?",
          "original_form": "샌드위치도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100009444.1.13",
          "form": "그럼요~ 파리바게트 샌드위치 예전에 자주 사먹었어요 샌드위치 좋아하시는군요",
          "original_form": "그럼요~ 파리바게트 샌드위치 예전에 자주 사먹었어요 샌드위치 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100009444.1.14",
          "form": "네ㅎㅎ 양상추 많이 들어간 샌드위치를 좋아해요",
          "original_form": "네ㅎㅎ 양상추 많이 들어간 샌드위치를 좋아해요",
          "speaker_id": "1",
          "time": "20210823 13:53"
        },
        {
          "id": "MDRW2100009444.1.15",
          "form": "지금은 김치찌개먹는중입니다 ㅎㅎ",
          "original_form": "지금은 김치찌개먹는중입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100009444.1.16",
          "form": "전 맛살 들어간 샌드위치 좋아해요. 오늘 점심은 된장찌개 먹었어요",
          "original_form": "전 맛살 들어간 샌드위치 좋아해요. 오늘 점심은 된장찌개 먹었어요",
          "speaker_id": "2",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100009444.1.17",
          "form": "저도 와사비맛살 좋아해요 ㅎㅎ",
          "original_form": "저도 와사비맛살 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100009444.1.18",
          "form": "된장찌개 얼큰한거 좋아하시나요?",
          "original_form": "된장찌개 얼큰한거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100009444.1.19",
          "form": "와사비 맛살 듣기만 해도 맛나겠네요",
          "original_form": "와사비 맛살 듣기만 해도 맛나겠네요",
          "speaker_id": "2",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100009444.1.20",
          "form": "네 된장에 고추장 조금 넣어요. 청양고추도 좀 넣고요",
          "original_form": "네 된장에 고추장 조금 넣어요. 청양고추도 좀 넣고요",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100009444.1.21",
          "form": "헉 저도 고추장넣는데 ㅎㅎ",
          "original_form": "헉 저도 고추장넣는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100009444.1.22",
          "form": "고추장찌개도 좋아합니다!",
          "original_form": "고추장찌개도 좋아합니다!",
          "speaker_id": "1",
          "time": "20210823 16:28"
        }
      ]
    }
