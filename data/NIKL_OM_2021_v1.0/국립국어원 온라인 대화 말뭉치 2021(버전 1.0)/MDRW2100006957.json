      "utterance": [
        {
          "id": "MDRW2100006957.1.1",
          "form": "혹시 회사 다니세요?",
          "original_form": "혹시 회사 다니세요?",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006957.1.2",
          "form": "넵 다닙니다!",
          "original_form": "넵 다닙니다!",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006957.1.3",
          "form": "도어투도어 몇시간 걸리세요?",
          "original_form": "도어투도어 몇시간 걸리세요?",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006957.1.4",
          "form": "전 한시간 5분요",
          "original_form": "전 한시간 5분요",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006957.1.5",
          "form": "전 한 30분 정도...?",
          "original_form": "전 한 30분 정도...?",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006957.1.6",
          "form": "꽤 멀리 다니시네요 ㅠㅠ",
          "original_form": "꽤 멀리 다니시네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006957.1.7",
          "form": "우왕 좋으시겠다",
          "original_form": "우왕 좋으시겠다",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006957.1.8",
          "form": "네 이게 그나마 지금까지 중에선 가까운 편에 속해요",
          "original_form": "네 이게 그나마 지금까지 중에선 가까운 편에 속해요",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006957.1.9",
          "form": "헉 그게 가까우신 거라니 ㅠㅠ",
          "original_form": "헉 그게 가까우신 거라니 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006957.1.10",
          "form": "출퇴근은 대중교통 이용하시나요?",
          "original_form": "출퇴근은 대중교통 이용하시나요?",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006957.1.11",
          "form": "네 지하철요",
          "original_form": "네 지하철요",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006957.1.12",
          "form": "한번 갈아카요",
          "original_form": "한번 갈아카요",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006957.1.13",
          "form": "타요ㅋㅋ",
          "original_form": "타요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006957.1.14",
          "form": "저는 버스로 환승해서 가요... ㅠㅠ",
          "original_form": "저는 버스로 환승해서 가요... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006957.1.15",
          "form": "오오아 버스 타고 출근하는 거 제 로망이에요ㅋㅋ",
          "original_form": "오오아 버스 타고 출근하는 거 제 로망이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006957.1.16",
          "form": "울지 마세요",
          "original_form": "울지 마세요",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006957.1.17",
          "form": "버스가 로망이시구낰ㅋㅋㅋ",
          "original_form": "버스가 로망이시구낰ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006957.1.18",
          "form": "ㅋㅋㅋㅋㅋ 네 아무래도 버스는 가까운 느낌?",
          "original_form": "ㅋㅋㅋㅋㅋ 네 아무래도 버스는 가까운 느낌?",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006957.1.19",
          "form": "버스는 아무래도... 출퇴근시간에 사람들 많을 경우에 만원버스로 오면",
          "original_form": "버스는 아무래도... 출퇴근시간에 사람들 많을 경우에 만원버스로 오면",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006957.1.20",
          "form": "그냥 보내게 될 때가 있어서...",
          "original_form": "그냥 보내게 될 때가 있어서...",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006957.1.21",
          "form": "ㅇㅎㅇㅎ",
          "original_form": "ㅇㅎㅇㅎ",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006957.1.22",
          "form": "허얼 그건 앙대요ㅋㅋㅋㅋ",
          "original_form": "허얼 그건 앙대요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006957.1.23",
          "form": "쟈철은 한번 정도 보낸 적 있는 거 같애요ㅋㅋㅋ",
          "original_form": "쟈철은 한번 정도 보낸 적 있는 거 같애요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006957.1.24",
          "form": "사실 가장 좋은 건... 자차로 출퇴근 하는게 아닐까 하는 생각도 들어요...",
          "original_form": "사실 가장 좋은 건... 자차로 출퇴근 하는게 아닐까 하는 생각도 들어요...",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100006957.1.25",
          "form": "또르르",
          "original_form": "또르르",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100006957.1.26",
          "form": "아앜ㅋㅋㅋㅋㅋ 마자요",
          "original_form": "아앜ㅋㅋㅋㅋㅋ 마자요",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100006957.1.27",
          "form": "아 근데 저희 삼실 자차출근러는 넘 일찍 와요",
          "original_form": "아 근데 저희 삼실 자차출근러는 넘 일찍 와요",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006957.1.28",
          "form": "쫌만 늦으면 러시아워라고ㅋㅋㅋ",
          "original_form": "쫌만 늦으면 러시아워라고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006957.1.29",
          "form": "앗 러시아웤ㅋㅋㅋㅋㅋ 그건 그렇네요",
          "original_form": "앗 러시아웤ㅋㅋㅋㅋㅋ 그건 그렇네요",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006957.1.30",
          "form": "출퇴근길 너무 꽉꽉 막혀서 ㅠㅠㅋㅋㅋㅋ",
          "original_form": "출퇴근길 너무 꽉꽉 막혀서 ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006957.1.31",
          "form": "ㅋㅋㅋㅋㅋ큐ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006957.1.32",
          "form": "버스타고 오는데도 다른 차들때문에 매번 느려요...",
          "original_form": "버스타고 오는데도 다른 차들때문에 매번 느려요...",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006957.1.33",
          "form": "출근맘 숑숑 해도 회사 다니기 백배는 나아질듯",
          "original_form": "출근맘 숑숑 해도 회사 다니기 백배는 나아질듯",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006957.1.34",
          "form": "사실 출퇴근시간 아닐때에는 20분정도밖에 안되는 거리거든요ㅠ저는...",
          "original_form": "사실 출퇴근시간 아닐때에는 20분정도밖에 안되는 거리거든요ㅠ저는...",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006957.1.35",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006957.1.36",
          "form": "마자요 쟈철이.그 점에선 맘 편하더라구요",
          "original_form": "마자요 쟈철이.그 점에선 맘 편하더라구요",
          "speaker_id": "1",
          "time": "20210822 00:25"
        }
      ]
    }
