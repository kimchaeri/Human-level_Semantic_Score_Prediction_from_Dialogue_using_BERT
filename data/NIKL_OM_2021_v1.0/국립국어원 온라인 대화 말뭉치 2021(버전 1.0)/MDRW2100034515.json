      "utterance": [
        {
          "id": "MDRW2100034515.1.1",
          "form": "네 한번 먹어봐야겠네요!",
          "original_form": "네 한번 먹어봐야겠네요!",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100034515.1.2",
          "form": "강추해요",
          "original_form": "강추해요",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100034515.1.3",
          "form": "요샌 차돌박이짬뽕도 있더라구여",
          "original_form": "요샌 차돌박이짬뽕도 있더라구여",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100034515.1.4",
          "form": "마자요 차돌짬뽕도 맛있더라구요",
          "original_form": "마자요 차돌짬뽕도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100034515.1.5",
          "form": "친구네집가서 먹어봤는데",
          "original_form": "친구네집가서 먹어봤는데",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100034515.1.6",
          "form": "신세계였어요ㅋㄱㅋ",
          "original_form": "신세계였어요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100034515.1.7",
          "form": "맞아요 그거도 맛있어요 ㅋㅋㅋ",
          "original_form": "맞아요 그거도 맛있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034515.1.8",
          "form": "확실히 고기가 들어가면 국물이 좋아지는거같아요",
          "original_form": "확실히 고기가 들어가면 국물이 좋아지는거같아요",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034515.1.9",
          "form": "마자요 국물이 진짜 진하더라구요",
          "original_form": "마자요 국물이 진짜 진하더라구요",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100034515.1.10",
          "form": "원래 짬뽕 잘 안먹는데 그거는 자주먹게 되더라구요",
          "original_form": "원래 짬뽕 잘 안먹는데 그거는 자주먹게 되더라구요",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100034515.1.11",
          "form": "맞아요 밥말아먹고싶어요 진국이에요",
          "original_form": "맞아요 밥말아먹고싶어요 진국이에요",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100034515.1.12",
          "form": "최고죠 진짜!",
          "original_form": "최고죠 진짜!",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100034515.1.13",
          "form": "그져",
          "original_form": "그져",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100034515.1.14",
          "form": "연어회도 먹고싶네요ㅠ",
          "original_form": "연어회도 먹고싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100034515.1.15",
          "form": "전 연어초밥요ㅠㅠ",
          "original_form": "전 연어초밥요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100034515.1.16",
          "form": "그 위에올라간 양파랑 소스가 너무 좋아요",
          "original_form": "그 위에올라간 양파랑 소스가 너무 좋아요",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100034515.1.17",
          "form": "아 알죠알죠 \n양파랑 소스랑 같이먹으면 끝도없이 들어가더라구요ㅠ",
          "original_form": "아 알죠알죠 \n양파랑 소스랑 같이먹으면 끝도없이 들어가더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100034515.1.18",
          "form": "맞아요 ㅜ",
          "original_form": "맞아요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100034515.1.19",
          "form": "원래 연어 느끼했었는데 그건 ..최고에요",
          "original_form": "원래 연어 느끼했었는데 그건 ..최고에요",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100034515.1.20",
          "form": "마자요 최고죠ㅠ",
          "original_form": "마자요 최고죠ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100034515.1.21",
          "form": "침나오네여..",
          "original_form": "침나오네여..",
          "speaker_id": "1",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100034515.1.22",
          "form": "오늘 저녁 후보가 많아졌네요",
          "original_form": "오늘 저녁 후보가 많아졌네요",
          "speaker_id": "2",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100034515.1.23",
          "form": "먹을게 넘 많죠 ㅠㅜ",
          "original_form": "먹을게 넘 많죠 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100034515.1.24",
          "form": "요새 진짜 로제떡볶이에 꽂혀가지구",
          "original_form": "요새 진짜 로제떡볶이에 꽂혀가지구",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100034515.1.25",
          "form": "엄청 먹었어요",
          "original_form": "엄청 먹었어요",
          "speaker_id": "2",
          "time": "20210828 14:28"
        }
      ]
    }
