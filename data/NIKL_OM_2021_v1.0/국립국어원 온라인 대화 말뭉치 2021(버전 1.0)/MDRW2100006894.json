      "utterance": [
        {
          "id": "MDRW2100006894.1.1",
          "form": "비타민 먹어야 되는데",
          "original_form": "비타민 먹어야 되는데",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006894.1.2",
          "form": "오 샀어?",
          "original_form": "오 샀어?",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006894.1.3",
          "form": "종합비타민으로 사지",
          "original_form": "종합비타민으로 사지",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100006894.1.4",
          "form": "아니 사지도 않았어 ㅋ",
          "original_form": "아니 사지도 않았어 ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100006894.1.5",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100006894.1.6",
          "form": "종합비타민으로 하나 쟁여",
          "original_form": "종합비타민으로 하나 쟁여",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100006894.1.7",
          "form": "60알에 3~5만원 하더라",
          "original_form": "60알에 3~5만원 하더라",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100006894.1.8",
          "form": "덜덜덜",
          "original_form": "덜덜덜",
          "speaker_id": "2",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100006894.1.9",
          "form": "그래도 확실히 생기가 생김",
          "original_form": "그래도 확실히 생기가 생김",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006894.1.10",
          "form": "먹는 거랑 안 먹는 거랑 차이가 커",
          "original_form": "먹는 거랑 안 먹는 거랑 차이가 커",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006894.1.11",
          "form": "그럴 것 같아",
          "original_form": "그럴 것 같아",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006894.1.12",
          "form": "스리고 밥 자체를",
          "original_form": "스리고 밥 자체를",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006894.1.13",
          "form": "제대로 안 챙겨먹어서 ㅋ",
          "original_form": "제대로 안 챙겨먹어서 ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006894.1.14",
          "form": "먹어야 될 거 같음",
          "original_form": "먹어야 될 거 같음",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006894.1.15",
          "form": "엉 2끼는 챙겨 먹고",
          "original_form": "엉 2끼는 챙겨 먹고",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006894.1.16",
          "form": "운동하면 꼭 챙겨 먹어",
          "original_form": "운동하면 꼭 챙겨 먹어",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006894.1.17",
          "form": "그래야지...",
          "original_form": "그래야지...",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006894.1.18",
          "form": "긔찬타",
          "original_form": "긔찬타",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006894.1.19",
          "form": "그래야 근육이 붙는대",
          "original_form": "그래야 근육이 붙는대",
          "speaker_id": "1",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006894.1.20",
          "form": "영양소 잘 챙겨 먹고",
          "original_form": "영양소 잘 챙겨 먹고",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006894.1.21",
          "form": "허걱",
          "original_form": "허걱",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006894.1.22",
          "form": "파이팅...",
          "original_form": "파이팅...",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006894.1.23",
          "form": "건강맨 되자",
          "original_form": "건강맨 되자",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
