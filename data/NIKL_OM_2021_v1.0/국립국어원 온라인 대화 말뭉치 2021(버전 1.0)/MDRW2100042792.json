          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100042792.1.24",
          "form": "거의 애 하나 키우는 거에요",
          "original_form": "거의 애 하나 키우는 거에요",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100042792.1.25",
          "form": "예방접종도 해야하고",
          "original_form": "예방접종도 해야하고",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100042792.1.26",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100042792.1.27",
          "form": "대소변 치워줘야하고",
          "original_form": "대소변 치워줘야하고",
          "speaker_id": "1",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100042792.1.28",
          "form": "밥 주고 놀아주고",
          "original_form": "밥 주고 놀아주고",
