      "utterance": [
        {
          "id": "MDRW2100042792.1.1",
          "form": "아뇨 반려동물은 키워본 적이 없어요",
          "original_form": "아뇨 반려동물은 키워본 적이 없어요",
          "speaker_id": "2",
          "time": "20210907 10:32"
        },
        {
          "id": "MDRW2100042792.1.2",
          "form": "생명은 한번도 키워보지 않으셨던거군요",
          "original_form": "생명은 한번도 키워보지 않으셨던거군요",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100042792.1.3",
          "form": "사실 살아있는 무언가를 키운다는 건 큰 책임이 따르는 일이라",
          "original_form": "사실 살아있는 무언가를 키운다는 건 큰 책임이 따르는 일이라",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100042792.1.4",
          "form": "네 반려동물은요",
          "original_form": "네 반려동물은요",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100042792.1.5",
          "form": "쉽게 결정할 수 없지요",
          "original_form": "쉽게 결정할 수 없지요",
          "speaker_id": "1",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100042792.1.6",
          "form": "식물은 좀 키웠는데",
          "original_form": "식물은 좀 키웠는데",
          "speaker_id": "2",
          "time": "20210907 13:32"
        },
        {
          "id": "MDRW2100042792.1.7",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100042792.1.8",
          "form": "네 가족들이 다 직장/학교생활을 하고 있고",
          "original_form": "네 가족들이 다 직장/학교생활을 하고 있고",
          "speaker_id": "2",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100042792.1.9",
          "form": "반려동물이 혼자 있는 시간이 길어질 듯해서",
          "original_form": "반려동물이 혼자 있는 시간이 길어질 듯해서",
          "speaker_id": "2",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100042792.1.10",
          "form": "선뜻 키우지 못하겠더라고요",
          "original_form": "선뜻 키우지 못하겠더라고요",
          "speaker_id": "2",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100042792.1.11",
          "form": "맞아요 좋은 결정이에요",
          "original_form": "맞아요 좋은 결정이에요",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100042792.1.12",
          "form": "전 제가 주로 집에 있으니까 돌보는 일을 제가 거의 했어요",
          "original_form": "전 제가 주로 집에 있으니까 돌보는 일을 제가 거의 했어요",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100042792.1.13",
          "form": "딸이 본인이 다 케어하겠다고 허락해달라고 하더니만",
          "original_form": "딸이 본인이 다 케어하겠다고 허락해달라고 하더니만",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100042792.1.14",
          "form": "결국 제 몫이 되었죠",
          "original_form": "결국 제 몫이 되었죠",
          "speaker_id": "1",
          "time": "20210907 13:45"
        },
        {
          "id": "MDRW2100042792.1.15",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100042792.1.16",
          "form": "반려동물 키우셔요?",
          "original_form": "반려동물 키우셔요?",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100042792.1.17",
          "form": "지금은 아니고요 예전에 강아지 키웠어요",
          "original_form": "지금은 아니고요 예전에 강아지 키웠어요",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100042792.1.18",
          "form": "말티즈였죠",
          "original_form": "말티즈였죠",
          "speaker_id": "1",
          "time": "20210907 13:48"
        },
        {
          "id": "MDRW2100042792.1.19",
          "form": "아 말티즈 키우셨군요",
          "original_form": "아 말티즈 키우셨군요",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100042792.1.20",
          "form": "네 이름은 name3였어요",
          "original_form": "네 이름은 &name3&였어요",
          "speaker_id": "1",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100042792.1.21",
          "form": "강아지 키우는 것도 시간과 애정이 많이 필요한 것 같아요",
          "original_form": "강아지 키우는 것도 시간과 애정이 많이 필요한 것 같아요",
          "speaker_id": "2",
          "time": "20210907 13:49"
        },
        {
          "id": "MDRW2100042792.1.22",
          "form": "name3에서 따왔죠",
          "original_form": "&name3&에서 따왔죠",
