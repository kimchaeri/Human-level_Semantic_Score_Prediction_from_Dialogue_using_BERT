      "utterance": [
        {
          "id": "MDRW2100020183.1.1",
          "form": "집안일은 해도해도 끝이없네요ㅠㅠ",
          "original_form": "집안일은 해도해도 끝이없네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020183.1.2",
          "form": "저도 좀 거들어야하는데 제방치우기도 급급해요...",
          "original_form": "저도 좀 거들어야하는데 제방치우기도 급급해요...",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020183.1.3",
          "form": "ㅋㅋㅋ전 시작도못하겠더라구요",
          "original_form": "ㅋㅋㅋ전 시작도못하겠더라구요",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020183.1.4",
          "form": "주말엔 청소좀해야지..",
          "original_form": "주말엔 청소좀해야지..",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020183.1.5",
          "form": "집안일 가족들이랑 같이하시나요?",
          "original_form": "집안일 가족들이랑 같이하시나요?",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020183.1.6",
          "form": "보통은 남편이 거의다해요...ㅋㅋ머쓱",
          "original_form": "보통은 남편이 거의다해요...ㅋㅋ머쓱",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020183.1.7",
          "form": "전 아직 미혼에 부모님과 같이사는데 그냥 좀 돈으로 떼웁니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 아직 미혼에 부모님과 같이사는데 그냥 좀 돈으로 떼웁니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020183.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 가끔 청소연구소 이용하는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도 가끔 청소연구소 이용하는데",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020183.1.9",
          "form": "진짜 꿀이에요",
          "original_form": "진짜 꿀이에요",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020183.1.10",
          "form": "자주는못부르고",
          "original_form": "자주는못부르고",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020183.1.11",
          "form": "가끔이용하는데...",
          "original_form": "가끔이용하는데...",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020183.1.12",
          "form": "어떤 가사일이 제일 싫으세요? ㅋㅋ",
          "original_form": "어떤 가사일이 제일 싫으세요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020183.1.13",
          "form": "저는 빨래돌리고",
          "original_form": "저는 빨래돌리고",
          "speaker_id": "1",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100020183.1.14",
          "form": "그 정리하는 집안일이요 ㅋㅋㅋㅋ",
          "original_form": "그 정리하는 집안일이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100020183.1.15",
          "form": "저도 빨래 제일싫어요 ㅜㅜ",
          "original_form": "저도 빨래 제일싫어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020183.1.16",
          "form": "개는게 너무 귀찮아요..",
          "original_form": "개는게 너무 귀찮아요..",
          "speaker_id": "2",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020183.1.17",
          "form": "마자요 빨래개고",
          "original_form": "마자요 빨래개고",
          "speaker_id": "1",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100020183.1.18",
          "form": "그 정리하는거...",
          "original_form": "그 정리하는거...",
          "speaker_id": "1",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100020183.1.19",
          "form": "가져다두는거 너무귀찮고",
          "original_form": "가져다두는거 너무귀찮고",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020183.1.20",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020183.1.21",
          "form": "식세기 쓰세요?",
          "original_form": "식세기 쓰세요?",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020183.1.22",
          "form": "당연하죠!!! 필수템이에요 ㅋㅋ",
          "original_form": "당연하죠!!! 필수템이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020183.1.23",
          "form": "옷 개주는.기계도 나왔으면...",
          "original_form": "옷 개주는.기계도 나왔으면...",
          "speaker_id": "1",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020183.1.24",
          "form": "저희집은 식세기 아직 안사서 궁금해요!",
          "original_form": "저희집은 식세기 아직 안사서 궁금해요!",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020183.1.25",
          "form": "헛 당장주문하셔요!!!",
          "original_form": "헛 당장주문하셔요!!!",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020183.1.26",
          "form": "강추입니다 ㅋㅋㅋ",
          "original_form": "강추입니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020183.1.27",
          "form": "없던 삶으로 돌아갈수 없어요",
          "original_form": "없던 삶으로 돌아갈수 없어요",
          "speaker_id": "1",
          "time": "20210828 00:22"
        }
      ]
    }
  ]
