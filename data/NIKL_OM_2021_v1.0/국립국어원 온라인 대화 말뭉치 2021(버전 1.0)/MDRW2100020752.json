        {
          "id": "MDRW2100020752.1.1",
          "form": "평소에 운동해요?",
          "original_form": "평소에 운동해요?",
          "speaker_id": "2",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020752.1.2",
          "form": "홈트 간간이 해요~",
          "original_form": "홈트 간간이 해요~",
          "speaker_id": "1",
          "time": "20210828 12:08"
        },
        {
          "id": "MDRW2100020752.1.3",
          "form": "홈트 어떤거 해요?",
          "original_form": "홈트 어떤거 해요?",
          "speaker_id": "2",
          "time": "20210828 12:09"
        },
        {
          "id": "MDRW2100020752.1.4",
          "form": "맨몸이나 가벼운 덤벨들고 하는거 해요",
          "original_form": "맨몸이나 가벼운 덤벨들고 하는거 해요",
          "speaker_id": "1",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100020752.1.5",
          "form": "유투브보면서ㅎㅎ",
          "original_form": "유투브보면서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100020752.1.6",
          "form": "우와 집에 덤벨도 있어요?",
          "original_form": "우와 집에 덤벨도 있어요?",
          "speaker_id": "2",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100020752.1.7",
          "form": "몇킬로짜리에요?",
          "original_form": "몇킬로짜리에요?",
          "speaker_id": "2",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100020752.1.8",
          "form": "엄청가벼운거만 있어요ㅋㅋ",
          "original_form": "엄청가벼운거만 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100020752.1.9",
          "form": "1키로에서 5키로 무게별로 있어용",
          "original_form": "1키로에서 5키로 무게별로 있어용",
          "speaker_id": "1",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100020752.1.10",
          "form": "팔운동 주로해요",
          "original_form": "팔운동 주로해요",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020752.1.11",
          "form": "와 팔근육 있어요?",
          "original_form": "와 팔근육 있어요?",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020752.1.12",
          "form": "정말 미세하게요..ㅋㅋㅋ",
          "original_form": "정말 미세하게요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020752.1.13",
          "form": "잘 안붙더라구요 가볍게만하는편이라",
          "original_form": "잘 안붙더라구요 가볍게만하는편이라",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020752.1.14",
          "form": "한번 해보세요 name2님도",
          "original_form": "한번 해보세요 &name2&님도",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020752.1.15",
          "form": "저도 요새 팔근육 관심 생겨가지구 ㅎㅎ",
          "original_form": "저도 요새 팔근육 관심 생겨가지구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020752.1.16",
          "form": "제가 팔에 체지방이 400g밖에 없거든요",
          "original_form": "제가 팔에 체지방이 400g밖에 없거든요",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020752.1.17",
          "form": "저도 거의 없다시피ㅠ",
          "original_form": "저도 거의 없다시피ㅠ",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020752.1.18",
          "form": "그래도 하시면 늘거에요!",
          "original_form": "그래도 하시면 늘거에요!",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020752.1.19",
          "form": "집에 덤벨 같은 거 없는데 어떡하죠?",
          "original_form": "집에 덤벨 같은 거 없는데 어떡하죠?",
          "speaker_id": "2",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020752.1.20",
          "form": "기구 없이 할 수 있는 운동은 없어요?",
          "original_form": "기구 없이 할 수 있는 운동은 없어요?",
          "speaker_id": "2",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020752.1.21",
          "form": "팔운동은 거의 가벼운 아령쓰더라고요",
          "original_form": "팔운동은 거의 가벼운 아령쓰더라고요",
          "speaker_id": "1",
          "time": "20210828 12:19"
        },
        {
          "id": "MDRW2100020752.1.22",
          "form": "체중이용해서 하는방법은 있을거에요",
          "original_form": "체중이용해서 하는방법은 있을거에요",
          "speaker_id": "1",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020752.1.23",
          "form": "인터넷에보면",
          "original_form": "인터넷에보면",
          "speaker_id": "1",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020752.1.24",
          "form": "아 찾아보기도 귀찮네요 벌써 ㅜㅜ",
          "original_form": "아 찾아보기도 귀찮네요 벌써 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 12:21"
        },
        {
          "id": "MDRW2100020752.1.25",
          "form": "그냥 트레이너가 매주 한번씩 와서 운동 시켜줬음 좋겠어요",
          "original_form": "그냥 트레이너가 매주 한번씩 와서 운동 시켜줬음 좋겠어요",
