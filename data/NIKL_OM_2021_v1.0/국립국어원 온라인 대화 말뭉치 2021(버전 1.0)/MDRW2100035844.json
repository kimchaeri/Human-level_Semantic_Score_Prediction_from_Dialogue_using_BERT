      "utterance": [
        {
          "id": "MDRW2100035844.1.1",
          "form": "김병태 강의도 엄청 추천하더라 국어!",
          "original_form": "김병태 강의도 엄청 추천하더라 국어!",
          "speaker_id": "1",
          "time": "20210830 08:38"
        },
        {
          "id": "MDRW2100035844.1.2",
          "form": "그사람은 또누구래 흠",
          "original_form": "그사람은 또누구래 흠",
          "speaker_id": "2",
          "time": "20210830 08:38"
        },
        {
          "id": "MDRW2100035844.1.3",
          "form": "머리 하얗고 할아버지 ㅋㅋㅋㅋ",
          "original_form": "머리 하얗고 할아버지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 08:38"
        },
        {
          "id": "MDRW2100035844.1.4",
          "form": "선생님 죄송함다",
          "original_form": "선생님 죄송함다",
          "speaker_id": "1",
          "time": "20210830 08:39"
        },
        {
          "id": "MDRW2100035844.1.5",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "2",
          "time": "20210830 08:39"
        },
        {
          "id": "MDRW2100035844.1.6",
          "form": "나이거있을꺼같운 이름이엿우",
          "original_form": "나이거있을꺼같운 이름이엿우",
          "speaker_id": "2",
          "time": "20210830 08:39"
        },
        {
          "id": "MDRW2100035844.1.7",
          "form": "엄청 호통치고 소리지르고 한대",
          "original_form": "엄청 호통치고 소리지르고 한대",
          "speaker_id": "1",
          "time": "20210830 08:39"
        },
        {
          "id": "MDRW2100035844.1.8",
          "form": "이 외계인아~~~~",
          "original_form": "이 외계인아~~~~",
          "speaker_id": "1",
          "time": "20210830 08:39"
        },
        {
          "id": "MDRW2100035844.1.9",
          "form": "첨엔 띠용하는데 듣다보면 웃겨서 매력에 빠지게된다는데?ㅋㅋㅋ",
          "original_form": "첨엔 띠용하는데 듣다보면 웃겨서 매력에 빠지게된다는데?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 08:40"
        },
        {
          "id": "MDRW2100035844.1.10",
          "form": "과연",
          "original_form": "과연",
          "speaker_id": "2",
          "time": "20210830 08:40"
        },
        {
          "id": "MDRW2100035844.1.11",
          "form": "빠질까유..?",
          "original_form": "빠질까유..?",
          "speaker_id": "2",
          "time": "20210830 08:40"
        },
        {
          "id": "MDRW2100035844.1.12",
          "form": "ㅋㅋㅋ점수도 잘나오겠쥬?",
          "original_form": "ㅋㅋㅋ점수도 잘나오겠쥬?",
          "speaker_id": "1",
          "time": "20210830 08:40"
        },
        {
          "id": "MDRW2100035844.1.13",
          "form": "넌 국어는 괜찮을꺼같운데!?",
          "original_form": "넌 국어는 괜찮을꺼같운데!?",
          "speaker_id": "2",
          "time": "20210830 08:40"
        },
        {
          "id": "MDRW2100035844.1.14",
          "form": "난 영어든 국어든 문법다틀려ㅡㅡ",
          "original_form": "난 영어든 국어든 문법다틀려ㅡㅡ",
          "speaker_id": "2",
          "time": "20210830 08:41"
        },
        {
          "id": "MDRW2100035844.1.15",
          "form": "아냐 잘찍은거지!! 국어 칠때만 할머니 왔었나보다 ㅋㅋㅋ",
          "original_form": "아냐 잘찍은거지!! 국어 칠때만 할머니 왔었나보다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 08:41"
        },
        {
          "id": "MDRW2100035844.1.16",
          "form": "아진짜 영문법 어쩐대 ㅋㅋㅋ진짜 하기도 싫다",
          "original_form": "아진짜 영문법 어쩐대 ㅋㅋㅋ진짜 하기도 싫다",
          "speaker_id": "1",
          "time": "20210830 08:42"
        },
        {
          "id": "MDRW2100035844.1.17",
          "form": "샐러드 지금릴레이하는거 줄서서 내차례와서 가입햇당 ㅋㅋㅋㅋ",
          "original_form": "샐러드 지금릴레이하는거 줄서서 내차례와서 가입햇당 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100035844.1.18",
          "form": "진짜아는거같긴한데...적용안되ㅜ",
          "original_form": "진짜아는거같긴한데...적용안되ㅜ",
