      "utterance": [
        {
          "id": "MDRW2100018197.1.1",
          "form": "그러게여 ㅋㅋㅋㅋㅋ 정말 현 편입준비생은 웁니다.. ㅜㅜ",
          "original_form": "그러게여 ㅋㅋㅋㅋㅋ 정말 현 편입준비생은 웁니다.. ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018197.1.2",
          "form": "무조건 학사부터 따셔요",
          "original_form": "무조건 학사부터 따셔요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018197.1.3",
          "form": "맞아요 영어만 하면 그만인데 그게.이렇게나 힘들다니...",
          "original_form": "맞아요 영어만 하면 그만인데 그게.이렇게나 힘들다니...",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018197.1.4",
          "form": "학사따면 성신여대 이런곳은 우리땐 영어시험없이 면접 100프로였는데",
          "original_form": "학사따면 성신여대 이런곳은 우리땐 영어시험없이 면접 100프로였는데",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018197.1.5",
          "form": "근데 학사따면거 영어 공부 병행하기가 어렵지 않아ㅛ어요?",
          "original_form": "근데 학사따면거 영어 공부 병행하기가 어렵지 않아ㅛ어요?",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018197.1.6",
          "form": "학사 그거 동영상강의.틀어놓고 안보기....",
          "original_form": "학사 그거 동영상강의.틀어놓고 안보기....",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018197.1.7",
          "form": "지금은 코로나 때문에 면접이 아예 없고",
          "original_form": "지금은 코로나 때문에 면접이 아예 없고",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018197.1.8",
          "form": "시험도 오픈북",
          "original_form": "시험도 오픈북",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018197.1.9",
          "form": "다 시험 100이거나 자소서 보거나 그래요",
          "original_form": "다 시험 100이거나 자소서 보거나 그래요",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018197.1.10",
          "form": "자소서 왠말이야진짜...ㅠ",
          "original_form": "자소서 왠말이야진짜...ㅠ",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018197.1.11",
          "form": "오 그거 따는데 얼마나 걸려요?",
          "original_form": "오 그거 따는데 얼마나 걸려요?",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018197.1.12",
          "form": "차라리 시험 100이 낫겟네요",
          "original_form": "차라리 시험 100이 낫겟네요",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018197.1.13",
          "form": "전 학사는 아예 생각도 안해봐서",
          "original_form": "전 학사는 아예 생각도 안해봐서",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018197.1.14",
          "form": "저는 2학기 걸렸어요",
          "original_form": "저는 2학기 걸렸어요",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018197.1.15",
          "form": "1년",
          "original_form": "1년",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018197.1.16",
          "form": "편입재수하면서",
          "original_form": "편입재수하면서",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018197.1.17",
          "form": "그기간에 학사병행",
          "original_form": "그기간에 학사병행",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018197.1.18",
          "form": "아 그럼 저는.. 그냥 일반으로 가야겠어요.. ㅜㅜ",
          "original_form": "아 그럼 저는.. 그냥 일반으로 가야겠어요.. ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018197.1.19",
          "form": "ㅠㅠ근데진짜 달라요...저 일반땨",
          "original_form": "ㅠㅠ근데진짜 달라요...저 일반땨",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018197.1.20",
          "form": "원서 겁나게썻는데",
          "original_form": "원서 겁나게썻는데",
