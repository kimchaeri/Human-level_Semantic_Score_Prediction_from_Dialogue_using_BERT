      "utterance": [
        {
          "id": "MDRW2100016591.1.1",
          "form": "네 그거 분명히느꼈어요",
          "original_form": "네 그거 분명히느꼈어요",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016591.1.2",
          "form": "수영 오래한 사람들은 몸이 홀쭉해요",
          "original_form": "수영 오래한 사람들은 몸이 홀쭉해요",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016591.1.3",
          "form": "아 그렇군요 전시회나박물관 같은건 좋아하세요?",
          "original_form": "아 그렇군요 전시회나박물관 같은건 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016591.1.4",
          "form": "그냥 남들가는만큼 좋아해요",
          "original_form": "그냥 남들가는만큼 좋아해요",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016591.1.5",
          "form": "미술 전시회같은 거 관심있으신가요?",
          "original_form": "미술 전시회같은 거 관심있으신가요?",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016591.1.6",
          "form": "아뇨 그닥이요 ㅠㅠ 미술전시회 좋아하세요?",
          "original_form": "아뇨 그닥이요 ㅠㅠ 미술전시회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100016591.1.7",
          "form": "뮤지컬은 좋아하세요?",
          "original_form": "뮤지컬은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016591.1.8",
          "form": "저는 유명한 거 있으면 가끔 가는편이에요",
          "original_form": "저는 유명한 거 있으면 가끔 가는편이에요",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016591.1.9",
          "form": "어떤작가 좋아하세요?",
          "original_form": "어떤작가 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016591.1.10",
          "form": "화풍이나요",
          "original_form": "화풍이나요",
          "speaker_id": "1",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016591.1.11",
          "form": "아 저는 서양미술사에 관심이 많아서 전반적인 미술사 흐름같은거 보곤해요",
          "original_form": "아 저는 서양미술사에 관심이 많아서 전반적인 미술사 흐름같은거 보곤해요",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016591.1.12",
          "form": "우와 네 부러워요..ㅎㅎ",
          "original_form": "우와 네 부러워요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016591.1.13",
          "form": "저는 미술쪽은 영...",
          "original_form": "저는 미술쪽은 영...",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016591.1.14",
          "form": "상업디자인 쪽만 관심조금 있는정도에요",
          "original_form": "상업디자인 쪽만 관심조금 있는정도에요",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016591.1.15",
          "form": "와 혹시 관련 계열이세요?",
          "original_form": "와 혹시 관련 계열이세요?",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016591.1.16",
          "form": "아뇨 유튜브하는데요",
          "original_form": "아뇨 유튜브하는데요",
          "speaker_id": "1",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016591.1.17",
          "form": "제가 썸네일 만들다보니 디자인에 관심가서요",
          "original_form": "제가 썸네일 만들다보니 디자인에 관심가서요",
          "speaker_id": "1",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016591.1.18",
          "form": "아 유툽 썸네일이요?",
          "original_form": "아 유툽 썸네일이요?",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016591.1.19",
          "form": "네 썸네일 잘만들어야 클릭하니까",
          "original_form": "네 썸네일 잘만들어야 클릭하니까",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
