      "utterance": [
        {
          "id": "MDRW2100028256.1.1",
          "form": "코시국 전에 갔다온 여행지 있으세요??",
          "original_form": "코시국 전에 갔다온 여행지 있으세요??",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028256.1.2",
          "form": "저는 부산이야ㅡ!",
          "original_form": "저는 부산이야ㅡ!",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028256.1.3",
          "form": "부산이요!",
          "original_form": "부산이요!",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028256.1.4",
          "form": "오 저두요!! 작년이지만ㅠㅠ",
          "original_form": "오 저두요!! 작년이지만ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028256.1.5",
          "form": "당일치기로 갔었어요!",
          "original_form": "당일치기로 갔었어요!",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028256.1.6",
          "form": "오 어디 가보셨어요??",
          "original_form": "오 어디 가보셨어요??",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100028256.1.7",
          "form": "저 그때 핸드메이드&일러스트페어 친구랑 간거라",
          "original_form": "저 그때 핸드메이드&일러스트페어 친구랑 간거라",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028256.1.8",
          "form": "여기저기는 안갔어요",
          "original_form": "여기저기는 안갔어요",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028256.1.9",
          "form": "구경하구 광안리가서 펭수랑 사진찍고ㅎㅎ",
          "original_form": "구경하구 광안리가서 펭수랑 사진찍고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028256.1.10",
          "form": "오 광안리 다리가 예쁘죠",
          "original_form": "오 광안리 다리가 예쁘죠",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028256.1.11",
          "form": "광안리 야경으로 봤어야했는데",
          "original_form": "광안리 야경으로 봤어야했는데",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028256.1.12",
          "form": "당일치기라 돌아가야되는 시간땜에",
          "original_form": "당일치기라 돌아가야되는 시간땜에",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028256.1.13",
          "form": "제대로 못본게 좀 아쉬워요",
          "original_form": "제대로 못본게 좀 아쉬워요",
          "speaker_id": "1",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028256.1.14",
          "form": "부산이 당일치기로는 가기 너무 커서 참 슬퍼요ㅠㅠ",
          "original_form": "부산이 당일치기로는 가기 너무 커서 참 슬퍼요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028256.1.15",
          "form": "맞아요ㅜ 1박 하고 여기저기 가보싶은뎅",
          "original_form": "맞아요ㅜ 1박 하고 여기저기 가보싶은뎅",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100028256.1.16",
          "form": "부산은 당일치기 많이하는거 같아요ㅋㅋ",
          "original_form": "부산은 당일치기 많이하는거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100028256.1.17",
          "form": "name2님은 부산 어디 놀러갔어요??",
          "original_form": "&name2&님은 부산 어디 놀러갔어요??",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100028256.1.18",
          "form": "저도 광안리랑 해운대요!",
          "original_form": "저도 광안리랑 해운대요!",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100028256.1.19",
          "form": "전 바다가 좋더라고요",
          "original_form": "전 바다가 좋더라고요",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100028256.1.20",
          "form": "옹 역시 부산하면 바다죵!!",
          "original_form": "옹 역시 부산하면 바다죵!!",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100028256.1.21",
          "form": "바다 구경하는건 좋은데 젖는거 싫어해서",
          "original_form": "바다 구경하는건 좋은데 젖는거 싫어해서",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100028256.1.22",
          "form": "물에 젖는거 싫어해서 해변가 조심히 걸어요ㅋㅋㅋ",
          "original_form": "물에 젖는거 싫어해서 해변가 조심히 걸어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100028256.1.23",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100028256.1.24",
          "form": "보는건 예쁘지만 젖으면 그뒤에 너무힘들어서ㅋㅋㄱ",
          "original_form": "보는건 예쁘지만 젖으면 그뒤에 너무힘들어서ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100028256.1.25",
          "form": "물가에 가깝게 걷지만 최대한 조심히 걸어요ㅋㅋㅋ",
          "original_form": "물가에 가깝게 걷지만 최대한 조심히 걸어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100028256.1.26",
          "form": "ㅋㅋㅋ그죠 어릴때는 바다 들어가서 신나게 놀고했는데",
          "original_form": "ㅋㅋㅋ그죠 어릴때는 바다 들어가서 신나게 놀고했는데",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100028256.1.27",
          "form": "지금은 놀고나서의 뒤처리가 귀찮아서",
          "original_form": "지금은 놀고나서의 뒤처리가 귀찮아서",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100028256.1.28",
          "form": "코시국 전에 부산 해운대에서 불꽃놀이 봤던게 기억나네요",
          "original_form": "코시국 전에 부산 해운대에서 불꽃놀이 봤던게 기억나네요",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100028256.1.29",
          "form": "흑흑 그립다",
          "original_form": "흑흑 그립다",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100028256.1.30",
          "form": "밤에는 바닷가에 가본적이 없어서 그러는데",
          "original_form": "밤에는 바닷가에 가본적이 없어서 그러는데",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100028256.1.31",
          "form": "밤에 바닷가 가면 바다 잘 보여요??",
          "original_form": "밤에 바닷가 가면 바다 잘 보여요??",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100028256.1.32",
          "form": "으음 불빛도 있어서 완전 껌껌하지는 않지만",
          "original_form": "으음 불빛도 있어서 완전 껌껌하지는 않지만",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100028256.1.33",
          "form": "파도치는거는 보여요!",
          "original_form": "파도치는거는 보여요!",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100028256.1.34",
          "form": "어느정도는 바다 좀 보이는거 같아요",
          "original_form": "어느정도는 바다 좀 보이는거 같아요",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100028256.1.35",
          "form": "오 그렇군요?! 다음엔 밤에도 가봐야겠어요",
          "original_form": "오 그렇군요?! 다음엔 밤에도 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100028256.1.36",
          "form": "바다도 물멍이 너무 좋더라고요",
          "original_form": "바다도 물멍이 너무 좋더라고요",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100028256.1.37",
          "form": "담에 가보세요 밤바다만의 매력도 있어요~ㅎㅎ",
          "original_form": "담에 가보세요 밤바다만의 매력도 있어요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100028256.1.38",
          "form": "물멍이 물보면서 멍때리기 인가요???",
          "original_form": "물멍이 물보면서 멍때리기 인가요???",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100028256.1.39",
          "form": "첨 들어봤어요ㅋㅋㅋㅋㅋ",
          "original_form": "첨 들어봤어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100028256.1.40",
          "form": "네ㅋㅋㅋㅋ저는 불멍보단 물멍이더라고요",
          "original_form": "네ㅋㅋㅋㅋ저는 불멍보단 물멍이더라고요",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100028256.1.41",
          "form": "오ㅋㅋㅋ 둘다의 매력이 있는거같아오!!",
          "original_form": "오ㅋㅋㅋ 둘다의 매력이 있는거같아오!!",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100028256.1.42",
          "form": "파도소리 들으면서 물멍 진짜 좋은거같아요",
          "original_form": "파도소리 들으면서 물멍 진짜 좋은거같아요",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100028256.1.43",
          "form": "다만 멍때리다가 피부가 탈수 있다는거...?ㅋㅋㅋㅋ",
          "original_form": "다만 멍때리다가 피부가 탈수 있다는거...?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100028256.1.44",
          "form": "앜ㅋㅋㅋㅋㅋㅋ선크림 필수필수~~!",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ선크림 필수필수~~!",
