      "utterance": [
        {
          "id": "MDRW2100014323.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014323.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014323.1.3",
          "form": "봉사단체나 동호회 가입한적 있으신가요??",
          "original_form": "봉사단체나 동호회 가입한적 있으신가요??",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014323.1.4",
          "form": "아뇨ㅠㅠ그러고 싶은데 시간이ㅎㅎ 동호회 활동하세요?",
          "original_form": "아뇨ㅠㅠ그러고 싶은데 시간이ㅎㅎ 동호회 활동하세요?",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014323.1.5",
          "form": "저는 예전에 아빠따라 배드민턴동호회 가입한적있었어요~",
          "original_form": "저는 예전에 아빠따라 배드민턴동호회 가입한적있었어요~",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014323.1.6",
          "form": "오 재밌을거같아요ㅎㅎ 저도 코로나끝나면 운동 동호회들고싶어요ㅋㅋ",
          "original_form": "오 재밌을거같아요ㅎㅎ 저도 코로나끝나면 운동 동호회들고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014323.1.7",
          "form": "아 그쵸ㅠㅠ 코로나때문에 밖에 안나간지도 오래된것같아요",
          "original_form": "아 그쵸ㅠㅠ 코로나때문에 밖에 안나간지도 오래된것같아요",
          "speaker_id": "1",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014323.1.8",
          "form": "모임을 할수가 없네요ㅠㅠ",
          "original_form": "모임을 할수가 없네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014323.1.9",
          "form": "정말 코로나때문에 활동할 기회가 많이 줄어서 아쉬워요 ㅠ",
          "original_form": "정말 코로나때문에 활동할 기회가 많이 줄어서 아쉬워요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014323.1.10",
          "form": "네 독서모임같은것고 해보구 싶은데ㅠㅠ이젠 다 랜선이더라구요",
          "original_form": "네 독서모임같은것고 해보구 싶은데ㅠㅠ이젠 다 랜선이더라구요",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014323.1.11",
          "form": "오 독서모임 재밌겠어요!",
          "original_form": "오 독서모임 재밌겠어요!",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014323.1.12",
          "form": "전 코로나끝나면 자전거동호회 알아볼까해요 ㅎㅎ",
          "original_form": "전 코로나끝나면 자전거동호회 알아볼까해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014323.1.13",
          "form": "오 좋아보여요ㅋㅋ 따로 안배워도 되구ㅎㅎ",
          "original_form": "오 좋아보여요ㅋㅋ 따로 안배워도 되구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014323.1.14",
          "form": "맞아요! 그게 장점인거같아요",
          "original_form": "맞아요! 그게 장점인거같아요",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014323.1.15",
          "form": "근데 내성적이라 시작이 반이상이에요 ㅜ",
          "original_form": "근데 내성적이라 시작이 반이상이에요 ㅜ",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014323.1.16",
          "form": "저도 그래요ㅠㅠㅠ그래서 용기 내보고싶어요ㅋㅋ",
          "original_form": "저도 그래요ㅠㅠㅠ그래서 용기 내보고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:31"
        }
      ]
