      "utterance": [
        {
          "id": "MDRW2100044525.1.1",
          "form": "야채가 들어가나요?",
          "original_form": "야채가 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044525.1.2",
          "form": "국물이 있는 메뉴입니다!",
          "original_form": "국물이 있는 메뉴입니다!",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044525.1.3",
          "form": "야채도 들어가요",
          "original_form": "야채도 들어가요",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044525.1.4",
          "form": "김치찌개인가요?",
          "original_form": "김치찌개인가요?",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044525.1.5",
          "form": "헉 정답 정답",
          "original_form": "헉 정답 정답",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044525.1.6",
          "form": "되게 빨리 맞추셨어요 짝짝짝",
          "original_form": "되게 빨리 맞추셨어요 짝짝짝",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044525.1.7",
          "form": "아 고기로 접근해서 어려웠네요 ㅋㅋㅋ ㅗ",
          "original_form": "아 고기로 접근해서 어려웠네요 ㅋㅋㅋ ㅗ",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044525.1.8",
          "form": "돼지고기 김치찌개 먹었답니다",
          "original_form": "돼지고기 김치찌개 먹었답니다",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044525.1.9",
          "form": "헉 실수예요 ㅠㅠㅠ",
          "original_form": "헉 실수예요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044525.1.10",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044525.1.11",
          "form": "제 메뉴도 맞춰보세요",
          "original_form": "제 메뉴도 맞춰보세요",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044525.1.12",
          "form": "좋아요!",
          "original_form": "좋아요!",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044525.1.13",
          "form": "쌀이 들어가나요?",
          "original_form": "쌀이 들어가나요?",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044525.1.14",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044525.1.15",
          "form": "그럼 면 요리인가요?",
          "original_form": "그럼 면 요리인가요?",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044525.1.16",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044525.1.17",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044525.1.18",
          "form": "그럼 양식인가요?",
          "original_form": "그럼 양식인가요?",
