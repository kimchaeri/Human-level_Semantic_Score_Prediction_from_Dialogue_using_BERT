      "utterance": [
        {
          "id": "MDRW2100040992.1.1",
          "form": "코울슬로 맛있게 먹어주어 고마워",
          "original_form": "코울슬로 맛있게 먹어주어 고마워",
          "speaker_id": "2",
          "time": "20210905 19:38"
        },
        {
          "id": "MDRW2100040992.1.2",
          "form": "코울슬로 제조 감사합니다",
          "original_form": "코울슬로 제조 감사합니다",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100040992.1.3",
          "form": "맛있다",
          "original_form": "맛있다",
          "speaker_id": "1",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100040992.1.4",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100040992.1.5",
          "form": "오늘은 양배추 말고 다른 재료들도 풍족해서",
          "original_form": "오늘은 양배추 말고 다른 재료들도 풍족해서",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100040992.1.6",
          "form": "더 맛있게 된 듯",
          "original_form": "더 맛있게 된 듯",
          "speaker_id": "2",
          "time": "20210905 19:39"
        },
        {
          "id": "MDRW2100040992.1.7",
          "form": "그런가벼",
          "original_form": "그런가벼",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100040992.1.8",
          "form": "아주 입에 넣는순간 맛있더라고",
          "original_form": "아주 입에 넣는순간 맛있더라고",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100040992.1.9",
          "form": "ㅎㅎ근데 다음번엔 같은 맛일지 보장못함",
          "original_form": "ㅎㅎ근데 다음번엔 같은 맛일지 보장못함",
          "speaker_id": "2",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100040992.1.10",
          "form": "이번 걸 즐깁시다",
          "original_form": "이번 걸 즐깁시다",
          "speaker_id": "2",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100040992.1.11",
          "form": "응 최대한 맛있게 먹자",
          "original_form": "응 최대한 맛있게 먹자",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100040992.1.12",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100040992.1.13",
          "form": "맛있게 됐을 때 최대한 즐긴다!",
          "original_form": "맛있게 됐을 때 최대한 즐긴다!",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100040992.1.14",
          "form": "내일 아침은 새로한 밥에 찌개쓰~",
          "original_form": "내일 아침은 새로한 밥에 찌개쓰~",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100040992.1.15",
          "form": "찌개 좋지",
          "original_form": "찌개 좋지",
          "speaker_id": "1",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100040992.1.16",
          "form": "내일 모레부터 도시락 온다으",
          "original_form": "내일 모레부터 도시락 온다으",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100040992.1.17",
          "form": "설레네",
          "original_form": "설레네",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100040992.1.18",
          "form": "오 그렇네",
          "original_form": "오 그렇네",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100040992.1.19",
          "form": "도시락 맛있으면 좋겠다",
          "original_form": "도시락 맛있으면 좋겠다",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100040992.1.20",
          "form": "양도 적당하고",
          "original_form": "양도 적당하고",
          "speaker_id": "2",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100040992.1.21",
          "form": "그르게",
          "original_form": "그르게",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100040992.1.22",
          "form": "양은 좀 작겠지",
          "original_form": "양은 좀 작겠지",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100040992.1.23",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100040992.1.24",
          "form": "그럼 먹다가 아쉬운디",
          "original_form": "그럼 먹다가 아쉬운디",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100040992.1.25",
          "form": "ㅋㅋㅋ뭐 그럼 치킨도 있고",
          "original_form": "ㅋㅋㅋ뭐 그럼 치킨도 있고",
          "speaker_id": "1",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100040992.1.26",
          "form": "먹을건 있쥐",
          "original_form": "먹을건 있쥐",
          "speaker_id": "1",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100040992.1.27",
          "form": "요거트도 구입해야 하려나?",
          "original_form": "요거트도 구입해야 하려나?",
          "speaker_id": "1",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100040992.1.28",
          "form": "차치키용?",
          "original_form": "차치키용?",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100040992.1.29",
          "form": "응응 겸사겸사",
          "original_form": "응응 겸사겸사",
          "speaker_id": "1",
          "time": "20210905 20:02"
        }
      ]
    }
