      "utterance": [
        {
          "id": "MDRW2100034294.1.1",
          "form": "누가대신 빼주면 좋겠어요 전 먹기만하고 ㅋㅋㅋㅋㅋ",
          "original_form": "누가대신 빼주면 좋겠어요 전 먹기만하고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100034294.1.2",
          "form": "최고죠ㅋㅋㄱ\r\n체중계 올라가기 전부터  찐게 느껴지지않나요",
          "original_form": "최고죠ㅋㅋㄱ\r\n체중계 올라가기 전부터  찐게 느껴지지않나요",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034294.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034294.1.4",
          "form": "진짜 올라가기 싫어요 ㅠ",
          "original_form": "진짜 올라가기 싫어요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034294.1.5",
          "form": "저는 오늘은 올라갔겠구나 싶은게 느껴지더라구요ㅋㅋ",
          "original_form": "저는 오늘은 올라갔겠구나 싶은게 느껴지더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034294.1.6",
          "form": "맞아요 ㅋㅋㅋ끔찍 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ끔찍 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034294.1.7",
          "form": "그니까요 미리 짐작했어도",
          "original_form": "그니까요 미리 짐작했어도",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034294.1.8",
          "form": "찌는건 순식간인데  빼는건....",
          "original_form": "찌는건 순식간인데  빼는건....",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034294.1.9",
          "form": "또 올라가서 보면  짜증나요ㅋㅋ",
          "original_form": "또 올라가서 보면  짜증나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034294.1.10",
          "form": "제말이요",
          "original_form": "제말이요",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034294.1.11",
          "form": "맞아요 근데 이상하게",
          "original_form": "맞아요 근데 이상하게",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034294.1.12",
          "form": "빠지는건 100그램도 잘 안빠지는데",
          "original_form": "빠지는건 100그램도 잘 안빠지는데",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034294.1.13",
          "form": "안올라가면 맘편하긴한데..한켠엔 불안한..",
          "original_form": "안올라가면 맘편하긴한데..한켠엔 불안한..",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034294.1.14",
          "form": "마자요ㅋㅋ",
          "original_form": "마자요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034294.1.15",
          "form": "하 맞아요 몸무게 줘패고싶어요 증말",
          "original_form": "하 맞아요 몸무게 줘패고싶어요 증말",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100034294.1.16",
          "form": "넘싫어요 ㅠ",
          "original_form": "넘싫어요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100034294.1.17",
          "form": "내가 안잰사이에 엄청 올랐을까봐",
          "original_form": "내가 안잰사이에 엄청 올랐을까봐",
