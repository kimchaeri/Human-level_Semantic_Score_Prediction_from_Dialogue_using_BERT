      "utterance": [
        {
          "id": "MDRW2100029350.1.1",
          "form": "치킨vs 삼겹살 뭐가 더 좋아요?",
          "original_form": "치킨vs 삼겹살 뭐가 더 좋아요?",
          "speaker_id": "1",
          "time": "20210903 02:30"
        },
        {
          "id": "MDRW2100029350.1.2",
          "form": "와... 난제다...",
          "original_form": "와... 난제다...",
          "speaker_id": "2",
          "time": "20210903 02:31"
        },
        {
          "id": "MDRW2100029350.1.3",
          "form": "골라야해요 하나만",
          "original_form": "골라야해요 하나만",
          "speaker_id": "1",
          "time": "20210903 02:31"
        },
        {
          "id": "MDRW2100029350.1.4",
          "form": "치킨을 조금더 사랑해요ㅋㅋ",
          "original_form": "치킨을 조금더 사랑해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 02:36"
        },
        {
          "id": "MDRW2100029350.1.5",
          "form": "이건 like 의 문제가 아닌 love의 문제죠",
          "original_form": "이건 like 의 문제가 아닌 love의 문제죠",
          "speaker_id": "2",
          "time": "20210903 02:36"
        },
        {
          "id": "MDRW2100029350.1.6",
          "form": "아...돼지 삐지겠네요",
          "original_form": "아...돼지 삐지겠네요",
          "speaker_id": "1",
          "time": "20210903 02:36"
        },
        {
          "id": "MDRW2100029350.1.7",
          "form": "삼겹살에 족발수육까지 얹어주시면 넘어갑니다",
          "original_form": "삼겹살에 족발수육까지 얹어주시면 넘어갑니다",
          "speaker_id": "2",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029350.1.8",
          "form": "악ㅋㅋㅋㅋㅋ 최애치킨은 뭐에여?",
          "original_form": "악ㅋㅋㅋㅋㅋ 최애치킨은 뭐에여?",
          "speaker_id": "1",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029350.1.9",
          "form": "흔치않겠지만 멕시카나 후라이드요!",
          "original_form": "흔치않겠지만 멕시카나 후라이드요!",
          "speaker_id": "2",
          "time": "20210903 02:39"
        },
        {
          "id": "MDRW2100029350.1.10",
          "form": "전국적으로 맛없기로 소문났지만 저희는 최고의 품질을 자랑한답니다ㅋㅋ",
          "original_form": "전국적으로 맛없기로 소문났지만 저희는 최고의 품질을 자랑한답니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 02:39"
        },
        {
          "id": "MDRW2100029350.1.11",
          "form": "아 name2네 동네 멕시카나가 맛있나보네요!!!",
          "original_form": "아 &name2&네 동네 멕시카나가 맛있나보네요!!!",
          "speaker_id": "1",
          "time": "20210903 02:40"
        },
        {
          "id": "MDRW2100029350.1.12",
          "form": "맞아요ㅎㅎ 그다음은 교촌 허니콤보!!",
          "original_form": "맞아요ㅎㅎ 그다음은 교촌 허니콤보!!",
          "speaker_id": "2",
          "time": "20210903 02:40"
        },
        {
          "id": "MDRW2100029350.1.13",
          "form": "달달하고 짭쪼름하니 양도 푸짐하고 맥주 큰캔에 딱입니다ㅋㅋ",
          "original_form": "달달하고 짭쪼름하니 양도 푸짐하고 맥주 큰캔에 딱입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 02:41"
        },
        {
          "id": "MDRW2100029350.1.14",
          "form": "오 맥주 좋아하시나요 저도 소주보단 맥주",
          "original_form": "오 맥주 좋아하시나요 저도 소주보단 맥주",
          "speaker_id": "1",
          "time": "20210903 02:42"
        },
        {
          "id": "MDRW2100029350.1.15",
          "form": "대학 초반에는 소주였는데 전역하고 나니 안되겠더라구요ㅠ",
          "original_form": "대학 초반에는 소주였는데 전역하고 나니 안되겠더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210903 02:42"
        },
        {
          "id": "MDRW2100029350.1.16",
          "form": "음료처럼 마실수있는 맥주가 좋습니다ㅎ",
          "original_form": "음료처럼 마실수있는 맥주가 좋습니다ㅎ",
          "speaker_id": "2",
          "time": "20210903 02:43"
        },
        {
          "id": "MDRW2100029350.1.17",
          "form": "맥주 원샷하실 수 있어요??",
          "original_form": "맥주 원샷하실 수 있어요??",
          "speaker_id": "1",
          "time": "20210903 02:43"
        },
        {
          "id": "MDRW2100029350.1.18",
          "form": "그러고 싶은데 못하겠어요ㅠㅜ",
          "original_form": "그러고 싶은데 못하겠어요ㅠㅜ",
