      "utterance": [
        {
          "id": "MDRW2100020087.1.1",
          "form": "젤 힘들었던 알바는 설거지였어요",
          "original_form": "젤 힘들었던 알바는 설거지였어요",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020087.1.2",
          "form": "그건 딱 두달하고 때려치움ㅋㅋㅋ",
          "original_form": "그건 딱 두달하고 때려치움ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020087.1.3",
          "form": "ㅋㅋㅋㅋ설거지 계속 밀려들었죠?",
          "original_form": "ㅋㅋㅋㅋ설거지 계속 밀려들었죠?",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020087.1.4",
          "form": "주말엔 진짜 1초도 못쉬었어요ㅋㅋㅋ",
          "original_form": "주말엔 진짜 1초도 못쉬었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:24"
        },
        {
          "id": "MDRW2100020087.1.5",
          "form": "멍청하게 오픈 초기에 알바해서",
          "original_form": "멍청하게 오픈 초기에 알바해서",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020087.1.6",
          "form": "사람도 엄청많고ㅋㅋㅋ",
          "original_form": "사람도 엄청많고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020087.1.7",
          "form": "으으으...설거지는 쌓이고ㅋㅋㅋㅋ",
          "original_form": "으으으...설거지는 쌓이고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020087.1.8",
          "form": "전 오늘 업무썰 풀었어요...",
          "original_form": "전 오늘 업무썰 풀었어요...",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020087.1.9",
          "form": "속시원",
          "original_form": "속시원",
          "speaker_id": "2",
          "time": "20210828 00:30"
        },
        {
          "id": "MDRW2100020087.1.10",
          "form": "아까 화났다고했었잖아요ㅋㅋㅋㅋ",
          "original_form": "아까 화났다고했었잖아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020087.1.11",
          "form": "그사건ㅋㅋㅋㅋ",
          "original_form": "그사건ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:31"
        },
        {
          "id": "MDRW2100020087.1.12",
          "form": "뭐였어요??",
          "original_form": "뭐였어요??",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020087.1.13",
          "form": "안알랴줌",
          "original_form": "안알랴줌",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020087.1.14",
          "form": "아 까비",
          "original_form": "아 까비",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020087.1.15",
          "form": "진짜 아까부터 궁금했던건데",
          "original_form": "진짜 아까부터 궁금했던건데",
          "speaker_id": "1",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020087.1.16",
          "form": "어쩔 수 없이 넘어가드릴게요",
          "original_form": "어쩔 수 없이 넘어가드릴게요",
          "speaker_id": "1",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020087.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ전 이제 자러갑니다...낼 얘기해주께여",
          "original_form": "ㅋㅋㅋㅋㅋㅋ전 이제 자러갑니다...낼 얘기해주께여",
          "speaker_id": "2",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100020087.1.18",
          "form": "알바 대타때매 하루종일 합니다ㅜㅜ 피곤해 죽겠음",
          "original_form": "알바 대타때매 하루종일 합니다ㅜㅜ 피곤해 죽겠음",
          "speaker_id": "1",
          "time": "20210829 10:25"
        },
        {
          "id": "MDRW2100020087.1.19",
          "form": "알바대타했나여ㅋㅋ",
          "original_form": "알바대타했나여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:18"
        },
        {
          "id": "MDRW2100020087.1.20",
          "form": "카페알바한댔죠",
          "original_form": "카페알바한댔죠",
          "speaker_id": "2",
          "time": "20210829 12:18"
        },
        {
          "id": "MDRW2100020087.1.21",
          "form": "네... 오늘까지 정신없네요",
          "original_form": "네... 오늘까지 정신없네요",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100020087.1.22",
          "form": "비오는데 인간들 집에서 좀 쉬지",
          "original_form": "비오는데 인간들 집에서 좀 쉬지",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100020087.1.23",
          "form": "거긴 비오나요...",
          "original_form": "거긴 비오나요...",
          "speaker_id": "2",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100020087.1.24",
          "form": "비오는날 카페들을 더 잘 가는 거 같네요",
          "original_form": "비오는날 카페들을 더 잘 가는 거 같네요",
          "speaker_id": "2",
          "time": "20210829 18:17"
        },
        {
          "id": "MDRW2100020087.1.25",
          "form": "따뜻한 라떼 먹으면 기분 좀 풀리긴 하죠",
          "original_form": "따뜻한 라떼 먹으면 기분 좀 풀리긴 하죠",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100020087.1.26",
          "form": "전 유당불내증때문에 못마십니다ㅜㅜ",
          "original_form": "전 유당불내증때문에 못마십니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100020087.1.27",
          "form": "두유로 바꿔 넣어주는곳에서 마셔야겠네요",
          "original_form": "두유로 바꿔 넣어주는곳에서 마셔야겠네요",
          "speaker_id": "2",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100020087.1.28",
          "form": "어우 두유는 싫어해서 제가",
          "original_form": "어우 두유는 싫어해서 제가",
          "speaker_id": "1",
          "time": "20210830 09:48"
        },
        {
          "id": "MDRW2100020087.1.29",
          "form": "아 ㅋㅋㅋㅋ그렇다면 어쩔수없..",
          "original_form": "아 ㅋㅋㅋㅋ그렇다면 어쩔수없..",
          "speaker_id": "2",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100020087.1.30",
          "form": "아...월요일 너무 일하기싫어요",
          "original_form": "아...월요일 너무 일하기싫어요",
          "speaker_id": "2",
          "time": "20210830 13:04"
        },
        {
          "id": "MDRW2100020087.1.31",
          "form": "귀차나...",
          "original_form": "귀차나...",
          "speaker_id": "2",
          "time": "20210830 13:04"
        },
        {
          "id": "MDRW2100020087.1.32",
          "form": "월요일 좋아~~",
          "original_form": "월요일 좋아~~",
