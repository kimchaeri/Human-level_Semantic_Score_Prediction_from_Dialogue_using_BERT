      "utterance": [
        {
          "id": "MDRW2100020627.1.1",
          "form": "한겨울에도 아이스죠 역시",
          "original_form": "한겨울에도 아이스죠 역시",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020627.1.2",
          "form": "커피는 어떤거 좋아하시나요",
          "original_form": "커피는 어떤거 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020627.1.3",
          "form": "얼죽아 회원이시군요 반갑습니다. 그마음 변치마시고 올 겨울도",
          "original_form": "얼죽아 회원이시군요 반갑습니다. 그마음 변치마시고 올 겨울도",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020627.1.4",
          "form": "활발한 활동을..",
          "original_form": "활발한 활동을..",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.5",
          "form": "전 단거좋아해서",
          "original_form": "전 단거좋아해서",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.6",
          "form": "올겨울도 시린 손으로 보내보아요",
          "original_form": "올겨울도 시린 손으로 보내보아요",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.7",
          "form": "카라멜마끼아또",
          "original_form": "카라멜마끼아또",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.8",
          "form": "라떼..",
          "original_form": "라떼..",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.9",
          "form": "아 향기 좋죠",
          "original_form": "아 향기 좋죠",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.10",
          "form": "이런거",
          "original_form": "이런거",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.11",
          "form": "근데 모카는 싫어요",
          "original_form": "근데 모카는 싫어요",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.12",
          "form": "향이 맛을 못따라가는 느낌",
          "original_form": "향이 맛을 못따라가는 느낌",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.13",
          "form": "오 맞아요..!",
          "original_form": "오 맞아요..!",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.14",
          "form": "델리만쥬같은 녀석이랄까",
          "original_form": "델리만쥬같은 녀석이랄까",
          "speaker_id": "1",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.15",
          "form": "맛이 향을 못따라가서",
          "original_form": "맛이 향을 못따라가서",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020627.1.16",
          "form": "너무 통탄스러울 때가 있어요",
          "original_form": "너무 통탄스러울 때가 있어요",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.17",
          "form": "제가 그래서 아아를 먹죠",
          "original_form": "제가 그래서 아아를 먹죠",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.18",
          "form": "기대했는데 오메 이게뭐당가",
          "original_form": "기대했는데 오메 이게뭐당가",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.19",
          "form": "이분야 갑 헤이즐넛...",
          "original_form": "이분야 갑 헤이즐넛...",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.20",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.21",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.22",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ그러다가 편의점에",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러다가 편의점에",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.24",
          "form": "걘친짜 사기즐넛이라 바꿔야해",
          "original_form": "걘친짜 사기즐넛이라 바꿔야해",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.25",
          "form": "헤이즐넛 아메리카노를 마셔봤는네",
          "original_form": "헤이즐넛 아메리카노를 마셔봤는네",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.26",
          "form": "어떻던가요",
          "original_form": "어떻던가요",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.27",
          "form": "얘는.. 얘는 달라요",
          "original_form": "얘는.. 얘는 달라요",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020627.1.28",
          "form": "얘는 정말 맛있어요",
          "original_form": "얘는 정말 맛있어요",
          "speaker_id": "2",
          "time": "20210828 10:37"
        }
      ]
    }
  ]
