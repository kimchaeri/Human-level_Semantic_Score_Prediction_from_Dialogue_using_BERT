      "utterance": [
        {
          "id": "MDRW2100000317.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000317.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000317.1.3",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000317.1.4",
          "form": "강아지는 사랑이에요 정말",
          "original_form": "강아지는 사랑이에요 정말",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000317.1.5",
          "form": "저도 댕댕이 키웠는데 작년에 무지개다리 건넜어요",
          "original_form": "저도 댕댕이 키웠는데 작년에 무지개다리 건넜어요",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000317.1.6",
          "form": "아직도 매일매일 보고 싶어요",
          "original_form": "아직도 매일매일 보고 싶어요",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000317.1.7",
          "form": "저도요 다시들인지 3년됐어요 먼저간친구보다 더 잘해주려구요",
          "original_form": "저도요 다시들인지 3년됐어요 먼저간친구보다 더 잘해주려구요",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000317.1.8",
          "form": "저는 아직 준비가..",
          "original_form": "저는 아직 준비가..",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000317.1.9",
          "form": "미안한 마음이 클 것 같아서요",
          "original_form": "미안한 마음이 클 것 같아서요",
          "speaker_id": "2",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000317.1.10",
          "form": "저도준비가안되었는데  동생은 준비가됐나 동생이입양했어요 처음엔 힘들었는데 시간이 지나고 새로운아이에게 열심하니 이젠 간아이 보다새로운 아이에게 더 신경쓰고있어요",
          "original_form": "저도준비가안되었는데  동생은 준비가됐나 동생이입양했어요 처음엔 힘들었는데 시간이 지나고 새로운아이에게 열심하니 이젠 간아이 보다새로운 아이에게 더 신경쓰고있어요",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000317.1.11",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000317.1.12",
          "form": "다들 그렇게 말씀하시더라고요",
          "original_form": "다들 그렇게 말씀하시더라고요",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000317.1.13",
          "form": "또 새로운 가족이 들어오면 마음이 자연스럽게 진정이 된다고요",
          "original_form": "또 새로운 가족이 들어오면 마음이 자연스럽게 진정이 된다고요",
          "speaker_id": "2",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000317.1.14",
          "form": "그런거같아요 경험해보니 더욱더 그런것같아요 아까운동하고왔는데 피곤한지 밥먹고자네요",
          "original_form": "그런거같아요 경험해보니 더욱더 그런것같아요 아까운동하고왔는데 피곤한지 밥먹고자네요",
          "speaker_id": "1",
          "time": "20210819 19:22"
        },
        {
          "id": "MDRW2100000317.1.15",
          "form": "귀요미 잘 때 그렇게 귀엽더라고요",
          "original_form": "귀요미 잘 때 그렇게 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100000317.1.16",
          "form": "괜히 깨우고 싶고!",
          "original_form": "괜히 깨우고 싶고!",
          "speaker_id": "2",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100000317.1.17",
          "form": "안깨워요 사람이나 강아지나 다르다 생각안해요 인간의 욕구나 강아지의 욕구나 다 똑같다 생각하니깐요",
          "original_form": "안깨워요 사람이나 강아지나 다르다 생각안해요 인간의 욕구나 강아지의 욕구나 다 똑같다 생각하니깐요",
          "speaker_id": "1",
