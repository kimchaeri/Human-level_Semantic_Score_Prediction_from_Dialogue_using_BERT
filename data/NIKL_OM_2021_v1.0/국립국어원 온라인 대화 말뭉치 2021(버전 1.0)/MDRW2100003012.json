        {
          "id": "MDRW2100003012.1.1",
          "form": "벌써 시켰어?",
          "original_form": "벌써 시켰어?",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003012.1.2",
          "form": "ㅇㅇ 미리 받아놔도 되니깐",
          "original_form": "ㅇㅇ 미리 받아놔도 되니깐",
          "speaker_id": "1",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003012.1.3",
          "form": "ㅎㅎ 곧 오겠지",
          "original_form": "ㅎㅎ 곧 오겠지",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003012.1.4",
          "form": "비 와서 안오나",
          "original_form": "비 와서 안오나",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003012.1.5",
          "form": "그럴수도",
          "original_form": "그럴수도",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003012.1.6",
          "form": "회에 6가지 뭐 다른거 준다는 거 같아 소라무침이랑 ㅎㅎ",
          "original_form": "회에 6가지 뭐 다른거 준다는 거 같아 소라무침이랑 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003012.1.7",
          "form": "오 맛있겠네",
          "original_form": "오 맛있겠네",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003012.1.8",
          "form": "소라 무침 오랜만에 먹어보네~ 광어 우럭도",
          "original_form": "소라 무침 오랜만에 먹어보네~ 광어 우럭도",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003012.1.9",
          "form": "ㅎㅎ 나도",
          "original_form": "ㅎㅎ 나도",
          "speaker_id": "2",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003012.1.10",
          "form": "미안 ㅠㅠ 내가 별로 안좋아해서 신랑까지 못먹고",
          "original_form": "미안 ㅠㅠ 내가 별로 안좋아해서 신랑까지 못먹고",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003012.1.11",
          "form": "ㅎㅎ 괜찮아 오늘 먹잖아",
          "original_form": "ㅎㅎ 괜찮아 오늘 먹잖아",
          "speaker_id": "2",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003012.1.12",
          "form": "ㅇㅇ 오늘 많이 먹어요",
          "original_form": "ㅇㅇ 오늘 많이 먹어요",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003012.1.13",
          "form": "거기 손님 많은 것 같더라고",
          "original_form": "거기 손님 많은 것 같더라고",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003012.1.14",
          "form": "ㅇㅇ 나도 아까 찾아보니 꽤 맛있나봐",
          "original_form": "ㅇㅇ 나도 아까 찾아보니 꽤 맛있나봐",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100003012.1.15",
          "form": "기대중 ㅎㅎ",
          "original_form": "기대중 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100003012.1.16",
          "form": "저번 피자집보다 나으면 돼 ㅋㅋ",
