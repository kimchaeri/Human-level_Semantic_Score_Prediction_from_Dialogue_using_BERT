      "utterance": [
        {
          "id": "MDRW2100024894.1.1",
          "form": "혹시 이 쪽이 이직이 흔한가요?",
          "original_form": "혹시 이 쪽이 이직이 흔한가요?",
          "speaker_id": "2",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100024894.1.2",
          "form": "음 그렇죠",
          "original_form": "음 그렇죠",
          "speaker_id": "1",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100024894.1.3",
          "form": "워낙 힘든 직종이기도하고",
          "original_form": "워낙 힘든 직종이기도하고",
          "speaker_id": "1",
          "time": "20210831 20:24"
        },
        {
          "id": "MDRW2100024894.1.4",
          "form": "좀 잦은편이예요",
          "original_form": "좀 잦은편이예요",
          "speaker_id": "1",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024894.1.5",
          "form": "아하~  아에 다른 쪽으로 가는건가요? 디자인을 안하고?",
          "original_form": "아하~  아에 다른 쪽으로 가는건가요? 디자인을 안하고?",
          "speaker_id": "2",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024894.1.6",
          "form": "아 처음에 들어왔다가 포기하고",
          "original_form": "아 처음에 들어왔다가 포기하고",
          "speaker_id": "1",
          "time": "20210831 20:25"
        },
        {
          "id": "MDRW2100024894.1.7",
          "form": "다른쪽가는것도 종종 있죠",
          "original_form": "다른쪽가는것도 종종 있죠",
          "speaker_id": "1",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100024894.1.8",
          "form": "근데 이렇게 만난것도 진짜 신기하네요",
          "original_form": "근데 이렇게 만난것도 진짜 신기하네요",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100024894.1.9",
          "form": "이직하면 다 서로 건너건너 아는 사람이겠어요 ㅋㅋ",
          "original_form": "이직하면 다 서로 건너건너 아는 사람이겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100024894.1.10",
          "form": "맞아요 업계가 좁죠",
          "original_form": "맞아요 업계가 좁죠",
          "speaker_id": "1",
          "time": "20210831 20:26"
        },
        {
          "id": "MDRW2100024894.1.11",
          "form": "이직을 두번했는데",
          "original_form": "이직을 두번했는데",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100024894.1.12",
          "form": "두번째 회사에서 첫번째회사 다니셨던분들",
          "original_form": "두번째 회사에서 첫번째회사 다니셨던분들",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100024894.1.13",
          "form": "몇번봤어요",
          "original_form": "몇번봤어요",
          "speaker_id": "1",
          "time": "20210831 20:27"
        },
        {
          "id": "MDRW2100024894.1.14",
          "form": "오아우...!!  세에상에",
          "original_form": "오아우...!!  세에상에",
          "speaker_id": "2",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024894.1.15",
          "form": "진짜 가족이네요",
          "original_form": "진짜 가족이네요",
          "speaker_id": "2",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024894.1.16",
          "form": "모션패밀리",
          "original_form": "모션패밀리",
          "speaker_id": "2",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024894.1.17",
          "form": "가족은 되고싶지 않네요 ㅋㅋㅋ",
          "original_form": "가족은 되고싶지 않네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024894.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 회사는 회사로 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 회사는 회사로 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024894.1.19",
          "form": "무슨 영상하세요?",
          "original_form": "무슨 영상하세요?",
          "speaker_id": "1",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100024894.1.20",
          "form": "저희는 다 해요",
          "original_form": "저희는 다 해요",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100024894.1.21",
          "form": "음 에이전시?",
          "original_form": "음 에이전시?",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100024894.1.22",
          "form": "아 ㅜㅜㅜ 다양하거 다받아서 하시는군요",
          "original_form": "아 ㅜㅜㅜ 다양하거 다받아서 하시는군요",
          "speaker_id": "1",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100024894.1.23",
          "form": "네 ㅋㅋㅋ 그래서 더 헤메고 있는 걸 수도 있어요 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ 그래서 더 헤메고 있는 걸 수도 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100024894.1.24",
          "form": "선배님은요?",
          "original_form": "선배님은요?",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100024894.1.25",
          "form": "아 에이전시면 진짜힘들지 않아요?ㅜ",
          "original_form": "아 에이전시면 진짜힘들지 않아요?ㅜ",
          "speaker_id": "1",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100024894.1.26",
          "form": "저는 지금은 이러닝 인서트해요",
          "original_form": "저는 지금은 이러닝 인서트해요",
          "speaker_id": "1",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100024894.1.27",
          "form": "처음에는 홈쇼핑했고 두번째는 oap했어요",
          "original_form": "처음에는 홈쇼핑했고 두번째는 oap했어요",
          "speaker_id": "1",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100024894.1.28",
          "form": "영상인데 계속 분야는 바꿨네요 ㅎㅎ",
          "original_form": "영상인데 계속 분야는 바꿨네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:31"
        }
      ]
    }
