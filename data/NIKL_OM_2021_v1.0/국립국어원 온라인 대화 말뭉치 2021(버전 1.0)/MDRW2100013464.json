      "utterance": [
        {
          "id": "MDRW2100013464.1.1",
          "form": "제주도 여행어떠셨나요",
          "original_form": "제주도 여행어떠셨나요",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100013464.1.2",
          "form": "저는 제주도 수학여행갔을때",
          "original_form": "저는 제주도 수학여행갔을때",
          "speaker_id": "1",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100013464.1.3",
          "form": "흙돼지 먹었죠",
          "original_form": "흙돼지 먹었죠",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100013464.1.4",
          "form": "성산 일출봉도 봤어요",
          "original_form": "성산 일출봉도 봤어요",
          "speaker_id": "2",
          "time": "20210825 14:44"
        },
        {
          "id": "MDRW2100013464.1.5",
          "form": "성산 일출봉 열심히 걸어 올라갔는데 생각보다 심심한 전경이라 놀랐어요",
          "original_form": "성산 일출봉 열심히 걸어 올라갔는데 생각보다 심심한 전경이라 놀랐어요",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100013464.1.6",
          "form": "일출봉..",
          "original_form": "일출봉..",
          "speaker_id": "1",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100013464.1.7",
          "form": "저도 갈뻔했는데 안가길잘했네요",
          "original_form": "저도 갈뻔했는데 안가길잘했네요",
          "speaker_id": "1",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100013464.1.8",
          "form": "제가 바다 평소에 많이 봐서 익숙한 풍경이거든요",
          "original_form": "제가 바다 평소에 많이 봐서 익숙한 풍경이거든요",
          "speaker_id": "2",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100013464.1.9",
          "form": "그날 비가 내려갖고 일부 같은반친구들은 버스에서 안내리더라고요",
          "original_form": "그날 비가 내려갖고 일부 같은반친구들은 버스에서 안내리더라고요",
          "speaker_id": "1",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100013464.1.10",
          "form": "등산한걸로 괜찮죠뭐",
          "original_form": "등산한걸로 괜찮죠뭐",
          "speaker_id": "2",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100013464.1.11",
          "form": "평지에서 등반하자마자",
          "original_form": "평지에서 등반하자마자",
          "speaker_id": "1",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100013464.1.12",
          "form": "저때는 아주 맑았답니닷 @-@",
          "original_form": "저때는 아주 맑았답니닷 @-@",
          "speaker_id": "2",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100013464.1.13",
          "form": "흑돼지 좋아하세요?",
          "original_form": "흑돼지 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013464.1.14",
          "form": "요즘 내리는 변덕스러운비처럼 쫙쫙 내려서",
          "original_form": "요즘 내리는 변덕스러운비처럼 쫙쫙 내려서",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013464.1.15",
          "form": "금방 후퇴했어요",
          "original_form": "금방 후퇴했어요",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013464.1.16",
          "form": "비가 많이 내렸내요",
          "original_form": "비가 많이 내렸내요",
          "speaker_id": "2",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013464.1.17",
          "form": "억지로 단체사진찍어서",
          "original_form": "억지로 단체사진찍어서",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013464.1.18",
          "form": "힘드셨겠다",
          "original_form": "힘드셨겠다",
          "speaker_id": "2",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013464.1.19",
          "form": "저는 기억도 안나요",
          "original_form": "저는 기억도 안나요",
          "speaker_id": "2",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013464.1.20",
          "form": "버스타고 다음 행선지로 떠났었어요",
          "original_form": "버스타고 다음 행선지로 떠났었어요",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013464.1.21",
          "form": "저 해외여행 가고싶어요",
          "original_form": "저 해외여행 가고싶어요",
          "speaker_id": "1",
          "time": "20210825 15:53"
        }
      ]
