      "utterance": [
        {
          "id": "MDRW2100025243.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 02:32"
        },
        {
          "id": "MDRW2100025243.1.2",
          "form": "안녕하세요 스포츠 좋아하세요?",
          "original_form": "안녕하세요 스포츠 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 02:34"
        },
        {
          "id": "MDRW2100025243.1.3",
          "form": "네 보는거 좋아해요 ㅎㅎ",
          "original_form": "네 보는거 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 02:35"
        },
        {
          "id": "MDRW2100025243.1.4",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210901 02:35"
        },
        {
          "id": "MDRW2100025243.1.5",
          "form": "저도요. 전 야구 보는 거 좋아해요",
          "original_form": "저도요. 전 야구 보는 거 좋아해요",
          "speaker_id": "2",
          "time": "20210901 02:35"
        },
        {
          "id": "MDRW2100025243.1.6",
          "form": "오 어디 팬이신가오",
          "original_form": "오 어디 팬이신가오",
          "speaker_id": "1",
          "time": "20210901 02:37"
        },
        {
          "id": "MDRW2100025243.1.7",
          "form": "저는 두산이요. 야구 보세요?",
          "original_form": "저는 두산이요. 야구 보세요?",
          "speaker_id": "2",
          "time": "20210901 02:42"
        },
        {
          "id": "MDRW2100025243.1.8",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:44"
        },
        {
          "id": "MDRW2100025243.1.9",
          "form": "키움입니다ㅋㅋ",
          "original_form": "키움입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:44"
        },
        {
          "id": "MDRW2100025243.1.10",
          "form": "직관도 종종 가세요?",
          "original_form": "직관도 종종 가세요?",
          "speaker_id": "2",
          "time": "20210901 02:45"
        },
        {
          "id": "MDRW2100025243.1.11",
          "form": "가끔 갔는데 코로나 터지고는 안갔어요ㅋㅋ",
          "original_form": "가끔 갔는데 코로나 터지고는 안갔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:46"
        },
        {
          "id": "MDRW2100025243.1.12",
          "form": "재작년에 키움 코시에서",
          "original_form": "재작년에 키움 코시에서",
          "speaker_id": "1",
          "time": "20210901 02:47"
        },
        {
          "id": "MDRW2100025243.1.13",
          "form": "두산에게 줘터질때",
          "original_form": "두산에게 줘터질때",
          "speaker_id": "1",
          "time": "20210901 02:47"
        },
        {
          "id": "MDRW2100025243.1.14",
          "form": "현장에 있었습니다ㅋㅋㅋ",
          "original_form": "현장에 있었습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:47"
        },
        {
          "id": "MDRW2100025243.1.15",
          "form": "그때 그래도 4경기 다 엄청 치열했던 것 같아요",
          "original_form": "그때 그래도 4경기 다 엄청 치열했던 것 같아요",
          "speaker_id": "2",
          "time": "20210901 02:49"
        },
        {
          "id": "MDRW2100025243.1.16",
          "form": "저도 코로나 후에는 못 가봤어요",
          "original_form": "저도 코로나 후에는 못 가봤어요",
          "speaker_id": "2",
          "time": "20210901 02:49"
        },
        {
          "id": "MDRW2100025243.1.17",
          "form": "치열하게 싸우다가",
          "original_form": "치열하게 싸우다가",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025243.1.18",
          "form": "4경기 다 내줬습니다...",
          "original_form": "4경기 다 내줬습니다...",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025243.1.19",
          "form": "솔직히 분위기 넘어가서 4번째 경기 질거 같았는데",
          "original_form": "솔직히 분위기 넘어가서 4번째 경기 질거 같았는데",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025243.1.20",
          "form": "두산팬인 형 때문에.....",
          "original_form": "두산팬인 형 때문에.....",
          "speaker_id": "1",
          "time": "20210901 02:51"
        },
        {
          "id": "MDRW2100025243.1.21",
          "form": "표 팔면 더블이었는데ㅋㅋㅋ",
          "original_form": "표 팔면 더블이었는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:51"
        },
        {
          "id": "MDRW2100025243.1.22",
          "form": "따라가셨군요ㅎㅎ",
          "original_form": "따라가셨군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 02:51"
        },
        {
          "id": "MDRW2100025243.1.23",
          "form": "키움은 요즘 어린 선수들이 나와서 잘 하고 있는 것 같아요",
          "original_form": "키움은 요즘 어린 선수들이 나와서 잘 하고 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210901 02:52"
        },
        {
          "id": "MDRW2100025243.1.24",
          "form": "제가 예매했습니다ㅋㅋㅋㅋ",
          "original_form": "제가 예매했습니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:52"
        },
        {
          "id": "MDRW2100025243.1.25",
          "form": "금손으로 4연석 티켓팅 성공했는데...",
          "original_form": "금손으로 4연석 티켓팅 성공했는데...",
          "speaker_id": "1",
          "time": "20210901 02:52"
        },
        {
          "id": "MDRW2100025243.1.26",
          "form": "ㅋㅋㅋ 4연석이라니 진짜 금손이시네요",
          "original_form": "ㅋㅋㅋ 4연석이라니 진짜 금손이시네요",
