      "utterance": [
        {
          "id": "MDRW2100018245.1.1",
          "form": "어디지역분이신가요?",
          "original_form": "어디지역분이신가요?",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018245.1.2",
          "form": "저는 인천에 살아요",
          "original_form": "저는 인천에 살아요",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018245.1.3",
          "form": "아하 저는 안양이여",
          "original_form": "아하 저는 안양이여",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018245.1.4",
          "form": "인천도 갈데 많져?",
          "original_form": "인천도 갈데 많져?",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018245.1.5",
          "form": "오 그러시구나 넹",
          "original_form": "오 그러시구나 넹",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018245.1.6",
          "form": "은근히 갈만한 곳이 있는데",
          "original_form": "은근히 갈만한 곳이 있는데",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018245.1.7",
          "form": "저 월미도도 가고 강화도도 가고 차이나타운도 가고",
          "original_form": "저 월미도도 가고 강화도도 가고 차이나타운도 가고",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018245.1.8",
          "form": "사람 취향이 많이 걸릴거 같아서",
          "original_form": "사람 취향이 많이 걸릴거 같아서",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018245.1.9",
          "form": "송도도가고",
          "original_form": "송도도가고",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018245.1.10",
          "form": "인천은가까워서",
          "original_form": "인천은가까워서",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018245.1.11",
          "form": "ㅋㅋㅋㅋㅋ 조용하니 좋지 않아요?",
          "original_form": "ㅋㅋㅋㅋㅋ 조용하니 좋지 않아요?",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018245.1.12",
          "form": "송도는 진짜 조용해여ㅋㅋ",
          "original_form": "송도는 진짜 조용해여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018245.1.13",
          "form": "청라도",
          "original_form": "청라도",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018245.1.14",
          "form": "지금 말씀하신 곳들 다른 곳보다 되게 한적해요",
          "original_form": "지금 말씀하신 곳들 다른 곳보다 되게 한적해요",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018245.1.15",
          "form": "거긴 더 한적하던데 ㅋㅋㅋㅋㅋ",
          "original_form": "거긴 더 한적하던데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018245.1.16",
          "form": "인천사람들은 서울로 놀러가서 그런가봐옄ㅋㅋㅋ",
          "original_form": "인천사람들은 서울로 놀러가서 그런가봐옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018245.1.17",
          "form": "그래서 지하철이 그렇게..",
          "original_form": "그래서 지하철이 그렇게..",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018245.1.18",
          "form": "사람이 많군요ㅋ",
          "original_form": "사람이 많군요ㅋ",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018245.1.19",
          "form": "서울에서 노시나봐요",
          "original_form": "서울에서 노시나봐요",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018245.1.20",
          "form": "서울도 코로나땜에 상권 죽은데 많아서",
          "original_form": "서울도 코로나땜에 상권 죽은데 많아서",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018245.1.21",
          "form": "저는 인천 구월동이나 가여 귀찮아요",
          "original_form": "저는 인천 구월동이나 가여 귀찮아요",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018245.1.22",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018245.1.23",
          "form": "구월동 알아여",
          "original_form": "구월동 알아여",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018245.1.24",
          "form": "맞아요 홍대쪽 진짜 오래간만에 갔더니 비어있고 한적해서 놀랬어요",
          "original_form": "맞아요 홍대쪽 진짜 오래간만에 갔더니 비어있고 한적해서 놀랬어요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018245.1.25",
          "form": "그 버스터미널있는데 아니에요?",
          "original_form": "그 버스터미널있는데 아니에요?",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018245.1.26",
          "form": "맞아요 거긴데 롯데백화점으로 바뀌고 한적해졌어요",
          "original_form": "맞아요 거긴데 롯데백화점으로 바뀌고 한적해졌어요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018245.1.27",
          "form": "사람이 읎어",
          "original_form": "사람이 읎어",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018245.1.28",
          "form": "그렇군여",
          "original_form": "그렇군여",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018245.1.29",
          "form": "코로나라 그런게 아니라 상권이 죽었어요",
          "original_form": "코로나라 그런게 아니라 상권이 죽었어요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018245.1.30",
          "form": "해외 가고 싶어요",
          "original_form": "해외 가고 싶어요",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018245.1.31",
          "form": "해외여행 어디로 가고 싶으세요?",
          "original_form": "해외여행 어디로 가고 싶으세요?",
          "speaker_id": "2",
          "time": "20210826 23:26"
        }
      ]
    }
