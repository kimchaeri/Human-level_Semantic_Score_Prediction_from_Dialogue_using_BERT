      "utterance": [
        {
          "id": "MDRW2100033647.1.1",
          "form": "무슨 공연 좋아하시나요?",
          "original_form": "무슨 공연 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 20:02"
        },
        {
          "id": "MDRW2100033647.1.2",
          "form": "전 영화요 ㅎㅎ",
          "original_form": "전 영화요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:04"
        },
        {
          "id": "MDRW2100033647.1.3",
          "form": "예전엔 친한언니가 연극표 나오면 가끔 데려가 줬는데 이젠 지방이라 갈일리 없어요",
          "original_form": "예전엔 친한언니가 연극표 나오면 가끔 데려가 줬는데 이젠 지방이라 갈일리 없어요",
          "speaker_id": "2",
          "time": "20210824 20:05"
        },
        {
          "id": "MDRW2100033647.1.4",
          "form": "뭐좋아하세요?",
          "original_form": "뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 20:05"
        },
        {
          "id": "MDRW2100033647.1.5",
          "form": "오 저 올해 처음인가요..최근에 모가디슈 봤어요",
          "original_form": "오 저 올해 처음인가요..최근에 모가디슈 봤어요",
          "speaker_id": "1",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100033647.1.6",
          "form": "어머 저도 보고 싶었는데",
          "original_form": "어머 저도 보고 싶었는데",
          "speaker_id": "2",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100033647.1.7",
          "form": "전 한달전 분노의 질주요",
          "original_form": "전 한달전 분노의 질주요",
          "speaker_id": "2",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100033647.1.8",
          "form": "모가디슈 잼나요?",
          "original_form": "모가디슈 잼나요?",
          "speaker_id": "2",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100033647.1.9",
          "form": "조인성 나오는 거 그거 맞죠?",
          "original_form": "조인성 나오는 거 그거 맞죠?",
          "speaker_id": "2",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100033647.1.10",
          "form": "맞아요 재밌더라구요",
          "original_form": "맞아요 재밌더라구요",
          "speaker_id": "1",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100033647.1.11",
          "form": "ㅋㅋ배우도 중요해요",
          "original_form": "ㅋㅋ배우도 중요해요",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100033647.1.12",
          "form": "전 극장 1년 반만에 갔는데",
          "original_form": "전 극장 1년 반만에 갔는데",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100033647.1.13",
          "form": "노마스 땜시 안가려고요",
          "original_form": "노마스 땜시 안가려고요",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100033647.1.14",
          "form": "분노의 질주는 어때요?",
          "original_form": "분노의 질주는 어때요?",
          "speaker_id": "1",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100033647.1.15",
          "form": "아 남자들 왜그런지",
          "original_form": "아 남자들 왜그런지",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100033647.1.16",
          "form": "뭐 그냥 화끈하죠",
          "original_form": "뭐 그냥 화끈하죠",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100033647.1.17",
          "form": "그렇군요 영화관에서 노마스크라니 ㅜㅠ",
          "original_form": "그렇군요 영화관에서 노마스크라니 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100033647.1.18",
          "form": "그죠 밀폐된곳에 노마스크 넘 겁나요",
          "original_form": "그죠 밀폐된곳에 노마스크 넘 겁나요",
          "speaker_id": "2",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100033647.1.19",
          "form": "빨리 맘편히 가고파요",
          "original_form": "빨리 맘편히 가고파요",
          "speaker_id": "2",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100033647.1.20",
          "form": "직원이 좀 돌아다니며 주의주면좋겠어요",
          "original_form": "직원이 좀 돌아다니며 주의주면좋겠어요",
          "speaker_id": "2",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100033647.1.21",
          "form": "전 모가디슈 유플 나옴 봐야겠어요",
          "original_form": "전 모가디슈 유플 나옴 봐야겠어요",
          "speaker_id": "2",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100033647.1.22",
          "form": "전 예고편 보는거 참좋아해요 유튜브에서요 ㅎㅎ",
          "original_form": "전 예고편 보는거 참좋아해요 유튜브에서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100033647.1.23",
          "form": "샹치 기대돼요 마블 좋아해요 ㅎㅎ",
          "original_form": "샹치 기대돼요 마블 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100033647.1.24",
          "form": "아 저는 남편이 마블덕후요~^^",
          "original_form": "아 저는 남편이 마블덕후요~^^",
          "speaker_id": "1",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100033647.1.25",
          "form": "ㅋㅋ저도 덕후요",
          "original_form": "ㅋㅋ저도 덕후요",
          "speaker_id": "2",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100033647.1.26",
          "form": "근데 나이 들어서 세계관이 헷갈리고 막 섞러서 기억이 안나요 ㅠㅠ",
          "original_form": "근데 나이 들어서 세계관이 헷갈리고 막 섞러서 기억이 안나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 20:21"
        },
        {
          "id": "MDRW2100033647.1.27",
          "form": "한국영화 좋아하세요?",
          "original_form": "한국영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 20:26"
        },
        {
          "id": "MDRW2100033647.1.28",
          "form": "전 주로 액션 스릴러 좋아해요",
          "original_form": "전 주로 액션 스릴러 좋아해요",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100033647.1.29",
          "form": "어느 장르 좋아 하세요?",
          "original_form": "어느 장르 좋아 하세요?",
          "speaker_id": "2",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100033647.1.30",
          "form": "전 로맨스 빼곤 다 좋아해요 ㅎㅎ",
          "original_form": "전 로맨스 빼곤 다 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100033647.1.31",
          "form": "영화는 로맨스를 안보게 되네요",
          "original_form": "영화는 로맨스를 안보게 되네요",
          "speaker_id": "2",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100033647.1.32",
          "form": "헉..저는 로맨스를 좋아해요^^ 남편이 시시하다고 싫어한다는",
          "original_form": "헉..저는 로맨스를 좋아해요^^ 남편이 시시하다고 싫어한다는",
          "speaker_id": "1",
