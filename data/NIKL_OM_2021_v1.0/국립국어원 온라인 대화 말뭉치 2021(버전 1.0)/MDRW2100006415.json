      "utterance": [
        {
          "id": "MDRW2100006415.1.1",
          "form": "제주도에서 기억에 남는 음식 있나요",
          "original_form": "제주도에서 기억에 남는 음식 있나요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006415.1.2",
          "form": "어릴때라 잘 기억 안나진 않네요",
          "original_form": "어릴때라 잘 기억 안나진 않네요",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006415.1.3",
          "form": "흑돼지 먹으러 갔던거 같아요",
          "original_form": "흑돼지 먹으러 갔던거 같아요",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006415.1.4",
          "form": "흑돼지..맛있죠 멜젖 아세요?",
          "original_form": "흑돼지..맛있죠 멜젖 아세요?",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006415.1.5",
          "form": "멜젓..",
          "original_form": "멜젓..",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006415.1.6",
          "form": "들어만 봤어요ㅋㅋㅋ",
          "original_form": "들어만 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006415.1.7",
          "form": "흑돼지랑 잘 어울리더라구요 ㅎ",
          "original_form": "흑돼지랑 잘 어울리더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006415.1.8",
          "form": "어떤거에요? 새우젓같은건가요?",
          "original_form": "어떤거에요? 새우젓같은건가요?",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006415.1.9",
          "form": "네 그런식인것 같더라구요 요즘은 마트에서 아예 팔더라구요 ㅎㅎ",
          "original_form": "네 그런식인것 같더라구요 요즘은 마트에서 아예 팔더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006415.1.10",
          "form": "아ㅋㅋ 요샌 안 파는게 없긴 하져",
          "original_form": "아ㅋㅋ 요샌 안 파는게 없긴 하져",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100006415.1.11",
          "form": "마트다면 어떤거 사세요? 먹을거",
          "original_form": "마트다면 어떤거 사세요? 먹을거",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100006415.1.12",
          "form": "마트면 보통 과자나 음료 같은걸 사요. 유제품도 자주 사고요.",
          "original_form": "마트면 보통 과자나 음료 같은걸 사요. 유제품도 자주 사고요.",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006415.1.13",
          "form": "그러시군여 좋아하는.과자 뭔가요",
          "original_form": "그러시군여 좋아하는.과자 뭔가요",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006415.1.14",
          "form": "홈런볼이요ㅋㅋ 얼려먹어두 맛있어요",
          "original_form": "홈런볼이요ㅋㅋ 얼려먹어두 맛있어요",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006415.1.15",
          "form": "맞아요. 약간 빵같기도 하구ㅋㅋ",
          "original_form": "맞아요. 약간 빵같기도 하구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100006415.1.16",
          "form": "어릴땐 홈런볼 약간 고급과자느낌이었어요..ㅎㅎ",
          "original_form": "어릴땐 홈런볼 약간 고급과자느낌이었어요..ㅎㅎ",
