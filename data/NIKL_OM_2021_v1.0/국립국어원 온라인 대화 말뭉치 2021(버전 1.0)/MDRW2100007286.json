      "utterance": [
        {
          "id": "MDRW2100007286.1.1",
          "form": "ㅋㅋ요즘 밥 머해드세요?",
          "original_form": "ㅋㅋ요즘 밥 머해드세요?",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007286.1.2",
          "form": "간계밥이요ㅋㅋㅋㅋ",
          "original_form": "간계밥이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007286.1.3",
          "form": "밥하기 너무귀찮지않나용ㅎ",
          "original_form": "밥하기 너무귀찮지않나용ㅎ",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007286.1.4",
          "form": "양파를 그 뭐죠 중국 소스 그거랑 달달 볶아서 같이 먹으면 간단하고 맛나요",
          "original_form": "양파를 그 뭐죠 중국 소스 그거랑 달달 볶아서 같이 먹으면 간단하고 맛나요",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007286.1.5",
          "form": "ㅇㅇ 그나마 여름 지나서 불 앞에 좀 있을 수 있을듯요ㅋㅋ",
          "original_form": "ㅇㅇ 그나마 여름 지나서 불 앞에 좀 있을 수 있을듯요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007286.1.6",
          "form": "굴소스요?아 내일 그거해먹어야겠어요!",
          "original_form": "굴소스요?아 내일 그거해먹어야겠어요!",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007286.1.7",
          "form": "안그래도 집에 계란만있어서",
          "original_form": "안그래도 집에 계란만있어서",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007286.1.8",
          "form": "아 맞다 굴소스ㅋㅋㅋㅋ 그게 기억이 안났네ㅋㅋㅋㅋ",
          "original_form": "아 맞다 굴소스ㅋㅋㅋㅋ 그게 기억이 안났네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007286.1.9",
          "form": "계란 비싼데 잘 사놓으셨네요ㅋㅋㅋㅋ",
          "original_form": "계란 비싼데 잘 사놓으셨네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007286.1.10",
          "form": "계란 더 올랐더라구요 머선일이고ㅠ",
          "original_form": "계란 더 올랐더라구요 머선일이고ㅠ",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007286.1.11",
          "form": "허얼 더 올랐다구요?",
          "original_form": "허얼 더 올랐다구요?",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007286.1.12",
          "form": "너무하네ㅜㅜ",
          "original_form": "너무하네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007286.1.13",
          "form": "자취하다 알게된 건데 참치캔이 은근 비싸더라구요ㅋㅋㅋ",
          "original_form": "자취하다 알게된 건데 참치캔이 은근 비싸더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007286.1.14",
          "form": "얼마전까지 가끔 동네서 오천원 팔았는데 요즘 세일이 칠천원이더라구요ㅠ",
          "original_form": "얼마전까지 가끔 동네서 오천원 팔았는데 요즘 세일이 칠천원이더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007286.1.15",
          "form": "헐 서른개에요?",
          "original_form": "헐 서른개에요?",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007286.1.16",
          "form": "참치비싸죠ㅠ 잘안먹어요",
          "original_form": "참치비싸죠ㅠ 잘안먹어요",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007286.1.17",
          "form": "그럼 갠춘한 거 같은데",
          "original_form": "그럼 갠춘한 거 같은데",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007286.1.18",
          "form": "네네 서른개",
          "original_form": "네네 서른개",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007286.1.19",
          "form": "그래용?우리동네가 싼건가",
          "original_form": "그래용?우리동네가 싼건가",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007286.1.20",
          "form": "저 한 열흘전이었나... 마넌이었어요 여긴 흙",
          "original_form": "저 한 열흘전이었나... 마넌이었어요 여긴 흙",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007286.1.21",
          "form": "그 사이에 그나마 내린거면 좋겠어요ㅋㅋ",
          "original_form": "그 사이에 그나마 내린거면 좋겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007286.1.22",
          "form": "앗 만원은 한번도 본적없어요ㅠ 우리동네가 싼가봐요",
          "original_form": "앗 만원은 한번도 본적없어요ㅠ 우리동네가 싼가봐요",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007286.1.23",
          "form": "이런 저 사는 데 안되겠어요ㅋㅋㅋㅋ",
          "original_form": "이런 저 사는 데 안되겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007286.1.24",
          "form": "계란 사러 원정 가야할까봐요ㅋㅋㅋ",
          "original_form": "계란 사러 원정 가야할까봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007286.1.25",
          "form": "오셔용ㅎ 차비가 더들려나",
          "original_form": "오셔용ㅎ 차비가 더들려나",
          "speaker_id": "1",
          "time": "20210822 01:19"
        }
      ]
    }
  ]
