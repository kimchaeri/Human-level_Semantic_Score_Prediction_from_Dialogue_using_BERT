      "utterance": [
        {
          "id": "MDRW2100022018.1.1",
          "form": "오늘 저녁 드셨나요?",
          "original_form": "오늘 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100022018.1.2",
          "form": "네! 닭갈비 먹었어요",
          "original_form": "네! 닭갈비 먹었어요",
          "speaker_id": "2",
          "time": "20210829 00:59"
        },
        {
          "id": "MDRW2100022018.1.3",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210829 00:59"
        },
        {
          "id": "MDRW2100022018.1.4",
          "form": "오늘 고기랑 스파게티 먹었어요",
          "original_form": "오늘 고기랑 스파게티 먹었어요",
          "speaker_id": "1",
          "time": "20210829 00:59"
        },
        {
          "id": "MDRW2100022018.1.5",
          "form": "닭갈비 직접 해드셨나요?",
          "original_form": "닭갈비 직접 해드셨나요?",
          "speaker_id": "1",
          "time": "20210829 00:59"
        },
        {
          "id": "MDRW2100022018.1.6",
          "form": "배민으로 시켜먹었죠ㅎㅎ",
          "original_form": "배민으로 시켜먹었죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:01"
        },
        {
          "id": "MDRW2100022018.1.7",
          "form": "고기랑 스파게티 맛있는 조합이네요ㅜㅜ",
          "original_form": "고기랑 스파게티 맛있는 조합이네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100022018.1.8",
          "form": "네 오랜만에 먹었더니 맛있더라구요!",
          "original_form": "네 오랜만에 먹었더니 맛있더라구요!",
          "speaker_id": "1",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100022018.1.9",
          "form": "배달 자주 시키세요?",
          "original_form": "배달 자주 시키세요?",
          "speaker_id": "1",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100022018.1.10",
          "form": "넹넹 이제 적당히 시켜먹으려구요ㅜㅠ",
          "original_form": "넹넹 이제 적당히 시켜먹으려구요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100022018.1.11",
          "form": "배달이 너무 편해서 그런지 자주 시키게 되더라구요ㅠㅠ",
          "original_form": "배달이 너무 편해서 그런지 자주 시키게 되더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100022018.1.12",
          "form": "어떤 음식 자주 주문하시나요?",
          "original_form": "어떤 음식 자주 주문하시나요?",
          "speaker_id": "1",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022018.1.13",
          "form": "저는 자바칩 자주 주문하는 편이에용",
          "original_form": "저는 자바칩 자주 주문하는 편이에용",
          "speaker_id": "2",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100022018.1.14",
          "form": "너무 맛있어서 끊기가 힘드네요ㅜㅜ",
          "original_form": "너무 맛있어서 끊기가 힘드네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100022018.1.15",
          "form": "name1은 어떤 음식 자주 주문하시나요?",
          "original_form": "&name1&은 어떤 음식 자주 주문하시나요?",
          "speaker_id": "2",
          "time": "20210829 01:12"
        },
        {
          "id": "MDRW2100022018.1.16",
          "form": "저는 치킨,피자 주로 주문하는데",
          "original_form": "저는 치킨,피자 주로 주문하는데",
          "speaker_id": "1",
          "time": "20210829 01:40"
        },
        {
          "id": "MDRW2100022018.1.17",
          "form": "요즘엔 카페 많이 시키게 되더라구요!",
          "original_form": "요즘엔 카페 많이 시키게 되더라구요!",
          "speaker_id": "1",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022018.1.18",
          "form": "카페 자주 가시나요?",
          "original_form": "카페 자주 가시나요?",
          "speaker_id": "1",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022018.1.19",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210829 01:46"
        },
        {
          "id": "MDRW2100022018.1.20",
          "form": "일하고나면 피곤해서 계속 집에만 있게 되더라구요ㅎ ㅎ",
          "original_form": "일하고나면 피곤해서 계속 집에만 있게 되더라구요ㅎ ㅎ",
          "speaker_id": "2",
          "time": "20210829 01:46"
        },
        {
          "id": "MDRW2100022018.1.21",
          "form": "아 맞아요....",
          "original_form": "아 맞아요....",
          "speaker_id": "1",
          "time": "20210829 01:46"
        },
        {
          "id": "MDRW2100022018.1.22",
          "form": "퇴근후엔 무조건 집...",
          "original_form": "퇴근후엔 무조건 집...",
          "speaker_id": "1",
          "time": "20210829 01:46"
        },
        {
          "id": "MDRW2100022018.1.23",
          "form": "카페 가면 주로 뭐드시나요?",
          "original_form": "카페 가면 주로 뭐드시나요?",
          "speaker_id": "1",
          "time": "20210829 01:47"
        },
        {
          "id": "MDRW2100022018.1.24",
          "form": "저는 커피를 잘 못마셔서 아이스티나 에이드 종류로 많이 마셔요!",
          "original_form": "저는 커피를 잘 못마셔서 아이스티나 에이드 종류로 많이 마셔요!",
          "speaker_id": "2",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022018.1.25",
          "form": "오 저도 커피 잘안마셔서 에이드 종류 자주 마셔요",
          "original_form": "오 저도 커피 잘안마셔서 에이드 종류 자주 마셔요",
          "speaker_id": "1",
          "time": "20210829 09:20"
        },
        {
          "id": "MDRW2100022018.1.26",
          "form": "무슨 에이드 좋아하세요?",
          "original_form": "무슨 에이드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 09:20"
        },
        {
          "id": "MDRW2100022018.1.27",
          "form": "저는 청포도 좋아해요!",
          "original_form": "저는 청포도 좋아해요!",
          "speaker_id": "2",
