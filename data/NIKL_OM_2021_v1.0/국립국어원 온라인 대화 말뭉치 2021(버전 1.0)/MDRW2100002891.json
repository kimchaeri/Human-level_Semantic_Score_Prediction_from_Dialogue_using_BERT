      "utterance": [
        {
          "id": "MDRW2100002891.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210821 12:53"
        },
        {
          "id": "MDRW2100002891.1.2",
          "form": "네 빨래 설겆이 다하죠 청소",
          "original_form": "네 빨래 설겆이 다하죠 청소",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100002891.1.3",
          "form": "가장하기 싫은 집안일 있으세요?",
          "original_form": "가장하기 싫은 집안일 있으세요?",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100002891.1.4",
          "form": "청소가 가장 힘들죠",
          "original_form": "청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100002891.1.5",
          "form": "로봇청소기가 있어서 나은데 화장실청소는 진짜 하기싫은거 같아요",
          "original_form": "로봇청소기가 있어서 나은데 화장실청소는 진짜 하기싫은거 같아요",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100002891.1.6",
          "form": "아 락스 힘들죠",
          "original_form": "아 락스 힘들죠",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100002891.1.7",
          "form": "네 머리카락같은것도 싫구요",
          "original_form": "네 머리카락같은것도 싫구요",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100002891.1.8",
          "form": "네 그것도ㅠ일이네여",
          "original_form": "네 그것도ㅠ일이네여",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100002891.1.9",
          "form": "안하면 냄새가..",
          "original_form": "안하면 냄새가..",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100002891.1.10",
          "form": "냄새도 냄새지만 때가 끼이는게 보여서 안할수가 없죠",
          "original_form": "냄새도 냄새지만 때가 끼이는게 보여서 안할수가 없죠",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100002891.1.11",
          "form": "눈이 맞아요",
          "original_form": "눈이 맞아요",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100002891.1.12",
          "form": "맞아요 손이 가게되죠",
          "original_form": "맞아요 손이 가게되죠",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100002891.1.13",
          "form": "어쩔수 없이",
          "original_form": "어쩔수 없이",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100002891.1.14",
          "form": "네네",
          "original_form": "네네",
