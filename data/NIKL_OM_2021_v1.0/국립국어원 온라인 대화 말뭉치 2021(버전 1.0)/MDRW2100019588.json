      "utterance": [
        {
          "id": "MDRW2100019588.1.1",
          "form": "요즘 날씨가 선선하네요~",
          "original_form": "요즘 날씨가 선선하네요~",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019588.1.2",
          "form": "날이좋네요~~~",
          "original_form": "날이좋네요~~~",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019588.1.3",
          "form": "오늘 반팔 입고 나가셨어요?",
          "original_form": "오늘 반팔 입고 나가셨어요?",
          "speaker_id": "1",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019588.1.4",
          "form": "음 가디건들구 나갔어여!",
          "original_form": "음 가디건들구 나갔어여!",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019588.1.5",
          "form": "잘 챙겨가셨네요 ㅜㅜ",
          "original_form": "잘 챙겨가셨네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019588.1.6",
          "form": "전 반팔 입고 나갔더니 좀 으슬으슬 추웠어요",
          "original_form": "전 반팔 입고 나갔더니 좀 으슬으슬 추웠어요",
          "speaker_id": "1",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019588.1.7",
          "form": "아후 날씨가 진짜",
          "original_form": "아후 날씨가 진짜",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019588.1.8",
          "form": "오락가락 ㅠㅠㅠ",
          "original_form": "오락가락 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019588.1.9",
          "form": "대구는 오늘 갑자기 비가와서 ㅠㅠ",
          "original_form": "대구는 오늘 갑자기 비가와서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019588.1.10",
          "form": "허걱 우산은 챙겼어요?",
          "original_form": "허걱 우산은 챙겼어요?",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019588.1.11",
          "form": "아 물론이죠! 항상 가방에 넣어다녀요 ㅠㅠ",
          "original_form": "아 물론이죠! 항상 가방에 넣어다녀요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019588.1.12",
          "form": "단우산이요?",
          "original_form": "단우산이요?",
          "speaker_id": "1",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019588.1.13",
          "form": "네네 접는우산",
          "original_form": "네네 접는우산",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019588.1.14",
          "form": "현명하시네요.",
          "original_form": "현명하시네요.",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019588.1.15",
          "form": "근데 비 안오는 날이 더 많은데 번거롭지는 않아요?",
          "original_form": "근데 비 안오는 날이 더 많은데 번거롭지는 않아요?",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019588.1.16",
          "form": "언제 비가올지 모르니까,,,",
          "original_form": "언제 비가올지 모르니까,,,",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019588.1.17",
          "form": "음..그래도 우산 막 새로사고",
          "original_form": "음..그래도 우산 막 새로사고",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019588.1.18",
          "form": "그러는것보단",
          "original_form": "그러는것보단",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019588.1.19",
          "form": "가방에 넣어두는게 편하다 생각해서 그래요!",
          "original_form": "가방에 넣어두는게 편하다 생각해서 그래요!",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019588.1.20",
          "form": "어.. 그런 생각해본 적이 없는데 진짜 그러네요.",
          "original_form": "어.. 그런 생각해본 적이 없는데 진짜 그러네요.",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019588.1.21",
          "form": "저는 비가 오면 그냥 비 맞고 가는 편이에요 ㅋㅋㅋ",
          "original_form": "저는 비가 오면 그냥 비 맞고 가는 편이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019588.1.22",
          "form": "아아아 그럴수도있져!!!!",
          "original_form": "아아아 그럴수도있져!!!!",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019588.1.23",
          "form": "사람마다 성격이다르니까",
          "original_form": "사람마다 성격이다르니까",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019588.1.24",
          "form": "비오는 날 단우산 젖어있으면 가방에 넣기도 뭐하고.. 보관하기도 힘들지 않아요?",
          "original_form": "비오는 날 단우산 젖어있으면 가방에 넣기도 뭐하고.. 보관하기도 힘들지 않아요?",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019588.1.25",
          "form": "아아 그건그래여!!!",
          "original_form": "아아 그건그래여!!!",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019588.1.26",
          "form": "그래서 집에서 말리고 다시 가방에넣고,,,",
          "original_form": "그래서 집에서 말리고 다시 가방에넣고,,,",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019588.1.27",
          "form": "그렇죠뭐",
          "original_form": "그렇죠뭐",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019588.1.28",
          "form": "보통 음식점이나 쇼핑몰가면 우산꽂이에넣었다가",
          "original_form": "보통 음식점이나 쇼핑몰가면 우산꽂이에넣었다가",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019588.1.29",
          "form": "ㅠㅠ 가게에 우산꽂이 넣는 것도 짜증나요",
          "original_form": "ㅠㅠ 가게에 우산꽂이 넣는 것도 짜증나요",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019588.1.30",
          "form": "장우산들 틈에 묻혀가지고 꺼내기도 힘들고..",
          "original_form": "장우산들 틈에 묻혀가지고 꺼내기도 힘들고..",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019588.1.31",
          "form": "아 맞아여 ㅠㅠ 다 젖어있고",
          "original_form": "아 맞아여 ㅠㅠ 다 젖어있고",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019588.1.32",
          "form": "특히 손잡이 젖어 있으면 너무 끔찍해요 ㅜㅜ",
          "original_form": "특히 손잡이 젖어 있으면 너무 끔찍해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019588.1.33",
          "form": "아 그죠 ㅠㅠ",
          "original_form": "아 그죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019588.1.34",
          "form": "그래도 겨울에 눈 오는날엔 단우산 짱짱!",
          "original_form": "그래도 겨울에 눈 오는날엔 단우산 짱짱!",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019588.1.35",
          "form": "맞아여 퐁퐁!!!",
          "original_form": "맞아여 퐁퐁!!!",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019588.1.36",
          "form": "대구는 눈이 잘안와서 ㅠㅠ",
          "original_form": "대구는 눈이 잘안와서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019588.1.37",
          "form": "부산은 안오는 건 알았지만 대구도 안오는 편이에요?",
          "original_form": "부산은 안오는 건 알았지만 대구도 안오는 편이에요?",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019588.1.38",
          "form": "부산이랑 비슷해요!",
          "original_form": "부산이랑 비슷해요!",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019588.1.39",
          "form": "대신 겨울에 따뜻해서 좋겠네요~",
          "original_form": "대신 겨울에 따뜻해서 좋겠네요~",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019588.1.40",
          "form": "부산두요!!@",
          "original_form": "부산두요!!@",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019588.1.41",
          "form": "눈 때문에 차 막힐 일도 없잖아요 ㅎㅎ",
          "original_form": "눈 때문에 차 막힐 일도 없잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019588.1.42",
          "form": "그쵸 예전에 겨울에 부산여행 갔을 때 좋았어요",
          "original_form": "그쵸 예전에 겨울에 부산여행 갔을 때 좋았어요",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019588.1.43",
          "form": "아아아아!!",
          "original_form": "아아아아!!",
          "speaker_id": "2",
          "time": "20210827 20:55"
        }
      ]
    }
