      "utterance": [
        {
          "id": "MDRW2100043560.1.1",
          "form": "신기생뎐은 나도 봄 ㅋㅋㅋ",
          "original_form": "신기생뎐은 나도 봄 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043560.1.2",
          "form": "임성한 작가자나요ㅋ",
          "original_form": "임성한 작가자나요ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043560.1.3",
          "form": "여기서 성훈이 임성한작가꺼 데뷔한거지",
          "original_form": "여기서 성훈이 임성한작가꺼 데뷔한거지",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043560.1.4",
          "form": "그때랑  얼굴이 틀림ㅋㅋ",
          "original_form": "그때랑  얼굴이 틀림ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043560.1.5",
          "form": "ㅋㅋ조금 사진이 암울하다",
          "original_form": "ㅋㅋ조금 사진이 암울하다",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043560.1.6",
          "form": "근데 박윤재 나온지는 전혀 몰랗음",
          "original_form": "근데 박윤재 나온지는 전혀 몰랗음",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043560.1.7",
          "form": "임수향은똑같네",
          "original_form": "임수향은똑같네",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043560.1.8",
          "form": "그러네요 근데 수술한거 같음",
          "original_form": "그러네요 근데 수술한거 같음",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043560.1.9",
          "form": "나두 어디에서 나왔지 열심히 본건데도 모름",
          "original_form": "나두 어디에서 나왔지 열심히 본건데도 모름",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043560.1.10",
          "form": "그니깐요 조연인데 조금 나왔나 봐요",
          "original_form": "그니깐요 조연인데 조금 나왔나 봐요",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043560.1.11",
          "form": "성훈도 생각보다 출연작이 좀 잇을듯하네",
          "original_form": "성훈도 생각보다 출연작이 좀 잇을듯하네",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043560.1.12",
          "form": "그러게요 시청률도 28프로",
          "original_form": "그러게요 시청률도 28프로",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043560.1.13",
          "form": "그리고 불굴의 며느리에서 주연으로 나오네",
          "original_form": "그리고 불굴의 며느리에서 주연으로 나오네",
