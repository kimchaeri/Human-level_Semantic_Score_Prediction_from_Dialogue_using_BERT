      "utterance": [
        {
          "id": "MDRW2100015484.1.1",
          "form": "일은 어때요? 스트레스 많이 받으시죠?",
          "original_form": "일은 어때요? 스트레스 많이 받으시죠?",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015484.1.2",
          "form": "일은 항상 그렇죠ㅋㅋ\r\n요새 일 많이 바쁘신가봐요",
          "original_form": "일은 항상 그렇죠ㅋㅋ\r\n요새 일 많이 바쁘신가봐요",
          "speaker_id": "2",
          "time": "20210826 08:20"
        },
        {
          "id": "MDRW2100015484.1.3",
          "form": "일은 늘바뻐요ㅠ",
          "original_form": "일은 늘바뻐요ㅠ",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100015484.1.4",
          "form": "진짜 돈을쬐끔주면서ㅎㅎ",
          "original_form": "진짜 돈을쬐끔주면서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100015484.1.5",
          "form": "업무 스트레스 많이받으세요?",
          "original_form": "업무 스트레스 많이받으세요?",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100015484.1.6",
          "form": "ㅜㅠ업무스트레스는 다있죠",
          "original_form": "ㅜㅠ업무스트레스는 다있죠",
          "speaker_id": "2",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100015484.1.7",
          "form": "아 다 비슷하죠ㅜㅜ",
          "original_form": "아 다 비슷하죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100015484.1.8",
          "form": "아무래도 그렇죠ㅍㅍ",
          "original_form": "아무래도 그렇죠ㅍㅍ",
          "speaker_id": "1",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100015484.1.9",
          "form": "죄송요ㅎㅎ 어제쓰러짐ㅎㅎ 일에취해서ㅎ",
          "original_form": "죄송요ㅎㅎ 어제쓰러짐ㅎㅎ 일에취해서ㅎ",
          "speaker_id": "1",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100015484.1.10",
          "form": "아아 아니에요 저도 쓰러져서ㅋㅋㅋㅋㅋ",
          "original_form": "아아 아니에요 저도 쓰러져서ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100015484.1.11",
          "form": "야근하셨나봐요",
          "original_form": "야근하셨나봐요",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100015484.1.12",
          "form": "오늘도 야근인듯요ㅎㅎ",
          "original_form": "오늘도 야근인듯요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100015484.1.13",
          "form": "일을 이렇게 열씸히해야하는데.. 잘안되네요ㅎㅎ",
          "original_form": "일을 이렇게 열씸히해야하는데.. 잘안되네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100015484.1.14",
          "form": "ㅋㅋ일은 항상 그렇죠",
          "original_form": "ㅋㅋ일은 항상 그렇죠",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100015484.1.15",
          "form": "일에대한 스트레스를 수다로 푸는중입니당ㅎㅎ",
          "original_form": "일에대한 스트레스를 수다로 푸는중입니당ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100015484.1.16",
          "form": "스트레스 어떻게푸세요?",
          "original_form": "스트레스 어떻게푸세요?",
          "speaker_id": "1",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100015484.1.17",
          "form": "저는 단거먹거나 자요ㅋㅋㅋㅋ수다떨기도 하고ㅋㅋㅋㄱ",
          "original_form": "저는 단거먹거나 자요ㅋㅋㅋㅋ수다떨기도 하고ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210829 19:50"
        },
        {
          "id": "MDRW2100015484.1.18",
          "form": "단거는 초코?",
          "original_form": "단거는 초코?",
          "speaker_id": "1",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100015484.1.19",
          "form": "\r\n단거는 젤리나 새콤달콤을ㅋㅋㅋ",
          "original_form": "\r\n단거는 젤리나 새콤달콤을ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:51"
        },
        {
          "id": "MDRW2100015484.1.20",
          "form": "아아ㅎㅎ",
          "original_form": "아아ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100015484.1.21",
          "form": "어머님이랑 같이사세요?",
          "original_form": "어머님이랑 같이사세요?",
          "speaker_id": "1",
          "time": "20210829 19:54"
        },
        {
          "id": "MDRW2100015484.1.22",
          "form": "좋죠ㅎㅎ 전 초코매니아라ㅎㅎ 커피도 초코ㅎ",
          "original_form": "좋죠ㅎㅎ 전 초코매니아라ㅎㅎ 커피도 초코ㅎ",
