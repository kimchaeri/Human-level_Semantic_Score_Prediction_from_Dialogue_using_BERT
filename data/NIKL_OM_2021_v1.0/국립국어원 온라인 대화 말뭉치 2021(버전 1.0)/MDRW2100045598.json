      "utterance": [
        {
          "id": "MDRW2100045598.1.1",
          "form": "네네 근데 가격이 너무.. ㅠㅠ",
          "original_form": "네네 근데 가격이 너무.. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045598.1.2",
          "form": "육포 좋아하세요?",
          "original_form": "육포 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045598.1.3",
          "form": "맞아요 육포 비싸요 ㅠㅠ",
          "original_form": "맞아요 육포 비싸요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045598.1.4",
          "form": "전 마른오징어랑 쥐포도 좋아해요",
          "original_form": "전 마른오징어랑 쥐포도 좋아해요",
          "speaker_id": "1",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045598.1.5",
          "form": "그춍.. 마카오에 육포가 진짜 싸거든요",
          "original_form": "그춍.. 마카오에 육포가 진짜 싸거든요",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045598.1.6",
          "form": "에이퍼 종이 한장보다 큰데 만원 정도 해요.. 하 갑자기 생각나네요..",
          "original_form": "에이퍼 종이 한장보다 큰데 만원 정도 해요.. 하 갑자기 생각나네요..",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045598.1.7",
          "form": "오징어도 맛있쥬",
          "original_form": "오징어도 맛있쥬",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045598.1.8",
          "form": "아니 거긴 육포가 왜그렇게 싸요",
          "original_form": "아니 거긴 육포가 왜그렇게 싸요",
          "speaker_id": "1",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045598.1.9",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "1",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045598.1.10",
          "form": "저도 의문이에요 ㅋㅋㅋ 근데 대박 맛있어요..",
          "original_form": "저도 의문이에요 ㅋㅋㅋ 근데 대박 맛있어요..",
          "speaker_id": "2",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045598.1.11",
          "form": "ㅠㅠㅜ육포랑 맥주....",
          "original_form": "ㅠㅠㅜ육포랑 맥주....",
          "speaker_id": "1",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045598.1.12",
          "form": "근데 육포는 반입이 안되더라고요 입국할때 ㅋㅋㅋ",
          "original_form": "근데 육포는 반입이 안되더라고요 입국할때 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045598.1.13",
          "form": "아니 왜요?",
          "original_form": "아니 왜요?",
          "speaker_id": "1",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045598.1.14",
          "form": "뭐 동물뭐시기 어쩌구 법 이라던데 저도 잘 모르겠어요 ㅋㅋㅋ",
          "original_form": "뭐 동물뭐시기 어쩌구 법 이라던데 저도 잘 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045598.1.15",
          "form": "음식관련 법인가 암튼.. ㅠ",
          "original_form": "음식관련 법인가 암튼.. ㅠ",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045598.1.16",
          "form": "ㅠㅠ언젠가 가게되면 한가득 싸오려햇더니",
          "original_form": "ㅠㅠ언젠가 가게되면 한가득 싸오려햇더니",
          "speaker_id": "1",
          "time": "20210908 04:04"
        },
        {
          "id": "MDRW2100045598.1.17",
          "form": "그르니까요 그래서 공항에서 뜯어먹었어요 엄마랑.. 지금 생각하니 넘 웃기네요 ㅋㅋ",
          "original_form": "그르니까요 그래서 공항에서 뜯어먹었어요 엄마랑.. 지금 생각하니 넘 웃기네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:04"
        },
        {
          "id": "MDRW2100045598.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ잊지 못할 기억이겟네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ잊지 못할 기억이겟네요",
          "speaker_id": "1",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100045598.1.19",
          "form": "네 맨날 거기 육포 맛있었다고 회상해요 ㅋㅋ",
          "original_form": "네 맨날 거기 육포 맛있었다고 회상해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045598.1.20",
          "form": "혹시 못드시는 음식 얘기했던가요 ㅋㅋㅋ",
          "original_form": "혹시 못드시는 음식 얘기했던가요 ㅋㅋㅋ",
