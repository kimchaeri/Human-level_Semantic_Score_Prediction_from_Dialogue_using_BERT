      "utterance": [
        {
          "id": "MDRW2100042812.1.1",
          "form": "열심히 하고",
          "original_form": "열심히 하고",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042812.1.2",
          "form": "저녁에 다시 만나자",
          "original_form": "저녁에 다시 만나자",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042812.1.3",
          "form": "출근하면 살 수 있어요",
          "original_form": "출근하면 살 수 있어요",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042812.1.4",
          "form": "할 수 있어요",
          "original_form": "할 수 있어요",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042812.1.5",
          "form": "난 지금 깔딱고개를 지나가고 있어요",
          "original_form": "난 지금 깔딱고개를 지나가고 있어요",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042812.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋ 힘내라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋ 힘내라",
          "speaker_id": "2",
          "time": "20210907 09:30"
        },
        {
          "id": "MDRW2100042812.1.7",
          "form": "비오나??",
          "original_form": "비오나??",
          "speaker_id": "2",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042812.1.8",
          "form": "원 우산 챙겨보냈는데",
          "original_form": "원 우산 챙겨보냈는데",
          "speaker_id": "2",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042812.1.9",
          "form": "안가져오진 않겠지??",
          "original_form": "안가져오진 않겠지??",
          "speaker_id": "2",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042812.1.10",
          "form": "비는 안오는데 춥네요",
          "original_form": "비는 안오는데 춥네요",
          "speaker_id": "1",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042812.1.11",
          "form": "지금 안 와서 안 가져올 거 같은데",
          "original_form": "지금 안 와서 안 가져올 거 같은데",
          "speaker_id": "1",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042812.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042812.1.13",
          "form": "비가 와 야 우산을 쓰고 오는 거 아니에요",
          "original_form": "비가 와 야 우산을 쓰고 오는 거 아니에요",
          "speaker_id": "1",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042812.1.14",
          "form": "춥나??",
          "original_form": "춥나??",
          "speaker_id": "2",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042812.1.15",
          "form": "반팔 입어도 되겠지??",
          "original_form": "반팔 입어도 되겠지??",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042812.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 비올거같길래",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 비올거같길래",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042812.1.17",
          "form": "언니 내가 추우면 진짜 추운 거예요",
          "original_form": "언니 내가 추우면 진짜 추운 거예요",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042812.1.18",
          "form": "애들 반팔 원피스 입혀 보냈는데",
          "original_form": "애들 반팔 원피스 입혀 보냈는데",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042812.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그렇네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 그렇네",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042812.1.20",
          "form": "원도 반팔 입혀가고",
          "original_form": "원도 반팔 입혀가고",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042812.1.21",
          "form": "투도 원피스 입고 갔는데",
          "original_form": "투도 원피스 입고 갔는데",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042812.1.22",
          "form": "춥다니...ㄷㄷㄷ",
          "original_form": "춥다니...ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042812.1.23",
          "form": "그니까요 나 중앙공원 쭈 모 믄 땀나는데",
          "original_form": "그니까요 나 중앙공원 쭈 모 믄 땀나는데",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042812.1.24",
          "form": "팔에 닭살 돋았음",
          "original_form": "팔에 닭살 돋았음",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042812.1.25",
          "form": "긴 옷이제 꺼내놔야 되겠어요",
          "original_form": "긴 옷이제 꺼내놔야 되겠어요",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042812.1.26",
          "form": "헐 그마이??",
          "original_form": "헐 그마이??",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042812.1.27",
          "form": "맞네",
          "original_form": "맞네",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042812.1.28",
          "form": "근데 내가 긴 옷을 꺼내면 다시 더워질 듯",
          "original_form": "근데 내가 긴 옷을 꺼내면 다시 더워질 듯",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042812.1.29",
          "form": "이제 긴.팔. 이 표본 해야겠다",
          "original_form": "이제 긴.팔. 이 표본 해야겠다",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042812.1.30",
          "form": "입혀본",
          "original_form": "입혀본",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042812.1.31",
          "form": "잘 좀 해라",
          "original_form": "잘 좀 해라",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042812.1.32",
          "form": "아 웃겨",
          "original_form": "아 웃겨",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042812.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:40"
        }
      ]
