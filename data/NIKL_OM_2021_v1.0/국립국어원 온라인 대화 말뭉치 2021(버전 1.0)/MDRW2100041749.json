      "utterance": [
        {
          "id": "MDRW2100041749.1.1",
          "form": "저는 트램폴린 한번 해보고싶더라구요 ㅋㅋ",
          "original_form": "저는 트램폴린 한번 해보고싶더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:22"
        },
        {
          "id": "MDRW2100041749.1.2",
          "form": "전에는 사실 막 하는 운동이 그렇게 많지 않았어요 ㅁ",
          "original_form": "전에는 사실 막 하는 운동이 그렇게 많지 않았어요 ㅁ",
          "speaker_id": "2",
          "time": "20210906 15:23"
        },
        {
          "id": "MDRW2100041749.1.3",
          "form": "우리동네에서는 방방이라고 불렀는데요 ㅎㅎ",
          "original_form": "우리동네에서는 방방이라고 불렀는데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041749.1.4",
          "form": "저는 운동을 좋아해서 볼링도 배워서 몇년했었고",
          "original_form": "저는 운동을 좋아해서 볼링도 배워서 몇년했었고",
          "speaker_id": "1",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041749.1.5",
          "form": "맞아요 ㅋㅋㅋ 방방잌",
          "original_form": "맞아요 ㅋㅋㅋ 방방잌",
          "speaker_id": "2",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041749.1.6",
          "form": "수영배워서 한 10년 넘게 했어요",
          "original_form": "수영배워서 한 10년 넘게 했어요",
          "speaker_id": "1",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041749.1.7",
          "form": "볼링 재미있죠 ㅋㅋ",
          "original_form": "볼링 재미있죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041749.1.8",
          "form": "와 수영 오래하셨네요",
          "original_form": "와 수영 오래하셨네요",
          "speaker_id": "2",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041749.1.9",
          "form": "근데 작년부터 코로나로 수영장에 못가게 됐네요",
          "original_form": "근데 작년부터 코로나로 수영장에 못가게 됐네요",
          "speaker_id": "1",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041749.1.10",
          "form": "맞아요 ㅠㅠ 코로나때문에 못가죠ㅠ",
          "original_form": "맞아요 ㅠㅠ 코로나때문에 못가죠ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100041749.1.11",
          "form": "저도 수영배우고싶었는데",
          "original_form": "저도 수영배우고싶었는데",
          "speaker_id": "2",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041749.1.12",
          "form": "결국 못배웠네요",
          "original_form": "결국 못배웠네요",
          "speaker_id": "2",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041749.1.13",
          "form": "자유영부터 접영까지 1년정도면 배우니까",
          "original_form": "자유영부터 접영까지 1년정도면 배우니까",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041749.1.14",
          "form": "코로나 끝나면 한번 배워보세요",
          "original_form": "코로나 끝나면 한번 배워보세요",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041749.1.15",
          "form": "나중에 도움 될 일이 생길 수도 있어요",
          "original_form": "나중에 도움 될 일이 생길 수도 있어요",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041749.1.16",
          "form": "네네 한번 코로나끝나면",
          "original_form": "네네 한번 코로나끝나면",
          "speaker_id": "2",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041749.1.17",
          "form": "배워봐야되겠어요",
          "original_form": "배워봐야되겠어요",
          "speaker_id": "2",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041749.1.18",
          "form": "수영잘하고싶었는데 ㅋㅋ",
          "original_form": "수영잘하고싶었는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041749.1.19",
          "form": "수영은 실제 물에 빠졌을 때도 도움이 되니까요",
          "original_form": "수영은 실제 물에 빠졌을 때도 도움이 되니까요",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041749.1.20",
          "form": "일종의 기술이죠",
          "original_form": "일종의 기술이죠",
          "speaker_id": "1",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041749.1.21",
          "form": "네네 ㅋㅋ 그렇죠 ㅋㅋ",
          "original_form": "네네 ㅋㅋ 그렇죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041749.1.22",
          "form": "정말 옛날에 동생이 물에 빠진적있었는데 구해준다고 들어갔다가 같이 빠져서 ㅋㅋㅋ 정말 식겁했어요",
          "original_form": "정말 옛날에 동생이 물에 빠진적있었는데 구해준다고 들어갔다가 같이 빠져서 ㅋㅋㅋ 정말 식겁했어요",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041749.1.23",
          "form": "그러니까요 함부로 들어가면 안돼요",
          "original_form": "그러니까요 함부로 들어가면 안돼요",
          "speaker_id": "1",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041749.1.24",
          "form": "둘다 위험해져요",
          "original_form": "둘다 위험해져요",
          "speaker_id": "1",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041749.1.25",
          "form": "네 진짜 죽을뻔했어요 겨우 나왔어요 ㅋ",
          "original_form": "네 진짜 죽을뻔했어요 겨우 나왔어요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 15:31"
        }
      ]
    }
