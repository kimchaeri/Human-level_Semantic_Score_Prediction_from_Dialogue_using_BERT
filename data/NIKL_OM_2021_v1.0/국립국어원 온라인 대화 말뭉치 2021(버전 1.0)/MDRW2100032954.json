      "utterance": [
        {
          "id": "MDRW2100032954.1.1",
          "form": "책 좋아함?",
          "original_form": "책 좋아함?",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100032954.1.2",
          "form": "가끔 읽음 요즘 못읽음",
          "original_form": "가끔 읽음 요즘 못읽음",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100032954.1.3",
          "form": "그냥 잡다한거",
          "original_form": "그냥 잡다한거",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100032954.1.4",
          "form": "님은",
          "original_form": "님은",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100032954.1.5",
          "form": "나는 덕후였는데 요즘은 못읽음 ㅜㅜ",
          "original_form": "나는 덕후였는데 요즘은 못읽음 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100032954.1.6",
          "form": "어디쪽 좋아함",
          "original_form": "어디쪽 좋아함",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100032954.1.7",
          "form": "바빠서?",
          "original_form": "바빠서?",
          "speaker_id": "2",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100032954.1.8",
          "form": "나는 거의 소설만 읽음 !! 바쁘기도하고 손에 잘 안잡힘 ㅜㅜ",
          "original_form": "나는 거의 소설만 읽음 !! 바쁘기도하고 손에 잘 안잡힘 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100032954.1.9",
          "form": "책 읽기가 쉽지않음",
          "original_form": "책 읽기가 쉽지않음",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100032954.1.10",
          "form": "펼치면 읽는데",
          "original_form": "펼치면 읽는데",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100032954.1.11",
          "form": "펼치지를 않음..",
          "original_form": "펼치지를 않음..",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100032954.1.12",
          "form": "어 완전 공감 나도 펼치면 그자리에서 읽는데 그게 어려움",
          "original_form": "어 완전 공감 나도 펼치면 그자리에서 읽는데 그게 어려움",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100032954.1.13",
          "form": "운동도 그럼",
          "original_form": "운동도 그럼",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100032954.1.14",
          "form": "그래서 그냥 머리 비우고해야함",
          "original_form": "그래서 그냥 머리 비우고해야함",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100032954.1.15",
          "form": "잡생각하면 내적갈등 생겨서",
          "original_form": "잡생각하면 내적갈등 생겨서",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100032954.1.16",
          "form": "귀찮아짐",
          "original_form": "귀찮아짐",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100032954.1.17",
          "form": "그래서 생각안하고 함",
          "original_form": "그래서 생각안하고 함",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100032954.1.18",
          "form": "오 동감 걍 우선 하고봐야함",
          "original_form": "오 동감 걍 우선 하고봐야함",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100032954.1.19",
          "form": "맞음",
          "original_form": "맞음",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100032954.1.20",
          "form": "근데 하는게 쉽지 않음",
          "original_form": "근데 하는게 쉽지 않음",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100032954.1.21",
          "form": "알면서 안함",
          "original_form": "알면서 안함",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100032954.1.22",
          "form": "맞음 맘먹기가 점점힘들어짐",
          "original_form": "맞음 맘먹기가 점점힘들어짐",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100032954.1.23",
          "form": "많이 돌아다님?",
          "original_form": "많이 돌아다님?",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100032954.1.24",
          "form": "나름?.. 책이야기하니까 책읽고싶다",
          "original_form": "나름?.. 책이야기하니까 책읽고싶다",
