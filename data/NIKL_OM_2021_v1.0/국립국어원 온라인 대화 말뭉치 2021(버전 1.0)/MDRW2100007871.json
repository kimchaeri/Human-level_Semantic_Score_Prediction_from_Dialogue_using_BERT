        {
          "id": "MDRW2100007871.1.1",
          "form": "아까",
          "original_form": "아까",
          "speaker_id": "1",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100007871.1.2",
          "form": "분명 나가서 구경하자 했는데",
          "original_form": "분명 나가서 구경하자 했는데",
          "speaker_id": "1",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100007871.1.3",
          "form": "왜 아무곳도 안가는가",
          "original_form": "왜 아무곳도 안가는가",
          "speaker_id": "1",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007871.1.4",
          "form": "ㅋㅋ나갈수잇어여?",
          "original_form": "ㅋㅋ나갈수잇어여?",
          "speaker_id": "2",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007871.1.5",
          "form": "귀찮을턴데?",
          "original_form": "귀찮을턴데?",
          "speaker_id": "2",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007871.1.6",
          "form": "한시간만 나가면 양평이 눈앞에 펼쳐질텐데",
          "original_form": "한시간만 나가면 양평이 눈앞에 펼쳐질텐데",
          "speaker_id": "1",
          "time": "20210822 11:32"
        },
        {
          "id": "MDRW2100007871.1.7",
          "form": "ㅋㅋ그러게 근데 사람도 많겟죠",
          "original_form": "ㅋㅋ그러게 근데 사람도 많겟죠",
          "speaker_id": "2",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100007871.1.8",
          "form": "결국안갔어",
          "original_form": "결국안갔어",
          "speaker_id": "1",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100007871.1.9",
          "form": "이번주도 집콕이야",
          "original_form": "이번주도 집콕이야",
          "speaker_id": "1",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100007871.1.10",
          "form": "여행간지가 언제인가",
          "original_form": "여행간지가 언제인가",
          "speaker_id": "1",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100007871.1.11",
          "form": "여행이 뭐죠",
          "original_form": "여행이 뭐죠",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100007871.1.12",
          "form": "나도 모르는것..",
          "original_form": "나도 모르는것..",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100007871.1.13",
          "form": "확실한건 집에 있는건 아닌것",
          "original_form": "확실한건 집에 있는건 아닌것",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100007871.1.14",
          "form": "ㅋㅋ나가는것",
          "original_form": "ㅋㅋ나가는것",
          "speaker_id": "2",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100007871.1.15",
          "form": "이곳을 떠나는것",
          "original_form": "이곳을 떠나는것",
          "speaker_id": "1",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100007871.1.16",
          "form": "ㅋㅋ담주나 가평이나 가볼까 계혹중",
          "original_form": "ㅋㅋ담주나 가평이나 가볼까 계혹중",
          "speaker_id": "2",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100007871.1.17",
          "form": "와우 가평에 뭐있어?",
          "original_form": "와우 가평에 뭐있어?",
          "speaker_id": "1",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100007871.1.18",
          "form": "마이다스호텔?",
          "original_form": "마이다스호텔?",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100007871.1.19",
          "form": "애기데꼬 가기좋다길래여ㅋㅋ",
          "original_form": "애기데꼬 가기좋다길래여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100007871.1.20",
          "form": "오 무려 호텔이야",
          "original_form": "오 무려 호텔이야",
          "speaker_id": "1",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100007871.1.21",
          "form": "ㅋㅋ말만",
          "original_form": "ㅋㅋ말만",
          "speaker_id": "2",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100007871.1.22",
          "form": "리조트같은거던데여?",
          "original_form": "리조트같은거던데여?",
          "speaker_id": "2",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100007871.1.23",
          "form": "오 그래도 좋다",
          "original_form": "오 그래도 좋다",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100007871.1.24",
          "form": "지금이 놀기 딱 좋은 계절",
          "original_form": "지금이 놀기 딱 좋은 계절",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100007871.1.25",
          "form": "ㅋㅋ애낳기전에머라도 ..",
          "original_form": "ㅋㅋ애낳기전에머라도 ..",
          "speaker_id": "2",
          "time": "20210822 18:55"
        },
        {
          "id": "MDRW2100007871.1.26",
          "form": "맞아 이제 둘되면 힘들겠다 다니기도",
          "original_form": "맞아 이제 둘되면 힘들겠다 다니기도",
          "speaker_id": "1",
          "time": "20210822 19:52"
        },
        {
          "id": "MDRW2100007871.1.27",
          "form": "그니까여ㅋㅋ마지막여행",
          "original_form": "그니까여ㅋㅋ마지막여행",
          "speaker_id": "2",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100007871.1.28",
          "form": "마지막을 불태워보자고",
          "original_form": "마지막을 불태워보자고",
          "speaker_id": "1",
          "time": "20210823 08:41"
        },
        {
          "id": "MDRW2100007871.1.29",
          "form": "하지만 여행가면 나 다시 시녀",
          "original_form": "하지만 여행가면 나 다시 시녀",
          "speaker_id": "1",
          "time": "20210823 08:41"
        },
        {
          "id": "MDRW2100007871.1.30",
          "form": "ㅋㅋ그쳐 난 시녀신세",
          "original_form": "ㅋㅋ그쳐 난 시녀신세",
          "speaker_id": "2",
          "time": "20210823 09:05"
        },
        {
          "id": "MDRW2100007871.1.31",
          "form": "여행가서도 뒤치닥거리 ㅋㅋ",
          "original_form": "여행가서도 뒤치닥거리 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 09:06"
        },
        {
          "id": "MDRW2100007871.1.32",
          "form": "ㅋ집에서나 밖에서나",
          "original_form": "ㅋ집에서나 밖에서나",
          "speaker_id": "2",
          "time": "20210823 09:09"
        }
      ]
