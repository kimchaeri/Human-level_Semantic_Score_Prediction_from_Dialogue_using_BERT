      "utterance": [
        {
          "id": "MDRW2100001912.1.1",
          "form": "즐겨보는 티비프로가있나요",
          "original_form": "즐겨보는 티비프로가있나요",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001912.1.2",
          "form": "요즘에 넷플",
          "original_form": "요즘에 넷플",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001912.1.3",
          "form": "월간집 잼께보고있어여",
          "original_form": "월간집 잼께보고있어여",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001912.1.4",
          "form": "정소민 나오는 드라마요?",
          "original_form": "정소민 나오는 드라마요?",
          "speaker_id": "1",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001912.1.5",
          "form": "맞아용 ㅎㅎ 그리고 펜트하우스!",
          "original_form": "맞아용 ㅎㅎ 그리고 펜트하우스!",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001912.1.6",
          "form": "너는나의봄도 재밌어요",
          "original_form": "너는나의봄도 재밌어요",
          "speaker_id": "2",
          "time": "20210819 22:08"
        },
        {
          "id": "MDRW2100001912.1.7",
          "form": "name1님은려?",
          "original_form": "&name1&님은려?",
          "speaker_id": "2",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001912.1.8",
          "form": "저도 펜트하우스 라켓소년단 그리고 결혼작사이혼인가 이거 보고있어요ㅎㅎ",
          "original_form": "저도 펜트하우스 라켓소년단 그리고 결혼작사이혼인가 이거 보고있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:09"
        },
        {
          "id": "MDRW2100001912.1.9",
          "form": "예능도 보세요?",
          "original_form": "예능도 보세요?",
          "speaker_id": "1",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001912.1.10",
          "form": "오 라켓소년단 볼까햇는데 잼써요?",
          "original_form": "오 라켓소년단 볼까햇는데 잼써요?",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001912.1.11",
          "form": "예능은 가끔씩 런닝맨만 보구 ㅠㅠ 요즘 재밌는게없어여",
          "original_form": "예능은 가끔씩 런닝맨만 보구 ㅠㅠ 요즘 재밌는게없어여",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001912.1.12",
          "form": "아 놀면뭐하니는 보네요 !!",
          "original_form": "아 놀면뭐하니는 보네요 !!",
          "speaker_id": "2",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001912.1.13",
          "form": "네 완전 힐링템.. 한 회에 한 번씩 울어요",
          "original_form": "네 완전 힐링템.. 한 회에 한 번씩 울어요",
          "speaker_id": "1",
          "time": "20210819 22:12"
        },
        {
          "id": "MDRW2100001912.1.14",
          "form": "아진짜요??",
          "original_form": "아진짜요??",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001912.1.15",
          "form": "함 봐야겠네용 ㅜ/ㅜ 재밌을듯용",
          "original_form": "함 봐야겠네용 ㅜ/ㅜ 재밌을듯용",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001912.1.16",
          "form": "네 사람 감성을 자극해요 꼭 보세요",
          "original_form": "네 사람 감성을 자극해요 꼭 보세요",
          "speaker_id": "1",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001912.1.17",
          "form": "또 추천해주실 드라마있나영 ?",
          "original_form": "또 추천해주실 드라마있나영 ?",
          "speaker_id": "2",
          "time": "20210819 22:13"
        },
        {
          "id": "MDRW2100001912.1.18",
          "form": "눈이부시게나 멜로가체질 보셨나요",
          "original_form": "눈이부시게나 멜로가체질 보셨나요",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001912.1.19",
          "form": "아 안녕,나야? 이것도 재밌게봤어요",
          "original_form": "아 안녕,나야? 이것도 재밌게봤어요",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001912.1.20",
          "form": "안녕나야 봤어요 !! 재밌어요 ㅠㅠ",
          "original_form": "안녕나야 봤어요 !! 재밌어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001912.1.21",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001912.1.22",
          "form": "눈이부시게만 안봤네요 !!",
          "original_form": "눈이부시게만 안봤네요 !!",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001912.1.23",
          "form": "강추하시나용 ㅋㅋㅋㅋ?",
          "original_form": "강추하시나용 ㅋㅋㅋㅋ?",
          "speaker_id": "2",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001912.1.24",
          "form": "진짜 잘 만든 드라마였는데..!",
          "original_form": "진짜 잘 만든 드라마였는데..!",
          "speaker_id": "1",
          "time": "20210819 22:15"
        },
        {
          "id": "MDRW2100001912.1.25",
          "form": "네 눈이부시게 재밌어요",
          "original_form": "네 눈이부시게 재밌어요",
          "speaker_id": "1",
          "time": "20210819 22:15"
        }
      ]
