      "utterance": [
        {
          "id": "MDRW2100041672.1.1",
          "form": "커피 뭐 드셔요??",
          "original_form": "커피 뭐 드셔요??",
          "speaker_id": "1",
          "time": "20210906 14:20"
        },
        {
          "id": "MDRW2100041672.1.2",
          "form": "저는 무조건 얼죽아에요",
          "original_form": "저는 무조건 얼죽아에요",
          "speaker_id": "2",
          "time": "20210906 14:22"
        },
        {
          "id": "MDRW2100041672.1.3",
          "form": "출근하자마자 커피때려넣슴다ㅜ",
          "original_form": "출근하자마자 커피때려넣슴다ㅜ",
          "speaker_id": "2",
          "time": "20210906 14:22"
        },
        {
          "id": "MDRW2100041672.1.4",
          "form": "아아메입니까",
          "original_form": "아아메입니까",
          "speaker_id": "1",
          "time": "20210906 14:23"
        },
        {
          "id": "MDRW2100041672.1.5",
          "form": "네ㅋㅋ무조건 아메욥",
          "original_form": "네ㅋㅋ무조건 아메욥",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041672.1.6",
          "form": "카페서 주로 머드시나용",
          "original_form": "카페서 주로 머드시나용",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041672.1.7",
          "form": "저도 아침엔 아아메 ㅋㅋㅋ",
          "original_form": "저도 아침엔 아아메 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041672.1.8",
          "form": "마카롱과 먹었죠 아침에 ㅋㅋ",
          "original_form": "마카롱과 먹었죠 아침에 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041672.1.9",
          "form": "카페서는 달달구리. 집에서는 아메요",
          "original_form": "카페서는 달달구리. 집에서는 아메요",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041672.1.10",
          "form": "마카롱 크~~~~",
          "original_form": "마카롱 크~~~~",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041672.1.11",
          "form": "저도좋아해요",
          "original_form": "저도좋아해요",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041672.1.12",
          "form": "뚱카롱!",
          "original_form": "뚱카롱!",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041672.1.13",
          "form": "뚱카롱이 젤맛나요",
          "original_form": "뚱카롱이 젤맛나요",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041672.1.14",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100041672.1.15",
          "form": "꼬끄 쫀득한거 킬포",
          "original_form": "꼬끄 쫀득한거 킬포",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100041672.1.16",
          "form": "ㅋㅋㅋ배달시켜드시나요?",
          "original_form": "ㅋㅋㅋ배달시켜드시나요?",
          "speaker_id": "2",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041672.1.17",
          "form": "아니요 이번에 택배로 한번 시켜봤는데 안녹고 잘왔어요",
          "original_form": "아니요 이번에 택배로 한번 시켜봤는데 안녹고 잘왔어요",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041672.1.18",
          "form": "오 저도 전에 택배시켜먹엇는데",
          "original_form": "오 저도 전에 택배시켜먹엇는데",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041672.1.19",
          "form": "엄청 맛잇엇어용",
          "original_form": "엄청 맛잇엇어용",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100041672.1.20",
