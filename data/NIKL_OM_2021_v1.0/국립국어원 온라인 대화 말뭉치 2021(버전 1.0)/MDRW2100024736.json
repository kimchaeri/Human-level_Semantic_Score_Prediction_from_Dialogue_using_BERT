      "utterance": [
        {
          "id": "MDRW2100024736.1.1",
          "form": "ㅋㅋㅋㅋ아니 다행히 해외여행 가서 굶은 적은 없는데",
          "original_form": "ㅋㅋㅋㅋ아니 다행히 해외여행 가서 굶은 적은 없는데",
          "speaker_id": "2",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100024736.1.2",
          "form": "장기로 가는거면 돈 배분 삐끗하지 않게 조심해야 할 것 같아서 말했어",
          "original_form": "장기로 가는거면 돈 배분 삐끗하지 않게 조심해야 할 것 같아서 말했어",
          "speaker_id": "2",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100024736.1.3",
          "form": "ㅋㅋㅋㅋㅋ많이 먹고 와",
          "original_form": "ㅋㅋㅋㅋㅋ많이 먹고 와",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024736.1.4",
          "form": "맞네 그생각을 못했다",
          "original_form": "맞네 그생각을 못했다",
          "speaker_id": "1",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024736.1.5",
          "form": "언제 또 외국음식을 한달동안 각 잡고 먹어보겠어",
          "original_form": "언제 또 외국음식을 한달동안 각 잡고 먹어보겠어",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024736.1.6",
          "form": "패키지여행 말고 자유여행 가는 거지?",
          "original_form": "패키지여행 말고 자유여행 가는 거지?",
          "speaker_id": "2",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024736.1.7",
          "form": "응 친구랑 둘이서 움직이거나 혼자 다니지않을까",
          "original_form": "응 친구랑 둘이서 움직이거나 혼자 다니지않을까",
          "speaker_id": "1",
          "time": "20210831 17:56"
        },
        {
          "id": "MDRW2100024736.1.8",
          "form": "안전도 살짝ㅈ걱정돼",
          "original_form": "안전도 살짝ㅈ걱정돼",
          "speaker_id": "1",
          "time": "20210831 17:56"
        },
        {
          "id": "MDRW2100024736.1.9",
          "form": "되도록이면 친구랑 같이 이동하는 게 좋아보인다",
          "original_form": "되도록이면 친구랑 같이 이동하는 게 좋아보인다",
          "speaker_id": "2",
          "time": "20210831 17:56"
        },
        {
          "id": "MDRW2100024736.1.10",
          "form": "낮에 집주변에 혼자가는건 어때",
          "original_form": "낮에 집주변에 혼자가는건 어때",
          "speaker_id": "1",
          "time": "20210831 17:57"
        },
        {
          "id": "MDRW2100024736.1.11",
          "form": "캐나다도 슬럼가가 있나? 쨌든 후미진 곳 피해다니면 괜찮을 것 같다",
          "original_form": "캐나다도 슬럼가가 있나? 쨌든 후미진 곳 피해다니면 괜찮을 것 같다",
          "speaker_id": "2",
          "time": "20210831 17:57"
        },
        {
          "id": "MDRW2100024736.1.12",
          "form": "넌 혼자 다니는 걸 좋아해?",
          "original_form": "넌 혼자 다니는 걸 좋아해?",
          "speaker_id": "2",
          "time": "20210831 17:57"
        },
        {
          "id": "MDRW2100024736.1.13",
          "form": "친구가 항상 같이 다닐수는",
          "original_form": "친구가 항상 같이 다닐수는",
          "speaker_id": "1",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100024736.1.14",
          "form": "없을테니까",
          "original_form": "없을테니까",
          "speaker_id": "1",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100024736.1.15",
          "form": "혹시나 혼자 다닐일도 생길것가ㄸ아서",
          "original_form": "혹시나 혼자 다닐일도 생길것가ㄸ아서",
          "speaker_id": "1",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100024736.1.16",
          "form": "밖에서 점심을 혼자먹는다던가",
          "original_form": "밖에서 점심을 혼자먹는다던가",
          "speaker_id": "1",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100024736.1.17",
          "form": "날 밝을 때 밝은 곳만 골라서 다니면 괜찮지 않을까",
          "original_form": "날 밝을 때 밝은 곳만 골라서 다니면 괜찮지 않을까",
          "speaker_id": "2",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100024736.1.18",
          "form": "캐나다가 살기좋은지도",
          "original_form": "캐나다가 살기좋은지도",
          "speaker_id": "1",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100024736.1.19",
          "form": "체험해봐야지",
          "original_form": "체험해봐야지",
          "speaker_id": "1",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100024736.1.20",
          "form": "나도 그러고싶다",
          "original_form": "나도 그러고싶다",
          "speaker_id": "2",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100024736.1.21",
          "form": "넌 이번에 해외여행 마지막으로 하는 거야?",
          "original_form": "넌 이번에 해외여행 마지막으로 하는 거야?",
          "speaker_id": "2",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100024736.1.22",
          "form": "마지막. . ?",
          "original_form": "마지막. . ?",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024736.1.23",
          "form": "더 다닐래",
          "original_form": "더 다닐래",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024736.1.24",
          "form": "일본도가고",
          "original_form": "일본도가고",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024736.1.25",
          "form": "대만도각ᆢ",
          "original_form": "대만도각ᆢ",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024736.1.26",
          "form": "필리핀도갈래",
          "original_form": "필리핀도갈래",
          "speaker_id": "1",
          "time": "20210831 18:01"
        },
        {
          "id": "MDRW2100024736.1.27",
          "form": "ㅎㅎㅎㅎㅎㅎ그럼 나중에 나랑 같이 글로벌 투어 다니자",
          "original_form": "ㅎㅎㅎㅎㅎㅎ그럼 나중에 나랑 같이 글로벌 투어 다니자",
          "speaker_id": "2",
          "time": "20210831 18:02"
        },
        {
          "id": "MDRW2100024736.1.28",
          "form": "돈 많이 벌어야겠다",
          "original_form": "돈 많이 벌어야겠다",
          "speaker_id": "1",
          "time": "20210831 18:02"
        },
        {
          "id": "MDRW2100024736.1.29",
          "form": "두마리 토끼를 잡도록",
          "original_form": "두마리 토끼를 잡도록",
          "speaker_id": "1",
          "time": "20210831 18:02"
        }
      ]
