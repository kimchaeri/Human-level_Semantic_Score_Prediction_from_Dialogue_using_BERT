        {
          "id": "MDRW2100004367.1.1",
          "form": "31개 맛이 있다는 그 아이스크림 좋아해?",
          "original_form": "31개 맛이 있다는 그 아이스크림 좋아해?",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004367.1.2",
          "form": "응 종종 가",
          "original_form": "응 종종 가",
          "speaker_id": "1",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004367.1.3",
          "form": "무슨 맛 좋아해?",
          "original_form": "무슨 맛 좋아해?",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004367.1.4",
          "form": "나는 민트 초코맛",
          "original_form": "나는 민트 초코맛",
          "speaker_id": "1",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004367.1.5",
          "form": "민트초코?!",
          "original_form": "민트초코?!",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004367.1.6",
          "form": "치약맛이잖아",
          "original_form": "치약맛이잖아",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004367.1.7",
          "form": "어머 반민초단이구나",
          "original_form": "어머 반민초단이구나",
          "speaker_id": "1",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004367.1.8",
          "form": "ㅋㅋㅋㅋ반민초단",
          "original_form": "ㅋㅋㅋㅋ반민초단",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004367.1.9",
          "form": "체리쥬빌레도 좋아해",
          "original_form": "체리쥬빌레도 좋아해",
          "speaker_id": "1",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004367.1.10",
          "form": "오 나는 거의 체리쥬빌레만 먹어",
          "original_form": "오 나는 거의 체리쥬빌레만 먹어",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004367.1.11",
          "form": "엄마는 외계인도 가끔 먹고",
          "original_form": "엄마는 외계인도 가끔 먹고",
          "speaker_id": "1",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004367.1.12",
          "form": "엄외 그 초코볼이 맛있어",
          "original_form": "엄외 그 초코볼이 맛있어",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004367.1.13",
          "form": "맞아 그거 맛있어",
          "original_form": "맞아 그거 맛있어",
          "speaker_id": "1",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004367.1.14",
          "form": "엄마는 외계인 블라스트도 맛있어",
          "original_form": "엄마는 외계인 블라스트도 맛있어",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004367.1.15",
