        },
        {
          "id": "MDRW2100044616.1.2",
          "form": "땡땡입니더",
          "original_form": "땡땡입니더",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044616.1.3",
          "form": "해산물 볶음...이라",
          "original_form": "해산물 볶음...이라",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044616.1.4",
          "form": "다리가 여러개인 해물입니다",
          "original_form": "다리가 여러개인 해물입니다",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044616.1.5",
          "form": "정말 다 알려드럈어요",
          "original_form": "정말 다 알려드럈어요",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044616.1.6",
          "form": "정답 쭈꾸미볶음!",
          "original_form": "정답 쭈꾸미볶음!",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044616.1.7",
          "form": "엄청난 오답..!!!",
          "original_form": "엄청난 오답..!!!",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044616.1.8",
          "form": "다왔어요!",
          "original_form": "다왔어요!",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044616.1.9",
          "form": "ㅋㅌㅌㅋㅋㅋㅋ이럴수가",
          "original_form": "ㅋㅌㅌㅋㅋㅋㅋ이럴수가",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044616.1.10",
          "form": "다시 도전! 오징어볶음!!!",
          "original_form": "다시 도전! 오징어볶음!!!",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044616.1.11",
          "form": "와 드디어 정답!",
          "original_form": "와 드디어 정답!",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044616.1.12",
          "form": "고생하셨어요ㅠㅠ",
          "original_form": "고생하셨어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044616.1.13",
          "form": "와아아 와아아 와아아",
          "original_form": "와아아 와아아 와아아",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044616.1.14",
          "form": "뿌듯하네요",
          "original_form": "뿌듯하네요",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044616.1.15",
          "form": "ㅋㅋㅋㅋ김치찌개 바로 맞추신 디디님이",
          "original_form": "ㅋㅋㅋㅋ김치찌개 바로 맞추신 디디님이",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044616.1.16",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044616.1.17",
          "form": "ㅋㅋㅋㅋ저도 신기하네요",
          "original_form": "ㅋㅋㅋㅋ저도 신기하네요",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044616.1.18",
          "form": "넘 어려웠죠 그래도 낚지볶음 다음으로 맞추신게 아니라 다행이에요",
          "original_form": "넘 어려웠죠 그래도 낚지볶음 다음으로 맞추신게 아니라 다행이에요",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044616.1.19",
          "form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044616.1.20",
          "form": "다리가 여러개인 친구들",
          "original_form": "다리가 여러개인 친구들",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044616.1.21",
          "form": "맛있어요..",
          "original_form": "맛있어요..",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044616.1.22",
          "form": "셋 다 넘 맛있는 친구들이죠",
          "original_form": "셋 다 넘 맛있는 친구들이죠",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044616.1.23",
          "form": "이번엔 지금 제가 먹고싶은 음식 맞춰보실래요?",
          "original_form": "이번엔 지금 제가 먹고싶은 음식 맞춰보실래요?",
          "speaker_id": "2",
          "time": "20210907 23:17"
