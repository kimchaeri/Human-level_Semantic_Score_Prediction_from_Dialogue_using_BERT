      "utterance": [
        {
          "id": "MDRW2100000472.1.1",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000472.1.2",
          "form": "완전 좋아하는데 코로나로 거의 못가서 아쉬워요ㅜ",
          "original_form": "완전 좋아하는데 코로나로 거의 못가서 아쉬워요ㅜ",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000472.1.3",
          "form": "저도요ㅜㅜ코로나전엔 진짜 틈만나면 다녔었는데",
          "original_form": "저도요ㅜㅜ코로나전엔 진짜 틈만나면 다녔었는데",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000472.1.4",
          "form": "ㅜㅡㅜ가장 좋아하셨던 여행지 혹시 있으세요?",
          "original_form": "ㅜㅡㅜ가장 좋아하셨던 여행지 혹시 있으세요?",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000472.1.5",
          "form": "저 남미요ㅜㅜㅜㅜ진짜 너무 힘들었는데 힘들었던만큼 넘 좋았어요",
          "original_form": "저 남미요ㅜㅜㅜㅜ진짜 너무 힘들었는데 힘들었던만큼 넘 좋았어요",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000472.1.6",
          "form": "우와 저 진짜 남미 가보고 싶었어요.",
          "original_form": "우와 저 진짜 남미 가보고 싶었어요.",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000472.1.7",
          "form": "어디가셨었어요?",
          "original_form": "어디가셨었어요?",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000472.1.8",
          "form": "저 시간이없어서 딱딱 랜드마크들다녀왔어요",
          "original_form": "저 시간이없어서 딱딱 랜드마크들다녀왔어요",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000472.1.9",
          "form": "페루 마추픽추 볼리비아 우유니",
          "original_form": "페루 마추픽추 볼리비아 우유니",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000472.1.10",
          "form": "브라질 리우",
          "original_form": "브라질 리우",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000472.1.11",
          "form": "중간에 작은도시들 경유하면서구경하구여",
          "original_form": "중간에 작은도시들 경유하면서구경하구여",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000472.1.12",
          "form": "진짜 우유니 별거없는데 감동받고왔어요ㅜ",
          "original_form": "진짜 우유니 별거없는데 감동받고왔어요ㅜ",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000472.1.13",
          "form": "헐헐 저 마추픽추랑 우유니 꼭 가보고 싶어요",
          "original_form": "헐헐 저 마추픽추랑 우유니 꼭 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000472.1.14",
          "form": "리우는 분위기 어때요?",
          "original_form": "리우는 분위기 어때요?",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000472.1.15",
          "form": "리우는 밤에 너무무서운데 낮엔 또 밝고 좋았어요 ㅋㅋㅋ",
          "original_form": "리우는 밤에 너무무서운데 낮엔 또 밝고 좋았어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000472.1.16",
          "form": "해변에 바다도 이쁘고 사람들도 여유롭고 핫바디",
          "original_form": "해변에 바다도 이쁘고 사람들도 여유롭고 핫바디",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000472.1.17",
          "form": "ㅋㅋ너무 고요해서 무서운가요?",
          "original_form": "ㅋㅋ너무 고요해서 무서운가요?",
          "speaker_id": "1",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000472.1.18",
          "form": "ㅋㅋ핫바디!",
          "original_form": "ㅋㅋ핫바디!",
          "speaker_id": "1",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000472.1.19",
          "form": "아뇨 진짜 목숨의위협 ㅋㅋㅋㅋㄴ",
          "original_form": "아뇨 진짜 목숨의위협 ㅋㅋㅋㅋㄴ",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000472.1.20",
          "form": "그 총기사고나 강도들이많다고해서",
          "original_form": "그 총기사고나 강도들이많다고해서",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000472.1.21",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "1",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000472.1.22",
          "form": "무섭네욬ㅋㅋ",
          "original_form": "무섭네욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000472.1.23",
          "form": "한국은 밤에도 가게들많이열려있고한데",
          "original_form": "한국은 밤에도 가게들많이열려있고한데",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000472.1.24",
          "form": "브라질은 깜깜하고 홈리스들 눈빛무섭게 쳐다보고",
          "original_form": "브라질은 깜깜하고 홈리스들 눈빛무섭게 쳐다보고",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000472.1.25",
          "form": "버스에서 폰꺼냈더니 폰집어넣으라고 위험하다고 제지당했었어요",
          "original_form": "버스에서 폰꺼냈더니 폰집어넣으라고 위험하다고 제지당했었어요",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000472.1.26",
          "form": "남미 버스로 거의 다다녔었는데 20시간넘게타면서 매번 버스강도 만날까봐 긴장하며 탔었던ㅜ",
          "original_form": "남미 버스로 거의 다다녔었는데 20시간넘게타면서 매번 버스강도 만날까봐 긴장하며 탔었던ㅜ",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000472.1.27",
          "form": "헐..ㅠ..",
          "original_form": "헐..ㅠ..",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000472.1.28",
          "form": "여러명이서 가셨어요?",
          "original_form": "여러명이서 가셨어요?",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000472.1.29",
          "form": "여행자체는 혼자갔었는데요 현지에서 한국인분들이랑 같이 다녔어요 ㅎㅎ",
          "original_form": "여행자체는 혼자갔었는데요 현지에서 한국인분들이랑 같이 다녔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000472.1.30",
          "form": "제가불쌍해보였는지 다들끼워주시더라구요 ㅋㅋㅋㅋ",
          "original_form": "제가불쌍해보였는지 다들끼워주시더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000472.1.31",
          "form": "우와 혼자 가신거 멋져요",
          "original_form": "우와 혼자 가신거 멋져요",
          "speaker_id": "1",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000472.1.32",
          "form": "저도 진짜 가고싶네요",
          "original_form": "저도 진짜 가고싶네요",
          "speaker_id": "1",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000472.1.33",
          "form": "루트들이 다비슷비슷해서 혼자가도 여행지에서 사람만나서가면 괜찮은것같아요",
          "original_form": "루트들이 다비슷비슷해서 혼자가도 여행지에서 사람만나서가면 괜찮은것같아요",
          "speaker_id": "2",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000472.1.34",
          "form": "여자분들이랑!",
          "original_form": "여자분들이랑!",
          "speaker_id": "2",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000472.1.35",
          "form": "name1님은 어디가 가장좋으셨어요?",
          "original_form": "&name1&님은 어디가 가장좋으셨어요?",
          "speaker_id": "2",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000472.1.36",
          "form": "저는 발리가 제일 좋았어요!!",
          "original_form": "저는 발리가 제일 좋았어요!!",
          "speaker_id": "1",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000472.1.37",
          "form": "약간 바다쪽이 좋은 거 같아요",
          "original_form": "약간 바다쪽이 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000472.1.38",
          "form": "아 마추픽추는 진짜 가고싶네요ㅎ",
          "original_form": "아 마추픽추는 진짜 가고싶네요ㅎ",
          "speaker_id": "1",
