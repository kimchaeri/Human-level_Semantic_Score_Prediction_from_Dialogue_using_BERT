      "utterance": [
        {
          "id": "MDRW2100039054.1.1",
          "form": "계속 공격하는데",
          "original_form": "계속 공격하는데",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100039054.1.2",
          "form": "계속 회복돼;;;",
          "original_form": "계속 회복돼;;;",
          "speaker_id": "1",
          "time": "20210903 11:14"
        },
        {
          "id": "MDRW2100039054.1.3",
          "form": "멘스?",
          "original_form": "멘스?",
          "speaker_id": "2",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100039054.1.4",
          "form": "아니 물의 정령",
          "original_form": "아니 물의 정령",
          "speaker_id": "1",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100039054.1.5",
          "form": "ㅇㅎ.......물의정령 왜?",
          "original_form": "ㅇㅎ.......물의정령 왜?",
          "speaker_id": "2",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100039054.1.6",
          "form": "물캐 있어?",
          "original_form": "물캐 있어?",
          "speaker_id": "2",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100039054.1.7",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210903 13:03"
        },
        {
          "id": "MDRW2100039054.1.8",
          "form": "아 무상의 물인데",
          "original_form": "아 무상의 물인데",
          "speaker_id": "1",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100039054.1.9",
          "form": "기존에 무상의 불/전기/바람 등등은",
          "original_form": "기존에 무상의 불/전기/바람 등등은",
          "speaker_id": "1",
          "time": "20210903 13:04"
        },
        {
          "id": "MDRW2100039054.1.10",
          "form": "체력 간당하게 깎여야 부활타임 있잖아",
          "original_form": "체력 간당하게 깎여야 부활타임 있잖아",
          "speaker_id": "1",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100039054.1.11",
          "form": "근데 얜 걍 주기적으로 체력 보완해주는 보조몹이 등장람",
          "original_form": "근데 얜 걍 주기적으로 체력 보완해주는 보조몹이 등장람",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100039054.1.12",
          "form": "헐.....",
          "original_form": "헐.....",
          "speaker_id": "2",
          "time": "20210903 13:11"
        },
        {
          "id": "MDRW2100039054.1.13",
          "form": "깡딜로 찍어눌러야겠네",
          "original_form": "깡딜로 찍어눌러야겠네",
          "speaker_id": "2",
          "time": "20210903 13:11"
        },
        {
          "id": "MDRW2100039054.1.14",
          "form": "걔 그 무음의 절현인가 여튼 그 전기박쥐는",
          "original_form": "걔 그 무음의 절현인가 여튼 그 전기박쥐는",
          "speaker_id": "2",
          "time": "20210903 13:12"
        },
        {
          "id": "MDRW2100039054.1.15",
          "form": "잡을만 해",
          "original_form": "잡을만 해",
          "speaker_id": "2",
          "time": "20210903 13:12"
        },
        {
          "id": "MDRW2100039054.1.16",
          "form": "걍 패턴 잘 피하면서",
          "original_form": "걍 패턴 잘 피하면서",
          "speaker_id": "2",
          "time": "20210903 13:12"
        },
        {
          "id": "MDRW2100039054.1.17",
          "form": "잘때리면 돼",
          "original_form": "잘때리면 돼",
          "speaker_id": "2",
          "time": "20210903 13:12"
        },
        {
          "id": "MDRW2100039054.1.18",
          "form": "ㅇㅎ 그나마 다행이네",
          "original_form": "ㅇㅎ 그나마 다행이네",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100039054.1.19",
          "form": "무상의 물 처음에 웬 슬라임을 소환하길ㄹ9",
          "original_form": "무상의 물 처음에 웬 슬라임을 소환하길ㄹ9",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100039054.1.20",
          "form": "래",
          "original_form": "래",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100039054.1.21",
          "form": "보스만 때려야지~~",
          "original_form": "보스만 때려야지~~",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100039054.1.22",
          "form": "하면서 슬라임 일부러 안건드렸었는ㄷ",
          "original_form": "하면서 슬라임 일부러 안건드렸었는ㄷ",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100039054.1.23",
          "form": "는데",
          "original_form": "는데",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100039054.1.24",
          "form": "걔가 보스 체력 채워주더라 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "걔가 보스 체력 채워주더라 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100039054.1.25",
          "form": "전기타입 필수네 그럼",
          "original_form": "전기타입 필수네 그럼",
          "speaker_id": "2",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100039054.1.26",
          "form": "얼탱×",
          "original_form": "얼탱×",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100039054.1.27",
          "form": "전기보다는",
          "original_form": "전기보다는",
          "speaker_id": "1",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039054.1.28",
          "form": "얼음이 좋은듯",
          "original_form": "얼음이 좋은듯",
          "speaker_id": "1",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039054.1.29",
          "form": "불딜은 거의 기본 공격력만 먹히는거같고...",
          "original_form": "불딜은 거의 기본 공격력만 먹히는거같고...",
          "speaker_id": "1",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039054.1.30",
          "form": "ㅇㅏ 필드보스 잡기 싫다",
          "original_form": "ㅇㅏ 필드보스 잡기 싫다",
          "speaker_id": "2",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039054.1.31",
          "form": "ㅇㅇ...",
          "original_form": "ㅇㅇ...",
          "speaker_id": "1",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039054.1.32",
          "form": "간신히 무상의 전기랑 화염나무 익숙해져놧더니",
          "original_form": "간신히 무상의 전기랑 화염나무 익숙해져놧더니",
          "speaker_id": "1",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039054.1.33",
          "form": "신규몹 뮤슨일....",
          "original_form": "신규몹 뮤슨일....",
          "speaker_id": "1",
          "time": "20210903 13:15"
        },
        {
          "id": "MDRW2100039054.1.34",
          "form": "나 이번에 월드렙 올리면서 깨달았어",
          "original_form": "나 이번에 월드렙 올리면서 깨달았어",
          "speaker_id": "2",
          "time": "20210903 13:16"
        },
        {
          "id": "MDRW2100039054.1.35",
          "form": "내가 엄청난 멍청짓을",
          "original_form": "내가 엄청난 멍청짓을",
          "speaker_id": "2",
          "time": "20210903 13:16"
        },
        {
          "id": "MDRW2100039054.1.36",
          "form": "했었단걸",
          "original_form": "했었단걸",
          "speaker_id": "2",
          "time": "20210903 13:16"
        },
        {
          "id": "MDRW2100039054.1.37",
          "form": "무상의 전기 피 다 닳았을때 세개 깨는거",
          "original_form": "무상의 전기 피 다 닳았을때 세개 깨는거",
          "speaker_id": "2",
          "time": "20210903 13:17"
        },
        {
          "id": "MDRW2100039054.1.38",
          "form": "얼음으로도 되는거였어........",
          "original_form": "얼음으로도 되는거였어........",
          "speaker_id": "2",
          "time": "20210903 13:17"
        },
        {
          "id": "MDRW2100039054.1.39",
          "form": "진짜 바본가봐....그동안 베넷 궁이랑 스킬로 억케든 한번에 다 깨려고 아등바등했는데......",
          "original_form": "진짜 바본가봐....그동안 베넷 궁이랑 스킬로 억케든 한번에 다 깨려고 아등바등했는데......",
          "speaker_id": "2",
          "time": "20210903 13:18"
        },
        {
          "id": "MDRW2100039054.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:20"
        },
        {
          "id": "MDRW2100039054.1.41",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210903 13:20"
        },
        {
          "id": "MDRW2100039054.1.42",
          "form": "얼음이 전기에도 잘먹혀",
          "original_form": "얼음이 전기에도 잘먹혀",
          "speaker_id": "1",
          "time": "20210903 13:20"
        },
        {
          "id": "MDRW2100039054.1.43",
          "form": "우인단 전기몹은 얼음으로 보호막 깨는게 제일임",
          "original_form": "우인단 전기몹은 얼음으로 보호막 깨는게 제일임",
          "speaker_id": "1",
          "time": "20210903 13:20"
        },
        {
          "id": "MDRW2100039054.1.44",
          "form": "오히려 다이루크로 깨려고 하면",
          "original_form": "오히려 다이루크로 깨려고 하면",
          "speaker_id": "1",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039054.1.45",
          "form": "과부하만 왕창 걸려서 튕겨..",
          "original_form": "과부하만 왕창 걸려서 튕겨..",
          "speaker_id": "1",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039054.1.46",
          "form": "그거는 그렇게 하는데",
          "original_form": "그거는 그렇게 하는데",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039054.1.47",
          "form": "왜 저거는 지금까지",
          "original_form": "왜 저거는 지금까지",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039054.1.48",
          "form": "생각을 못했을까...",
          "original_form": "생각을 못했을까...",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039054.1.49",
          "form": "왜 언니가 보내는거 진동이 안오지?",
          "original_form": "왜 언니가 보내는거 진동이 안오지?",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039054.1.50",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039054.1.51",
          "form": "알림?",
          "original_form": "알림?",
          "speaker_id": "1",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039054.1.52",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039054.1.53",
          "form": "나 상단바는 잘 안봐서",
          "original_form": "나 상단바는 잘 안봐서",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100039054.1.54",
          "form": "진동가지고 체크하는데",
          "original_form": "진동가지고 체크하는데",
          "speaker_id": "2",
          "time": "20210903 13:22"
        },
        {
          "id": "MDRW2100039054.1.55",
          "form": "엥 뭐지",
          "original_form": "엥 뭐지",
          "speaker_id": "1",
          "time": "20210903 13:27"
        },
        {
          "id": "MDRW2100039054.1.56",
          "form": "알림은 칼같이 오던데",
          "original_form": "알림은 칼같이 오던데",
          "speaker_id": "1",
          "time": "20210903 13:27"
        },
        {
          "id": "MDRW2100039054.1.57",
          "form": "지금도 안왔어",
          "original_form": "지금도 안왔어",
          "speaker_id": "2",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100039054.1.58",
          "form": "진짜 뭐지...",
          "original_form": "진짜 뭐지...",
          "speaker_id": "1",
