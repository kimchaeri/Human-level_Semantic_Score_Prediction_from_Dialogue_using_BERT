      "utterance": [
        {
          "id": "MDRW2100002116.1.1",
          "form": "이번에 방학때 룸메랑 살아보니까",
          "original_form": "이번에 방학때 룸메랑 살아보니까",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002116.1.2",
          "form": "확실히 혼자사는게 좋은거같음",
          "original_form": "확실히 혼자사는게 좋은거같음",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002116.1.3",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002116.1.4",
          "form": "아니면 넓은집에서 간섭없이 사는거",
          "original_form": "아니면 넓은집에서 간섭없이 사는거",
          "speaker_id": "2",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002116.1.5",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002116.1.6",
          "form": "닌 룸메랑 같이살때",
          "original_form": "닌 룸메랑 같이살때",
          "speaker_id": "1",
          "time": "20210819 23:44"
        },
        {
          "id": "MDRW2100002116.1.7",
          "form": "뭐가 불편했었어",
          "original_form": "뭐가 불편했었어",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002116.1.8",
          "form": "나야 뭐",
          "original_form": "나야 뭐",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002116.1.9",
          "form": "집이 넓은듯 좁은데",
          "original_form": "집이 넓은듯 좁은데",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002116.1.10",
          "form": "방음이 잘 안되니까 힘들지",
          "original_form": "방음이 잘 안되니까 힘들지",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002116.1.11",
          "form": "근데 그거말고는 재밌더라",
          "original_form": "근데 그거말고는 재밌더라",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002116.1.12",
          "form": "아아 역시 소리가...",
          "original_form": "아아 역시 소리가...",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002116.1.13",
          "form": "니도 친한친구랑 살면 좋았을텐대",
          "original_form": "니도 친한친구랑 살면 좋았을텐대",
          "speaker_id": "2",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002116.1.14",
          "form": "그치 일단 나이차이가 많이나니까...",
          "original_form": "그치 일단 나이차이가 많이나니까...",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002116.1.15",
          "form": "그리고 성향차이도 중요한거같음",
          "original_form": "그리고 성향차이도 중요한거같음",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002116.1.16",
          "form": "맨날 밤에 밖에나가 ㅠ",
          "original_form": "맨날 밤에 밖에나가 ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:45"
        },
        {
          "id": "MDRW2100002116.1.17",
          "form": "재밌었던건 뭐가있어",
          "original_form": "재밌었던건 뭐가있어",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.18",
          "form": "매일놀러가",
          "original_form": "매일놀러가",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.19",
          "form": "우리는 매일 밤에 놀러가",
          "original_form": "우리는 매일 밤에 놀러가",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.20",
          "form": "같이 매일 야식먹고",
          "original_form": "같이 매일 야식먹고",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.21",
          "form": "혼자있으면 밥해먹기 귀찮잖아",
          "original_form": "혼자있으면 밥해먹기 귀찮잖아",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.22",
          "form": "아 야식같이먹을 수 있는거",
          "original_form": "아 야식같이먹을 수 있는거",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.23",
          "form": "너무 부럽다",
          "original_form": "너무 부럽다",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.24",
          "form": "근데 친구가 밥 해먹는거 좋아해서 나도 가끔 해먹으면 재밌고",
          "original_form": "근데 친구가 밥 해먹는거 좋아해서 나도 가끔 해먹으면 재밌고",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.25",
          "form": "맞아 근데 그리고 같이 살 때 내가",
          "original_form": "맞아 근데 그리고 같이 살 때 내가",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.26",
          "form": "결국 혼자시키는거랑 양은 똑같은데 돈도 나눌수있고",
          "original_form": "결국 혼자시키는거랑 양은 똑같은데 돈도 나눌수있고",
          "speaker_id": "1",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.27",
          "form": "여유가 있던 때라서 매일 그냥 집에서 친규 기다리면서 뒹굴뒹굴",
          "original_form": "여유가 있던 때라서 매일 그냥 집에서 친규 기다리면서 뒹굴뒹굴",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.28",
          "form": "맞아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:46"
        },
        {
          "id": "MDRW2100002116.1.29",
          "form": "그래도 이제 새로 자취하면 개인냉장고 있으니까",
          "original_form": "그래도 이제 새로 자취하면 개인냉장고 있으니까",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002116.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ혼자 있는거보다 덜 심심했겠네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ혼자 있는거보다 덜 심심했겠네",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002116.1.31",
          "form": "뭐 시켜서 냉동실행 고",
          "original_form": "뭐 시켜서 냉동실행 고",
          "speaker_id": "2",
          "time": "20210819 23:47"
        }
      ]
    }
