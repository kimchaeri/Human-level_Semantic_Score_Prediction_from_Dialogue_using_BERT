        },
        {
          "id": "MDRW2100027917.1.2",
          "form": "무슨 게임",
          "original_form": "무슨 게임",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027917.1.3",
          "form": "아무거나... 사실 막 땡기는 건 없는",
          "original_form": "아무거나... 사실 막 땡기는 건 없는",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027917.1.4",
          "form": "난 몬헌 언제하지",
          "original_form": "난 몬헌 언제하지",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027917.1.5",
          "form": "게임 안 하시나요",
          "original_form": "게임 안 하시나요",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027917.1.6",
          "form": "해야죠",
          "original_form": "해야죠",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027917.1.7",
          "form": "밀린 과제 말하듯",
          "original_form": "밀린 과제 말하듯",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027917.1.8",
          "form": "웃겨",
          "original_form": "웃겨",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027917.1.9",
          "form": "실제로도 밀리긴 함",
          "original_form": "실제로도 밀리긴 함",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027917.1.10",
          "form": "옆집 뉴비는 벌써 진도를 저만큼 나갔다는데",
          "original_form": "옆집 뉴비는 벌써 진도를 저만큼 나갔다는데",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027917.1.11",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027917.1.12",
          "form": "벌써 아본 입성하셨다고",
          "original_form": "벌써 아본 입성하셨다고",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027917.1.13",
          "form": "우리 반이 진도 제일 느리네",
          "original_form": "우리 반이 진도 제일 느리네",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027917.1.14",
          "form": "너희반이 진도 제일 느려",
          "original_form": "너희반이 진도 제일 느려",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027917.1.15",
          "form": "몬헌 펴라",
          "original_form": "몬헌 펴라",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027917.1.16",
          "form": "아~ 선생님은 맨날 그 소리",
          "original_form": "아~ 선생님은 맨날 그 소리",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027917.1.17",
          "form": "첫사랑얘기해주세요",
          "original_form": "첫사랑얘기해주세요",
          "speaker_id": "2",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027917.1.18",
          "form": "신대륙으로 떠났다 다음",
          "original_form": "신대륙으로 떠났다 다음",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027917.1.19",
          "form": "개웃겨",
          "original_form": "개웃겨",
          "speaker_id": "2",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027917.1.20",
          "form": "내일 가면 뭐하지",
          "original_form": "내일 가면 뭐하지",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027917.1.21",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027917.1.22",
          "form": "넬기 잡기",
          "original_form": "넬기 잡기",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027917.1.23",
          "form": "3시간넬기 보여준다",
          "original_form": "3시간넬기 보여준다",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027917.1.24",
          "form": "딱대",
          "original_form": "딱대",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027917.1.25",
          "form": "3시간이면 빠르네",
          "original_form": "3시간이면 빠르네",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027917.1.26",
          "form": "가보자고",
          "original_form": "가보자고",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027917.1.27",
          "form": "아 빠른 거야",
          "original_form": "아 빠른 거야",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027917.1.28",
          "form": "걍 뭐 평균",
          "original_form": "걍 뭐 평균",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027917.1.29",
          "form": "그럼 3일넬기(;;",
          "original_form": "그럼 3일넬기(;;",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027917.1.30",
          "form": "수비대 장비도 있으니까",
          "original_form": "수비대 장비도 있으니까",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027917.1.31",
          "form": "그렇게 오래는 안 걸림",
          "original_form": "그렇게 오래는 안 걸림",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027917.1.32",
          "form": "하지만 수비대 장비로 왕넬기",
          "original_form": "하지만 수비대 장비로 왕넬기",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027917.1.33",
          "form": "이건 3일넬기 각이죠",
          "original_form": "이건 3일넬기 각이죠",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027917.1.34",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027917.1.35",
          "form": "두렵다 넬기선셍.",
          "original_form": "두렵다 넬기선셍.",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027917.1.36",
          "form": "ㄱㅊ 방어구가 좋으니까",
          "original_form": "ㄱㅊ 방어구가 좋으니까",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027917.1.37",
          "form": "별로 안 아픔",
          "original_form": "별로 안 아픔",
          "speaker_id": "1",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100027917.1.38",
          "form": "일어날 수 있으니 각오해라어쩌고",
          "original_form": "일어날 수 있으니 각오해라어쩌고",
          "speaker_id": "2",
