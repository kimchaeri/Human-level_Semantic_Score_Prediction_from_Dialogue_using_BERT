      "utterance": [
        {
          "id": "MDRW2100029104.1.1",
          "form": "아 밤중에 갑자기 빵 먹고 싶네요...",
          "original_form": "아 밤중에 갑자기 빵 먹고 싶네요...",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029104.1.2",
          "form": "케이크나 디저트...",
          "original_form": "케이크나 디저트...",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029104.1.3",
          "form": "도 좋아하시나요??",
          "original_form": "도 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029104.1.4",
          "form": "좋아함니다...",
          "original_form": "좋아함니다...",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029104.1.5",
          "form": "단거 짱조아해요",
          "original_form": "단거 짱조아해요",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029104.1.6",
          "form": "ㅠㅠㅋㅋ",
          "original_form": "ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029104.1.7",
          "form": "앗...저두요",
          "original_form": "앗...저두요",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029104.1.8",
          "form": "저 대학생때 취미가",
          "original_form": "저 대학생때 취미가",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029104.1.9",
          "form": "야밤에 먹을거 이야기하면 위험한데...",
          "original_form": "야밤에 먹을거 이야기하면 위험한데...",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029104.1.10",
          "form": "좋아하는 케이크 종류 있나요?",
          "original_form": "좋아하는 케이크 종류 있나요?",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029104.1.11",
          "form": "애프터눈 티세트 먹고다니는 거였거든요",
          "original_form": "애프터눈 티세트 먹고다니는 거였거든요",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029104.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029104.1.13",
          "form": "앗 케이크는 안 느끼한 거 좋아해요",
          "original_form": "앗 케이크는 안 느끼한 거 좋아해요",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029104.1.14",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029104.1.15",
          "form": "저는 당근케이크 좋아합니다",
          "original_form": "저는 당근케이크 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029104.1.16",
          "form": "와 애프터눈 티세트..! 맛있나요? 한번도 안가봤어요",
          "original_form": "와 애프터눈 티세트..! 맛있나요? 한번도 안가봤어요",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029104.1.17",
          "form": "네ㅋㅋㅋㅋ근데 둘이서 먹기는 진짜 힘들구요",
          "original_form": "네ㅋㅋㅋㅋ근데 둘이서 먹기는 진짜 힘들구요",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029104.1.18",
          "form": "단 거 좋아하는데도...먹다보면 버겁긴 해요",
          "original_form": "단 거 좋아하는데도...먹다보면 버겁긴 해요",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029104.1.19",
          "form": "그래두 홍차도 좋아해서 좋았어요!!",
          "original_form": "그래두 홍차도 좋아해서 좋았어요!!",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029104.1.20",
          "form": "오 당근케이크! 전 한번두 먹어본 적 없어요",
          "original_form": "오 당근케이크! 전 한번두 먹어본 적 없어요",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029104.1.21",
          "form": "홍차 좋아하시는 군여! 저도 조아하는! 근데 초보라 정말 기본홍차만 마셔요ㅎㅎ",
          "original_form": "홍차 좋아하시는 군여! 저도 조아하는! 근데 초보라 정말 기본홍차만 마셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029104.1.22",
          "form": "당근케이크 맛있어요! 투썸도 괜찮슴니다ㅎㅎ",
          "original_form": "당근케이크 맛있어요! 투썸도 괜찮슴니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029104.1.23",
          "form": "저도 알지는 못하는데 그냥 막 마셔요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 알지는 못하는데 그냥 막 마셔요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029104.1.24",
          "form": "잉글리시 블랙퍼스트 좋은 거 가타용",
          "original_form": "잉글리시 블랙퍼스트 좋은 거 가타용",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029104.1.25",
          "form": "오오 투썸도 당케 있군요!",
          "original_form": "오오 투썸도 당케 있군요!",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029104.1.26",
          "form": "당케 뭔가 너무 건강한 맛일 거 같아서 두려웠는데ㅠㅜ 도전해봐야겠어요",
          "original_form": "당케 뭔가 너무 건강한 맛일 거 같아서 두려웠는데ㅠㅜ 도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029104.1.27",
          "form": "저 생당근 못먹는데 당케는 또 맛있더라구요ㅠㅠㅋㅋ",
          "original_form": "저 생당근 못먹는데 당케는 또 맛있더라구요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:52"
        },
        {
          "id": "MDRW2100029104.1.28",
          "form": "홍차 냉침해먹는거 좋아해서 여름에 잘마셨었어요ㅋㅋ",
          "original_form": "홍차 냉침해먹는거 좋아해서 여름에 잘마셨었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029104.1.29",
          "form": "마자요 냉침 너무 좋죵,,ㅠ",
          "original_form": "마자요 냉침 너무 좋죵,,ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029104.1.30",
          "form": "당케 오늘의 호기심품목이에요",
          "original_form": "당케 오늘의 호기심품목이에요",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029104.1.31",
          "form": "홍차음료중에 중국의 삥홍차? 가 요즘 유행이래서",
          "original_form": "홍차음료중에 중국의 삥홍차? 가 요즘 유행이래서",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029104.1.32",
          "form": "그것두 호기심품목이에요",
          "original_form": "그것두 호기심품목이에요",
          "speaker_id": "1",
          "time": "20210902 23:53"
        },
        {
          "id": "MDRW2100029104.1.33",
          "form": "오 첨 들어봐서 검색 해봤어요ㅋ",
          "original_form": "오 첨 들어봐서 검색 해봤어요ㅋ",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100029104.1.34",
          "form": "마라탕집 냉장고에 있는 아이군여",
          "original_form": "마라탕집 냉장고에 있는 아이군여",
