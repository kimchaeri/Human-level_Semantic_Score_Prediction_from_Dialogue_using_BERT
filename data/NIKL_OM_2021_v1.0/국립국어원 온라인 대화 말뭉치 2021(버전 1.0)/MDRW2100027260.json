        },
        {
          "id": "MDRW2100027260.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100027260.1.3",
          "form": "고양이 키워요",
          "original_form": "고양이 키워요",
          "speaker_id": "2",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100027260.1.4",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100027260.1.5",
          "form": "전 아직",
          "original_form": "전 아직",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027260.1.6",
          "form": "키울 예정이신가요?",
          "original_form": "키울 예정이신가요?",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100027260.1.7",
          "form": "시간이 되면요",
          "original_form": "시간이 되면요",
          "speaker_id": "1",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027260.1.8",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027260.1.9",
          "form": "강아지 키우고싶은데",
          "original_form": "강아지 키우고싶은데",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100027260.1.10",
          "form": "강아지요?",
          "original_form": "강아지요?",
          "speaker_id": "2",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100027260.1.11",
          "form": "강아지 귀엽죠",
          "original_form": "강아지 귀엽죠",
          "speaker_id": "2",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100027260.1.12",
          "form": "같이 산책하다보면 건강도 챙기고 좋을것같아요",
          "original_form": "같이 산책하다보면 건강도 챙기고 좋을것같아요",
          "speaker_id": "2",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100027260.1.13",
          "form": "네 즐거울거 같아요.",
          "original_form": "네 즐거울거 같아요.",
          "speaker_id": "1",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100027260.1.14",
          "form": "네 키우는거 쉽지않지만 잘 키우세요",
          "original_form": "네 키우는거 쉽지않지만 잘 키우세요",
          "speaker_id": "2",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100027260.1.15",
          "form": "캐어가 일이죠",
          "original_form": "캐어가 일이죠",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100027260.1.16",
          "form": "그렇긴하죠",
          "original_form": "그렇긴하죠",
          "speaker_id": "2",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100027260.1.17",
          "form": "그래도 같이 있는게 좋으니까",
          "original_form": "그래도 같이 있는게 좋으니까",
          "speaker_id": "2",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100027260.1.18",
          "form": "시간이 문제죠",
          "original_form": "시간이 문제죠",
