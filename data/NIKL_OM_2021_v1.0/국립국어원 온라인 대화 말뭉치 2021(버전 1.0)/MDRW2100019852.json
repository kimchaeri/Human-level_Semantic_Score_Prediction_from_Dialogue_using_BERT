      "utterance": [
        {
          "id": "MDRW2100019852.1.1",
          "form": "야식드시나요?",
          "original_form": "야식드시나요?",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019852.1.2",
          "form": "ㅎㅎ 생각중이에ㅛㅎ",
          "original_form": "ㅎㅎ 생각중이에ㅛㅎ",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019852.1.3",
          "form": "저도 오늘 소주한잔해야하는뎅ㅠㅠ",
          "original_form": "저도 오늘 소주한잔해야하는뎅ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019852.1.4",
          "form": "내일 일하러가야해서ㅠ name1님은요?",
          "original_form": "내일 일하러가야해서ㅠ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019852.1.5",
          "form": "아 소주요 전 독해서 맥주",
          "original_form": "아 소주요 전 독해서 맥주",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019852.1.6",
          "form": "아아ㅎㅎ 안주는요?",
          "original_form": "아아ㅎㅎ 안주는요?",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019852.1.7",
          "form": "오늘은 생라연",
          "original_form": "오늘은 생라연",
          "speaker_id": "1",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019852.1.8",
          "form": "왜요ㅠ",
          "original_form": "왜요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019852.1.9",
          "form": "끓여드시지요ㅠㅠ",
          "original_form": "끓여드시지요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019852.1.10",
          "form": "맥주니깐요 끓여먹으면 안주가 안됨",
          "original_form": "맥주니깐요 끓여먹으면 안주가 안됨",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019852.1.11",
          "form": "아참 그리고 죄송해요ㅠ 당첨 되어야 했는데ㅠ 제일 활동많이 하시는분인데ㅠ",
          "original_form": "아참 그리고 죄송해요ㅠ 당첨 되어야 했는데ㅠ 제일 활동많이 하시는분인데ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019852.1.12",
          "form": "아닙니다^^ 전 만족합니다~맥주~",
          "original_form": "아닙니다^^ 전 만족합니다~맥주~",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019852.1.13",
          "form": "ㅎㅎ name1님,name3님은 제가 다 챙겨드릴꺼에요ㅎ",
          "original_form": "ㅎㅎ &name1&님,&name3&님은 제가 다 챙겨드릴꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019852.1.14",
          "form": "저 이번주 일요일날 오티하고 계속 잘해가지고ㅎ 꼭 정보 따올께요ㅎ",
          "original_form": "저 이번주 일요일날 오티하고 계속 잘해가지고ㅎ 꼭 정보 따올께요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019852.1.15",
          "form": "좋은 정보로 보답할께요ㅎㅎ",
          "original_form": "좋은 정보로 보답할께요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019852.1.16",
          "form": "와근데 진심 대단하세요",
          "original_form": "와근데 진심 대단하세요",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019852.1.17",
          "form": "멋지십니다",
          "original_form": "멋지십니다",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019852.1.18",
          "form": "아니에요ㅎㅎ",
          "original_form": "아니에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019852.1.19",
          "form": "다같이 잘살면좋잖아요ㅎ",
          "original_form": "다같이 잘살면좋잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019852.1.20",
          "form": "세상이 팍팍하지만 그래도 다같이 살아가는게 좋은거니깐ㅎㅎ 그리고 저도 해야해요ㅎ 워낙에 먹는것도 돈많이쓰고 그래서 절약해야해요ㅎ",
          "original_form": "세상이 팍팍하지만 그래도 다같이 살아가는게 좋은거니깐ㅎㅎ 그리고 저도 해야해요ㅎ 워낙에 먹는것도 돈많이쓰고 그래서 절약해야해요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019852.1.21",
          "form": "마음가짐  먹고살자^^",
          "original_form": "마음가짐  먹고살자^^",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019852.1.22",
          "form": "많이 힘드시진 않으세요?ㅎ 애들 식사할때 엄청나죠?",
          "original_form": "많이 힘드시진 않으세요?ㅎ 애들 식사할때 엄청나죠?",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019852.1.23",
          "form": "애들은 먹이고 저먹죠 항상ㅋ",
          "original_form": "애들은 먹이고 저먹죠 항상ㅋ",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019852.1.24",
          "form": "그렇죠ㅎㅎ 알죠ㅎ",
          "original_form": "그렇죠ㅎㅎ 알죠ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019852.1.25",
          "form": "애가 먹는것만 보면 너무 행복해요ㅎㅎ",
          "original_form": "애가 먹는것만 보면 너무 행복해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019852.1.26",
          "form": "맞아요 배부르죠",
          "original_form": "맞아요 배부르죠",
          "speaker_id": "1",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019852.1.27",
          "form": "식사는 애들 뭐 잘먹어요?",
          "original_form": "식사는 애들 뭐 잘먹어요?",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019852.1.28",
          "form": "애들은 볶음밥이 편해요 주기에ㅋ",
          "original_form": "애들은 볶음밥이 편해요 주기에ㅋ",
          "speaker_id": "1",
          "time": "20210827 22:43"
        }
