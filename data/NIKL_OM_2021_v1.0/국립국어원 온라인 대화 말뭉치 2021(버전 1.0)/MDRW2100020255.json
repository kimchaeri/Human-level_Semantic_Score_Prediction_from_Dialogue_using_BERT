      "utterance": [
        {
          "id": "MDRW2100020255.1.1",
          "form": "게임 뭐 좋아하세요?",
          "original_form": "게임 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020255.1.2",
          "form": "저는 사천성 왕 좋아해요",
          "original_form": "저는 사천성 왕 좋아해요",
          "speaker_id": "1",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020255.1.3",
          "form": "요즘에 하는거는 쿠키런킹덤이요!",
          "original_form": "요즘에 하는거는 쿠키런킹덤이요!",
          "speaker_id": "2",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020255.1.4",
          "form": "하신지 오래되셨어요?",
          "original_form": "하신지 오래되셨어요?",
          "speaker_id": "2",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020255.1.5",
          "form": "사천성 예전부터 했어요!",
          "original_form": "사천성 예전부터 했어요!",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020255.1.6",
          "form": "어,,, 뭔가 아이템있는게임은 하기싫더라고요",
          "original_form": "어,,, 뭔가 아이템있는게임은 하기싫더라고요",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020255.1.7",
          "form": "제친구도 쿠키런킹덤 좋아하는데",
          "original_form": "제친구도 쿠키런킹덤 좋아하는데",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020255.1.8",
          "form": "재밌나요",
          "original_form": "재밌나요",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020255.1.9",
          "form": "넵 재밌어요!! 초반에는 미쳐서 했었는데",
          "original_form": "넵 재밌어요!! 초반에는 미쳐서 했었는데",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020255.1.10",
          "form": "지금은 그냥 의무감에 하고있긴해요",
          "original_form": "지금은 그냥 의무감에 하고있긴해요",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020255.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020255.1.12",
          "form": "의무감ㅋㅋㅋㅋㅋㅋ",
          "original_form": "의무감ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020255.1.13",
          "form": "프로게이머 좋아하는 사람 있으세요?",
          "original_form": "프로게이머 좋아하는 사람 있으세요?",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020255.1.14",
          "form": "전 없어요",
          "original_form": "전 없어요",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020255.1.15",
          "form": "저 게임방송은 잘 안봐서요!",
          "original_form": "저 게임방송은 잘 안봐서요!",
          "speaker_id": "2",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020255.1.16",
          "form": "사천성 말고 또 하시는거 있으세요?",
          "original_form": "사천성 말고 또 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210828 00:53"
        },
        {
          "id": "MDRW2100020255.1.17",
          "form": "음 모두의마블이었나 캐시템 나오기 전까지는 재밌게했어요",
          "original_form": "음 모두의마블이었나 캐시템 나오기 전까지는 재밌게했어요",
          "speaker_id": "1",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020255.1.18",
          "form": "저 크아 잘해요",
          "original_form": "저 크아 잘해요",
          "speaker_id": "1",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020255.1.19",
          "form": "와저도 예전에 모두의마블 초반에 했었어요!",
          "original_form": "와저도 예전에 모두의마블 초반에 했었어요!",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020255.1.20",
          "form": "근데 크아도 캐시템 너무 많이 써서안해요 ,,",
          "original_form": "근데 크아도 캐시템 너무 많이 써서안해요 ,,",
          "speaker_id": "1",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020255.1.21",
          "form": "오 크아 세대시군요ㅋㅋㅋㅋㅋ",
          "original_form": "오 크아 세대시군요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020255.1.22",
          "form": "크아는 가~~~~~~끔 생각나면 한번씩 들어가봐요",
          "original_form": "크아는 가~~~~~~끔 생각나면 한번씩 들어가봐요",
          "speaker_id": "2",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020255.1.23",
          "form": "할때마다 아이디 잊어버려서 새로만드는 크아,,",
          "original_form": "할때마다 아이디 잊어버려서 새로만드는 크아,,",
          "speaker_id": "1",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020255.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ 저는 오래키운 아이디라서 버리긴 아깝더라구요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저는 오래키운 아이디라서 버리긴 아깝더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 00:57"
        },
        {
          "id": "MDRW2100020255.1.25",
          "form": "요즘 인기있는 신작게임은,, 뭐가 있는지 아세요 ?",
