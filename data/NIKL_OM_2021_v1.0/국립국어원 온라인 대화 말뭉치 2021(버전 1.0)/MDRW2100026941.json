      "utterance": [
        {
          "id": "MDRW2100026941.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100026941.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100026941.1.3",
          "form": "혹시 고양이 좋아하시나요",
          "original_form": "혹시 고양이 좋아하시나요",
          "speaker_id": "1",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026941.1.4",
          "form": "하 당연한 것을 묻고 계시네요",
          "original_form": "하 당연한 것을 묻고 계시네요",
          "speaker_id": "2",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026941.1.5",
          "form": "랜선집사입니다",
          "original_form": "랜선집사입니다",
          "speaker_id": "2",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026941.1.6",
          "form": "전 실제로 주인님을 모시고 있습니다",
          "original_form": "전 실제로 주인님을 모시고 있습니다",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026941.1.7",
          "form": "",
          "original_form": "{emoji:뿌듯}",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026941.1.8",
          "form": "이미 알고 잇자나? 울 냥이 ㅋㅋㅋㅋ",
          "original_form": "이미 알고 잇자나? 울 냥이 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026941.1.9",
          "form": "와 좋.겠.다.",
          "original_form": "와 좋.겠.다.",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026941.1.10",
          "form": "(영혼가득)",
          "original_form": "(영혼가득)",
          "speaker_id": "2",
          "time": "20210902 01:36"
        },
        {
          "id": "MDRW2100026941.1.11",
          "form": "요즘 우리 냥이 살빼려고",
          "original_form": "요즘 우리 냥이 살빼려고",
          "speaker_id": "1",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100026941.1.12",
          "form": "사료를 습식+건식 으로 바꾸고",
          "original_form": "사료를 습식+건식 으로 바꾸고",
          "speaker_id": "1",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100026941.1.13",
          "form": "메타볼릭에서 로얄캐닌으로 바꿧는데",
          "original_form": "메타볼릭에서 로얄캐닌으로 바꿧는데",
          "speaker_id": "1",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100026941.1.14",
          "form": "가족들이 아무도 습식을 안주고 자꾸 건식만 줘서",
          "original_form": "가족들이 아무도 습식을 안주고 자꾸 건식만 줘서",
          "speaker_id": "1",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100026941.1.15",
          "form": "불만이에요",
          "original_form": "불만이에요",
          "speaker_id": "1",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100026941.1.16",
          "form": "건식보다 습식이 체중감량에 좀 더 도움이 된다고 하거든요",
          "original_form": "건식보다 습식이 체중감량에 좀 더 도움이 된다고 하거든요",
          "speaker_id": "1",
          "time": "20210902 01:41"
        },
        {
          "id": "MDRW2100026941.1.17",
          "form": "수분이 더 함유돼 있으니까 기초대사에 도움이 되나봐요",
          "original_form": "수분이 더 함유돼 있으니까 기초대사에 도움이 되나봐요",
          "speaker_id": "1",
          "time": "20210902 01:41"
        },
        {
          "id": "MDRW2100026941.1.18",
          "form": "꺄악",
          "original_form": "꺄악",
          "speaker_id": "2",
          "time": "20210902 01:42"
        },
        {
          "id": "MDRW2100026941.1.19",
          "form": "그래서 300g이 빠졌군요-!!",
          "original_form": "그래서 300g이 빠졌군요-!!",
          "speaker_id": "2",
          "time": "20210902 01:42"
        },
        {
          "id": "MDRW2100026941.1.20",
          "form": "습식으로 줄 수 있도록 사료 주는 곳에",
          "original_form": "습식으로 줄 수 있도록 사료 주는 곳에",
          "speaker_id": "2",
          "time": "20210902 01:44"
        },
        {
          "id": "MDRW2100026941.1.21",
          "form": "메모를 써보아요!",
          "original_form": "메모를 써보아요!",
          "speaker_id": "2",
          "time": "20210902 01:44"
        },
        {
          "id": "MDRW2100026941.1.22",
          "form": "냥이 급식표는 만들었는데",
          "original_form": "냥이 급식표는 만들었는데",
          "speaker_id": "1",
          "time": "20210902 01:45"
        },
        {
          "id": "MDRW2100026941.1.23",
          "form": "적어놔도 귀찮은지 다들 그냥 건식만 주고",
          "original_form": "적어놔도 귀찮은지 다들 그냥 건식만 주고",
          "speaker_id": "1",
          "time": "20210902 01:45"
        },
        {
          "id": "MDRW2100026941.1.24",
          "form": "얼마전에 name3가 건식만 주는걸 목격해서",
          "original_form": "얼마전에 &name3&가 건식만 주는걸 목격해서",
          "speaker_id": "1",
          "time": "20210902 01:45"
        },
        {
          "id": "MDRW2100026941.1.25",
          "form": "습식도 줘야해!!라고 하니까",
          "original_form": "습식도 줘야해!!라고 하니까",
          "speaker_id": "1",
          "time": "20210902 01:45"
        },
        {
          "id": "MDRW2100026941.1.26",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 01:45"
        },
        {
          "id": "MDRW2100026941.1.27",
          "form": "그건 누나가 줄때 그렇게 하고 나는 건식주면되지",
          "original_form": "그건 누나가 줄때 그렇게 하고 나는 건식주면되지",
          "speaker_id": "1",
          "time": "20210902 01:46"
        },
        {
          "id": "MDRW2100026941.1.28",
          "form": "하면서 거절당했습니다",
          "original_form": "하면서 거절당했습니다",
          "speaker_id": "1",
          "time": "20210902 01:46"
        },
        {
          "id": "MDRW2100026941.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 01:47"
        },
        {
          "id": "MDRW2100026941.1.30",
          "form": "고양이는 사랑이에요",
          "original_form": "고양이는 사랑이에요",
          "speaker_id": "2",
          "time": "20210902 01:47"
        },
        {
          "id": "MDRW2100026941.1.31",
          "form": "냥이는 평소에도 물을 잘 마시나요",
          "original_form": "냥이는 평소에도 물을 잘 마시나요",
          "speaker_id": "2",
          "time": "20210902 01:47"
        },
        {
          "id": "MDRW2100026941.1.32",
          "form": "많이 안마셔요",
          "original_form": "많이 안마셔요",
          "speaker_id": "1",
          "time": "20210902 01:50"
        },
        {
          "id": "MDRW2100026941.1.33",
          "form": "그래서 습식이 더 필요해요",
          "original_form": "그래서 습식이 더 필요해요",
          "speaker_id": "1",
          "time": "20210902 01:50"
        },
        {
          "id": "MDRW2100026941.1.34",
          "form": "냥이 습식지켜....!!!",
          "original_form": "냥이 습식지켜....!!!",
          "speaker_id": "2",
          "time": "20210902 01:51"
        },
        {
          "id": "MDRW2100026941.1.35",
          "form": "고양이 권장 급수량은",
          "original_form": "고양이 권장 급수량은",
          "speaker_id": "1",
          "time": "20210902 01:51"
        },
        {
          "id": "MDRW2100026941.1.36",
          "form": "kg당 200ml라고 하네요",
          "original_form": "kg당 200ml라고 하네요",
          "speaker_id": "1",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100026941.1.37",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100026941.1.38",
          "form": "아니 50ml요",
          "original_form": "아니 50ml요",
          "speaker_id": "1",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100026941.1.39",
          "form": "4배나 띄워버렷네요",
          "original_form": "4배나 띄워버렷네요",
          "speaker_id": "1",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100026941.1.40",
          "form": "초록창에서 찾아보셨군요",
          "original_form": "초록창에서 찾아보셨군요",
          "speaker_id": "2",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100026941.1.41",
          "form": "암튼 저희 냥이는 7키로니까",
          "original_form": "암튼 저희 냥이는 7키로니까",
          "speaker_id": "1",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100026941.1.42",
          "form": "350ml를 하루에 마셔야하는데",
          "original_form": "350ml를 하루에 마셔야하는데",
          "speaker_id": "1",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100026941.1.43",
          "form": "350은 커녕 하루에 50ml 마실까 말까한거 같아요",
          "original_form": "350은 커녕 하루에 50ml 마실까 말까한거 같아요",
          "speaker_id": "1",
          "time": "20210902 01:53"
        },
        {
          "id": "MDRW2100026941.1.44",
          "form": "거기에 움직이지도 않고",
          "original_form": "거기에 움직이지도 않고",
          "speaker_id": "1",
          "time": "20210902 01:53"
        },
        {
          "id": "MDRW2100026941.1.45",
          "form": "잠만 자고",
          "original_form": "잠만 자고",
          "speaker_id": "1",
          "time": "20210902 01:53"
        },
        {
          "id": "MDRW2100026941.1.46",
          "form": "살이 빠질래야 빠질수가 없네요",
          "original_form": "살이 빠질래야 빠질수가 없네요",
          "speaker_id": "1",
          "time": "20210902 01:53"
        },
        {
          "id": "MDRW2100026941.1.47",
          "form": "운동을 시켜줍시다",
          "original_form": "운동을 시켜줍시다",
          "speaker_id": "2",
          "time": "20210902 01:53"
        },
        {
          "id": "MDRW2100026941.1.48",
          "form": "집사가 모범을 보여주자구요",
          "original_form": "집사가 모범을 보여주자구요",
          "speaker_id": "2",
          "time": "20210902 01:53"
        },
        {
          "id": "MDRW2100026941.1.49",
          "form": "고양이 급수기는 사용 안하시나봐요",
          "original_form": "고양이 급수기는 사용 안하시나봐요",
          "speaker_id": "2",
          "time": "20210902 01:54"
        },
        {
          "id": "MDRW2100026941.1.50",
          "form": "네 급수기 따로 사용 안하고 매일마다 물만 갈아주고 있어요",
          "original_form": "네 급수기 따로 사용 안하고 매일마다 물만 갈아주고 있어요",
          "speaker_id": "1",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026941.1.51",
          "form": "사줘도...별로 안쓸것 같은...",
          "original_form": "사줘도...별로 안쓸것 같은...",
          "speaker_id": "1",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026941.1.52",
          "form": "저희 냥이가 겁이 너무 많아서",
          "original_form": "저희 냥이가 겁이 너무 많아서",
          "speaker_id": "1",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026941.1.53",
          "form": "신문물 사오면 항상 무서워해서요...",
          "original_form": "신문물 사오면 항상 무서워해서요...",
          "speaker_id": "1",
          "time": "20210902 01:55"
        },
        {
          "id": "MDRW2100026941.1.54",
          "form": "당신의 주인님은 무서운게 많으시군요...",
          "original_form": "당신의 주인님은 무서운게 많으시군요...",
          "speaker_id": "2",
          "time": "20210902 01:57"
        },
        {
          "id": "MDRW2100026941.1.55",
          "form": "유튭에 보면 고앵쓰 정수기로 물 챱챱 마시는게",
          "original_form": "유튭에 보면 고앵쓰 정수기로 물 챱챱 마시는게",
          "speaker_id": "2",
          "time": "20210902 01:57"
        },
        {
          "id": "MDRW2100026941.1.56",
          "form": "참 귀엽더라구요 쏘큩",
          "original_form": "참 귀엽더라구요 쏘큩",
