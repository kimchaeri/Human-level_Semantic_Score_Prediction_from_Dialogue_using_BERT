      "utterance": [
        {
          "id": "MDRW2100020597.1.1",
          "form": "뭔가 시리얼 이름 같은 거였어요",
          "original_form": "뭔가 시리얼 이름 같은 거였어요",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020597.1.2",
          "form": "슈퍼펭귄시리얼.. 이런건가..",
          "original_form": "슈퍼펭귄시리얼.. 이런건가..",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020597.1.3",
          "form": "....!",
          "original_form": "....!",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020597.1.4",
          "form": "ㅋㅋㅋ설마했는데",
          "original_form": "ㅋㅋㅋ설마했는데",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020597.1.5",
          "form": "그거에요?",
          "original_form": "그거에요?",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020597.1.6",
          "form": "와 최고다 진짜",
          "original_form": "와 최고다 진짜",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020597.1.7",
          "form": "나 그거 치고있는데 시리얼이라그래서ㅋㅋㅋㅋ",
          "original_form": "나 그거 치고있는데 시리얼이라그래서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020597.1.8",
          "form": "이름이 희안해서 다시 못찾을거같았는네ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이름이 희안해서 다시 못찾을거같았는네ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020597.1.9",
          "form": "진짜 고마워요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 고마워요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020597.1.10",
          "form": "저거 저번에 재출시됐었는데",
          "original_form": "저거 저번에 재출시됐었는데",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020597.1.11",
          "form": "다시 들어가더라고요",
          "original_form": "다시 들어가더라고요",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020597.1.12",
          "form": "재출시된거보면 찾는사람이 많다는 소리인데",
          "original_form": "재출시된거보면 찾는사람이 많다는 소리인데",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020597.1.13",
          "form": "맞아요 왜 내가 좋아하는건 다 들어가는지 모르겠어요",
          "original_form": "맞아요 왜 내가 좋아하는건 다 들어가는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020597.1.14",
          "form": "다시내줬으면",
          "original_form": "다시내줬으면",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020597.1.15",
          "form": "기업의 국룰인가봐요",
          "original_form": "기업의 국룰인가봐요",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020597.1.16",
          "form": "하 덕분에 찾았네요",
          "original_form": "하 덕분에 찾았네요",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020597.1.17",
          "form": "내가 좋아하면..단종시키는..",
          "original_form": "내가 좋아하면..단종시키는..",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020597.1.18",
          "form": "펭귄일줄이야",
          "original_form": "펭귄일줄이야",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020597.1.19",
          "form": "...그니까요 ㅠ",
          "original_form": "...그니까요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:18"
        }
      ]
