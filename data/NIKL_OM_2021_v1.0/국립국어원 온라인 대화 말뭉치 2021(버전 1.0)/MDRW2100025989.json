      "utterance": [
        {
          "id": "MDRW2100025989.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100025989.1.2",
          "form": "안녕하세요?ㅋ반갑습니다",
          "original_form": "안녕하세요?ㅋ반갑습니다",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025989.1.3",
          "form": "네에 주부신가여?",
          "original_form": "네에 주부신가여?",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025989.1.4",
          "form": "네 주부입니다ㅋ",
          "original_form": "네 주부입니다ㅋ",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025989.1.5",
          "form": "주부이세요?",
          "original_form": "주부이세요?",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025989.1.6",
          "form": "아뇨 전 30대후반이예여",
          "original_form": "아뇨 전 30대후반이예여",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025989.1.7",
          "form": "얼마전에 이사해서",
          "original_form": "얼마전에 이사해서",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025989.1.8",
          "form": "집안일 할게 많네여",
          "original_form": "집안일 할게 많네여",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025989.1.9",
          "form": "아 네네 집안일할게많죠ㅜㅜ",
          "original_form": "아 네네 집안일할게많죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025989.1.10",
          "form": "넘칩니다ㅋ",
          "original_form": "넘칩니다ㅋ",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025989.1.11",
          "form": "혹시 세탁기 청소팁 있으신가여",
          "original_form": "혹시 세탁기 청소팁 있으신가여",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025989.1.12",
          "form": "세탁조클리너인가 그거 한번 해야한다고 하더라구요",
          "original_form": "세탁조클리너인가 그거 한번 해야한다고 하더라구요",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025989.1.13",
          "form": "네네 세탁조클리너사용하면",
          "original_form": "네네 세탁조클리너사용하면",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100025989.1.14",
          "form": "깨끗해지는 느낌있어요ㅋㅋ",
          "original_form": "깨끗해지는 느낌있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100025989.1.15",
          "form": "저는 그렇게하다가",
          "original_form": "저는 그렇게하다가",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100025989.1.16",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100025989.1.17",
          "form": "3년에 한번씩",
          "original_form": "3년에 한번씩",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100025989.1.18",
          "form": "청소업체에 맡겨요ㅋ",
          "original_form": "청소업체에 맡겨요ㅋ",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100025989.1.19",
          "form": "세탁기 청소업체도 있군요",
          "original_form": "세탁기 청소업체도 있군요",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100025989.1.20",
          "form": "네네 ㅋ비용은 있지만 깨끗해져요ㅋㅋ",
          "original_form": "네네 ㅋ비용은 있지만 깨끗해져요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100025989.1.21",
          "form": "새것같이 ㅋㅋ",
          "original_form": "새것같이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100025989.1.22",
          "form": "오 서울살때는 세탁빨래 관심이 없다가",
          "original_form": "오 서울살때는 세탁빨래 관심이 없다가",
          "speaker_id": "1",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100025989.1.23",
          "form": "제주오니 습해서 꿉꿉한 냄새에 예민해지네요",
          "original_form": "제주오니 습해서 꿉꿉한 냄새에 예민해지네요",
          "speaker_id": "1",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100025989.1.24",
          "form": "그러시군요ㅋㅋ습하죠ㅜㅜ비땜에 더 그런듯요.",
          "original_form": "그러시군요ㅋㅋ습하죠ㅜㅜ비땜에 더 그런듯요.",
          "speaker_id": "2",
          "time": "20210901 19:02"
        }
      ]
