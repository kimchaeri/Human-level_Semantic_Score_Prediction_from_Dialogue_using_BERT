      "utterance": [
        {
          "id": "MDRW2100021909.1.1",
          "form": "갑자기 더워졌어요  ..",
          "original_form": "갑자기 더워졌어요  ..",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100021909.1.2",
          "form": "그래요? 비가 계속 와서 서늘하지 않나요?",
          "original_form": "그래요? 비가 계속 와서 서늘하지 않나요?",
          "speaker_id": "1",
          "time": "20210829 08:55"
        },
        {
          "id": "MDRW2100021909.1.3",
          "form": "어제는 조금 더웠는데 다시 쌀쌀하네요 ㅎㅎ",
          "original_form": "어제는 조금 더웠는데 다시 쌀쌀하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:59"
        },
        {
          "id": "MDRW2100021909.1.4",
          "form": "처서가 지났다더니...ㅋㅋㅌ",
          "original_form": "처서가 지났다더니...ㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100021909.1.5",
          "form": "비는 좀 그만왔으면 좋겠어요ㅜㅜ 빨래가..",
          "original_form": "비는 좀 그만왔으면 좋겠어요ㅜㅜ 빨래가..",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100021909.1.6",
          "form": "ㅇㅅㅠ 건조기가 필요해요ㅠㅜ",
          "original_form": "ㅇㅅㅠ 건조기가 필요해요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210829 20:14"
        },
        {
          "id": "MDRW2100021909.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 20:14"
        },
        {
          "id": "MDRW2100021909.1.8",
          "form": "집이 좁아서 건조기를 못사는데..",
          "original_form": "집이 좁아서 건조기를 못사는데..",
          "speaker_id": "2",
          "time": "20210829 20:14"
        },
        {
          "id": "MDRW2100021909.1.9",
          "form": "저번 여름부터 너무 간절해요",
          "original_form": "저번 여름부터 너무 간절해요",
          "speaker_id": "2",
          "time": "20210829 20:14"
        },
        {
          "id": "MDRW2100021909.1.10",
          "form": "다우니 실내건조 세제? 그거 사보셨어요?",
          "original_form": "다우니 실내건조 세제? 그거 사보셨어요?",
          "speaker_id": "1",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100021909.1.11",
          "form": "헉 아니요?!",
          "original_form": "헉 아니요?!",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100021909.1.12",
          "form": "그런게있나요?",
          "original_form": "그런게있나요?",
          "speaker_id": "2",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100021909.1.13",
          "form": "후기로는 나쁘지 않았던 것 같아요",
          "original_form": "후기로는 나쁘지 않았던 것 같아요",
          "speaker_id": "1",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100021909.1.14",
          "form": "한번 시도해볼게요 ㅎㅎ",
          "original_form": "한번 시도해볼게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100021909.1.15",
          "form": "잘 맞았으면 좋겠네요",
          "original_form": "잘 맞았으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100021909.1.16",
          "form": "네네ㅎ 정보감사해요!",
          "original_form": "네네ㅎ 정보감사해요!",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
