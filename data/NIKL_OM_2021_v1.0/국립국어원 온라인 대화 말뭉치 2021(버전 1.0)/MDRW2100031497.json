        {
          "id": "MDRW2100031497.1.1",
          "form": "집안일 힘들죠ㅠ",
          "original_form": "집안일 힘들죠ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100031497.1.2",
          "form": "그쵸 ㅠ 쉽지 않죠",
          "original_form": "그쵸 ㅠ 쉽지 않죠",
          "speaker_id": "2",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100031497.1.3",
          "form": "재벌되서 사람쓰고 싶네요ㅠㅠ",
          "original_form": "재벌되서 사람쓰고 싶네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031497.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031497.1.5",
          "form": "공감합니당...",
          "original_form": "공감합니당...",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031497.1.6",
          "form": "너무 힘들죠 뭐든 해도해도 도돌이표..ㅠㅠㅠ",
          "original_form": "너무 힘들죠 뭐든 해도해도 도돌이표..ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100031497.1.7",
          "form": "맞아요ㅜㅜ 티도 안나고",
          "original_form": "맞아요ㅜㅜ 티도 안나고",
          "speaker_id": "1",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100031497.1.8",
          "form": "돌아서면 금방 더러워지고",
          "original_form": "돌아서면 금방 더러워지고",
          "speaker_id": "1",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100031497.1.9",
          "form": "맞아요..ㅠㅠ",
          "original_form": "맞아요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 22:34"
        },
        {
          "id": "MDRW2100031497.1.10",
          "form": "집안일 중에 제일 하기 싫으신게 있으신가요?",
          "original_form": "집안일 중에 제일 하기 싫으신게 있으신가요?",
          "speaker_id": "2",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100031497.1.11",
          "form": "화장실청소요ㅜㅜ",
          "original_form": "화장실청소요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 22:35"
        },
        {
          "id": "MDRW2100031497.1.12",
          "form": "설거지였는데 식세기 샀어요",
          "original_form": "설거지였는데 식세기 샀어요",
          "speaker_id": "1",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100031497.1.13",
          "form": "아 ㅜㅜ 그러시군요ㅜㅜ",
          "original_form": "아 ㅜㅜ 그러시군요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 22:38"
        },
        {
          "id": "MDRW2100031497.1.14",
          "form": "특히 여름에 화장실청소는 더 힘든것같습니다..ㅜㅜ",
          "original_form": "특히 여름에 화장실청소는 더 힘든것같습니다..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100031497.1.15",
          "form": "더운데 습하고..",
          "original_form": "더운데 습하고..",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100031497.1.16",
          "form": "바로바로 했었는데 식세기에 넣으려고 쌓아놓게되네요..",
          "original_form": "바로바로 했었는데 식세기에 넣으려고 쌓아놓게되네요..",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100031497.1.17",
          "form": "금방 물때껴서ㅠㅠ",
          "original_form": "금방 물때껴서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100031497.1.18",
          "form": "아..ㅎㅎ그쵸 조금 넣고 돌리면 아까우니까ㅠㅍ",
          "original_form": "아..ㅎㅎ그쵸 조금 넣고 돌리면 아까우니까ㅠㅍ",
          "speaker_id": "2",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100031497.1.19",
          "form": "화장실도... 로봇청소기 같은게 있었으면 좋겠어요 ㅎㅎ",
          "original_form": "화장실도... 로봇청소기 같은게 있었으면 좋겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100031497.1.20",
          "form": "자동으로 청소되는...",
          "original_form": "자동으로 청소되는...",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100031497.1.21",
