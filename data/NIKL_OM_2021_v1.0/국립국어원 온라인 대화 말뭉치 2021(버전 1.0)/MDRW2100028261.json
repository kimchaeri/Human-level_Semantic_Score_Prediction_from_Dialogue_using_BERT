      "utterance": [
        {
          "id": "MDRW2100028261.1.1",
          "form": "책 좋아하시나요,",
          "original_form": "책 좋아하시나요,",
          "speaker_id": "1",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028261.1.2",
          "form": "요즘 다시 읽고있어요 어떤 분야좋아하세요?",
          "original_form": "요즘 다시 읽고있어요 어떤 분야좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028261.1.3",
          "form": "저는 그냥 마음 속에만 품고있는 편이에요 ㅋㅋㅋ",
          "original_form": "저는 그냥 마음 속에만 품고있는 편이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028261.1.4",
          "form": "가끔 읽게되면 시나 소설 좋아해요",
          "original_form": "가끔 읽게되면 시나 소설 좋아해요",
          "speaker_id": "1",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028261.1.5",
          "form": "ㅋㅋ저도 소설 위주로 봐요ㅎㅎ 흥미위주ㅋㅋ",
          "original_form": "ㅋㅋ저도 소설 위주로 봐요ㅎㅎ 흥미위주ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100028261.1.6",
          "form": "윌라로 한번 책을 들어봤는데 나름 괜찮더라구요",
          "original_form": "윌라로 한번 책을 들어봤는데 나름 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028261.1.7",
          "form": "성우 목소리가 책이랑도 어울려서 정말 좋았어요",
          "original_form": "성우 목소리가 책이랑도 어울려서 정말 좋았어요",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028261.1.8",
          "form": "아 진짜요? 저는 그냥 로봇이 읽어주는거만 들어봤는데ㅋㅋ",
          "original_form": "아 진짜요? 저는 그냥 로봇이 읽어주는거만 들어봤는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100028261.1.9",
          "form": "다음에 윌라 찾아봐야겠어요!",
          "original_form": "다음에 윌라 찾아봐야겠어요!",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100028261.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028261.1.11",
          "form": "저는 성우가 읽어주는걸로 들었어요",
          "original_form": "저는 성우가 읽어주는걸로 들었어요",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028261.1.12",
          "form": "배우가 읽어주는 것도 있더라고요 네이버 이북에는",
          "original_form": "배우가 읽어주는 것도 있더라고요 네이버 이북에는",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028261.1.13",
          "form": "이북을 살 수밖에 없네요ㅋㅋ 눈이 아파서 종이책으로 보려고했는데요ㅎㅎ",
          "original_form": "이북을 살 수밖에 없네요ㅋㅋ 눈이 아파서 종이책으로 보려고했는데요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028261.1.14",
          "form": "저는 이북으로 보게되묜 자꾸 핸드폰 다른 짓을 하게돼섴ㅋㅋㅋ",
          "original_form": "저는 이북으로 보게되묜 자꾸 핸드폰 다른 짓을 하게돼섴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028261.1.15",
          "form": "읽다가 카톧알람오면 갔다오고",
          "original_form": "읽다가 카톧알람오면 갔다오고",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028261.1.16",
          "form": "인스타 디엠오면 보게되고 ㅋㅋ",
          "original_form": "인스타 디엠오면 보게되고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028261.1.17",
          "form": "집중이 잘안돼요ㅠ",
          "original_form": "집중이 잘안돼요ㅠ",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028261.1.18",
          "form": "맞아요 폰을 아예 꺼놓고 해야지 안그럼 계속 확인하게 돼요ㅡㅡ",
          "original_form": "맞아요 폰을 아예 꺼놓고 해야지 안그럼 계속 확인하게 돼요ㅡㅡ",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028261.1.19",
          "form": "ㅡㅜ",
          "original_form": "ㅡㅜ",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028261.1.20",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028261.1.21",
          "form": "그리고 종이책이 주는 감성도 있는거같아요",
          "original_form": "그리고 종이책이 주는 감성도 있는거같아요",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028261.1.22",
          "form": "맞아요 그 감성 전자책으로 따라잡긴 힘들어요",
          "original_form": "맞아요 그 감성 전자책으로 따라잡긴 힘들어요",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028261.1.23",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:47"
        }
      ]
    }
