        {
          "id": "MDRW2100027623.1.1",
          "form": "그래서 어제 그렇게 데이트하고 끝이에요?ㅋㅋ",
          "original_form": "그래서 어제 그렇게 데이트하고 끝이에요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027623.1.2",
          "form": "뭐..ㅎㅎㅎ 아니요 ...ㅎㅎㅎㅎ",
          "original_form": "뭐..ㅎㅎㅎ 아니요 ...ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100027623.1.3",
          "form": "패배하고 시무룩했어요?",
          "original_form": "패배하고 시무룩했어요?",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027623.1.4",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027623.1.5",
          "form": "아니요 뭐 불꽃같이 타올랐죠",
          "original_form": "아니요 뭐 불꽃같이 타올랐죠",
          "speaker_id": "1",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027623.1.6",
          "form": "오락실 동물원",
          "original_form": "오락실 동물원",
          "speaker_id": "2",
          "time": "20210902 13:54"
        },
        {
          "id": "MDRW2100027623.1.7",
          "form": "진짜 연애초기에 자주갔던",
          "original_form": "진짜 연애초기에 자주갔던",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027623.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027623.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027623.1.10",
          "form": "이제 한 10일 뒤에나",
          "original_form": "이제 한 10일 뒤에나",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027623.1.11",
          "form": "여친사격했을때 뒤에서 찍은사진 아직도 있네요 ㅋㅋ",
          "original_form": "여친사격했을때 뒤에서 찍은사진 아직도 있네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027623.1.12",
          "form": "만나야 될거 같아서",
          "original_form": "만나야 될거 같아서",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027623.1.13",
          "form": "주말에 잠깐도 안봐요?",
          "original_form": "주말에 잠깐도 안봐요?",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027623.1.14",
          "form": "방탈출 하러갈까 생각중이에옷",
          "original_form": "방탈출 하러갈까 생각중이에옷",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027623.1.15",
          "form": "아님 평일에",
          "original_form": "아님 평일에",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027623.1.16",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "2",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027623.1.17",
          "form": "네 토욜 백신맞고 상태안좋을거같은데",
          "original_form": "네 토욜 백신맞고 상태안좋을거같은데",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100027623.1.18",
          "form": "2년차때 자주갔던 방탈출",
          "original_form": "2년차때 자주갔던 방탈출",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027623.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027623.1.20",
          "form": "일욜은 남친이 당직이라 ㅋㅋㅋ",
          "original_form": "일욜은 남친이 당직이라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027623.1.21",
          "form": "아 당직",
          "original_form": "아 당직",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027623.1.22",
          "form": "ㅌㅋㅋㅋㅋㅋ2년차에 가셨군오",
          "original_form": "ㅌㅋㅋㅋㅋㅋ2년차에 가셨군오",
          "speaker_id": "1",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027623.1.23",
          "form": "그래서 많이 싸우셨나",
          "original_form": "그래서 많이 싸우셨나",
          "speaker_id": "1",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027623.1.24",
          "form": "힌트가지고",
          "original_form": "힌트가지고",
          "speaker_id": "1",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027623.1.25",
          "form": "방탈출 못해서 싸운?",
          "original_form": "방탈출 못해서 싸운?",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027623.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027623.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027623.1.28",
          "form": "여친옆에 솔로친구가 있었는데",
          "original_form": "여친옆에 솔로친구가 있었는데",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027623.1.29",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027623.1.30",
          "form": "그친구가 자꾸",
          "original_form": "그친구가 자꾸",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027623.1.31",
          "form": "데이트중에 연락하고",
          "original_form": "데이트중에 연락하고",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027623.1.32",
          "form": "보자고 그러고",
          "original_form": "보자고 그러고",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027623.1.33",
          "form": "남사친이에옷??",
          "original_form": "남사친이에옷??",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027623.1.34",
          "form": "아뇨 여자요",
          "original_form": "아뇨 여자요",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027623.1.35",
          "form": "남사친 아예 없어요",
          "original_form": "남사친 아예 없어요",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027623.1.36",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027623.1.37",
          "form": "전 연애하면서 여사친 연락 다 끊겼는데",
          "original_form": "전 연애하면서 여사친 연락 다 끊겼는데",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027623.1.38",
          "form": "참다참다 1년참고 터짐...요",
          "original_form": "참다참다 1년참고 터짐...요",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027623.1.39",
          "form": "적당히해야지....",
          "original_form": "적당히해야지....",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027623.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋ왜 그랬지 그친구는",
          "original_form": "ㅋㅋㅋㅋㅋㅋ왜 그랬지 그친구는",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027623.1.41",
          "form": "지금도 그러는데",
          "original_form": "지금도 그러는데",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027623.1.42",
          "form": "이젠 그러려니 합니다",
          "original_form": "이젠 그러려니 합니다",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027623.1.43",
          "form": "아 진짜요",
          "original_form": "아 진짜요",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027623.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027623.1.45",
          "form": "와 집착 엄청 하는 친구네요",
          "original_form": "와 집착 엄청 하는 친구네요",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027623.1.46",
          "form": "그때보단 덜해요",
          "original_form": "그때보단 덜해요",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027623.1.47",
          "form": "맨날보자고하고",
          "original_form": "맨날보자고하고",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027623.1.48",
          "form": "와오",
          "original_form": "와오",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027623.1.49",
          "form": "나랑보기로한 영화 같이보자고 하고",
          "original_form": "나랑보기로한 영화 같이보자고 하고",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027623.1.50",
          "form": "여친분도 피곤하겠네요",
          "original_form": "여친분도 피곤하겠네요",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027623.1.51",
          "form": "근데 여친이 또 그걸 같이 보고서",
          "original_form": "근데 여친이 또 그걸 같이 보고서",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027623.1.52",
          "form": "나랑 또봐도 된다고",
          "original_form": "나랑 또봐도 된다고",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027623.1.53",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027623.1.54",
