      "utterance": [
        {
          "id": "MDRW2100029940.1.1",
          "form": "무슨 책이 궁금하신지요",
          "original_form": "무슨 책이 궁금하신지요",
          "speaker_id": "2",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029940.1.2",
          "form": "음... 그럼...",
          "original_form": "음... 그럼...",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100029940.1.3",
          "form": "내가 호응해줄 수 있는 책은 뭐가 있을까",
          "original_form": "내가 호응해줄 수 있는 책은 뭐가 있을까",
          "speaker_id": "1",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100029940.1.4",
          "form": "아까 그 코렛트 얘기를 해주자면",
          "original_form": "아까 그 코렛트 얘기를 해주자면",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029940.1.5",
          "form": "하데스가 남주잖아",
          "original_form": "하데스가 남주잖아",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029940.1.6",
          "form": "이건 약사 여주가 하데스를 치료하는 거라",
          "original_form": "이건 약사 여주가 하데스를 치료하는 거라",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029940.1.7",
          "form": "근데 치료도 금세 끝나고",
          "original_form": "근데 치료도 금세 끝나고",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029940.1.8",
          "form": "둘이 연애하는 얘기임",
          "original_form": "둘이 연애하는 얘기임",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029940.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나 그 제우스 나오고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ나 그 제우스 나오고",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029940.1.10",
          "form": "코렛트가 나중에 페르세포네 될거임",
          "original_form": "코렛트가 나중에 페르세포네 될거임",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029940.1.11",
          "form": "급노잼되서 멈춤",
          "original_form": "급노잼되서 멈춤",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029940.1.12",
          "form": "페르세포네 이름 중 하나가 코렛트인가가 있어서",
          "original_form": "페르세포네 이름 중 하나가 코렛트인가가 있어서",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.13",
          "form": "ㅇㅇ 그것도 좀",
          "original_form": "ㅇㅇ 그것도 좀",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.14",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.15",
          "form": "넘 장기화됐지",
          "original_form": "넘 장기화됐지",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.16",
          "form": "그런 연관성 좋네",
          "original_form": "그런 연관성 좋네",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.17",
          "form": "순정만화는 짧게쳐주라 제발",
          "original_form": "순정만화는 짧게쳐주라 제발",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.18",
          "form": "난 길게 읽고싶은 건",
          "original_form": "난 길게 읽고싶은 건",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.19",
          "form": "늘 빨리 끝나고",
          "original_form": "늘 빨리 끝나고",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.20",
          "form": "그래서 데메테르랑 좋은 연 만들고",
          "original_form": "그래서 데메테르랑 좋은 연 만들고",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.21",
          "form": "신과도 우호적이고ㅋㅋㅋ",
          "original_form": "신과도 우호적이고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.22",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.23",
          "form": "특히 요즘은",
          "original_form": "특히 요즘은",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029940.1.24",
          "form": "굳이 이렇게 길 필요가?싶으면",
          "original_form": "굳이 이렇게 길 필요가?싶으면",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029940.1.25",
          "form": "늘어져",
          "original_form": "늘어져",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029940.1.26",
          "form": "신작을 내서 흥한다는 전제가 없으니까",
          "original_form": "신작을 내서 흥한다는 전제가 없으니까",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029940.1.27",
          "form": "순정만화도 길게 늘려버더러",
          "original_form": "순정만화도 길게 늘려버더러",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029940.1.28",
          "form": "ㅜㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅜㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029940.1.29",
          "form": "옛날 3~5권짜리 생각하면",
          "original_form": "옛날 3~5권짜리 생각하면",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029940.1.30",
          "form": "몹시슬프지",
          "original_form": "몹시슬프지",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029940.1.31",
          "form": "세키네씨의 사랑 더 연재해주세요ㅠㅠㅠㅠㅠ",
          "original_form": "세키네씨의 사랑 더 연재해주세요ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029940.1.32",
          "form": "그런남주 진짜 흔하지않은데....",
          "original_form": "그런남주 진짜 흔하지않은데....",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029940.1.33",
          "form": "아쉬비야...",
          "original_form": "아쉬비야...",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029940.1.34",
          "form": "현대물보다 판타지가 늘리기 쉬운 건 알겟지만...",
          "original_form": "현대물보다 판타지가 늘리기 쉬운 건 알겟지만...",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029940.1.35",
          "form": "너에게 닿기를 도 길어지면서",
          "original_form": "너에게 닿기를 도 길어지면서",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029940.1.36",
          "form": "노잼됏으니까",
          "original_form": "노잼됏으니까",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029940.1.37",
          "form": "현대물도 늘어나고있오..",
          "original_form": "현대물도 늘어나고있오..",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029940.1.38",
          "form": "하나노이군의 상사병도",
          "original_form": "하나노이군의 상사병도",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029940.1.39",
          "form": "초반엔 귀여웟는데",
          "original_form": "초반엔 귀여웟는데",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029940.1.40",
          "form": "스피디하게 사귀고 꽁냥거리는 내용으로",
          "original_form": "스피디하게 사귀고 꽁냥거리는 내용으로",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029940.1.41",
          "form": "뭔가... 내용이없음",
          "original_form": "뭔가... 내용이없음",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029940.1.42",
          "form": "이거 보면서 반성한다",
          "original_form": "이거 보면서 반성한다",
          "speaker_id": "1",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100029940.1.43",
          "form": "아 꽁냥물은 은근 재미가없으니",
          "original_form": "아 꽁냥물은 은근 재미가없으니",
          "speaker_id": "1",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100029940.1.44",
          "form": "완결내야한다..",
          "original_form": "완결내야한다..",
          "speaker_id": "1",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100029940.1.45",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029940.1.46",
          "form": "계속 꽁냥을 유지하는 게 쉽지 않아서",
          "original_form": "계속 꽁냥을 유지하는 게 쉽지 않아서",
          "speaker_id": "2",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029940.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029940.1.48",
          "form": "근데....",
          "original_form": "근데....",
          "speaker_id": "1",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029940.1.49",
          "form": "사귀자마자완결은",
          "original_form": "사귀자마자완결은",
          "speaker_id": "1",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029940.1.50",
          "form": "너무해요..",
          "original_form": "너무해요..",
          "speaker_id": "1",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029940.1.51",
          "form": "제발 적당하 보여줘요...",
          "original_form": "제발 적당하 보여줘요...",
          "speaker_id": "1",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029940.1.52",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "2",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100029940.1.53",
          "form": "너에게닿기를이",
          "original_form": "너에게닿기를이",
          "speaker_id": "2",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100029940.1.54",
          "form": "친구연애 다보여줫잖아",
          "original_form": "친구연애 다보여줫잖아",
          "speaker_id": "2",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100029940.1.55",
          "form": "그냥 자르는게 맞아 냉ㅅ정",
          "original_form": "그냥 자르는게 맞아 냉ㅅ정",
          "speaker_id": "2",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100029940.1.56",
          "form": "ㅋㅋㅋㅋ친구연애 노관심이라구요",
          "original_form": "ㅋㅋㅋㅋ친구연애 노관심이라구요",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029940.1.57",
          "form": "제발 따로 연재했으면 퓨",
          "original_form": "제발 따로 연재했으면 퓨",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029940.1.58",
          "form": "외전잇자나 와전",
          "original_form": "외전잇자나 와전",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029940.1.59",
          "form": "외..전",
          "original_form": "외..전",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029940.1.60",
          "form": "너닿도 애니로 볼 때만해도 정말",
          "original_form": "너닿도 애니로 볼 때만해도 정말",
          "speaker_id": "1",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100029940.1.61",
          "form": "재밌눈 작품이었어",
          "original_form": "재밌눈 작품이었어",
          "speaker_id": "1",
