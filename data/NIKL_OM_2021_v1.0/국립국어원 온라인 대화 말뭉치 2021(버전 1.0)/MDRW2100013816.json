        },
        {
          "id": "MDRW2100013816.1.4",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013816.1.5",
          "form": "요즘 하시는 게임있으세요?",
          "original_form": "요즘 하시는 게임있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013816.1.6",
          "form": "저는 핸드폰 게임 중에",
          "original_form": "저는 핸드폰 게임 중에",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013816.1.7",
          "form": "꿈의 정원이라고 아세여?",
          "original_form": "꿈의 정원이라고 아세여?",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013816.1.8",
          "form": "그거해요",
          "original_form": "그거해요",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013816.1.9",
          "form": "아 저는 모바일 게임은 요즘",
          "original_form": "아 저는 모바일 게임은 요즘",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013816.1.10",
          "form": "안하고",
          "original_form": "안하고",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013816.1.11",
          "form": "아 해리포터게임조금 해요",
          "original_form": "아 해리포터게임조금 해요",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013816.1.12",
          "form": "pc로 하는거에여?",
          "original_form": "pc로 하는거에여?",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013816.1.13",
          "form": "아니오 해리포터 미스터리 라는 모바일 게임이있어욯ㅎ",
          "original_form": "아니오 해리포터 미스터리 라는 모바일 게임이있어욯ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013816.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013816.1.15",
          "form": "재밌어보여요",
          "original_form": "재밌어보여요",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013816.1.16",
          "form": "혹시 아이폰에서만 가능한 게임인가여?",
          "original_form": "혹시 아이폰에서만 가능한 게임인가여?",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013816.1.17",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013816.1.18",
          "form": "안드로이드도 이써요",
          "original_form": "안드로이드도 이써요",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013816.1.19",
          "form": "꿈의정원은 퍼즐 게임인가요?",
          "original_form": "꿈의정원은 퍼즐 게임인가요?",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013816.1.20",
          "form": "그 애니팡 같은",
          "original_form": "그 애니팡 같은",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013816.1.21",
          "form": "그런거에여",
          "original_form": "그런거에여",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013816.1.22",
          "form": "아 그렇군요 pc게임은 하시나요?",
          "original_form": "아 그렇군요 pc게임은 하시나요?",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013816.1.23",
          "form": "저는 심즈 좋아해요ㅎㅎ!",
          "original_form": "저는 심즈 좋아해요ㅎㅎ!",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013816.1.24",
          "form": "저는 pc는 초딩때 하다가 안해여",
          "original_form": "저는 pc는 초딩때 하다가 안해여",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013816.1.25",
          "form": "심즈는 재밌어 보이더라구요",
          "original_form": "심즈는 재밌어 보이더라구요",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013816.1.26",
          "form": "심즈 때문에 할까말까 고민 중이에여 ㅋㅋㅋ",
          "original_form": "심즈 때문에 할까말까 고민 중이에여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013816.1.27",
          "form": "ㅎㅎ 마자요 심즈 모바일이라는 게임아시나요?",
          "original_form": "ㅎㅎ 마자요 심즈 모바일이라는 게임아시나요?",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013816.1.28",
          "form": "아뇨아뇨 그건 몰라요",
          "original_form": "아뇨아뇨 그건 몰라요",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013816.1.29",
          "form": "심즈 게임 사시기 전에 심즈 모바일 해보시면 좋을 것 같아요",
          "original_form": "심즈 게임 사시기 전에 심즈 모바일 해보시면 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013816.1.30",
          "form": "심즈는 유저들이 아이템 만드는 게 좋아요ㅎㅎ!",
          "original_form": "심즈는 유저들이 아이템 만드는 게 좋아요ㅎㅎ!",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013816.1.31",
          "form": "모바일은 그런부분이 안되서 조금 아쉽긴 한데",
          "original_form": "모바일은 그런부분이 안되서 조금 아쉽긴 한데",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013816.1.32",
          "form": "그래도맛보기로 좋을것같아요",
          "original_form": "그래도맛보기로 좋을것같아요",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013816.1.33",
          "form": "오 추천 감사해용",
          "original_form": "오 추천 감사해용",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013816.1.34",
          "form": "한번 깔아서 해볼게여",
          "original_form": "한번 깔아서 해볼게여",
          "speaker_id": "1",
          "time": "20210825 16:14"
        }
      ]
