      "utterance": [
        {
          "id": "MDRW2100039271.1.1",
          "form": "츠케멘 내놔",
          "original_form": "츠케멘 내놔",
          "speaker_id": "2",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039271.1.2",
          "form": "츠케멘은 진짜 언제먹어",
          "original_form": "츠케멘은 진짜 언제먹어",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039271.1.3",
          "form": "국내에 찾으면 있을거같아",
          "original_form": "국내에 찾으면 있을거같아",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039271.1.4",
          "form": "셩지엔은 내가 찾아봤는데 상하이만 나오더라ㅎ",
          "original_form": "셩지엔은 내가 찾아봤는데 상하이만 나오더라ㅎ",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039271.1.5",
          "form": "츠케멘은 은데",
          "original_form": "츠케멘은 은데",
          "speaker_id": "2",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100039271.1.6",
          "form": "한국에서 많이먹었는데",
          "original_form": "한국에서 많이먹었는데",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039271.1.7",
          "form": "그맛이안나",
          "original_form": "그맛이안나",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039271.1.8",
          "form": "면부터 달라",
          "original_form": "면부터 달라",
          "speaker_id": "1",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039271.1.9",
          "form": "몰까...",
          "original_form": "몰까...",
          "speaker_id": "1",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039271.1.10",
          "form": "맛의 비결...방사능인가",
          "original_form": "맛의 비결...방사능인가",
          "speaker_id": "1",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039271.1.11",
          "form": "근데 잏본에서도",
          "original_form": "근데 잏본에서도",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100039271.1.12",
          "form": "오사카 거기가맛있오",
          "original_form": "오사카 거기가맛있오",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039271.1.13",
          "form": "ㅇㅈㅇㅈ 거기맛있었다",
          "original_form": "ㅇㅈㅇㅈ 거기맛있었다",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039271.1.14",
          "form": "두번 먹을만했어",
          "original_form": "두번 먹을만했어",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039271.1.15",
          "form": "세번먹지않았냐",
          "original_form": "세번먹지않았냐",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100039271.1.16",
          "form": "안니야 두번",
          "original_form": "안니야 두번",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039271.1.17",
          "form": "다음에가면 대짜다",
          "original_form": "다음에가면 대짜다",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039271.1.18",
          "form": "하루는 내가 너무 굶고먹어서 잘못먹었고 그다음날 완식한듯",
          "original_form": "하루는 내가 너무 굶고먹어서 잘못먹었고 그다음날 완식한듯",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039271.1.19",
          "form": "린정 계란 두개..",
          "original_form": "린정 계란 두개..",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039271.1.20",
          "form": "인정 차슈도추가할거야",
          "original_form": "인정 차슈도추가할거야",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039271.1.21",
          "form": "조아",
          "original_form": "조아",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100039271.1.22",
          "form": "먹고나서 꼬치집 또가자",
          "original_form": "먹고나서 꼬치집 또가자",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100039271.1.23",
          "form": "맥주달려야지",
          "original_form": "맥주달려야지",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100039271.1.24",
