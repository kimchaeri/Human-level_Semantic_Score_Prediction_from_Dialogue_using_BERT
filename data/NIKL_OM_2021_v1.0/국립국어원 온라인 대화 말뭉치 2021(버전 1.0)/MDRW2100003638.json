      "utterance": [
        {
          "id": "MDRW2100003638.1.1",
          "form": "보통 보살펴주면",
          "original_form": "보통 보살펴주면",
          "speaker_id": "2",
          "time": "20210821 09:28"
        },
        {
          "id": "MDRW2100003638.1.2",
          "form": "마음을 열지 않나요",
          "original_form": "마음을 열지 않나요",
          "speaker_id": "2",
          "time": "20210821 09:28"
        },
        {
          "id": "MDRW2100003638.1.3",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:28"
        },
        {
          "id": "MDRW2100003638.1.4",
          "form": "쟤는 지가 왕이에요",
          "original_form": "쟤는 지가 왕이에요",
          "speaker_id": "1",
          "time": "20210821 09:28"
        },
        {
          "id": "MDRW2100003638.1.5",
          "form": "진돗개도 이겨먹어요",
          "original_form": "진돗개도 이겨먹어요",
          "speaker_id": "1",
          "time": "20210821 09:28"
        },
        {
          "id": "MDRW2100003638.1.6",
          "form": "사실 개가 봐주는거긴 한데",
          "original_form": "사실 개가 봐주는거긴 한데",
          "speaker_id": "1",
          "time": "20210821 09:28"
        },
        {
          "id": "MDRW2100003638.1.7",
          "form": "근데 원래",
          "original_form": "근데 원래",
          "speaker_id": "2",
          "time": "20210821 09:28"
        },
        {
          "id": "MDRW2100003638.1.8",
          "form": "고양이가 그렇자나요",
          "original_form": "고양이가 그렇자나요",
          "speaker_id": "2",
          "time": "20210821 09:28"
        },
        {
          "id": "MDRW2100003638.1.9",
          "form": "냥바냥",
          "original_form": "냥바냥",
          "speaker_id": "1",
          "time": "20210821 09:29"
        },
        {
          "id": "MDRW2100003638.1.10",
          "form": "약간 욕심도 많고",
          "original_form": "약간 욕심도 많고",
          "speaker_id": "1",
          "time": "20210821 09:29"
        },
        {
          "id": "MDRW2100003638.1.11",
          "form": "좀 너무 오냐오냐 키워서 그런가",
          "original_form": "좀 너무 오냐오냐 키워서 그런가",
          "speaker_id": "1",
          "time": "20210821 09:29"
        },
        {
          "id": "MDRW2100003638.1.12",
          "form": "혼내주세요",
          "original_form": "혼내주세요",
          "speaker_id": "2",
          "time": "20210821 09:29"
        },
        {
          "id": "MDRW2100003638.1.13",
          "form": "자기가 잘못해놓고 되려 큰소리치는",
          "original_form": "자기가 잘못해놓고 되려 큰소리치는",
          "speaker_id": "1",
          "time": "20210821 09:29"
        },
        {
          "id": "MDRW2100003638.1.14",
          "form": "궁디를 팡팡",
          "original_form": "궁디를 팡팡",
          "speaker_id": "2",
          "time": "20210821 09:29"
        },
        {
          "id": "MDRW2100003638.1.15",
          "form": "뻔뻔함이 남다르기도 하고",
          "original_form": "뻔뻔함이 남다르기도 하고",
          "speaker_id": "1",
          "time": "20210821 09:29"
        },
        {
          "id": "MDRW2100003638.1.16",
          "form": "마당있는집에서",
          "original_form": "마당있는집에서",
          "speaker_id": "2",
          "time": "20210821 09:30"
        },
        {
          "id": "MDRW2100003638.1.17",
          "form": "그에비해 진돗개는 엄청 순하거든요",
          "original_form": "그에비해 진돗개는 엄청 순하거든요",
          "speaker_id": "1",
          "time": "20210821 09:30"
        },
        {
          "id": "MDRW2100003638.1.18",
          "form": "리트리버 키우는게 로망이었는데",
          "original_form": "리트리버 키우는게 로망이었는데",
          "speaker_id": "2",
          "time": "20210821 09:30"
        },
        {
          "id": "MDRW2100003638.1.19",
          "form": "진돗개도 좋네요",
          "original_form": "진돗개도 좋네요",
          "speaker_id": "2",
          "time": "20210821 09:30"
        },
        {
          "id": "MDRW2100003638.1.20",
          "form": "리트리버 좋죠",
          "original_form": "리트리버 좋죠",
          "speaker_id": "1",
          "time": "20210821 09:30"
        },
        {
          "id": "MDRW2100003638.1.21",
          "form": "착하고 순하고",
          "original_form": "착하고 순하고",
          "speaker_id": "1",
          "time": "20210821 09:30"
        },
        {
          "id": "MDRW2100003638.1.22",
          "form": "리트리버 애기",
          "original_form": "리트리버 애기",
          "speaker_id": "2",
          "time": "20210821 09:30"
        },
        {
          "id": "MDRW2100003638.1.23",
          "form": "진짜 말 안듣던데",
          "original_form": "진짜 말 안듣던데",
          "speaker_id": "2",
          "time": "20210821 09:31"
        },
        {
          "id": "MDRW2100003638.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:31"
        },
        {
          "id": "MDRW2100003638.1.25",
          "form": "애기들은ㅋㅋㅋ 다 그래요",
          "original_form": "애기들은ㅋㅋㅋ 다 그래요",
          "speaker_id": "1",
          "time": "20210821 09:31"
        },
        {
          "id": "MDRW2100003638.1.26",
          "form": "사회성을 만들어주는건 인간이 할 일이니까",
          "original_form": "사회성을 만들어주는건 인간이 할 일이니까",
          "speaker_id": "1",
          "time": "20210821 09:31"
        }
      ]
