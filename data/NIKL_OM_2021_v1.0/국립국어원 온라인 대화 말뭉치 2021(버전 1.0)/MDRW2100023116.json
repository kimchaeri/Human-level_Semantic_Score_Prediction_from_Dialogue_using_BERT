      "utterance": [
        {
          "id": "MDRW2100023116.1.1",
          "form": "음 조개구이?",
          "original_form": "음 조개구이?",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023116.1.2",
          "form": "조개 구이 맛있지",
          "original_form": "조개 구이 맛있지",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023116.1.3",
          "form": "여행가서 먹었던것 중에 젤 맛있었던거 있어?",
          "original_form": "여행가서 먹었던것 중에 젤 맛있었던거 있어?",
          "speaker_id": "2",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023116.1.4",
          "form": "너무 많은데?",
          "original_form": "너무 많은데?",
          "speaker_id": "1",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023116.1.5",
          "form": "ㅋㅋㅋㅋ 그래?",
          "original_form": "ㅋㅋㅋㅋ 그래?",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100023116.1.6",
          "form": "제주도에서 먹었던 것들이 주로 다 맛있었어",
          "original_form": "제주도에서 먹었던 것들이 주로 다 맛있었어",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023116.1.7",
          "form": "나도 제주도 가면 한 번 먹어봐야겠다",
          "original_form": "나도 제주도 가면 한 번 먹어봐야겠다",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023116.1.8",
          "form": "고등어회도 맛있고 생 전복 진짜 맛있어",
          "original_form": "고등어회도 맛있고 생 전복 진짜 맛있어",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023116.1.9",
          "form": "전복 맛있지 예전에 한 번 먹어봤는데",
          "original_form": "전복 맛있지 예전에 한 번 먹어봤는데",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023116.1.10",
          "form": "응 생으로 아무것도 안하고 그냥 뜯어 먹으면 진짜 맛있어",
          "original_form": "응 생으로 아무것도 안하고 그냥 뜯어 먹으면 진짜 맛있어",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023116.1.11",
          "form": "맞아 맛있더라고",
          "original_form": "맞아 맛있더라고",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023116.1.12",
          "form": "아 그렇게 먹어봤어?",
          "original_form": "아 그렇게 먹어봤어?",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023116.1.13",
          "form": "아니 생으로는 안먹어봤는데 그럴거 같아서",
          "original_form": "아니 생으로는 안먹어봤는데 그럴거 같아서",
          "speaker_id": "2",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023116.1.14",
          "form": "생으로 먹는 게 전복 향이 잘 느껴져서 정말 맛있어",
          "original_form": "생으로 먹는 게 전복 향이 잘 느껴져서 정말 맛있어",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023116.1.15",
          "form": "지금 커피 마시면 잠에 방해되겠지?",
          "original_form": "지금 커피 마시면 잠에 방해되겠지?",
          "speaker_id": "1",
          "time": "20210829 22:51"
        }
