      "utterance": [
        {
          "id": "MDRW2100017933.1.1",
          "form": "비타민제",
          "original_form": "비타민제",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017933.1.2",
          "form": "랑",
          "original_form": "랑",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017933.1.3",
          "form": "유산균먹네요!",
          "original_form": "유산균먹네요!",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017933.1.4",
          "form": "비타민도 종류가 여러가지던데 어떤거 챙겨먹어요?",
          "original_form": "비타민도 종류가 여러가지던데 어떤거 챙겨먹어요?",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017933.1.5",
          "form": "저는 주로 제품은 솔가!",
          "original_form": "저는 주로 제품은 솔가!",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017933.1.6",
          "form": "솔가제품꺼먹구",
          "original_form": "솔가제품꺼먹구",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017933.1.7",
          "form": "종합비타민제 먹어용",
          "original_form": "종합비타민제 먹어용",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017933.1.8",
          "form": "아 현명하시네요 ㅎㅎ",
          "original_form": "아 현명하시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017933.1.9",
          "form": "저는 유산균 먹다가 효과 잘모르겠어서 안먹어요 가격도 만만치 않은데 ㅜㅜ",
          "original_form": "저는 유산균 먹다가 효과 잘모르겠어서 안먹어요 가격도 만만치 않은데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017933.1.10",
          "form": "유산균 같은 경우는 거기에 옭매이기 보단",
          "original_form": "유산균 같은 경우는 거기에 옭매이기 보단",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017933.1.11",
          "form": "유산균하고 같이 뭔가 좋은 습관들을 잡아여",
          "original_form": "유산균하고 같이 뭔가 좋은 습관들을 잡아여",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017933.1.12",
          "form": "효과가 증폭되더라고용",
          "original_form": "효과가 증폭되더라고용",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017933.1.13",
          "form": "그냥 유산균만 먹으니 효과가 거의없는거같아서",
          "original_form": "그냥 유산균만 먹으니 효과가 거의없는거같아서",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017933.1.14",
          "form": "아 좋은 습관은 어떤 것들 말하는거에요?",
          "original_form": "아 좋은 습관은 어떤 것들 말하는거에요?",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017933.1.15",
          "form": "물을 자주먹는 습관이라던자",
          "original_form": "물을 자주먹는 습관이라던자",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017933.1.16",
          "form": "식이섬유 들어간 채소들도 먹어주면 좋아용",
          "original_form": "식이섬유 들어간 채소들도 먹어주면 좋아용",
          "speaker_id": "2",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017933.1.17",
          "form": "물 먹는 하마가 되기 진짜 어렵더라구요",
          "original_form": "물 먹는 하마가 되기 진짜 어렵더라구요",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017933.1.18",
          "form": "하루에 2리터를 어떻게 마시냐고요 ㅜㅜ",
          "original_form": "하루에 2리터를 어떻게 마시냐고요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017933.1.19",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017933.1.20",
          "form": "저도 그래서 조금조금씩 늘렸죠",
          "original_form": "저도 그래서 조금조금씩 늘렸죠",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017933.1.21",
          "form": "님은 그렇게 먹고 있나요?ㅠㅠ",
          "original_form": "님은 그렇게 먹고 있나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017933.1.22",
          "form": "저도 지금은 2L는 다 못목고",
          "original_form": "저도 지금은 2L는 다 못목고",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017933.1.23",
          "form": "한 1.5L정돈 먹는거같아요",
          "original_form": "한 1.5L정돈 먹는거같아요",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017933.1.24",
          "form": "그래도 많이 드시네요 ㅜ",
          "original_form": "그래도 많이 드시네요 ㅜ",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017933.1.25",
          "form": "물값도 만만치 않으시겠는데요?",
          "original_form": "물값도 만만치 않으시겠는데요?",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017933.1.26",
          "form": "물값도 좀 나가지만",
          "original_form": "물값도 좀 나가지만",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017933.1.27",
          "form": "어쩔수없죠 건강위해선ㅋㅋㅋ",
          "original_form": "어쩔수없죠 건강위해선ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017933.1.28",
          "form": "그렇게 했더니 정말 효과 있었어요?",
          "original_form": "그렇게 했더니 정말 효과 있었어요?",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017933.1.29",
          "form": "전 있었어요!",
          "original_form": "전 있었어요!",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017933.1.30",
          "form": "사람 기호에 따라 다르긴하겠지만",
          "original_form": "사람 기호에 따라 다르긴하겠지만",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017933.1.31",
          "form": "우선 전 그렇게해서 변비 탈출했거든요 ㅎㅎ",
          "original_form": "우선 전 그렇게해서 변비 탈출했거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017933.1.32",
          "form": "오 진짜 좋으시겠어요",
          "original_form": "오 진짜 좋으시겠어요",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017933.1.33",
          "form": "식이섬유 많은 채소는 뭐가 있어요?",
          "original_form": "식이섬유 많은 채소는 뭐가 있어요?",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017933.1.34",
          "form": "네네 ㅠㅠ 진짜 이 변비는 해결하고싶었는데",
          "original_form": "네네 ㅠㅠ 진짜 이 변비는 해결하고싶었는데",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017933.1.35",
          "form": "식이섬유가 많은건 주로",
          "original_form": "식이섬유가 많은건 주로",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017933.1.36",
          "form": "고구마가 좀 대표적이긴하겠네용",
          "original_form": "고구마가 좀 대표적이긴하겠네용",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017933.1.37",
          "form": "그리고 과일인 사과하고",
          "original_form": "그리고 과일인 사과하고",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017933.1.38",
          "form": "미역도 많습니다!",
          "original_form": "미역도 많습니다!",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017933.1.39",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017933.1.40",
          "form": "근데 고구마 많이 먹고 살찐 강아지는 뭐죠 ㅠㅠ",
          "original_form": "근데 고구마 많이 먹고 살찐 강아지는 뭐죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017933.1.41",
          "form": "ㅋㅋㅋ큐ㅠㅠ 과하게 먹으면 안되긴하죵",
          "original_form": "ㅋㅋㅋ큐ㅠㅠ 과하게 먹으면 안되긴하죵",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017933.1.42",
          "form": "참고하겠습니다..",
          "original_form": "참고하겠습니다..",
          "speaker_id": "1",
          "time": "20210826 21:54"
        }
      ]
    }
