      "utterance": [
        {
          "id": "MDRW2100046209.1.1",
          "form": "근데 ㅇ우리나라는",
          "original_form": "근데 ㅇ우리나라는",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046209.1.2",
          "form": "해밀턴잘몰라서",
          "original_form": "해밀턴잘몰라서",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046209.1.3",
          "form": "내한도못올너낌",
          "original_form": "내한도못올너낌",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046209.1.4",
          "form": "미국역사인데다",
          "original_form": "미국역사인데다",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046209.1.5",
          "form": "해밀턴지식이없고",
          "original_form": "해밀턴지식이없고",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046209.1.6",
          "form": "랩뮤지컬이라",
          "original_form": "랩뮤지컬이라",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046209.1.7",
          "form": "로컬화가 쉽지않을듯",
          "original_form": "로컬화가 쉽지않을듯",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046209.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046209.1.9",
          "form": "진짜 맘먹고 준비하는거 아니면",
          "original_form": "진짜 맘먹고 준비하는거 아니면",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046209.1.10",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046209.1.11",
          "form": "웬만해선 올라올 일 없겟네..",
          "original_form": "웬만해선 올라올 일 없겟네..",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046209.1.12",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046209.1.13",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046209.1.14",
          "form": "그럴바에야 다른 라센을 사오겟지...",
          "original_form": "그럴바에야 다른 라센을 사오겟지...",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046209.1.15",
          "form": "우리나ㄱ뮤시장이발전해서",
          "original_form": "우리나ㄱ뮤시장이발전해서",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046209.1.16",
          "form": "위키드같은거",
          "original_form": "위키드같은거",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046209.1.17",
          "form": "올라오는것도 대단한느낌",
          "original_form": "올라오는것도 대단한느낌",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046209.1.18",
          "form": "이전엔약가못올라올거라",
          "original_form": "이전엔약가못올라올거라",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046209.1.19",
          "form": "다들철썩같이믿고있었는데",
          "original_form": "다들철썩같이믿고있었는데",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046209.1.20",
          "form": "어느순간 잘올라오는극이되었ㅇ7ㅏ",
          "original_form": "어느순간 잘올라오는극이되었ㅇ7ㅏ",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046209.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046209.1.22",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046209.1.23",
          "form": "정말",
          "original_form": "정말",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046209.1.24",
