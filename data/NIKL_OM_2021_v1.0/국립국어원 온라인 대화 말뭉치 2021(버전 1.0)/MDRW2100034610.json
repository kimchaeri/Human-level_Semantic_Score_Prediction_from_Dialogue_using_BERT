      "utterance": [
        {
          "id": "MDRW2100034610.1.1",
          "form": "놀러도 잘 다니는데요 ㅜ",
          "original_form": "놀러도 잘 다니는데요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100034610.1.2",
          "form": "아 진짜요ㅜ",
          "original_form": "아 진짜요ㅜ",
          "speaker_id": "2",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100034610.1.3",
          "form": "예전에 별일도 아닌였는데",
          "original_form": "예전에 별일도 아닌였는데",
          "speaker_id": "2",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100034610.1.4",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100034610.1.5",
          "form": "이제 진짜 소중한 일상이 되어버렸어요ㅠ",
          "original_form": "이제 진짜 소중한 일상이 되어버렸어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100034610.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100034610.1.7",
          "form": "일상을 잃어버린 느낌",
          "original_form": "일상을 잃어버린 느낌",
          "speaker_id": "1",
          "time": "20210828 16:17"
        },
        {
          "id": "MDRW2100034610.1.8",
          "form": "그래도 이제 치료제도 나오고 하면",
          "original_form": "그래도 이제 치료제도 나오고 하면",
          "speaker_id": "2",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100034610.1.9",
          "form": "조금은 더 예전처럼 살수있겠죠ㅠ",
          "original_form": "조금은 더 예전처럼 살수있겠죠ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100034610.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100034610.1.11",
          "form": "얼른 나왔으면 좋겠어요",
          "original_form": "얼른 나왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100034610.1.12",
          "form": "열심히 연구하고있으니까  곧 나오겠죠ㅠ",
          "original_form": "열심히 연구하고있으니까  곧 나오겠죠ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034610.1.13",
          "form": "백신을 엄청 믿었는데",
          "original_form": "백신을 엄청 믿었는데",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034610.1.14",
          "form": "델타변이때문에..ㅜㅜ",
          "original_form": "델타변이때문에..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034610.1.15",
          "form": "그니까요 새로운 변이 바이러스가  계속 나오니까",
          "original_form": "그니까요 새로운 변이 바이러스가  계속 나오니까",
          "speaker_id": "2",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034610.1.16",
          "form": "더 불안해져요ㅠ",
          "original_form": "더 불안해져요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034610.1.17",
          "form": "무서워요",
          "original_form": "무서워요",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034610.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034610.1.19",
          "form": "외국인들 입국하면서 들어오기도 하구여",
          "original_form": "외국인들 입국하면서 들어오기도 하구여",
          "speaker_id": "1",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100034610.1.20",
          "form": "그니까요ㅠ",
          "original_form": "그니까요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100034610.1.21",
          "form": "얼른 끝났으면 해요",
