      "utterance": [
        {
          "id": "MDRW2100013516.1.1",
          "form": "혹시 키우는 동물 있으세요?",
          "original_form": "혹시 키우는 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013516.1.2",
          "form": "아뇨 제 자신도 버거워서 ㅠㅠ",
          "original_form": "아뇨 제 자신도 버거워서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013516.1.3",
          "form": "name1님은 있으신가요?",
          "original_form": "&name1&님은 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013516.1.4",
          "form": "저희집은 강아지 키우고있어요",
          "original_form": "저희집은 강아지 키우고있어요",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013516.1.5",
          "form": "오 어떤 종 키우시나요?",
          "original_form": "오 어떤 종 키우시나요?",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013516.1.6",
          "form": "저희집은 시츄키우고 있어요",
          "original_form": "저희집은 시츄키우고 있어요",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013516.1.7",
          "form": "강아지 키우세요?",
          "original_form": "강아지 키우세요?",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013516.1.8",
          "form": "아녀 저는 동물 잘 못 돌볼 거 같아서 ㅋㅋㅋ",
          "original_form": "아녀 저는 동물 잘 못 돌볼 거 같아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013516.1.9",
          "form": "시츄 완전 귀엽겠네요!",
          "original_form": "시츄 완전 귀엽겠네요!",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013516.1.10",
          "form": "몇살인가요?",
          "original_form": "몇살인가요?",
          "speaker_id": "2",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013516.1.11",
          "form": "올해로 9살이네요",
          "original_form": "올해로 9살이네요",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013516.1.12",
          "form": "오 9살이면 사람 나이로 몇살인가요?",
          "original_form": "오 9살이면 사람 나이로 몇살인가요?",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013516.1.13",
          "form": "한 60살쯤 아닐까요?",
          "original_form": "한 60살쯤 아닐까요?",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013516.1.14",
          "form": "강아지 평균수명이 15살이라더라구요",
          "original_form": "강아지 평균수명이 15살이라더라구요",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013516.1.15",
          "form": "그래도 시츄는 좀 장수하는 편이라 들었어용!",
          "original_form": "그래도 시츄는 좀 장수하는 편이라 들었어용!",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013516.1.16",
          "form": "요즘은 산책시키기 힘드시겠어요",
          "original_form": "요즘은 산책시키기 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013516.1.17",
          "form": "저희 동네에 사람이 많은편은 아니라 그래도 괜찮은거같아요",
          "original_form": "저희 동네에 사람이 많은편은 아니라 그래도 괜찮은거같아요",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013516.1.18",
          "form": "강아지 더위는 안타나요?",
          "original_form": "강아지 더위는 안타나요?",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013516.1.19",
          "form": "더위는 안타는데 바람부는 소리 들릴때마다 날뛰어요",
          "original_form": "더위는 안타는데 바람부는 소리 들릴때마다 날뛰어요",
          "speaker_id": "1",
          "time": "20210825 16:34"
        }
