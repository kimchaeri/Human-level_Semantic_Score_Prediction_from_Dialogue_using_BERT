      "utterance": [
        {
          "id": "MDRW2100035086.1.1",
          "form": "초콜릿이랑 차종류 구입했었어요",
          "original_form": "초콜릿이랑 차종류 구입했었어요",
          "speaker_id": "1",
          "time": "20210829 02:21"
        },
        {
          "id": "MDRW2100035086.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210829 02:21"
        },
        {
          "id": "MDRW2100035086.1.3",
          "form": "오 차도 잇엇군요",
          "original_form": "오 차도 잇엇군요",
          "speaker_id": "2",
          "time": "20210829 02:21"
        },
        {
          "id": "MDRW2100035086.1.4",
          "form": "저는 초콜릿이랑 차종류 구매했어요",
          "original_form": "저는 초콜릿이랑 차종류 구매했어요",
          "speaker_id": "1",
          "time": "20210829 02:21"
        },
        {
          "id": "MDRW2100035086.1.5",
          "form": "저는 엄마 아는분덕분에 먹어봣는데 백련초크런치가 너무너무 맛잇더라고요",
          "original_form": "저는 엄마 아는분덕분에 먹어봣는데 백련초크런치가 너무너무 맛잇더라고요",
          "speaker_id": "2",
          "time": "20210829 02:21"
        },
        {
          "id": "MDRW2100035086.1.6",
          "form": "유자차 처럼 타먹을수 있던걸로 기억하는데",
          "original_form": "유자차 처럼 타먹을수 있던걸로 기억하는데",
          "speaker_id": "1",
          "time": "20210829 02:21"
        },
        {
          "id": "MDRW2100035086.1.7",
          "form": "결국 그걸 아무도 못 먹었어요ㅜㅜ",
          "original_form": "결국 그걸 아무도 못 먹었어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 02:22"
        },
        {
          "id": "MDRW2100035086.1.8",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210829 02:22"
        },
        {
          "id": "MDRW2100035086.1.9",
          "form": "엄마가 아껴두신다고 잘 보관하다가...",
          "original_form": "엄마가 아껴두신다고 잘 보관하다가...",
          "speaker_id": "1",
          "time": "20210829 02:23"
        },
        {
          "id": "MDRW2100035086.1.10",
          "form": "아... 마음씨가..ㅎ",
          "original_form": "아... 마음씨가..ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:23"
        },
        {
          "id": "MDRW2100035086.1.11",
          "form": "몇년지나서 찾았거든요ㅋㅋㅋㅋ",
          "original_form": "몇년지나서 찾았거든요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:23"
        },
        {
          "id": "MDRW2100035086.1.12",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210829 02:23"
        },
        {
          "id": "MDRW2100035086.1.13",
          "form": "곰팡이요?",
          "original_form": "곰팡이요?",
          "speaker_id": "2",
          "time": "20210829 02:23"
        },
        {
          "id": "MDRW2100035086.1.14",
          "form": "그걸 찾고도 이게 왜 여기에 있는건지 아무도 모를뻔 하다가",
          "original_form": "그걸 찾고도 이게 왜 여기에 있는건지 아무도 모를뻔 하다가",
          "speaker_id": "1",
          "time": "20210829 02:23"
        },
        {
          "id": "MDRW2100035086.1.15",
          "form": "헐....",
          "original_form": "헐....",
          "speaker_id": "2",
          "time": "20210829 02:23"
        },
        {
          "id": "MDRW2100035086.1.16",
          "form": "겨우 기억해냈답니다",
          "original_form": "겨우 기억해냈답니다",
          "speaker_id": "1",
          "time": "20210829 02:23"
        },
        {
          "id": "MDRW2100035086.1.17",
          "form": "몇면인데요?",
          "original_form": "몇면인데요?",
          "speaker_id": "2",
          "time": "20210829 02:23"
        },
        {
          "id": "MDRW2100035086.1.18",
          "form": "몇ㄴ.년인데요?",
          "original_form": "몇ㄴ.년인데요?",
          "speaker_id": "2",
          "time": "20210829 02:23"
        },
        {
          "id": "MDRW2100035086.1.19",
          "form": "자세히는 잘 기억나지 않아요",
          "original_form": "자세히는 잘 기억나지 않아요",
          "speaker_id": "1",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100035086.1.20",
          "form": "헐... 그러시구나...",
          "original_form": "헐... 그러시구나...",
