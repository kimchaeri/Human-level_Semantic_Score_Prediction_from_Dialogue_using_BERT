      "utterance": [
        {
          "id": "MDRW2100033682.1.1",
          "form": "알바 해보셨어요?",
          "original_form": "알바 해보셨어요?",
          "speaker_id": "1",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100033682.1.2",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100033682.1.3",
          "form": "저는 빵집알바를 햇었어요",
          "original_form": "저는 빵집알바를 햇었어요",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100033682.1.4",
          "form": "헉 힘드셨겠다",
          "original_form": "헉 힘드셨겠다",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100033682.1.5",
          "form": "베이킹이 그렇게 중노동이람서요",
          "original_form": "베이킹이 그렇게 중노동이람서요",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100033682.1.6",
          "form": "헉 아녀아녀 저는 그 빵포장..!!!",
          "original_form": "헉 아녀아녀 저는 그 빵포장..!!!",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100033682.1.7",
          "form": "저는 아주 쉬웠답니다",
          "original_form": "저는 아주 쉬웠답니다",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100033682.1.8",
          "form": "앗 다행이내요 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 다행이내요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100033682.1.9",
          "form": "빵집알바하는 친구가 맨날 욕하는거 들어서 그런가 ㅋㅋㅋ",
          "original_form": "빵집알바하는 친구가 맨날 욕하는거 들어서 그런가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100033682.1.10",
          "form": "괜찮았나요?",
          "original_form": "괜찮았나요?",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100033682.1.11",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100033682.1.12",
          "form": "친구가 정확합니다.",
          "original_form": "친구가 정확합니다.",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100033682.1.13",
          "form": "뭐라말했든 친구분 말에 100퍼 동의",
          "original_form": "뭐라말했든 친구분 말에 100퍼 동의",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100033682.1.14",
          "form": "아니 웃겨",
          "original_form": "아니 웃겨",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100033682.1.15",
          "form": "ㅠㅠㅠㅠㅜㅜㅜㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅜㅜㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100033682.1.16",
          "form": "진짜 왕힘든가 보네요...",
          "original_form": "진짜 왕힘든가 보네요...",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100033682.1.17",
          "form": "오픈이시던가요?",
          "original_form": "오픈이시던가요?",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100033682.1.18",
          "form": "혼자 하는 곳보다 두명이 하는 곳이 헬이에요",
          "original_form": "혼자 하는 곳보다 두명이 하는 곳이 헬이에요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100033682.1.19",
          "form": "한 명은 샌드위치 노예거든요",
          "original_form": "한 명은 샌드위치 노예거든요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100033682.1.20",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100033682.1.21",
          "form": "하루종일 샌드유ㅣ치 만들어요?",
          "original_form": "하루종일 샌드유ㅣ치 만들어요?",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100033682.1.22",
          "form": "네네 한 명은요",
          "original_form": "네네 한 명은요",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100033682.1.23",
          "form": "근데 한명은 계속 빵포장이랑 손님응대하고 커피만들어요",
          "original_form": "근데 한명은 계속 빵포장이랑 손님응대하고 커피만들어요",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100033682.1.24",
          "form": "근데 안바쁜 빵집은 꿀이에요",
          "original_form": "근데 안바쁜 빵집은 꿀이에요",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100033682.1.25",
          "form": "와 진짜 바쁜 빵집같네요...",
          "original_form": "와 진짜 바쁜 빵집같네요...",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100033682.1.26",
          "form": "친구는 빠바다녀서 바쁜것같더라고요",
          "original_form": "친구는 빠바다녀서 바쁜것같더라고요",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100033682.1.27",
          "form": "빠바다니시는 구나",
          "original_form": "빠바다니시는 구나",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100033682.1.28",
          "form": "저도 갠빵집 하다가 빠바로 옮겼오요!!",
          "original_form": "저도 갠빵집 하다가 빠바로 옮겼오요!!",
          "speaker_id": "2",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100033682.1.29",
          "form": "어느쪽이 더 힘들던가요!",
          "original_form": "어느쪽이 더 힘들던가요!",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100033682.1.30",
          "form": "갠빵집이요ㅜㅜㅜ",
          "original_form": "갠빵집이요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100033682.1.31",
          "form": "하ㅠㅠㅠㅠㅠ",
          "original_form": "하ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100033682.1.32",
          "form": "거기는 식빵 cm",
          "original_form": "거기는 식빵 cm",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100033682.1.33",
          "form": "조절해서 잘라줘야해요",
          "original_form": "조절해서 잘라줘야해요",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100033682.1.34",
          "form": "헐.....",
          "original_form": "헐.....",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100033682.1.35",
          "form": "그걸 직접하는거였어요????",
          "original_form": "그걸 직접하는거였어요????",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100033682.1.36",
          "form": "진짜 사람갈아나오는 빵",
          "original_form": "진짜 사람갈아나오는 빵",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100033682.1.37",
