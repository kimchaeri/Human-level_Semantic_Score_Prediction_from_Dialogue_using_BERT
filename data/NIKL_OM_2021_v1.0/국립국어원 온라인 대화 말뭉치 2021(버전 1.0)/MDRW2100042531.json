      "utterance": [
        {
          "id": "MDRW2100042531.1.1",
          "form": "야식이 제일신나",
          "original_form": "야식이 제일신나",
          "speaker_id": "2",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042531.1.2",
          "form": "막 담근 겉절이랑 밥 먹으면 진짜 맛있겠다",
          "original_form": "막 담근 겉절이랑 밥 먹으면 진짜 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042531.1.3",
          "form": "너네 집은 김치에 부재료 넣어?",
          "original_form": "너네 집은 김치에 부재료 넣어?",
          "speaker_id": "1",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042531.1.4",
          "form": "응 너무맛있어",
          "original_form": "응 너무맛있어",
          "speaker_id": "2",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042531.1.5",
          "form": "많이넣어",
          "original_form": "많이넣어",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042531.1.6",
          "form": "해산물 같은 거 넣어?",
          "original_form": "해산물 같은 거 넣어?",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042531.1.7",
          "form": "아 굴같은건 이,ㆍㄴ넣어",
          "original_form": "아 굴같은건 이,ㆍㄴ넣어",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042531.1.8",
          "form": "미나리나 쪽파 정도?",
          "original_form": "미나리나 쪽파 정도?",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042531.1.9",
          "form": "뭐라구요",
          "original_form": "뭐라구요",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042531.1.10",
          "form": "아 쪽파 맛있겠다",
          "original_form": "아 쪽파 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042531.1.11",
          "form": "파김치도 일타 이피로 담는구나 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "파김치도 일타 이피로 담는구나 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042531.1.12",
          "form": "파김치도 좋아하니",
          "original_form": "파김치도 좋아하니",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042531.1.13",
          "form": "응 파김치 짱좋아해",
          "original_form": "응 파김치 짱좋아해",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042531.1.14",
          "form": "그 알싸한 맛이 너무 좋아",
          "original_form": "그 알싸한 맛이 너무 좋아",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042531.1.15",
          "form": "맞아 양념 착착 쳐서 매콤하게 담그면 너무 맛있더라",
          "original_form": "맞아 양념 착착 쳐서 매콤하게 담그면 너무 맛있더라",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042531.1.16",
          "form": "난 삭힌오이지는 싫어해",
          "original_form": "난 삭힌오이지는 싫어해",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042531.1.17",
          "form": "너도 그래?",
          "original_form": "너도 그래?",
          "speaker_id": "1",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042531.1.18",
          "form": "응 오이소박이같은거",
          "original_form": "응 오이소박이같은거",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042531.1.19",
          "form": "나도싫더라",
          "original_form": "나도싫더라",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042531.1.20",
          "form": "피자 먹을 때 피클도 안 먹어?",
          "original_form": "피자 먹을 때 피클도 안 먹어?",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042531.1.21",
          "form": "응 거의 안먹고",
          "original_form": "응 거의 안먹고",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042531.1.22",
          "form": "피클 햄버거에도 안넣어",
          "original_form": "피클 햄버거에도 안넣어",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042531.1.23",
          "form": "나도 피자 피클 안 먹어",
          "original_form": "나도 피자 피클 안 먹어",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042531.1.24",
          "form": "햄버거는 요즘 좀 봐주는 중이야",
          "original_form": "햄버거는 요즘 좀 봐주는 중이야",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042531.1.25",
          "form": "조금은 먹어",
          "original_form": "조금은 먹어",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042531.1.26",
          "form": "피클은 추방이야",
          "original_form": "피클은 추방이야",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042531.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ피클이 울겠다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ피클이 울겠다",
          "speaker_id": "1",
          "time": "20210906 22:45"
        }
      ]
    }
  ]
