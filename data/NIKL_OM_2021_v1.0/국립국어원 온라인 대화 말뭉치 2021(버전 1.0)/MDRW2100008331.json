        },
        {
          "id": "MDRW2100008331.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008331.1.3",
          "form": "혹시 연애중이신가요?",
          "original_form": "혹시 연애중이신가요?",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008331.1.4",
          "form": "아니요 제작년에 헤어졌어요",
          "original_form": "아니요 제작년에 헤어졌어요",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008331.1.5",
          "form": "name1님은 연야중이신가요?",
          "original_form": "&name1&님은 연야중이신가요?",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008331.1.6",
          "form": "연애",
          "original_form": "연애",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008331.1.7",
          "form": "헉 저도요..",
          "original_form": "헉 저도요..",
          "speaker_id": "1",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008331.1.8",
          "form": "한 번 헤어지니 다른 사람 만나기 힘들어요",
          "original_form": "한 번 헤어지니 다른 사람 만나기 힘들어요",
          "speaker_id": "1",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008331.1.9",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008331.1.10",
          "form": "요즘에는 코로나 때문에 사람 자체를 안 만나서",
          "original_form": "요즘에는 코로나 때문에 사람 자체를 안 만나서",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008331.1.11",
          "form": "별로 인연도 없어요",
          "original_form": "별로 인연도 없어요",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008331.1.12",
          "form": "맞아요 그런 것도 있어요",
          "original_form": "맞아요 그런 것도 있어요",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008331.1.13",
          "form": "이상형은 어떻게 되세요?",
          "original_form": "이상형은 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008331.1.14",
          "form": "저는 제가 못 하는 거 잘하면 멋있어 보이더라고요 그거 말고는 섬세하거나 암튼 예의바른 사람이 좋은 거 같아요",
          "original_form": "저는 제가 못 하는 거 잘하면 멋있어 보이더라고요 그거 말고는 섬세하거나 암튼 예의바른 사람이 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008331.1.15",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008331.1.16",
          "form": "저랑 가치관이 잘 맞는 사람이 좋아요 그래야 덜 싸우는 거 같아요ㅎㅎ",
          "original_form": "저랑 가치관이 잘 맞는 사람이 좋아요 그래야 덜 싸우는 거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008331.1.17",
          "form": "가치관 중요하죠 대화도 잘 통하고",
          "original_form": "가치관 중요하죠 대화도 잘 통하고",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008331.1.18",
          "form": "진짜 중요한 거 같아요",
          "original_form": "진짜 중요한 거 같아요",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008331.1.19",
          "form": "연애는 보통 어떻게 시작하시나요?",
          "original_form": "연애는 보통 어떻게 시작하시나요?",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008331.1.20",
          "form": "아직 학생이라 학교에서 만나는게 전부예요",
          "original_form": "아직 학생이라 학교에서 만나는게 전부예요",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008331.1.21",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008331.1.22",
          "form": "저도 학교 다닐 때 썸 오래 타다가 만났어요",
          "original_form": "저도 학교 다닐 때 썸 오래 타다가 만났어요",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008331.1.23",
          "form": "저는 썸타면서 이것저것 맞춰보는 편이라",
          "original_form": "저는 썸타면서 이것저것 맞춰보는 편이라",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100008331.1.24",
          "form": "연애하기 전에 맞춰보는거 좋죠",
          "original_form": "연애하기 전에 맞춰보는거 좋죠",
          "speaker_id": "1",
