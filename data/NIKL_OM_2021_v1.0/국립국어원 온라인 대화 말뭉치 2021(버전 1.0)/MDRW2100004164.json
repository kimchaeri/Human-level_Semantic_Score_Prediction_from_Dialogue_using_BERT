        },
        {
          "id": "MDRW2100004164.1.3",
          "form": "요즘 운동하시는거 있나요?",
          "original_form": "요즘 운동하시는거 있나요?",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004164.1.4",
          "form": "내일부터 홈트 시작하려고용",
          "original_form": "내일부터 홈트 시작하려고용",
          "speaker_id": "1",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004164.1.5",
          "form": "운동을 너무 안해서ㅜㅜ",
          "original_form": "운동을 너무 안해서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004164.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004164.1.7",
          "form": "오 홈트는 주로 뭐해요?",
          "original_form": "오 홈트는 주로 뭐해요?",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004164.1.8",
          "form": "저도 요즘 숨쉬기운동만^^",
          "original_form": "저도 요즘 숨쉬기운동만^^",
          "speaker_id": "2",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004164.1.9",
          "form": "홈트는 역시 땅끄부부요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "홈트는 역시 땅끄부부요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004164.1.10",
          "form": "필라테스 하고싶었는데",
          "original_form": "필라테스 하고싶었는데",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004164.1.11",
          "form": "가격이 후덜덜하던데요....",
          "original_form": "가격이 후덜덜하던데요....",
          "speaker_id": "1",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004164.1.12",
          "form": "필라 비싸죠 6대1하면 1회 15000정도?",
          "original_form": "필라 비싸죠 6대1하면 1회 15000정도?",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004164.1.13",
          "form": "젤싼게 그정도인거같아요",
          "original_form": "젤싼게 그정도인거같아요",
          "speaker_id": "2",
          "time": "20210821 15:26"
        },
        {
          "id": "MDRW2100004164.1.14",
          "form": "그쵸ㅠㅠ 학생 입장에선 너무 부담되더라구요",
          "original_form": "그쵸ㅠㅠ 학생 입장에선 너무 부담되더라구요",
          "speaker_id": "1",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004164.1.15",
          "form": "꾸준히 나갈지도 미지수고...",
          "original_form": "꾸준히 나갈지도 미지수고...",
          "speaker_id": "1",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004164.1.16",
          "form": "코로나땜에 가기도 꺼려지더라구요",
          "original_form": "코로나땜에 가기도 꺼려지더라구요",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004164.1.17",
          "form": "홈트 열심히 하면 좋을거같은데요",
          "original_form": "홈트 열심히 하면 좋을거같은데요",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004164.1.18",
          "form": "맞아요 내일부터 열심히 해야죠",
          "original_form": "맞아요 내일부터 열심히 해야죠",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004164.1.19",
          "form": "운동하면 할때는 죽을거같은데",
          "original_form": "운동하면 할때는 죽을거같은데",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004164.1.20",
          "form": "그래도 다 하고 나면",
          "original_form": "그래도 다 하고 나면",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004164.1.21",
          "form": "성취감이 있어서 좋은거같아요",
          "original_form": "성취감이 있어서 좋은거같아요",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004164.1.22",
          "form": "상쾌하고 기분좋죠 ㅎ",
          "original_form": "상쾌하고 기분좋죠 ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004164.1.23",
          "form": "저도 줌바댄스 해봤는데 그것도 잼나요",
          "original_form": "저도 줌바댄스 해봤는데 그것도 잼나요",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004164.1.24",
          "form": "오 줌바댄스 색다르네요 나중에 해봐야겠어요",
          "original_form": "오 줌바댄스 색다르네요 나중에 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004164.1.25",
          "form": "라틴 레게 살사 이런 음악듣고 춤추는거라 재밌어요",
          "original_form": "라틴 레게 살사 이런 음악듣고 춤추는거라 재밌어요",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004164.1.26",
          "form": "땀도엄청 나고",
          "original_form": "땀도엄청 나고",
          "speaker_id": "2",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004164.1.27",
          "form": "오 완전 열정적인 춤일것같아요!",
          "original_form": "오 완전 열정적인 춤일것같아요!",
          "speaker_id": "1",
          "time": "20210821 15:42"
        },
        {
          "id": "MDRW2100004164.1.28",
          "form": "춤이라긴보단 율동에 가깝긴해요ㅎ",
          "original_form": "춤이라긴보단 율동에 가깝긴해요ㅎ",
