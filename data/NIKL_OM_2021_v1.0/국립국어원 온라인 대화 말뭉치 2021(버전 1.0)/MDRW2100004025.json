        },
        {
          "id": "MDRW2100004025.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004025.1.3",
          "form": "저는 언니요 ㅎㅎ",
          "original_form": "저는 언니요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004025.1.4",
          "form": "집안일은 분담하시나요?",
          "original_form": "집안일은 분담하시나요?",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004025.1.5",
          "form": "음 같이 살 때는 분담하긴했는데",
          "original_form": "음 같이 살 때는 분담하긴했는데",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004025.1.6",
          "form": "보통 제가 다 했어요",
          "original_form": "보통 제가 다 했어요",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004025.1.7",
          "form": "name1님은 분담하시나요?",
          "original_form": "&name1&님은 분담하시나요?",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004025.1.8",
          "form": "오 멋지세용",
          "original_form": "오 멋지세용",
          "speaker_id": "1",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004025.1.9",
          "form": "네네 혼자하기엔 일이 넘 많아요 ㅠㅠ",
          "original_form": "네네 혼자하기엔 일이 넘 많아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004025.1.10",
          "form": "주로 어떤 일 분담하세요",
          "original_form": "주로 어떤 일 분담하세요",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004025.1.11",
          "form": "흠 한명이 밥을하면 한명은 설거지하고 ㅋㅋ 그렇게여",
          "original_form": "흠 한명이 밥을하면 한명은 설거지하고 ㅋㅋ 그렇게여",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100004025.1.12",
          "form": "설거지 너무 귀찮지않나요",
          "original_form": "설거지 너무 귀찮지않나요",
          "speaker_id": "2",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004025.1.13",
          "form": "저는 하기싫어서 마지막에 먹은사람이 설거지하기 하고 도망쳤는데",
          "original_form": "저는 하기싫어서 마지막에 먹은사람이 설거지하기 하고 도망쳤는데",
          "speaker_id": "2",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004025.1.14",
          "form": "너무 약오르나요",
          "original_form": "너무 약오르나요",
          "speaker_id": "2",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004025.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ그럴수도있죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그럴수도있죠",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004025.1.16",
          "form": "저는 설거지는 괜찮은데",
          "original_form": "저는 설거지는 괜찮은데",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004025.1.17",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004025.1.18",
          "form": "빨래개고하는게 넘 귀찮아여",
          "original_form": "빨래개고하는게 넘 귀찮아여",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004025.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004025.1.20",
          "form": "그거 너무 귀찮죠",
          "original_form": "그거 너무 귀찮죠",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004025.1.21",
          "form": "세탁기도 건조기도 있는데 왜 빨래정리기계는 없는거지",
          "original_form": "세탁기도 건조기도 있는데 왜 빨래정리기계는 없는거지",
          "speaker_id": "2",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004025.1.22",
          "form": "그니깐요 ㅋㅋ 건조기땜에 일은 수월해졌는데",
          "original_form": "그니깐요 ㅋㅋ 건조기땜에 일은 수월해졌는데",
