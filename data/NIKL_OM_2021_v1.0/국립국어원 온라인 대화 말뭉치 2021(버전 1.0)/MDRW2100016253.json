      "utterance": [
        {
          "id": "MDRW2100016253.1.1",
          "form": "아 나 요즘 피마새 읽어",
          "original_form": "아 나 요즘 피마새 읽어",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016253.1.2",
          "form": "재밌음?",
          "original_form": "재밌음?",
          "speaker_id": "1",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016253.1.3",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016253.1.4",
          "form": "개그 코드가 잘 맞아",
          "original_form": "개그 코드가 잘 맞아",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016253.1.5",
          "form": "그거 시리즈 읽는 순서 있지 않음?",
          "original_form": "그거 시리즈 읽는 순서 있지 않음?",
          "speaker_id": "1",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016253.1.6",
          "form": "눈마새 다음이 피마새",
          "original_form": "눈마새 다음이 피마새",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016253.1.7",
          "form": "피마새 다음에는 그림자 자국 같은 단편집",
          "original_form": "피마새 다음에는 그림자 자국 같은 단편집",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016253.1.8",
          "form": "안 읽어봤어ㅋㅋ",
          "original_form": "안 읽어봤어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016253.1.9",
          "form": "읽자아아",
          "original_form": "읽자아아",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016253.1.10",
          "form": "재밌어어",
          "original_form": "재밌어어",
          "speaker_id": "2",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016253.1.11",
          "form": "고전 명작",
          "original_form": "고전 명작",
          "speaker_id": "2",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016253.1.12",
          "form": "요샌 책에 손이 안가ㅋㅋㅋ",
          "original_form": "요샌 책에 손이 안가ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016253.1.13",
          "form": "요전에 그림책도 다 삼",
          "original_form": "요전에 그림책도 다 삼",
          "speaker_id": "1",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016253.1.14",
          "form": "만회?",
          "original_form": "만회?",
          "speaker_id": "2",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016253.1.15",
          "form": "만화",
          "original_form": "만화",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016253.1.16",
          "form": "아니 그냥 동화책같은?ㅋㅋㅋ",
          "original_form": "아니 그냥 동화책같은?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016253.1.17",
          "form": "그림 예뻐서 샀음",
          "original_form": "그림 예뻐서 샀음",
          "speaker_id": "1",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016253.1.18",
          "form": "오 애기들꺼?",
          "original_form": "오 애기들꺼?",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016253.1.19",
          "form": "내용은 애들용 아닌것 같기두 하고..",
          "original_form": "내용은 애들용 아닌것 같기두 하고..",
          "speaker_id": "1",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016253.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100016253.1.21",
          "form": "제목이 모야",
          "original_form": "제목이 모야",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100016253.1.22",
          "form": "뭐였지 가서 봐야댐",
          "original_form": "뭐였지 가서 봐야댐",
