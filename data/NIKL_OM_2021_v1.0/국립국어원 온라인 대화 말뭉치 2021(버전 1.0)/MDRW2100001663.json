      "utterance": [
        {
          "id": "MDRW2100001663.1.1",
          "form": "안녕하세요 name2이님 요즘 어떤 일 하시나요?",
          "original_form": "안녕하세요 &name2&이님 요즘 어떤 일 하시나요?",
          "speaker_id": "1",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100001663.1.2",
          "form": "저는 학생인데요",
          "original_form": "저는 학생인데요",
          "speaker_id": "2",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100001663.1.3",
          "form": "아 정말요? 학생이라도 어떤 일을 하시지 않으실까요??",
          "original_form": "아 정말요? 학생이라도 어떤 일을 하시지 않으실까요??",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100001663.1.4",
          "form": "안하는데요 아르바이트는 좀 하기싶긴해요",
          "original_form": "안하는데요 아르바이트는 좀 하기싶긴해요",
          "speaker_id": "2",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100001663.1.5",
          "form": "쿠팡이라던가",
          "original_form": "쿠팡이라던가",
          "speaker_id": "2",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100001663.1.6",
          "form": "단기알바요",
          "original_form": "단기알바요",
          "speaker_id": "2",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100001663.1.7",
          "form": "아하 단기알바말씀이시군요",
          "original_form": "아하 단기알바말씀이시군요",
          "speaker_id": "1",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100001663.1.8",
          "form": "그래도 장기알바가 좋지않을까요?",
          "original_form": "그래도 장기알바가 좋지않을까요?",
          "speaker_id": "1",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100001663.1.9",
          "form": "장기알바는 좀 부담스러워서요",
          "original_form": "장기알바는 좀 부담스러워서요",
          "speaker_id": "2",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100001663.1.10",
          "form": "오래 일해야될거같고 그만두는것도 좀 어렵고",
          "original_form": "오래 일해야될거같고 그만두는것도 좀 어렵고",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100001663.1.11",
          "form": "그렇잖아요",
          "original_form": "그렇잖아요",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100001663.1.12",
          "form": "아~~ 그러시구나~~~^^",
          "original_form": "아~~ 그러시구나~~~^^",
          "speaker_id": "1",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100001663.1.13",
          "form": "저도 쿠팡을 해보았는데요",
          "original_form": "저도 쿠팡을 해보았는데요",
          "speaker_id": "1",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100001663.1.14",
          "form": "인센티브 줄때만 가는거 추천해요",
          "original_form": "인센티브 줄때만 가는거 추천해요",
          "speaker_id": "1",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100001663.1.15",
          "form": "우와 꿀팁이네요! 감사해요",
          "original_form": "우와 꿀팁이네요! 감사해요",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100001663.1.16",
          "form": "업무스트레스같은건 없으셧나요?",
          "original_form": "업무스트레스같은건 없으셧나요?",
          "speaker_id": "2",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100001663.1.17",
          "form": "응 없었어",
          "original_form": "응 없었어",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100001663.1.18",
          "form": "아니 있었어",
          "original_form": "아니 있었어",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100001663.1.19",
          "form": "먼데",
          "original_form": "먼데",
          "speaker_id": "2",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100001663.1.20",
          "form": "머엿는데",
          "original_form": "머엿는데",
          "speaker_id": "2",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100001663.1.21",
          "form": "머냐면...",
          "original_form": "머냐면...",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100001663.1.22",
          "form": "옆자리 앉은 사람이 귀찮게 굴었어",
          "original_form": "옆자리 앉은 사람이 귀찮게 굴었어",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100001663.1.23",
          "form": "말 안하고싶은데 자꾸 말걸고",
          "original_form": "말 안하고싶은데 자꾸 말걸고",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100001663.1.24",
          "form": "쓸데없는 친절 베풀어서 따증났어",
          "original_form": "쓸데없는 친절 베풀어서 따증났어",
          "speaker_id": "1",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100001663.1.25",
          "form": "짜증났어",
          "original_form": "짜증났어",
          "speaker_id": "1",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100001663.1.26",
          "form": "헐 완전 별로다 그런 고충도있네",
          "original_form": "헐 완전 별로다 그런 고충도있네",
          "speaker_id": "2",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100001663.1.27",
          "form": "그치..진짜 짜증났다고",
          "original_form": "그치..진짜 짜증났다고",
          "speaker_id": "1",
          "time": "20210819 17:58"
        }
      ]
    }
  ]
