      "utterance": [
        {
          "id": "MDRW2100023764.1.1",
          "form": "가족이 동물같은거 키우나여?",
          "original_form": "가족이 동물같은거 키우나여?",
          "speaker_id": "1",
          "time": "20210830 16:54"
        },
        {
          "id": "MDRW2100023764.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210830 16:55"
        },
        {
          "id": "MDRW2100023764.1.3",
          "form": "은퇴 후라면 모르겠네요",
          "original_form": "은퇴 후라면 모르겠네요",
          "speaker_id": "2",
          "time": "20210830 16:56"
        },
        {
          "id": "MDRW2100023764.1.4",
          "form": "ㅋㅋㅋ 키워보고 싶엇던적도 없어요?",
          "original_form": "ㅋㅋㅋ 키워보고 싶엇던적도 없어요?",
          "speaker_id": "1",
          "time": "20210830 17:01"
        },
        {
          "id": "MDRW2100023764.1.5",
          "form": "키워보고 싶기야 하죠 큰 개를..",
          "original_form": "키워보고 싶기야 하죠 큰 개를..",
          "speaker_id": "2",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100023764.1.6",
          "form": "시베리안 허스키?",
          "original_form": "시베리안 허스키?",
          "speaker_id": "2",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100023764.1.7",
          "form": "근데 개랑 놀아줄 시간과 공간이 없어요 ㅎ",
          "original_form": "근데 개랑 놀아줄 시간과 공간이 없어요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100023764.1.8",
          "form": "헉.. 본격적이시네여..",
          "original_form": "헉.. 본격적이시네여..",
          "speaker_id": "1",
          "time": "20210830 17:05"
        },
        {
          "id": "MDRW2100023764.1.9",
          "form": "저도 마당만 있으면.. 호랑이나 곰키워보고 깊은.. ㅋㅋㅋ",
          "original_form": "저도 마당만 있으면.. 호랑이나 곰키워보고 깊은.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:06"
        },
        {
          "id": "MDRW2100023764.1.10",
          "form": "맹수 좋아하시네요",
          "original_form": "맹수 좋아하시네요",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100023764.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:48"
        },
        {
          "id": "MDRW2100023764.1.12",
          "form": "동물은 다좋아요.. \n여우도 좋고... 퓨마도 좋고..",
          "original_form": "동물은 다좋아요.. \n여우도 좋고... 퓨마도 좋고..",
          "speaker_id": "1",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100023764.1.13",
          "form": "외국은 키울 수 있는데가 잇더라구요",
          "original_form": "외국은 키울 수 있는데가 잇더라구요",
          "speaker_id": "1",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100023764.1.14",
          "form": "그래요?? 그래도 흔하진 않을거 같아요ㅋㅋ",
          "original_form": "그래요?? 그래도 흔하진 않을거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:50"
        },
        {
          "id": "MDRW2100023764.1.15",
          "form": "뭘 키우든 간에 끝까지 책임을 져야한다는게",
          "original_form": "뭘 키우든 간에 끝까지 책임을 져야한다는게",
          "speaker_id": "2",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100023764.1.16",
          "form": "그게 정말 쉽지 않은 일이라 앞으로도 키울진 모르겠어요..",
          "original_form": "그게 정말 쉽지 않은 일이라 앞으로도 키울진 모르겠어요..",
          "speaker_id": "2",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100023764.1.17",
          "form": "게다가 늙고 약해지고 결국 죽으면 마음 아프잖아요",
          "original_form": "게다가 늙고 약해지고 결국 죽으면 마음 아프잖아요",
          "speaker_id": "2",
          "time": "20210830 17:52"
        },
        {
          "id": "MDRW2100023764.1.18",
          "form": "그건 그래요... 다 생명들이고...",
          "original_form": "그건 그래요... 다 생명들이고...",
          "speaker_id": "1",
          "time": "20210830 17:56"
        },
        {
          "id": "MDRW2100023764.1.19",
          "form": "강아지 유기 너무 잔인해요..",
          "original_form": "강아지 유기 너무 잔인해요..",
          "speaker_id": "1",
          "time": "20210830 17:57"
        },
        {
          "id": "MDRW2100023764.1.20",
          "form": "그니까요.. 그건 진짜 나쁜 것 같아요",
          "original_form": "그니까요.. 그건 진짜 나쁜 것 같아요",
          "speaker_id": "2",
          "time": "20210830 18:00"
        },
        {
          "id": "MDRW2100023764.1.21",
          "form": "친구들이 고양이 많이 키우는데 귀엽긴 하더라구요",
          "original_form": "친구들이 고양이 많이 키우는데 귀엽긴 하더라구요",
          "speaker_id": "2",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100023764.1.22",
          "form": "고양이는 갓이죠..ㅋㅋㅋㅋㅋ",
          "original_form": "고양이는 갓이죠..ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100023764.1.23",
          "form": "저 최애가 고양이에요..",
          "original_form": "저 최애가 고양이에요..",
          "speaker_id": "1",
          "time": "20210830 18:04"
        },
        {
          "id": "MDRW2100023764.1.24",
          "form": "어떤 고양이 좋아하세요??",
          "original_form": "어떤 고양이 좋아하세요??",
          "speaker_id": "2",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100023764.1.25",
          "form": "전 귀 작은 고양이가 귀엽던데ㅎㅎ",
          "original_form": "전 귀 작은 고양이가 귀엽던데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100023764.1.26",
          "form": "폴드 말구여? 전 렉돌이나.. 브리숏이요 ㅋㅋㅋ",
          "original_form": "폴드 말구여? 전 렉돌이나.. 브리숏이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:07"
        },
        {
          "id": "MDRW2100023764.1.27",
          "form": "옆에서 동생이 유전병이 있을 수 있는 종이라는고..ㅋㅋ 자기는 러시안블루 키우고 싶대요",
          "original_form": "옆에서 동생이 유전병이 있을 수 있는 종이라는고..ㅋㅋ 자기는 러시안블루 키우고 싶대요",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023764.1.28",
          "form": "아마 교배종이라 그런 것 같네요",
          "original_form": "아마 교배종이라 그런 것 같네요",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100023764.1.29",
          "form": "폴드는 유전병 맞아여.. 사실 기형인데.. 사람들이 귀엽다궁..",
          "original_form": "폴드는 유전병 맞아여.. 사실 기형인데.. 사람들이 귀엽다궁..",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023764.1.30",
          "form": "러블이 애교많고 건강해서 사람들이 좋아하는듯",
          "original_form": "러블이 애교많고 건강해서 사람들이 좋아하는듯",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100023764.1.31",
          "form": "건강한게 최고져 ㅎㅎ",
          "original_form": "건강한게 최고져 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:22"
        }
      ]
