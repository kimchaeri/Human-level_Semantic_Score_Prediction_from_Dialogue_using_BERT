      "utterance": [
        {
          "id": "MDRW2100026858.1.1",
          "form": "방가요.\n오늘 날씨 어떤가요",
          "original_form": "방가요.\n오늘 날씨 어떤가요",
          "speaker_id": "1",
          "time": "20210903 13:18"
        },
        {
          "id": "MDRW2100026858.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210903 13:32"
        },
        {
          "id": "MDRW2100026858.1.3",
          "form": "오늘 날씨 좋네요",
          "original_form": "오늘 날씨 좋네요",
          "speaker_id": "2",
          "time": "20210903 13:32"
        },
        {
          "id": "MDRW2100026858.1.4",
          "form": "선선하고 따땃하고",
          "original_form": "선선하고 따땃하고",
          "speaker_id": "2",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100026858.1.5",
          "form": "햇살이 아직은 뜨겁네요!",
          "original_form": "햇살이 아직은 뜨겁네요!",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100026858.1.6",
          "form": "그러게요ㅋㅋ 어디 가고싶은 날씨에요",
          "original_form": "그러게요ㅋㅋ 어디 가고싶은 날씨에요",
          "speaker_id": "2",
          "time": "20210903 13:48"
        },
        {
          "id": "MDRW2100026858.1.7",
          "form": "어느 그늘에 누워있음 선선하니 좋을 거 같아요",
          "original_form": "어느 그늘에 누워있음 선선하니 좋을 거 같아요",
          "speaker_id": "1",
          "time": "20210903 13:50"
        },
        {
          "id": "MDRW2100026858.1.8",
          "form": "딱이겠네요",
          "original_form": "딱이겠네요",
          "speaker_id": "2",
          "time": "20210903 14:01"
        },
        {
          "id": "MDRW2100026858.1.9",
          "form": "잠이 솔솔 올듯요ㅋㅋ",
          "original_form": "잠이 솔솔 올듯요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:01"
        },
        {
          "id": "MDRW2100026858.1.10",
          "form": "바닷가 솔 숲에 누워서 파도 소리도 들으며 낮잠자면 딱 좋을 날씨  같아요",
          "original_form": "바닷가 솔 숲에 누워서 파도 소리도 들으며 낮잠자면 딱 좋을 날씨  같아요",
          "speaker_id": "1",
          "time": "20210903 14:03"
        },
        {
          "id": "MDRW2100026858.1.11",
          "form": "2주 동안 비 쏟아지더니.. 참",
          "original_form": "2주 동안 비 쏟아지더니.. 참",
          "speaker_id": "2",
          "time": "20210903 14:04"
        },
        {
          "id": "MDRW2100026858.1.12",
          "form": "담주 비 소식 있던데요 ㅎㅎ",
          "original_form": "담주 비 소식 있던데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100026858.1.13",
          "form": "또요?...",
          "original_form": "또요?...",
          "speaker_id": "2",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100026858.1.14",
          "form": "징글징글하네요",
          "original_form": "징글징글하네요",
          "speaker_id": "2",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100026858.1.15",
          "form": "네 8일경 인거 같아요..",
          "original_form": "네 8일경 인거 같아요..",
          "speaker_id": "1",
          "time": "20210903 14:35"
        },
        {
          "id": "MDRW2100026858.1.16",
          "form": "이젠,그만 왔으면...",
          "original_form": "이젠,그만 왔으면...",
          "speaker_id": "2",
          "time": "20210903 14:38"
        },
        {
          "id": "MDRW2100026858.1.17",
          "form": "이제야 가을 날씨 같은데 거기에 비를 또 뿌리나요 ㅎㅎ",
          "original_form": "이제야 가을 날씨 같은데 거기에 비를 또 뿌리나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 14:41"
        },
        {
          "id": "MDRW2100026858.1.18",
          "form": "그러게요 딱 좋은데",
          "original_form": "그러게요 딱 좋은데",
          "speaker_id": "2",
          "time": "20210903 15:12"
        },
        {
          "id": "MDRW2100026858.1.19",
          "form": "비오고 갑자기 추워지는거 아닐까요",
          "original_form": "비오고 갑자기 추워지는거 아닐까요",
          "speaker_id": "2",
          "time": "20210903 15:12"
        },
        {
          "id": "MDRW2100026858.1.20",
          "form": "아마 더 가을 날씨가 될 거 같아요.\n코스모스 언제 피려나.. 아직 코스모스 안 핀거죠?",
          "original_form": "아마 더 가을 날씨가 될 거 같아요.\n코스모스 언제 피려나.. 아직 코스모스 안 핀거죠?",
          "speaker_id": "1",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100026858.1.21",
          "form": "글쎄요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "글쎄요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100026858.1.22",
          "form": "요새 기온 이상해서",
          "original_form": "요새 기온 이상해서",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100026858.1.23",
          "form": "잠자리는 장마전에 많이 날라다녔어요",
          "original_form": "잠자리는 장마전에 많이 날라다녔어요",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100026858.1.24",
          "form": "여기 시골인데도 잠자리가 안 보여요..",
          "original_form": "여기 시골인데도 잠자리가 안 보여요..",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100026858.1.25",
          "form": "왜죠ㅠ 지금 딱 좋을듯한데",
          "original_form": "왜죠ㅠ 지금 딱 좋을듯한데",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100026858.1.26",
          "form": "여긴 경기돈데 8월 초중반에",
          "original_form": "여긴 경기돈데 8월 초중반에",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100026858.1.27",
          "form": "잠자리 진짜 많았어요",
          "original_form": "잠자리 진짜 많았어요",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100026858.1.28",
          "form": "뭐지 싶을 정도로",
          "original_form": "뭐지 싶을 정도로",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100026858.1.29",
          "form": "여기도 경기도에요!\n그 많던 잠자리가 다 어디 갔을까요..\n나비는 많이 날아다디던데",
          "original_form": "여기도 경기도에요!\n그 많던 잠자리가 다 어디 갔을까요..\n나비는 많이 날아다디던데",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100026858.1.30",
          "form": "글게요.. 이상기후 무서워요",
          "original_form": "글게요.. 이상기후 무서워요",
          "speaker_id": "2",
          "time": "20210903 15:46"
        },
        {
          "id": "MDRW2100026858.1.31",
          "form": "사람힘으로도 어쩌지 못하는게 날씨 같아요",
          "original_form": "사람힘으로도 어쩌지 못하는게 날씨 같아요",
          "speaker_id": "1",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100026858.1.32",
          "form": "그러게요 다음 세대는 더 고생할텐데",
          "original_form": "그러게요 다음 세대는 더 고생할텐데",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100026858.1.33",
          "form": "맞아요 우리 아이들이 걱정이에요",
          "original_form": "맞아요 우리 아이들이 걱정이에요",
          "speaker_id": "1",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100026858.1.34",
          "form": "나름대로 잘 살겠지만.. 최대한 보존해서 내려갔음 좋겠습니다",
          "original_form": "나름대로 잘 살겠지만.. 최대한 보존해서 내려갔음 좋겠습니다",
          "speaker_id": "2",
          "time": "20210903 16:44"
        }
      ]
    }
