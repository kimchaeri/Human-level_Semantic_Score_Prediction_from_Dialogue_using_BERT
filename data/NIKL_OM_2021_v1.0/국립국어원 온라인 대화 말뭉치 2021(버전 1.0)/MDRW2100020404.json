          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100020404.1.31",
          "form": "셋다안함",
          "original_form": "셋다안함",
          "speaker_id": "1",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100020404.1.32",
          "form": "ㅋㅋㅋㅋ진입장벽이 넘 높음",
          "original_form": "ㅋㅋㅋㅋ진입장벽이 넘 높음",
          "speaker_id": "2",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100020404.1.33",
          "form": "리니지",
          "original_form": "리니지",
          "speaker_id": "1",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100020404.1.34",
          "form": "메이플보다 더 돈발라여하는 게임아닌갑숑",
          "original_form": "메이플보다 더 돈발라여하는 게임아닌갑숑",
          "speaker_id": "1",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100020404.1.35",
          "form": "넉ㅂ",
          "original_form": "넉ㅂ",
          "speaker_id": "2",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100020404.1.36",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100020404.1.37",
          "form": "돈없어서 못하는거임",
          "original_form": "돈없어서 못하는거임",
          "speaker_id": "1",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100020404.1.38",
          "form": "쒸익...",
          "original_form": "쒸익...",
          "speaker_id": "1",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100020404.1.39",
          "form": "ㅋㅋㅋ그렇죠 뭐",
          "original_form": "ㅋㅋㅋ그렇죠 뭐",
          "speaker_id": "2",
          "time": "20210828 02:57"
        },
        {
          "id": "MDRW2100020404.1.40",
          "form": "메이플 게임도 안대죠",
          "original_form": "메이플 게임도 안대죠",
          "speaker_id": "2",
          "time": "20210828 02:57"
        },
        {
          "id": "MDRW2100020404.1.41",
