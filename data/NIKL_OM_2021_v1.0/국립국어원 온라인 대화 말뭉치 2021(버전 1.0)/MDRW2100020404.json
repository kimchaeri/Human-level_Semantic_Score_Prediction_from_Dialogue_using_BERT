      "utterance": [
        {
          "id": "MDRW2100020404.1.1",
          "form": "도탁스가 도타 커뮤로 시작한거에여",
          "original_form": "도탁스가 도타 커뮤로 시작한거에여",
          "speaker_id": "2",
          "time": "20210828 02:53"
        },
        {
          "id": "MDRW2100020404.1.2",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210828 02:53"
        },
        {
          "id": "MDRW2100020404.1.3",
          "form": "첨알앗네여",
          "original_form": "첨알앗네여",
          "speaker_id": "1",
          "time": "20210828 02:53"
        },
        {
          "id": "MDRW2100020404.1.4",
          "form": "아니 제 친구가",
          "original_form": "아니 제 친구가",
          "speaker_id": "2",
          "time": "20210828 02:54"
        },
        {
          "id": "MDRW2100020404.1.5",
          "form": "게임학과갔는데",
          "original_form": "게임학과갔는데",
          "speaker_id": "2",
          "time": "20210828 02:54"
        },
        {
          "id": "MDRW2100020404.1.6",
          "form": "근데 지금 도탁스에 도타보다 딴소리하는사람이 더많을듯",
          "original_form": "근데 지금 도탁스에 도타보다 딴소리하는사람이 더많을듯",
          "speaker_id": "1",
          "time": "20210828 02:54"
        },
        {
          "id": "MDRW2100020404.1.7",
          "form": "한국 게임의 이해란 전공을 첫 수업으로",
          "original_form": "한국 게임의 이해란 전공을 첫 수업으로",
          "speaker_id": "2",
          "time": "20210828 02:54"
        },
        {
          "id": "MDRW2100020404.1.8",
          "form": "들었대요",
          "original_form": "들었대요",
          "speaker_id": "2",
          "time": "20210828 02:54"
        },
        {
          "id": "MDRW2100020404.1.9",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210828 02:54"
        },
        {
          "id": "MDRW2100020404.1.10",
          "form": "한국게임 역사 전환점이 3번있었다함",
          "original_form": "한국게임 역사 전환점이 3번있었다함",
          "speaker_id": "2",
          "time": "20210828 02:54"
        },
        {
          "id": "MDRW2100020404.1.11",
          "form": "언젠줄알아여?",
          "original_form": "언젠줄알아여?",
          "speaker_id": "2",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.12",
          "form": "롤",
          "original_form": "롤",
          "speaker_id": "1",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.13",
          "form": "2012년 기줏",
          "original_form": "2012년 기줏",
          "speaker_id": "2",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.14",
          "form": "스타",
          "original_form": "스타",
          "speaker_id": "1",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.15",
