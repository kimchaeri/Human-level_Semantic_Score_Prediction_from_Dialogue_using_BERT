          "original_form": "기줏",
          "speaker_id": "2",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.16",
          "form": "땡",
          "original_form": "땡",
          "speaker_id": "2",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.17",
          "form": "스타가 먼저아닌가여",
          "original_form": "스타가 먼저아닌가여",
          "speaker_id": "1",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.18",
          "form": "옵치",
          "original_form": "옵치",
          "speaker_id": "1",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.19",
          "form": "한국게임이라니까요ㅋㅋㅋ",
          "original_form": "한국게임이라니까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.20",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.21",
          "form": "이래서 어린친구는",
          "original_form": "이래서 어린친구는",
          "speaker_id": "2",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.22",
          "form": "도타요",
          "original_form": "도타요",
          "speaker_id": "1",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.23",
          "form": "ㅋㅋㅋ한국게임이라고요",
          "original_form": "ㅋㅋㅋ한국게임이라고요",
          "speaker_id": "2",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.24",
          "form": "처음은 바람",
          "original_form": "처음은 바람",
          "speaker_id": "2",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.25",
          "form": "바람의나라?",
          "original_form": "바람의나라?",
          "speaker_id": "1",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.26",
          "form": "둘은 리니지",
          "original_form": "둘은 리니지",
          "speaker_id": "2",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.27",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210828 02:55"
        },
        {
          "id": "MDRW2100020404.1.28",
          "form": "리니지하심?",
          "original_form": "리니지하심?",
          "speaker_id": "1",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100020404.1.29",
          "form": "셋은 아이온",
          "original_form": "셋은 아이온",
