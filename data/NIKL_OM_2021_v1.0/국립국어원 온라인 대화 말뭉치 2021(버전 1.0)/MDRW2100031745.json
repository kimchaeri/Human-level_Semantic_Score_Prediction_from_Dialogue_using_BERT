          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100031745.1.18",
          "form": "네! 서울살아요!!",
          "original_form": "네! 서울살아요!!",
          "speaker_id": "1",
          "time": "20210905 00:04"
        },
        {
          "id": "MDRW2100031745.1.19",
          "form": "서울사세요?",
