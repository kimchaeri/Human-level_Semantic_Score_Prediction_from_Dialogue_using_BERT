      "utterance": [
        {
          "id": "MDRW2100031745.1.1",
          "form": "좋아하는 음식 있으세요??",
          "original_form": "좋아하는 음식 있으세요??",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031745.1.2",
