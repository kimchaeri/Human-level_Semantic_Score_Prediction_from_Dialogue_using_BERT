      "utterance": [
        {
          "id": "MDRW2100004008.1.1",
          "form": "종합비타민이랑",
          "original_form": "종합비타민이랑",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100004008.1.2",
          "form": "응 그리고",
          "original_form": "응 그리고",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100004008.1.3",
          "form": "프로폴리스 실리마린",
          "original_form": "프로폴리스 실리마린",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004008.1.4",
          "form": "알티지 오메가쓰리",
          "original_form": "알티지 오메가쓰리",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004008.1.5",
          "form": "몇 개야 ㅋㅋㅋ",
          "original_form": "몇 개야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004008.1.6",
          "form": "비타민 비, 디",
          "original_form": "비타민 비, 디",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004008.1.7",
          "form": "칼슘",
          "original_form": "칼슘",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004008.1.8",
          "form": "우와 많이 챙겨 먹네",
          "original_form": "우와 많이 챙겨 먹네",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004008.1.9",
          "form": "ㅋㅋㅋ 매일 다 먹진 않고",
          "original_form": "ㅋㅋㅋ 매일 다 먹진 않고",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004008.1.10",
          "form": "다먹는 건 아냐?",
          "original_form": "다먹는 건 아냐?",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004008.1.11",
          "form": "필요에 따라서 그때그때",
          "original_form": "필요에 따라서 그때그때",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004008.1.12",
          "form": "아 나는 유산균 챙겨먹어",
          "original_form": "아 나는 유산균 챙겨먹어",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004008.1.13",
          "form": "어 나도",
          "original_form": "어 나도",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004008.1.14",
          "form": "면역력 때문에",
          "original_form": "면역력 때문에",
          "speaker_id": "2",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004008.1.15",
          "form": "아 나도 면역력 떨어져서 운동해야해",
          "original_form": "아 나도 면역력 떨어져서 운동해야해",
          "speaker_id": "1",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004008.1.16",
          "form": "운동이 제일이긴 하지",
          "original_form": "운동이 제일이긴 하지",
          "speaker_id": "2",
          "time": "20210821 14:34"
        }
      ]
