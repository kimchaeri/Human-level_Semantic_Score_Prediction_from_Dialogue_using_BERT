      "utterance": [
        {
          "id": "MDRW2100027205.1.1",
          "form": "토스트 다 먹엇당 히히",
          "original_form": "토스트 다 먹엇당 히히",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027205.1.2",
          "form": "토스트에 뭐뭐 들어 있었나요~~?",
          "original_form": "토스트에 뭐뭐 들어 있었나요~~?",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027205.1.3",
          "form": "저 그거 시켰어요 에그감자토스트!",
          "original_form": "저 그거 시켰어요 에그감자토스트!",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027205.1.4",
          "form": "아니 진짜 너무 에그랑 감자를 많이 주셔서 ㅋㅋㅋㅋ 놀랐어요!",
          "original_form": "아니 진짜 너무 에그랑 감자를 많이 주셔서 ㅋㅋㅋㅋ 놀랐어요!",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027205.1.5",
          "form": "아 너무 맛있죠!",
          "original_form": "아 너무 맛있죠!",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027205.1.6",
          "form": "사장님이 거의 두배 주셨어요",
          "original_form": "사장님이 거의 두배 주셨어요",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027205.1.7",
          "form": "깜놀했다니까요 ㅋㅋㅋ",
          "original_form": "깜놀했다니까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027205.1.8",
          "form": "좋네요ㅎㅎ",
          "original_form": "좋네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027205.1.9",
          "form": "토스트 자주 드시나요?",
          "original_form": "토스트 자주 드시나요?",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027205.1.10",
          "form": "저 생각나면 시켜요!",
          "original_form": "저 생각나면 시켜요!",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027205.1.11",
          "form": "꽤 좋아합니당",
          "original_form": "꽤 좋아합니당",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027205.1.12",
          "form": "집근처에 있거든요 토스트집",
          "original_form": "집근처에 있거든요 토스트집",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027205.1.13",
          "form": "저는 근처에 토스트 집이 없어서 안 먹은지 오래된 거 같아요ㅠㅠ",
          "original_form": "저는 근처에 토스트 집이 없어서 안 먹은지 오래된 거 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027205.1.14",
          "form": "헉 안돼ㅠㅠㅠ 그러면 넘 아쉽죠",
          "original_form": "헉 안돼ㅠㅠㅠ 그러면 넘 아쉽죠",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027205.1.15",
          "form": "근데 진짜 근처에 있어야 먹게 돼요",
          "original_form": "근데 진짜 근처에 있어야 먹게 돼요",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100027205.1.16",
          "form": "맞아요! 중학교 때는 하굣길에 이삭토스트가 있어서 자주 갔었어요",
          "original_form": "맞아요! 중학교 때는 하굣길에 이삭토스트가 있어서 자주 갔었어요",
          "speaker_id": "2",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100027205.1.17",
          "form": "우와 정작 저 이사하니까 이삭없어요 근처에ㅠㅠ",
          "original_form": "우와 정작 저 이사하니까 이삭없어요 근처에ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027205.1.18",
          "form": "아쉽... 이삭 진짜 맛난데ㅠㅠ",
          "original_form": "아쉽... 이삭 진짜 맛난데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027205.1.19",
          "form": "맞아요~ㅜㅜ",
          "original_form": "맞아요~ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100027205.1.20",
          "form": "토스트에 설탕 뿌려 드시나요?",
          "original_form": "토스트에 설탕 뿌려 드시나요?",
          "speaker_id": "2",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100027205.1.21",
          "form": "네 설탕 러버예여",
          "original_form": "네 설탕 러버예여",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027205.1.22",
          "form": "설탕 없어서 못먹죠!",
          "original_form": "설탕 없어서 못먹죠!",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027205.1.23",
          "form": "설탕 잘 뿌려드세요?",
