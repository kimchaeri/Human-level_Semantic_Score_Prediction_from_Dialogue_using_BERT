        },
        {
          "id": "MDRW2100016981.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100016981.1.4",
          "form": "저는 피파온라인해요",
          "original_form": "저는 피파온라인해요",
          "speaker_id": "1",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100016981.1.5",
          "form": "피온 저도 햇는데 요즘에 안 하고 있네요ㅋㅋ",
          "original_form": "피온 저도 햇는데 요즘에 안 하고 있네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100016981.1.6",
          "form": "팀뭐에여",
          "original_form": "팀뭐에여",
          "speaker_id": "2",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100016981.1.7",
          "form": "psg입니다ㅎㅎ",
          "original_form": "psg입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100016981.1.8",
          "form": "옼ㅋㅋ이번 이적시장 업뎃하셨나요",
          "original_form": "옼ㅋㅋ이번 이적시장 업뎃하셨나요",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100016981.1.9",
          "form": "네네 했습니다ㅎㅎ",
          "original_form": "네네 했습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100016981.1.10",
          "form": "메시 시즌 뭘로 사셨나요",
          "original_form": "메시 시즌 뭘로 사셨나요",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100016981.1.11",
          "form": "아직 메시는 못샀네요ㅎㅎ",
          "original_form": "아직 메시는 못샀네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100016981.1.12",
          "form": "ㅋㅋㅋㅋ메시사셔야죠!",
          "original_form": "ㅋㅋㅋㅋ메시사셔야죠!",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100016981.1.13",
          "form": "지금 펌핑 좀 되었으려나요",
          "original_form": "지금 펌핑 좀 되었으려나요",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100016981.1.14",
          "form": "이미 펌핑완료일듯요ㅎㅎ",
          "original_form": "이미 펌핑완료일듯요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100016981.1.15",
          "form": "저는 리버풀인데",
          "original_form": "저는 리버풀인데",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100016981.1.16",
          "form": "안들어간지 꽤 됐네요ㅋㅋ",
          "original_form": "안들어간지 꽤 됐네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100016981.1.17",
          "form": "가치 좀 올랐으려나요",
          "original_form": "가치 좀 올랐으려나요",
