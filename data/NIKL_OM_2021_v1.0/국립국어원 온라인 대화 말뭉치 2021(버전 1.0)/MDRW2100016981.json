        {
          "id": "MDRW2100016981.1.1",
          "form": "겜 좋아하시나요",
          "original_form": "겜 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 17:57"
        },
        {
          "id": "MDRW2100016981.1.2",
          "form": "게임 좋아하는데 할 시간이 없네욥",
          "original_form": "게임 좋아하는데 할 시간이 없네욥",
