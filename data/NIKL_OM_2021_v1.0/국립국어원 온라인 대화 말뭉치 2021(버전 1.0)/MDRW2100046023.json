        },
        {
          "id": "MDRW2100046023.1.3",
          "form": "ㅋㅋㅋㅋ역시",
          "original_form": "ㅋㅋㅋㅋ역시",
          "speaker_id": "2",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046023.1.4",
          "form": "근데 어제 갯마울 보고 잤잖아",
          "original_form": "근데 어제 갯마울 보고 잤잖아",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100046023.1.5",
          "form": "김선호 아니고 이상이가 꿈에 나옴",
          "original_form": "김선호 아니고 이상이가 꿈에 나옴",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.6",
          "form": "왜죠.., 선호쒸 왜 저를 피하시나요",
          "original_form": "왜죠.., 선호쒸 왜 저를 피하시나요",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.8",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.9",
          "form": "거기이상이도나오무",
          "original_form": "거기이상이도나오무",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ웃기지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ웃기지",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.11",
          "form": "이상이가 서브남주야!!!",
          "original_form": "이상이가 서브남주야!!!",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.12",
          "form": "4회 마지막에 등장!",
          "original_form": "4회 마지막에 등장!",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.13",
          "form": "앗 스포인가요",
          "original_form": "앗 스포인가요",
          "speaker_id": "1",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.14",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.16",
          "form": "몰랏네",
          "original_form": "몰랏네",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100046023.1.17",
          "form": "암튼 물론 이상이씨도 좋은데",
          "original_form": "암튼 물론 이상이씨도 좋은데",
          "speaker_id": "1",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100046023.1.18",
