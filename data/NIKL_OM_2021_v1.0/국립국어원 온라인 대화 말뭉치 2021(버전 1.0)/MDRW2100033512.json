      "utterance": [
        {
          "id": "MDRW2100033512.1.1",
          "form": "음 누룽지 먹음",
          "original_form": "음 누룽지 먹음",
          "speaker_id": "2",
          "time": "20210824 09:53"
        },
        {
          "id": "MDRW2100033512.1.2",
          "form": "현미 누룽지인데 과자같은",
          "original_form": "현미 누룽지인데 과자같은",
          "speaker_id": "2",
          "time": "20210824 09:53"
        },
        {
          "id": "MDRW2100033512.1.3",
          "form": "베리굿굿",
          "original_form": "베리굿굿",
          "speaker_id": "1",
          "time": "20210824 09:53"
        },
        {
          "id": "MDRW2100033512.1.4",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210824 09:53"
        },
        {
          "id": "MDRW2100033512.1.5",
          "form": "누룽지를 아주얇게 특수 기술 가진 기계로 눌러서",
          "original_form": "누룽지를 아주얇게 특수 기술 가진 기계로 눌러서",
          "speaker_id": "2",
          "time": "20210824 09:53"
        },
        {
          "id": "MDRW2100033512.1.6",
          "form": "손바닥 사이즈로 만든건데",
          "original_form": "손바닥 사이즈로 만든건데",
          "speaker_id": "2",
          "time": "20210824 09:53"
        },
        {
          "id": "MDRW2100033512.1.7",
          "form": "과자같음",
          "original_form": "과자같음",
          "speaker_id": "2",
          "time": "20210824 09:54"
        },
        {
          "id": "MDRW2100033512.1.8",
          "form": "나도 그런거좋아함",
          "original_form": "나도 그런거좋아함",
          "speaker_id": "1",
          "time": "20210824 09:54"
        },
        {
          "id": "MDRW2100033512.1.9",
          "form": "탄수화물중독인듯",
          "original_form": "탄수화물중독인듯",
          "speaker_id": "1",
          "time": "20210824 09:54"
        },
        {
          "id": "MDRW2100033512.1.10",
          "form": "나도 난 밀가루중독",
          "original_form": "나도 난 밀가루중독",
          "speaker_id": "2",
          "time": "20210824 09:56"
        },
        {
          "id": "MDRW2100033512.1.11",
          "form": "다이어트 용 이라도 ㅋㅋ",
          "original_form": "다이어트 용 이라도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:56"
        },
        {
          "id": "MDRW2100033512.1.12",
          "form": "그 정도 먹으면 ㅋㅋ",
          "original_form": "그 정도 먹으면 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:57"
        },
        {
          "id": "MDRW2100033512.1.13",
          "form": "과식은 금물이제",
          "original_form": "과식은 금물이제",
          "speaker_id": "1",
          "time": "20210824 09:57"
        },
        {
          "id": "MDRW2100033512.1.14",
          "form": "음료도 난 식혜 좋아함",
          "original_form": "음료도 난 식혜 좋아함",
          "speaker_id": "1",
          "time": "20210824 09:57"
        },
        {
          "id": "MDRW2100033512.1.15",
          "form": "와 진짜 1일 1식 해야지라고 맘 먹었는데",
          "original_form": "와 진짜 1일 1식 해야지라고 맘 먹었는데",
          "speaker_id": "2",
          "time": "20210824 10:02"
        },
        {
          "id": "MDRW2100033512.1.16",
          "form": "세상에 맛난게 너무 많아서 슬픔",
          "original_form": "세상에 맛난게 너무 많아서 슬픔",
          "speaker_id": "2",
          "time": "20210824 10:03"
        },
        {
          "id": "MDRW2100033512.1.17",
          "form": "쉽지가아너",
          "original_form": "쉽지가아너",
          "speaker_id": "1",
          "time": "20210824 10:07"
        },
        {
          "id": "MDRW2100033512.1.18",
          "form": "스트레쓰 쌓임",
          "original_form": "스트레쓰 쌓임",
          "speaker_id": "1",
          "time": "20210824 10:07"
        },
        {
          "id": "MDRW2100033512.1.19",
          "form": "위 절제술",
          "original_form": "위 절제술",
          "speaker_id": "2",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100033512.1.20",
          "form": "처음 들었을때 어이없는 소리 같았는데",
          "original_form": "처음 들었을때 어이없는 소리 같았는데",
          "speaker_id": "2",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100033512.1.21",
          "form": "이해가",
          "original_form": "이해가",
          "speaker_id": "2",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100033512.1.22",
          "form": "감",
          "original_form": "감",
          "speaker_id": "2",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100033512.1.23",
          "form": "난  아직 이해안가ㅋㅋㅋ",
          "original_form": "난  아직 이해안가ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:25"
        },
        {
          "id": "MDRW2100033512.1.24",
          "form": "참는게 넘 힘들어요",
          "original_form": "참는게 넘 힘들어요",
          "speaker_id": "2",
          "time": "20210824 10:41"
        },
        {
          "id": "MDRW2100033512.1.25",
          "form": "바깥음식 맛있어",
          "original_form": "바깥음식 맛있어",
          "speaker_id": "2",
          "time": "20210824 10:41"
        },
        {
          "id": "MDRW2100033512.1.26",
          "form": "배민으로 음식 시켜먹는게 아니라",
          "original_form": "배민으로 음식 시켜먹는게 아니라",
          "speaker_id": "2",
          "time": "20210824 10:41"
        },
        {
          "id": "MDRW2100033512.1.27",
          "form": "배민 주식을 살걸",
          "original_form": "배민 주식을 살걸",
          "speaker_id": "2",
          "time": "20210824 10:41"
        },
        {
          "id": "MDRW2100033512.1.28",
          "form": "ㅋㅋㅋㅋㅋ 오",
          "original_form": "ㅋㅋㅋㅋㅋ 오",
          "speaker_id": "1",
          "time": "20210824 10:42"
        },
        {
          "id": "MDRW2100033512.1.29",
          "form": "배민 나 아직한번도 안써봄",
          "original_form": "배민 나 아직한번도 안써봄",
          "speaker_id": "1",
          "time": "20210824 10:42"
        },
        {
          "id": "MDRW2100033512.1.30",
          "form": "시작하지 않는것이 현명함 ㅋ",
          "original_form": "시작하지 않는것이 현명함 ㅋ",
          "speaker_id": "2",
          "time": "20210824 10:42"
        },
        {
          "id": "MDRW2100033512.1.31",
