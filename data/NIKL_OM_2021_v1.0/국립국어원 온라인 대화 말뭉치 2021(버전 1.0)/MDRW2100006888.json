        {
          "id": "MDRW2100006888.1.1",
          "form": "네 프랑스처럼 건물도 이쁘죠",
          "original_form": "네 프랑스처럼 건물도 이쁘죠",
          "speaker_id": "2",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100006888.1.2",
          "form": "그 호수보면 물이 깨끗한가요?",
          "original_form": "그 호수보면 물이 깨끗한가요?",
          "speaker_id": "1",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100006888.1.3",
          "form": "가까이가면 오염되어있어요",
          "original_form": "가까이가면 오염되어있어요",
          "speaker_id": "2",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100006888.1.4",
          "form": "그렇구나 수질이 안좋군요 운치있던데",
          "original_form": "그렇구나 수질이 안좋군요 운치있던데",
          "speaker_id": "1",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100006888.1.5",
          "form": "멀리서보면요",
          "original_form": "멀리서보면요",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100006888.1.6",
          "form": "가까이신  냄새나요",
          "original_form": "가까이신  냄새나요",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100006888.1.7",
          "form": "등산도 해보았나요?",
          "original_form": "등산도 해보았나요?",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100006888.1.8",
          "form": "랑비앙산에 가보았지요",
          "original_form": "랑비앙산에 가보았지요",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100006888.1.9",
          "form": "거기 엄청 높다면서요",
          "original_form": "거기 엄청 높다면서요",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100006888.1.10",
          "form": "올라가면 진짜 고생, 지프타야해요",
          "original_form": "올라가면 진짜 고생, 지프타야해요",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100006888.1.11",
          "form": "사서고생할 여행은 안하는 게 낫죠",
          "original_form": "사서고생할 여행은 안하는 게 낫죠",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100006888.1.12",
          "form": "근데 경관은 기가막혀요",
          "original_form": "근데 경관은 기가막혀요",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100006888.1.13",
          "form": "그래서 추천하시는 군요",
          "original_form": "그래서 추천하시는 군요",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100006888.1.14",
          "form": "네 랑비앙등산 추천해요",
          "original_form": "네 랑비앙등산 추천해요",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100006888.1.15",
          "form": "달랏엔 또 무엇이 있지요?",
          "original_form": "달랏엔 또 무엇이 있지요?",
          "speaker_id": "1",
          "time": "20210908 11:28"
