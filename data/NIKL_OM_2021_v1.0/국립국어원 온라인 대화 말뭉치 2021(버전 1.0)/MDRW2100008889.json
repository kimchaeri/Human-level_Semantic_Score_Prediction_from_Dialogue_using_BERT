      "utterance": [
        {
          "id": "MDRW2100008889.1.1",
          "form": "커뮤니티 해봣어요?",
          "original_form": "커뮤니티 해봣어요?",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008889.1.2",
          "form": "커뮤니티 요즘 안하는 사람 있을까요? 전 주로 네이버카페요",
          "original_form": "커뮤니티 요즘 안하는 사람 있을까요? 전 주로 네이버카페요",
          "speaker_id": "2",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008889.1.3",
          "form": "오오 저도 해봣어욤",
          "original_form": "오오 저도 해봣어욤",
          "speaker_id": "1",
          "time": "20210822 19:27"
        },
        {
          "id": "MDRW2100008889.1.4",
          "form": "꾸준히 오래는 안하시나봐요?",
          "original_form": "꾸준히 오래는 안하시나봐요?",
          "speaker_id": "2",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008889.1.5",
          "form": "네네 금방 싫증이 나는편이라서요 ㅋㅋ",
          "original_form": "네네 금방 싫증이 나는편이라서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:28"
        },
        {
          "id": "MDRW2100008889.1.6",
          "form": "전 커뮤니티 사랑해요 ㅎㅎ 몇년 쭉 같은곳 이용하고요",
          "original_form": "전 커뮤니티 사랑해요 ㅎㅎ 몇년 쭉 같은곳 이용하고요",
          "speaker_id": "2",
          "time": "20210822 19:31"
        },
        {
          "id": "MDRW2100008889.1.7",
          "form": "오오 커뮤니티 사람들과 만나본적잇어요??",
          "original_form": "오오 커뮤니티 사람들과 만나본적잇어요??",
          "speaker_id": "1",
          "time": "20210822 19:33"
        },
        {
          "id": "MDRW2100008889.1.8",
          "form": "만나지는 않지만 그 안에서 소모임 만들고 취미활동 함께해요",
          "original_form": "만나지는 않지만 그 안에서 소모임 만들고 취미활동 함께해요",
          "speaker_id": "2",
          "time": "20210822 19:34"
        },
        {
          "id": "MDRW2100008889.1.9",
          "form": "아하 그렇군용!!",
          "original_form": "아하 그렇군용!!",
          "speaker_id": "1",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008889.1.10",
          "form": "그럼 온라인으로 취미활동 한것을 공유하는건가요??",
          "original_form": "그럼 온라인으로 취미활동 한것을 공유하는건가요??",
          "speaker_id": "1",
          "time": "20210822 19:35"
        },
        {
          "id": "MDRW2100008889.1.11",
          "form": "같이 목표 정하고 인증하는 방식이에요 ㅎㅎ 못하면 벌금이ㅡ있기도 하고요",
          "original_form": "같이 목표 정하고 인증하는 방식이에요 ㅎㅎ 못하면 벌금이ㅡ있기도 하고요",
          "speaker_id": "2",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008889.1.12",
          "form": "우와 신기하네용 벌금 내기싫어서라도 열심히 할듯요 ㅋㅋ",
          "original_form": "우와 신기하네용 벌금 내기싫어서라도 열심히 할듯요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008889.1.13",
          "form": "네 지금은 운동 인증, 독서 인증 하고 있어요",
          "original_form": "네 지금은 운동 인증, 독서 인증 하고 있어요",
          "speaker_id": "2",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100008889.1.14",
          "form": "오 좋은취미네용",
          "original_form": "오 좋은취미네용",
          "speaker_id": "1",
          "time": "20210822 19:46"
        },
        {
          "id": "MDRW2100008889.1.15",
          "form": "커뮤니티 통해 사람만난적있으세요?",
          "original_form": "커뮤니티 통해 사람만난적있으세요?",
          "speaker_id": "2",
          "time": "20210822 19:53"
        },
        {
          "id": "MDRW2100008889.1.16",
          "form": "아뇽 없어요!!",
          "original_form": "아뇽 없어요!!",
          "speaker_id": "1",
          "time": "20210822 19:54"
        },
        {
          "id": "MDRW2100008889.1.17",
          "form": "그럼 거의 활동 경험이 없으시군요",
          "original_form": "그럼 거의 활동 경험이 없으시군요",
          "speaker_id": "2",
          "time": "20210822 19:55"
        },
        {
          "id": "MDRW2100008889.1.18",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210822 19:55"
        },
        {
          "id": "MDRW2100008889.1.19",
          "form": "그럼 취미 뭐하세요",
          "original_form": "그럼 취미 뭐하세요",
