      "utterance": [
        {
          "id": "MDRW2100017650.1.1",
          "form": "요즘 미세먼지가 없어서 너무 좋아요!",
          "original_form": "요즘 미세먼지가 없어서 너무 좋아요!",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017650.1.2",
          "form": "그러네요!",
          "original_form": "그러네요!",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017650.1.3",
          "form": "곧 가을이라 바람도 선선해요",
          "original_form": "곧 가을이라 바람도 선선해요",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017650.1.4",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017650.1.5",
          "form": "가을 옷 사고싶네요",
          "original_form": "가을 옷 사고싶네요",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017650.1.6",
          "form": "사셨어요?",
          "original_form": "사셨어요?",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017650.1.7",
          "form": "전 작년거 그대로 입으려구요",
          "original_form": "전 작년거 그대로 입으려구요",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017650.1.8",
          "form": "니트는 샀는데",
          "original_form": "니트는 샀는데",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017650.1.9",
          "form": "트렌치코트도 사고싶어요 ㅠ",
          "original_form": "트렌치코트도 사고싶어요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017650.1.10",
          "form": "캬아~ 트렌치코트",
          "original_form": "캬아~ 트렌치코트",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017650.1.11",
          "form": "계절이랑 잘 어울리네요",
          "original_form": "계절이랑 잘 어울리네요",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017650.1.12",
          "form": "무슨 계절 제일 좋아하세요?",
          "original_form": "무슨 계절 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017650.1.13",
          "form": "저는 가을 좋아해요!",
          "original_form": "저는 가을 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017650.1.14",
          "form": "바람맞는걸 좋아해서요",
          "original_form": "바람맞는걸 좋아해서요",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017650.1.15",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017650.1.16",
          "form": "전 봄이요 ㅎㅎ",
          "original_form": "전 봄이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017650.1.17",
          "form": "온도가 완벽해서요",
          "original_form": "온도가 완벽해서요",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017650.1.18",
          "form": "봄도 따뜻하고 화사하니 좋죠!",
          "original_form": "봄도 따뜻하고 화사하니 좋죠!",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017650.1.19",
          "form": "여름이 낫나요 겨울이 낫나요?",
          "original_form": "여름이 낫나요 겨울이 낫나요?",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017650.1.20",
          "form": "겨울이요~",
          "original_form": "겨울이요~",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017650.1.21",
          "form": "여름 벌레 넘 많아요 ㅠㅠ",
          "original_form": "여름 벌레 넘 많아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017650.1.22",
          "form": "저도 겨울이요",
          "original_form": "저도 겨울이요",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017650.1.23",
          "form": "더운거보다 추운게나은것같아요",
          "original_form": "더운거보다 추운게나은것같아요",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017650.1.24",
          "form": "오 똑같네요",
          "original_form": "오 똑같네요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017650.1.25",
          "form": "겨울 하니까 귤 생각나요 ㅎㅎ",
          "original_form": "겨울 하니까 귤 생각나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017650.1.26",
          "form": "요즘 날씨는 맘에 드세요?",
          "original_form": "요즘 날씨는 맘에 드세요?",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017650.1.27",
          "form": "제 너무 좋아요 습한것만 빼면 ㅎㅎ",
          "original_form": "제 너무 좋아요 습한것만 빼면 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017650.1.28",
          "form": "전 모기 많아 괴롭네요",
          "original_form": "전 모기 많아 괴롭네요",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017650.1.29",
          "form": "개미도 많고 ㅠㅠ",
          "original_form": "개미도 많고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:40"
        },
        {
          "id": "MDRW2100017650.1.30",
          "form": "산 근처에 살거든요",
          "original_form": "산 근처에 살거든요",
          "speaker_id": "2",
          "time": "20210826 21:40"
        }
      ]
    }
  ]
