      "utterance": [
        {
          "id": "MDRW2100026177.1.1",
          "form": "책을 많이 읽다보면 창작욕구도 생기자나요",
          "original_form": "책을 많이 읽다보면 창작욕구도 생기자나요",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026177.1.2",
          "form": "기자생활도 했는데 읽는게 더 좋습니다^^",
          "original_form": "기자생활도 했는데 읽는게 더 좋습니다^^",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026177.1.3",
          "form": "요새 책 내기 쉬워서",
          "original_form": "요새 책 내기 쉬워서",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026177.1.4",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026177.1.5",
          "form": "근데 아직은 필살기가 없어요ㅎㅎ",
          "original_form": "근데 아직은 필살기가 없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026177.1.6",
          "form": "쓰는데 왕도가 있나요",
          "original_form": "쓰는데 왕도가 있나요",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026177.1.7",
          "form": "그럼 종이책을 더 좋아하시겠어요",
          "original_form": "그럼 종이책을 더 좋아하시겠어요",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100026177.1.8",
          "form": "꼭그렇진않아요",
          "original_form": "꼭그렇진않아요",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026177.1.9",
          "form": "환경때매 이북만사요",
          "original_form": "환경때매 이북만사요",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026177.1.10",
          "form": "종이책은 도서관만",
          "original_form": "종이책은 도서관만",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026177.1.11",
          "form": "탭으로 읽어요. 예스 북클럽이랑 밀리의서재 구독합니다",
          "original_form": "탭으로 읽어요. 예스 북클럽이랑 밀리의서재 구독합니다",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026177.1.12",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026177.1.13",
          "form": "저도 종이책을 봐야지하면서",
          "original_form": "저도 종이책을 봐야지하면서",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026177.1.14",
          "form": "아니 이북을",
          "original_form": "아니 이북을",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026177.1.15",
          "form": "종이책을 선호합니다",
          "original_form": "종이책을 선호합니다",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026177.1.16",
          "form": "이북은 메모하고 밑줄긋고 분류하기좋아요",
          "original_form": "이북은 메모하고 밑줄긋고 분류하기좋아요",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026177.1.17",
          "form": "종이는 종이만의 매력이져",
          "original_form": "종이는 종이만의 매력이져",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026177.1.18",
          "form": "넘길때 진짜 책읽는 기분",
          "original_form": "넘길때 진짜 책읽는 기분",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026177.1.19",
          "form": "이북리더기는 느려터져서",
          "original_form": "이북리더기는 느려터져서",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026177.1.20",
          "form": "서울에서 내려올때 책 처분하느라 고생했어요",
          "original_form": "서울에서 내려올때 책 처분하느라 고생했어요",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026177.1.21",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026177.1.22",
          "form": "제주도는 배송비가 붙어서",
          "original_form": "제주도는 배송비가 붙어서",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026177.1.23",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026177.1.24",
          "form": "그렇구뇽",
          "original_form": "그렇구뇽",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026177.1.25",
          "form": "우체국쇼핑몰은 배송비가 안붙어요",
          "original_form": "우체국쇼핑몰은 배송비가 안붙어요",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026177.1.26",
