        {
          "id": "MDRW2100013547.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013547.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013547.1.3",
          "form": "게임 하시는거 있나요?",
          "original_form": "게임 하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013547.1.4",
          "form": "아니요 저 게임을 즐겨하는편은 아니라서",
          "original_form": "아니요 저 게임을 즐겨하는편은 아니라서",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013547.1.5",
          "form": "어떤게임하세요?",
          "original_form": "어떤게임하세요?",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013547.1.6",
          "form": "전 요즘은 쿠키런 킹덤 하고있어요",
          "original_form": "전 요즘은 쿠키런 킹덤 하고있어요",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013547.1.7",
          "form": "폰게임이 소소하게 시간 잘가더라구요",
          "original_form": "폰게임이 소소하게 시간 잘가더라구요",
          "speaker_id": "1",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013547.1.8",
          "form": "아하! 저도 쿠키런했었는데 킹덤은 못해봤네요",
          "original_form": "아하! 저도 쿠키런했었는데 킹덤은 못해봤네요",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013547.1.9",
          "form": "그냥 쿠키런이랑 다른가요?",
          "original_form": "그냥 쿠키런이랑 다른가요?",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013547.1.10",
          "form": "네. 그냥 왕국 꾸미고 키우고 이런거예요",
          "original_form": "네. 그냥 왕국 꾸미고 키우고 이런거예요",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013547.1.11",
          "form": "저는 방탈출에 꽂혀서 방탈출게임 해요ㅋㅋ",
          "original_form": "저는 방탈출에 꽂혀서 방탈출게임 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013547.1.12",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013547.1.13",
          "form": "실제로 가서 하세요?",
          "original_form": "실제로 가서 하세요?",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013547.1.14",
          "form": "아~그래서 인기가 더 좋아졌군요",
          "original_form": "아~그래서 인기가 더 좋아졌군요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013547.1.15",
          "form": "네 방탈출 엄청 재밌어요",
          "original_form": "네 방탈출 엄청 재밌어요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013547.1.16",
          "form": "아직 안해보셨나요?",
          "original_form": "아직 안해보셨나요?",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013547.1.17",
          "form": "네. 한번도 안가봤어요ㅜㅜ",
          "original_form": "네. 한번도 안가봤어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013547.1.18",
          "form": "문제풀고 추리하고 이런거 좋아하시면 재밌으실텐데ㅠ",
          "original_form": "문제풀고 추리하고 이런거 좋아하시면 재밌으실텐데ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013547.1.19",
          "form": "한번 해보시면 계속 하게되실거에요!",
          "original_form": "한번 해보시면 계속 하게되실거에요!",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013547.1.20",
          "form": "진짜 좋아하긴 한데 집 근처에 방탈출 카페가 코로나 직격탄맞고 사라졌어요ㅜㅜ",
          "original_form": "진짜 좋아하긴 한데 집 근처에 방탈출 카페가 코로나 직격탄맞고 사라졌어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013547.1.21",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013547.1.22",
          "form": "에구ㅠ맞아요 코로나때문에 문닫는곳도 꽤  있는거같더라구요",
          "original_form": "에구ㅠ맞아요 코로나때문에 문닫는곳도 꽤  있는거같더라구요",
          "speaker_id": "2",
          "time": "20210825 15:51"
        }
      ]
