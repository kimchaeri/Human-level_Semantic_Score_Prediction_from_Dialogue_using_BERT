      "utterance": [
        {
          "id": "MDRW2100033854.1.1",
          "form": "떡볶이는 좋아하세요?",
          "original_form": "떡볶이는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 12:56"
        },
        {
          "id": "MDRW2100033854.1.2",
          "form": "네 없어서 못먹죠 ㅎㅎ",
          "original_form": "네 없어서 못먹죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100033854.1.3",
          "form": "어떤브랜드 좋아하세요?",
          "original_form": "어떤브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:01"
        },
        {
          "id": "MDRW2100033854.1.4",
          "form": "전 신전요",
          "original_form": "전 신전요",
          "speaker_id": "2",
          "time": "20210825 13:02"
        },
        {
          "id": "MDRW2100033854.1.5",
          "form": "그리고 동네 가게에서 자주 먹어요 ㅎㅎ",
          "original_form": "그리고 동네 가게에서 자주 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100033854.1.6",
          "form": "가깝고 맛도 적당해요",
          "original_form": "가깝고 맛도 적당해요",
          "speaker_id": "2",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100033854.1.7",
          "form": "아 비오는데 튀김이랑 저녁에 먹어야 겠어요",
          "original_form": "아 비오는데 튀김이랑 저녁에 먹어야 겠어요",
          "speaker_id": "2",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100033854.1.8",
          "form": "신전 존맛이죠ㅋㅋㅋ",
          "original_form": "신전 존맛이죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100033854.1.9",
          "form": "맞아요 저도",
          "original_form": "맞아요 저도",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100033854.1.10",
          "form": "엽떡vs신전하면",
          "original_form": "엽떡vs신전하면",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100033854.1.11",
          "form": "신전이예요",
          "original_form": "신전이예요",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100033854.1.12",
          "form": "신전이죠",
          "original_form": "신전이죠",
          "speaker_id": "2",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100033854.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100033854.1.14",
          "form": "오뎅튀김에 스팸컵밥 윽",
          "original_form": "오뎅튀김에 스팸컵밥 윽",
          "speaker_id": "2",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100033854.1.15",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:24"
        },
        {
          "id": "MDRW2100033854.1.16",
          "form": "로제는 어떠세요?",
          "original_form": "로제는 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100033854.1.17",
          "form": "음 것도 먹어요",
          "original_form": "음 것도 먹어요",
          "speaker_id": "2",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100033854.1.18",
          "form": "전좀 오리지날쪽이나",
          "original_form": "전좀 오리지날쪽이나",
          "speaker_id": "2",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100033854.1.19",
          "form": "치즈 추가요",
          "original_form": "치즈 추가요",
          "speaker_id": "2",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100033854.1.20",
          "form": "ㅋㅋㅋ 오리지널쪽이시군요",
          "original_form": "ㅋㅋㅋ 오리지널쪽이시군요",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100033854.1.21",
          "form": "떡볶이에 면사리는 뭐넣으시나요",
          "original_form": "떡볶이에 면사리는 뭐넣으시나요",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100033854.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100033854.1.23",
          "form": "로제세요?",
          "original_form": "로제세요?",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100033854.1.24",
          "form": "그냥 전 라면이요",
          "original_form": "그냥 전 라면이요",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100033854.1.25",
          "form": "전 사리는",
          "original_form": "전 사리는",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100033854.1.26",
          "form": "우동사리파입니다ㅋㅋㅋ",
          "original_form": "우동사리파입니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100033854.1.27",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
