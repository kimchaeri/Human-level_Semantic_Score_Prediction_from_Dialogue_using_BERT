      "utterance": [
        {
          "id": "MDRW2100009530.1.1",
          "form": "와~~~전찌르고 그런거 ㅠㅠ윽",
          "original_form": "와~~~전찌르고 그런거 ㅠㅠ윽",
          "speaker_id": "1",
          "time": "20210823 00:13"
        },
        {
          "id": "MDRW2100009530.1.2",
          "form": "제가 이상한가봐 범죄스릴러 이런거 좋아해요",
          "original_form": "제가 이상한가봐 범죄스릴러 이런거 좋아해요",
          "speaker_id": "2",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009530.1.3",
          "form": "그런거 좋아하시는분 많더라구요~",
          "original_form": "그런거 좋아하시는분 많더라구요~",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009530.1.4",
          "form": "펜하말고는 즐겨보시는거 있으세요?",
          "original_form": "펜하말고는 즐겨보시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 00:14"
        },
        {
          "id": "MDRW2100009530.1.5",
          "form": "얼마전에 알고있지만 봤는데 잼나던데요",
          "original_form": "얼마전에 알고있지만 봤는데 잼나던데요",
          "speaker_id": "2",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100009530.1.6",
          "form": "나이먹은 제가 봐도 풋풋하고 이뻐보이더라구요",
          "original_form": "나이먹은 제가 봐도 풋풋하고 이뻐보이더라구요",
          "speaker_id": "2",
          "time": "20210823 00:15"
        },
        {
          "id": "MDRW2100009530.1.7",
          "form": "아 그건못봤어요",
          "original_form": "아 그건못봤어요",
          "speaker_id": "1",
          "time": "20210823 00:16"
        },
        {
          "id": "MDRW2100009530.1.8",
          "form": "한소희나오는거네요~",
          "original_form": "한소희나오는거네요~",
          "speaker_id": "1",
          "time": "20210823 00:16"
        },
        {
          "id": "MDRW2100009530.1.9",
          "form": "송강 넘 잘생기고 귀엽더라구요 웃는모습이",
          "original_form": "송강 넘 잘생기고 귀엽더라구요 웃는모습이",
          "speaker_id": "2",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009530.1.10",
          "form": "이쁘더라구요ㅋㅋㅋ 송혜교닮았다고",
          "original_form": "이쁘더라구요ㅋㅋㅋ 송혜교닮았다고",
          "speaker_id": "1",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009530.1.11",
          "form": "피부가 엄청좋고",
          "original_form": "피부가 엄청좋고",
          "speaker_id": "2",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009530.1.12",
          "form": "요즘 연예인들은 전부",
          "original_form": "요즘 연예인들은 전부",
          "speaker_id": "1",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009530.1.13",
          "form": "한소희는 그 김희애나오는거에서 나왔었죠",
          "original_form": "한소희는 그 김희애나오는거에서 나왔었죠",
          "speaker_id": "2",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009530.1.14",
          "form": "피지컬이",
          "original_form": "피지컬이",
          "speaker_id": "1",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009530.1.15",
          "form": "그니까 연예인하겠죠ㅎ 부럽다는",
          "original_form": "그니까 연예인하겠죠ㅎ 부럽다는",
          "speaker_id": "2",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009530.1.16",
          "form": "네~ 그 바람피는드라마",
          "original_form": "네~ 그 바람피는드라마",
          "speaker_id": "1",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009530.1.17",
          "form": "거기나와서 그런지 왠지 대학생이 안어울리는 느낌ㅋ",
          "original_form": "거기나와서 그런지 왠지 대학생이 안어울리는 느낌ㅋ",
          "speaker_id": "2",
          "time": "20210823 00:18"
        }
      ]
