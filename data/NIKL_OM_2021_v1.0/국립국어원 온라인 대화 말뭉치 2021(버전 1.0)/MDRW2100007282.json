      "utterance": [
        {
          "id": "MDRW2100007282.1.1",
          "form": "미용과 ,,, 건강,,, 나 둘 다 버리고 있을지도 ㅎ",
          "original_form": "미용과 ,,, 건강,,, 나 둘 다 버리고 있을지도 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007282.1.2",
          "form": "나도..ㅎ",
          "original_form": "나도..ㅎ",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007282.1.3",
          "form": "최근에 컴퓨터에 돈지르고",
          "original_form": "최근에 컴퓨터에 돈지르고",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007282.1.4",
          "form": "나도 돈좀써보자 싶어서",
          "original_form": "나도 돈좀써보자 싶어서",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007282.1.5",
          "form": "쌍수도했어",
          "original_form": "쌍수도했어",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007282.1.6",
          "form": "헐헐 대박",
          "original_form": "헐헐 대박",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007282.1.7",
          "form": "어때???",
          "original_form": "어때???",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007282.1.8",
          "form": "아직 눈 위로뜨면 땅기는느낌드는데",
          "original_form": "아직 눈 위로뜨면 땅기는느낌드는데",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007282.1.9",
          "form": "2주 좀 넘었거든",
          "original_form": "2주 좀 넘었거든",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007282.1.10",
          "form": "평생이러려나 ㅋㅋㅋㅋ ㅜㅜ",
          "original_form": "평생이러려나 ㅋㅋㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007282.1.11",
          "form": "그리고 눈만지면 아픈거같애아직",
          "original_form": "그리고 눈만지면 아픈거같애아직",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007282.1.12",
          "form": "그렇구나 아직 2주니까 너무 만지지말어~~",
          "original_form": "그렇구나 아직 2주니까 너무 만지지말어~~",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007282.1.13",
          "form": "난 원래 미용쪽으로는 챙길 머리가 없어서 진짜 거리감 든다 ㅋ큐ㅠㅠ",
          "original_form": "난 원래 미용쪽으로는 챙길 머리가 없어서 진짜 거리감 든다 ㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007282.1.14",
          "form": "나도진짜 안하는데",
          "original_form": "나도진짜 안하는데",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007282.1.15",
          "form": "최근에넘우울해서 뭐라도해보고싶어졌어",
          "original_form": "최근에넘우울해서 뭐라도해보고싶어졌어",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007282.1.16",
          "form": "그냥 뭐라도 변화가있으면좋겠다 싶어서",
          "original_form": "그냥 뭐라도 변화가있으면좋겠다 싶어서",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007282.1.17",
          "form": "확실히 바꾸는게 도움이 되는 거 같긴 하더라",
          "original_form": "확실히 바꾸는게 도움이 되는 거 같긴 하더라",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007282.1.18",
          "form": "글고보니까 나도 머리 숏컷으로 잘랐엉 ㅎㅋㅋ",
          "original_form": "글고보니까 나도 머리 숏컷으로 잘랐엉 ㅎㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007282.1.19",
          "form": "웡래 어깨아래는 내려갔는데 자르니까 진짜 완전 시원해 굳굳",
          "original_form": "웡래 어깨아래는 내려갔는데 자르니까 진짜 완전 시원해 굳굳",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007282.1.20",
          "form": "난 머리숏컷인지가 8년째라",
          "original_form": "난 머리숏컷인지가 8년째라",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007282.1.21",
          "form": "최근에 기를까 싶어지고",
          "original_form": "최근에 기를까 싶어지고",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007282.1.22",
          "form": "헐 8년 유지했구나",
          "original_form": "헐 8년 유지했구나",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007282.1.23",
          "form": "한번자르니까 편해서 못기르겠더라고!",
          "original_form": "한번자르니까 편해서 못기르겠더라고!",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007282.1.24",
          "form": "머리 숏컷 치니까 아침마다 머리 답이 없는데 원래 이래..? ",
          "original_form": "머리 숏컷 치니까 아침마다 머리 답이 없는데 원래 이래..? ",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007282.1.25",
          "form": "편하긴 진짜 편한뎅 ㅎ",
          "original_form": "편하긴 진짜 편한뎅 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007282.1.26",
          "form": "난원래 매일 나가기전에 아침마다 머리 감아야해서",
          "original_form": "난원래 매일 나가기전에 아침마다 머리 감아야해서",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007282.1.27",
          "form": "그냥 금방감고 안말리고나가도되는게좋았어",
          "original_form": "그냥 금방감고 안말리고나가도되는게좋았어",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007282.1.28",
          "form": "나는 밤마다 감고 아침엔 걍 빗고만 나갔는데 숏컷하고 나니까 덕분에 아침에 부지런걸 되어버렸어 ㅋㅋㅋㅠ",
          "original_form": "나는 밤마다 감고 아침엔 걍 빗고만 나갔는데 숏컷하고 나니까 덕분에 아침에 부지런걸 되어버렸어 ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007282.1.29",
          "form": "좋은 습관 얻어간당...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "좋은 습관 얻어간당...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007282.1.30",
          "form": "ㅋㅋㅋㅋ난 소용없는게 극 지성이라",
          "original_form": "ㅋㅋㅋㅋ난 소용없는게 극 지성이라",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007282.1.31",
          "form": "자기전에감고자면 아침에 떡져있으니깐",
          "original_form": "자기전에감고자면 아침에 떡져있으니깐",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007282.1.32",
          "form": "아침에감아도 저녁때되면 떡져있고",
          "original_form": "아침에감아도 저녁때되면 떡져있고",
