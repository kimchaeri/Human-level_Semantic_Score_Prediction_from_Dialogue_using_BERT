      "utterance": [
        {
          "id": "MDRW2100005902.1.1",
          "form": "집안일하시나요?",
          "original_form": "집안일하시나요?",
          "speaker_id": "2",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100005902.1.2",
          "form": "name1님?",
          "original_form": "&name1&님?",
          "speaker_id": "2",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100005902.1.3",
          "form": "네 집안일 집에 있을 때 좀 하는 편이에요",
          "original_form": "네 집안일 집에 있을 때 좀 하는 편이에요",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100005902.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100005902.1.5",
          "form": "반말해도돼",
          "original_form": "반말해도돼",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100005902.1.6",
          "form": "어떤집안일해?",
          "original_form": "어떤집안일해?",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100005902.1.7",
          "form": "나 설거지랑 밥 짓기 자주 하는 편이야!",
          "original_form": "나 설거지랑 밥 짓기 자주 하는 편이야!",
          "speaker_id": "1",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100005902.1.8",
          "form": "오.. 설거지하는건알고있었디",
          "original_form": "오.. 설거지하는건알고있었디",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100005902.1.9",
          "form": "가족이랑 같이살면서 불편한점있어?",
          "original_form": "가족이랑 같이살면서 불편한점있어?",
          "speaker_id": "2",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100005902.1.10",
          "form": "음 우리 집이 좀 프리해서 불편한건 못 느낀거같은데",
          "original_form": "음 우리 집이 좀 프리해서 불편한건 못 느낀거같은데",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005902.1.11",
          "form": "넌 불편했던거 있었어?",
          "original_form": "넌 불편했던거 있었어?",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005902.1.12",
          "form": "난 통금 ㅠㅠ",
          "original_form": "난 통금 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005902.1.13",
          "form": "그래서 혼자사는게 편하긴하더라",
          "original_form": "그래서 혼자사는게 편하긴하더라",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100005902.1.14",
          "form": "대신 집안일 x2",
          "original_form": "대신 집안일 x2",
          "speaker_id": "2",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100005902.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100005902.1.16",
          "form": "ㅋㅋㅋㅋㅋ통금...",
          "original_form": "ㅋㅋㅋㅋㅋ통금...",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100005902.1.17",
          "form": "통금이 크긴하지",
          "original_form": "통금이 크긴하지",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100005902.1.18",
          "form": "근데 집안일 두배는 좀 빡셀 것 같은데?",
          "original_form": "근데 집안일 두배는 좀 빡셀 것 같은데?",
          "speaker_id": "1",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100005902.1.19",
          "form": "그건맞아",
          "original_form": "그건맞아",
          "speaker_id": "2",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100005902.1.20",
          "form": "그래도 새벽롤 가능한개",
          "original_form": "그래도 새벽롤 가능한개",
          "speaker_id": "2",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100005902.1.21",
          "form": "너무행복해",
          "original_form": "너무행복해",
          "speaker_id": "2",
          "time": "20210907 15:58"
        },
        {
          "id": "MDRW2100005902.1.22",
          "form": "맞지 밤낮 안 바뀌어?",
          "original_form": "맞지 밤낮 안 바뀌어?",
          "speaker_id": "1",
          "time": "20210907 15:59"
        },
        {
          "id": "MDRW2100005902.1.23",
          "form": "너도 같이 바뀌는것같던데?",
          "original_form": "너도 같이 바뀌는것같던데?",
          "speaker_id": "2",
          "time": "20210907 15:59"
        },
        {
          "id": "MDRW2100005902.1.24",
          "form": "룸메이트 없어서 너무 좋긴햐",
          "original_form": "룸메이트 없어서 너무 좋긴햐",
          "speaker_id": "2",
          "time": "20210907 15:59"
        },
        {
          "id": "MDRW2100005902.1.25",
          "form": "집에서 그럼 집안일 역할이 있는거야?",
          "original_form": "집에서 그럼 집안일 역할이 있는거야?",
          "speaker_id": "2",
          "time": "20210907 15:59"
        },
        {
          "id": "MDRW2100005902.1.26",
          "form": "그래도 룸메이트 있으면 집안일도 줄고",
          "original_form": "그래도 룸메이트 있으면 집안일도 줄고",
          "speaker_id": "1",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100005902.1.27",
          "form": "심심한것도 없어지지않아?",
          "original_form": "심심한것도 없어지지않아?",
          "speaker_id": "1",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100005902.1.28",
          "form": "그건맞지",
          "original_form": "그건맞지",
          "speaker_id": "2",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100005902.1.29",
          "form": "근데 보이스하면서 롤을 못해",
          "original_form": "근데 보이스하면서 롤을 못해",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100005902.1.30",
          "form": "마우스도 딸깍거리구",
          "original_form": "마우스도 딸깍거리구",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100005902.1.31",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100005902.1.32",
          "form": "어떻게 게임 하는 분으로 못 구해??",
          "original_form": "어떻게 게임 하는 분으로 못 구해??",
          "speaker_id": "1",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100005902.1.33",
          "form": "여자애중에 나같은애가 어디 많니^&....",
          "original_form": "여자애중에 나같은애가 어디 많니^&....",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100005902.1.34",
          "form": "남사친이랑 동거는 좀 그렇잖아",
          "original_form": "남사친이랑 동거는 좀 그렇잖아",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100005902.1.35",
          "form": "아니야?",
          "original_form": "아니야?",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100005902.1.36",
          "form": "ㅎㅏ하",
          "original_form": "ㅎㅏ하",
          "speaker_id": "2",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100005902.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋ그건 맞지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그건 맞지",
