      "utterance": [
        {
          "id": "MDRW2100009184.1.1",
          "form": "글죠ㅎ 나이가 들어서ㅎㅎ 가사가 너무힘들어요",
          "original_form": "글죠ㅎ 나이가 들어서ㅎㅎ 가사가 너무힘들어요",
          "speaker_id": "2",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009184.1.2",
          "form": "외벌이시면 가사분담은 안하실려나요ㅎ",
          "original_form": "외벌이시면 가사분담은 안하실려나요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009184.1.3",
          "form": "음... 음...",
          "original_form": "음... 음...",
          "speaker_id": "2",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009184.1.4",
          "form": "집안일도 도와주세요?",
          "original_form": "집안일도 도와주세요?",
          "speaker_id": "1",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009184.1.5",
          "form": "빨래빼고 제가다해요ㅠ",
          "original_form": "빨래빼고 제가다해요ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009184.1.6",
          "form": "아이관련된 일은 아내가하구요ㅎ",
          "original_form": "아이관련된 일은 아내가하구요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009184.1.7",
          "form": "아 그때 말씀하셨네요ㅋㅋ",
          "original_form": "아 그때 말씀하셨네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009184.1.8",
          "form": "섬유유연제",
          "original_form": "섬유유연제",
          "speaker_id": "1",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009184.1.9",
          "form": "아ㅎㅎㅎ 생각나시는구나ㅎ",
          "original_form": "아ㅎㅎㅎ 생각나시는구나ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009184.1.10",
          "form": "주말내내 뒹굴다 낼출근해야되는데 집안일이 쌓여있네요",
          "original_form": "주말내내 뒹굴다 낼출근해야되는데 집안일이 쌓여있네요",
          "speaker_id": "1",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009184.1.11",
          "form": "빨래는 분명 세탁기가 하는데",
          "original_form": "빨래는 분명 세탁기가 하는데",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009184.1.12",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009184.1.13",
          "form": "원래그래요ㅎ",
          "original_form": "원래그래요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009184.1.14",
          "form": "월요일이 다가오니ㅠ",
          "original_form": "월요일이 다가오니ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009184.1.15",
          "form": "ㅎㅎ 그래도 빨래라도  안하니 좋자나요",
          "original_form": "ㅎㅎ 그래도 빨래라도  안하니 좋자나요",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009184.1.16",
          "form": "밥먹은 설거지해야되는데 귀찮고",
          "original_form": "밥먹은 설거지해야되는데 귀찮고",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009184.1.17",
          "form": "음쓰도 버리고  분리수거도 해야되고ㅋ",
          "original_form": "음쓰도 버리고  분리수거도 해야되고ㅋ",
          "speaker_id": "1",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100009184.1.18",
          "form": "빨래라도...퓨",
          "original_form": "빨래라도...퓨",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009184.1.19",
          "form": "다 귀찮네요ㅎ 시켜드셔야겟다ㅎㅎㅎㅎ",
          "original_form": "다 귀찮네요ㅎ 시켜드셔야겟다ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009184.1.20",
          "form": "배달도 잘안시키는데 플라스틱은 왜이리 많은지 몰겠어요",
          "original_form": "배달도 잘안시키는데 플라스틱은 왜이리 많은지 몰겠어요",
          "speaker_id": "1",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009184.1.21",
          "form": "원래 그런거죠뭐ㅎ 다 귀찮아도ㅎ 사랑하는사람먹는거 보면 귀찮은게 사라져요ㅎ",
          "original_form": "원래 그런거죠뭐ㅎ 다 귀찮아도ㅎ 사랑하는사람먹는거 보면 귀찮은게 사라져요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009184.1.22",
          "form": "택배상자는 왜이렇게 많은지",
          "original_form": "택배상자는 왜이렇게 많은지",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009184.1.23",
          "form": "하루에 기본 3~4개ㅠㅠ",
          "original_form": "하루에 기본 3~4개ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009184.1.24",
          "form": "ㅎㅎ 그런가요? 자식이 먹는것만 봐도 배부르다는게  사실이예요?",
          "original_form": "ㅎㅎ 그런가요? 자식이 먹는것만 봐도 배부르다는게  사실이예요?",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009184.1.25",
          "form": "솔직히....",
          "original_form": "솔직히....",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009184.1.26",
          "form": "나도 배고파요ㅎㅎ",
          "original_form": "나도 배고파요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009184.1.27",
          "form": "ㅋㅋ 글쵸?",
          "original_form": "ㅋㅋ 글쵸?",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009184.1.28",
          "form": "근데 아이먼저 챙기죠ㅎ 그러다가 조금 주워먹으면 아기가 째려보고",
          "original_form": "근데 아이먼저 챙기죠ㅎ 그러다가 조금 주워먹으면 아기가 째려보고",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009184.1.29",
          "form": "그리고 젓가락 놓고 그래요ㅎ",
          "original_form": "그리고 젓가락 놓고 그래요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009184.1.30",
          "form": "요즘 아이께 더 맛있는거 알죠?",
          "original_form": "요즘 아이께 더 맛있는거 알죠?",
          "speaker_id": "2",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009184.1.31",
          "form": "아이들껀 재료도 좋자나요ㅎ",
          "original_form": "아이들껀 재료도 좋자나요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009184.1.32",
          "form": "저흰 조미료 천지인데",
          "original_form": "저흰 조미료 천지인데",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009184.1.33",
          "form": "아이들꺼에도 조미료 안들어가면 맛없어서 안먹는대요ㅎㅎ",
          "original_form": "아이들꺼에도 조미료 안들어가면 맛없어서 안먹는대요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009184.1.34",
          "form": "오히려 더 들었을수도 있어요 ㅡㅎ",
          "original_form": "오히려 더 들었을수도 있어요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210822 22:12"
        }
      ]
