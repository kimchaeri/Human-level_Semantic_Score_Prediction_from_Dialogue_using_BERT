      "utterance": [
        {
          "id": "MDRW2100039610.1.1",
          "form": "오늘 저녁은 뭐 드셨어요?",
          "original_form": "오늘 저녁은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039610.1.2",
          "form": "옛날도시락 먹었어요ㅋㅋㅋ볶은김치넣어서",
          "original_form": "옛날도시락 먹었어요ㅋㅋㅋ볶은김치넣어서",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039610.1.3",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100039610.1.4",
          "form": "저는 가지볶음이랑 양배추찜 먹었어요",
          "original_form": "저는 가지볶음이랑 양배추찜 먹었어요",
          "speaker_id": "1",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100039610.1.5",
          "form": "옛날 도시락 맛있겠어요",
          "original_form": "옛날 도시락 맛있겠어요",
          "speaker_id": "1",
          "time": "20210903 19:44"
        },
        {
          "id": "MDRW2100039610.1.6",
          "form": "오 양배추찜 맛있는데 싸먹는거였나요?",
          "original_form": "오 양배추찜 맛있는데 싸먹는거였나요?",
          "speaker_id": "2",
          "time": "20210903 19:48"
        },
        {
          "id": "MDRW2100039610.1.7",
          "form": "네 ㅎ 갑자기 땡겨서 비엔나소세지 넣어서 먹었어여",
          "original_form": "네 ㅎ 갑자기 땡겨서 비엔나소세지 넣어서 먹었어여",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100039610.1.8",
          "form": "네 양배추찜에 두부쌈장 만들어서 먹었어요",
          "original_form": "네 양배추찜에 두부쌈장 만들어서 먹었어요",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039610.1.9",
          "form": "저는 옛날 도시락에 분홍소세지 든거 좋아해요 비엔나도 맛있겠어요ㅎㅎ",
          "original_form": "저는 옛날 도시락에 분홍소세지 든거 좋아해요 비엔나도 맛있겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039610.1.10",
          "form": "헉 대박 맛있겠네요 두부쌈장이라니!!!!!!",
          "original_form": "헉 대박 맛있겠네요 두부쌈장이라니!!!!!!",
          "speaker_id": "2",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039610.1.11",
          "form": "요리 잘하시나봐요",
          "original_form": "요리 잘하시나봐요",
          "speaker_id": "2",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039610.1.12",
          "form": "맞아요 ㅎ 일단 분홍소세지없어서 급하게 비엔나 넣어먹었어여",
          "original_form": "맞아요 ㅎ 일단 분홍소세지없어서 급하게 비엔나 넣어먹었어여",
          "speaker_id": "2",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039610.1.13",
          "form": "헉 아뇨 그냥 시판용 쌈장에 두부밖에 안넣었어요!ㅋㅋㅋㅋㅋ",
          "original_form": "헉 아뇨 그냥 시판용 쌈장에 두부밖에 안넣었어요!ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039610.1.14",
          "form": "분홍소세지 좋아해요 그 밀가루맛 ㅋㅋㅋ",
          "original_form": "분홍소세지 좋아해요 그 밀가루맛 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039610.1.15",
          "form": "아하 ㅋㅋㅋㅋ그래도 맛있겠네요",
          "original_form": "아하 ㅋㅋㅋㅋ그래도 맛있겠네요",
          "speaker_id": "2",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039610.1.16",
          "form": "맞아요 그 약간 질퍽한 (?)질감",
          "original_form": "맞아요 그 약간 질퍽한 (?)질감",
          "speaker_id": "2",
          "time": "20210903 19:59"
        },
        {
          "id": "MDRW2100039610.1.17",
          "form": "좀 비싼애로 사봤는데 싼게 더 맛있었어요 ㅋㅋㅋㅋ",
          "original_form": "좀 비싼애로 사봤는데 싼게 더 맛있었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100039610.1.18",
          "form": "역시ㅋㅋㅋㅋ대중적인 아이가 맛있는거같아요",
          "original_form": "역시ㅋㅋㅋㅋ대중적인 아이가 맛있는거같아요",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039610.1.19",
          "form": "익숙한 ㅋㅋ",
          "original_form": "익숙한 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100039610.1.20",
          "form": "조금 비싼맛이라 어중간해서 더 맛이 없었어요ㅋㅋㅋ",
          "original_form": "조금 비싼맛이라 어중간해서 더 맛이 없었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100039610.1.21",
          "form": "소세지 맛있는거 추천 좀 해주세요 ㅎㅎ",
          "original_form": "소세지 맛있는거 추천 좀 해주세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039610.1.22",
          "form": "그냥 소세지요!?",
          "original_form": "그냥 소세지요!?",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039610.1.23",
          "form": "넹 ㅋㅋ",
          "original_form": "넹 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039610.1.24",
          "form": "맛있는 비엔나소세지 있나 싶어서요",
          "original_form": "맛있는 비엔나소세지 있나 싶어서요",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039610.1.25",
          "form": "맨날 사먹는거만 사먹는거 같아서 ㅎㅎ",
          "original_form": "맨날 사먹는거만 사먹는거 같아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039610.1.26",
          "form": "저는 통통한 비엔나는 맛이없더라고요",
          "original_form": "저는 통통한 비엔나는 맛이없더라고요",
          "speaker_id": "2",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039610.1.27",
          "form": "롯데햄이나 동원꺼가 맛있더라고용",
          "original_form": "롯데햄이나 동원꺼가 맛있더라고용",
          "speaker_id": "2",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100039610.1.28",
          "form": "역시! 저도 마늘햄때문에 롯데햄 먹게됐는데 맛있더라고요ㅎㅎ",
