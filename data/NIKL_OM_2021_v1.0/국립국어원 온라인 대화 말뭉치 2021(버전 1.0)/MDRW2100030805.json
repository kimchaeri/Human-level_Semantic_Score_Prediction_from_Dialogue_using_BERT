      "utterance": [
        {
          "id": "MDRW2100030805.1.1",
          "form": "곧 점인지 점인지 알아보는 거는 차트 공부를 해야 하나요",
          "original_form": "곧 점인지 점인지 알아보는 거는 차트 공부를 해야 하나요",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030805.1.2",
          "form": "근데 그 타이밍을 모르니까 분할해서 사요",
          "original_form": "근데 그 타이밍을 모르니까 분할해서 사요",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030805.1.3",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030805.1.4",
          "form": "왜 제 말을 인식하지 못 할까요",
          "original_form": "왜 제 말을 인식하지 못 할까요",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030805.1.5",
          "form": "우선 사경인책이든 보고",
          "original_form": "우선 사경인책이든 보고",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030805.1.6",
          "form": "쇼핑은 어렵군요",
          "original_form": "쇼핑은 어렵군요",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030805.1.7",
          "form": "이 회사 적정주가를 봐야해요",
          "original_form": "이 회사 적정주가를 봐야해요",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030805.1.8",
          "form": "아 그걸로 알 수 있는 거군요",
          "original_form": "아 그걸로 알 수 있는 거군요",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030805.1.9",
          "form": "회사마다 적정주가가 있어요",
          "original_form": "회사마다 적정주가가 있어요",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030805.1.10",
          "form": "나중에 나가서 주식 관련 책을 빌려 봐야 되겠어요",
          "original_form": "나중에 나가서 주식 관련 책을 빌려 봐야 되겠어요",
          "speaker_id": "2",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100030805.1.11",
          "form": "거기에 못미치면 싼건데",
          "original_form": "거기에 못미치면 싼건데",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030805.1.12",
          "form": "이회사가 이슈도 있어야해요",
          "original_form": "이회사가 이슈도 있어야해요",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030805.1.13",
          "form": "per 같은 거 보고도 차는 거 맞나요",
          "original_form": "per 같은 거 보고도 차는 거 맞나요",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030805.1.14",
          "form": "이슈를 재료라해요",
          "original_form": "이슈를 재료라해요",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030805.1.15",
          "form": "퍼는 품목마다달라요",
          "original_form": "퍼는 품목마다달라요",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030805.1.16",
          "form": "수소 폴더블폰 이런 이슈 같은 거요",
          "original_form": "수소 폴더블폰 이런 이슈 같은 거요",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030805.1.17",
          "form": "관련업종 평균 퍼를 보세요",
          "original_form": "관련업종 평균 퍼를 보세요",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030805.1.18",
          "form": "품목에 비해서 맞으면은 저평가된 건가요",
          "original_form": "품목에 비해서 맞으면은 저평가된 건가요",
          "speaker_id": "2",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030805.1.19",
          "form": "바이오는 무의미해요",
          "original_form": "바이오는 무의미해요",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100030805.1.20",
          "form": "하지만 요즘에는 바이오가 워낙 핫 해서 단타를 하려면 그 쪽으로 해야 하지 않을까요",
          "original_form": "하지만 요즘에는 바이오가 워낙 핫 해서 단타를 하려면 그 쪽으로 해야 하지 않을까요",
          "speaker_id": "2",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030805.1.21",
          "form": "네근데 회사가 제대로 굴러가는지 공시도 보고요",
          "original_form": "네근데 회사가 제대로 굴러가는지 공시도 보고요",
          "speaker_id": "1",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030805.1.22",
          "form": "회사명 자주 바뀌는지",
          "original_form": "회사명 자주 바뀌는지",
          "speaker_id": "1",
          "time": "20210904 12:46"
        },
        {
          "id": "MDRW2100030805.1.23",
          "form": "아 그런 거 알아보는게 정말 힘들 것 같아요",
          "original_form": "아 그런 거 알아보는게 정말 힘들 것 같아요",
          "speaker_id": "2",
          "time": "20210904 12:46"
        }
      ]
