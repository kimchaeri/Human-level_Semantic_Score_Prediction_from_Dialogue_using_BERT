      "utterance": [
        {
          "id": "MDRW2100011295.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 10:50"
        },
        {
          "id": "MDRW2100011295.1.2",
          "form": "네 비가와서 그런가 날씨가 선선하네요",
          "original_form": "네 비가와서 그런가 날씨가 선선하네요",
          "speaker_id": "2",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100011295.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100011295.1.4",
          "form": "아직도 비가오나요?",
          "original_form": "아직도 비가오나요?",
          "speaker_id": "1",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100011295.1.5",
          "form": "갑자기 해 떴어요!!!다행 이렇게 비 끝났으면 좋겠는데..",
          "original_form": "갑자기 해 떴어요!!!다행 이렇게 비 끝났으면 좋겠는데..",
          "speaker_id": "2",
          "time": "20210824 11:41"
        },
        {
          "id": "MDRW2100011295.1.6",
          "form": "비오는날 좋아하세요?",
          "original_form": "비오는날 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100011295.1.7",
          "form": "네네 저 비오는거좋아해요",
          "original_form": "네네 저 비오는거좋아해요",
          "speaker_id": "1",
          "time": "20210824 11:50"
        },
        {
          "id": "MDRW2100011295.1.8",
          "form": "asmr같고ㅋㅋㅋ",
          "original_form": "asmr같고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100011295.1.9",
          "form": "아 저는 제가 실내에 있을때 오는 비 좋아해요",
          "original_form": "아 저는 제가 실내에 있을때 오는 비 좋아해요",
          "speaker_id": "2",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100011295.1.10",
          "form": "근데 최근엔 자주와서 좀 꿉꿉ㅠㅠ",
          "original_form": "근데 최근엔 자주와서 좀 꿉꿉ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 16:35"
        },
        {
          "id": "MDRW2100011295.1.11",
          "form": "저도 비 좋아해요",
          "original_form": "저도 비 좋아해요",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100011295.1.12",
          "form": "근데 습하긴하죠ㅠ",
          "original_form": "근데 습하긴하죠ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100011295.1.13",
          "form": "맞아유 빨래가 잘 안말라서 불만입니다ㅋㅋㅋ",
          "original_form": "맞아유 빨래가 잘 안말라서 불만입니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100011295.1.14",
          "form": "ㅋㅋㅋ 현실적인",
          "original_form": "ㅋㅋㅋ 현실적인",
          "speaker_id": "1",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100011295.1.15",
          "form": "이유네요ㅠㅠ",
          "original_form": "이유네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100011295.1.16",
          "form": "ㅋㅋㅋ요새 옷은 어떻게 입으세요?",
          "original_form": "ㅋㅋㅋ요새 옷은 어떻게 입으세요?",
          "speaker_id": "2",
          "time": "20210825 00:58"
        },
        {
          "id": "MDRW2100011295.1.17",
          "form": "약간 간절기 느낌이라",
          "original_form": "약간 간절기 느낌이라",
          "speaker_id": "2",
          "time": "20210825 00:58"
        },
        {
          "id": "MDRW2100011295.1.18",
          "form": "저는 더위를 많이 타서 상의는 아직 반팔입어요",
          "original_form": "저는 더위를 많이 타서 상의는 아직 반팔입어요",
          "speaker_id": "2",
          "time": "20210825 00:58"
        },
        {
          "id": "MDRW2100011295.1.19",
          "form": "ㅋㅋㅋ 저도반팔이요",
          "original_form": "ㅋㅋㅋ 저도반팔이요",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100011295.1.20",
          "form": "긴팔은 덥더라고요",
          "original_form": "긴팔은 덥더라고요",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100011295.1.21",
          "form": "계절은 어느계절 좋아하세요~?!",
          "original_form": "계절은 어느계절 좋아하세요~?!",
          "speaker_id": "2",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100011295.1.22",
          "form": "아묻따",
          "original_form": "아묻따",
          "speaker_id": "1",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100011295.1.23",
          "form": "가을이죠!",
          "original_form": "가을이죠!",
          "speaker_id": "1",
          "time": "20210825 01:11"
        },
        {
          "id": "MDRW2100011295.1.24",
          "form": "역시 가을~~ 천고마비의 계절 아니겠습니까",
          "original_form": "역시 가을~~ 천고마비의 계절 아니겠습니까",
