      "utterance": [
        {
          "id": "MDRW2100025130.1.1",
          "form": "게임해요?",
          "original_form": "게임해요?",
          "speaker_id": "1",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100025130.1.2",
          "form": "게임하죠",
          "original_form": "게임하죠",
          "speaker_id": "2",
          "time": "20210831 23:28"
        },
        {
          "id": "MDRW2100025130.1.3",
          "form": "저는 오버워치 해요!",
          "original_form": "저는 오버워치 해요!",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100025130.1.4",
          "form": "전 브롤해요",
          "original_form": "전 브롤해요",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100025130.1.5",
          "form": "브롤 많이 들어봤는데 폰겜인가요?",
          "original_form": "브롤 많이 들어봤는데 폰겜인가요?",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100025130.1.6",
          "form": "네 폰이여",
          "original_form": "네 폰이여",
          "speaker_id": "1",
          "time": "20210831 23:56"
        },
        {
          "id": "MDRW2100025130.1.7",
          "form": "어떤 겜인지 설명해주실 수 있나요?",
          "original_form": "어떤 겜인지 설명해주실 수 있나요?",
          "speaker_id": "2",
          "time": "20210831 23:57"
        },
        {
          "id": "MDRW2100025130.1.8",
          "form": "초딩겜이여",
          "original_form": "초딩겜이여",
          "speaker_id": "1",
          "time": "20210901 00:01"
        },
        {
          "id": "MDRW2100025130.1.9",
          "form": "총쏘는",
          "original_form": "총쏘는",
          "speaker_id": "1",
          "time": "20210901 00:01"
        },
        {
          "id": "MDRW2100025130.1.10",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025130.1.11",
          "form": "혹시 데바데라는 겜 아세요?",
          "original_form": "혹시 데바데라는 겜 아세요?",
          "speaker_id": "2",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100025130.1.12",
          "form": "아뇨 어떤거요?",
          "original_form": "아뇨 어떤거요?",
          "speaker_id": "1",
          "time": "20210901 00:03"
        },
        {
          "id": "MDRW2100025130.1.13",
          "form": "스팀게임인데 살인마가 생존자 잡으러 다니는 겜이에요",
          "original_form": "스팀게임인데 살인마가 생존자 잡으러 다니는 겜이에요",
          "speaker_id": "2",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025130.1.14",
          "form": "어렵죠?",
          "original_form": "어렵죠?",
          "speaker_id": "1",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100025130.1.15",
          "form": "첨엔 쫌 어려운데 하다보면 재밌어요",
          "original_form": "첨엔 쫌 어려운데 하다보면 재밌어요",
          "speaker_id": "2",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025130.1.16",
          "form": "익숙해지면괜찮아 지는데 어렵네오",
          "original_form": "익숙해지면괜찮아 지는데 어렵네오",
          "speaker_id": "1",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025130.1.17",
          "form": "무서운거 좋아하시면 재밌게 할 수 있어요",
          "original_form": "무서운거 좋아하시면 재밌게 할 수 있어요",
          "speaker_id": "2",
          "time": "20210901 00:12"
        }
      ]
