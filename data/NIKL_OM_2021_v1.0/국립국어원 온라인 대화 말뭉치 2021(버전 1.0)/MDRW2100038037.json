        },
        {
          "id": "MDRW2100038037.1.2",
          "form": "경주 가봤니",
          "original_form": "경주 가봤니",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038037.1.3",
          "form": "신라의 수도 말하니",
          "original_form": "신라의 수도 말하니",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038037.1.4",
          "form": "그래 경주 말이야 황리단 길 가봤어.",
          "original_form": "그래 경주 말이야 황리단 길 가봤어.",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038037.1.5",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038037.1.6",
          "form": "거기 한옥 카페들이 엄청 많더라",
          "original_form": "거기 한옥 카페들이 엄청 많더라",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100038037.1.7",
          "form": "거기에 운석 빵이 있대",
          "original_form": "거기에 운석 빵이 있대",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100038037.1.8",
          "form": "운석 이 떨어졌나",
          "original_form": "운석 이 떨어졌나",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100038037.1.9",
          "form": "어 그래서 생긴 곳이 래",
          "original_form": "어 그래서 생긴 곳이 래",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100038037.1.10",
          "form": "경주에 먹을 거 진짜 많더라",
          "original_form": "경주에 먹을 거 진짜 많더라",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100038037.1.11",
          "form": "맞아 나 불고기 먹었는데 진짜 맛있었어",
          "original_form": "맞아 나 불고기 먹었는데 진짜 맛있었어",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038037.1.12",
          "form": "경주는 특색이 있어서 좋더라",
          "original_form": "경주는 특색이 있어서 좋더라",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038037.1.13",
          "form": "맞아 한옥으로 된 곳이 진짜 많아",
          "original_form": "맞아 한옥으로 된 곳이 진짜 많아",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038037.1.14",
          "form": "그리고 유적지도 많잖아",
          "original_form": "그리고 유적지도 많잖아",
          "speaker_id": "2",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038037.1.15",
          "form": "안압지가봤니?",
          "original_form": "안압지가봤니?",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100038037.1.16",
          "form": "응응 ㅇㅑ간개장 가봤어",
          "original_form": "응응 ㅇㅑ간개장 가봤어",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100038037.1.17",
          "form": "호수에 불켜진거 반사되는게 진짜 멋있더라",
          "original_form": "호수에 불켜진거 반사되는게 진짜 멋있더라",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100038037.1.18",
          "form": "나도 거기서 사진 많이 찍었어",
          "original_form": "나도 거기서 사진 많이 찍었어",
