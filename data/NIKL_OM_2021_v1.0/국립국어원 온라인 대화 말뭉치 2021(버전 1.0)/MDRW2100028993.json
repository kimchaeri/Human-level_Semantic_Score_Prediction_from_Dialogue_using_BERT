      "utterance": [
        {
          "id": "MDRW2100028993.1.1",
          "form": "근데 외동하들이잖아요ㅎ",
          "original_form": "근데 외동하들이잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028993.1.2",
          "form": "아들ㅎㅎ",
          "original_form": "아들ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028993.1.3",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028993.1.4",
          "form": "외롭다고 하진않아요? 애기가?",
          "original_form": "외롭다고 하진않아요? 애기가?",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028993.1.5",
          "form": "아뇨 ㅎㅎ 전혀요",
          "original_form": "아뇨 ㅎㅎ 전혀요",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028993.1.6",
          "form": "남의 동생은 예뻐하는데",
          "original_form": "남의 동생은 예뻐하는데",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028993.1.7",
          "form": "본인 동생은 싫데요 (엄마 뺏길까봐 ㅎ",
          "original_form": "본인 동생은 싫데요 (엄마 뺏길까봐 ㅎ",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028993.1.8",
          "form": "아아아ㅎㅎ",
          "original_form": "아아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028993.1.9",
          "form": "엄마가 이세상에 전부니깐ㅎㄹ",
          "original_form": "엄마가 이세상에 전부니깐ㅎㄹ",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028993.1.10",
          "form": "저희 애도 동생이야기는 안하더라구오",
          "original_form": "저희 애도 동생이야기는 안하더라구오",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028993.1.11",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028993.1.12",
          "form": "다들 알더라구여 본능적ㅇ으로",
          "original_form": "다들 알더라구여 본능적ㅇ으로",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028993.1.13",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028993.1.14",
          "form": "애들은 질투 많이할꺼에요ㅎ",
          "original_form": "애들은 질투 많이할꺼에요ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028993.1.15",
          "form": "지금도 안놀아주면 아빠한테 삐치고 그러는데ㅎㅎ",
          "original_form": "지금도 안놀아주면 아빠한테 삐치고 그러는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028993.1.16",
          "form": "ㅎㅎㅎ  동샌질투오면 퇴행하기더 해요",
          "original_form": "ㅎㅎㅎ  동샌질투오면 퇴행하기더 해요",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028993.1.17",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028993.1.18",
          "form": "퇴행하기가 애기처럼 행동하기인가요?",
          "original_form": "퇴행하기가 애기처럼 행동하기인가요?",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028993.1.19",
          "form": "오늘 갑자기 애기처럼 옆으로 안아주라하던데",
          "original_form": "오늘 갑자기 애기처럼 옆으로 안아주라하던데",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028993.1.20",
          "form": "네네 맞아요 ㅎㅎ 애기처럼 ㅎ",
          "original_form": "네네 맞아요 ㅎㅎ 애기처럼 ㅎ",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028993.1.21",
          "form": "스트레스 받느면 나오는 현상",
          "original_form": "스트레스 받느면 나오는 현상",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028993.1.22",
          "form": "아아ㅎ",
          "original_form": "아아ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028993.1.23",
          "form": "스트레스 받구나 우리 아이가ㅠ왠지슬프네요ㅠ",
          "original_form": "스트레스 받구나 우리 아이가ㅠ왠지슬프네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028993.1.24",
          "form": "ㅎㅎ 근데 삐치는 이유가 많으니 대홀를 해야죠 ㅎㅎ",
          "original_form": "ㅎㅎ 근데 삐치는 이유가 많으니 대홀를 해야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028993.1.25",
          "form": "대화는 자주하는데ㅎ",
          "original_form": "대화는 자주하는데ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100028993.1.26",
          "form": "너무자주해서 탈이라ㅎㅎㅎ",
          "original_form": "너무자주해서 탈이라ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100028993.1.27",
          "form": "ㅎㅎㅎ 와 오타 친거 ㅋ 잘알아보네요 ㅋ",
          "original_form": "ㅎㅎㅎ 와 오타 친거 ㅋ 잘알아보네요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100028993.1.28",
          "form": "제가맨날 오타쳐서ㅎㅎ 잘알죠ㄹㅎ",
          "original_form": "제가맨날 오타쳐서ㅎㅎ 잘알죠ㄹㅎ",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028993.1.29",
          "form": "우리애도 엄청난 수다쟁이에요ㅎㅎ",
          "original_form": "우리애도 엄청난 수다쟁이에요ㅎㅎ",
