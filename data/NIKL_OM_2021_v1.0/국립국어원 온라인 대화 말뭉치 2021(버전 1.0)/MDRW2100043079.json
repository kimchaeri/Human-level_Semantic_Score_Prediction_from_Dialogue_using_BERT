      "utterance": [
        {
          "id": "MDRW2100043079.1.1",
          "form": "요즘 게임 머함",
          "original_form": "요즘 게임 머함",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.2",
          "form": "쿠킹덤",
          "original_form": "쿠킹덤",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.3",
          "form": "재미써?",
          "original_form": "재미써?",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.4",
          "form": "마이 원앤온리게임",
          "original_form": "마이 원앤온리게임",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.5",
          "form": "습관임",
          "original_form": "습관임",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.6",
          "form": "닌텐도는 안함?",
          "original_form": "닌텐도는 안함?",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.7",
          "form": "있어야하지",
          "original_form": "있어야하지",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.8",
          "form": "잼는게임 뭐 없나",
          "original_form": "잼는게임 뭐 없나",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.9",
          "form": "울 오빠네 회사 게임 곧 나올것같던데",
          "original_form": "울 오빠네 회사 게임 곧 나올것같던데",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.10",
          "form": "나오면 하실?",
          "original_form": "나오면 하실?",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.11",
          "form": "오 먼데",
          "original_form": "오 먼데",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.12",
          "form": "재밌어?",
          "original_form": "재밌어?",
          "speaker_id": "2",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.13",
          "form": "나도몰라",
          "original_form": "나도몰라",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.14",
          "form": "뭐라그랬는데",
          "original_form": "뭐라그랬는데",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100043079.1.15",
          "form": "아직 개발중",
          "original_form": "아직 개발중",
          "speaker_id": "1",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043079.1.16",
          "form": "알피지임?",
          "original_form": "알피지임?",
          "speaker_id": "2",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043079.1.17",
          "form": "응 폰게임",
          "original_form": "응 폰게임",
          "speaker_id": "1",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043079.1.18",
          "form": "잼있음ㄹ할래",
          "original_form": "잼있음ㄹ할래",
          "speaker_id": "2",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043079.1.19",
          "form": "그랑사가 만든곳인가??",
          "original_form": "그랑사가 만든곳인가??",
          "speaker_id": "2",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043079.1.20",
          "form": "어케알았지",
          "original_form": "어케알았지",
          "speaker_id": "1",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043079.1.21",
          "form": "내가말했었니",
          "original_form": "내가말했었니",
          "speaker_id": "1",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043079.1.22",
          "form": "저번에 니가 말했는데",
          "original_form": "저번에 니가 말했는데",
          "speaker_id": "2",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043079.1.23",
          "form": "그랑사가는 시스템이 넘 복잡했어",
          "original_form": "그랑사가는 시스템이 넘 복잡했어",
          "speaker_id": "1",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043079.1.24",
          "form": "같이하자매",
          "original_form": "같이하자매",
          "speaker_id": "2",
          "time": "20210907 14:02"
        }
      ]
    }
