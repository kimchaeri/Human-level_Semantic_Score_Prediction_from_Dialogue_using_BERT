      "utterance": [
        {
          "id": "MDRW2100039198.1.1",
          "form": "점심은 드셨나요?",
          "original_form": "점심은 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 13:31"
        },
        {
          "id": "MDRW2100039198.1.2",
          "form": "네 점심먹고 배부르네요",
          "original_form": "네 점심먹고 배부르네요",
          "speaker_id": "2",
          "time": "20210903 13:46"
        },
        {
          "id": "MDRW2100039198.1.3",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210903 13:46"
        },
        {
          "id": "MDRW2100039198.1.4",
          "form": "돼지양념갈비를 구워 먹었답니다",
          "original_form": "돼지양념갈비를 구워 먹었답니다",
          "speaker_id": "1",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039198.1.5",
          "form": "와 맛있겠어요",
          "original_form": "와 맛있겠어요",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039198.1.6",
          "form": "네.. 지인이 10키로를 보내줘서 간단히 굽기만 하면 됐답니다",
          "original_form": "네.. 지인이 10키로를 보내줘서 간단히 굽기만 하면 됐답니다",
          "speaker_id": "1",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039198.1.7",
          "form": "커피 좋아하세요?^^",
          "original_form": "커피 좋아하세요?^^",
          "speaker_id": "1",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100039198.1.8",
          "form": "아메리카노 매니아에요ㅎ",
          "original_form": "아메리카노 매니아에요ㅎ",
          "speaker_id": "2",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100039198.1.9",
          "form": "카페가면 아메 시키시는군요",
          "original_form": "카페가면 아메 시키시는군요",
          "speaker_id": "1",
          "time": "20210906 06:53"
        },
        {
          "id": "MDRW2100039198.1.10",
          "form": "혹시 얼죽아?",
          "original_form": "혹시 얼죽아?",
          "speaker_id": "1",
          "time": "20210906 06:53"
        },
        {
          "id": "MDRW2100039198.1.11",
          "form": "앗 맞아요",
          "original_form": "앗 맞아요",
          "speaker_id": "2",
          "time": "20210906 10:57"
        },
        {
          "id": "MDRW2100039198.1.12",
          "form": "얼죽아ㅋ",
          "original_form": "얼죽아ㅋ",
          "speaker_id": "2",
          "time": "20210906 10:57"
        },
        {
          "id": "MDRW2100039198.1.13",
          "form": "제가 여기서 배웠어요 얼죽아",
          "original_form": "제가 여기서 배웠어요 얼죽아",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100039198.1.14",
          "form": "저는 커피 향이 좋아서 마시고 싶은데 마시면 잠을 못자요",
          "original_form": "저는 커피 향이 좋아서 마시고 싶은데 마시면 잠을 못자요",
          "speaker_id": "1",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100039198.1.15",
          "form": "전 그냥 뻗어요ㅎ",
          "original_form": "전 그냥 뻗어요ㅎ",
          "speaker_id": "2",
          "time": "20210906 14:43"
        },
        {
          "id": "MDRW2100039198.1.16",
          "form": "카페인에 강한 체질이군요",
          "original_form": "카페인에 강한 체질이군요",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100039198.1.17",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100039198.1.18",
          "form": "앗 그런가요ㅎ",
          "original_form": "앗 그런가요ㅎ",
          "speaker_id": "2",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100039198.1.19",
          "form": "저는 마시고 싶어도 잠을 못자니 향만 맡습니다",
