        {
          "id": "MDRW2100008650.1.1",
          "form": "반려동물 기르시나요?",
          "original_form": "반려동물 기르시나요?",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008650.1.2",
          "form": "고양이 기르고있어요",
          "original_form": "고양이 기르고있어요",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008650.1.3",
          "form": "고양이 좋아하시나요?",
          "original_form": "고양이 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008650.1.4",
          "form": "헉 ㅠㅠ",
          "original_form": "헉 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008650.1.5",
          "form": "아 안좋아하시나보다",
          "original_form": "아 안좋아하시나보다",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008650.1.6",
          "form": "귀여울 것 같아요",
          "original_form": "귀여울 것 같아요",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008650.1.7",
          "form": "죄송해요",
          "original_form": "죄송해요",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008650.1.8",
          "form": "아니오 좋아해서 ㅠㅠ",
          "original_form": "아니오 좋아해서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008650.1.9",
          "form": "앗 아니구나",
          "original_form": "앗 아니구나",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008650.1.10",
          "form": "발바닥 너무 좋아요",
          "original_form": "발바닥 너무 좋아요",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008650.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008650.1.12",
          "form": "말랑말랑 너무 귀엽죠",
          "original_form": "말랑말랑 너무 귀엽죠",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008650.1.13",
          "form": "맞아여!!!",
          "original_form": "맞아여!!!",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008650.1.14",
          "form": "진짜 깨물고싶다는 표현이 딱이에요",
          "original_form": "진짜 깨물고싶다는 표현이 딱이에요",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008650.1.15",
          "form": "그리고 혓바닥이 까끌한 것도 신기해요",
          "original_form": "그리고 혓바닥이 까끌한 것도 신기해요",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008650.1.16",
          "form": "혹시 경험해보셨나요",
          "original_form": "혹시 경험해보셨나요",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008650.1.17",
          "form": "예전 학원 선생님이 키우셔서",
          "original_form": "예전 학원 선생님이 키우셔서",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008650.1.18",
          "form": "길냥이 키우셨는데 정말 귀엽더라고요",
          "original_form": "길냥이 키우셨는데 정말 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008650.1.19",
          "form": "그 고양이는 무슨색을 지니고있던가요",
          "original_form": "그 고양이는 무슨색을 지니고있던가요",
          "speaker_id": "1",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008650.1.20",
          "form": "코숏이었던 것 같아요",
          "original_form": "코숏이었던 것 같아요",
          "speaker_id": "2",
          "time": "20210822 17:12"
