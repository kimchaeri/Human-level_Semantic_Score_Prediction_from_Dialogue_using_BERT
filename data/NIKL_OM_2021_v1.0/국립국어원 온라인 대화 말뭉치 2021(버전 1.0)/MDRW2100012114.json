      "utterance": [
        {
          "id": "MDRW2100012114.1.1",
          "form": "신유빈 탁구하는거 봤어?",
          "original_form": "신유빈 탁구하는거 봤어?",
          "speaker_id": "1",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100012114.1.2",
          "form": "어 봤어",
          "original_form": "어 봤어",
          "speaker_id": "2",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100012114.1.3",
          "form": "너무귀엽드라",
          "original_form": "너무귀엽드라",
          "speaker_id": "2",
          "time": "20210824 19:19"
        },
        {
          "id": "MDRW2100012114.1.4",
          "form": "탁구하는것도 귀엽지",
          "original_form": "탁구하는것도 귀엽지",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012114.1.5",
          "form": "키도 크고",
          "original_form": "키도 크고",
          "speaker_id": "1",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100012114.1.6",
          "form": "어 치는 포즈도 너무귀엽",
          "original_form": "어 치는 포즈도 너무귀엽",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012114.1.7",
          "form": "탁구도 잘하고 부럽드라",
          "original_form": "탁구도 잘하고 부럽드라",
          "speaker_id": "2",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012114.1.8",
          "form": "17살인데 돈 벌잖아",
          "original_form": "17살인데 돈 벌잖아",
          "speaker_id": "1",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012114.1.9",
          "form": "탁구실업팀 간듯",
          "original_form": "탁구실업팀 간듯",
          "speaker_id": "1",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100012114.1.10",
          "form": "고등학교 안가고",
          "original_form": "고등학교 안가고",
          "speaker_id": "1",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100012114.1.11",
          "form": "아 맞나",
          "original_form": "아 맞나",
          "speaker_id": "2",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100012114.1.12",
          "form": "괜찮네 탁구잘하니까",
          "original_form": "괜찮네 탁구잘하니까",
          "speaker_id": "2",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100012114.1.13",
          "form": "탁구로 국대까지하고 성공했지 메달은 못 땄어도",
          "original_form": "탁구로 국대까지하고 성공했지 메달은 못 땄어도",
          "speaker_id": "1",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100012114.1.14",
          "form": "어 광고도 찍겠네 탁구잘하고 외모되고",
          "original_form": "어 광고도 찍겠네 탁구잘하고 외모되고",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012114.1.15",
          "form": "탁구실력도 좋으니깐",
          "original_form": "탁구실력도 좋으니깐",
          "speaker_id": "1",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012114.1.16",
          "form": "탁구 경기 재밌더나?",
          "original_form": "탁구 경기 재밌더나?",
          "speaker_id": "1",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012114.1.17",
          "form": "어릴때도 잘했더라 탁구",
          "original_form": "어릴때도 잘했더라 탁구",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012114.1.18",
          "form": "무한도전에 나왔어 신동으로",
          "original_form": "무한도전에 나왔어 신동으로",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100012114.1.19",
          "form": "나는 탁구 재미없더라ㅜ지루함",
          "original_form": "나는 탁구 재미없더라ㅜ지루함",
          "speaker_id": "1",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100012114.1.20",
          "form": "이번올림픽 잘친거같더라",
          "original_form": "이번올림픽 잘친거같더라",
          "speaker_id": "2",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100012114.1.21",
          "form": "경기가 잼있었다고 하드라",
          "original_form": "경기가 잼있었다고 하드라",
          "speaker_id": "2",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100012114.1.22",
          "form": "맞나 난 지루하더라",
          "original_form": "맞나 난 지루하더라",
          "speaker_id": "1",
          "time": "20210824 20:19"
        }
      ]
