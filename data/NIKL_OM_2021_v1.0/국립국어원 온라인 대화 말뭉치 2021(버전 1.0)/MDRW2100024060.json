      "utterance": [
        {
          "id": "MDRW2100024060.1.1",
          "form": "최근 쇼핑뭐하셨어요?",
          "original_form": "최근 쇼핑뭐하셨어요?",
          "speaker_id": "1",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024060.1.2",
          "form": "전..김사서 밥먹었는데요",
          "original_form": "전..김사서 밥먹었는데요",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024060.1.3",
          "form": "name1님은 우아하게 하이오 커피사셨죠",
          "original_form": "&name1&님은 우아하게 하이오 커피사셨죠",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024060.1.4",
          "form": "name1님은 그거말고 머사셨나요",
          "original_form": "&name1&님은 그거말고 머사셨나요",
          "speaker_id": "2",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100024060.1.5",
          "form": "쿠팡에서 볼펜샀어요",
          "original_form": "쿠팡에서 볼펜샀어요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024060.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024060.1.7",
          "form": "볼펜 어떤거에요",
          "original_form": "볼펜 어떤거에요",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024060.1.8",
          "form": "ㅋㅋ지워지는 볼펜 5가지 색으로ㅋㅋ",
          "original_form": "ㅋㅋ지워지는 볼펜 5가지 색으로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024060.1.9",
          "form": "제가 쓰고 있는게 넘 비싸서 좀 저렴한걸로 시켰는데",
          "original_form": "제가 쓰고 있는게 넘 비싸서 좀 저렴한걸로 시켰는데",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024060.1.10",
          "form": "아 그거 다이소에서 봤는데",
          "original_form": "아 그거 다이소에서 봤는데",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024060.1.11",
          "form": "내일 배송올거에요ㅋ",
          "original_form": "내일 배송올거에요ㅋ",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024060.1.12",
          "form": "요새 핫하네요 지위지는거",
          "original_form": "요새 핫하네요 지위지는거",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024060.1.13",
          "form": "음....?\r\n요새아니구요..",
          "original_form": "음....?\r\n요새아니구요..",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024060.1.14",
          "form": "한 5년전에 나온거 같네요..",
          "original_form": "한 5년전에 나온거 같네요..",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024060.1.15",
          "form": "혹시 연세가?",
          "original_form": "혹시 연세가?",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024060.1.16",
          "form": "ㅜㅜ크흑흑",
          "original_form": "ㅜㅜ크흑흑",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024060.1.17",
          "form": "내가..구세대라니",
          "original_form": "내가..구세대라니",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024060.1.18",
          "form": "연세가?",
          "original_form": "연세가?",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024060.1.19",
          "form": "실례일까요?ㅋㅋㅋ",
          "original_form": "실례일까요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024060.1.20",
          "form": "전 3세 꽃다운나이 입니다",
          "original_form": "전 3세 꽃다운나이 입니다",
          "speaker_id": "2",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100024060.1.21",
          "form": "아..",
          "original_form": "아..",
