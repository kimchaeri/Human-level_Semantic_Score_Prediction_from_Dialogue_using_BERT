      "utterance": [
        {
          "id": "MDRW2100005308.1.1",
          "form": "사실 구직활동이 이젠 겁나네요",
          "original_form": "사실 구직활동이 이젠 겁나네요",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005308.1.2",
          "form": "넘 오래 쉬었어요",
          "original_form": "넘 오래 쉬었어요",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005308.1.3",
          "form": "쉬지 않고 다니셨나요?",
          "original_form": "쉬지 않고 다니셨나요?",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005308.1.4",
          "form": "네 저는 아직은 쉬지않고 다니는 중이예요",
          "original_form": "네 저는 아직은 쉬지않고 다니는 중이예요",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005308.1.5",
          "form": "오 관두지 않고 다니는게 좋은거 같아요 경력단절되면 정말 재취업이 힘들어요",
          "original_form": "오 관두지 않고 다니는게 좋은거 같아요 경력단절되면 정말 재취업이 힘들어요",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005308.1.6",
          "form": "오래되면 망설이고요",
          "original_form": "오래되면 망설이고요",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005308.1.7",
          "form": "요즘은 좀 쉬면서 충전하고 싶다는 생각도 종종 하는데",
          "original_form": "요즘은 좀 쉬면서 충전하고 싶다는 생각도 종종 하는데",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005308.1.8",
          "form": "코로나에 취업이 함들어서",
          "original_form": "코로나에 취업이 함들어서",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005308.1.9",
          "form": "직장 동료들과 정도 들었고",
          "original_form": "직장 동료들과 정도 들었고",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005308.1.10",
          "form": "힘들어서 고민되시죠",
          "original_form": "힘들어서 고민되시죠",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005308.1.11",
          "form": "아그렇죠",
          "original_form": "아그렇죠",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005308.1.12",
          "form": "취업이 쉬운 것도 아니니",
          "original_form": "취업이 쉬운 것도 아니니",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005308.1.13",
          "form": "좋은 동료 만나셨나봐요",
          "original_form": "좋은 동료 만나셨나봐요",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005308.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005308.1.15",
          "form": "전 잠깐 간 알바 팀장이 완전 돌은 분이셨어요 ㅜㅜ",
          "original_form": "전 잠깐 간 알바 팀장이 완전 돌은 분이셨어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005308.1.16",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005308.1.17",
          "form": "나오는날 뭐라하고 나왔어요",
          "original_form": "나오는날 뭐라하고 나왔어요",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005308.1.18",
          "form": "힘든 일을 겪으셨군요",
          "original_form": "힘든 일을 겪으셨군요",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005308.1.19",
          "form": "다쳐서 입원했는데 언제 나오냐  화만 내고",
          "original_form": "다쳐서 입원했는데 언제 나오냐  화만 내고",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005308.1.20",
          "form": "아프냐 물어보질않고",
          "original_form": "아프냐 물어보질않고",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005308.1.21",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005308.1.22",
          "form": "인성이 엉망 이였어요",
          "original_form": "인성이 엉망 이였어요",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005308.1.23",
          "form": "너무 하네요",
          "original_form": "너무 하네요",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005308.1.24",
          "form": "건강이 먼저지",
          "original_form": "건강이 먼저지",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005308.1.25",
          "form": "돈버는개 참 어려운거 같아요",
          "original_form": "돈버는개 참 어려운거 같아요",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005308.1.26",
          "form": "정말 잘하고 계신거예요",
          "original_form": "정말 잘하고 계신거예요",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005308.1.27",
          "form": "일은 안 힘드세요?",
          "original_form": "일은 안 힘드세요?",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005308.1.28",
          "form": "힘들기도 한데",
          "original_form": "힘들기도 한데",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005308.1.29",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005308.1.30",
          "form": "어떤 일인들 안힘들겠나 싶어서",
          "original_form": "어떤 일인들 안힘들겠나 싶어서",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005308.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005308.1.32",
          "form": "감사한 마음으로 해야지 싶어요...",
          "original_form": "감사한 마음으로 해야지 싶어요...",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005308.1.33",
          "form": "다 똑같아요",
          "original_form": "다 똑같아요",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005308.1.34",
          "form": "요샌 더그렇죠 ㅎㅎ",
          "original_form": "요샌 더그렇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005308.1.35",
          "form": "근데 또 일하다 힘들면 어휴 다른 일 할걸 하고ㅎㅎ",
          "original_form": "근데 또 일하다 힘들면 어휴 다른 일 할걸 하고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005308.1.36",
          "form": "맞아요 사람맘 다같아요",
          "original_form": "맞아요 사람맘 다같아요",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005308.1.37",
          "form": "직장생활 초반엔 이직생각도 많았는데",
          "original_form": "직장생활 초반엔 이직생각도 많았는데",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005308.1.38",
          "form": "점점 그냥 이게 내 일이려니 싶네요",
          "original_form": "점점 그냥 이게 내 일이려니 싶네요",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005308.1.39",
          "form": "이직안하고 잘버티셨네요",
          "original_form": "이직안하고 잘버티셨네요",
          "speaker_id": "2",
          "time": "20210821 21:52"
        }
      ]
    }
