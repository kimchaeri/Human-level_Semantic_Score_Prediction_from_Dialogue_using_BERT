      "utterance": [
        {
          "id": "MDRW2100047459.1.1",
          "form": "저녁은 새우까스에 타르타르 소스를 얹어서 먹었어요 ㅋㅋㅋ",
          "original_form": "저녁은 새우까스에 타르타르 소스를 얹어서 먹었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100047459.1.2",
          "form": "근데 벌써 배고프네요ㅠㅠㅠ 배달음식도 맨날 카츠류 먹는데 제일 만만하네요",
          "original_form": "근데 벌써 배고프네요ㅠㅠㅠ 배달음식도 맨날 카츠류 먹는데 제일 만만하네요",
          "speaker_id": "2",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100047459.1.3",
          "form": "와우 새우까스좋네요ㅋㅋㅋ",
          "original_form": "와우 새우까스좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:00"
        },
        {
          "id": "MDRW2100047459.1.4",
          "form": "쫄면과곁들여도 꿀맛이죠",
          "original_form": "쫄면과곁들여도 꿀맛이죠",
          "speaker_id": "1",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100047459.1.5",
          "form": "타르타르 소스에 너무 잘어울려서...햄볶",
          "original_form": "타르타르 소스에 너무 잘어울려서...햄볶",
          "speaker_id": "2",
          "time": "20210827 01:01"
        },
        {
          "id": "MDRW2100047459.1.6",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100047459.1.7",
          "form": "전 저녁을",
          "original_form": "전 저녁을",
          "speaker_id": "1",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100047459.1.8",
          "form": "햄버거먹었어요",
          "original_form": "햄버거먹었어요",
          "speaker_id": "1",
          "time": "20210827 01:04"
        },
        {
          "id": "MDRW2100047459.1.9",
          "form": "어디껄까요?",
          "original_form": "어디껄까요?",
          "speaker_id": "1",
          "time": "20210827 01:04"
        },
        {
