      "utterance": [
        {
          "id": "MDRW2100034631.1.1",
          "form": "김민경이랑 오상욱 나옴",
          "original_form": "김민경이랑 오상욱 나옴",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034631.1.2",
          "form": "오상욱이가 누고?",
          "original_form": "오상욱이가 누고?",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034631.1.3",
          "form": "그 펜싱",
          "original_form": "그 펜싱",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034631.1.4",
          "form": "금메달 26살",
          "original_form": "금메달 26살",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034631.1.5",
          "form": "아 그래 인터넷 뉴스에 먹방 찍었다고 되어 있었다",
          "original_form": "아 그래 인터넷 뉴스에 먹방 찍었다고 되어 있었다",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034631.1.6",
          "form": "엉 숙소가 넓던데",
          "original_form": "엉 숙소가 넓던데",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034631.1.7",
          "form": "이제 아무나 다 나오네",
          "original_form": "이제 아무나 다 나오네",
          "speaker_id": "1",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034631.1.8",
          "form": "9월까지는 나오지 싶다 녹화하고 방송하니",
          "original_form": "9월까지는 나오지 싶다 녹화하고 방송하니",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034631.1.9",
          "form": "운동선수는 운동만 했으면 하는데 부르겠디?",
          "original_form": "운동선수는 운동만 했으면 하는데 부르겠디?",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034631.1.10",
          "form": "8월에 올림픽 끝나고 방송녹화한거 9월까지는 가겠지",
          "original_form": "8월에 올림픽 끝나고 방송녹화한거 9월까지는 가겠지",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034631.1.11",
          "form": "너무 여기저기 나오니 별로다 이미지소모다",
          "original_form": "너무 여기저기 나오니 별로다 이미지소모다",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034631.1.12",
          "form": "그래 똑같은 얘기고ㅈ",
          "original_form": "그래 똑같은 얘기고ㅈ",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034631.1.13",
          "form": "엉 그래 하는 얘기가 똑같다",
          "original_form": "엉 그래 하는 얘기가 똑같다",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034631.1.14",
          "form": "얼굴 잘 생기면 더 나오고",
          "original_form": "얼굴 잘 생기면 더 나오고",
          "speaker_id": "1",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034631.1.15",
          "form": "ㅋㅋ 안생겨도 나오더라 메달 따니까",
          "original_form": "ㅋㅋ 안생겨도 나오더라 메달 따니까",
          "speaker_id": "2",
          "time": "20210828 16:45"
        }
