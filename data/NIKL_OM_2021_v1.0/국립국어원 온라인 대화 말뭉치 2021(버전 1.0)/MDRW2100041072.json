      "utterance": [
        {
          "id": "MDRW2100041072.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100041072.1.2",
          "form": "좋아해요ㅜ",
          "original_form": "좋아해요ㅜ",
          "speaker_id": "2",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100041072.1.3",
          "form": "근데 못간지 오래됐어요",
          "original_form": "근데 못간지 오래됐어요",
          "speaker_id": "2",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100041072.1.4",
          "form": "어딜 가장 마지막으로 다녀오셨어요?",
          "original_form": "어딜 가장 마지막으로 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100041072.1.5",
          "form": "제주도요",
          "original_form": "제주도요",
          "speaker_id": "2",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100041072.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100041072.1.7",
          "form": "저도 제주도요",
          "original_form": "저도 제주도요",
          "speaker_id": "1",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100041072.1.8",
          "form": "혼자 다녀오셨나요?",
          "original_form": "혼자 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210905 16:32"
        },
        {
          "id": "MDRW2100041072.1.9",
          "form": "아니요 가족이랑요",
          "original_form": "아니요 가족이랑요",
          "speaker_id": "2",
          "time": "20210905 16:33"
        },
        {
          "id": "MDRW2100041072.1.10",
          "form": "마지막에 언제 가셨어요?",
          "original_form": "마지막에 언제 가셨어요?",
          "speaker_id": "2",
          "time": "20210905 16:33"
        },
        {
          "id": "MDRW2100041072.1.11",
          "form": "전 작년 여름이요",
          "original_form": "전 작년 여름이요",
          "speaker_id": "1",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100041072.1.12",
          "form": "여행은 혼자 가시는 걸 좋아하세요? 다 같이 가시는 걸 좋아하세요?",
          "original_form": "여행은 혼자 가시는 걸 좋아하세요? 다 같이 가시는 걸 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100041072.1.13",
          "form": "저는 혼자보다는 그래도 누구랑 같이가는거요",
          "original_form": "저는 혼자보다는 그래도 누구랑 같이가는거요",
          "speaker_id": "2",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100041072.1.14",
          "form": "혼자가면 쓸쓸해서 ㅎ",
          "original_form": "혼자가면 쓸쓸해서 ㅎ",
          "speaker_id": "2",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100041072.1.15",
          "form": "호캉스는 좋아하시나요?",
          "original_form": "호캉스는 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100041072.1.16",
          "form": "가면 좋을것 같은데",
          "original_form": "가면 좋을것 같은데",
          "speaker_id": "2",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100041072.1.17",
          "form": "가본적은 없어요",
          "original_form": "가본적은 없어요",
          "speaker_id": "2",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100041072.1.18",
          "form": "호캉스 가보셨어요?",
          "original_form": "호캉스 가보셨어요?",
          "speaker_id": "2",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100041072.1.19",
          "form": "네 전 자주 가는 편이에요",
          "original_form": "네 전 자주 가는 편이에요",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100041072.1.20",
          "form": "특히 코로나 이후로는 여행대신 호캉스로",
          "original_form": "특히 코로나 이후로는 여행대신 호캉스로",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100041072.1.21",
          "form": "자주 가요",
          "original_form": "자주 가요",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100041072.1.22",
          "form": "그쵸 어디 다른데 가는것보다 낫겠네요",
          "original_form": "그쵸 어디 다른데 가는것보다 낫겠네요",
          "speaker_id": "2",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100041072.1.23",
          "form": "네네 코로나라 워낙 무서워서 ㅠㅠ",
          "original_form": "네네 코로나라 워낙 무서워서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 16:38"
        }
      ]
