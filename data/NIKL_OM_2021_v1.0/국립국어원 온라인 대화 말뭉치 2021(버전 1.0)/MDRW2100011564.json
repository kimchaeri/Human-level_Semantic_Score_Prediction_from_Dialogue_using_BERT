      "utterance": [
        {
          "id": "MDRW2100011564.1.1",
          "form": "책상안오노ㅋㅋ",
          "original_form": "책상안오노ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:01"
        },
        {
          "id": "MDRW2100011564.1.2",
          "form": "어젠 스탠드샀다",
          "original_form": "어젠 스탠드샀다",
          "speaker_id": "2",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011564.1.3",
          "form": "쌀은 핫딜놓쳐가 마음 쓰리다",
          "original_form": "쌀은 핫딜놓쳐가 마음 쓰리다",
          "speaker_id": "2",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011564.1.4",
          "form": "ㅋㅋㅋ아 언니ㅋㅋ",
          "original_form": "ㅋㅋㅋ아 언니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011564.1.5",
          "form": "쌀은 쿠팡에서 샀어요?",
          "original_form": "쌀은 쿠팡에서 샀어요?",
          "speaker_id": "1",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100011564.1.6",
          "form": "진짜 그타이밍에 딱 오다니 ..쌀이..흠",
          "original_form": "진짜 그타이밍에 딱 오다니 ..쌀이..흠",
          "speaker_id": "1",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100011564.1.7",
          "form": "책상은 샀어요?",
          "original_form": "책상은 샀어요?",
          "speaker_id": "1",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100011564.1.8",
          "form": "샀는데아직안오네",
          "original_form": "샀는데아직안오네",
          "speaker_id": "2",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011564.1.9",
          "form": "쌀은 61000원ㅡㅡ",
          "original_form": "쌀은 61000원ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011564.1.10",
          "form": "에휴 비싸게맛있게 먹어야지",
          "original_form": "에휴 비싸게맛있게 먹어야지",
          "speaker_id": "2",
          "time": "20210824 14:51"
        },
        {
          "id": "MDRW2100011564.1.11",
          "form": "헉 ㅜㅜ61000원이요?!?",
          "original_form": "헉 ㅜㅜ61000원이요?!?",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011564.1.12",
          "form": "웁스....",
          "original_form": "웁스....",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011564.1.13",
          "form": "맛있는거샀네요ㅋㅋ",
          "original_form": "맛있는거샀네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011564.1.14",
          "form": "속쓰르다 ㅋㅋ",
          "original_form": "속쓰르다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100011564.1.15",
          "form": "하루만 늦게살껄ㅡㅡ",
          "original_form": "하루만 늦게살껄ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100011564.1.16",
          "form": "뭐 이래 부지런해서ㅋ",
          "original_form": "뭐 이래 부지런해서ㅋ",
          "speaker_id": "2",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100011564.1.17",
          "form": "그러게요..ㅠㅠ",
          "original_form": "그러게요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 15:28"
        },
        {
          "id": "MDRW2100011564.1.18",
          "form": "아쉽아쉽 ㅜㅠ",
          "original_form": "아쉽아쉽 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210824 15:28"
        },
        {
          "id": "MDRW2100011564.1.19",
          "form": "취소도안되죠?ㅢ",
          "original_form": "취소도안되죠?ㅢ",
          "speaker_id": "1",
          "time": "20210824 15:28"
        },
        {
          "id": "MDRW2100011564.1.20",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "1",
          "time": "20210824 15:28"
        },
        {
          "id": "MDRW2100011564.1.21",
          "form": "왔다",
          "original_form": "왔다",
          "speaker_id": "2",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011564.1.22",
          "form": "로켓으로ㅋㅋㅋ",
          "original_form": "로켓으로ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011564.1.23",
          "form": "지금 현관앞에있음ㅋ",
          "original_form": "지금 현관앞에있음ㅋ",
          "speaker_id": "2",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011564.1.24",
          "form": "ㅋㅋ로켓했군요ㅋ",
          "original_form": "ㅋㅋ로켓했군요ㅋ",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011564.1.25",
          "form": "전 장보고왔어요. 오리고기ㅋㅋ",
          "original_form": "전 장보고왔어요. 오리고기ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011564.1.26",
          "form": "상추랑 ㅋㅋㅋ",
          "original_form": "상추랑 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011564.1.27",
          "form": "오리고기맛있겠다ㅋ",
          "original_form": "오리고기맛있겠다ㅋ",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011564.1.28",
          "form": "네 맛있을것같아서 사왔죠 히히",
          "original_form": "네 맛있을것같아서 사왔죠 히히",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011564.1.29",
          "form": "애들 학원가고 여유롭다ㅋㅋ",
          "original_form": "애들 학원가고 여유롭다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011564.1.30",
          "form": "난 그냥 로켓와우 ㅋㅋ",
          "original_form": "난 그냥 로켓와우 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100011564.1.31",
          "form": "오늘 또 쇼핑함",
          "original_form": "오늘 또 쇼핑함",
          "speaker_id": "2",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100011564.1.32",
          "form": "언니 나도 오늘 쇼핑했어요",
          "original_form": "언니 나도 오늘 쇼핑했어요",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100011564.1.33",
          "form": "쿠팡은 사랑이죠",
          "original_form": "쿠팡은 사랑이죠",
          "speaker_id": "1",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100011564.1.34",
          "form": "그래 쿠팡 많이 애용하자",
          "original_form": "그래 쿠팡 많이 애용하자",
          "speaker_id": "2",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100011564.1.35",
          "form": "핫딜 못 보니 아쉽",
          "original_form": "핫딜 못 보니 아쉽",
          "speaker_id": "2",
          "time": "20210824 22:43"
        }
      ]
