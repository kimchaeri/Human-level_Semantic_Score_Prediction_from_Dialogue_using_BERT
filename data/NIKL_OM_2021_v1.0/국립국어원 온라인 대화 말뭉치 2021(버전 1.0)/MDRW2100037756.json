        {
          "id": "MDRW2100037756.1.1",
          "form": "카시트커버 분리해서 세탁하고ㅋㅋ",
          "original_form": "카시트커버 분리해서 세탁하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100037756.1.2",
          "form": "신랑이불세탁함ㅋ",
          "original_form": "신랑이불세탁함ㅋ",
          "speaker_id": "1",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100037756.1.3",
          "form": "오늘 많은 일을햇구마이 ㅋㅋㅋㅋㅋ",
          "original_form": "오늘 많은 일을햇구마이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100037756.1.4",
          "form": "해놓고나믄 뿌듯한데이 ㅋㅋㅋ",
          "original_form": "해놓고나믄 뿌듯한데이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100037756.1.5",
          "form": "그러니깐 내일 마저 다 세탁해야지ㅋ",
          "original_form": "그러니깐 내일 마저 다 세탁해야지ㅋ",
          "speaker_id": "1",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100037756.1.6",
          "form": "그래도 카시트 깨끗하니 기분이좋다",
          "original_form": "그래도 카시트 깨끗하니 기분이좋다",
          "speaker_id": "1",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100037756.1.7",
          "form": "하고나믄 뿌듯허다 ㅋㅋㅋㅋ하기 까지가 힘들다 ㅋㅋㅋㅋ",
          "original_form": "하고나믄 뿌듯허다 ㅋㅋㅋㅋ하기 까지가 힘들다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037756.1.8",
          "form": "우린 육퇴 ㅋㅋㅋㅋ",
          "original_form": "우린 육퇴 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037756.1.9",
          "form": "맞다 와",
          "original_form": "맞다 와",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037756.1.10",
          "form": "자유를 즐겨본더 ㅋㅋㅋㅋ",
          "original_form": "자유를 즐겨본더 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037756.1.11",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037756.1.12",
          "form": "난 이제 ㅋㅋ",
          "original_form": "난 이제 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037756.1.13",
          "form": "재우려고ㅋㅋ",
          "original_form": "재우려고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037756.1.14",
          "form": "자유아름답네ㅋ",
          "original_form": "자유아름답네ㅋ",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037756.1.15",
          "form": "티비를 좀 봐야긋다 ㅋㅋㅋ",
          "original_form": "티비를 좀 봐야긋다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037756.1.16",
          "form": "전부 지들꺼 틀어달라해가",
          "original_form": "전부 지들꺼 틀어달라해가",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037756.1.17",
          "form": "내가 좋아하는걸 볼 시간이 없다 ㅋㅋㅋ",
          "original_form": "내가 좋아하는걸 볼 시간이 없다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037756.1.18",
          "form": "좋다좋다ㅋㅋ",
          "original_form": "좋다좋다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037756.1.19",
          "form": "오늘 목욜이었으면",
          "original_form": "오늘 목욜이었으면",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037756.1.20",
          "form": "슬기로운의사생활 하는데ㅋ",
          "original_form": "슬기로운의사생활 하는데ㅋ",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037756.1.21",
          "form": "드라마를 못본다 ㅋㅋㅋ",
          "original_form": "드라마를 못본다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037756.1.22",
          "form": "그러니깐은ㅋㅋ",
          "original_form": "그러니깐은ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037756.1.23",
          "form": "못 이어보니 ㅋㅋㅋ",
          "original_form": "못 이어보니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037756.1.24",
          "form": "그건 지안이도같이볼라한다ㅜㅋㅋㄱ",
          "original_form": "그건 지안이도같이볼라한다ㅜㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037756.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞다 맞다~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞다 맞다~",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037756.1.26",
          "form": "내 애들 재우고올게ㅋㅋ",
          "original_form": "내 애들 재우고올게ㅋㅋ",
