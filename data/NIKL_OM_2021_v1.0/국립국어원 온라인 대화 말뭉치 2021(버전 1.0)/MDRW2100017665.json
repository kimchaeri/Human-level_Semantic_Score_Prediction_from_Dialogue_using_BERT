      "utterance": [
        {
          "id": "MDRW2100017665.1.1",
          "form": "최근에 위시템이 있으실까요?",
          "original_form": "최근에 위시템이 있으실까요?",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100017665.1.2",
          "form": "위시템.. 위시템이라..",
          "original_form": "위시템.. 위시템이라..",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100017665.1.3",
          "form": "에어팟..?",
          "original_form": "에어팟..?",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100017665.1.4",
          "form": "강아지가 저번에 십어먹어서",
          "original_form": "강아지가 저번에 십어먹어서",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100017665.1.5",
          "form": "하나 사야해요",
          "original_form": "하나 사야해요",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100017665.1.6",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100017665.1.7",
          "form": "어머세상에나..ㅜㅜ",
          "original_form": "어머세상에나..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100017665.1.8",
          "form": "저는 삼성제트플립이요!!",
          "original_form": "저는 삼성제트플립이요!!",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100017665.1.9",
          "form": "플립 이번에 너무  잘빠졌더라구요",
          "original_form": "플립 이번에 너무  잘빠졌더라구요",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100017665.1.10",
          "form": "색상이 진짜.. 잘뽑혔어요",
          "original_form": "색상이 진짜.. 잘뽑혔어요",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100017665.1.11",
          "form": "맞아요... 특히 전 라벤더 ㅜ",
          "original_form": "맞아요... 특히 전 라벤더 ㅜ",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100017665.1.12",
          "form": "그리고 콜라보도",
          "original_form": "그리고 콜라보도",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100017665.1.13",
          "form": "되게 많이 해서 케이스 고르는 재미도 있겠더라구요",
          "original_form": "되게 많이 해서 케이스 고르는 재미도 있겠더라구요",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100017665.1.14",
          "form": "30가지인가 됐던거같은데",
          "original_form": "30가지인가 됐던거같은데",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100017665.1.15",
          "form": "맞아요~ 완전 디자인팀에서 칼갈고나왓던데요?ㅋㅋ",
          "original_form": "맞아요~ 완전 디자인팀에서 칼갈고나왓던데요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100017665.1.16",
          "form": "핸드폰 어느브랜드 사용중이세요",
          "original_form": "핸드폰 어느브랜드 사용중이세요",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100017665.1.17",
          "form": "아 이거 하고있는거보면 삼성이겠구나",
          "original_form": "아 이거 하고있는거보면 삼성이겠구나",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100017665.1.18",
          "form": "바보같은 질문이었네요",
          "original_form": "바보같은 질문이었네요",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100017665.1.19",
          "form": "아니에요 ㅋㅋ 너무나도 좋은질문!",
          "original_form": "아니에요 ㅋㅋ 너무나도 좋은질문!",
          "speaker_id": "1",
          "time": "20210827 12:35"
        },
        {
          "id": "MDRW2100017665.1.20",
          "form": "놀랍게도 전 지금 엘지폰유저입니다~~",
          "original_form": "놀랍게도 전 지금 엘지폰유저입니다~~",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100017665.1.21",
          "form": "엇.. 엘지 사업 접으셔서",
          "original_form": "엇.. 엘지 사업 접으셔서",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100017665.1.22",
          "form": "속상하시겠어요..",
          "original_form": "속상하시겠어요..",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100017665.1.23",
          "form": "엘지 쓰시는 분은 엘지만 쓰시던데",
          "original_form": "엘지 쓰시는 분은 엘지만 쓰시던데",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100017665.1.24",
          "form": "맞아요 ㅠㅠ 속상한사람 저에요 .. 강경엘지폰유저엿는데 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ 속상한사람 저에요 .. 강경엘지폰유저엿는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100017665.1.25",
          "form": "하루아침에 브랜드가 사라지다니..ㅠ",
          "original_form": "하루아침에 브랜드가 사라지다니..ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100017665.1.26",
          "form": "익숙함이란게 있는건데",
          "original_form": "익숙함이란게 있는건데",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100017665.1.27",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100017665.1.28",
          "form": "맞죠맞죠 근데 어쩔수없죠뭐",
          "original_form": "맞죠맞죠 근데 어쩔수없죠뭐",
          "speaker_id": "1",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100017665.1.29",
          "form": "그래서 안드에 익숙하니 다음폰은 갤럭시생각중이에요",
          "original_form": "그래서 안드에 익숙하니 다음폰은 갤럭시생각중이에요",
