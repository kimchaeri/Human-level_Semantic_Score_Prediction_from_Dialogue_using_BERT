      "utterance": [
        {
          "id": "MDRW2100003349.1.1",
          "form": "오늘 도서관도 갔니",
          "original_form": "오늘 도서관도 갔니",
          "speaker_id": "1",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003349.1.2",
          "form": "응 가서 반납만 하고 옴ㅋ",
          "original_form": "응 가서 반납만 하고 옴ㅋ",
          "speaker_id": "2",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003349.1.3",
          "form": "왜 안빌렸어 ㅋㅋ",
          "original_form": "왜 안빌렸어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003349.1.4",
          "form": "내가 빌리고 싶은거 누가 반납 안함ㅋㅋㄱ",
          "original_form": "내가 빌리고 싶은거 누가 반납 안함ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210820 22:26"
        },
        {
          "id": "MDRW2100003349.1.5",
          "form": "담주는 나도 도서관이나 가볼까",
          "original_form": "담주는 나도 도서관이나 가볼까",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003349.1.6",
          "form": "아 ㅋㅋ 딱 보고 싶은게 있었구나",
          "original_form": "아 ㅋㅋ 딱 보고 싶은게 있었구나",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003349.1.7",
          "form": "도서관에 빌릴 책 있니",
          "original_form": "도서관에 빌릴 책 있니",
          "speaker_id": "2",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003349.1.8",
          "form": "가면 책이야 가득하니 ㅋㅋ",
          "original_form": "가면 책이야 가득하니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003349.1.9",
          "form": "이슬아 작가 유명하던데 근래에 .. 빌려볼까",
          "original_form": "이슬아 작가 유명하던데 근래에 .. 빌려볼까",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003349.1.10",
          "form": "이슬아? 첨 들어봄",
          "original_form": "이슬아? 첨 들어봄",
          "speaker_id": "2",
          "time": "20210820 22:41"
        },
        {
          "id": "MDRW2100003349.1.11",
          "form": "소설 작가임?",
          "original_form": "소설 작가임?",
          "speaker_id": "2",
          "time": "20210820 22:41"
        },
        {
          "id": "MDRW2100003349.1.12",
          "form": "에세이 산문집으로 유명",
          "original_form": "에세이 산문집으로 유명",
          "speaker_id": "1",
          "time": "20210820 22:42"
        },
        {
          "id": "MDRW2100003349.1.13",
          "form": "아 글쿠나 나도 한번 봐야겠넴ㅋㅋ",
          "original_form": "아 글쿠나 나도 한번 봐야겠넴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:46"
        },
        {
          "id": "MDRW2100003349.1.14",
          "form": "밀리의 서재에서 달러구트 꿈 백화점 읽은게 젤 최근 독서인데 나름 독특하고 잼 있더라",
          "original_form": "밀리의 서재에서 달러구트 꿈 백화점 읽은게 젤 최근 독서인데 나름 독특하고 잼 있더라",
          "speaker_id": "1",
          "time": "20210820 22:48"
        },
        {
          "id": "MDRW2100003349.1.15",
          "form": "밀리의 서재 가입했니?",
          "original_form": "밀리의 서재 가입했니?",
          "speaker_id": "2",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003349.1.16",
          "form": "전에 핸드폰사고 3개월권 받은거 ㅋㅋㅋ",
          "original_form": "전에 핸드폰사고 3개월권 받은거 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003349.1.17",
          "form": "아 그게 3개월이나 되는구나",
          "original_form": "아 그게 3개월이나 되는구나",
          "speaker_id": "2",
          "time": "20210820 23:15"
        },
        {
          "id": "MDRW2100003349.1.18",
          "form": "근데 그거 한권 읽었다는 거 ㅋㅋㅋ",
          "original_form": "근데 그거 한권 읽었다는 거 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:15"
        },
        {
          "id": "MDRW2100003349.1.19",
          "form": "음ㅋㅋㅋㅋㅋ",
          "original_form": "음ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:15"
        },
        {
          "id": "MDRW2100003349.1.20",
          "form": "눈이 금방 피로해지고 이북 잘 안봐지더라고",
          "original_form": "눈이 금방 피로해지고 이북 잘 안봐지더라고",
          "speaker_id": "1",
          "time": "20210820 23:16"
        },
        {
          "id": "MDRW2100003349.1.21",
          "form": "또 보고싶은건 없고",
          "original_form": "또 보고싶은건 없고",
          "speaker_id": "1",
          "time": "20210820 23:16"
        },
        {
          "id": "MDRW2100003349.1.22",
          "form": "밀리의 서재 책은 많아?",
          "original_form": "밀리의 서재 책은 많아?",
          "speaker_id": "2",
          "time": "20210820 23:17"
        },
        {
          "id": "MDRW2100003349.1.23",
          "form": "그냥 뭐 유명한 책은 있는듯하지만 은근 또 없는것도 많은 듯 이북이 다 그런듯",
          "original_form": "그냥 뭐 유명한 책은 있는듯하지만 은근 또 없는것도 많은 듯 이북이 다 그런듯",
          "speaker_id": "1",
          "time": "20210820 23:18"
        }
      ]
    }
  ]
