      "utterance": [
        {
          "id": "MDRW2100042010.1.1",
          "form": "비 와요ㅜㅜ",
          "original_form": "비 와요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100042010.1.2",
          "form": "우산 객기로 안가져왔어요ㅜ",
          "original_form": "우산 객기로 안가져왔어요ㅜ",
          "speaker_id": "2",
          "time": "20210906 18:37"
        },
        {
          "id": "MDRW2100042010.1.3",
          "form": "앗 이런ㅠ",
          "original_form": "앗 이런ㅠ",
          "speaker_id": "1",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100042010.1.4",
          "form": "우산 없어도 될 정도의 비인가요?",
          "original_form": "우산 없어도 될 정도의 비인가요?",
          "speaker_id": "1",
          "time": "20210906 18:39"
        },
        {
          "id": "MDRW2100042010.1.5",
          "form": "네 들기도 애매 안들기도 애매ㅋㅋ",
          "original_form": "네 들기도 애매 안들기도 애매ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100042010.1.6",
          "form": "이제 집도착해요",
          "original_form": "이제 집도착해요",
          "speaker_id": "2",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100042010.1.7",
          "form": "아 비 더 내리기 전에 집에 들어가신거죠?",
          "original_form": "아 비 더 내리기 전에 집에 들어가신거죠?",
          "speaker_id": "1",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100042010.1.8",
          "form": "네! 간발의 차였어요!!!",
          "original_form": "네! 간발의 차였어요!!!",
          "speaker_id": "2",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100042010.1.9",
          "form": "거기도 비 많이 와요?",
          "original_form": "거기도 비 많이 와요?",
          "speaker_id": "2",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100042010.1.10",
          "form": "저희 동네는 꽤 많이 와요",
          "original_form": "저희 동네는 꽤 많이 와요",
          "speaker_id": "1",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100042010.1.11",
          "form": "집 안에서도 빗소리 들릴정도",
          "original_form": "집 안에서도 빗소리 들릴정도",
          "speaker_id": "1",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100042010.1.12",
          "form": "여기도요!!",
          "original_form": "여기도요!!",
          "speaker_id": "2",
          "time": "20210906 19:29"
        },
        {
          "id": "MDRW2100042010.1.13",
          "form": "집에 있을때 비오면 기분 좋아요!",
          "original_form": "집에 있을때 비오면 기분 좋아요!",
          "speaker_id": "2",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100042010.1.14",
          "form": "저도 퇴근하고 집에 온 다음에 본격적으로 비가 내려서",
          "original_form": "저도 퇴근하고 집에 온 다음에 본격적으로 비가 내려서",
          "speaker_id": "1",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100042010.1.15",
          "form": "지금은 집에서 빗소리 듣고 있어요",
          "original_form": "지금은 집에서 빗소리 듣고 있어요",
          "speaker_id": "1",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100042010.1.16",
          "form": "name2님도 간발의 차로 큰 비는 피했다니 다행이네요",
          "original_form": "&name2&님도 간발의 차로 큰 비는 피했다니 다행이네요",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042010.1.17",
          "form": "따뜻한 방에서 누리세요!",
          "original_form": "따뜻한 방에서 누리세요!",
          "speaker_id": "2",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042010.1.18",
          "form": "그러니까요 큰일날뻔",
          "original_form": "그러니까요 큰일날뻔",
          "speaker_id": "2",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042010.1.19",
          "form": "낼 출근길엔 안왔으면",
          "original_form": "낼 출근길엔 안왔으면",
          "speaker_id": "2",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042010.1.20",
          "form": "네네 집안에서 듣는 빗소리는 좋죠",
          "original_form": "네네 집안에서 듣는 빗소리는 좋죠",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042010.1.21",
          "form": "저희 동네는 내일 오전까지 온다고는 그러더라고요",
          "original_form": "저희 동네는 내일 오전까지 온다고는 그러더라고요",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042010.1.22",
          "form": "일기예보 보니",
          "original_form": "일기예보 보니",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042010.1.23",
          "form": "부디 틀려서 빨리 그쳤음 좋겠어요!",
          "original_form": "부디 틀려서 빨리 그쳤음 좋겠어요!",
          "speaker_id": "2",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100042010.1.24",
          "form": "네 출근길은 맑길",
          "original_form": "네 출근길은 맑길",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100042010.1.25",
          "form": "비 오는 출근길은 싫어요ㅜ",
          "original_form": "비 오는 출근길은 싫어요ㅜ",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100042010.1.26",
          "form": "ㅋㅋ 맞아요ㅜㅜ 마구 쏟아질때 출근하려고하면 속상해요",
          "original_form": "ㅋㅋ 맞아요ㅜㅜ 마구 쏟아질때 출근하려고하면 속상해요",
          "speaker_id": "2",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100042010.1.27",
          "form": "저는 출퇴근을 대중교통으로 해서",
          "original_form": "저는 출퇴근을 대중교통으로 해서",
          "speaker_id": "1",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042010.1.28",
          "form": "비 오면 정말 축축하고 힘들어요",
          "original_form": "비 오면 정말 축축하고 힘들어요",
          "speaker_id": "1",
          "time": "20210906 19:47"
        }
      ]
    }
  ]
