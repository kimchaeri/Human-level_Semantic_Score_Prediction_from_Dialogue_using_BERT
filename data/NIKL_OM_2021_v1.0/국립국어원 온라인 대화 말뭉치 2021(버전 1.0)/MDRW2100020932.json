      "utterance": [
        {
          "id": "MDRW2100020932.1.1",
          "form": "지금 커피마시고 싶어서 아스크림 먹응",
          "original_form": "지금 커피마시고 싶어서 아스크림 먹응",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020932.1.2",
          "form": "엄청나게",
          "original_form": "엄청나게",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020932.1.3",
          "form": "와 맛있겠다",
          "original_form": "와 맛있겠다",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020932.1.4",
          "form": "커피맛?",
          "original_form": "커피맛?",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020932.1.5",
          "form": "커피 타먹기귀차나서",
          "original_form": "커피 타먹기귀차나서",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020932.1.6",
          "form": "딸기요거트맛",
          "original_form": "딸기요거트맛",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020932.1.7",
          "form": "상큼할거라고",
          "original_form": "상큼할거라고",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020932.1.8",
          "form": "ㅋㅋ뭥?",
          "original_form": "ㅋㅋ뭥?",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020932.1.9",
          "form": "상큼한 하루 보낸다고 딸기요거트맛 아니냐며",
          "original_form": "상큼한 하루 보낸다고 딸기요거트맛 아니냐며",
          "speaker_id": "1",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020932.1.10",
          "form": "커피도 마셔야사는데",
          "original_form": "커피도 마셔야사는데",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020932.1.11",
          "form": "난 마심",
          "original_form": "난 마심",
          "speaker_id": "1",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020932.1.12",
          "form": "카레빵이랑 먹음",
          "original_form": "카레빵이랑 먹음",
          "speaker_id": "1",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020932.1.13",
          "form": "내가 타먹는 커피는 맛엏어서",
          "original_form": "내가 타먹는 커피는 맛엏어서",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020932.1.14",
          "form": "기다리는중",
          "original_form": "기다리는중",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020932.1.15",
          "form": "누가 타주는게 젤 맛있음",
          "original_form": "누가 타주는게 젤 맛있음",
          "speaker_id": "1",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020932.1.16",
          "form": "누구라도 커피를 다오",
          "original_form": "누구라도 커피를 다오",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020932.1.17",
          "form": "내가 커피를 기다리느뇨 ㅋㅋ",
          "original_form": "내가 커피를 기다리느뇨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020932.1.18",
          "form": "오늘은 아이스가 땡기는날",
          "original_form": "오늘은 아이스가 땡기는날",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020932.1.19",
          "form": "난 항상 아아",
          "original_form": "난 항상 아아",
          "speaker_id": "1",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020932.1.20",
          "form": "ㅋㅋㅋ전원래 핫",
          "original_form": "ㅋㅋㅋ전원래 핫",
          "speaker_id": "2",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020932.1.21",
          "form": "난 핫 먹으면 가끔 체해서 아아로 마심",
          "original_form": "난 핫 먹으면 가끔 체해서 아아로 마심",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020932.1.22",
          "form": "근데 점점 아아",
          "original_form": "근데 점점 아아",
          "speaker_id": "2",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020932.1.23",
          "form": "산미있는건 별로",
          "original_form": "산미있는건 별로",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020932.1.24",
          "form": "전 핫이 소화되더라구여ㅋㅋ",
          "original_form": "전 핫이 소화되더라구여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020932.1.25",
          "form": "ㅋㅋㅋㅋㅋ 반대의 입맛",
          "original_form": "ㅋㅋㅋㅋㅋ 반대의 입맛",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020932.1.26",
          "form": "저도 그냥 부드러운걸로ㅋㅋ 하지만 커알못",
          "original_form": "저도 그냥 부드러운걸로ㅋㅋ 하지만 커알못",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020932.1.27",
          "form": "근데 핫이 좋지",
          "original_form": "근데 핫이 좋지",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020932.1.28",
          "form": "ㅋㅋ지금은 아직 넘 더움",
          "original_form": "ㅋㅋ지금은 아직 넘 더움",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020932.1.29",
          "form": "남편이 오븐으로 뭘 만드는걸까",
          "original_form": "남편이 오븐으로 뭘 만드는걸까",
          "speaker_id": "1",
          "time": "20210829 16:37"
        }
      ]
    }
