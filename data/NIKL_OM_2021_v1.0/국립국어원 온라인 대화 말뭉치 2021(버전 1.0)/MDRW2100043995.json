      "utterance": [
        {
          "id": "MDRW2100043995.1.1",
          "form": "애초에 멸치육수에 간이 되어서",
          "original_form": "애초에 멸치육수에 간이 되어서",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043995.1.2",
          "form": "파스타는 어떤종류가 좋아",
          "original_form": "파스타는 어떤종류가 좋아",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043995.1.3",
          "form": "아무것도 안 넣는 걸 제일 좋아해",
          "original_form": "아무것도 안 넣는 걸 제일 좋아해",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043995.1.4",
          "form": "파스타는 토마토마토마토마토",
          "original_form": "파스타는 토마토마토마토마토",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043995.1.5",
          "form": "신기하다 난 잔치국수 싱거워서 아쉽던데",
          "original_form": "신기하다 난 잔치국수 싱거워서 아쉽던데",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043995.1.6",
          "form": "토마토에 어떤거 넣니",
          "original_form": "토마토에 어떤거 넣니",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043995.1.7",
          "form": "넌 국수에 김치 필수야?",
          "original_form": "넌 국수에 김치 필수야?",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043995.1.8",
          "form": "있으면 좋은것같아",
          "original_form": "있으면 좋은것같아",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043995.1.9",
          "form": "간장양념두?",
          "original_form": "간장양념두?",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043995.1.10",
          "form": "응 넣는편이야",
          "original_form": "응 넣는편이야",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043995.1.11",
          "form": "난 파스타는 토마토에 닭고기나 새우 넣는 거 좋아해",
          "original_form": "난 파스타는 토마토에 닭고기나 새우 넣는 거 좋아해",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043995.1.12",
          "form": "조개류는 먹어본 적 없고",
          "original_form": "조개류는 먹어본 적 없고",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043995.1.13",
          "form": "새우 진짜 어른되니",
          "original_form": "새우 진짜 어른되니",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043995.1.14",
          "form": "아 미트소스도 좋아",
          "original_form": "아 미트소스도 좋아",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043995.1.15",
          "form": "밥공기만큼 넣고싶어",
          "original_form": "밥공기만큼 넣고싶어",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043995.1.16",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043995.1.17",
          "form": "어른의 사치다",
          "original_form": "어른의 사치다",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043995.1.18",
          "form": "예전에는 새우 싫어했어?",
          "original_form": "예전에는 새우 싫어했어?",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043995.1.19",
          "form": "아니 예전부터 새우는 좋아했어",
          "original_form": "아니 예전부터 새우는 좋아했어",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043995.1.20",
          "form": "새우 예전부터 좋더라",
          "original_form": "새우 예전부터 좋더라",
