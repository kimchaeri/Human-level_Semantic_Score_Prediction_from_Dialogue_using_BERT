      "utterance": [
        {
          "id": "MDRW2100012104.1.1",
          "form": "좋아하는 책 있으세요?",
          "original_form": "좋아하는 책 있으세요?",
          "speaker_id": "1",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012104.1.2",
          "form": "좋아하는 책...저는 소설책 좋아해요",
          "original_form": "좋아하는 책...저는 소설책 좋아해요",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012104.1.3",
          "form": "판타지소설이나 역사소설?",
          "original_form": "판타지소설이나 역사소설?",
          "speaker_id": "2",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100012104.1.4",
          "form": "어떤 소설 좋아하세요?",
          "original_form": "어떤 소설 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100012104.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100012104.1.6",
          "form": "재프리님은요?",
          "original_form": "재프리님은요?",
          "speaker_id": "2",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100012104.1.7",
          "form": "판타지는 ㅋㅋ해리포터 비슷한 류의 책들이고",
          "original_form": "판타지는 ㅋㅋ해리포터 비슷한 류의 책들이고",
          "speaker_id": "2",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100012104.1.8",
          "form": "저도 해리포터 좋아해요!!!!!",
          "original_form": "저도 해리포터 좋아해요!!!!!",
          "speaker_id": "1",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100012104.1.9",
          "form": "책으로 아직 다 보진 못했지만요",
          "original_form": "책으로 아직 다 보진 못했지만요",
          "speaker_id": "1",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100012104.1.10",
          "form": "역사소설은 서태후소설이나..",
          "original_form": "역사소설은 서태후소설이나..",
          "speaker_id": "2",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100012104.1.11",
          "form": "사도세자관련소설?",
          "original_form": "사도세자관련소설?",
          "speaker_id": "2",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100012104.1.12",
          "form": "오 재밌나요? 궁금하네",
          "original_form": "오 재밌나요? 궁금하네",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012104.1.13",
          "form": "한번 보고싶어요",
          "original_form": "한번 보고싶어요",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012104.1.14",
          "form": "음...연인 서태후라는 소설은 개인적으로 꽤 재밌었구요",
          "original_form": "음...연인 서태후라는 소설은 개인적으로 꽤 재밌었구요",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100012104.1.15",
          "form": "찐 역사소설인가요???",
          "original_form": "찐 역사소설인가요???",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012104.1.16",
          "form": "러브라인???",
          "original_form": "러브라인???",
          "speaker_id": "1",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012104.1.17",
          "form": "역사고증이 잘된건지는 모르겠지만ㅋㅋ",
          "original_form": "역사고증이 잘된건지는 모르겠지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:22"
        },
        {
          "id": "MDRW2100012104.1.18",
          "form": "그런류 소설 한번도 안봤는데",
          "original_form": "그런류 소설 한번도 안봤는데",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012104.1.19",
          "form": "봐보ㅓ야겠어요 ㅎ",
          "original_form": "봐보ㅓ야겠어요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012104.1.20",
          "form": "서태후의 일대기에요. 근데 악녀라고 알고있는 서태후보다는 뭔가 더 인간적인 모습으로 보여지는...?",
          "original_form": "서태후의 일대기에요. 근데 악녀라고 알고있는 서태후보다는 뭔가 더 인간적인 모습으로 보여지는...?",
          "speaker_id": "2",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100012104.1.21",
          "form": "아마 작가가 상상력을 꽤 보탠 것 같기도하구요ㅋㅋ",
          "original_form": "아마 작가가 상상력을 꽤 보탠 것 같기도하구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100012104.1.22",
          "form": "꼭 한번 봐야겠어요ㅋㅋㅋㅋㅋㅋ 궁금해요",
          "original_form": "꼭 한번 봐야겠어요ㅋㅋㅋㅋㅋㅋ 궁금해요",
          "speaker_id": "1",
