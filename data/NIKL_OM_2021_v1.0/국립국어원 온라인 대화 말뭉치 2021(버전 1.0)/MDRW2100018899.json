      "utterance": [
        {
          "id": "MDRW2100018899.1.1",
          "form": "안녕 name2~",
          "original_form": "안녕 &name2&~",
          "speaker_id": "1",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018899.1.2",
          "form": "나 심심이 시작했음",
          "original_form": "나 심심이 시작했음",
          "speaker_id": "1",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018899.1.3",
          "form": "아냐쎄여",
          "original_form": "아냐쎄여",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018899.1.4",
          "form": "name1에요~",
          "original_form": "&name1&에요~",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018899.1.5",
          "form": "아...ㅎ네^^;;",
          "original_form": "아...ㅎ네^^;;",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018899.1.6",
          "form": "온라인 커뮤니티..하는 거라고는 트위터 뿐이라 하하",
          "original_form": "온라인 커뮤니티..하는 거라고는 트위터 뿐이라 하하",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018899.1.7",
          "form": "하하하 트위터 재미있죠",
          "original_form": "하하하 트위터 재미있죠",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018899.1.8",
          "form": "오늘 핫이슈는 뭔가요?",
          "original_form": "오늘 핫이슈는 뭔가요?",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018899.1.9",
          "form": "어제 루카스 폭로 보셨나요!",
          "original_form": "어제 루카스 폭로 보셨나요!",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018899.1.10",
          "form": "봤지요",
          "original_form": "봤지요",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018899.1.11",
          "form": "루카스 양다리도 아니고 4다리나...",
          "original_form": "루카스 양다리도 아니고 4다리나...",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018899.1.12",
          "form": "루카스라이팅",
          "original_form": "루카스라이팅",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018899.1.13",
          "form": "만났던 여자친구분 재력이 엄청나다는 생각이 듭니다",
          "original_form": "만났던 여자친구분 재력이 엄청나다는 생각이 듭니다",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018899.1.14",
          "form": "정말 드럽더라구요",
          "original_form": "정말 드럽더라구요",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018899.1.15",
          "form": "맞아요 완전 꽃뱀이던데요 루카스",
          "original_form": "맞아요 완전 꽃뱀이던데요 루카스",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018899.1.16",
          "form": "떼잉",
          "original_form": "떼잉",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018899.1.17",
          "form": "그것보다",
          "original_form": "그것보다",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018899.1.18",
          "form": "연경홀릭 어떻게 되는 거죠",
          "original_form": "연경홀릭 어떻게 되는 거죠",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018899.1.19",
          "form": "남성이 얌전치가 못해서",
          "original_form": "남성이 얌전치가 못해서",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018899.1.20",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018899.1.21",
          "form": "소속사가 입장문 내고",
          "original_form": "소속사가 입장문 내고",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018899.1.22",
          "form": "그거 없어져야 하는 거 아닌가요",
          "original_form": "그거 없어져야 하는 거 아닌가요",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018899.1.23",
          "form": "갤에서 입장문 더 정리해서 보내기로 하던 중에",
          "original_form": "갤에서 입장문 더 정리해서 보내기로 하던 중에",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018899.1.24",
          "form": "스타일리스트가=연홀",
          "original_form": "스타일리스트가=연홀",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018899.1.25",
          "form": "인스타 아이디를 영어자판으로",
          "original_form": "인스타 아이디를 영어자판으로",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018899.1.26",
          "form": "어쩌라고 ㅋㅋ",
          "original_form": "어쩌라고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018899.1.27",
          "form": "로 바꿔서",
          "original_form": "로 바꿔서",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018899.1.28",
          "form": "사그라들던 불씨에 장작을 넣어줬어요",
          "original_form": "사그라들던 불씨에 장작을 넣어줬어요",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018899.1.29",
          "form": "와우..",
          "original_form": "와우..",
          "speaker_id": "1",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018899.1.30",
          "form": "그래서 갤에서 입장문 소속사에 보내구",
          "original_form": "그래서 갤에서 입장문 소속사에 보내구",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018899.1.31",
          "form": "대응이 별루면",
          "original_form": "대응이 별루면",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018899.1.32",
          "form": "해결되지 않았군요",
          "original_form": "해결되지 않았군요",
          "speaker_id": "1",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018899.1.33",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 13:33"
        }
      ]
    }
