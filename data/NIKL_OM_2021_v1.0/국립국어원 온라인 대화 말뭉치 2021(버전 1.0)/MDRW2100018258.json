      "utterance": [
        {
          "id": "MDRW2100018258.1.1",
          "form": "오늘도 비오네 ㅠ",
          "original_form": "오늘도 비오네 ㅠ",
          "speaker_id": "1",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100018258.1.2",
          "form": "진짜 일주일동안 하루빼고 다 비내리네 ㅎㅎ",
          "original_form": "진짜 일주일동안 하루빼고 다 비내리네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100018258.1.3",
          "form": "가을 장마 맞나봐 ㅋ",
          "original_form": "가을 장마 맞나봐 ㅋ",
          "speaker_id": "1",
          "time": "20210827 09:34"
        },
        {
          "id": "MDRW2100018258.1.4",
          "form": "그래도 오늘은 습도 좀 떨어진듯",
          "original_form": "그래도 오늘은 습도 좀 떨어진듯",
          "speaker_id": "2",
          "time": "20210827 09:35"
        },
        {
          "id": "MDRW2100018258.1.5",
          "form": "비오는데 습도는 떨어지고 신기해 ㅎㅎ",
          "original_form": "비오는데 습도는 떨어지고 신기해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 09:35"
        },
        {
          "id": "MDRW2100018258.1.6",
          "form": "집안 습도 몇 프로야?",
          "original_form": "집안 습도 몇 프로야?",
          "speaker_id": "2",
          "time": "20210827 09:35"
        },
        {
          "id": "MDRW2100018258.1.7",
          "form": "지금 59프로 아주 살만해 ㅎㅎ",
          "original_form": "지금 59프로 아주 살만해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 09:35"
        },
        {
          "id": "MDRW2100018258.1.8",
          "form": "비 오고 나니 많이 쌀쌀해졌어",
          "original_form": "비 오고 나니 많이 쌀쌀해졌어",
          "speaker_id": "2",
          "time": "20210827 09:35"
        },
        {
          "id": "MDRW2100018258.1.9",
          "form": "신랑 이제 가디건이라도 가지고 다녀야겠다 ㅎㅎ",
          "original_form": "신랑 이제 가디건이라도 가지고 다녀야겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 09:36"
        },
        {
          "id": "MDRW2100018258.1.10",
          "form": "아직은 괜찮아 ㅋ 10도 대로 떨어지면 그 때 챙기지 뭐",
          "original_form": "아직은 괜찮아 ㅋ 10도 대로 떨어지면 그 때 챙기지 뭐",
          "speaker_id": "2",
          "time": "20210827 09:36"
        },
        {
          "id": "MDRW2100018258.1.11",
          "form": "애들 책받으러 갈 때도 비오면 안될텐데",
          "original_form": "애들 책받으러 갈 때도 비오면 안될텐데",
          "speaker_id": "1",
          "time": "20210827 09:36"
        },
        {
          "id": "MDRW2100018258.1.12",
          "form": "하늘보니 올 수도 있을 것 같아",
          "original_form": "하늘보니 올 수도 있을 것 같아",
          "speaker_id": "2",
          "time": "20210827 09:36"
        },
        {
          "id": "MDRW2100018258.1.13",
          "form": "그러게.. 하늘에 완전 먹구름이 가득이네 ㅠㅠ",
          "original_form": "그러게.. 하늘에 완전 먹구름이 가득이네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 09:37"
        },
        {
          "id": "MDRW2100018258.1.14",
          "form": "조금 개이는 느낌이네~ 햇빛 보이기 시작한다",
          "original_form": "조금 개이는 느낌이네~ 햇빛 보이기 시작한다",
