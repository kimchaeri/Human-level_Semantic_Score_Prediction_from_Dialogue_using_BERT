      "utterance": [
        {
          "id": "MDRW2100012229.1.1",
          "form": "이제 진정한 가을인가",
          "original_form": "이제 진정한 가을인가",
          "speaker_id": "2",
          "time": "20210824 20:30"
        },
        {
          "id": "MDRW2100012229.1.2",
          "form": "기온이 뚝 떨어진 것보니 맞는 것 같아 ㅎㅎ",
          "original_form": "기온이 뚝 떨어진 것보니 맞는 것 같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:30"
        },
        {
          "id": "MDRW2100012229.1.3",
          "form": "이젠 에어컨 없이도 저녁엔 23도네",
          "original_form": "이젠 에어컨 없이도 저녁엔 23도네",
          "speaker_id": "2",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100012229.1.4",
          "form": "그래도 아직 긴팔 입긴 더워",
          "original_form": "그래도 아직 긴팔 입긴 더워",
          "speaker_id": "1",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100012229.1.5",
          "form": "글치 ㅎㅎ 긴팔은 10도대로 떨어져야지",
          "original_form": "글치 ㅎㅎ 긴팔은 10도대로 떨어져야지",
          "speaker_id": "2",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100012229.1.6",
          "form": "나 어제 긴바지 입고 잤다가 새벽 3시에 깸 ㅎㅎ",
          "original_form": "나 어제 긴바지 입고 잤다가 새벽 3시에 깸 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100012229.1.7",
          "form": "너무 더웠구나? ㅋ",
          "original_form": "너무 더웠구나? ㅋ",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012229.1.8",
          "form": "ㅇㅇ 귀찮아서 참고 자려다가 도저히 못참고 반바지 입으니 천국같았어 ㅋㅋㅋ",
          "original_form": "ㅇㅇ 귀찮아서 참고 자려다가 도저히 못참고 반바지 입으니 천국같았어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012229.1.9",
          "form": "ㅋㅋㅋ 새벽에 부스럭 거리는 소리가 색시였구만",
          "original_form": "ㅋㅋㅋ 새벽에 부스럭 거리는 소리가 색시였구만",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012229.1.10",
          "form": "어찌나 시원하던지 ㅎㅎ",
          "original_form": "어찌나 시원하던지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012229.1.11",
          "form": "아직까진 반팔 입어야 해",
          "original_form": "아직까진 반팔 입어야 해",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012229.1.12",
          "form": "신랑은 사계절 반팔이잖아 ㅋ",
          "original_form": "신랑은 사계절 반팔이잖아 ㅋ",
          "speaker_id": "1",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012229.1.13",
          "form": "암튼 여름보다는 가을이 시원하고 좋다",
          "original_form": "암튼 여름보다는 가을이 시원하고 좋다",
          "speaker_id": "1",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012229.1.14",
          "form": "나도 ㅎㅎ 가을이 훨씬 나아",
          "original_form": "나도 ㅎㅎ 가을이 훨씬 나아",
          "speaker_id": "2",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012229.1.15",
          "form": "아까 산책하다보니까 송충이들이 엄청 많더라",
          "original_form": "아까 산책하다보니까 송충이들이 엄청 많더라",
