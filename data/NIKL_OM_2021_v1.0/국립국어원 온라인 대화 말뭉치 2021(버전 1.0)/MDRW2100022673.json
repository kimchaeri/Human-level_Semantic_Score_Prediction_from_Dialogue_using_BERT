      "utterance": [
        {
          "id": "MDRW2100022673.1.1",
          "form": "학교가 아수라장이 됏던거에요 왜 자기네는",
          "original_form": "학교가 아수라장이 됏던거에요 왜 자기네는",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022673.1.2",
          "form": "못가냐고",
          "original_form": "못가냐고",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022673.1.3",
          "form": "그 와중에 학교에 사람이 아니고",
          "original_form": "그 와중에 학교에 사람이 아니고",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022673.1.4",
          "form": "괴물이나",
          "original_form": "괴물이나",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022673.1.5",
          "form": "그럴수가",
          "original_form": "그럴수가",
          "speaker_id": "2",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022673.1.6",
          "form": "인간이 아닌 존재들도",
          "original_form": "인간이 아닌 존재들도",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022673.1.7",
          "form": "취직해서 일하는데 보통은",
          "original_form": "취직해서 일하는데 보통은",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022673.1.8",
          "form": "악당출신인 ..",
          "original_form": "악당출신인 ..",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022673.1.9",
          "form": "그닥 성격이 좋지 못한",
          "original_form": "그닥 성격이 좋지 못한",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022673.1.10",
          "form": "사람들임",
          "original_form": "사람들임",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022673.1.11",
          "form": "걔네가 선두로 달려서",
          "original_form": "걔네가 선두로 달려서",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022673.1.12",
          "form": "커맨더가 쓰인 물건이 존재하긴 해서",
          "original_form": "커맨더가 쓰인 물건이 존재하긴 해서",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022673.1.13",
          "form": "그걸가지고 운동장에서",
          "original_form": "그걸가지고 운동장에서",
          "speaker_id": "1",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022673.1.14",
          "form": "올",
          "original_form": "올",
          "speaker_id": "2",
          "time": "20210829 16:54"
        },
        {
          "id": "MDRW2100022673.1.15",
          "form": "서로 가지겟다고 육탄전이 벌어짐",
          "original_form": "서로 가지겟다고 육탄전이 벌어짐",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022673.1.16",
          "form": "근데 누가 연막탄을 써서",
          "original_form": "근데 누가 연막탄을 써서",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022673.1.17",
          "form": "검은 안개 사이로 악당들이 흩어지는데",
          "original_form": "검은 안개 사이로 악당들이 흩어지는데",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022673.1.18",
          "form": "검은안개 위로",
          "original_form": "검은안개 위로",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022673.1.19",
          "form": "푸른용이 솟구치면서 커맨더를 가지고",
          "original_form": "푸른용이 솟구치면서 커맨더를 가지고",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022673.1.20",
          "form": "날아올랐어요",
          "original_form": "날아올랐어요",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022673.1.21",
          "form": "근데 용은 커맨더를 가진게 무색하게",
          "original_form": "근데 용은 커맨더를 가진게 무색하게",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022673.1.22",
          "form": "진짜 날아올라서 그 펜트하우스에 갈수잇엇던거임",
          "original_form": "진짜 날아올라서 그 펜트하우스에 갈수잇엇던거임",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022673.1.23",
          "form": "그래서 펜트하우스로 들어갔는데",
          "original_form": "그래서 펜트하우스로 들어갔는데",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022673.1.24",
          "form": "하필 그 입구기",
          "original_form": "하필 그 입구기",
          "speaker_id": "1",
          "time": "20210829 16:55"
        },
        {
          "id": "MDRW2100022673.1.25",
          "form": "용도있다니",
          "original_form": "용도있다니",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.26",
          "form": "펜트하우스 냉동거엿던거임",
          "original_form": "펜트하우스 냉동거엿던거임",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.27",
          "form": "냉동고는 얼음으로 이루어져있는데",
          "original_form": "냉동고는 얼음으로 이루어져있는데",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.28",
          "form": "거기에 얼음굴이 있어서",
          "original_form": "거기에 얼음굴이 있어서",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.29",
          "form": "거기로 들어간거에요",
          "original_form": "거기로 들어간거에요",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.30",
          "form": "근데 그 굴이 용이 들어가기엔",
          "original_form": "근데 그 굴이 용이 들어가기엔",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.31",
          "form": "용이 너무너무 길어서",
          "original_form": "용이 너무너무 길어서",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.32",
          "form": "용이 얼음들 사이에 낀채로",
          "original_form": "용이 얼음들 사이에 낀채로",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.33",
          "form": "머리만 겨우 냉동고까지",
          "original_form": "머리만 겨우 냉동고까지",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.34",
          "form": "리자몽됐네",
          "original_form": "리자몽됐네",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.35",
          "form": "닿앗어요",
          "original_form": "닿앗어요",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022673.1.37",
          "form": "그래서 용이 슬퍼하면서",
          "original_form": "그래서 용이 슬퍼하면서",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.38",
          "form": "왜 그렇게 싸우며 여기 온건지 모르겟다고",
          "original_form": "왜 그렇게 싸우며 여기 온건지 모르겟다고",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.39",
          "form": "자기도 놀고싶다고 생각하고있었는데",
          "original_form": "자기도 놀고싶다고 생각하고있었는데",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.40",
          "form": "냉동고라서 누가 올일이 없는데",
          "original_form": "냉동고라서 누가 올일이 없는데",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.41",
          "form": "누가 문을 여는소리가 들리는거임",
          "original_form": "누가 문을 여는소리가 들리는거임",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.42",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.43",
          "form": "근데 그게 저엿음",
          "original_form": "근데 그게 저엿음",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.44",
          "form": "제가 그냥 다른곳 둘러보다가",
          "original_form": "제가 그냥 다른곳 둘러보다가",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.45",
          "form": "여기 오게됏대요",
          "original_form": "여기 오게됏대요",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.46",
          "form": "그래서 빵 갖다주겟다고 하더니",
          "original_form": "그래서 빵 갖다주겟다고 하더니",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.47",
          "form": "글쿠먼",
          "original_form": "글쿠먼",
          "speaker_id": "2",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.48",
          "form": "접시에 빵이랑 케이크랑 마카롱같은거",
          "original_form": "접시에 빵이랑 케이크랑 마카롱같은거",
          "speaker_id": "1",
          "time": "20210829 16:57"
        },
        {
          "id": "MDRW2100022673.1.49",
          "form": "쌓아와서",
          "original_form": "쌓아와서",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022673.1.50",
          "form": "용 주니까 너무 좋아하고 맛있게",
          "original_form": "용 주니까 너무 좋아하고 맛있게",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022673.1.51",
          "form": "먹음",
          "original_form": "먹음",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022673.1.52",
          "form": "그러고 깻어요",
          "original_form": "그러고 깻어요",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022673.1.53",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ오",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ오",
          "speaker_id": "2",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022673.1.54",
          "form": "용 개맛있는거",
          "original_form": "용 개맛있는거",
          "speaker_id": "2",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022673.1.55",
          "form": "먹었네..",
          "original_form": "먹었네..",
          "speaker_id": "2",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022673.1.56",
          "form": "ㄱㄴㄲ암튼",
          "original_form": "ㄱㄴㄲ암튼",
          "speaker_id": "1",
          "time": "20210829 16:58"
        }
      ]
