      "utterance": [
        {
          "id": "MDRW2100002556.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100002556.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100002556.1.3",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100002556.1.4",
          "form": "네 요즘 못가서 아쉬워요",
          "original_form": "네 요즘 못가서 아쉬워요",
          "speaker_id": "2",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100002556.1.5",
          "form": "마지막여행은 어디 가보셨어요?",
          "original_form": "마지막여행은 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100002556.1.6",
          "form": "저 강릉이요ㅋㅋㅋ",
          "original_form": "저 강릉이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:11"
        },
        {
          "id": "MDRW2100002556.1.7",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 11:11"
        },
        {
          "id": "MDRW2100002556.1.8",
          "form": "저는 여수요",
          "original_form": "저는 여수요",
          "speaker_id": "2",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100002556.1.9",
          "form": "비가 많이와서 제대로 구경은 못했지만요",
          "original_form": "비가 많이와서 제대로 구경은 못했지만요",
          "speaker_id": "2",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100002556.1.10",
          "form": "와우 여수좋죠",
          "original_form": "와우 여수좋죠",
          "speaker_id": "1",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100002556.1.11",
          "form": "장범준의 여수",
          "original_form": "장범준의 여수",
          "speaker_id": "1",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100002556.1.12",
          "form": "네 음식도 맛있고 좋았어요",
          "original_form": "네 음식도 맛있고 좋았어요",
          "speaker_id": "2",
          "time": "20210824 11:20"
        },
        {
          "id": "MDRW2100002556.1.13",
          "form": "여행은 계획을 철저히 세우고 다니시는 편이세요?",
          "original_form": "여행은 계획을 철저히 세우고 다니시는 편이세요?",
          "speaker_id": "2",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100002556.1.14",
          "form": "아니요 저는",
          "original_form": "아니요 저는",
          "speaker_id": "1",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100002556.1.15",
          "form": "그냥 여행지만 정하는 스타일이예요ㅋㅋㅋ",
          "original_form": "그냥 여행지만 정하는 스타일이예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100002556.1.16",
          "form": "거기에 맛집정도?",
          "original_form": "거기에 맛집정도?",
          "speaker_id": "1",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100002556.1.17",
          "form": "저도 그게 느긋해서 좋더라고요",
          "original_form": "저도 그게 느긋해서 좋더라고요",
          "speaker_id": "2",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100002556.1.18",
          "form": "계획대로 안되면 짜증나서 차리리 안세우는게ㅋㅋ",
          "original_form": "계획대로 안되면 짜증나서 차리리 안세우는게ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100002556.1.19",
          "form": "여행은 쉬러가는거니까요ㅋㅋ",
          "original_form": "여행은 쉬러가는거니까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100002556.1.20",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:36"
        },
        {
          "id": "MDRW2100002556.1.21",
          "form": "숙소랑 여행지 교통편정도면",
          "original_form": "숙소랑 여행지 교통편정도면",
          "speaker_id": "1",
          "time": "20210824 11:36"
        },
        {
          "id": "MDRW2100002556.1.22",
          "form": "끝이죠",
          "original_form": "끝이죠",
          "speaker_id": "1",
          "time": "20210824 11:36"
        },
        {
          "id": "MDRW2100002556.1.23",
          "form": "네 예전에는 막 엄청걸으면서 여기저기 다녀서\r\n저녁에 파스붙이고 그랬는데",
          "original_form": "네 예전에는 막 엄청걸으면서 여기저기 다녀서\r\n저녁에 파스붙이고 그랬는데",
          "speaker_id": "2",
          "time": "20210824 11:41"
        },
        {
          "id": "MDRW2100002556.1.24",
          "form": "지금생각하면 왜그랫나 싶어요",
          "original_form": "지금생각하면 왜그랫나 싶어요",
          "speaker_id": "2",
          "time": "20210824 11:41"
        },
        {
          "id": "MDRW2100002556.1.25",
          "form": "ㅋㅋㅋ맞아요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:50"
        },
        {
          "id": "MDRW2100002556.1.26",
          "form": "어릴때의 패기라해야하나요",
          "original_form": "어릴때의 패기라해야하나요",
          "speaker_id": "1",
          "time": "20210824 11:50"
        },
        {
          "id": "MDRW2100002556.1.27",
          "form": "나이드니 편한게좋더라고요",
          "original_form": "나이드니 편한게좋더라고요",
          "speaker_id": "1",
          "time": "20210824 11:50"
        }
      ]
    }
  ]
