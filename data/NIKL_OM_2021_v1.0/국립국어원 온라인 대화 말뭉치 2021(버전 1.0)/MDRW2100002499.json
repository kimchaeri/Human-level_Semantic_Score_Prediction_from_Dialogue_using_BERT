      "utterance": [
        {
          "id": "MDRW2100002499.1.1",
          "form": "와 10대",
          "original_form": "와 10대",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002499.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002499.1.3",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002499.1.4",
          "form": "10대예요??",
          "original_form": "10대예요??",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002499.1.5",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002499.1.6",
          "form": "음료 뭐 좋아하세여",
          "original_form": "음료 뭐 좋아하세여",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002499.1.7",
          "form": "와 학교안가나요?\r\n저는 스벅좋아합니다",
          "original_form": "와 학교안가나요?\r\n저는 스벅좋아합니다",
          "speaker_id": "1",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002499.1.8",
          "form": "지금 방학이에요ㅎㅎ",
          "original_form": "지금 방학이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002499.1.9",
          "form": "스벅 어떤 메뉴가 좋으신가요ㅎㅎ",
          "original_form": "스벅 어떤 메뉴가 좋으신가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002499.1.10",
          "form": "오 방학끝나면 학교가요? 10대때가아득하네요 좋겠어요",
          "original_form": "오 방학끝나면 학교가요? 10대때가아득하네요 좋겠어요",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002499.1.11",
          "form": "자바칩프랍좋아해요ㅋㅋㅋ",
          "original_form": "자바칩프랍좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002499.1.12",
          "form": "10대는 무슨카페가나요?",
          "original_form": "10대는 무슨카페가나요?",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002499.1.13",
          "form": "저도 스타벅스 자주 가요ㅎㅎ",
          "original_form": "저도 스타벅스 자주 가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002499.1.14",
          "form": "이번에 나온 아이스크림블랜디드? 드셔보셨어요?",
          "original_form": "이번에 나온 아이스크림블랜디드? 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002499.1.15",
          "form": "오 아니요ㅋㅋㅋ 요즘은 10대도 스벅을가는군요",
          "original_form": "오 아니요ㅋㅋㅋ 요즘은 10대도 스벅을가는군요",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002499.1.16",
          "form": "급식은 맛있나요?",
          "original_form": "급식은 맛있나요?",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002499.1.17",
          "form": "스벅은 어디든 있잖아요ㅎㅎ",
          "original_form": "스벅은 어디든 있잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002499.1.18",
          "form": "급식 먹은지 꽤 됐네요",
          "original_form": "급식 먹은지 꽤 됐네요",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002499.1.19",
          "form": "부담가는 가격이아닌가보네요ㅋㅋㅋ 세상이 바뀌었군요",
          "original_form": "부담가는 가격이아닌가보네요ㅋㅋㅋ 세상이 바뀌었군요",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002499.1.20",
          "form": "코로나때문에 안 먹었거든요",
          "original_form": "코로나때문에 안 먹었거든요",
          "speaker_id": "2",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002499.1.21",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002499.1.22",
          "form": "그럼 뭐먹어요?",
          "original_form": "그럼 뭐먹어요?",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002499.1.23",
          "form": "저희는 계속 재택했었고",
          "original_form": "저희는 계속 재택했었고",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002499.1.24",
          "form": "몇번 나아져서 등교해도",
          "original_form": "몇번 나아져서 등교해도",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002499.1.25",
          "form": "일찍 끝났어요!",
          "original_form": "일찍 끝났어요!",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002499.1.26",
          "form": "헐 고등학생맞죠? 대박이네요",
          "original_form": "헐 고등학생맞죠? 대박이네요",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002499.1.27",
          "form": "급식이 12시~1시일텐데",
          "original_form": "급식이 12시~1시일텐데",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002499.1.28",
          "form": "그전에학교가끝난다니",
          "original_form": "그전에학교가끝난다니",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002499.1.29",
          "form": "코로나가 심하긴하네요 급식도못먹고",
          "original_form": "코로나가 심하긴하네요 급식도못먹고",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002499.1.30",
          "form": "ㅋㅋㅋ올해 같은 반된 애들도",
          "original_form": "ㅋㅋㅋ올해 같은 반된 애들도",
          "speaker_id": "2",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002499.1.31",
          "form": "몇번못봐서 안 친해여..",
          "original_form": "몇번못봐서 안 친해여..",
          "speaker_id": "2",
          "time": "20210820 16:11"
        }
      ]
    }
