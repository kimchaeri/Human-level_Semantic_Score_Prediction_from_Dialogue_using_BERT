      "utterance": [
        {
          "id": "MDRW2100017335.1.1",
          "form": "안녕하세요~ 저녁 드셨나요?",
          "original_form": "안녕하세요~ 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017335.1.2",
          "form": "아직요!",
          "original_form": "아직요!",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017335.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017335.1.4",
          "form": "저도 곧 먹을거같아요! 메뉴 정하셨나요?",
          "original_form": "저도 곧 먹을거같아요! 메뉴 정하셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017335.1.5",
          "form": "고기 먹을것겉네요 ㅎㅎ",
          "original_form": "고기 먹을것겉네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100017335.1.6",
          "form": "좀 신나요",
          "original_form": "좀 신나요",
          "speaker_id": "2",
          "time": "20210826 18:51"
        },
        {
          "id": "MDRW2100017335.1.7",
          "form": "뭐드시나요?",
          "original_form": "뭐드시나요?",
          "speaker_id": "2",
          "time": "20210826 18:51"
        },
        {
          "id": "MDRW2100017335.1.8",
          "form": "전 피자요ㅎㅎ 집에서 구워드세요?",
          "original_form": "전 피자요ㅎㅎ 집에서 구워드세요?",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017335.1.9",
          "form": "네! 집에서 먹어야 잔뜩먹죠 ㅎㅎ",
          "original_form": "네! 집에서 먹어야 잔뜩먹죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017335.1.10",
          "form": "어디 피자 드셨어요?",
          "original_form": "어디 피자 드셨어요?",
          "speaker_id": "2",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017335.1.11",
          "form": "도미노요! 소고기드신거세요?",
          "original_form": "도미노요! 소고기드신거세요?",
          "speaker_id": "1",
          "time": "20210826 19:58"
        },
        {
          "id": "MDRW2100017335.1.12",
          "form": "아니요 삼겹살이요 ㅎㅎ",
          "original_form": "아니요 삼겹살이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017335.1.13",
          "form": "양파랑 먹으니 최고네요",
          "original_form": "양파랑 먹으니 최고네요",
          "speaker_id": "2",
          "time": "20210826 20:00"
        },
        {
          "id": "MDRW2100017335.1.14",
          "form": "구운마늘 좋아하세요?",
          "original_form": "구운마늘 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017335.1.15",
          "form": "헐 너무 사랑해요",
          "original_form": "헐 너무 사랑해요",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017335.1.16",
          "form": "저두요ㅠㅠ",
          "original_form": "저두요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100017335.1.17",
          "form": "파채는요?",
          "original_form": "파채는요?",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100017335.1.18",
          "form": "완전 좋죠 ㅠㅠㅠ",
          "original_form": "완전 좋죠 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100017335.1.19",
          "form": "콩나물도 같이 김치랑 샥샥 볶아먹고싶어요",
          "original_form": "콩나물도 같이 김치랑 샥샥 볶아먹고싶어요",
          "speaker_id": "2",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100017335.1.20",
          "form": "오늘 점심 뭐 드시나요?",
          "original_form": "오늘 점심 뭐 드시나요?",
          "speaker_id": "1",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100017335.1.21",
          "form": "음 방금 일어나서 좀 고민해야할것같어요 ㅋㅋ",
          "original_form": "음 방금 일어나서 좀 고민해야할것같어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:01"
