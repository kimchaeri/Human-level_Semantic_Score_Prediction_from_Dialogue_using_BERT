      "utterance": [
        {
          "id": "MDRW2100042516.1.1",
          "form": "지금 김장김치에",
          "original_form": "지금 김장김치에",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042516.1.2",
          "form": "밥먹는중",
          "original_form": "밥먹는중",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042516.1.3",
          "form": "앗 못 봤다 뭘 제일 싫어한다고?",
          "original_form": "앗 못 봤다 뭘 제일 싫어한다고?",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042516.1.4",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042516.1.5",
          "form": "오이넣은 볶음밥",
          "original_form": "오이넣은 볶음밥",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042516.1.6",
          "form": "왜? 듣기에는 멀쩡한 음식같아보이는데",
          "original_form": "왜? 듣기에는 멀쩡한 음식같아보이는데",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042516.1.7",
          "form": "오이에서 빠진 수분이",
          "original_form": "오이에서 빠진 수분이",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042516.1.8",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042516.1.9",
          "form": "볶음밥을 밟힌 눈처럼 만들어",
          "original_form": "볶음밥을 밟힌 눈처럼 만들어",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042516.1.10",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042516.1.11",
          "form": "밥 질감이 진눈깨비처럼 질척거려?",
          "original_form": "밥 질감이 진눈깨비처럼 질척거려?",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042516.1.12",
          "form": "응 그리고 맛에서 썩은 오이맛나",
          "original_form": "응 그리고 맛에서 썩은 오이맛나",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042516.1.13",
          "form": "우우우우웁",
          "original_form": "우우우우웁",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042516.1.14",
          "form": "쉰내나?",
          "original_form": "쉰내나?",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042516.1.15",
          "form": "응 토나올것같아",
          "original_form": "응 토나올것같아",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042516.1.16",
          "form": "속이 울렁거려",
          "original_form": "속이 울렁거려",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042516.1.17",
          "form": "으 진짜 싫다 절대 안 먹을게",
          "original_form": "으 진짜 싫다 절대 안 먹을게",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042516.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042516.1.19",
          "form": "애호박도 잘못 넣으면 밥이 질척해지던데",
          "original_form": "애호박도 잘못 넣으면 밥이 질척해지던데",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042516.1.20",
          "form": "오이는 더 심하겠구나",
          "original_form": "오이는 더 심하겠구나",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042516.1.21",
          "form": "김치는 지금 담갔니",
          "original_form": "김치는 지금 담갔니",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042516.1.22",
          "form": "응 지금 난리",
          "original_form": "응 지금 난리",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042516.1.23",
          "form": "대박! 겉절이랑 밥 먹는 거였구나!",
          "original_form": "대박! 겉절이랑 밥 먹는 거였구나!",
          "speaker_id": "1",
          "time": "20210906 22:40"
        }
      ]
