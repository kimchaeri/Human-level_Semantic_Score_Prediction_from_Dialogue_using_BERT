      "utterance": [
        {
          "id": "MDRW2100041152.1.1",
          "form": "엄마가 주로 요리하셔서 주말엔 해방시켜드릴려고ㅎㅎ",
          "original_form": "엄마가 주로 요리하셔서 주말엔 해방시켜드릴려고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 18:30"
        },
        {
          "id": "MDRW2100041152.1.2",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210905 18:31"
        },
        {
          "id": "MDRW2100041152.1.3",
          "form": "저희 집도 엄마가 힘들다 하셨는데",
          "original_form": "저희 집도 엄마가 힘들다 하셨는데",
          "speaker_id": "1",
          "time": "20210905 18:31"
        },
        {
          "id": "MDRW2100041152.1.4",
          "form": "사 와서 먹거나 배달에서 먹어요",
          "original_form": "사 와서 먹거나 배달에서 먹어요",
          "speaker_id": "1",
          "time": "20210905 18:31"
        },
        {
          "id": "MDRW2100041152.1.5",
          "form": "네 그래도 설거지거리는 나오지만 엄만 좋아하시더라고요",
          "original_form": "네 그래도 설거지거리는 나오지만 엄만 좋아하시더라고요",
          "speaker_id": "2",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100041152.1.6",
          "form": "설거진 자식들 몫",
          "original_form": "설거진 자식들 몫",
          "speaker_id": "2",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100041152.1.7",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100041152.1.8",
          "form": "매일 매일 요리하는게 엄청 힘들 거 같아요",
          "original_form": "매일 매일 요리하는게 엄청 힘들 거 같아요",
          "speaker_id": "1",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100041152.1.9",
          "form": "맞아요 그래서 냉동식품좀 사다 쟁여놨어요",
          "original_form": "맞아요 그래서 냉동식품좀 사다 쟁여놨어요",
          "speaker_id": "2",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100041152.1.10",
          "form": "냉동식품 잇으면 밥 해먹기 편하죠",
          "original_form": "냉동식품 잇으면 밥 해먹기 편하죠",
          "speaker_id": "1",
          "time": "20210905 18:37"
        },
        {
          "id": "MDRW2100041152.1.11",
          "form": "맞아요 만두랑 떡갈비 유용합니다",
          "original_form": "맞아요 만두랑 떡갈비 유용합니다",
          "speaker_id": "2",
          "time": "20210905 18:40"
        },
        {
          "id": "MDRW2100041152.1.12",
          "form": "한성해물경단??",
          "original_form": "한성해물경단??",
          "speaker_id": "1",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100041152.1.13",
          "form": "그거맛있었어요",
          "original_form": "그거맛있었어요",
          "speaker_id": "1",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100041152.1.14",
          "form": "아 진짜요? 접수하겠습니다!",
          "original_form": "아 진짜요? 접수하겠습니다!",
          "speaker_id": "2",
          "time": "20210905 18:42"
        },
        {
          "id": "MDRW2100041152.1.15",
          "form": "함 드셔보세요 ㅎㅎ",
          "original_form": "함 드셔보세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100041152.1.16",
          "form": "오징어 조각 씹혀요",
          "original_form": "오징어 조각 씹혀요",
          "speaker_id": "1",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100041152.1.17",
          "form": "간편하게 되어있죠?",
          "original_form": "간편하게 되어있죠?",
          "speaker_id": "2",
          "time": "20210905 18:47"
        },
        {
          "id": "MDRW2100041152.1.18",
          "form": "네 동그랑땡 모양아요 굽기만하명돼요",
          "original_form": "네 동그랑땡 모양아요 굽기만하명돼요",
          "speaker_id": "1",
          "time": "20210905 18:48"
        },
        {
          "id": "MDRW2100041152.1.19",
          "form": "네이버 치니 나오네요",
          "original_form": "네이버 치니 나오네요",
          "speaker_id": "1",
          "time": "20210905 18:48"
        },
        {
          "id": "MDRW2100041152.1.20",
          "form": "엄마가 혼낼텐데 우선 사봅니다ㅋㅋ",
          "original_form": "엄마가 혼낼텐데 우선 사봅니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100041152.1.21",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100041152.1.22",
          "form": "이거 나름 유명한 제품인가봐요 후기많네요",
          "original_form": "이거 나름 유명한 제품인가봐요 후기많네요",
          "speaker_id": "1",
          "time": "20210905 18:53"
        },
        {
          "id": "MDRW2100041152.1.23",
          "form": "지금 캡쳐했어요!!!! ㅋㅋ",
          "original_form": "지금 캡쳐했어요!!!! ㅋㅋ",
