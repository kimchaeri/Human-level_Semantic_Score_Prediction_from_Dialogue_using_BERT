      "utterance": [
        {
          "id": "MDRW2100007245.1.1",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007245.1.2",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007245.1.3",
          "form": "나 초코파이 먹는 중",
          "original_form": "나 초코파이 먹는 중",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007245.1.4",
          "form": "와 나도 줘",
          "original_form": "와 나도 줘",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007245.1.5",
          "form": "집에 이상한 과자 있어",
          "original_form": "집에 이상한 과자 있어",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007245.1.6",
          "form": "꼬깔콘 새우마요맛",
          "original_form": "꼬깔콘 새우마요맛",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007245.1.7",
          "form": "새우마요? 진짜이상하다",
          "original_form": "새우마요? 진짜이상하다",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007245.1.8",
          "form": "꼬깔콘은 매콤달콤이지",
          "original_form": "꼬깔콘은 매콤달콤이지",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007245.1.9",
          "form": "근데 걍 꼬깔코맛이랑",
          "original_form": "근데 걍 꼬깔코맛이랑",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007245.1.10",
          "form": "별로 차이 안 남",
          "original_form": "별로 차이 안 남",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007245.1.11",
          "form": "난 군옥수수맛이 좋아",
          "original_form": "난 군옥수수맛이 좋아",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007245.1.12",
          "form": "ㅇㅎ안머거봄",
          "original_form": "ㅇㅎ안머거봄",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007245.1.13",
          "form": "너왜빨랑답장안하냐",
          "original_form": "너왜빨랑답장안하냐",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007245.1.14",
          "form": "나 지금 녹색전상 좋아하는 잼수러랑 대화중이라",
          "original_form": "나 지금 녹색전상 좋아하는 잼수러랑 대화중이라",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007245.1.15",
          "form": "머쓱",
          "original_form": "머쓱",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007245.1.16",
          "form": "어쩌다 이런 인연이?",
          "original_form": "어쩌다 이런 인연이?",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007245.1.17",
          "form": "난 다 무시하고 너만 기다렸는데..",
          "original_form": "난 다 무시하고 너만 기다렸는데..",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007245.1.18",
          "form": "야아~",
          "original_form": "야아~",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007245.1.19",
          "form": "한탕해야지~",
          "original_form": "한탕해야지~",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007245.1.20",
          "form": "낯가리는데..",
          "original_form": "낯가리는데..",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007245.1.21",
          "form": "이럴수가 친구한테 영업 ㄱㄱ",
          "original_form": "이럴수가 친구한테 영업 ㄱㄱ",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007245.1.22",
          "form": "아니 우리 식음료 이야기해여하는데",
          "original_form": "아니 우리 식음료 이야기해여하는데",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007245.1.23",
          "form": "다른 이야기했네",
          "original_form": "다른 이야기했네",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007245.1.24",
          "form": "우동먹고싳픔",
          "original_form": "우동먹고싳픔",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007245.1.25",
          "form": "난 라볶애",
          "original_form": "난 라볶애",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007245.1.26",
          "form": "맛잇겟다",
          "original_form": "맛잇겟다",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007245.1.27",
          "form": "오늘 냉면먹었는데",
          "original_form": "오늘 냉면먹었는데",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007245.1.28",
          "form": "또 냉면먹고싶음",
          "original_form": "또 냉면먹고싶음",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007245.1.29",
          "form": "장난하나..",
          "original_form": "장난하나..",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007245.1.30",
          "form": "넌 냉면중독이야..",
          "original_form": "넌 냉면중독이야..",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007245.1.31",
          "form": "난 갈비천왕 시켰는데 남김",
          "original_form": "난 갈비천왕 시켰는데 남김",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007245.1.32",
          "form": "그런 호화로운 행위를? 나주랗",
          "original_form": "그런 호화로운 행위를? 나주랗",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007245.1.33",
          "form": "낼 먹읗거임 ㅎㅎ",
          "original_form": "낼 먹읗거임 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007245.1.34",
          "form": "밥 비벼서 ㅎㅎ",
          "original_form": "밥 비벼서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007245.1.35",
          "form": "아 맛잘알",
          "original_form": "아 맛잘알",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007245.1.36",
          "form": "놀라운사실하나말해줌",
          "original_form": "놀라운사실하나말해줌",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007245.1.37",
          "form": "나 갈비천왕안머거봄",
          "original_form": "나 갈비천왕안머거봄",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007245.1.38",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007245.1.39",
          "form": "그거 밥 비벼먹으면 진짜야",
          "original_form": "그거 밥 비벼먹으면 진짜야",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007245.1.40",
          "form": "내가 놓치다니억울함",
          "original_form": "내가 놓치다니억울함",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007245.1.41",
          "form": "인생 손해보는 중",
          "original_form": "인생 손해보는 중",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007245.1.42",
          "form": "밥도둑임",
          "original_form": "밥도둑임",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007245.1.43",
          "form": "밥비벼먹으면 졸은 치킨 하나더 있지아뇨나??",
          "original_form": "밥비벼먹으면 졸은 치킨 하나더 있지아뇨나??",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007245.1.44",
          "form": "모지??",
          "original_form": "모지??",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007245.1.45",
          "form": "모르겟는디...",
          "original_form": "모르겟는디...",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007245.1.46",
          "form": "지코바?",
          "original_form": "지코바?",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007245.1.47",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007245.1.48",
          "form": "이걱도 못먹어봄",
          "original_form": "이걱도 못먹어봄",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007245.1.49",
          "form": "아쉽..",
          "original_form": "아쉽..",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007245.1.50",
          "form": "나도 안 먹어봄",
          "original_form": "나도 안 먹어봄",
          "speaker_id": "2",
          "time": "20210822 01:19"
        }
      ]
    }
  ]
}
