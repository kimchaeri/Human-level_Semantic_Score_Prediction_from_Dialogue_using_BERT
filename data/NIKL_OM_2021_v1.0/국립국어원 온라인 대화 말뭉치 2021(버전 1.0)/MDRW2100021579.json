      "utterance": [
        {
          "id": "MDRW2100021579.1.1",
          "form": "가족은 화목한 편이에요?",
          "original_form": "가족은 화목한 편이에요?",
          "speaker_id": "1",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100021579.1.2",
          "form": "나름 화목해요!",
          "original_form": "나름 화목해요!",
          "speaker_id": "2",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100021579.1.3",
          "form": "name1은요??",
          "original_form": "&name1&은요??",
          "speaker_id": "2",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100021579.1.4",
          "form": "저희 집은 좀 다툼이 잦은 편이네요 ㅜㅜ",
          "original_form": "저희 집은 좀 다툼이 잦은 편이네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021579.1.5",
          "form": "아무래도 그런건 빠질 수 없죠ㅜ",
          "original_form": "아무래도 그런건 빠질 수 없죠ㅜ",
          "speaker_id": "2",
          "time": "20210828 20:48"
        },
        {
          "id": "MDRW2100021579.1.6",
          "form": "ㅠㅠㅜ 가족의 소중함 느껴본 적 있어요?",
          "original_form": "ㅠㅠㅜ 가족의 소중함 느껴본 적 있어요?",
          "speaker_id": "1",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021579.1.7",
          "form": "아무래도 아플때가 큰거같아요",
          "original_form": "아무래도 아플때가 큰거같아요",
          "speaker_id": "2",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021579.1.8",
          "form": "이렇게 걱정해주는 가족이 있구나 하면서요",
          "original_form": "이렇게 걱정해주는 가족이 있구나 하면서요",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021579.1.9",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021579.1.10",
          "form": "앗 ㅎㅎ 저는 급전 필요할 때요 ㅋㅋㅋ",
          "original_form": "앗 ㅎㅎ 저는 급전 필요할 때요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100021579.1.11",
          "form": "전 아프면 숨기는 타입이라서 ㅜㅜ위로는 못받아봈어요",
          "original_form": "전 아프면 숨기는 타입이라서 ㅜㅜ위로는 못받아봈어요",
          "speaker_id": "1",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100021579.1.12",
          "form": "앗ㅋㅋㅋ 그때도 있죠ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋ 그때도 있죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100021579.1.13",
          "form": "숨기시는 편이시구나ㅠ",
          "original_form": "숨기시는 편이시구나ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021579.1.14",
          "form": "저는 알아줘!!! 하는 편이라서요ㅋㅋ",
          "original_form": "저는 알아줘!!! 하는 편이라서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021579.1.15",
          "form": "ㅋㅋㅋ아프면 왜 아프냐고 덜컥 화를내셔서요..",
          "original_form": "ㅋㅋㅋ아프면 왜 아프냐고 덜컥 화를내셔서요..",
          "speaker_id": "1",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100021579.1.16",
          "form": "그래서 다 낫고나서 한참 뒤에 얘기하긴해요",
          "original_form": "그래서 다 낫고나서 한참 뒤에 얘기하긴해요",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021579.1.17",
          "form": "앗.. 서운하지 않으세요?",
          "original_form": "앗.. 서운하지 않으세요?",
          "speaker_id": "2",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021579.1.18",
          "form": "ㅋㅋㅋㅋ서운하기보단 좀 억울하더라구요",
          "original_form": "ㅋㅋㅋㅋ서운하기보단 좀 억울하더라구요",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021579.1.19",
          "form": "내가 아프고 싶어서 아팠냐고요 ㅜㅜ",
          "original_form": "내가 아프고 싶어서 아팠냐고요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021579.1.20",
          "form": "아 그쵸그쵸",
          "original_form": "아 그쵸그쵸",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021579.1.21",
          "form": "저는 그러면 말 안해요ㅋㅋ크ㅠ",
          "original_form": "저는 그러면 말 안해요ㅋㅋ크ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021579.1.22",
          "form": "ㅎㅎ그럼 가족들이 본인 많이 좋아해주는 편이에요?",
          "original_form": "ㅎㅎ그럼 가족들이 본인 많이 좋아해주는 편이에요?",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021579.1.23",
          "form": "음.. 그건 잘모르겠어요ㅋㅋㅋㅋ",
          "original_form": "음.. 그건 잘모르겠어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021579.1.24",
          "form": "어려운 질문이네욥..",
          "original_form": "어려운 질문이네욥..",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021579.1.25",
          "form": "오냐오냐해주시는건 있으시긴해요ㅋㅋ",
          "original_form": "오냐오냐해주시는건 있으시긴해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021579.1.26",
          "form": "ㅋㅋㅋ의견 잘 들어주고 그런거요~",
          "original_form": "ㅋㅋㅋ의견 잘 들어주고 그런거요~",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100021579.1.27",
          "form": "저는 예전에 찬밥 신세였는데 요즘은 잘해주시네요",
          "original_form": "저는 예전에 찬밥 신세였는데 요즘은 잘해주시네요",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100021579.1.28",
          "form": "아! 목소리 큰 사람이 이긴다랄까요..ㅋㅋ",
          "original_form": "아! 목소리 큰 사람이 이긴다랄까요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021579.1.29",
          "form": "가부장적인게 있어서 저도 최근에나 괜찮아졌어요",
          "original_form": "가부장적인게 있어서 저도 최근에나 괜찮아졌어요",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021579.1.30",
