      "utterance": [
        {
          "id": "MDRW2100029971.1.1",
          "form": "안녕하세요 ㅎ.ㅎ",
          "original_form": "안녕하세요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029971.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100029971.1.3",
          "form": "혹시 이번 올림픽 보셨나요?",
          "original_form": "혹시 이번 올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100029971.1.4",
          "form": "아 저 스포츠를 별로 안좋아해서 안보고 그냥 유튜브 돌아다니는거요",
          "original_form": "아 저 스포츠를 별로 안좋아해서 안보고 그냥 유튜브 돌아다니는거요",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029971.1.5",
          "form": "양궁잘하더라고요",
          "original_form": "양궁잘하더라고요",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029971.1.6",
          "form": "아하 양궁 재밌었죠",
          "original_form": "아하 양궁 재밌었죠",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100029971.1.7",
          "form": "그럼 주변에 스포츠 좋아하시는 분들도 없나요?",
          "original_form": "그럼 주변에 스포츠 좋아하시는 분들도 없나요?",
          "speaker_id": "1",
          "time": "20210903 18:02"
        },
        {
          "id": "MDRW2100029971.1.8",
          "form": "친구가 야구 좋아하던데",
          "original_form": "친구가 야구 좋아하던데",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100029971.1.9",
          "form": "한화요",
          "original_form": "한화요",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100029971.1.10",
          "form": "예곧님은요?",
          "original_form": "예곧님은요?",
          "speaker_id": "2",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100029971.1.11",
          "form": "앗 저는 야구는 잘 모르구요",
          "original_form": "앗 저는 야구는 잘 모르구요",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100029971.1.12",
          "form": "여자배구 관심있어요 ㅎㅎ",
          "original_form": "여자배구 관심있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:12"
        },
        {
          "id": "MDRW2100029971.1.13",
          "form": "아 김연경 선수 멋있죠!!",
          "original_form": "아 김연경 선수 멋있죠!!",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100029971.1.14",
          "form": "배구도 사실 잘 안보긴했는데",
          "original_form": "배구도 사실 잘 안보긴했는데",
          "speaker_id": "2",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100029971.1.15",
          "form": "이번에 은퇴하셨다면서요ㅠㅠ",
          "original_form": "이번에 은퇴하셨다면서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100029971.1.16",
          "form": "맞아요..ㅠㅠ 개인적으로는 선수 생활 계속 하는데",
          "original_form": "맞아요..ㅠㅠ 개인적으로는 선수 생활 계속 하는데",
          "speaker_id": "1",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100029971.1.17",
          "form": "국가대표는 이제 출전 안하신다구..",
          "original_form": "국가대표는 이제 출전 안하신다구..",
          "speaker_id": "1",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100029971.1.18",
          "form": "그죠ㅠㅜ",
          "original_form": "그죠ㅠㅜ",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100029971.1.19",
          "form": "아쉽네요ㅠㅠ",
          "original_form": "아쉽네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100029971.1.20",
          "form": "그 다른분도 엄청 핫하시던데 누구지",
          "original_form": "그 다른분도 엄청 핫하시던데 누구지",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100029971.1.21",
          "form": "김희진 선수..?",
          "original_form": "김희진 선수..?",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100029971.1.22",
          "form": "김희진 선수가 좀 핫했죠",
          "original_form": "김희진 선수가 좀 핫했죠",
          "speaker_id": "1",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100029971.1.23",
