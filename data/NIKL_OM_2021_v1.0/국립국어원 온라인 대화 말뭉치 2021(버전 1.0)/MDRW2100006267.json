        {
          "id": "MDRW2100006267.1.1",
          "form": "리유저블컵 쓰시나요?",
          "original_form": "리유저블컵 쓰시나요?",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006267.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006267.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006267.1.4",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006267.1.5",
          "form": "저는 자주 써요",
          "original_form": "저는 자주 써요",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006267.1.6",
          "form": "정말요? 댜박",
          "original_form": "정말요? 댜박",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006267.1.7",
          "form": "커피숍 가섣ㅎ 써요",
          "original_form": "커피숍 가섣ㅎ 써요",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006267.1.8",
          "form": "저도 예요",
          "original_form": "저도 예요",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006267.1.9",
          "form": "커피 자주 담아 마셔요",
          "original_form": "커피 자주 담아 마셔요",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006267.1.10",
          "form": "꿀꺽하면",
          "original_form": "꿀꺽하면",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006267.1.11",
          "form": "똥 잘 나올 것 같죠",
          "original_form": "똥 잘 나올 것 같죠",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006267.1.12",
          "form": "커피는 똥잘싸는데 아무런 도움을 주지 않아요",
          "original_form": "커피는 똥잘싸는데 아무런 도움을 주지 않아요",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006267.1.13",
          "form": "전 잘 나오던데?",
          "original_form": "전 잘 나오던데?",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006267.1.14",
          "form": "님 변비세요?",
          "original_form": "님 변비세요?",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006267.1.15",
          "form": "수면에도 도움이 되지 않아요",
          "original_form": "수면에도 도움이 되지 않아요",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006267.1.16",
          "form": "그건 너가 예왼거에요",
          "original_form": "그건 너가 예왼거에요",
