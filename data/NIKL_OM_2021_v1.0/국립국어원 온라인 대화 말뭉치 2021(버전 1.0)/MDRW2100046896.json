      "utterance": [
        {
          "id": "MDRW2100046896.1.1",
          "form": "재난징원금 신청했나",
          "original_form": "재난징원금 신청했나",
          "speaker_id": "1",
          "time": "20211001 18:35"
        },
        {
          "id": "MDRW2100046896.1.2",
          "form": "어했다ㅋㅋ",
          "original_form": "어했다ㅋㅋ",
          "speaker_id": "2",
          "time": "20211001 18:36"
        },
        {
          "id": "MDRW2100046896.1.3",
          "form": "아라띠ㅡ",
          "original_form": "아라띠ㅡ",
          "speaker_id": "1",
          "time": "20211001 18:36"
        },
        {
          "id": "MDRW2100046896.1.4",
          "form": "얼마신청해야되는가",
          "original_form": "얼마신청해야되는가",
          "speaker_id": "1",
          "time": "20211001 18:36"
        },
        {
          "id": "MDRW2100046896.1.5",
          "form": "얼마아니고25만원이잖아",
          "original_form": "얼마아니고25만원이잖아",
          "speaker_id": "2",
          "time": "20211001 18:36"
        },
        {
          "id": "MDRW2100046896.1.6",
          "form": "그렇군 얼른신청해야지",
          "original_form": "그렇군 얼른신청해야지",
          "speaker_id": "1",
          "time": "20211001 18:37"
        },
        {
          "id": "MDRW2100046896.1.7",
          "form": "신청해서 학원비내야된다",
          "original_form": "신청해서 학원비내야된다",
          "speaker_id": "2",
          "time": "20211001 18:37"
        },
        {
          "id": "MDRW2100046896.1.8",
          "form": "학원비가 얼만데?",
          "original_form": "학원비가 얼만데?",
          "speaker_id": "1",
          "time": "20211001 18:37"
        },
        {
          "id": "MDRW2100046896.1.9",
          "form": "학원비 수업마다다르지",
          "original_form": "학원비 수업마다다르지",
          "speaker_id": "2",
          "time": "20211001 18:37"
        },
        {
          "id": "MDRW2100046896.1.10",
          "form": "금액도다르고",
          "original_form": "금액도다르고",
          "speaker_id": "2",
          "time": "20211001 18:37"
        },
        {
          "id": "MDRW2100046896.1.11",
          "form": "그렇군 얼른 카드로신청해야겠디ㅣ",
          "original_form": "그렇군 얼른 카드로신청해야겠디ㅣ",
          "speaker_id": "1",
          "time": "20211001 18:37"
        },
        {
          "id": "MDRW2100046896.1.12",
          "form": "빨리해라 이제는 순서대로안해도된다",
          "original_form": "빨리해라 이제는 순서대로안해도된다",
          "speaker_id": "2",
          "time": "20211001 18:38"
        },
        {
          "id": "MDRW2100046896.1.13",
          "form": "그럼 아무카드나 해야겠네",
          "original_form": "그럼 아무카드나 해야겠네",
          "speaker_id": "1",
          "time": "20211001 18:38"
        },
        {
          "id": "MDRW2100046896.1.14",
          "form": "그렇지ㅋㅋㅋ 아무카드나 하나 선정해서신청",
          "original_form": "그렇지ㅋㅋㅋ 아무카드나 하나 선정해서신청",
          "speaker_id": "2",
          "time": "20211001 18:38"
        },
        {
          "id": "MDRW2100046896.1.15",
          "form": "오케이 재난지원금 우리는 다 합치면 얼마지?",
          "original_form": "오케이 재난지원금 우리는 다 합치면 얼마지?",
          "speaker_id": "1",
          "time": "20211001 18:39"
        },
        {
          "id": "MDRW2100046896.1.16",
          "form": "그건 뭐 일인단25만원이지",
          "original_form": "그건 뭐 일인단25만원이지",
          "speaker_id": "2",
          "time": "20211001 18:39"
        },
        {
          "id": "MDRW2100046896.1.17",
          "form": "일인당 25만원 그래",
          "original_form": "일인당 25만원 그래",
          "speaker_id": "1",
          "time": "20211001 18:39"
        },
        {
          "id": "MDRW2100046896.1.18",
          "form": "12월달 안에 소비해야된다",
          "original_form": "12월달 안에 소비해야된다",
          "speaker_id": "2",
          "time": "20211001 18:40"
        },
        {
          "id": "MDRW2100046896.1.19",
          "form": "그래 그건알지 12월안에 다 쓸수있음",
          "original_form": "그래 그건알지 12월안에 다 쓸수있음",
          "speaker_id": "1",
          "time": "20211001 18:40"
        },
        {
          "id": "MDRW2100046896.1.20",
          "form": "ㅋㅋ 맞다 학원비내면 끝이다",
          "original_form": "ㅋㅋ 맞다 학원비내면 끝이다",
          "speaker_id": "2",
          "time": "20211001 18:40"
        },
        {
          "id": "MDRW2100046896.1.21",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "2",
          "time": "20211001 18:40"
        },
        {
          "id": "MDRW2100046896.1.22",
          "form": "자전거도사고 해야지",
          "original_form": "자전거도사고 해야지",
          "speaker_id": "1",
          "time": "20211001 18:40"
        },
        {
          "id": "MDRW2100046896.1.23",
          "form": "다른거필요한것도",
          "original_form": "다른거필요한것도",
          "speaker_id": "1",
          "time": "20211001 18:41"
        },
        {
          "id": "MDRW2100046896.1.24",
          "form": "그래 뭐필요한데? 다른사람은 텐트산다던데",
          "original_form": "그래 뭐필요한데? 다른사람은 텐트산다던데",
          "speaker_id": "2",
          "time": "20211001 18:41"
        },
        {
          "id": "MDRW2100046896.1.25",
          "form": "비싼데 텐트사면끝나겠네",
          "original_form": "비싼데 텐트사면끝나겠네",
          "speaker_id": "1",
          "time": "20211001 18:41"
        },
        {
          "id": "MDRW2100046896.1.26",
          "form": "그렇지 우리는 일단 고민해보자",
          "original_form": "그렇지 우리는 일단 고민해보자",
          "speaker_id": "2",
          "time": "20211001 18:41"
        },
        {
          "id": "MDRW2100046896.1.27",
          "form": "오케이 뭐할지 먼저생각하겠음",
          "original_form": "오케이 뭐할지 먼저생각하겠음",
          "speaker_id": "1",
          "time": "20211001 18:42"
        },
        {
          "id": "MDRW2100046896.1.28",
          "form": "나도 내필요한거사도되나",
          "original_form": "나도 내필요한거사도되나",
          "speaker_id": "2",
          "time": "20211001 18:44"
        },
        {
          "id": "MDRW2100046896.1.29",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "2",
          "time": "20211001 18:44"
        },
        {
          "id": "MDRW2100046896.1.30",
          "form": "그래 뭐 그러던지",
          "original_form": "그래 뭐 그러던지",
          "speaker_id": "1",
          "time": "20211001 18:44"
        },
        {
          "id": "MDRW2100046896.1.31",
          "form": "내필요한거산다고",
          "original_form": "내필요한거산다고",
          "speaker_id": "2",
          "time": "20211001 18:45"
        },
        {
          "id": "MDRW2100046896.1.32",
          "form": "알겠다고 어휴",
          "original_form": "알겠다고 어휴",
          "speaker_id": "1",
          "time": "20211001 18:45"
        },
        {
          "id": "MDRW2100046896.1.33",
          "form": "오케이",
          "original_form": "오케이",
          "speaker_id": "1",
          "time": "20211001 18:45"
        },
        {
          "id": "MDRW2100046896.1.34",
