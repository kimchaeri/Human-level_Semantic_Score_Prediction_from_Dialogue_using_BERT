      "utterance": [
        {
          "id": "MDRW2100036707.1.1",
          "form": "어 ㄹㅋㅋ",
          "original_form": "어 ㄹㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:50"
        },
        {
          "id": "MDRW2100036707.1.2",
          "form": "잘려고 다같이 누웠는데 혼자 막 로봇놀이,,괴물놀이..",
          "original_form": "잘려고 다같이 누웠는데 혼자 막 로봇놀이,,괴물놀이..",
          "speaker_id": "1",
          "time": "20210831 09:50"
        },
        {
          "id": "MDRW2100036707.1.3",
          "form": "신랑이 진짜 궁디 맴매할뻔..",
          "original_form": "신랑이 진짜 궁디 맴매할뻔..",
          "speaker_id": "1",
          "time": "20210831 09:51"
        },
        {
          "id": "MDRW2100036707.1.4",
          "form": "혼자 막 힘이 넘쳐가 우아!!!!!!이러고있고..",
          "original_form": "혼자 막 힘이 넘쳐가 우아!!!!!!이러고있고..",
          "speaker_id": "1",
          "time": "20210831 09:51"
        },
        {
          "id": "MDRW2100036707.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㄱㅋㄱㅋㅋㅋㅋㅋㄱㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋㄱㅋㄱㅋㅋㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210831 09:51"
        },
        {
          "id": "MDRW2100036707.1.6",
          "form": "파이팅 넘친다 진짜",
          "original_form": "파이팅 넘친다 진짜",
          "speaker_id": "2",
          "time": "20210831 09:51"
        },
        {
          "id": "MDRW2100036707.1.7",
          "form": "아들이라 또 에너지가 넘치네",
          "original_form": "아들이라 또 에너지가 넘치네",
          "speaker_id": "2",
          "time": "20210831 09:52"
        },
        {
          "id": "MDRW2100036707.1.8",
          "form": "그러니깐 ㅋㅋ 너무 피곤하다. ㅋㅋㅋ",
          "original_form": "그러니깐 ㅋㅋ 너무 피곤하다. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:52"
        },
        {
          "id": "MDRW2100036707.1.9",
          "form": "첫째는 그래도",
          "original_form": "첫째는 그래도",
          "speaker_id": "1",
          "time": "20210831 09:52"
        },
        {
          "id": "MDRW2100036707.1.10",
          "form": "막 기절하듯이 잠들거든 ㅋㅋ",
          "original_form": "막 기절하듯이 잠들거든 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:52"
        },
        {
          "id": "MDRW2100036707.1.11",
          "form": "둘째는 영 잠이없는건지...아님 에너지가 덜 분출된건지....",
          "original_form": "둘째는 영 잠이없는건지...아님 에너지가 덜 분출된건지....",
          "speaker_id": "1",
          "time": "20210831 09:52"
        },
        {
          "id": "MDRW2100036707.1.12",
          "form": "매일 둘째 잠 재우다가 내가 먼저 든다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "매일 둘째 잠 재우다가 내가 먼저 든다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:52"
        },
        {
          "id": "MDRW2100036707.1.13",
          "form": "그래 우리집도 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래 우리집도 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100036707.1.14",
          "form": "name3은 기절",
          "original_form": "&name3&은 기절",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100036707.1.15",
          "form": "name4는 뒹굴 ㅋㅋㅋㅋ",
          "original_form": "&name4&는 뒹굴 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100036707.1.16",
          "form": "나도 요새는 피곤하니까 그냥 누워서 내가 먼저 잠듦 ㅋㅋ",
          "original_form": "나도 요새는 피곤하니까 그냥 누워서 내가 먼저 잠듦 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100036707.1.17",
          "form": "맞어 맞어 ㅋㅋ 언니 피곤할듯 ㅋㅋ",
          "original_form": "맞어 맞어 ㅋㅋ 언니 피곤할듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:04"
        },
        {
          "id": "MDRW2100036707.1.18",
          "form": "너무 빡빡하다 ㅋㅋ",
          "original_form": "너무 빡빡하다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:04"
        },
        {
          "id": "MDRW2100036707.1.19",
          "form": "엉 ㅎㅎ 오늘은 언어센터가는날",
          "original_form": "엉 ㅎㅎ 오늘은 언어센터가는날",
          "speaker_id": "2",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100036707.1.20",
          "form": "그래도 name4가 좋아하니 가는데",
          "original_form": "그래도 &name4&가 좋아하니 가는데",
          "speaker_id": "2",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100036707.1.21",
          "form": "왔다갔다 귀찮다이",
          "original_form": "왔다갔다 귀찮다이",
          "speaker_id": "2",
          "time": "20210831 10:05"
        },
        {
          "id": "MDRW2100036707.1.22",
          "form": "ㅋㅋ 맞어 오늘 센터 가는날이구만 ㅋㅋ",
          "original_form": "ㅋㅋ 맞어 오늘 센터 가는날이구만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:06"
        },
        {
          "id": "MDRW2100036707.1.23",
          "form": "언니 일정도 진짜 바쁘다 일도 하구 애들도 보고 ㅋㅋ",
          "original_form": "언니 일정도 진짜 바쁘다 일도 하구 애들도 보고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:06"
        },
        {
          "id": "MDRW2100036707.1.24",
          "form": "나는 오늘, 내일, 모레까지 쉬는날 ㅋㅋㅋ 히히",
          "original_form": "나는 오늘, 내일, 모레까지 쉬는날 ㅋㅋㅋ 히히",
          "speaker_id": "1",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100036707.1.25",
          "form": "오 좋다이 \r\n요새는 그럼 월금 만 가나??",
          "original_form": "오 좋다이 \r\n요새는 그럼 월금 만 가나??",
          "speaker_id": "2",
          "time": "20210831 10:08"
        },
        {
          "id": "MDRW2100036707.1.26",
          "form": "응응 월 금만간다ㅋ",
          "original_form": "응응 월 금만간다ㅋ",
          "speaker_id": "1",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100036707.1.27",
          "form": "좋구먼 ㅎㅎ",
          "original_form": "좋구먼 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036707.1.28",
          "form": "쉴때 뭐하노 ㅎㅎ",
          "original_form": "쉴때 뭐하노 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036707.1.29",
          "form": "쉴때 ㅋㅋㅋ 이벤트하고 ㅋㅋㅋㅋ 공모주관련해서 공부하고 ㅋㅋ집안일하고 밥먹고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "쉴때 ㅋㅋㅋ 이벤트하고 ㅋㅋㅋㅋ 공모주관련해서 공부하고 ㅋㅋ집안일하고 밥먹고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100036707.1.30",
          "form": "이리 저리 근데 이벤트 많이 한다 ㅋㅋㅋ 잼있다. 용돈벌이 되니깐",
          "original_form": "이리 저리 근데 이벤트 많이 한다 ㅋㅋㅋ 잼있다. 용돈벌이 되니깐",
          "speaker_id": "1",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100036707.1.31",
          "form": "좋다 나도 공모주 공부하고 싶다",
          "original_form": "좋다 나도 공모주 공부하고 싶다",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100036707.1.32",
          "form": "하루 날 잡아서 좀 가르쳐도",
          "original_form": "하루 날 잡아서 좀 가르쳐도",
          "speaker_id": "2",
          "time": "20210831 10:33"
        },
        {
          "id": "MDRW2100036707.1.33",
          "form": "무슨말인지 일단 감은 잡아야 할수 있으니",
          "original_form": "무슨말인지 일단 감은 잡아야 할수 있으니",
