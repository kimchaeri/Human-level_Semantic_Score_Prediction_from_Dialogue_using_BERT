      "utterance": [
        {
          "id": "MDRW2100019856.1.1",
          "form": "다시여름이에요ㅠ",
          "original_form": "다시여름이에요ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019856.1.2",
          "form": "맞아요 다시 엄청 습하네요 ㅠㅠ",
          "original_form": "맞아요 다시 엄청 습하네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019856.1.3",
          "form": "그쵸 여기도 그래요",
          "original_form": "그쵸 여기도 그래요",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100019856.1.4",
          "form": "name1님은 겨울 좋아하세요?",
          "original_form": "&name1&님은 겨울 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019856.1.5",
          "form": "네 저는 가을겨울",
          "original_form": "네 저는 가을겨울",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019856.1.6",
          "form": "가을겨울을 좋아하시는 이유가 더운걸 싫어하셔서인가요?",
          "original_form": "가을겨울을 좋아하시는 이유가 더운걸 싫어하셔서인가요?",
          "speaker_id": "2",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100019856.1.7",
          "form": "맞아요 땀나고 끈적한거 너무 싫어요",
          "original_form": "맞아요 땀나고 끈적한거 너무 싫어요",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019856.1.8",
          "form": "저랑 정반대시네요 ㅋㅋㅋㅋㅋ",
          "original_form": "저랑 정반대시네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019856.1.9",
          "form": "전 추운게 너무 싫어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 추운게 너무 싫어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019856.1.10",
          "form": "그쵸 저는 추운게 나아요ㅠ",
          "original_form": "그쵸 저는 추운게 나아요ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019856.1.11",
          "form": "더위도 물론 싫지만 추위를 너무 타서 ㅋㅋㅋㅋㅋ",
          "original_form": "더위도 물론 싫지만 추위를 너무 타서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019856.1.12",
          "form": "봄가을이 길면 얼마나 좋을까요 ㅠㅠ",
          "original_form": "봄가을이 길면 얼마나 좋을까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019856.1.13",
          "form": "그쵸 점점 여름이 길어져요",
          "original_form": "그쵸 점점 여름이 길어져요",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019856.1.14",
          "form": "여름이랑 겨울만 너무 긴것같아요 날씨가너무 극단적ㅋㅋㅋㅋ",
          "original_form": "여름이랑 겨울만 너무 긴것같아요 날씨가너무 극단적ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019856.1.15",
          "form": "이상기온큰일이에요",
          "original_form": "이상기온큰일이에요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019856.1.16",
          "form": "맞아요 요즘은 진짜 무서워요",
          "original_form": "맞아요 요즘은 진짜 무서워요",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019856.1.17",
          "form": "그쵸 무서워요 정말",
          "original_form": "그쵸 무서워요 정말",
          "speaker_id": "1",
