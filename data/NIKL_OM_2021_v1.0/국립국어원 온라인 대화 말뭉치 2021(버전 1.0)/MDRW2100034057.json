      "utterance": [
        {
          "id": "MDRW2100034057.1.1",
          "form": "최근 들은 신곡이 있으신가요?",
          "original_form": "최근 들은 신곡이 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100034057.1.2",
          "form": "태연의 위켄드 듣고 있어요",
          "original_form": "태연의 위켄드 듣고 있어요",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100034057.1.3",
          "form": "태연 음색이 돋보적이죠ㅠ",
          "original_form": "태연 음색이 돋보적이죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100034057.1.4",
          "form": "맞아요. 어떤 곡이든 잘 소화하구요.",
          "original_form": "맞아요. 어떤 곡이든 잘 소화하구요.",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100034057.1.5",
          "form": "사계라는 곡도 엄청 좋아했거든요",
          "original_form": "사계라는 곡도 엄청 좋아했거든요",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100034057.1.6",
          "form": "저도 사계 엄청 들었어요! 저는 개인적으로 들리나요? 이후로 가장 좋았어요",
          "original_form": "저도 사계 엄청 들었어요! 저는 개인적으로 들리나요? 이후로 가장 좋았어요",
          "speaker_id": "2",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100034057.1.7",
          "form": "ㅋㅋㅋ비긴어게인 나왔을때도 너무 좋더라구요",
          "original_form": "ㅋㅋㅋ비긴어게인 나왔을때도 너무 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100034057.1.8",
          "form": "맞아요ㅋㅋㅋ 특유의 분위기가 좋았어요",
          "original_form": "맞아요ㅋㅋㅋ 특유의 분위기가 좋았어요",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100034057.1.9",
          "form": "전 불티도 좋더라구요 강한 느낌ㅋㅋㅋ",
          "original_form": "전 불티도 좋더라구요 강한 느낌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100034057.1.10",
          "form": "어느 분위기든 다 잘어울려서요ㅋㅋㅋ",
          "original_form": "어느 분위기든 다 잘어울려서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100034057.1.11",
          "form": "요번에 소녀시대 컴백한다고 하던데요",
          "original_form": "요번에 소녀시대 컴백한다고 하던데요",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100034057.1.12",
          "form": "정말요? 언제쯤에요?",
          "original_form": "정말요? 언제쯤에요?",
          "speaker_id": "2",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100034057.1.13",
          "form": "모르겠어요. 한다는 소식만 들어서요ㅋㅋ",
          "original_form": "모르겠어요. 한다는 소식만 들어서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100034057.1.14",
          "form": "찾아봐야겠네요",
          "original_form": "찾아봐야겠네요",
          "speaker_id": "2",
