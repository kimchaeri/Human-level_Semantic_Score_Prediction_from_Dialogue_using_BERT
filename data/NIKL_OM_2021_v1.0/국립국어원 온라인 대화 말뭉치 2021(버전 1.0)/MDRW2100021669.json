      "utterance": [
        {
          "id": "MDRW2100021669.1.1",
          "form": "전 좋아해요 넘 편해요",
          "original_form": "전 좋아해요 넘 편해요",
          "speaker_id": "2",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100021669.1.2",
          "form": "인터넷 쇼핑요?",
          "original_form": "인터넷 쇼핑요?",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100021669.1.3",
          "form": "오류가 잘 나네요",
          "original_form": "오류가 잘 나네요",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100021669.1.4",
          "form": "스케쳐스 신발이요",
          "original_form": "스케쳐스 신발이요",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100021669.1.5",
          "form": "아하 ㅋㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100021669.1.6",
          "form": "스케쳐스 신발 편하죠 저희 엄마도 그거만 신으시더라구요",
          "original_form": "스케쳐스 신발 편하죠 저희 엄마도 그거만 신으시더라구요",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100021669.1.7",
          "form": "근데 디자인이 쪼끔 아쉬워요ㅠ",
          "original_form": "근데 디자인이 쪼끔 아쉬워요ㅠ",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100021669.1.8",
          "form": "맞아요 좀 투박해요 그래도 살 거 아니까 그러는 것 같기도 해요ㅠㅋㅋ",
          "original_form": "맞아요 좀 투박해요 그래도 살 거 아니까 그러는 것 같기도 해요ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100021669.1.9",
          "form": "ㅋㅋㅋㅋ 신는 사람은 그것만 신더라구요",
          "original_form": "ㅋㅋㅋㅋ 신는 사람은 그것만 신더라구요",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100021669.1.10",
          "form": "네 제가 바로 그 사람입니다 ㅋㅋ 이번에 새로 나온 거 사고픈데 가격이 안 내리네요",
          "original_form": "네 제가 바로 그 사람입니다 ㅋㅋ 이번에 새로 나온 거 사고픈데 가격이 안 내리네요",
          "speaker_id": "2",
          "time": "20210830 07:23"
        },
        {
          "id": "MDRW2100021669.1.11",
          "form": "ㅋㅋㅋㅋㅋ 제 친구도 진짜 거의 10년동안 그것만 색바꿔가며 신어요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 제 친구도 진짜 거의 10년동안 그것만 색바꿔가며 신어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 07:29"
        },
        {
          "id": "MDRW2100021669.1.12",
          "form": "하도 그래서 얼마나 좋길래? 싶더라구요",
          "original_form": "하도 그래서 얼마나 좋길래? 싶더라구요",
          "speaker_id": "1",
          "time": "20210830 07:29"
        },
        {
          "id": "MDRW2100021669.1.13",
          "form": "걷기가 달라요 주로 운동화 샌들은 여기로 구매해요",
          "original_form": "걷기가 달라요 주로 운동화 샌들은 여기로 구매해요",
          "speaker_id": "2",
          "time": "20210830 07:31"
        },
        {
          "id": "MDRW2100021669.1.14",
          "form": "아 뉴발란스 샌들 이번에 구매해 봤어요 디자인 좋고 발도 편하데요 동생이ㅋㅋ",
          "original_form": "아 뉴발란스 샌들 이번에 구매해 봤어요 디자인 좋고 발도 편하데요 동생이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 07:32"
        },
        {
          "id": "MDRW2100021669.1.15",
          "form": "ㅋㅋ 저는 샌들은 테바 신어요!",
          "original_form": "ㅋㅋ 저는 샌들은 테바 신어요!",
          "speaker_id": "1",
          "time": "20210830 13:09"
        },
        {
          "id": "MDRW2100021669.1.16",
          "form": "밑에가 운동화 깔창처럼 생겨서 좋더라구요",
          "original_form": "밑에가 운동화 깔창처럼 생겨서 좋더라구요",
          "speaker_id": "1",
          "time": "20210830 13:09"
        },
        {
          "id": "MDRW2100021669.1.17",
          "form": "테바 유명하죠? 들어 본 것 같아요ㅎㅎ",
          "original_form": "테바 유명하죠? 들어 본 것 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100021669.1.18",
          "form": "나중에 샌들 살 때 고려해야 겠어요",
          "original_form": "나중에 샌들 살 때 고려해야 겠어요",
          "speaker_id": "2",
          "time": "20210830 16:20"
        },
        {
          "id": "MDRW2100021669.1.19",
          "form": "넹 유명해요 ㅋㅋㅋ 투박하긴 한데",
          "original_form": "넹 유명해요 ㅋㅋㅋ 투박하긴 한데",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100021669.1.20",
          "form": "그래두 편함이 압승.. 가격도 저렴하구 좋아요 나중에 한 번 츄라이 해보세여",
          "original_form": "그래두 편함이 압승.. 가격도 저렴하구 좋아요 나중에 한 번 츄라이 해보세여",
          "speaker_id": "1",
          "time": "20210830 18:43"
        },
        {
          "id": "MDRW2100021669.1.21",
          "form": "네 편하고 저렴하면 좋죠 추천 감사해여 ㅎㅎ",
          "original_form": "네 편하고 저렴하면 좋죠 추천 감사해여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:40"
        }
      ]
    }
