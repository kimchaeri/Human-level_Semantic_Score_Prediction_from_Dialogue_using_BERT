      "utterance": [
        {
          "id": "MDRW2100043826.1.1",
          "form": "저도 먹는낙으로 사는사람이라서",
          "original_form": "저도 먹는낙으로 사는사람이라서",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043826.1.2",
          "form": "공감합니닷",
          "original_form": "공감합니닷",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043826.1.3",
          "form": "좋아하는 음식이 뭐세요?",
          "original_form": "좋아하는 음식이 뭐세요?",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043826.1.4",
          "form": "저는 진짜 다 잘먹어욤",
          "original_form": "저는 진짜 다 잘먹어욤",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043826.1.5",
          "form": "없어서 못먹쥬",
          "original_form": "없어서 못먹쥬",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043826.1.6",
          "form": "흐흐 저는 제일 자주먹고 오래 먹어온 것은 불막창이에요 ㅎㅎ 청정원 안주야꺼",
          "original_form": "흐흐 저는 제일 자주먹고 오래 먹어온 것은 불막창이에요 ㅎㅎ 청정원 안주야꺼",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043826.1.7",
          "form": "안주야꺼",
          "original_form": "안주야꺼",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043826.1.8",
          "form": "먹어밧져",
          "original_form": "먹어밧져",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043826.1.9",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043826.1.10",
          "form": "정말 맛나요 ㅎㅎ 질리지가 않네요 ㅋㅋ양파 찹찹썰어서 볶볶",
          "original_form": "정말 맛나요 ㅎㅎ 질리지가 않네요 ㅋㅋ양파 찹찹썰어서 볶볶",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043826.1.11",
          "form": "ㅋㅋ맞아요",
          "original_form": "ㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043826.1.12",
          "form": "양파ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "양파ㅋㅋㅋㅋㅋㅋㅋ",
