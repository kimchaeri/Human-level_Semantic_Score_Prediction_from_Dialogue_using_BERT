      "utterance": [
        {
          "id": "MDRW2100031278.1.1",
          "form": "저희집도 그래요",
          "original_form": "저희집도 그래요",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031278.1.2",
          "form": "치킨집이랑 파리바게트가 끝 ㅠ",
          "original_form": "치킨집이랑 파리바게트가 끝 ㅠ",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031278.1.3",
          "form": "최근에 여행 가신적 있나요?",
          "original_form": "최근에 여행 가신적 있나요?",
          "speaker_id": "1",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031278.1.4",
          "form": "아뇨 없어용",
          "original_form": "아뇨 없어용",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031278.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031278.1.6",
          "form": "그쵸 코로나땜시 ㅜ",
          "original_form": "그쵸 코로나땜시 ㅜ",
          "speaker_id": "1",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031278.1.7",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031278.1.8",
          "form": "가고싶은데 겁쟁이라",
          "original_form": "가고싶은데 겁쟁이라",
          "speaker_id": "1",
          "time": "20210904 19:51"
        },
        {
          "id": "MDRW2100031278.1.9",
          "form": "ㅠㅠㅠ 저두요..",
          "original_form": "ㅠㅠㅠ 저두요..",
          "speaker_id": "2",
          "time": "20210904 19:52"
        },
        {
          "id": "MDRW2100031278.1.10",
          "form": "뮤지엄 산 가보셨어요?",
          "original_form": "뮤지엄 산 가보셨어요?",
          "speaker_id": "2",
          "time": "20210904 19:52"
        },
        {
          "id": "MDRW2100031278.1.11",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210904 19:52"
        },
        {
          "id": "MDRW2100031278.1.12",
          "form": "어디예요?",
          "original_form": "어디예요?",
          "speaker_id": "1",
          "time": "20210904 19:52"
        },
        {
          "id": "MDRW2100031278.1.13",
          "form": "원준가 그랬던거 같은데 예뻐서 한번 여쭤봤어요!ㅋㅋㅋㅋ",
          "original_form": "원준가 그랬던거 같은데 예뻐서 한번 여쭤봤어요!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:52"
        },
        {
          "id": "MDRW2100031278.1.14",
          "form": "오 예쁜곳 좋죠",
          "original_form": "오 예쁜곳 좋죠",
          "speaker_id": "1",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100031278.1.15",
          "form": "닉네임에 name1은 생일이세요?",
          "original_form": "닉네임에 &name1&은 생일이세요?",
          "speaker_id": "2",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100031278.1.16",
          "form": "마지막에 가신곳이 원주예요?",
          "original_form": "마지막에 가신곳이 원주예요?",
          "speaker_id": "1",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100031278.1.17",
          "form": "아뇨 아무의미 없어요 ㅋㄱㅋㄱ",
          "original_form": "아뇨 아무의미 없어요 ㅋㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100031278.1.18",
          "form": "아뇨아뇨 거기가 제일 가보고 싶어서요ㅋㅋㅋㅋ",
          "original_form": "아뇨아뇨 거기가 제일 가보고 싶어서요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100031278.1.19",
          "form": "저도 한번 가보고싶네요",
          "original_form": "저도 한번 가보고싶네요",
          "speaker_id": "1",
          "time": "20210904 19:53"
        },
        {
          "id": "MDRW2100031278.1.20",
          "form": "마지막으로 여행가신게 어디예요?",
          "original_form": "마지막으로 여행가신게 어디예요?",
          "speaker_id": "1",
          "time": "20210904 19:54"
        },
        {
          "id": "MDRW2100031278.1.21",
          "form": "앗 ㅋㅋㅋ오해했네요 사실 name1이 제가 좋아하는 아이돌 데뷔일이라 그 아이돌이 그 뮤지엄에 갔었거든요 그래서 혹시나 하구요 여기서 같은 팬들 많이 봐서 오해했어요 ㅠ ㅎㅎ",
          "original_form": "앗 ㅋㅋㅋ오해했네요 사실 &name1&이 제가 좋아하는 아이돌 데뷔일이라 그 아이돌이 그 뮤지엄에 갔었거든요 그래서 혹시나 하구요 여기서 같은 팬들 많이 봐서 오해했어요 ㅠ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 19:54"
        },
        {
          "id": "MDRW2100031278.1.22",
          "form": "저 마지막으로 갔던게 아마 부산이었어요",
          "original_form": "저 마지막으로 갔던게 아마 부산이었어요",
          "speaker_id": "2",
          "time": "20210904 19:55"
        },
        {
          "id": "MDRW2100031278.1.23",
          "form": "부산 멋지죠",
          "original_form": "부산 멋지죠",
          "speaker_id": "1",
          "time": "20210904 19:55"
        },
        {
          "id": "MDRW2100031278.1.24",
          "form": "19년이요!",
          "original_form": "19년이요!",
          "speaker_id": "2",
          "time": "20210904 19:55"
        },
        {
          "id": "MDRW2100031278.1.25",
          "form": "ㅋㅋㅋㅋㅋ",
