      "utterance": [
        {
          "id": "MDRW2100035658.1.1",
          "form": "아뇨 없어요",
          "original_form": "아뇨 없어요",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035658.1.2",
          "form": "님은 제습기 있으신가요?",
          "original_form": "님은 제습기 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035658.1.3",
          "form": "넹넹 ㅋㅋㅋ",
          "original_form": "넹넹 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035658.1.4",
          "form": "장마철에 너무 습해서",
          "original_form": "장마철에 너무 습해서",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035658.1.5",
          "form": "2년전쯤 샀는데",
          "original_form": "2년전쯤 샀는데",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035658.1.6",
          "form": "좋은가요?",
          "original_form": "좋은가요?",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035658.1.7",
          "form": "제습기 돌리면 진짜뽀송해요",
          "original_form": "제습기 돌리면 진짜뽀송해요",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.8",
          "form": "오..!",
          "original_form": "오..!",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.9",
          "form": "비오면 막 습하잖아용",
          "original_form": "비오면 막 습하잖아용",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.10",
          "form": "저는 대신 보일러 조금 돌려요",
          "original_form": "저는 대신 보일러 조금 돌려요",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.11",
          "form": "습기때문에 침구도 눅눅하고 ㅡ",
          "original_form": "습기때문에 침구도 눅눅하고 ㅡ",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.12",
          "form": "아 그것도 방법이겠네요",
          "original_form": "아 그것도 방법이겠네요",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.13",
          "form": "습기가 날아가고 곰팡이도 덜슬어요ㅋㅋ",
          "original_form": "습기가 날아가고 곰팡이도 덜슬어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.14",
          "form": "올해 장마올때 한시간정도 돌렸는데",
          "original_form": "올해 장마올때 한시간정도 돌렸는데",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.15",
          "form": "물통이 가득 찼더라구요",
          "original_form": "물통이 가득 찼더라구요",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.16",
          "form": "물먹는하마급이네요",
          "original_form": "물먹는하마급이네요",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.17",
          "form": "그게 다 공기중에 있는 습기였나봐요",
          "original_form": "그게 다 공기중에 있는 습기였나봐요",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.18",
          "form": "아 신기해요 ㅋㅋ",
          "original_form": "아 신기해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035658.1.19",
          "form": "그춍ㅋㅋㅋㅋ",
          "original_form": "그춍ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:04"
        }
      ]
