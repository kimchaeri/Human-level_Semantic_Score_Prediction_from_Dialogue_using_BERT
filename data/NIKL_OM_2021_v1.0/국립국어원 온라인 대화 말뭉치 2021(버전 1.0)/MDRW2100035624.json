      "utterance": [
        {
          "id": "MDRW2100035624.1.1",
          "form": "저는 비염이 있어서 키우지는 못하고",
          "original_form": "저는 비염이 있어서 키우지는 못하고",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035624.1.2",
          "form": "맨날 동영상으로 랜선 이모 하고있어요ㅠㅠ",
          "original_form": "맨날 동영상으로 랜선 이모 하고있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035624.1.3",
          "form": "개인적으로 말티즈를 너무 사랑해요ㅋㅋㅋ",
          "original_form": "개인적으로 말티즈를 너무 사랑해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035624.1.4",
          "form": "아쉽겠어요ㅜ",
          "original_form": "아쉽겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035624.1.5",
          "form": "참지않는 말티즈!",
          "original_form": "참지않는 말티즈!",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035624.1.6",
          "form": "하얀 말티즈들 넘 귀엽죠",
          "original_form": "하얀 말티즈들 넘 귀엽죠",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035624.1.7",
          "form": "저는 말티즈가 그런 성격인걸",
          "original_form": "저는 말티즈가 그런 성격인걸",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035624.1.8",
          "form": "나중에서야 알았어요",
          "original_form": "나중에서야 알았어요",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035624.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035624.1.10",
          "form": "충격이셨나요",
          "original_form": "충격이셨나요",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035624.1.11",
          "form": "삼촌이 말티즈를 키우셔서 저희집에 종종 맡기셨는데",
          "original_form": "삼촌이 말티즈를 키우셔서 저희집에 종종 맡기셨는데",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035624.1.12",
          "form": "짖는것도 딱 한번밖에 못보고",
          "original_form": "짖는것도 딱 한번밖에 못보고",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035624.1.13",
          "form": "그아이는 순했나요",
          "original_form": "그아이는 순했나요",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035624.1.14",
          "form": "진짜 너무 순했거든요",
          "original_form": "진짜 너무 순했거든요",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035624.1.15",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035624.1.16",
          "form": "그랬군요ㅋㅋ",
          "original_form": "그랬군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035624.1.17",
          "form": "그것도 사람한테는 절대 안짖고",
          "original_form": "그것도 사람한테는 절대 안짖고",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035624.1.18",
          "form": "시추가 정말 순하대요",
          "original_form": "시추가 정말 순하대요",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035624.1.19",
          "form": "다른 강아지들한테만 짖었어요",
          "original_form": "다른 강아지들한테만 짖었어요",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035624.1.20",
          "form": "아아 넘 귀여워요ㅠㅠㅠ",
          "original_form": "아아 넘 귀여워요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035624.1.21",
          "form": "교육이 잘된 말티즈였나봐요",
          "original_form": "교육이 잘된 말티즈였나봐요",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035624.1.22",
          "form": "그런가봐요!ㅎㅎㅎ",
          "original_form": "그런가봐요!ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:28"
        }
      ]
    }
