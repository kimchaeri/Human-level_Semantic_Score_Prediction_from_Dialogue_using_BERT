      "utterance": [
        {
          "id": "MDRW2100020764.1.1",
          "form": "안녕하세요, 봉사 하시나요?",
          "original_form": "안녕하세요, 봉사 하시나요?",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020764.1.2",
          "form": "아니요 학생때는 했었는데 요즘은 못하고 있어요",
          "original_form": "아니요 학생때는 했었는데 요즘은 못하고 있어요",
          "speaker_id": "2",
          "time": "20210828 12:26"
        },
        {
          "id": "MDRW2100020764.1.3",
          "form": "자주하시는 편이신가요?",
          "original_form": "자주하시는 편이신가요?",
          "speaker_id": "2",
          "time": "20210828 12:26"
        },
        {
          "id": "MDRW2100020764.1.4",
          "form": "주에 한 번 아름다운 가게에서 봉사해요",
          "original_form": "주에 한 번 아름다운 가게에서 봉사해요",
          "speaker_id": "1",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020764.1.5",
          "form": "오옷",
          "original_form": "오옷",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020764.1.6",
          "form": "자주 하시는구나",
          "original_form": "자주 하시는구나",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020764.1.7",
          "form": "아름다운가게 봉사면 물품정리나  판매인가요?",
          "original_form": "아름다운가게 봉사면 물품정리나  판매인가요?",
          "speaker_id": "2",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020764.1.8",
          "form": "네네! 무급 아르바이트 같은 느낌의 봉사예요",
          "original_form": "네네! 무급 아르바이트 같은 느낌의 봉사예요",
          "speaker_id": "1",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020764.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020764.1.10",
          "form": "그렇게 이야기하니 재밌을거 같은데요 ㅎㅎ",
          "original_form": "그렇게 이야기하니 재밌을거 같은데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020764.1.11",
          "form": "봉사는 단체로 하시는건가요?",
          "original_form": "봉사는 단체로 하시는건가요?",
          "speaker_id": "2",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100020764.1.12",
          "form": "개인적으로 신청해서 하고있어요",
          "original_form": "개인적으로 신청해서 하고있어요",
          "speaker_id": "1",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020764.1.13",
          "form": "우왕 대단해요",
          "original_form": "우왕 대단해요",
          "speaker_id": "2",
          "time": "20210828 13:04"
        },
        {
          "id": "MDRW2100020764.1.14",
          "form": "사실 봉사는 평소에 관심을 갖고 해야하는건데",
          "original_form": "사실 봉사는 평소에 관심을 갖고 해야하는건데",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020764.1.15",
          "form": "꾸준히 하시는게 대단해요",
          "original_form": "꾸준히 하시는게 대단해요",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020764.1.16",
          "form": "그만둘 타이밍을 놓친 것 같아요ㅋㅋㅋ",
          "original_form": "그만둘 타이밍을 놓친 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020764.1.17",
          "form": "불편한게 없으면 계속하는게 좋죠.. 설사 불편해서 그만두시더라도 봉사하던 마음이 그대로니까 어디서든 계속 하실거 같아요",
          "original_form": "불편한게 없으면 계속하는게 좋죠.. 설사 불편해서 그만두시더라도 봉사하던 마음이 그대로니까 어디서든 계속 하실거 같아요",
          "speaker_id": "2",
