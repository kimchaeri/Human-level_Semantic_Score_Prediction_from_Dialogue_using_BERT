      "utterance": [
        {
          "id": "MDRW2100005732.1.1",
          "form": "제주는 어때요??",
          "original_form": "제주는 어때요??",
          "speaker_id": "2",
          "time": "20210907 10:02"
        },
        {
          "id": "MDRW2100005732.1.2",
          "form": "확진자 많이 나오나요??",
          "original_form": "확진자 많이 나오나요??",
          "speaker_id": "2",
          "time": "20210907 10:02"
        },
        {
          "id": "MDRW2100005732.1.3",
          "form": "8월 관광객 빠지고 나아지고 있어요",
          "original_form": "8월 관광객 빠지고 나아지고 있어요",
          "speaker_id": "1",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100005732.1.4",
          "form": "그래도 아직 4단계예요",
          "original_form": "그래도 아직 4단계예요",
          "speaker_id": "1",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100005732.1.5",
          "form": "아 그렇군요 제주도 4단계라니..ㅠ",
          "original_form": "아 그렇군요 제주도 4단계라니..ㅠ",
          "speaker_id": "2",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100005732.1.6",
          "form": "관광객이 계속 있으니",
          "original_form": "관광객이 계속 있으니",
          "speaker_id": "2",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100005732.1.7",
          "form": "조심해야겠어요",
          "original_form": "조심해야겠어요",
          "speaker_id": "2",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100005732.1.8",
          "form": "그래도 방학 시즌이 끝나서",
          "original_form": "그래도 방학 시즌이 끝나서",
          "speaker_id": "2",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100005732.1.9",
          "form": "조금 다행이네요",
          "original_form": "조금 다행이네요",
          "speaker_id": "2",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100005732.1.10",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100005732.1.11",
          "form": "마스크만 벗어도 좋겠어요",
          "original_form": "마스크만 벗어도 좋겠어요",
          "speaker_id": "1",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100005732.1.12",
          "form": "그러게말이예요",
          "original_form": "그러게말이예요",
          "speaker_id": "2",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100005732.1.13",
          "form": "애들이 마스크자국이 얼굴에",
          "original_form": "애들이 마스크자국이 얼굴에",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100005732.1.14",
          "form": "남아있어서",
          "original_form": "남아있어서",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100005732.1.15",
          "form": "여름되니까 마스크 쓴 부분빼고 다 타니까",
          "original_form": "여름되니까 마스크 쓴 부분빼고 다 타니까",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100005732.1.16",
          "form": "안타깝더라구요",
          "original_form": "안타깝더라구요",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100005732.1.17",
          "form": "매일 땀맺혀서 찝찝해하고",
          "original_form": "매일 땀맺혀서 찝찝해하고",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100005732.1.18",
          "form": "위드코로나는 사실 시민들에겐 일상인듯해요",
          "original_form": "위드코로나는 사실 시민들에겐 일상인듯해요",
          "speaker_id": "1",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100005732.1.19",
          "form": "맞아요 이제 함께 살아가는...",
          "original_form": "맞아요 이제 함께 살아가는...",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100005732.1.20",
          "form": "작은아이늑 마스크 없는 삶을 기억도 못하구요",
          "original_form": "작은아이늑 마스크 없는 삶을 기억도 못하구요",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100005732.1.21",
          "form": "큰아이는",
          "original_form": "큰아이는",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100005732.1.22",
          "form": "빨리 마스크벗고 싶다고 하더라구요",
          "original_form": "빨리 마스크벗고 싶다고 하더라구요",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100005732.1.23",
          "form": "애기들 마스크 쓰고 있음 미안해요",
          "original_form": "애기들 마스크 쓰고 있음 미안해요",
          "speaker_id": "1",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100005732.1.24",
          "form": "아히고",
          "original_form": "아히고",
          "speaker_id": "1",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100005732.1.25",
          "form": "네 진짜 애들이",
          "original_form": "네 진짜 애들이",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100005732.1.26",
          "form": "밖에 나갈때",
          "original_form": "밖에 나갈때",
          "speaker_id": "2",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100005732.1.27",
          "form": "학교 안가는 건 좋아해요? 비대면",
          "original_form": "학교 안가는 건 좋아해요? 비대면",
          "speaker_id": "1",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100005732.1.28",
          "form": "스스로 마스크 챙겨서 쓰고 할때마다 미안하더라구요",
          "original_form": "스스로 마스크 챙겨서 쓰고 할때마다 미안하더라구요",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100005732.1.29",
          "form": "올해부터",
          "original_form": "올해부터",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100005732.1.30",
          "form": "1.2학년은 전면 등교로 바껴서",
          "original_form": "1.2학년은 전면 등교로 바껴서",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100005732.1.31",
          "form": "계속 등교하고 있어요",
          "original_form": "계속 등교하고 있어요",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100005732.1.32",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100005732.1.33",
          "form": "3~6학년은 가끔 줌수업 하는거 같구요",
          "original_form": "3~6학년은 가끔 줌수업 하는거 같구요",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100005732.1.34",
          "form": "교실안에서도 마스크 쓰나요?",
          "original_form": "교실안에서도 마스크 쓰나요?",
          "speaker_id": "1",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100005732.1.35",
          "form": "아이들이 줌수업하니 적응을 못한다고 해서요",
          "original_form": "아이들이 줌수업하니 적응을 못한다고 해서요",
          "speaker_id": "2",
          "time": "20210907 10:15"
        }
      ]
    }
