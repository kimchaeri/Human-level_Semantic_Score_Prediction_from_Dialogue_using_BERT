      "utterance": [
        {
          "id": "MDRW2100041307.1.1",
          "form": "요즘 보는 프로그램 있으신가요?",
          "original_form": "요즘 보는 프로그램 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100041307.1.2",
          "form": "음 챙겨보는 프로는... 아 그나마 펜트하우스??",
          "original_form": "음 챙겨보는 프로는... 아 그나마 펜트하우스??",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100041307.1.3",
          "form": "오 거의 끝나간다고 봤는데 맞나요?",
          "original_form": "오 거의 끝나간다고 봤는데 맞나요?",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100041307.1.4",
          "form": "맞아요 ㅋㅋ이번주가 드디어 막방이에요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋ이번주가 드디어 막방이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100041307.1.5",
          "form": "1년정도 됐다는데 정말 오래 하네요ㅎㅎ",
          "original_form": "1년정도 됐다는데 정말 오래 하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100041307.1.6",
          "form": "꾸준히 보시다니 대단하세요!",
          "original_form": "꾸준히 보시다니 대단하세요!",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100041307.1.7",
          "form": "ㅋㅋㅋ저도 근데 모든 편을 다본건아니고 정말 띄엄 띄엄 봤어뇨",
          "original_form": "ㅋㅋㅋ저도 근데 모든 편을 다본건아니고 정말 띄엄 띄엄 봤어뇨",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100041307.1.8",
          "form": "그래도 재밌더라구요 ㅋㅋㅋ",
          "original_form": "그래도 재밌더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100041307.1.9",
          "form": "제 친구도 띄엄띄엄 보더라구요ㅎㅎ",
          "original_form": "제 친구도 띄엄띄엄 보더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100041307.1.10",
          "form": "가장 재밌게 본 드라마가 뭔가요?",
          "original_form": "가장 재밌게 본 드라마가 뭔가요?",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100041307.1.11",
          "form": "흐음 ㅋㅋ잘기억안나는데 아 질투의 화신 정말 재밌게봤었어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "흐음 ㅋㅋ잘기억안나는데 아 질투의 화신 정말 재밌게봤었어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100041307.1.12",
          "form": "오 추천 드라마에서 본 거 같아요!",
          "original_form": "오 추천 드라마에서 본 거 같아요!",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100041307.1.13",
          "form": "조정석 연기를 그렇게 잘한다던데",
          "original_form": "조정석 연기를 그렇게 잘한다던데",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100041307.1.14",
          "form": "ㅋㅋㅋ진짜 넘넘재밌었어요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ진짜 넘넘재밌었어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100041307.1.15",
          "form": "킬미힐미도 재밌었는데",
          "original_form": "킬미힐미도 재밌었는데",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100041307.1.16",
          "form": "보셨나요",
          "original_form": "보셨나요",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100041307.1.17",
          "form": "앞부분만 보고 못봤어요ㅠㅠ",
          "original_form": "앞부분만 보고 못봤어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100041307.1.18",
          "form": "추천하시나요?",
          "original_form": "추천하시나요?",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100041307.1.19",
          "form": "ㅋㅋㅋ진짜 재밌어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ진짜 재밌어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100041307.1.20",
          "form": "지성이 진짜 연기를 너무잘해요 ㅋㅋㅋㅋ",
          "original_form": "지성이 진짜 연기를 너무잘해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100041307.1.21",
          "form": "오 킬미힐미도 봐야겠어요",
          "original_form": "오 킬미힐미도 봐야겠어요",
          "speaker_id": "1",
          "time": "20210908 00:00"
        }
      ]
    }
