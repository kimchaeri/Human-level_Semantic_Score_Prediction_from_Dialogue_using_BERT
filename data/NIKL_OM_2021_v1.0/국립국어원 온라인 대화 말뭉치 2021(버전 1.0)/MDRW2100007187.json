      "utterance": [
        {
          "id": "MDRW2100007187.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007187.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007187.1.3",
          "form": "여행하는거 좋아하세요?",
          "original_form": "여행하는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007187.1.4",
          "form": "친구들과 함께 하는 여행 좋아요",
          "original_form": "친구들과 함께 하는 여행 좋아요",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007187.1.5",
          "form": "맞아요 친구들이랑 여행가면 너무 좋죠~",
          "original_form": "맞아요 친구들이랑 여행가면 너무 좋죠~",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007187.1.6",
          "form": "가보신 여행지 중에서 젤 좋았던 곳이 어디신가요?",
          "original_form": "가보신 여행지 중에서 젤 좋았던 곳이 어디신가요?",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007187.1.7",
          "form": "전 여수 좋았어요! 케이블카타고 본 야경이 멋있었어요",
          "original_form": "전 여수 좋았어요! 케이블카타고 본 야경이 멋있었어요",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007187.1.8",
          "form": "여수 좋다는 소리만 많이 듣고 한번도 못가봤어요 .. 야경 너무 예뻤을 것 같아요 ㅠ",
          "original_form": "여수 좋다는 소리만 많이 듣고 한번도 못가봤어요 .. 야경 너무 예뻤을 것 같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007187.1.9",
          "form": "한번 시간내서 가보세요 잘 꾸며놓아서 도시가 예뻐요",
          "original_form": "한번 시간내서 가보세요 잘 꾸며놓아서 도시가 예뻐요",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007187.1.10",
          "form": "추천할만한 다른 여행지 있으신가요?",
          "original_form": "추천할만한 다른 여행지 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007187.1.11",
          "form": "저는 제주도요! 여행하면 제주도가 진짜 좋은 것 같아요",
          "original_form": "저는 제주도요! 여행하면 제주도가 진짜 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007187.1.12",
          "form": "바다가 너무 예쁘더라구요",
          "original_form": "바다가 너무 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007187.1.13",
          "form": "제주도 좋죠 \r\n요즘 코로나라 못가는게 아쉽네요",
          "original_form": "제주도 좋죠 \r\n요즘 코로나라 못가는게 아쉽네요",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007187.1.14",
          "form": "그러니까요.. 제주도 가서 해산물 먹고싶어요ㅠ",
          "original_form": "그러니까요.. 제주도 가서 해산물 먹고싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007187.1.15",
          "form": "코로나 끝나면 가고싶은 여행지 있으세요?",
          "original_form": "코로나 끝나면 가고싶은 여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007187.1.16",
          "form": "유럽에 가고 싶어요 한번도 안가봐서 궁금하네요",
          "original_form": "유럽에 가고 싶어요 한번도 안가봐서 궁금하네요",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007187.1.17",
          "form": "유럽 어디쪽 가보고싶으세요? 코로나 터지기 전에 갔다왔는데 정말 좋았어요!",
          "original_form": "유럽 어디쪽 가보고싶으세요? 코로나 터지기 전에 갔다왔는데 정말 좋았어요!",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007187.1.18",
          "form": "저는 이탈리아 가보고 싶어요",
          "original_form": "저는 이탈리아 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 01:30"
        }
      ]
