      "utterance": [
        {
          "id": "MDRW2100042902.1.1",
          "form": "너네가 내향형 애들이 많이 모인건 알고 있다 나서기 싫은 건 안다 이거 꼭 붙임",
          "original_form": "너네가 내향형 애들이 많이 모인건 알고 있다 나서기 싫은 건 안다 이거 꼭 붙임",
          "speaker_id": "2",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042902.1.2",
          "form": "몰라도알고싶지않음",
          "original_form": "몰라도알고싶지않음",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042902.1.3",
          "form": "아니 알면 시키지 마",
          "original_form": "아니 알면 시키지 마",
          "speaker_id": "2",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042902.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042902.1.5",
          "form": "그니가",
          "original_form": "그니가",
          "speaker_id": "2",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042902.1.6",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042902.1.7",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042902.1.8",
          "form": "내향인거알면서",
          "original_form": "내향인거알면서",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042902.1.9",
          "form": "왜시키는거임",
          "original_form": "왜시키는거임",
          "speaker_id": "1",
          "time": "20210907 11:05"
        },
        {
          "id": "MDRW2100042902.1.10",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.11",
          "form": "일부러고통주는건가",
          "original_form": "일부러고통주는건가",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.12",
          "form": "아니 본인들이 아이엠 그라운드라도 하던지요",
          "original_form": "아니 본인들이 아이엠 그라운드라도 하던지요",
          "speaker_id": "2",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.13",
          "form": "왜 그니까",
          "original_form": "왜 그니까",
          "speaker_id": "2",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그거좋다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그거좋다",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.15",
          "form": "고학번이라 먼저 해야 한다고",
          "original_form": "고학번이라 먼저 해야 한다고",
          "speaker_id": "2",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.16",
          "form": "자기소개좋아하는",
          "original_form": "자기소개좋아하는",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.17",
          "form": "미칭럼들아",
          "original_form": "미칭럼들아",
          "speaker_id": "2",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.18",
          "form": "교수끼리",
          "original_form": "교수끼리",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.19",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.20",
          "form": "아이엠그라운드하기",
          "original_form": "아이엠그라운드하기",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.21",
          "form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.22",
          "form": "어느정도로자기소개함?",
          "original_form": "어느정도로자기소개함?",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.23",
          "form": "피피티만드는건아니지?",
          "original_form": "피피티만드는건아니지?",
          "speaker_id": "1",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042902.1.24",
          "form": "1분 스피치",
          "original_form": "1분 스피치",
