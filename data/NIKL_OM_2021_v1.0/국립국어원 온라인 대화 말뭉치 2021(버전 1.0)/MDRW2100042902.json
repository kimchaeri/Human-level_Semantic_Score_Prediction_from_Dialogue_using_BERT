          "speaker_id": "1",
          "time": "20210907 11:07"
        },
        {
          "id": "MDRW2100042902.1.27",
          "form": "그냥 시켜 그리고 질문을 겁나 많이 해",
          "original_form": "그냥 시켜 그리고 질문을 겁나 많이 해",
          "speaker_id": "2",
          "time": "20210907 11:07"
        },
        {
          "id": "MDRW2100042902.1.28",
          "form": "그나마다행이네",
          "original_form": "그나마다행이네",
          "speaker_id": "1",
          "time": "20210907 11:07"
        },
        {
          "id": "MDRW2100042902.1.29",
          "form": "질문할게뭐가있음",
          "original_form": "질문할게뭐가있음",
          "speaker_id": "1",
          "time": "20210907 11:07"
        },
        {
          "id": "MDRW2100042902.1.30",
