        },
        {
          "id": "MDRW2100043830.1.2",
          "form": "우리 이번에 못 만나서 조개구이 못먹었네",
          "original_form": "우리 이번에 못 만나서 조개구이 못먹었네",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043830.1.3",
          "form": "이번에 묵져",
          "original_form": "이번에 묵져",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043830.1.4",
          "form": "곱창도 먹고 싶음",
          "original_form": "곱창도 먹고 싶음",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043830.1.5",
          "form": "김덕후 털어야 하는디",
          "original_form": "김덕후 털어야 하는디",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043830.1.6",
          "form": "김덕후는 신림....",
          "original_form": "김덕후는 신림....",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043830.1.7",
          "form": "도시락밥 맛난디ㅠㅠ",
          "original_form": "도시락밥 맛난디ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043830.1.8",
          "form": "ㅎ ㅏ 아랫쪽 없나?? 찾아봐야디",
          "original_form": "ㅎ ㅏ 아랫쪽 없나?? 찾아봐야디",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043830.1.9",
          "form": "읍네..",
          "original_form": "읍네..",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043830.1.10",
          "form": "아 서산 해미읍성에",
          "original_form": "아 서산 해미읍성에",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043830.1.11",
          "form": "먹고 싶은거 있나오",
          "original_form": "먹고 싶은거 있나오",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043830.1.12",
          "form": "골목식당 나온 돼지곱창집이 있는데",
          "original_form": "골목식당 나온 돼지곱창집이 있는데",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043830.1.13",
          "form": "돼지곱창 맛있더라",
          "original_form": "돼지곱창 맛있더라",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043830.1.14",
          "form": "헐 돼지곱창....",
          "original_form": "헐 돼지곱창....",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043830.1.15",
          "form": "묵었꾸나",
          "original_form": "묵었꾸나",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043830.1.16",
          "form": "응ㅋㅋㅋㅋ",
          "original_form": "응ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043830.1.17",
          "form": "곱 들어있는 거 첨알았음",
          "original_form": "곱 들어있는 거 첨알았음",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043830.1.18",
          "form": "소곱창만 그런 줉ㅋㅋ",
          "original_form": "소곱창만 그런 줉ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043830.1.19",
          "form": "왜 그것이 찐이구나ㅋㅋㅋ",
          "original_form": "왜 그것이 찐이구나ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043830.1.20",
          "form": "그거 묵으면 김덕후 못먹는거 아니냐",
          "original_form": "그거 묵으면 김덕후 못먹는거 아니냐",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043830.1.21",
          "form": "곱창 좋져....",
          "original_form": "곱창 좋져....",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043830.1.22",
          "form": "그 김덕후 곱창소스도 조은데",
          "original_form": "그 김덕후 곱창소스도 조은데",
