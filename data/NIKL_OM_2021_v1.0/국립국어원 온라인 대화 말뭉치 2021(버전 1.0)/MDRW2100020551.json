      "utterance": [
        {
          "id": "MDRW2100020551.1.1",
          "form": "어휴 깨 안먹는 분들이 많아서",
          "original_form": "어휴 깨 안먹는 분들이 많아서",
          "speaker_id": "2",
          "time": "20210828 09:38"
        },
        {
          "id": "MDRW2100020551.1.2",
          "form": "혹시그러실까 걱정했네요",
          "original_form": "혹시그러실까 걱정했네요",
          "speaker_id": "2",
          "time": "20210828 09:38"
        },
        {
          "id": "MDRW2100020551.1.3",
          "form": "아닙니다",
          "original_form": "아닙니다",
          "speaker_id": "1",
          "time": "20210828 09:38"
        },
        {
          "id": "MDRW2100020551.1.4",
          "form": "전 다 먹습니다",
          "original_form": "전 다 먹습니다",
          "speaker_id": "1",
          "time": "20210828 09:38"
        },
        {
          "id": "MDRW2100020551.1.5",
          "form": "깨좋아요",
          "original_form": "깨좋아요",
          "speaker_id": "1",
          "time": "20210828 09:38"
        },
        {
          "id": "MDRW2100020551.1.6",
          "form": "참기름 발린 김밥에",
          "original_form": "참기름 발린 김밥에",
          "speaker_id": "2",
          "time": "20210828 09:38"
        },
        {
          "id": "MDRW2100020551.1.7",
          "form": "깨가 화룡정점이죠",
          "original_form": "깨가 화룡정점이죠",
          "speaker_id": "2",
          "time": "20210828 09:38"
        },
        {
          "id": "MDRW2100020551.1.8",
          "form": "김은.. 이름이 잘 기억안나는데",
          "original_form": "김은.. 이름이 잘 기억안나는데",
          "speaker_id": "2",
          "time": "20210828 09:39"
        },
        {
          "id": "MDRW2100020551.1.9",
          "form": "유명한 김 좋아합니다",
          "original_form": "유명한 김 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 09:39"
        },
        {
          "id": "MDRW2100020551.1.10",
          "form": "진짜 약간 김밥의 꽃 같은 느낌",
          "original_form": "진짜 약간 김밥의 꽃 같은 느낌",
          "speaker_id": "1",
          "time": "20210828 09:39"
        },
        {
          "id": "MDRW2100020551.1.11",
          "form": "잠시 네이버 다녀올게요",
          "original_form": "잠시 네이버 다녀올게요",
          "speaker_id": "2",
          "time": "20210828 09:39"
        },
        {
          "id": "MDRW2100020551.1.12",
          "form": "대천김이라고 아시나요",
          "original_form": "대천김이라고 아시나요",
          "speaker_id": "1",
          "time": "20210828 09:39"
        },
        {
          "id": "MDRW2100020551.1.13",
          "form": "포장지가 주황색이 섞인건데",
          "original_form": "포장지가 주황색이 섞인건데",
          "speaker_id": "1",
          "time": "20210828 09:39"
        },
        {
          "id": "MDRW2100020551.1.14",
          "form": "금방 검색하는데 뜨더라구요",
          "original_form": "금방 검색하는데 뜨더라구요",
          "speaker_id": "2",
          "time": "20210828 09:40"
        },
        {
          "id": "MDRW2100020551.1.15",
          "form": "맛있나요",
          "original_form": "맛있나요",
          "speaker_id": "2",
          "time": "20210828 09:40"
        },
        {
          "id": "MDRW2100020551.1.16",
          "form": "이전에 먹었던 김은",
          "original_form": "이전에 먹었던 김은",
          "speaker_id": "1",
          "time": "20210828 09:40"
        },
        {
          "id": "MDRW2100020551.1.17",
          "form": "김이아니었음을",
          "original_form": "김이아니었음을",
          "speaker_id": "1",
          "time": "20210828 09:40"
        },
        {
          "id": "MDRW2100020551.1.18",
          "form": "대천김을 먹고 느꼈습니다",
          "original_form": "대천김을 먹고 느꼈습니다",
          "speaker_id": "1",
          "time": "20210828 09:40"
        },
        {
          "id": "MDRW2100020551.1.19",
          "form": "앗 익숙한 포장지네요",
          "original_form": "앗 익숙한 포장지네요",
          "speaker_id": "2",
          "time": "20210828 09:40"
        },
        {
          "id": "MDRW2100020551.1.20",
          "form": "김밥용김",
          "original_form": "김밥용김",
          "speaker_id": "2",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020551.1.21",
          "form": "먹어본거같아요",
          "original_form": "먹어본거같아요",
          "speaker_id": "2",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020551.1.22",
          "form": "김밥용김은 무조건 비싼걸 사야해요",
          "original_form": "김밥용김은 무조건 비싼걸 사야해요",
          "speaker_id": "2",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020551.1.23",
          "form": "저 김이 진짜 맛있어요",
          "original_form": "저 김이 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020551.1.24",
          "form": "싼건 비린내가 난답니다",
          "original_form": "싼건 비린내가 난답니다",
          "speaker_id": "2",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020551.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020551.1.26",
          "form": "저 김은 종종 집에서 먹어봤어요",
          "original_form": "저 김은 종종 집에서 먹어봤어요",
          "speaker_id": "2",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020551.1.27",
          "form": "김 싼 거 쓰면",
          "original_form": "김 싼 거 쓰면",
          "speaker_id": "1",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020551.1.28",
          "form": "늘어지고",
          "original_form": "늘어지고",
          "speaker_id": "1",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020551.1.29",
          "form": "합격인듯",
          "original_form": "합격인듯",
          "speaker_id": "2",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020551.1.30",
          "form": "냄새나요",
          "original_form": "냄새나요",
          "speaker_id": "1",
          "time": "20210828 09:41"
        }
      ]
    }
  ]
