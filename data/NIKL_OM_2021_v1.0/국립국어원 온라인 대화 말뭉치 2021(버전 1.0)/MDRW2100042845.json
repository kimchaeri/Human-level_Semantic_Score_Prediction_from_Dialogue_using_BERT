      "utterance": [
        {
          "id": "MDRW2100042845.1.1",
          "form": "참 식세기 유리 이 안 나가나?",
          "original_form": "참 식세기 유리 이 안 나가나?",
          "speaker_id": "1",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042845.1.2",
          "form": "한번씩 나간다",
          "original_form": "한번씩 나간다",
          "speaker_id": "2",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042845.1.3",
          "form": "마쪠!",
          "original_form": "마쪠!",
          "speaker_id": "1",
          "time": "20210907 09:57"
        },
        {
          "id": "MDRW2100042845.1.4",
          "form": "유리컵이랑 반찬통... 이 나간게 보여",
          "original_form": "유리컵이랑 반찬통... 이 나간게 보여",
          "speaker_id": "1",
          "time": "20210907 09:58"
        },
        {
          "id": "MDRW2100042845.1.5",
          "form": "왜 나가는거지? 부딪히는거 같진 않은데ㅠ",
          "original_form": "왜 나가는거지? 부딪히는거 같진 않은데ㅠ",
          "speaker_id": "1",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042845.1.6",
          "form": "그러니깐 ㅠㅠ",
          "original_form": "그러니깐 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042845.1.7",
          "form": "나도 그래서 몇개 버리긴했어",
          "original_form": "나도 그래서 몇개 버리긴했어",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042845.1.8",
          "form": "아깝네잉 ㅠㅠㅜ",
          "original_form": "아깝네잉 ㅠㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100042845.1.9",
          "form": "그러니깐 ㅋㅋ",
          "original_form": "그러니깐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042845.1.10",
          "form": "근데 또 그거 알제?",
          "original_form": "근데 또 그거 알제?",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042845.1.11",
          "form": "멀쩡한거 버리기는 싫고 ㅋ",
          "original_form": "멀쩡한거 버리기는 싫고 ㅋ",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042845.1.12",
          "form": "또 사기는 아까우니...이렇게 하나씩 하나씩 버리다가 새걸로 갈아타고싶다 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "또 사기는 아까우니...이렇게 하나씩 하나씩 버리다가 새걸로 갈아타고싶다 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042845.1.13",
          "form": "그릇",
          "original_form": "그릇",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042845.1.14",
          "form": "ㅋㅋㅋ 악 뭐야 큰그림",
          "original_form": "ㅋㅋㅋ 악 뭐야 큰그림",
          "speaker_id": "1",
          "time": "20210907 10:02"
        },
        {
          "id": "MDRW2100042845.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 웃겨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 웃겨",
          "speaker_id": "2",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100042845.1.16",
          "form": "지금 두고 보고 있다. ㅋㅋㅋ 코렐살라고",
          "original_form": "지금 두고 보고 있다. ㅋㅋㅋ 코렐살라고",
          "speaker_id": "2",
          "time": "20210907 10:04"
        },
        {
          "id": "MDRW2100042845.1.17",
          "form": "코렐이 진짜 안꺠지고 좋지 ㅎ 지금건 코렐 아니가",
          "original_form": "코렐이 진짜 안꺠지고 좋지 ㅎ 지금건 코렐 아니가",
          "speaker_id": "1",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100042845.1.18",
          "form": "응응 그러니깐 ㅋㅋ",
          "original_form": "응응 그러니깐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100042845.1.19",
          "form": "코렐이 젤 좋은듯",
          "original_form": "코렐이 젤 좋은듯",
          "speaker_id": "2",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100042845.1.20",
          "form": "진짜 설것이 하다가 확 떨쟈도 안꺠짐..",
          "original_form": "진짜 설것이 하다가 확 떨쟈도 안꺠짐..",
          "speaker_id": "1",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100042845.1.21",
          "form": "그러니깐 ㅋㅋㅋ 신기해",
          "original_form": "그러니깐 ㅋㅋㅋ 신기해",
          "speaker_id": "2",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100042845.1.22",
          "form": "근데 또 깨질때는 완전 심각하게 깨지던데 ㅋㅋ",
          "original_form": "근데 또 깨질때는 완전 심각하게 깨지던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100042845.1.23",
          "form": "이 나가는건 투명한 유리들이 나가더라 ㅠ 컵 반찬통 ㅠ",
          "original_form": "이 나가는건 투명한 유리들이 나가더라 ㅠ 컵 반찬통 ㅠ",
          "speaker_id": "1",
          "time": "20210907 10:08"
        }
      ]
