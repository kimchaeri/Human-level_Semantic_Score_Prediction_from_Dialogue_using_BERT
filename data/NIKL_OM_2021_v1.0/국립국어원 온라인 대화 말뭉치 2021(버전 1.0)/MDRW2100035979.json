        },
        {
          "id": "MDRW2100035979.1.2",
          "form": "찾으러왓습니당",
          "original_form": "찾으러왓습니당",
          "speaker_id": "2",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100035979.1.3",
          "form": "오늘 저녁 맛있겠군용ㅋㅋ",
          "original_form": "오늘 저녁 맛있겠군용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100035979.1.4",
          "form": "집가서 끓여먹어야하나유?",
          "original_form": "집가서 끓여먹어야하나유?",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100035979.1.5",
          "form": "지금은뜨거운데",
          "original_form": "지금은뜨거운데",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100035979.1.6",
          "form": "냄비옮겨서데워야지",
          "original_form": "냄비옮겨서데워야지",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100035979.1.7",
          "form": "넘무거운디",
          "original_form": "넘무거운디",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100035979.1.8",
          "form": "글쿤 언넝 집으로 갑시다!!!",
          "original_form": "글쿤 언넝 집으로 갑시다!!!",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100035979.1.9",
          "form": "소짠디 완전 국물파티인가 ㅋㅋ",
          "original_form": "소짠디 완전 국물파티인가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100035979.1.10",
          "form": "포장이라 푸짐하게 줬나 ㅋㅋ",
          "original_form": "포장이라 푸짐하게 줬나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100035979.1.11",
          "form": "재난지원금",
          "original_form": "재난지원금",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100035979.1.12",
          "form": "각자신청하는건가?",
          "original_form": "각자신청하는건가?",
