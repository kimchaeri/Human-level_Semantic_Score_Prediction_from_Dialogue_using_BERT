      "utterance": [
        {
          "id": "MDRW2100039332.1.1",
          "form": "좋아하시는 음료있으신가요?ㅎ",
          "original_form": "좋아하시는 음료있으신가요?ㅎ",
          "speaker_id": "2",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039332.1.2",
          "form": "프라푸치노 좋아합니당ㅋㅋ",
          "original_form": "프라푸치노 좋아합니당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:14"
        },
        {
          "id": "MDRW2100039332.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210903 19:15"
        },
        {
          "id": "MDRW2100039332.1.4",
          "form": "저도 달달한 커피는 다좋아요ㅋ 아 초딩입맛이랑 아메리카노같은거 말구요ㅋㅋ",
          "original_form": "저도 달달한 커피는 다좋아요ㅋ 아 초딩입맛이랑 아메리카노같은거 말구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039332.1.5",
          "form": "저는 아직 커피를 못 배워서ㅋㅋ",
          "original_form": "저는 아직 커피를 못 배워서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039332.1.6",
          "form": "입문하기 좋은 커피는 뭘까요?",
          "original_form": "입문하기 좋은 커피는 뭘까요?",
          "speaker_id": "1",
          "time": "20210903 19:28"
        },
        {
          "id": "MDRW2100039332.1.7",
          "form": "프라푸치노도 커피아닌가요?ㅎㅎ",
          "original_form": "프라푸치노도 커피아닌가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039332.1.8",
          "form": "커피 안들어간 메뉴로만 마셔서요 ㅋㅋ",
          "original_form": "커피 안들어간 메뉴로만 마셔서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:45"
        },
        {
          "id": "MDRW2100039332.1.9",
          "form": "안먹는게 좋긴하죠ㅎㅎ",
          "original_form": "안먹는게 좋긴하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:47"
        },
        {
          "id": "MDRW2100039332.1.10",
          "form": "ㅋㅋ 저녁은 드셨어요?",
          "original_form": "ㅋㅋ 저녁은 드셨어요?",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100039332.1.11",
          "form": "네ㅋ 순대국요ㅎ",
          "original_form": "네ㅋ 순대국요ㅎ",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100039332.1.12",
          "form": "ㅋㅋ맛있었겠네염",
          "original_form": "ㅋㅋ맛있었겠네염",
          "speaker_id": "1",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100039332.1.13",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210903 20:01"
        },
        {
          "id": "MDRW2100039332.1.14",
          "form": "저는 집밥 먹었어요 ㅋㅋㅋ",
          "original_form": "저는 집밥 먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:02"
        },
        {
          "id": "MDRW2100039332.1.15",
          "form": "저도 집밥이었어요ㅋㅋ냉동순대국ㅋㅋㅋ",
          "original_form": "저도 집밥이었어요ㅋㅋ냉동순대국ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039332.1.16",
          "form": "밀키트 구입하신거에여?",
          "original_form": "밀키트 구입하신거에여?",
