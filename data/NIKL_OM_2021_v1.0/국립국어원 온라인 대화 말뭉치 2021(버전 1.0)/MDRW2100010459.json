      "utterance": [
        {
          "id": "MDRW2100010459.1.1",
          "form": "네 밥알 동동 뜬 ㅎㅎㅎ",
          "original_form": "네 밥알 동동 뜬 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010459.1.2",
          "form": "비락식혜 너무 맛없어요",
          "original_form": "비락식혜 너무 맛없어요",
          "speaker_id": "1",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010459.1.3",
          "form": "너무 안 달아서 좋아요",
          "original_form": "너무 안 달아서 좋아요",
          "speaker_id": "2",
          "time": "20210823 18:33"
        },
        {
          "id": "MDRW2100010459.1.4",
          "form": "제가 코스트코에서 맛있다고 추천받은 식혜",
          "original_form": "제가 코스트코에서 맛있다고 추천받은 식혜",
          "speaker_id": "1",
          "time": "20210823 18:34"
        },
        {
          "id": "MDRW2100010459.1.5",
          "form": "속는척 넘어가서 사먹었는데",
          "original_form": "속는척 넘어가서 사먹었는데",
          "speaker_id": "1",
          "time": "20210823 18:34"
        },
        {
          "id": "MDRW2100010459.1.6",
          "form": "에잉 다 별로에요 공장식은",
          "original_form": "에잉 다 별로에요 공장식은",
          "speaker_id": "1",
          "time": "20210823 18:34"
        },
        {
          "id": "MDRW2100010459.1.7",
          "form": "아 코스트코 식혜도 유명한가봐요",
          "original_form": "아 코스트코 식혜도 유명한가봐요",
          "speaker_id": "2",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010459.1.8",
          "form": "특유의 그 인공맛? 이 느껴지잖아요 ㅠ",
          "original_form": "특유의 그 인공맛? 이 느껴지잖아요 ㅠ",
          "speaker_id": "2",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010459.1.9",
          "form": "그것도 그렇구 저는 밥알이 맛없더라구요",
          "original_form": "그것도 그렇구 저는 밥알이 맛없더라구요",
          "speaker_id": "1",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010459.1.10",
          "form": "플라스틱 인가 볍씨 씹는맛이 싫어요",
          "original_form": "플라스틱 인가 볍씨 씹는맛이 싫어요",
          "speaker_id": "1",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010459.1.11",
          "form": "아 맞아요 막 여러번 씹어야되는 그 식감",
          "original_form": "아 맞아요 막 여러번 씹어야되는 그 식감",
          "speaker_id": "2",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010459.1.12",
          "form": "집에서나 전통시장에서 만든식혜만 먹어요 ㅎㅎ",
          "original_form": "집에서나 전통시장에서 만든식혜만 먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010459.1.13",
          "form": "수정과는 안 좋아하시나요 ㅋㅋ",
          "original_form": "수정과는 안 좋아하시나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010459.1.14",
          "form": "으으리~ 하는 김보성광고기억나시나요",
          "original_form": "으으리~ 하는 김보성광고기억나시나요",
          "speaker_id": "1",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010459.1.15",
          "form": "전 계피는 싫어해용",
          "original_form": "전 계피는 싫어해용",
          "speaker_id": "1",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010459.1.16",
          "form": "아 계피향 싫어하시는구나",
          "original_form": "아 계피향 싫어하시는구나",
          "speaker_id": "2",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010459.1.17",
          "form": "그럼 카푸치노도 시나몬 안 뿌리시나요",
          "original_form": "그럼 카푸치노도 시나몬 안 뿌리시나요",
          "speaker_id": "2",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010459.1.18",
          "form": "저는 전생에 모기였나봐요",
          "original_form": "저는 전생에 모기였나봐요",
          "speaker_id": "1",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010459.1.19",
          "form": "계피냄새 싫어해용 ㅋㅋ",
          "original_form": "계피냄새 싫어해용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010459.1.20",
          "form": "name2님은 계피좋아하세요?",
          "original_form": "&name2&님은 계피좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 18:40"
        },
        {
          "id": "MDRW2100010459.1.21",
          "form": "모기 ㅋㅋㅋㅋ 네 저는 계피 좋아하고 향도 좋아해요",
          "original_form": "모기 ㅋㅋㅋㅋ 네 저는 계피 좋아하고 향도 좋아해요",
          "speaker_id": "2",
          "time": "20210823 18:40"
        },
        {
          "id": "MDRW2100010459.1.22",
          "form": "약간 쿰쿰한 거 좋아하는거 같아요 ㅋㅋㅋ",
          "original_form": "약간 쿰쿰한 거 좋아하는거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100010459.1.23",
          "form": "그러시군요..ㅋㅋㅋ 제가 평생 우연히 먹을 계피를 다 드리고싶네요",
          "original_form": "그러시군요..ㅋㅋㅋ 제가 평생 우연히 먹을 계피를 다 드리고싶네요",
          "speaker_id": "1",
          "time": "20210823 18:42"
        }
      ]
