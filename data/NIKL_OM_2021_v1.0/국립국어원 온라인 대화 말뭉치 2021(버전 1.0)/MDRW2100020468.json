      "utterance": [
        {
          "id": "MDRW2100020468.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 06:54"
        },
        {
          "id": "MDRW2100020468.1.2",
          "form": "네 즐겨 보고 있습니다 ㅎㅎ",
          "original_form": "네 즐겨 보고 있습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020468.1.3",
          "form": "최근에 모가디슈 봤어여",
          "original_form": "최근에 모가디슈 봤어여",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020468.1.4",
          "form": "오 그거 어떤가요?",
          "original_form": "오 그거 어떤가요?",
          "speaker_id": "1",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020468.1.5",
          "form": "주변에 평이 나뉘어서 궁금해요",
          "original_form": "주변에 평이 나뉘어서 궁금해요",
          "speaker_id": "1",
          "time": "20210828 08:17"
        },
        {
          "id": "MDRW2100020468.1.6",
          "form": "박진감있고 연기도 좋고 해서 추천해요!",
          "original_form": "박진감있고 연기도 좋고 해서 추천해요!",
          "speaker_id": "2",
          "time": "20210828 08:29"
        },
        {
          "id": "MDRW2100020468.1.7",
          "form": "감사합니다!",
          "original_form": "감사합니다!",
          "speaker_id": "1",
          "time": "20210828 08:30"
        },
        {
          "id": "MDRW2100020468.1.8",
          "form": "인생영화 있으신가요?",
          "original_form": "인생영화 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 08:30"
        },
        {
          "id": "MDRW2100020468.1.9",
          "form": "음 한 몇번씩 본 영화는 이터널 선샤인이에요",
          "original_form": "음 한 몇번씩 본 영화는 이터널 선샤인이에요",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020468.1.10",
          "form": "보셧나요",
          "original_form": "보셧나요",
          "speaker_id": "2",
          "time": "20210828 08:31"
        },
        {
          "id": "MDRW2100020468.1.11",
          "form": "예전에 본 적 있어요!",
          "original_form": "예전에 본 적 있어요!",
          "speaker_id": "1",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020468.1.12",
          "form": "님은 인생영화있어요?",
          "original_form": "님은 인생영화있어요?",
          "speaker_id": "2",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020468.1.13",
          "form": "분위기가 너무 좋았어요",
          "original_form": "분위기가 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210828 08:55"
        },
        {
          "id": "MDRW2100020468.1.14",
          "form": "저는 죽은시인의 사회요!",
          "original_form": "저는 죽은시인의 사회요!",
          "speaker_id": "1",
          "time": "20210828 08:56"
        },
        {
          "id": "MDRW2100020468.1.15",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "1",
          "time": "20210828 08:56"
        },
        {
          "id": "MDRW2100020468.1.16",
          "form": "오 네네 오래전에 본 영화네요ㅋㅋ",
          "original_form": "오 네네 오래전에 본 영화네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 08:56"
        },
        {
          "id": "MDRW2100020468.1.17",
          "form": "것도 여운이 있는 영화죠",
          "original_form": "것도 여운이 있는 영화죠",
          "speaker_id": "2",
          "time": "20210828 08:56"
        },
        {
          "id": "MDRW2100020468.1.18",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 08:57"
        },
        {
          "id": "MDRW2100020468.1.19",
          "form": "처음 봤을때는 너무 어려워서 이해 못해서 몇 번 봤었어요",
          "original_form": "처음 봤을때는 너무 어려워서 이해 못해서 몇 번 봤었어요",
          "speaker_id": "1",
          "time": "20210828 08:58"
        },
        {
          "id": "MDRW2100020468.1.20",
          "form": "좋은 배우분이셨는데",
          "original_form": "좋은 배우분이셨는데",
          "speaker_id": "2",
          "time": "20210828 09:00"
        },
        {
          "id": "MDRW2100020468.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020468.1.22",
          "form": "영화 고를때 뭘 중점에 두나여? 저는 주로 배우보고 가요ㅋㅋ",
          "original_form": "영화 고를때 뭘 중점에 두나여? 저는 주로 배우보고 가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:26"
        },
        {
          "id": "MDRW2100020468.1.23",
          "form": "한국영화는 배우고 외국은 줄거리를 보고 가요ㅋㅋ",
          "original_form": "한국영화는 배우고 외국은 줄거리를 보고 가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:27"
        },
        {
          "id": "MDRW2100020468.1.24",
          "form": "아 그쳐 외국은 배우에 대한 정보가 좀 적어서ㅋㅋㅋ",
          "original_form": "아 그쳐 외국은 배우에 대한 정보가 좀 적어서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020468.1.25",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020468.1.26",
          "form": "이미지 변신도 확확 되는 느낌이더라구요ㅋㅋ",
          "original_form": "이미지 변신도 확확 되는 느낌이더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:29"
        }
      ]
    }
