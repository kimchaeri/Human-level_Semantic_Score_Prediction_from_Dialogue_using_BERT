      "utterance": [
        {
          "id": "MDRW2100040039.1.1",
          "form": "맥도날드 햄버거 자주 드세요?",
          "original_form": "맥도날드 햄버거 자주 드세요?",
          "speaker_id": "1",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100040039.1.2",
          "form": "맥날도 먹는데...그보다 버거킹 자주 가요!",
          "original_form": "맥날도 먹는데...그보다 버거킹 자주 가요!",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100040039.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100040039.1.4",
          "form": "저는 버거킹은 자주 안 가고 맥도날드를 자주 가요",
          "original_form": "저는 버거킹은 자주 안 가고 맥도날드를 자주 가요",
          "speaker_id": "1",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100040039.1.5",
          "form": "맥날 무슨버거가 맛있나요?",
          "original_form": "맥날 무슨버거가 맛있나요?",
          "speaker_id": "2",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100040039.1.6",
          "form": "맥도날드에서는 슈비버거랑 베이톤 토마토 버거",
          "original_form": "맥도날드에서는 슈비버거랑 베이톤 토마토 버거",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040039.1.7",
          "form": "빅맥도 맛있어요",
          "original_form": "빅맥도 맛있어요",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040039.1.8",
          "form": "빅맥만 먹어봤네요",
          "original_form": "빅맥만 먹어봤네요",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100040039.1.9",
          "form": "버거킹 서는 뭐 자주 드세요?",
          "original_form": "버거킹 서는 뭐 자주 드세요?",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040039.1.10",
          "form": "슈비버거 새우인 거죠? 도전해봐야 겠어요ㅎ",
          "original_form": "슈비버거 새우인 거죠? 도전해봐야 겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040039.1.11",
          "form": "소고기랑 새우랑 한정식 들어 있어요",
          "original_form": "소고기랑 새우랑 한정식 들어 있어요",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040039.1.12",
          "form": "한장",
          "original_form": "한장",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100040039.1.13",
          "form": "헉 비가 비프군요.!",
          "original_form": "헉 비가 비프군요.!",
          "speaker_id": "2",
          "time": "20210904 01:30"
        },
        {
          "id": "MDRW2100040039.1.14",
          "form": "실해서 좋네요..!",
          "original_form": "실해서 좋네요..!",
          "speaker_id": "2",
          "time": "20210904 01:31"
        },
        {
          "id": "MDRW2100040039.1.15",
          "form": "네 새우도 통통하고 은근히 배부르고 맛있었어요",
          "original_form": "네 새우도 통통하고 은근히 배부르고 맛있었어요",
          "speaker_id": "1",
          "time": "20210904 01:31"
        },
        {
          "id": "MDRW2100040039.1.16",
          "form": "버거킹은 콰트로치즈와퍼 좋아해요!",
          "original_form": "버거킹은 콰트로치즈와퍼 좋아해요!",
          "speaker_id": "2",
          "time": "20210904 01:33"
        },
        {
          "id": "MDRW2100040039.1.17",
          "form": "트러플머쉬룸와퍼도 추천!",
          "original_form": "트러플머쉬룸와퍼도 추천!",
          "speaker_id": "2",
          "time": "20210904 01:33"
        },
        {
