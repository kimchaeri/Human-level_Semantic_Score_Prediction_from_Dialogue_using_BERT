      "utterance": [
        {
          "id": "MDRW2100045114.1.1",
          "form": "학교 선후배 스트레스는없었어요?",
          "original_form": "학교 선후배 스트레스는없었어요?",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045114.1.2",
          "form": "넹 ㅋㅋㅋ 님은요",
          "original_form": "넹 ㅋㅋㅋ 님은요",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045114.1.3",
          "form": "네 저두요 대신 cc했더니 언니들이 자꾸 친한척 ...ㅋㅋㅋㅋ",
          "original_form": "네 저두요 대신 cc했더니 언니들이 자꾸 친한척 ...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045114.1.4",
          "form": "에? 언니들이? 왜요?",
          "original_form": "에? 언니들이? 왜요?",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045114.1.5",
          "form": "옆에 오빠들같이다녀서ㅋㅋㅋㄱ",
          "original_form": "옆에 오빠들같이다녀서ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045114.1.6",
          "form": "cc해보셨어요?",
          "original_form": "cc해보셨어요?",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045114.1.7",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045114.1.8",
          "form": "소개시켜달라고?",
          "original_form": "소개시켜달라고?",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045114.1.9",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045114.1.10",
          "form": "네.... 뭔가 사연잇어보이는ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네.... 뭔가 사연잇어보이는ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045114.1.11",
          "form": "ㅋㅋㅋ 오빠들이 훈남이엇나봐요",
          "original_form": "ㅋㅋㅋ 오빠들이 훈남이엇나봐요",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045114.1.12",
          "form": "과대라서 그랫는거같아여ㅋㅋ",
