      "utterance": [
        {
          "id": "MDRW2100034299.1.1",
          "form": "아니요 피부는 따로 하는건없어요ㅠ",
          "original_form": "아니요 피부는 따로 하는건없어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034299.1.2",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034299.1.3",
          "form": "피부과는 너무 비싸요",
          "original_form": "피부과는 너무 비싸요",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034299.1.4",
          "form": "여드름 자주 나기는한데",
          "original_form": "여드름 자주 나기는한데",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034299.1.5",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034299.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034299.1.7",
          "form": "별로 해주는것도 없고 효과도 잘 모르겠는데",
          "original_form": "별로 해주는것도 없고 효과도 잘 모르겠는데",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034299.1.8",
          "form": "비싸더라구요ㅠ",
          "original_form": "비싸더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100034299.1.9",
          "form": "아니 고등학생때는 여드름이 안났는데",
          "original_form": "아니 고등학생때는 여드름이 안났는데",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034299.1.10",
          "form": "왜 성인되고 이렇게 나오는걸까요ㅠ",
          "original_form": "왜 성인되고 이렇게 나오는걸까요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034299.1.11",
          "form": "너무 비싸져",
          "original_form": "너무 비싸져",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034299.1.12",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034299.1.13",
          "form": "어렸을때 많앗는데",
          "original_form": "어렸을때 많앗는데",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100034299.1.14",
          "form": "지금은 덜하긴하지만 지금도 잘 나네여 ㅋㅋ",
          "original_form": "지금은 덜하긴하지만 지금도 잘 나네여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034299.1.15",
          "form": "어릴때 스트레스 많이 받으셨겠네요",
          "original_form": "어릴때 스트레스 많이 받으셨겠네요",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034299.1.16",
          "form": "많이 나는 이유를 알면 좋으련만ㅠ",
          "original_form": "많이 나는 이유를 알면 좋으련만ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034299.1.17",
          "form": "네 ㅠㅠ 한창 사춘기 때에 하필ㅋㅋㅋ",
          "original_form": "네 ㅠㅠ 한창 사춘기 때에 하필ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100034299.1.18",
          "form": "맞아요.. 저는 건성인데도 그러더라구여",
          "original_form": "맞아요.. 저는 건성인데도 그러더라구여",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034299.1.19",
          "form": "그때나는게 진짜 스트레스죠ㅠ",
          "original_form": "그때나는게 진짜 스트레스죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034299.1.20",
          "form": "피부타입이 어떻게 되세요?",
          "original_form": "피부타입이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034299.1.21",
          "form": "저도 건성인데 이래요ㅠ",
          "original_form": "저도 건성인데 이래요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034299.1.22",
          "form": "근데 건성인사람이 피부에 유분기 공급을 안해주면 날수있다그러더라구요",
          "original_form": "근데 건성인사람이 피부에 유분기 공급을 안해주면 날수있다그러더라구요",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100034299.1.23",
          "form": "오맞아여 저도 그런말 들었어요!",
          "original_form": "오맞아여 저도 그런말 들었어요!",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034299.1.24",
          "form": "너무 어려운거같아요",
          "original_form": "너무 어려운거같아요",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034299.1.25",
          "form": "그니까요 로션도 잘골라야되더라구요ㅠ",
          "original_form": "그니까요 로션도 잘골라야되더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:10"
        }
      ]
    }
  ]
