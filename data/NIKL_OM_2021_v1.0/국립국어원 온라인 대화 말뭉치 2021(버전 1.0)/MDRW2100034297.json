      "utterance": [
        {
          "id": "MDRW2100034297.1.1",
          "form": "안녕하세여~",
          "original_form": "안녕하세여~",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100034297.1.2",
          "form": "오 저는 아침에 떡먹었어요",
          "original_form": "오 저는 아침에 떡먹었어요",
          "speaker_id": "2",
          "time": "20210828 09:10"
        },
        {
          "id": "MDRW2100034297.1.3",
          "form": "아하 저는 지금 베이글먹고잇어요 ㅎㅎ",
          "original_form": "아하 저는 지금 베이글먹고잇어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 09:31"
        },
        {
          "id": "MDRW2100034297.1.4",
          "form": "베이글ㅎ 맛잇겠어요! 빵진짜 좋아해요 ㅎㅎ",
          "original_form": "베이글ㅎ 맛잇겠어요! 빵진짜 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100034297.1.5",
          "form": "코스트코 베이글 생각나요",
          "original_form": "코스트코 베이글 생각나요",
          "speaker_id": "2",
          "time": "20210828 09:34"
        },
        {
          "id": "MDRW2100034297.1.6",
          "form": "아! 코스트코가 대용량으로구비해두기엔 참 좋죠 ㅎㅎ",
          "original_form": "아! 코스트코가 대용량으로구비해두기엔 참 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100034297.1.7",
          "form": "베이글에 크림치즈발라먹는거 좋아해여",
          "original_form": "베이글에 크림치즈발라먹는거 좋아해여",
          "speaker_id": "1",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100034297.1.8",
          "form": "베이글에 크림치즈는 국룰이죠 ㅋㅋ",
          "original_form": "베이글에 크림치즈는 국룰이죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:38"
        },
        {
          "id": "MDRW2100034297.1.9",
          "form": "전 달달한 빵이 좋더라고요",
          "original_form": "전 달달한 빵이 좋더라고요",
          "speaker_id": "2",
          "time": "20210828 09:39"
        },
        {
          "id": "MDRW2100034297.1.10",
          "form": "아하 ! 모카번이나 도넛 이런거 좋아하시겠군요?",
          "original_form": "아하 ! 모카번이나 도넛 이런거 좋아하시겠군요?",
          "speaker_id": "1",
          "time": "20210828 09:40"
        },
        {
          "id": "MDRW2100034297.1.11",
          "form": "음 초코빵? 연유빵? 크림빵? 이런게 좋더라고요",
          "original_form": "음 초코빵? 연유빵? 크림빵? 이런게 좋더라고요",
          "speaker_id": "2",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100034297.1.12",
          "form": "파바가면 그때마다 그런종류나 신상 꼭사요 ㅋㅋ",
          "original_form": "파바가면 그때마다 그런종류나 신상 꼭사요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100034297.1.13",
          "form": "아! 그런거~ 저도 빵이라면 다좋아합니당ㅋㅋㅋ",
          "original_form": "아! 그런거~ 저도 빵이라면 다좋아합니당ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:48"
        },
        {
          "id": "MDRW2100034297.1.14",
          "form": "자칭빵순이에여 ㅋㅋ",
          "original_form": "자칭빵순이에여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:48"
        },
        {
          "id": "MDRW2100034297.1.15",
          "form": "저도요ㅜ",
          "original_form": "저도요ㅜ",
          "speaker_id": "2",
          "time": "20210828 09:50"
        },
        {
          "id": "MDRW2100034297.1.16",
          "form": "밀가루 끊어야하는데",
          "original_form": "밀가루 끊어야하는데",
          "speaker_id": "2",
          "time": "20210828 09:50"
        },
        {
          "id": "MDRW2100034297.1.17",
          "form": "하ㅜ 거진 마약인듯 ㅋㅋ",
          "original_form": "하ㅜ 거진 마약인듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:50"
        },
        {
          "id": "MDRW2100034297.1.18",
          "form": "그거들어보셨어요?",
          "original_form": "그거들어보셨어요?",
          "speaker_id": "2",
          "time": "20210828 09:50"
        },
        {
          "id": "MDRW2100034297.1.19",
          "form": "3대 합법 마약ㅋㅋ",
          "original_form": "3대 합법 마약ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:50"
        },
        {
          "id": "MDRW2100034297.1.20",
          "form": "오오  뭔데오뭔데요??",
          "original_form": "오오  뭔데오뭔데요??",
          "speaker_id": "1",
          "time": "20210828 09:52"
        },
        {
          "id": "MDRW2100034297.1.21",
          "form": "설탕 밀가루 흰쌀이요 ㅋㅋ",
          "original_form": "설탕 밀가루 흰쌀이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100034297.1.22",
          "form": "듣고 바로 저는 끄덕끄덕  했네요",
          "original_form": "듣고 바로 저는 끄덕끄덕  했네요",
          "speaker_id": "2",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100034297.1.23",
          "form": "오우 정답이네요 (확신의 끄덕임",
          "original_form": "오우 정답이네요 (확신의 끄덕임",
          "speaker_id": "1",
          "time": "20210828 13:39"
        }
      ]
    }
  ]
