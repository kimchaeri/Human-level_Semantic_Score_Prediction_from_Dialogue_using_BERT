      "utterance": [
        {
          "id": "MDRW2100039373.1.1",
          "form": "저두요....",
          "original_form": "저두요....",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039373.1.2",
          "form": "혹시 자담치킨 먹어보셨어요?",
          "original_form": "혹시 자담치킨 먹어보셨어요?",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100039373.1.3",
          "form": "헉 아니요?! 맛있나요!?",
          "original_form": "헉 아니요?! 맛있나요!?",
          "speaker_id": "1",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100039373.1.4",
          "form": "앗 저도 안먹어봐서 ㅋㅋㅋ 맵슐랭인가 맛있다고 하더라구요!",
          "original_form": "앗 저도 안먹어봐서 ㅋㅋㅋ 맵슐랭인가 맛있다고 하더라구요!",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100039373.1.5",
          "form": "근데 다음날 속에서 불난다 해서 두려워요ㅋㅋㅋ",
          "original_form": "근데 다음날 속에서 불난다 해서 두려워요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100039373.1.6",
          "form": "좋아하는 치킨 있으세요??",
          "original_form": "좋아하는 치킨 있으세요??",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100039373.1.7",
          "form": "헉.헉 그래도 뭔가 도전하고 싶네요.. 매운 거 완전 좋아하는 ㅠㅠ",
          "original_form": "헉.헉 그래도 뭔가 도전하고 싶네요.. 매운 거 완전 좋아하는 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100039373.1.8",
          "form": "막 최애는 없고 기본 후라이드랑 양념 좋아해요!",
          "original_form": "막 최애는 없고 기본 후라이드랑 양념 좋아해요!",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100039373.1.9",
          "form": "요즘 다양한 거 먹어보고 있는데 다 맛있더라구요",
          "original_form": "요즘 다양한 거 먹어보고 있는데 다 맛있더라구요",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100039373.1.10",
          "form": "최근엔..푸라닭...",
          "original_form": "최근엔..푸라닭...",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100039373.1.11",
          "form": "오오오 역시 기본이 진리긴 해요ㅎㅎㅎ",
          "original_form": "오오오 역시 기본이 진리긴 해요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039373.1.12",
          "form": "오 푸라닭!!! 저도 제일 많이 시켜먹어요",
          "original_form": "오 푸라닭!!! 저도 제일 많이 시켜먹어요",
          "speaker_id": "2",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039373.1.13",
          "form": "블랙알리오 좋아해요!!",
          "original_form": "블랙알리오 좋아해요!!",
          "speaker_id": "2",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039373.1.14",
          "form": "헉 그거 맛있나요? 블랙.. 뭔 맛이에요??",
          "original_form": "헉 그거 맛있나요? 블랙.. 뭔 맛이에요??",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100039373.1.15",
          "form": "걍 간장마늘치킨이에요!!",
          "original_form": "걍 간장마늘치킨이에요!!",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039373.1.16",
          "form": "진짜 무난하게 먹기 좋아요",
          "original_form": "진짜 무난하게 먹기 좋아요",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039373.1.17",
          "form": "푸라닭 뭐 먹어보셨어요??",
          "original_form": "푸라닭 뭐 먹어보셨어요??",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100039373.1.18",
          "form": "투움바 먹어봤어요!",
          "original_form": "투움바 먹어봤어요!",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039373.1.19",
          "form": "오오 전 투움바 떡볶이? 사이드 밖에 안먹어봤는데 맛있더라구요!",
          "original_form": "오오 전 투움바 떡볶이? 사이드 밖에 안먹어봤는데 맛있더라구요!",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039373.1.20",
          "form": "느끼한거 좋아하시면 go추마요도 맛있어요!",
          "original_form": "느끼한거 좋아하시면 go추마요도 맛있어요!",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039373.1.21",
          "form": "와.와..와..... 저 지금 듣기만 해도 설레요",
          "original_form": "와.와..와..... 저 지금 듣기만 해도 설레요",
          "speaker_id": "1",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100039373.1.22",
          "form": "그쵸 블랙알리오랑 go추마요 반반 순살 극락이에여",
          "original_form": "그쵸 블랙알리오랑 go추마요 반반 순살 극락이에여",
          "speaker_id": "2",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100039373.1.23",
          "form": "ㅋㅋㅋㅋㅋ아 넘 웃기는.. 허억 그렇게 반반이 되군요! 꼭 먹어볼게요 맛있겠다",
          "original_form": "ㅋㅋㅋㅋㅋ아 넘 웃기는.. 허억 그렇게 반반이 되군요! 꼭 먹어볼게요 맛있겠다",
          "speaker_id": "1",
          "time": "20210904 09:01"
        },
        {
          "id": "MDRW2100039373.1.24",
          "form": "왕추천이요ㅜㅜ",
          "original_form": "왕추천이요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100039373.1.25",
          "form": "그리고 푸라닭 순살은 다 닭다리라 좋아요!!",
          "original_form": "그리고 푸라닭 순살은 다 닭다리라 좋아요!!",
          "speaker_id": "2",
          "time": "20210904 12:54"
        },
        {
          "id": "MDRW2100039373.1.26",
          "form": "헉 짐짜요?!?!?!?",
          "original_form": "헉 짐짜요?!?!?!?",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100039373.1.27",
          "form": "완전 좋네요 ...",
          "original_form": "완전 좋네요 ...",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100039373.1.28",
          "form": "그쵸... 치밥해먹으면 최고입니다...bbb",
          "original_form": "그쵸... 치밥해먹으면 최고입니다...bbb",
          "speaker_id": "2",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100039373.1.29",
          "form": "와... 치밥.... 침 고여요",
          "original_form": "와... 치밥.... 침 고여요",
          "speaker_id": "1",
