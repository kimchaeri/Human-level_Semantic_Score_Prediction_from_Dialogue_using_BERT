      "utterance": [
        {
          "id": "MDRW2100030907.1.1",
          "form": "탁자에 앉는게",
          "original_form": "탁자에 앉는게",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030907.1.2",
          "form": "허리나 다리에는 좋아요",
          "original_form": "허리나 다리에는 좋아요",
          "speaker_id": "1",
          "time": "20210904 14:06"
        },
        {
          "id": "MDRW2100030907.1.3",
          "form": "맞아요 하지만 의자에 앉아도 양반다리 하게 돼요",
          "original_form": "맞아요 하지만 의자에 앉아도 양반다리 하게 돼요",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.4",
          "form": "양반다리가 다리에 안 좋잖아요",
          "original_form": "양반다리가 다리에 안 좋잖아요",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.5",
          "form": "왜 그런 걸까요",
          "original_form": "왜 그런 걸까요",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.6",
          "form": "그거는 많네요 저도 그러네요",
          "original_form": "그거는 많네요 저도 그러네요",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.7",
          "form": "머리로는 알지만 몸이 따라주지 않네요",
          "original_form": "머리로는 알지만 몸이 따라주지 않네요",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.8",
          "form": "의자에 앉은 것인가 방바닥에 앉은 것인가",
          "original_form": "의자에 앉은 것인가 방바닥에 앉은 것인가",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.9",
          "form": "다리 꼬고 앉는 습관도 고치려고는 하는데 저도 모르게 하게 되네요",
          "original_form": "다리 꼬고 앉는 습관도 고치려고는 하는데 저도 모르게 하게 되네요",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.10",
          "form": "그런 말이 있지요",
          "original_form": "그런 말이 있지요",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.11",
          "form": "편한 자세가 안 좋은 자세다",
          "original_form": "편한 자세가 안 좋은 자세다",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.12",
          "form": "의식적으로 왼쪽으로 하면 오른쪽도 해 주긴 하지만 왼쪽을 한다고 해서 오른쪽으로 끌어오는게 바로 잡아 주진 않고 더 꼬인다고 하더라고요",
          "original_form": "의식적으로 왼쪽으로 하면 오른쪽도 해 주긴 하지만 왼쪽을 한다고 해서 오른쪽으로 끌어오는게 바로 잡아 주진 않고 더 꼬인다고 하더라고요",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.13",
          "form": "백년허리라는 책 아시나요",
          "original_form": "백년허리라는 책 아시나요",
          "speaker_id": "1",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.14",
          "form": "엎드린 것도 좋지 않아요",
          "original_form": "엎드린 것도 좋지 않아요",
          "speaker_id": "2",
          "time": "20210904 14:07"
        },
        {
          "id": "MDRW2100030907.1.15",
          "form": "허리의 1인 잔데 볼 만한 거 같아요",
          "original_form": "허리의 1인 잔데 볼 만한 거 같아요",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030907.1.16",
          "form": "아니요 처음 들어 봐요 좋은 책인가요",
          "original_form": "아니요 처음 들어 봐요 좋은 책인가요",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030907.1.17",
          "form": "엎드리면 안 좋죠",
          "original_form": "엎드리면 안 좋죠",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030907.1.18",
          "form": "추천 감사합니다 나중에 한번 읽어 보도록 할게요",
          "original_form": "추천 감사합니다 나중에 한번 읽어 보도록 할게요",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030907.1.19",
          "form": "네 의사가 쓴 거니까 볼만해요",
          "original_form": "네 의사가 쓴 거니까 볼만해요",
          "speaker_id": "1",
          "time": "20210904 14:08"
        }
      ]
