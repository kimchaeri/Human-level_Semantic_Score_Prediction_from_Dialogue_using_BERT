      "utterance": [
        {
          "id": "MDRW2100041975.1.1",
          "form": "오늘 저녁메뉴먼가요?",
          "original_form": "오늘 저녁메뉴먼가요?",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041975.1.2",
          "form": "오늘 저녁은 굴비입니다",
          "original_form": "오늘 저녁은 굴비입니다",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041975.1.3",
          "form": "저녁 뭐 드세요?",
          "original_form": "저녁 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100041975.1.4",
          "form": "굴비 맛있겠다",
          "original_form": "굴비 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041975.1.5",
          "form": "저는 홈플치킨먹어오",
          "original_form": "저는 홈플치킨먹어오",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041975.1.6",
          "form": "소스는KFC꺼",
          "original_form": "소스는KFC꺼",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041975.1.7",
          "form": "홈플치킨은 안먹어봤는데 어때요?",
          "original_form": "홈플치킨은 안먹어봤는데 어때요?",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041975.1.8",
          "form": "홈플치킨은 그냥 저렴해서 무난하게먹어요",
          "original_form": "홈플치킨은 그냥 저렴해서 무난하게먹어요",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041975.1.9",
          "form": "저는BBQ가맛있더라구요",
          "original_form": "저는BBQ가맛있더라구요",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041975.1.10",
          "form": "KFC꺼는 너무 기름이 많아요",
          "original_form": "KFC꺼는 너무 기름이 많아요",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041975.1.11",
          "form": "맛있기는한데",
          "original_form": "맛있기는한데",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041975.1.12",
          "form": "앗 저도 bbq좋아해용",
          "original_form": "앗 저도 bbq좋아해용",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041975.1.13",
          "form": "홈플치킨은 가격이\n어떻게 되나영",
          "original_form": "홈플치킨은 가격이\n어떻게 되나영",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041975.1.14",
          "form": "제가안사서모르겠어요ㅋㅋㅋ",
          "original_form": "제가안사서모르겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041975.1.15",
          "form": "주문할때도 제가 곌제를안해서ㅜㅜ",
          "original_form": "주문할때도 제가 곌제를안해서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100041975.1.16",
          "form": "ㅋㅋ치킨 생각 없었는데\n치킨 얘기하니 땡기네여",
          "original_form": "ㅋㅋ치킨 생각 없었는데\n치킨 얘기하니 땡기네여",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100041975.1.17",
          "form": "1인1닭",
          "original_form": "1인1닭",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041975.1.18",
          "form": "치킨은 그렇게먹어야되요",
          "original_form": "치킨은 그렇게먹어야되요",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041975.1.19",
          "form": "같이 먹으면",
          "original_form": "같이 먹으면",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041975.1.20",
          "form": "맛있는부분 뺏겨버려서",
          "original_form": "맛있는부분 뺏겨버려서",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041975.1.21",
          "form": "와... 1인1닭....ㅈ",
          "original_form": "와... 1인1닭....ㅈ",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041975.1.22",
          "form": "퍽퍽이파랑",
          "original_form": "퍽퍽이파랑",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041975.1.23",
          "form": "롯데리아1인1닭은 비추",
          "original_form": "롯데리아1인1닭은 비추",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041975.1.24",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041975.1.25",
          "form": "먹어봤는데 진짜 맛없더라구요",
          "original_form": "먹어봤는데 진짜 맛없더라구요",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041975.1.26",
          "form": "ㅋㅋ롯데리아는 햄버거만ㅋㅋ",
          "original_form": "ㅋㅋ롯데리아는 햄버거만ㅋㅋ",
