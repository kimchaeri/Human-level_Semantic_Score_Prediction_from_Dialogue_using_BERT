          "speaker_id": "1",
          "time": "20210822 22:51"
        },
        {
          "id": "MDRW2100009344.1.4",
          "form": "어디로 가는 거여?",
          "original_form": "어디로 가는 거여?",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009344.1.5",
          "form": "해외여",
          "original_form": "해외여",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009344.1.6",
          "form": "님은여?",
          "original_form": "님은여?",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009344.1.7",
          "form": "저도 유럽 가고 싶은데",
          "original_form": "저도 유럽 가고 싶은데",
          "speaker_id": "1",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009344.1.8",
          "form": "전 유럽은 못가봤어요",
          "original_form": "전 유럽은 못가봤어요",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100009344.1.9",
          "form": "아시아만",
          "original_form": "아시아만",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100009344.1.10",
          "form": "동남아요?",
          "original_form": "동남아요?",
          "speaker_id": "1",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009344.1.11",
          "form": "넹 동남아 싸니까",
          "original_form": "넹 동남아 싸니까",
          "speaker_id": "2",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009344.1.12",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210822 22:54"
        },
        {
          "id": "MDRW2100009344.1.13",
          "form": "스페인 좋다고는하는데",
          "original_form": "스페인 좋다고는하는데",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009344.1.14",
          "form": "코로나 끝나면 가보고싶네요",
          "original_form": "코로나 끝나면 가보고싶네요",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
