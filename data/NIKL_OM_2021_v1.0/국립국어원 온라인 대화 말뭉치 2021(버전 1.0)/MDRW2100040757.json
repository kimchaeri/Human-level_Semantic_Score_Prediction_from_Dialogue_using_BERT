      "utterance": [
        {
          "id": "MDRW2100040757.1.1",
          "form": "ㅋㅋㅋㅋ별일이에요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ별일이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 02:03"
        },
        {
          "id": "MDRW2100040757.1.2",
          "form": "지자체마다 무슨 축제 엄청 해요",
          "original_form": "지자체마다 무슨 축제 엄청 해요",
          "speaker_id": "1",
          "time": "20210905 02:03"
        },
        {
          "id": "MDRW2100040757.1.3",
          "form": "그것도 세금도둑 같아요",
          "original_form": "그것도 세금도둑 같아요",
          "speaker_id": "1",
          "time": "20210905 02:03"
        },
        {
          "id": "MDRW2100040757.1.4",
          "form": "ㅋㅋㅋㅋ 맞을거에요",
          "original_form": "ㅋㅋㅋㅋ 맞을거에요",
          "speaker_id": "2",
          "time": "20210905 02:03"
        },
        {
          "id": "MDRW2100040757.1.5",
          "form": "안봐도 뻔해요",
          "original_form": "안봐도 뻔해요",
          "speaker_id": "2",
          "time": "20210905 02:03"
        },
        {
          "id": "MDRW2100040757.1.6",
          "form": "그냥 그런 돈은 저희한테 나눠 주면 안 되나요",
          "original_form": "그냥 그런 돈은 저희한테 나눠 주면 안 되나요",
          "speaker_id": "1",
          "time": "20210905 02:03"
        },
        {
          "id": "MDRW2100040757.1.7",
          "form": "국고낭비 심하네요",
          "original_form": "국고낭비 심하네요",
          "speaker_id": "2",
          "time": "20210905 02:04"
        },
        {
          "id": "MDRW2100040757.1.8",
          "form": "나눠줘도 또 엄청 이상하게 줘서",
          "original_form": "나눠줘도 또 엄청 이상하게 줘서",
          "speaker_id": "2",
          "time": "20210905 02:04"
        },
        {
          "id": "MDRW2100040757.1.9",
          "form": "아무 재미도 없는 거 포스터 만들고 동영상 만들고 사람 뭐 해야 되고 인사한다고 사람 부르고",
          "original_form": "아무 재미도 없는 거 포스터 만들고 동영상 만들고 사람 뭐 해야 되고 인사한다고 사람 부르고",
          "speaker_id": "1",
          "time": "20210905 02:04"
        },
        {
          "id": "MDRW2100040757.1.10",
          "form": "화날거 같은데요 ㅋㅋㅋ",
          "original_form": "화날거 같은데요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 02:04"
        },
        {
          "id": "MDRW2100040757.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 02:04"
        },
        {
          "id": "MDRW2100040757.1.12",
          "form": "분명히 정상적으로는 안줄테닠ㅋㅋㅋ",
          "original_form": "분명히 정상적으로는 안줄테닠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 02:04"
        },
        {
          "id": "MDRW2100040757.1.13",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "1",
          "time": "20210905 02:04"
        },
        {
          "id": "MDRW2100040757.1.14",
          "form": "결국 받는 사람만 또 받게 되겠죠",
          "original_form": "결국 받는 사람만 또 받게 되겠죠",
          "speaker_id": "2",
          "time": "20210905 02:04"
        },
        {
          "id": "MDRW2100040757.1.15",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210905 02:05"
        },
        {
          "id": "MDRW2100040757.1.16",
          "form": "진짜 아는 사람도 아는 사람인제",
          "original_form": "진짜 아는 사람도 아는 사람인제",
          "speaker_id": "2",
          "time": "20210905 02:05"
        },
        {
          "id": "MDRW2100040757.1.17",
          "form": "데",
          "original_form": "데",
          "speaker_id": "2",
          "time": "20210905 02:05"
        },
        {
          "id": "MDRW2100040757.1.18",
          "form": "그들만의 잔치죠",
          "original_form": "그들만의 잔치죠",
          "speaker_id": "1",
          "time": "20210905 02:05"
        },
        {
          "id": "MDRW2100040757.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210905 02:05"
        },
        {
          "id": "MDRW2100040757.1.20",
          "form": "그러니까욬ㅋㅋ",
          "original_form": "그러니까욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 02:05"
        },
        {
          "id": "MDRW2100040757.1.21",
          "form": "초반엔 한사람이 몇번씩 받을수 있게 하고",
          "original_form": "초반엔 한사람이 몇번씩 받을수 있게 하고",
          "speaker_id": "2",
          "time": "20210905 02:05"
        },
        {
          "id": "MDRW2100040757.1.22",
          "form": "나중에 막는 식으로 바꿀수 있으니까요",
          "original_form": "나중에 막는 식으로 바꿀수 있으니까요",
          "speaker_id": "2",
          "time": "20210905 02:05"
        },
        {
          "id": "MDRW2100040757.1.23",
          "form": "네 그죠",
          "original_form": "네 그죠",
          "speaker_id": "1",
          "time": "20210905 02:05"
        },
        {
          "id": "MDRW2100040757.1.24",
          "form": "그냥 뻔해요 하는짓잌ㅋㅋㅋ",
          "original_form": "그냥 뻔해요 하는짓잌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 02:06"
        },
        {
          "id": "MDRW2100040757.1.25",
          "form": "그럴거같아요...",
          "original_form": "그럴거같아요...",
          "speaker_id": "1",
          "time": "20210905 02:06"
        },
        {
          "id": "MDRW2100040757.1.26",
          "form": "네네 ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 02:06"
        },
        {
          "id": "MDRW2100040757.1.27",
          "form": "조금만 머리있으면 뻔히 보이는...",
          "original_form": "조금만 머리있으면 뻔히 보이는...",
          "speaker_id": "1",
          "time": "20210905 02:06"
        },
        {
          "id": "MDRW2100040757.1.28",
          "form": "코로나라 더 잠잠하게 지나가는거 같은데",
          "original_form": "코로나라 더 잠잠하게 지나가는거 같은데",
          "speaker_id": "2",
          "time": "20210905 02:06"
        },
        {
          "id": "MDRW2100040757.1.29",
          "form": "몇년후에 터질지도 몰라요",
          "original_form": "몇년후에 터질지도 몰라요",
          "speaker_id": "2",
          "time": "20210905 02:06"
        }
      ]
    }
