      "utterance": [
        {
          "id": "MDRW2100019892.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019892.1.2",
          "form": "네 집안일해요",
          "original_form": "네 집안일해요",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019892.1.3",
          "form": "어떤 집안일 하세요?",
          "original_form": "어떤 집안일 하세요?",
          "speaker_id": "2",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019892.1.4",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019892.1.5",
          "form": "청소담당이시군요 ㅋㅋㅋㅋ",
          "original_form": "청소담당이시군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019892.1.6",
          "form": "청소할때 제일 힘든점 잇으세요?",
          "original_form": "청소할때 제일 힘든점 잇으세요?",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019892.1.7",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019892.1.8",
          "form": "화장실청소 힘들죠 ㅠㅠ",
          "original_form": "화장실청소 힘들죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019892.1.9",
          "form": "물때는 어찌나 잘 끼는지 ㅠㅠ",
          "original_form": "물때는 어찌나 잘 끼는지 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019892.1.10",
          "form": "곰팡이도요",
          "original_form": "곰팡이도요",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019892.1.11",
          "form": "맞아요 곰팡이ㅠㅠ",
          "original_form": "맞아요 곰팡이ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019892.1.12",
          "form": "청소담당말고 다른 담당으로 변경하실 생각은 없으세요? ㅋㅋ",
          "original_form": "청소담당말고 다른 담당으로 변경하실 생각은 없으세요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019892.1.13",
          "form": "이게 젤 편해오",
          "original_form": "이게 젤 편해오",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019892.1.14",
          "form": "청소 체질이신가봐요 ㅋㅋㅋㅋ",
          "original_form": "청소 체질이신가봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019892.1.15",
          "form": "저도 집안일중에 청소가 제일 좋긴 해요",
          "original_form": "저도 집안일중에 청소가 제일 좋긴 해요",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019892.1.16",
          "form": "맞죠",
          "original_form": "맞죠",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019892.1.17",
          "form": "제일 싫어하는 집안일은 뭐예요?",
          "original_form": "제일 싫어하는 집안일은 뭐예요?",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
