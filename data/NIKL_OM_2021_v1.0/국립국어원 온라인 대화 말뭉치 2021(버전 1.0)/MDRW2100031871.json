      "utterance": [
        {
          "id": "MDRW2100031871.1.1",
          "form": "안녕하세요 키우고싶은 반려동물 있으신가요?",
          "original_form": "안녕하세요 키우고싶은 반려동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 06:46"
        },
        {
          "id": "MDRW2100031871.1.2",
          "form": "반려 인간 한 명있고 어린자식이 있서서 어려울 것 같아요",
          "original_form": "반려 인간 한 명있고 어린자식이 있서서 어려울 것 같아요",
          "speaker_id": "2",
          "time": "20210905 06:48"
        },
        {
          "id": "MDRW2100031871.1.3",
          "form": "그리고  끝까지 책임을 지지 못할 것 같은 생각에",
          "original_form": "그리고  끝까지 책임을 지지 못할 것 같은 생각에",
          "speaker_id": "2",
          "time": "20210905 06:49"
        },
        {
          "id": "MDRW2100031871.1.4",
          "form": "시도도 잘 못하고 있습니다",
          "original_form": "시도도 잘 못하고 있습니다",
          "speaker_id": "2",
          "time": "20210905 06:49"
        },
        {
          "id": "MDRW2100031871.1.5",
          "form": "그렇군요 저는 어릴때",
          "original_form": "그렇군요 저는 어릴때",
          "speaker_id": "1",
          "time": "20210905 06:51"
        },
        {
          "id": "MDRW2100031871.1.6",
          "form": "청거북이 키워봤어요",
          "original_form": "청거북이 키워봤어요",
          "speaker_id": "1",
          "time": "20210905 06:51"
        },
        {
          "id": "MDRW2100031871.1.7",
          "form": "5",
          "original_form": "5",
          "speaker_id": "2",
          "time": "20210905 06:51"
        },
        {
          "id": "MDRW2100031871.1.8",
          "form": "오 저도거북이 키웠습니다",
          "original_form": "오 저도거북이 키웠습니다",
          "speaker_id": "2",
          "time": "20210905 06:51"
        },
        {
          "id": "MDRW2100031871.1.9",
          "form": "거 우리나라 같이 생긴 수죠",
          "original_form": "거 우리나라 같이 생긴 수죠",
          "speaker_id": "2",
          "time": "20210905 06:52"
        },
        {
          "id": "MDRW2100031871.1.10",
          "form": "거북이 귀엽더라고요",
          "original_form": "거북이 귀엽더라고요",
          "speaker_id": "1",
          "time": "20210905 06:52"
        },
        {
          "id": "MDRW2100031871.1.11",
          "form": "완전 귀여웠어요",
          "original_form": "완전 귀여웠어요",
          "speaker_id": "2",
          "time": "20210905 06:52"
        },
        {
          "id": "MDRW2100031871.1.12",
          "form": "두 마리 같이 키웠어요",
          "original_form": "두 마리 같이 키웠어요",
          "speaker_id": "1",
          "time": "20210905 06:52"
        },
        {
          "id": "MDRW2100031871.1.13",
          "form": "사료도 주고 가끔 돼지고기도 줬어요",
          "original_form": "사료도 주고 가끔 돼지고기도 줬어요",
          "speaker_id": "1",
          "time": "20210905 06:52"
        },
        {
          "id": "MDRW2100031871.1.14",
          "form": "혼자 있으면 외로울 거 같습니다",
          "original_form": "혼자 있으면 외로울 거 같습니다",
          "speaker_id": "2",
          "time": "20210905 06:52"
        },
        {
          "id": "MDRW2100031871.1.15",
          "form": "네 2마리 같이 사니까 외롭지 않아 보였어요",
          "original_form": "네 2마리 같이 사니까 외롭지 않아 보였어요",
          "speaker_id": "1",
          "time": "20210905 06:52"
        },
        {
          "id": "MDRW2100031871.1.16",
          "form": "특 식도 있었네요",
          "original_form": "특 식도 있었네요",
          "speaker_id": "2",
          "time": "20210905 06:52"
        },
        {
          "id": "MDRW2100031871.1.17",
          "form": "네 그렇죠 ㅎㅎ 돼지고기",
          "original_form": "네 그렇죠 ㅎㅎ 돼지고기",
          "speaker_id": "1",
          "time": "20210905 06:53"
        },
        {
          "id": "MDRW2100031871.1.18",
          "form": "강아지랑 고양이 좋아하세요?",
          "original_form": "강아지랑 고양이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 06:54"
        },
        {
          "id": "MDRW2100031871.1.19",
          "form": "강아지랑 도 잘 놀고 고양이랑 도 잘 놀아요",
          "original_form": "강아지랑 도 잘 놀고 고양이랑 도 잘 놀아요",
          "speaker_id": "2",
          "time": "20210905 06:55"
        },
        {
          "id": "MDRW2100031871.1.20",
          "form": "큰 강아지도 좋아하고",
          "original_form": "큰 강아지도 좋아하고",
          "speaker_id": "2",
          "time": "20210905 06:55"
        },
        {
          "id": "MDRW2100031871.1.21",
          "form": "동물이랑 친하게 잘 지내시나 봐요",
          "original_form": "동물이랑 친하게 잘 지내시나 봐요",
          "speaker_id": "1",
          "time": "20210905 06:55"
        }
