      "utterance": [
        {
          "id": "MDRW2100044627.1.1",
          "form": "나 어제 부산 갔다왔는데 좋더라",
          "original_form": "나 어제 부산 갔다왔는데 좋더라",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044627.1.2",
          "form": "내일은 출근 아싸는 애한테 빌려뒀어ㅋ",
          "original_form": "내일은 출근 아싸는 애한테 빌려뒀어ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044627.1.3",
          "form": "다행ㅋ",
          "original_form": "다행ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044627.1.4",
          "form": "일주일만 참음ㅋ 추석ㅋ",
          "original_form": "일주일만 참음ㅋ 추석ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044627.1.5",
          "form": "창원가야지ㅋ",
          "original_form": "창원가야지ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044627.1.6",
          "form": "오 다행이다 창원와우",
          "original_form": "오 다행이다 창원와우",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044627.1.7",
          "form": "부산드라이브 잘했나?",
          "original_form": "부산드라이브 잘했나?",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044627.1.8",
          "form": "ㅇㅇ생각보다 괜찮았어",
          "original_form": "ㅇㅇ생각보다 괜찮았어",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044627.1.9",
          "form": "시간적인 여유만 있음 더 바다보이는곳 드라이브했을텐데",
          "original_form": "시간적인 여유만 있음 더 바다보이는곳 드라이브했을텐데",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044627.1.10",
          "form": "시간이없어서",
          "original_form": "시간이없어서",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044627.1.11",
          "form": "난 조만간 창원갈거라 좋다ㅋㅋ",
          "original_form": "난 조만간 창원갈거라 좋다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044627.1.12",
          "form": "ㅇㅇ 씬나겠엉 ㅋㅋ",
          "original_form": "ㅇㅇ 씬나겠엉 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044627.1.13",
          "form": "언제와서 언제가?",
          "original_form": "언제와서 언제가?",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044627.1.14",
          "form": "응ㅎ",
          "original_form": "응ㅎ",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044627.1.15",
          "form": "15일 가서 26일",
          "original_form": "15일 가서 26일",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044627.1.16",
          "form": "이번에 길어",
          "original_form": "이번에 길어",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044627.1.17",
          "form": "사람들 덜 움직일때",
          "original_form": "사람들 덜 움직일때",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044627.1.18",
          "form": "와 대박사건",
          "original_form": "와 대박사건",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044627.1.19",
          "form": "부모님이랑 어디갈수있게따",
          "original_form": "부모님이랑 어디갈수있게따",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044627.1.20",
          "form": "어디가려나 안갈걸ㅎ",
          "original_form": "어디가려나 안갈걸ㅎ",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044627.1.21",
          "form": "다들 일하신다들은듯",
          "original_form": "다들 일하신다들은듯",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044627.1.22",
          "form": "ㅜㅜ아쉽당",
          "original_form": "ㅜㅜ아쉽당",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044627.1.23",
          "form": "내는 집순이 행복해야징ㅋ",
          "original_form": "내는 집순이 행복해야징ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044627.1.24",
          "form": "차로가면 여행도 안전한디",
          "original_form": "차로가면 여행도 안전한디",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044627.1.25",
          "form": "창원집순이는 1년에 2번가능해서 귀해ㅋㅋ",
          "original_form": "창원집순이는 1년에 2번가능해서 귀해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044627.1.26",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044627.1.27",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044627.1.28",
          "form": "여행은 무리일듯",
          "original_form": "여행은 무리일듯",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044627.1.29",
          "form": "더군다나 추석연휴에 다니긴 좀 그랭ㅎ",
          "original_form": "더군다나 추석연휴에 다니긴 좀 그랭ㅎ",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044627.1.30",
          "form": "여행 최근에 어디갔었어",
          "original_form": "여행 최근에 어디갔었어",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044627.1.31",
          "form": "여행은 안간듯한데ㅎ",
          "original_form": "여행은 안간듯한데ㅎ",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044627.1.32",
          "form": "사람 없는곳 없을까",
          "original_form": "사람 없는곳 없을까",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044627.1.33",
          "form": "드라이브가는거 좋더라",
          "original_form": "드라이브가는거 좋더라",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044627.1.34",
          "form": "어디 가게 가는것도 진짜 어쩌다 한번 가니까\n가고싶긴한데 한편 불안하긴해",
          "original_form": "어디 가게 가는것도 진짜 어쩌다 한번 가니까\n가고싶긴한데 한편 불안하긴해",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044627.1.35",
          "form": "여행가기엔 코로나라",
          "original_form": "여행가기엔 코로나라",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044627.1.36",
          "form": "사람들 캠핑 많이 가더라",
          "original_form": "사람들 캠핑 많이 가더라",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044627.1.37",
          "form": "외부라 긍가",
          "original_form": "외부라 긍가",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044627.1.38",
          "form": "캠핑도 괜찮은데 캠핑용품이 없어서",
          "original_form": "캠핑도 괜찮은데 캠핑용품이 없어서",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044627.1.39",
          "form": "그리고 난 편한게 좋더라 ㅋㅋ",
          "original_form": "그리고 난 편한게 좋더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:24"
        }
      ]
    }
