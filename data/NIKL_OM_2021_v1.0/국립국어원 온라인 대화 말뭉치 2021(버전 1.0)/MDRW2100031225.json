      "utterance": [
        {
          "id": "MDRW2100031225.1.1",
          "form": "이제 놀면뭐하니 봐야지",
          "original_form": "이제 놀면뭐하니 봐야지",
          "speaker_id": "1",
          "time": "20210904 18:31"
        },
        {
          "id": "MDRW2100031225.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:31"
        },
        {
          "id": "MDRW2100031225.1.3",
          "form": "무도 나와서 보려구",
          "original_form": "무도 나와서 보려구",
          "speaker_id": "1",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031225.1.4",
          "form": "야구랑 왔다리 갔다리 볼건데 아마 놀뭐",
          "original_form": "야구랑 왔다리 갔다리 볼건데 아마 놀뭐",
          "speaker_id": "1",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031225.1.5",
          "form": "더 열심히 볼듯",
          "original_form": "더 열심히 볼듯",
          "speaker_id": "1",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031225.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ무한도전 엄청 좋아했었잖아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ무한도전 엄청 좋아했었잖아",
          "speaker_id": "2",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031225.1.7",
          "form": "맞아 무도는 진리야!",
          "original_form": "맞아 무도는 진리야!",
          "speaker_id": "1",
          "time": "20210904 18:33"
        },
        {
          "id": "MDRW2100031225.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ놀뭐에서 요즘 무도 확장판 해",
          "original_form": "ㅋㅋㅋㅋㅋㅋ놀뭐에서 요즘 무도 확장판 해",
          "speaker_id": "1",
          "time": "20210904 18:34"
        },
        {
          "id": "MDRW2100031225.1.9",
          "form": "맞아맞아 사람들이 노홍철 정형돈 못 나와서",
          "original_form": "맞아맞아 사람들이 노홍철 정형돈 못 나와서",
          "speaker_id": "2",
          "time": "20210904 18:35"
        },
        {
          "id": "MDRW2100031225.1.10",
          "form": "아쉽다고 하더라",
          "original_form": "아쉽다고 하더라",
          "speaker_id": "2",
          "time": "20210904 18:35"
        },
        {
          "id": "MDRW2100031225.1.11",
          "form": "왜나오냐고ㅡㅡ;",
          "original_form": "왜나오냐고ㅡㅡ;",
          "speaker_id": "1",
          "time": "20210904 18:37"
        },
        {
          "id": "MDRW2100031225.1.12",
          "form": "진심 그게 악플이지머야",
          "original_form": "진심 그게 악플이지머야",
          "speaker_id": "1",
          "time": "20210904 18:37"
        },
        {
          "id": "MDRW2100031225.1.13",
          "form": "난볼때마다 어이없어",
          "original_form": "난볼때마다 어이없어",
          "speaker_id": "1",
          "time": "20210904 18:37"
        },
        {
          "id": "MDRW2100031225.1.14",
          "form": "응?",
          "original_form": "응?",
          "speaker_id": "2",
          "time": "20210904 18:38"
        },
        {
          "id": "MDRW2100031225.1.15",
          "form": "뭔가 말이 잘렸어 가끔 이러더라",
          "original_form": "뭔가 말이 잘렸어 가끔 이러더라",
          "speaker_id": "2",
          "time": "20210904 18:38"
        },
        {
          "id": "MDRW2100031225.1.16",
          "form": "무도 종영 1주년도 다같이햇구만 ㅋㅋㅋ 그 막 사람들이 광희 세호 세형 왜나오냐고",
          "original_form": "무도 종영 1주년도 다같이햇구만 ㅋㅋㅋ 그 막 사람들이 광희 세호 세형 왜나오냐고",
          "speaker_id": "1",
          "time": "20210904 18:39"
        },
        {
          "id": "MDRW2100031225.1.17",
          "form": "자꾸 그래가지고",
          "original_form": "자꾸 그래가지고",
          "speaker_id": "1",
          "time": "20210904 18:39"
        },
        {
          "id": "MDRW2100031225.1.18",
          "form": "짜증나 사람들^^",
          "original_form": "짜증나 사람들^^",
          "speaker_id": "1",
          "time": "20210904 18:39"
        },
        {
          "id": "MDRW2100031225.1.19",
          "form": "아아 응응 그러게",
          "original_form": "아아 응응 그러게",
          "speaker_id": "2",
          "time": "20210904 18:39"
        },
        {
          "id": "MDRW2100031225.1.20",
          "form": "싫으면 무도팬 안해야지ㅋㅋㅋ",
          "original_form": "싫으면 무도팬 안해야지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:39"
        },
        {
          "id": "MDRW2100031225.1.21",
          "form": "그런 사람들 있어서 무도 다시하는게 힘들다고 하더라",
          "original_form": "그런 사람들 있어서 무도 다시하는게 힘들다고 하더라",
          "speaker_id": "2",
          "time": "20210904 18:39"
        },
        {
          "id": "MDRW2100031225.1.22",
          "form": "원하는게 다 달라서",
          "original_form": "원하는게 다 달라서",
          "speaker_id": "2",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100031225.1.23",
          "form": "김태호도 다들고맙다고 인터뷰한 마당에",
          "original_form": "김태호도 다들고맙다고 인터뷰한 마당에",
          "speaker_id": "1",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100031225.1.24",
          "form": "아직까지도 걔네빼라",
          "original_form": "아직까지도 걔네빼라",
          "speaker_id": "1",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100031225.1.25",
          "form": "이소리를ㅡㅠ",
          "original_form": "이소리를ㅡㅠ",
          "speaker_id": "1",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100031225.1.26",
          "form": "뭐 어린 친구들도 많으니까",
          "original_form": "뭐 어린 친구들도 많으니까",
          "speaker_id": "2",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100031225.1.27",
          "form": "얼마나 상처겠어 그리고같은 동료인데 어떻게 물자르듯이 버리냐",
          "original_form": "얼마나 상처겠어 그리고같은 동료인데 어떻게 물자르듯이 버리냐",
          "speaker_id": "1",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100031225.1.28",
          "form": "사람들 웃겨",
          "original_form": "사람들 웃겨",
          "speaker_id": "1",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100031225.1.29",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100031225.1.30",
          "form": "그럴거면 재방 봐야지",
          "original_form": "그럴거면 재방 봐야지",
          "speaker_id": "1",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100031225.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ난 재밌을 것 같던데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ난 재밌을 것 같던데",
          "speaker_id": "2",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100031225.1.32",
          "form": "머리도 잘 썼고 크루인가 패밀리인가",
          "original_form": "머리도 잘 썼고 크루인가 패밀리인가",
          "speaker_id": "2",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100031225.1.33",
          "form": "매주 안 나오는거",
          "original_form": "매주 안 나오는거",
          "speaker_id": "2",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100031225.1.34",
          "form": "근데 누구 버리구이런거 싫다는거지 솔직히나도 어릴때",
          "original_form": "근데 누구 버리구이런거 싫다는거지 솔직히나도 어릴때",
          "speaker_id": "1",
          "time": "20210904 18:44"
        },
        {
          "id": "MDRW2100031225.1.35",
          "form": "굴러들어왔다",
          "original_form": "굴러들어왔다",
          "speaker_id": "1",
          "time": "20210904 18:44"
        },
        {
          "id": "MDRW2100031225.1.36",
          "form": "이소리 했는데",
          "original_form": "이소리 했는데",
          "speaker_id": "1",
          "time": "20210904 18:44"
        },
        {
          "id": "MDRW2100031225.1.37",
          "form": "진짜 반성함 악플러야 그거",
          "original_form": "진짜 반성함 악플러야 그거",
          "speaker_id": "1",
          "time": "20210904 18:45"
        },
        {
          "id": "MDRW2100031225.1.38",
          "form": "그치 다들 열심히 일하는 사람들인데",
          "original_form": "그치 다들 열심히 일하는 사람들인데",
          "speaker_id": "2",
          "time": "20210904 18:45"
        },
        {
          "id": "MDRW2100031225.1.39",
          "form": "마쟝...!",
          "original_form": "마쟝...!",
          "speaker_id": "1",
          "time": "20210904 18:48"
        },
        {
          "id": "MDRW2100031225.1.40",
          "form": "놀면뭐하니 안봥?",
          "original_form": "놀면뭐하니 안봥?",
          "speaker_id": "1",
          "time": "20210904 18:49"
        }
      ]
    }
