      "utterance": [
        {
          "id": "MDRW2100033503.1.1",
          "form": "좀 웃긴데",
          "original_form": "좀 웃긴데",
          "speaker_id": "1",
          "time": "20210824 08:17"
        },
        {
          "id": "MDRW2100033503.1.2",
          "form": "그 배우분이 체격도 있으시지만",
          "original_form": "그 배우분이 체격도 있으시지만",
          "speaker_id": "1",
          "time": "20210824 08:17"
        },
        {
          "id": "MDRW2100033503.1.3",
          "form": "키도 있으시지만",
          "original_form": "키도 있으시지만",
          "speaker_id": "1",
          "time": "20210824 08:17"
        },
        {
          "id": "MDRW2100033503.1.4",
          "form": "미혼들은 좀더 꿈과 희망을 가져야한대요 ㅋㅋㅋ",
          "original_form": "미혼들은 좀더 꿈과 희망을 가져야한대요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 09:13"
        },
        {
          "id": "MDRW2100033503.1.5",
          "form": "가고싶은 여행지 있으세요",
          "original_form": "가고싶은 여행지 있으세요",
          "speaker_id": "1",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100033503.1.6",
          "form": "해외를 못가봐서 스페인 가보고싶어요",
          "original_form": "해외를 못가봐서 스페인 가보고싶어요",
          "speaker_id": "2",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100033503.1.7",
          "form": "스페인이면 그 예능 생각나네요 스페인하숙이요 기억나세요?",
          "original_form": "스페인이면 그 예능 생각나네요 스페인하숙이요 기억나세요?",
          "speaker_id": "1",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100033503.1.8",
          "form": "그거 재밌게봤어요",
          "original_form": "그거 재밌게봤어요",
          "speaker_id": "2",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100033503.1.9",
          "form": "산티애고 순례길",
          "original_form": "산티애고 순례길",
          "speaker_id": "2",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100033503.1.10",
          "form": "다양한 사람들의 이야기를 들을 수 있어서 좋았어요",
          "original_form": "다양한 사람들의 이야기를 들을 수 있어서 좋았어요",
          "speaker_id": "2",
          "time": "20210824 09:40"
        },
        {
          "id": "MDRW2100033503.1.11",
          "form": "아 저도 요새 많이 걸어서 그 길을 걸어보고 싶더라고요",
          "original_form": "아 저도 요새 많이 걸어서 그 길을 걸어보고 싶더라고요",
          "speaker_id": "1",
          "time": "20210824 09:40"
        },
        {
          "id": "MDRW2100033503.1.12",
          "form": "다녀온 사람들 말 들어보면 좋다고 하더라구요",
          "original_form": "다녀온 사람들 말 들어보면 좋다고 하더라구요",
          "speaker_id": "2",
          "time": "20210824 09:40"
        },
        {
          "id": "MDRW2100033503.1.13",
          "form": "약간 무장정 걷는게 무슨 의미가 있는가 했다가 걷다보면 생각도 정리되고 터닝포인트도 생긴다더라구요",
          "original_form": "약간 무장정 걷는게 무슨 의미가 있는가 했다가 걷다보면 생각도 정리되고 터닝포인트도 생긴다더라구요",
          "speaker_id": "2",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100033503.1.14",
          "form": "name1님은 가고싶은 여행지 있으신가요",
          "original_form": "&name1&님은 가고싶은 여행지 있으신가요",
          "speaker_id": "2",
          "time": "20210824 09:41"
        },
        {
          "id": "MDRW2100033503.1.15",
          "form": "저는 가까운 일본이요",
          "original_form": "저는 가까운 일본이요",
          "speaker_id": "1",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100033503.1.16",
          "form": "부담없게?",
          "original_form": "부담없게?",
          "speaker_id": "1",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100033503.1.17",
          "form": "일본 가까워서 좋죠",
          "original_form": "일본 가까워서 좋죠",
          "speaker_id": "2",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100033503.1.18",
          "form": "그리고 일본 료칸? 가보고싶어요",
          "original_form": "그리고 일본 료칸? 가보고싶어요",
          "speaker_id": "1",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100033503.1.19",
          "form": "료칸? 거기는 뭐가있나요",
          "original_form": "료칸? 거기는 뭐가있나요",
          "speaker_id": "2",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100033503.1.20",
          "form": "전통식 여관인데",
          "original_form": "전통식 여관인데",
          "speaker_id": "1",
          "time": "20210824 09:44"
        },
        {
          "id": "MDRW2100033503.1.21",
          "form": "아 지명인줄 알았어요ㅋㅋ",
          "original_form": "아 지명인줄 알았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:45"
        },
        {
          "id": "MDRW2100033503.1.22",
          "form": "미디어에서 봤던 기억이 있네요",
          "original_form": "미디어에서 봤던 기억이 있네요",
          "speaker_id": "2",
          "time": "20210824 09:45"
        },
        {
          "id": "MDRW2100033503.1.23",
          "form": "제가 일드를 많이봐서 약간 환상?이 있어요 ㅋㅋ",
          "original_form": "제가 일드를 많이봐서 약간 환상?이 있어요 ㅋㅋ",
