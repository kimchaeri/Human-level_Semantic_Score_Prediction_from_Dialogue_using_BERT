      "utterance": [
        {
          "id": "MDRW2100002570.1.1",
          "form": "영화 최근에 뭐보셨슴까",
          "original_form": "영화 최근에 뭐보셨슴까",
          "speaker_id": "1",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002570.1.2",
          "form": "극장을 잘 안가서 집에서 시동봤어요",
          "original_form": "극장을 잘 안가서 집에서 시동봤어요",
          "speaker_id": "2",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002570.1.3",
          "form": "혹시 아시나요?",
          "original_form": "혹시 아시나요?",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002570.1.4",
          "form": "마동석 나오는거 맞나요????",
          "original_form": "마동석 나오는거 맞나요????",
          "speaker_id": "1",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002570.1.5",
          "form": "어 맞아요!",
          "original_form": "어 맞아요!",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002570.1.6",
          "form": "저도 처음 들어보는 영화였는데 시간떼우기 좋더라구요",
          "original_form": "저도 처음 들어보는 영화였는데 시간떼우기 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002570.1.7",
          "form": "name1님은 최근에 본 영화있나요?",
          "original_form": "&name1&님은 최근에 본 영화있나요?",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002570.1.8",
          "form": "저도 마동석 나온",
          "original_form": "저도 마동석 나온",
          "speaker_id": "1",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002570.1.9",
          "form": "부산행을 뒤늦게 봤습니다!!!!",
          "original_form": "부산행을 뒤늦게 봤습니다!!!!",
          "speaker_id": "1",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002570.1.10",
          "form": "너무ㅠㅠㅠㅠㅠ재밌었어요ㅋㅋ",
          "original_form": "너무ㅠㅠㅠㅠㅠ재밌었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002570.1.11",
          "form": "부산행 재밌는데 넘 쫄렸어요",
          "original_form": "부산행 재밌는데 넘 쫄렸어요",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002570.1.12",
          "form": "약간 한국좀비는 매운맛",
          "original_form": "약간 한국좀비는 매운맛",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002570.1.13",
          "form": "진정한 광기",
          "original_form": "진정한 광기",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002570.1.14",
          "form": "좀비들 스피드가 달라요",
          "original_form": "좀비들 스피드가 달라요",
          "speaker_id": "1",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002570.1.15",
          "form": "진짜미쳤엉욬ㅋㅋㅋ",
          "original_form": "진짜미쳤엉욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002570.1.16",
          "form": "외국놈은 느릿하던디",
          "original_form": "외국놈은 느릿하던디",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002570.1.17",
          "form": "한국놈은 진짜",
          "original_form": "한국놈은 진짜",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002570.1.18",
          "form": "와씨 잡히면 끝이다 생각이 절로들더라구요",
          "original_form": "와씨 잡히면 끝이다 생각이 절로들더라구요",
          "speaker_id": "2",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002570.1.19",
          "form": "그니까옄ㅋㅋㅋㅋ",
          "original_form": "그니까옄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002570.1.20",
          "form": "역시 한국은 좀비도 빨라...",
          "original_form": "역시 한국은 좀비도 빨라...",
          "speaker_id": "1",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002570.1.21",
          "form": "외국좀비보다가 한국좀비보니까",
          "original_form": "외국좀비보다가 한국좀비보니까",
          "speaker_id": "1",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002570.1.22",
          "form": "외국좀비가 넘 레벨이 낮게느껴지고",
          "original_form": "외국좀비가 넘 레벨이 낮게느껴지고",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002570.1.23",
          "form": "역시..K 빨리빨리의 나라",
          "original_form": "역시..K 빨리빨리의 나라",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002570.1.24",
          "form": "좀비 승질머리도 빨리빨리",
          "original_form": "좀비 승질머리도 빨리빨리",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002570.1.25",
          "form": "느리면 진짜 살아남을 수 없는",
          "original_form": "느리면 진짜 살아남을 수 없는",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002570.1.26",
          "form": "엑소시즘도ㅋㅋ우리나라가 더 재밌어요",
          "original_form": "엑소시즘도ㅋㅋ우리나라가 더 재밌어요",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002570.1.27",
          "form": "검은사제들이나",
          "original_form": "검은사제들이나",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002570.1.28",
          "form": "사바하같은",
          "original_form": "사바하같은",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002570.1.29",
          "form": "진짜ㅋㅋ",
          "original_form": "진짜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002570.1.30",
          "form": "외국은 잔잔바리인데 한국은 그 매운맛이있어요",
          "original_form": "외국은 잔잔바리인데 한국은 그 매운맛이있어요",
          "speaker_id": "2",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002570.1.31",
          "form": "그니까옄ㅋㅋ",
          "original_form": "그니까옄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002570.1.32",
          "form": "한국은 쎄고 굵고",
          "original_form": "한국은 쎄고 굵고",
