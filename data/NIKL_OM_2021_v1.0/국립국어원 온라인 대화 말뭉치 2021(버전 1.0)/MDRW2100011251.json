      "utterance": [
        {
          "id": "MDRW2100011251.1.1",
          "form": "글체~난도 이카다 내 힘들은 걍 모른척함 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "글체~난도 이카다 내 힘들은 걍 모른척함 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100011251.1.2",
          "form": "난도 살아야 한다 ㅋㅋㅋㅋㅋ",
          "original_form": "난도 살아야 한다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100011251.1.3",
          "form": "그래 나도 그래",
          "original_form": "그래 나도 그래",
          "speaker_id": "1",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100011251.1.4",
          "form": "그래서 요즘엔 걱정 덜한다",
          "original_form": "그래서 요즘엔 걱정 덜한다",
          "speaker_id": "1",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100011251.1.5",
          "form": "다들 잘하더라고ㅋㅋ",
          "original_form": "다들 잘하더라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:06"
        },
        {
          "id": "MDRW2100011251.1.6",
          "form": "내가 새치생기겠다싶어서ㅋㅋ",
          "original_form": "내가 새치생기겠다싶어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:07"
        },
        {
          "id": "MDRW2100011251.1.7",
          "form": "이젠 덜하려고 한다ㅋㅋ",
          "original_form": "이젠 덜하려고 한다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:07"
        },
        {
          "id": "MDRW2100011251.1.8",
          "form": "맞다맞아~내가 이럴게 걱정하면 당사자들도 그게 느껴져서 ㅋㅋㅋㅋㅋ",
          "original_form": "맞다맞아~내가 이럴게 걱정하면 당사자들도 그게 느껴져서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011251.1.9",
          "form": "한번씯 버럭할때 있음 ㅋㅋㅋ",
          "original_form": "한번씯 버럭할때 있음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011251.1.10",
          "form": "내가 해달라고했나캄서 ㅋㅋㅋㅋㅋ",
          "original_form": "내가 해달라고했나캄서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011251.1.11",
          "form": "나 오타 왜이래 많너??ㅋㅋ",
          "original_form": "나 오타 왜이래 많너??ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011251.1.12",
          "form": "ㅋㅋㅋㅋ웃겨",
          "original_form": "ㅋㅋㅋㅋ웃겨",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011251.1.13",
          "form": "그러게ㅋㅋ",
          "original_form": "그러게ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011251.1.14",
          "form": "수술 잘될거니 너무 걱정말고ㅋㅋ",
          "original_form": "수술 잘될거니 너무 걱정말고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:14"
        },
        {
          "id": "MDRW2100011251.1.15",
          "form": "오타는 신경 좀 쓰고ㅋㅋㅋㅋㅋㅋㄱㅋㅋ아 웃겨",
          "original_form": "오타는 신경 좀 쓰고ㅋㅋㅋㅋㅋㅋㄱㅋㅋ아 웃겨",
          "speaker_id": "1",
          "time": "20210824 10:14"
        },
        {
          "id": "MDRW2100011251.1.16",
          "form": "응응~우리도 건강하쟈^^",
          "original_form": "응응~우리도 건강하쟈^^",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100011251.1.17",
          "form": "가족도 중요하지만~우리도 소중하니^^",
          "original_form": "가족도 중요하지만~우리도 소중하니^^",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100011251.1.18",
          "form": "그래 나는 진짜",
          "original_form": "그래 나는 진짜",
          "speaker_id": "1",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100011251.1.19",
          "form": "조금만 아프면 병원간다ㅋㅋ",
          "original_form": "조금만 아프면 병원간다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100011251.1.20",
          "form": "우리가 건강해야지",
          "original_form": "우리가 건강해야지",
          "speaker_id": "1",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100011251.1.21",
          "form": "식구들도 힘난다 잘살고 ㅋ",
          "original_form": "식구들도 힘난다 잘살고 ㅋ",
          "speaker_id": "1",
          "time": "20210824 10:16"
        },
        {
          "id": "MDRW2100011251.1.22",
          "form": "맞다~엄마가 무너지면 진짜 답없다 ㅋㅋㅋㅋ",
          "original_form": "맞다~엄마가 무너지면 진짜 답없다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:16"
        },
        {
          "id": "MDRW2100011251.1.23",
          "form": "잘하고있네이^^",
          "original_form": "잘하고있네이^^",
          "speaker_id": "2",
          "time": "20210824 10:16"
        },
        {
          "id": "MDRW2100011251.1.24",
          "form": "맛있는것도 많이 먹고 ㅋㅋㅋ",
          "original_form": "맛있는것도 많이 먹고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100011251.1.25",
