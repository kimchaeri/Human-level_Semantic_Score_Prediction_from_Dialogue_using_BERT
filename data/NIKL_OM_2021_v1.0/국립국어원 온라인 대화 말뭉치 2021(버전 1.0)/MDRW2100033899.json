      "utterance": [
        {
          "id": "MDRW2100033899.1.1",
          "form": "요즘 보는 프로그램 있으신가요?",
          "original_form": "요즘 보는 프로그램 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033899.1.2",
          "form": "해방타운이요ㅋㅋㅋ",
          "original_form": "해방타운이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100033899.1.3",
          "form": "오 저도 그거 가끔 챙겨봐요ㅋㅋ",
          "original_form": "오 저도 그거 가끔 챙겨봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100033899.1.4",
          "form": "장윤정님이 요리 맛있게 혼자서 해드시더라구요",
          "original_form": "장윤정님이 요리 맛있게 혼자서 해드시더라구요",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100033899.1.5",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100033899.1.6",
          "form": "오오 이프로 아시는군요?",
          "original_form": "오오 이프로 아시는군요?",
          "speaker_id": "2",
          "time": "20210825 15:19"
        },
        {
          "id": "MDRW2100033899.1.7",
          "form": "네 한번 봤는데 재밌어서 가끔 봐요ㅎㅎ",
          "original_form": "네 한번 봤는데 재밌어서 가끔 봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100033899.1.8",
          "form": "ㅋㅋㅋ 백지영도",
          "original_form": "ㅋㅋㅋ 백지영도",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100033899.1.9",
          "form": "나와요 요즘엔",
          "original_form": "나와요 요즘엔",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100033899.1.10",
          "form": "오 그건 못봤어요 봐야겠네요",
          "original_form": "오 그건 못봤어요 봐야겠네요",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100033899.1.11",
          "form": "ㅋㅋㅋ ㅋ 소소하게재밌더라고요",
          "original_form": "ㅋㅋㅋ ㅋ 소소하게재밌더라고요",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100033899.1.12",
          "form": "맞아요 소재가 참 좋은 것 같아요~",
          "original_form": "맞아요 소재가 참 좋은 것 같아요~",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100033899.1.13",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100033899.1.14",
          "form": "걍 밥먹으면서 틀어놓고보기좋은",
          "original_form": "걍 밥먹으면서 틀어놓고보기좋은",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100033899.1.15",
          "form": "그쵸그쵸ㅋㅋ 요즘 그런 소소한 프로그램 좋더라구요",
          "original_form": "그쵸그쵸ㅋㅋ 요즘 그런 소소한 프로그램 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100033899.1.16",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033899.1.17",
          "form": "크게고민안해는",
          "original_form": "크게고민안해는",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033899.1.18",
          "form": "맞아요 보기편하더라구요ㅎㅎ",
