        {
          "id": "MDRW2100002096.1.1",
          "form": "오늘 저녁메뉴는 ?",
          "original_form": "오늘 저녁메뉴는 ?",
          "speaker_id": "2",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002096.1.2",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002096.1.3",
          "form": "오 맛나겠네요",
          "original_form": "오 맛나겠네요",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002096.1.4",
          "form": "좋아하는 치킨가게?",
          "original_form": "좋아하는 치킨가게?",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002096.1.5",
          "form": "네네치킨 좋아요",
          "original_form": "네네치킨 좋아요",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002096.1.6",
          "form": "네네치킨 파닭도 맛나는데ㅠ",
          "original_form": "네네치킨 파닭도 맛나는데ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002096.1.7",
          "form": "내일은 배달시켜먹어야겟당",
          "original_form": "내일은 배달시켜먹어야겟당",
          "speaker_id": "2",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002096.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002096.1.9",
          "form": "불금은 배달이죠",
          "original_form": "불금은 배달이죠",
          "speaker_id": "1",
          "time": "20210819 23:39"
        },
        {
          "id": "MDRW2100002096.1.10",
          "form": "배달음식에 한잔해야죠 ㅋㅋ",
          "original_form": "배달음식에 한잔해야죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002096.1.11",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "1",
          "time": "20210819 23:40"
        },
        {
          "id": "MDRW2100002096.1.12",
          "form": "밤되니 배고프네요",
          "original_form": "밤되니 배고프네요",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002096.1.13",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210819 23:42"
        },
        {
          "id": "MDRW2100002096.1.14",
          "form": "쭈꾸미볶음 좋아해요?",
          "original_form": "쭈꾸미볶음 좋아해요?",
          "speaker_id": "2",
          "time": "20210819 23:42"
        },
