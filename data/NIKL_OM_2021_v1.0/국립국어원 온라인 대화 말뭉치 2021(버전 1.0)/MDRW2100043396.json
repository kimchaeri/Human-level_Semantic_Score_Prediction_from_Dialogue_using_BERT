      "utterance": [
        {
          "id": "MDRW2100043396.1.1",
          "form": "저는 동남아는 한번도 안가봤는데",
          "original_form": "저는 동남아는 한번도 안가봤는데",
          "speaker_id": "1",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100043396.1.2",
          "form": "야시장같은것도 즐길수있다면서요?",
          "original_form": "야시장같은것도 즐길수있다면서요?",
          "speaker_id": "1",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100043396.1.3",
          "form": "저두요! 너무 가보고싶어요ㅠㅠ",
          "original_form": "저두요! 너무 가보고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043396.1.4",
          "form": "코로나가 끝나서 여기저기 갔으면 좋겠네요",
          "original_form": "코로나가 끝나서 여기저기 갔으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043396.1.5",
          "form": "코로나 이후로 국내여행도 못가봤어요",
          "original_form": "코로나 이후로 국내여행도 못가봤어요",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043396.1.6",
          "form": "맞아요ㅜㅠ 불안해서 어딜 갈 수가 없어요",
          "original_form": "맞아요ㅜㅠ 불안해서 어딜 갈 수가 없어요",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043396.1.7",
          "form": "해외까진 아니어도 국내여행 가고싶어요",
          "original_form": "해외까진 아니어도 국내여행 가고싶어요",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043396.1.8",
          "form": "맞아요 근데 막상가려면 또 불안해서",
          "original_form": "맞아요 근데 막상가려면 또 불안해서",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043396.1.9",
          "form": "예약했디가 취소한적도 있네요",
          "original_form": "예약했디가 취소한적도 있네요",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043396.1.10",
          "form": "ㅋㅋ전 좀 나아졌을때 예약했다가 다시 터져서 취소했어요",
          "original_form": "ㅋㅋ전 좀 나아졌을때 예약했다가 다시 터져서 취소했어요",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043396.1.11",
          "form": "와우 너무 아깝네요",
          "original_form": "와우 너무 아깝네요",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043396.1.12",
          "form": "시기를 알수가 없으니",
          "original_form": "시기를 알수가 없으니",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043396.1.13",
          "form": "그니깐요.. 백신 많이들 접종하면 연말이나 내년 초는ㅋㅋ 여행가볼 수 있을까 기대중이에요",
          "original_form": "그니깐요.. 백신 많이들 접종하면 연말이나 내년 초는ㅋㅋ 여행가볼 수 있을까 기대중이에요",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043396.1.14",
          "form": "백신맞으면 여행전후 격리안해도되죠?",
          "original_form": "백신맞으면 여행전후 격리안해도되죠?",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043396.1.15",
          "form": "위험도 낮은 국가는 그렇다고 하던데요",
          "original_form": "위험도 낮은 국가는 그렇다고 하던데요",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043396.1.16",
          "form": "오오 그럼 내년부터는 여행활성화가 될수도 있겠군요",
          "original_form": "오오 그럼 내년부터는 여행활성화가 될수도 있겠군요",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043396.1.17",
          "form": "일단 올해는 국내여행이라도 가보는게 목표입니당",
          "original_form": "일단 올해는 국내여행이라도 가보는게 목표입니당",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043396.1.18",
          "form": "생각해둔데 있으세요?",
          "original_form": "생각해둔데 있으세요?",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043396.1.19",
          "form": "제주도 일단가고싶고",
          "original_form": "제주도 일단가고싶고",
          "speaker_id": "1",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043396.1.20",
          "form": "안되면 부산이라도 가고싶어요",
          "original_form": "안되면 부산이라도 가고싶어요",
          "speaker_id": "1",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043396.1.21",
          "form": "그게안되면 근교라도ㅋㅋ",
          "original_form": "그게안되면 근교라도ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:11"
        }
      ]
    }
