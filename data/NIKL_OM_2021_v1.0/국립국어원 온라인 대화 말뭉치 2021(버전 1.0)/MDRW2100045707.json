        },
        {
          "id": "MDRW2100045707.1.2",
          "form": "대학 가셨나요??",
          "original_form": "대학 가셨나요??",
          "speaker_id": "2",
          "time": "20210908 05:47"
        },
        {
          "id": "MDRW2100045707.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아잇 오타를 못 놀리다니 아깝다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아잇 오타를 못 놀리다니 아깝다",
          "speaker_id": "1",
          "time": "20210908 05:48"
        },
        {
          "id": "MDRW2100045707.1.4",
          "form": "네 대학 갔어요!",
          "original_form": "네 대학 갔어요!",
          "speaker_id": "1",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045707.1.5",
          "form": "헉 혹시 개강하셔서 일찍 주무시나요",
          "original_form": "헉 혹시 개강하셔서 일찍 주무시나요",
          "speaker_id": "1",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045707.1.6",
          "form": "먹는건가요ㅡ",
          "original_form": "먹는건가요ㅡ",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045707.1.7",
          "form": "학교ㅏ 뭐였죠?ㅋㅋㅋㅋㅋ",
          "original_form": "학교ㅏ 뭐였죠?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045707.1.8",
          "form": "ㅌㅌㅌㅋㅌㅌㅌㅋㅌㅌ먹는건가요ㅋㅋ",
          "original_form": "ㅌㅌㅌㅋㅌㅌㅌㅋㅌㅌ먹는건가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045707.1.9",
          "form": "학교는 학식얘기 하시려구요?",
          "original_form": "학교는 학식얘기 하시려구요?",
          "speaker_id": "1",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045707.1.10",
          "form": "아눀ㅋㅋ 다음엔 전공 맞추기는 어떨까 싶어서욬ㅋㅋㅋㅋ",
          "original_form": "아눀ㅋㅋ 다음엔 전공 맞추기는 어떨까 싶어서욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:54"
        },
        {
          "id": "MDRW2100045707.1.11",
          "form": "제 기억의 헉식운 정말 맛이 없었는데 주변에 뭐가 없어서 강제로 학식을 먹어야했죠ㅠㅠ",
          "original_form": "제 기억의 헉식운 정말 맛이 없었는데 주변에 뭐가 없어서 강제로 학식을 먹어야했죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:55"
        },
        {
          "id": "MDRW2100045707.1.12",
          "form": "ㅋㅋㅋㅋ저희도 학식 진짜 노맛이었어요",
          "original_form": "ㅋㅋㅋㅋ저희도 학식 진짜 노맛이었어요",
          "speaker_id": "1",
          "time": "20210908 05:56"
        },
        {
          "id": "MDRW2100045707.1.13",
          "form": "뭔가 저렴하긴 한데 저렴한 값을 했던 것 같아요",
          "original_form": "뭔가 저렴하긴 한데 저렴한 값을 했던 것 같아요",
          "speaker_id": "1",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045707.1.14",
          "form": "전공 맞추기 ㅋㅋㅋㅋㅋ짱 어렵겠네요",
          "original_form": "전공 맞추기 ㅋㅋㅋㅋㅋ짱 어렵겠네요",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045707.1.15",
          "form": "맞아요... 근데 맛보면 오히려 비싼거 같아요ㅠㅠ",
          "original_form": "맞아요... 근데 맛보면 오히려 비싼거 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045707.1.16",
          "form": "그쵸?ㅋㅋㅋㅋ",
          "original_form": "그쵸?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045707.1.17",
          "form": "아 맞아요 ㅋㅋㅋㅋ저렴한 가격마저",
          "original_form": "아 맞아요 ㅋㅋㅋㅋ저렴한 가격마저",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045707.1.18",
          "form": "비싸게 느껴지는 처절한 맛",
          "original_form": "비싸게 느껴지는 처절한 맛",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045707.1.19",
          "form": "그 이름하야 학식",
          "original_form": "그 이름하야 학식",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045707.1.20",
          "form": "진짜 너무먹기 싫었어요.....ㅠ",
          "original_form": "진짜 너무먹기 싫었어요.....ㅠ",
          "speaker_id": "2",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045707.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ거의 숨이 붙어있기 위해",
          "original_form": "ㅋㅋㅋㅋㅋㅋ거의 숨이 붙어있기 위해",
          "speaker_id": "1",
          "time": "20210908 06:01"
        },
        {
          "id": "MDRW2100045707.1.22",
          "form": "어쩔 수 없이 먹었던 것 같아요 ㅡㅋㅋㅋㅋ",
          "original_form": "어쩔 수 없이 먹었던 것 같아요 ㅡㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:01"
        },
        {
          "id": "MDRW2100045707.1.23",
          "form": "학교이 있으려면 어쩔 수 없죠...",
          "original_form": "학교이 있으려면 어쩔 수 없죠...",
          "speaker_id": "2",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100045707.1.24",
          "form": "도시락울 써가지고 다닐 수도 없구ㅠㅠ",
          "original_form": "도시락울 써가지고 다닐 수도 없구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100045707.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 그나마 학교 외부 주변에",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 그나마 학교 외부 주변에",
          "speaker_id": "1",
          "time": "20210908 06:04"
        },
        {
          "id": "MDRW2100045707.1.26",
          "form": "식당이 있어서 버텼던 것 같아요 ㅋㅋㅋ",
          "original_form": "식당이 있어서 버텼던 것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:04"
        },
        {
          "id": "MDRW2100045707.1.27",
          "form": "저는 그런게 없었어요ㅠㅠ",
          "original_form": "저는 그런게 없었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:04"
        },
        {
          "id": "MDRW2100045707.1.28",
          "form": "근데 식당도 저희도 많지 않았어요",
          "original_form": "근데 식당도 저희도 많지 않았어요",
          "speaker_id": "1",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100045707.1.29",
          "form": "생소할 수도 있는 요리 관련과..!",
          "original_form": "생소할 수도 있는 요리 관련과..!",
