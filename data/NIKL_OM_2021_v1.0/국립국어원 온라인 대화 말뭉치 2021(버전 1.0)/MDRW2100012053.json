      "utterance": [
        {
          "id": "MDRW2100012053.1.1",
          "form": "봉사 좋아허세요?",
          "original_form": "봉사 좋아허세요?",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012053.1.2",
          "form": "싫어하는 편도 아니지만 좋아하는 편도 아니에요",
          "original_form": "싫어하는 편도 아니지만 좋아하는 편도 아니에요",
          "speaker_id": "2",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012053.1.3",
          "form": "그냥 그저그런?",
          "original_form": "그냥 그저그런?",
          "speaker_id": "2",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012053.1.4",
          "form": "하라고 했을때 하게 되는...그렇게 되더라구요ㅋㅋㅋ",
          "original_form": "하라고 했을때 하게 되는...그렇게 되더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012053.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ저두 마찬가지예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저두 마찬가지예요",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100012053.1.6",
          "form": "어떤 봉사해보셨어요?",
          "original_form": "어떤 봉사해보셨어요?",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100012053.1.7",
          "form": "코로나 이전에는 간단하게 도서관 봉사나",
          "original_form": "코로나 이전에는 간단하게 도서관 봉사나",
          "speaker_id": "2",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100012053.1.8",
          "form": "유기견 봉사 이런거 했던 것같아요",
          "original_form": "유기견 봉사 이런거 했던 것같아요",
          "speaker_id": "2",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012053.1.9",
          "form": "샤인머스켓님은요?",
          "original_form": "샤인머스켓님은요?",
          "speaker_id": "2",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012053.1.10",
          "form": "와 강아지 좋아하시나봐요",
          "original_form": "와 강아지 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012053.1.11",
          "form": "저도 도서관봉사 조금 했어요",
          "original_form": "저도 도서관봉사 조금 했어요",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012053.1.12",
          "form": "책을 꽤 좋아하는 편이라서요",
          "original_form": "책을 꽤 좋아하는 편이라서요",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012053.1.13",
          "form": "도서관가면 마음이 편해지기도 하고...",
          "original_form": "도서관가면 마음이 편해지기도 하고...",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100012053.1.14",
          "form": "맞아요 책 냄새도 좋고 사람들 책 넘기는 소리 들으면 마음이 차분해지더라구요",
          "original_form": "맞아요 책 냄새도 좋고 사람들 책 넘기는 소리 들으면 마음이 차분해지더라구요",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100012053.1.15",
          "form": "맞아요!!! 책 냄새 짱 좋아요",
          "original_form": "맞아요!!! 책 냄새 짱 좋아요",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100012053.1.16",
          "form": "저는 그 종이냄새가 너무 좋더라구요",
          "original_form": "저는 그 종이냄새가 너무 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100012053.1.17",
          "form": "인정이에요",
          "original_form": "인정이에요",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100012053.1.18",
          "form": "그래서 한창 도서관 봉사할때 사서 한번 꿈꿔봤어요ㅋㅋㅋ",
          "original_form": "그래서 한창 도서관 봉사할때 사서 한번 꿈꿔봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100012053.1.19",
          "form": "앜ㅋㅋㅋ 저두 먼가 사서의 분위기가 좋더라구요",
          "original_form": "앜ㅋㅋㅋ 저두 먼가 사서의 분위기가 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100012053.1.20",
          "form": "조용한 도서관에서 사부작사부작거리기..",
          "original_form": "조용한 도서관에서 사부작사부작거리기..",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100012053.1.21",
          "form": "근데 큰 도서관이면 좀 싫을 것 같아요",
          "original_form": "근데 큰 도서관이면 좀 싫을 것 같아요",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100012053.1.22",
          "form": "엇 왜용??",
          "original_form": "엇 왜용??",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100012053.1.23",
          "form": "신경써야할게 엄청 많아질 것 같아서용..",
          "original_form": "신경써야할게 엄청 많아질 것 같아서용..",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100012053.1.24",
          "form": "아ㅋㅋ그렇겠네요ㅠ",
          "original_form": "아ㅋㅋ그렇겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100012053.1.25",
          "form": "그냥 읽으러 가는거면 큰 도서관은 좋은 것같아요",
          "original_form": "그냥 읽으러 가는거면 큰 도서관은 좋은 것같아요",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100012053.1.26",
          "form": "새로 지어진 도서관은 시설도 좋고 책도 많구요",
          "original_form": "새로 지어진 도서관은 시설도 좋고 책도 많구요",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100012053.1.27",
          "form": "그건 맞는 것 같아요!! 확실히 새도서관이 책이 많더라구요",
          "original_form": "그건 맞는 것 같아요!! 확실히 새도서관이 책이 많더라구요",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100012053.1.28",
          "form": "근데 약간 나무로 된 책장이 더 운치있는 것 같아요 ㅋㅋㅋ",
          "original_form": "근데 약간 나무로 된 책장이 더 운치있는 것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:38"
        }
      ]
