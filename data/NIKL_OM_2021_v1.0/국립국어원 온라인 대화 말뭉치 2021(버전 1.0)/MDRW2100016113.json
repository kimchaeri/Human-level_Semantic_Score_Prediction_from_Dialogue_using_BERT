      "utterance": [
        {
          "id": "MDRW2100016113.1.1",
          "form": "최근에 헤어스탈 바꾼적 있나여?",
          "original_form": "최근에 헤어스탈 바꾼적 있나여?",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100016113.1.2",
          "form": "저번에",
          "original_form": "저번에",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016113.1.3",
          "form": "파마함했는데",
          "original_form": "파마함했는데",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016113.1.4",
          "form": "저저번달인강",
          "original_form": "저저번달인강",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016113.1.5",
          "form": "이제다풀려가여...ㅎㅎㅎㅎ",
          "original_form": "이제다풀려가여...ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016113.1.6",
          "form": "관리를 너무안했나바염",
          "original_form": "관리를 너무안했나바염",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016113.1.7",
          "form": "우왕 무슨 펌 햇었어여?",
          "original_form": "우왕 무슨 펌 햇었어여?",
          "speaker_id": "1",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016113.1.8",
          "form": "베이베 히피펌이라공",
          "original_form": "베이베 히피펌이라공",
          "speaker_id": "2",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016113.1.9",
          "form": "했는뎅",
          "original_form": "했는뎅",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016113.1.10",
          "form": "이뻤어요 나름 ㅋㅋㅋ",
          "original_form": "이뻤어요 나름 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016113.1.11",
          "form": "지금도 많이풀리긴했는데",
          "original_form": "지금도 많이풀리긴했는데",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016113.1.12",
          "form": "좀 자연스런 웨이브~?",
          "original_form": "좀 자연스런 웨이브~?",
          "speaker_id": "2",
          "time": "20210826 13:22"
        },
        {
          "id": "MDRW2100016113.1.13",
          "form": "우왕ㅋㅋ 이름만 들어도 예쁘네영ㅋㅋ",
          "original_form": "우왕ㅋㅋ 이름만 들어도 예쁘네영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016113.1.14",
          "form": "지금 긴머리에영?",
          "original_form": "지금 긴머리에영?",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016113.1.15",
          "form": "넹 엄청 길렀슘돳 ㅋㅋ",
          "original_form": "넹 엄청 길렀슘돳 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016113.1.16",
          "form": "오 지금 어디까지 와요?",
          "original_form": "오 지금 어디까지 와요?",
          "speaker_id": "1",
          "time": "20210826 14:24"
        },
        {
          "id": "MDRW2100016113.1.17",
          "form": "허리~??",
          "original_form": "허리~??",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100016113.1.18",
          "form": "언청길었움 ㅋㅋ",
          "original_form": "언청길었움 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100016113.1.19",
          "form": "엥? 상상이 안간다",
          "original_form": "엥? 상상이 안간다",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100016113.1.20",
          "form": "거의 단발머리만 오래 봤어서ㅋㅋ",
          "original_form": "거의 단발머리만 오래 봤어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100016113.1.21",
          "form": "아그르네 ㅋㅋㅋㅋ",
          "original_form": "아그르네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100016113.1.22",
          "form": "나안그래도 머리잘 안자라는데 ㅋㅋ",
          "original_form": "나안그래도 머리잘 안자라는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100016113.1.23",
          "form": "그때도 잘았으니",
          "original_form": "그때도 잘았으니",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100016113.1.24",
          "form": "히피펌이면 그 웨이브가\n굵지 않고 엄청 빠글빠글하죵?",
          "original_form": "히피펌이면 그 웨이브가\n굵지 않고 엄청 빠글빠글하죵?",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100016113.1.25",
          "form": "나왜 알람이.안왔죠;;??",
          "original_form": "나왜 알람이.안왔죠;;??",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100016113.1.26",
          "form": "넹 빠글빠글한데",
          "original_form": "넹 빠글빠글한데",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100016113.1.27",
          "form": "베이비 히피펌은 뭔가 부드럽게??",
          "original_form": "베이비 히피펌은 뭔가 부드럽게??",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100016113.1.28",
          "form": "부담스럽지않아요 ㅎㅎ",
          "original_form": "부담스럽지않아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100016113.1.29",
          "form": "부담스럽지 않은 빠글이?",
          "original_form": "부담스럽지 않은 빠글이?",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100016113.1.30",
          "form": "머리는 그냥 말려주면 되낭?",
          "original_form": "머리는 그냥 말려주면 되낭?",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100016113.1.31",
          "form": "약간 꼬아서",
          "original_form": "약간 꼬아서",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100016113.1.32",
          "form": "말려두면 오래간다했는뎅",
          "original_form": "말려두면 오래간다했는뎅",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100016113.1.33",
          "form": "전 그냥말려버렸음ㅋㅋㅋㅋ...ㅎ",
          "original_form": "전 그냥말려버렸음ㅋㅋㅋㅋ...ㅎ",
          "speaker_id": "2",
          "time": "20210826 23:09"
        }
      ]
    }
