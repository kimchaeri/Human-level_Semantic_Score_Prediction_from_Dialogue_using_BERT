      "utterance": [
        {
          "id": "MDRW2100032125.1.1",
          "form": "백신 맞았나?",
          "original_form": "백신 맞았나?",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100032125.1.2",
          "form": "백신 저번주에 맞았지",
          "original_form": "백신 저번주에 맞았지",
          "speaker_id": "1",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100032125.1.3",
          "form": "니는?",
          "original_form": "니는?",
          "speaker_id": "1",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100032125.1.4",
          "form": "나도 한 오일정도 됐음",
          "original_form": "나도 한 오일정도 됐음",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100032125.1.5",
          "form": "이번 백신 10부제한거 잘한거 같지 않나",
          "original_form": "이번 백신 10부제한거 잘한거 같지 않나",
          "speaker_id": "1",
          "time": "20210905 12:50"
        },
        {
          "id": "MDRW2100032125.1.6",
          "form": "생각보다 안빡셌어",
          "original_form": "생각보다 안빡셌어",
          "speaker_id": "1",
          "time": "20210905 12:50"
        },
        {
          "id": "MDRW2100032125.1.7",
          "form": "그니까 왜 월이아니라 일인지 궁금했는데",
          "original_form": "그니까 왜 월이아니라 일인지 궁금했는데",
          "speaker_id": "2",
          "time": "20210905 12:51"
        },
        {
          "id": "MDRW2100032125.1.8",
          "form": "생가보다 월로 했을때 일정하지않더라...",
          "original_form": "생가보다 월로 했을때 일정하지않더라...",
          "speaker_id": "2",
          "time": "20210905 12:51"
        },
        {
          "id": "MDRW2100032125.1.9",
          "form": "진짜 월로하니까 생각보다 다르더라....",
          "original_form": "진짜 월로하니까 생각보다 다르더라....",
          "speaker_id": "1",
          "time": "20210905 12:52"
        },
        {
          "id": "MDRW2100032125.1.10",
          "form": "생각해보면 학교애서도 애들 생일 몰려있고 그러니까...",
          "original_form": "생각해보면 학교애서도 애들 생일 몰려있고 그러니까...",
          "speaker_id": "1",
          "time": "20210905 12:52"
        },
        {
          "id": "MDRW2100032125.1.11",
          "form": "그니까 아무래도 추우면 같이 붙어있게되어서 그런가ㅋㅋㅋㅋㅋ",
          "original_form": "그니까 아무래도 추우면 같이 붙어있게되어서 그런가ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:53"
        },
        {
          "id": "MDRW2100032125.1.12",
          "form": "이맘때쯤 생일많아ㅋㅋㅋ",
          "original_form": "이맘때쯤 생일많아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:54"
        },
        {
          "id": "MDRW2100032125.1.13",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:54"
        },
        {
          "id": "MDRW2100032125.1.14",
          "form": "진짜 ㅋㅋㅋㅋㅋㅋ 이맘때쯤. 많네 ㅋㅋㅋ",
          "original_form": "진짜 ㅋㅋㅋㅋㅋㅋ 이맘때쯤. 많네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:54"
        },
        {
          "id": "MDRW2100032125.1.15",
          "form": "우리애들 3명이나 저번달이랑 지금 생일이네",
          "original_form": "우리애들 3명이나 저번달이랑 지금 생일이네",
          "speaker_id": "1",
          "time": "20210905 12:54"
        },
        {
          "id": "MDRW2100032125.1.16",
          "form": "그니까ㅋㅋㅋㄱ생각보다 일별로하면 균등한가봐ㅋㅋㅋ",
          "original_form": "그니까ㅋㅋㅋㄱ생각보다 일별로하면 균등한가봐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:55"
        },
        {
          "id": "MDRW2100032125.1.17",
          "form": "니는 어디서 예약했음?",
          "original_form": "니는 어디서 예약했음?",
          "speaker_id": "2",
          "time": "20210905 12:55"
        },
        {
          "id": "MDRW2100032125.1.18",
          "form": "나는 그 백신 앱?국가껄로 했음",
          "original_form": "나는 그 백신 앱?국가껄로 했음",
          "speaker_id": "1",
          "time": "20210905 12:55"
        },
        {
          "id": "MDRW2100032125.1.19",
          "form": "아 나도 그걸로했는데",
          "original_form": "아 나도 그걸로했는데",
          "speaker_id": "2",
          "time": "20210905 12:56"
        },
        {
          "id": "MDRW2100032125.1.20",
          "form": "겁주던거랑 달리 엄청 금방되더라",
          "original_form": "겁주던거랑 달리 엄청 금방되더라",
          "speaker_id": "2",
          "time": "20210905 12:56"
        },
        {
          "id": "MDRW2100032125.1.21",
          "form": "나도 오분도 안걸림",
          "original_form": "나도 오분도 안걸림",
          "speaker_id": "1",
          "time": "20210905 12:56"
        },
        {
          "id": "MDRW2100032125.1.22",
          "form": "소아과로 호다닥 예약함ㅋㅋㅋㄱ",
          "original_form": "소아과로 호다닥 예약함ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 12:56"
        },
        {
          "id": "MDRW2100032125.1.23",
          "form": "사탕받고 싶었냐고 ㅋㅋㅋㅋㅋ",
          "original_form": "사탕받고 싶었냐고 ㅋㅋㅋㅋㅋ",
