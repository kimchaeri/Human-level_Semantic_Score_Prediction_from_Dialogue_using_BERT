      "utterance": [
        {
          "id": "MDRW2100034820.1.1",
          "form": "건조 간이요!",
          "original_form": "건조 간이요!",
          "speaker_id": "2",
          "time": "20210828 20:07"
        },
        {
          "id": "MDRW2100034820.1.2",
          "form": "오 애들 잘먹나요??",
          "original_form": "오 애들 잘먹나요??",
          "speaker_id": "1",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100034820.1.3",
          "form": "저희애는 간식을 입에도 안대요ㅜ",
          "original_form": "저희애는 간식을 입에도 안대요ㅜ",
          "speaker_id": "1",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100034820.1.4",
          "form": "네! 비쌀수록 잘먹는 기분이..",
          "original_form": "네! 비쌀수록 잘먹는 기분이..",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034820.1.5",
          "form": "추르도 잘 안먹나오?",
          "original_form": "추르도 잘 안먹나오?",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034820.1.6",
          "form": "네ㅜㅜ 극혐하더라구여",
          "original_form": "네ㅜㅜ 극혐하더라구여",
          "speaker_id": "1",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100034820.1.7",
          "form": "밥에 올려줬다가 그거먹고 웩 토해버렸어요",
          "original_form": "밥에 올려줬다가 그거먹고 웩 토해버렸어요",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100034820.1.8",
          "form": "얘 마음에안들면 토하거든여ㅜ",
          "original_form": "얘 마음에안들면 토하거든여ㅜ",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100034820.1.9",
          "form": "헉.. 추르가 만능이라했는데.. 아니였군요",
          "original_form": "헉.. 추르가 만능이라했는데.. 아니였군요",
          "speaker_id": "2",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100034820.1.10",
          "form": "속상하셨겠어요ㅠ",
          "original_form": "속상하셨겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100034820.1.11",
          "form": "네ㅜ 그런거 사놓고 안먹는거 다 길냥이들간식으로...",
          "original_form": "네ㅜ 그런거 사놓고 안먹는거 다 길냥이들간식으로...",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100034820.1.12",
          "form": "같은 스트릿출신이면서 왜케 가리는지",
          "original_form": "같은 스트릿출신이면서 왜케 가리는지",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100034820.1.13",
          "form": "스트릿출신이였군요",
          "original_form": "스트릿출신이였군요",
          "speaker_id": "2",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100034820.1.14",
          "form": "입양하신거에요?",
          "original_form": "입양하신거에요?",
          "speaker_id": "2",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100034820.1.15",
          "form": "첨에 동생이 길에 병걸려서 어미한테 버려진냥이 구조했다가 키우게됐어요",
          "original_form": "첨에 동생이 길에 병걸려서 어미한테 버려진냥이 구조했다가 키우게됐어요",
          "speaker_id": "1",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100034820.1.16",
          "form": "눈병에 피부병에 난리나고 밥도제대로못먹었는데 지금은 7킬로가넘어요...",
          "original_form": "눈병에 피부병에 난리나고 밥도제대로못먹었는데 지금은 7킬로가넘어요...",
          "speaker_id": "1",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100034820.1.17",
          "form": "확대시키셨군요",
          "original_form": "확대시키셨군요",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100034820.1.18",
          "form": "사랑이 느껴집니다",
          "original_form": "사랑이 느껴집니다",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100034820.1.19",
          "form": "ㅋㅋㅋ이게 사료만 먹어서 그런거같아요 간식안먹고 사료를 간식처럼또 먹으니까ㅜ",
          "original_form": "ㅋㅋㅋ이게 사료만 먹어서 그런거같아요 간식안먹고 사료를 간식처럼또 먹으니까ㅜ",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100034820.1.20",
          "form": "오냐오냐키웠더니ㅜㅜ",
          "original_form": "오냐오냐키웠더니ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100034820.1.21",
          "form": "남자애라서 덩치자체도 크긴하구여",
          "original_form": "남자애라서 덩치자체도 크긴하구여",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100034820.1.22",
          "form": "웃긴게 사료그렇게잘먹으면서 다이어트사료는 극혐하고안먹어요..",
          "original_form": "웃긴게 사료그렇게잘먹으면서 다이어트사료는 극혐하고안먹어요..",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100034820.1.23",
          "form": "우왕.. 미식가네요",
          "original_form": "우왕.. 미식가네요",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034820.1.24",
          "form": "미각이 뛰어나네요",
          "original_form": "미각이 뛰어나네요",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034820.1.25",
          "form": "아녀 저렴한것도 잘먹는데 호불호가 명확한거같아요",
          "original_form": "아녀 저렴한것도 잘먹는데 호불호가 명확한거같아요",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100034820.1.26",
          "form": "사료는 어떤거 먹이세요",
          "original_form": "사료는 어떤거 먹이세요",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100034820.1.27",
          "form": "지금은 halo 먹이고 있어요",
          "original_form": "지금은 halo 먹이고 있어요",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100034820.1.28",
          "form": "치킨맛이요!",
          "original_form": "치킨맛이요!",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100034820.1.29",
          "form": "헤일로 사료 괜찮나요? 모질이나 변이나",
          "original_form": "헤일로 사료 괜찮나요? 모질이나 변이나",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100034820.1.30",
          "form": "사료지금 바꿀까생각중이거든요ㅜ",
          "original_form": "사료지금 바꿀까생각중이거든요ㅜ",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100034820.1.31",
          "form": "헤일로 변 냄새 잡아주는건 잘 모르겠는데 모양은 꽤 괜찮아요!",
          "original_form": "헤일로 변 냄새 잡아주는건 잘 모르겠는데 모양은 꽤 괜찮아요!",
          "speaker_id": "2",
          "time": "20210828 23:54"
        }
      ]
    }
