      "utterance": [
        {
          "id": "MDRW2100044530.1.1",
          "form": "이삭토스트는 참말 맛잇져",
          "original_form": "이삭토스트는 참말 맛잇져",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044530.1.2",
          "form": "소스가 되게 달달하고 좋아요 ㅠㅠ",
          "original_form": "소스가 되게 달달하고 좋아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044530.1.3",
          "form": "맛아요 소스가맛잇져",
          "original_form": "맛아요 소스가맛잇져",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044530.1.4",
          "form": "근데 전 마요네즈만 뿌려도 맛잇더라고요",
          "original_form": "근데 전 마요네즈만 뿌려도 맛잇더라고요",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044530.1.5",
          "form": "맞아요!!!! 집에서 만들어도 맛있어요 ㅋㅋㅋ",
          "original_form": "맞아요!!!! 집에서 만들어도 맛있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044530.1.6",
          "form": "식빵 버터에 구우면 너뮤 맛나요 ㅋㅋ",
          "original_form": "식빵 버터에 구우면 너뮤 맛나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044530.1.7",
          "form": "버터는 어디에해도 맛나죠",
          "original_form": "버터는 어디에해도 맛나죠",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044530.1.8",
          "form": "버터 좋아하세요??",
          "original_form": "버터 좋아하세요??",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044530.1.9",
          "form": "네 넘 맛잇져",
          "original_form": "네 넘 맛잇져",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044530.1.10",
          "form": "외국에서는 덩어리로 넣드라구요 ㅋㅋㅋ",
          "original_form": "외국에서는 덩어리로 넣드라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044530.1.11",
          "form": "와 그럼진짜 맛잇겟네요ㅋㅋ",
          "original_form": "와 그럼진짜 맛잇겟네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044530.1.12",
          "form": "네네 ㅠㅠ 그 앙버터도 좋아하세요?",
          "original_form": "네네 ㅠㅠ 그 앙버터도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044530.1.13",
          "form": "팥은 별로지만 앙버터는 맛잇져",
          "original_form": "팥은 별로지만 앙버터는 맛잇져",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044530.1.14",
          "form": "느끼할줄 알았는데 너무 맛있드라구요!!",
          "original_form": "느끼할줄 알았는데 너무 맛있드라구요!!",
