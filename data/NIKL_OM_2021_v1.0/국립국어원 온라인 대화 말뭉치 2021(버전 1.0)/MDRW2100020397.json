      "utterance": [
        {
          "id": "MDRW2100020397.1.1",
          "form": "네 손녀한테 빙의되고..",
          "original_form": "네 손녀한테 빙의되고..",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020397.1.2",
          "form": "그 김보연",
          "original_form": "그 김보연",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020397.1.3",
          "form": "노주현 죽이고",
          "original_form": "노주현 죽이고",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020397.1.4",
          "form": "그냥 밥먹고 가라는 장면도",
          "original_form": "그냥 밥먹고 가라는 장면도",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020397.1.5",
          "form": "진정한 기 센 여자 ㅋㅋ",
          "original_form": "진정한 기 센 여자 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020397.1.6",
          "form": "아 ㅋㅋ 네 가정부가 귀신보는 것도요 ㅋㅋ",
          "original_form": "아 ㅋㅋ 네 가정부가 귀신보는 것도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020397.1.7",
          "form": "진짜 신선한 건 인정이에요",
          "original_form": "진짜 신선한 건 인정이에요",
          "speaker_id": "1",
          "time": "20210828 02:46"
        },
        {
          "id": "MDRW2100020397.1.8",
          "form": "네 예측이 안 돼요",
          "original_form": "네 예측이 안 돼요",
          "speaker_id": "2",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020397.1.9",
          "form": "네 ㅋㅋ 빨리 시즌 3 나왔으면 ㅠㅠ",
          "original_form": "네 ㅋㅋ 빨리 시즌 3 나왔으면 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020397.1.10",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020397.1.11",
          "form": "일일드라마였으면 좋았을 걸",
          "original_form": "일일드라마였으면 좋았을 걸",
          "speaker_id": "2",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020397.1.12",
          "form": "주변에 결사곡 보는 사람이 없어서 ㅠㅠ",
          "original_form": "주변에 결사곡 보는 사람이 없어서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020397.1.13",
          "form": "저도 그랬어요!",
          "original_form": "저도 그랬어요!",
          "speaker_id": "2",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020397.1.14",
          "form": "얘기가 통해서 반갑네요",
          "original_form": "얘기가 통해서 반갑네요",
          "speaker_id": "1",
          "time": "20210828 02:47"
        },
        {
          "id": "MDRW2100020397.1.15",
          "form": "저도요 ㅎㅎ",
          "original_form": "저도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 02:48"
        },
        {
          "id": "MDRW2100020397.1.16",
          "form": "전 부혜령이 쩰 싫었어요",
          "original_form": "전 부혜령이 쩰 싫었어요",
          "speaker_id": "1",
          "time": "20210828 02:48"
        },
        {
          "id": "MDRW2100020397.1.17",
          "form": "전 그 뮤지컬배우",
          "original_form": "전 그 뮤지컬배우",
          "speaker_id": "2",
          "time": "20210828 02:48"
        },
        {
          "id": "MDRW2100020397.1.18",
          "form": "ㅋㅋㅋ 그 여잔 탈락했죠..",
          "original_form": "ㅋㅋㅋ 그 여잔 탈락했죠..",
          "speaker_id": "1",
          "time": "20210828 02:48"
        },
        {
          "id": "MDRW2100020397.1.19",
          "form": "지금은 그냥 바벨 보고 있어요",
          "original_form": "지금은 그냥 바벨 보고 있어요",
          "speaker_id": "1",
          "time": "20210903 18:56"
        }
      ]
