      "utterance": [
        {
          "id": "MDRW2100009844.1.1",
          "form": "지금은 작은 아들 영어시간인가봐 ㅎㅎ",
          "original_form": "지금은 작은 아들 영어시간인가봐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100009844.1.2",
          "form": "어케 알았데?",
          "original_form": "어케 알았데?",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009844.1.3",
          "form": "어제 시간표 봤지 ㅎㅎ",
          "original_form": "어제 시간표 봤지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009844.1.4",
          "form": "훌 기억력도 좋아",
          "original_form": "훌 기억력도 좋아",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009844.1.5",
          "form": "아직 수업 잘 듣고 있어?",
          "original_form": "아직 수업 잘 듣고 있어?",
          "speaker_id": "1",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009844.1.6",
          "form": "ㅇㅇ 영어도 잘 따라하고",
          "original_form": "ㅇㅇ 영어도 잘 따라하고",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100009844.1.7",
          "form": "우리 아들이야 영어 최고지 뭐 ㅎㅎ",
          "original_form": "우리 아들이야 영어 최고지 뭐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100009844.1.8",
          "form": "발음 좋은 애들이 꽤 있네",
          "original_form": "발음 좋은 애들이 꽤 있네",
          "speaker_id": "2",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100009844.1.9",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100009844.1.10",
          "form": "ㅇㅇ 학원 다니는 애들인지 3~4명 정도는 발음이 꽤 좋아 ㅎㅎ",
          "original_form": "ㅇㅇ 학원 다니는 애들인지 3~4명 정도는 발음이 꽤 좋아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100009844.1.11",
          "form": "ㅎㅎ 그렇겠지~ 요즘엔 어릴 때부터 많이 보내니까",
          "original_form": "ㅎㅎ 그렇겠지~ 요즘엔 어릴 때부터 많이 보내니까",
          "speaker_id": "1",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100009844.1.12",
          "form": "작은 아들은 수업에 적극적이어서 예뻐",
          "original_form": "작은 아들은 수업에 적극적이어서 예뻐",
          "speaker_id": "2",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100009844.1.13",
          "form": "시킨 건 진짜 열심히 하지",
          "original_form": "시킨 건 진짜 열심히 하지",
          "speaker_id": "1",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100009844.1.14",
          "form": "집중력 떨어질 땐 이건 뭔가 싶다가도 집중할 때는 또 천재 같기도 하고 ㅋㅋㅋ",
          "original_form": "집중력 떨어질 땐 이건 뭔가 싶다가도 집중할 때는 또 천재 같기도 하고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100009844.1.15",
          "form": "걘 너무 극과 극을 달려",
          "original_form": "걘 너무 극과 극을 달려",
