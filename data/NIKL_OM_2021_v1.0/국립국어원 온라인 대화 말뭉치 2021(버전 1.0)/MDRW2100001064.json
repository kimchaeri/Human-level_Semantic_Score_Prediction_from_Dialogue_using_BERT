      "utterance": [
        {
          "id": "MDRW2100001064.1.1",
          "form": "혹시 좋아하는 영화나 드라마 있어요?",
          "original_form": "혹시 좋아하는 영화나 드라마 있어요?",
          "speaker_id": "1",
          "time": "20210722 22:38"
        },
        {
          "id": "MDRW2100001064.1.2",
          "form": "저는 sf좋아해요",
          "original_form": "저는 sf좋아해요",
          "speaker_id": "2",
          "time": "20210722 22:39"
        },
        {
          "id": "MDRW2100001064.1.3",
          "form": "영화는 인터스텔라",
          "original_form": "영화는 인터스텔라",
          "speaker_id": "2",
          "time": "20210722 22:39"
        },
        {
          "id": "MDRW2100001064.1.4",
          "form": "오오 그렇군요",
          "original_form": "오오 그렇군요",
          "speaker_id": "1",
          "time": "20210722 22:39"
        },
        {
          "id": "MDRW2100001064.1.5",
          "form": "드라마는 sf는 아니지만 나의 아저씨요",
          "original_form": "드라마는 sf는 아니지만 나의 아저씨요",
          "speaker_id": "2",
          "time": "20210722 22:39"
        },
        {
          "id": "MDRW2100001064.1.6",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210722 22:39"
        },
        {
          "id": "MDRW2100001064.1.7",
          "form": "저는 판타지랑 액션 좋아해요",
          "original_form": "저는 판타지랑 액션 좋아해요",
          "speaker_id": "1",
          "time": "20210722 22:39"
        },
        {
          "id": "MDRW2100001064.1.8",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210722 22:40"
        },
        {
          "id": "MDRW2100001064.1.9",
          "form": "어떤거 좋아하세요?",
          "original_form": "어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210722 22:40"
        },
        {
          "id": "MDRW2100001064.1.10",
          "form": "마블 영화 엄청 광팬입니다!ㅠㅠ",
          "original_form": "마블 영화 엄청 광팬입니다!ㅠㅠ",
          "speaker_id": "1",
          "time": "20210722 22:40"
        },
        {
          "id": "MDRW2100001064.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210722 22:40"
        },
        {
          "id": "MDRW2100001064.1.12",
          "form": "저도 마블 좋아해요",
          "original_form": "저도 마블 좋아해요",
          "speaker_id": "2",
          "time": "20210722 22:40"
        },
        {
          "id": "MDRW2100001064.1.13",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210722 22:40"
        },
        {
          "id": "MDRW2100001064.1.14",
          "form": "아직 블랙위도우는 못 봤지만",
          "original_form": "아직 블랙위도우는 못 봤지만",
          "speaker_id": "2",
          "time": "20210722 22:41"
        },
        {
          "id": "MDRW2100001064.1.15",
          "form": "나머지는 다 봤어요",
          "original_form": "나머지는 다 봤어요",
          "speaker_id": "2",
          "time": "20210722 22:41"
        },
        {
          "id": "MDRW2100001064.1.16",
          "form": "허어 블랙위도우 그거 꼭 보세요",
          "original_form": "허어 블랙위도우 그거 꼭 보세요",
          "speaker_id": "1",
          "time": "20210722 22:41"
        },
        {
          "id": "MDRW2100001064.1.17",
          "form": "진짜 너무 재밌어요ㅠ",
          "original_form": "진짜 너무 재밌어요ㅠ",
          "speaker_id": "1",
          "time": "20210722 22:41"
        },
        {
          "id": "MDRW2100001064.1.18",
          "form": "빨리 봐야겠네요ㅠㅠ",
          "original_form": "빨리 봐야겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 22:41"
        },
        {
          "id": "MDRW2100001064.1.19",
          "form": "상당히 스케일이 커요..",
          "original_form": "상당히 스케일이 커요..",
          "speaker_id": "1",
          "time": "20210722 22:42"
        },
        {
          "id": "MDRW2100001064.1.20",
          "form": "마블 영화 중에서 가장 좋아하는 영화는 어떤 거예요?",
          "original_form": "마블 영화 중에서 가장 좋아하는 영화는 어떤 거예요?",
          "speaker_id": "1",
          "time": "20210722 22:43"
        },
        {
          "id": "MDRW2100001064.1.21",
          "form": "그건 진짜 10번은 봤어요",
          "original_form": "그건 진짜 10번은 봤어요",
          "speaker_id": "2",
          "time": "20210722 22:44"
        },
        {
          "id": "MDRW2100001064.1.22",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210722 22:44"
        },
        {
          "id": "MDRW2100001064.1.23",
          "form": "아이언맨 1이요",
          "original_form": "아이언맨 1이요",
          "speaker_id": "2",
          "time": "20210722 22:45"
        },
        {
          "id": "MDRW2100001064.1.24",
          "form": "헐 저도 그거 진짜 많이 봤어요",
          "original_form": "헐 저도 그거 진짜 많이 봤어요",
          "speaker_id": "1",
          "time": "20210722 22:45"
        },
        {
          "id": "MDRW2100001064.1.25",
          "form": "진짜 아이언맨은 혁명..ㅠㅠㅠ",
          "original_form": "진짜 아이언맨은 혁명..ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210722 22:45"
        },
        {
          "id": "MDRW2100001064.1.26",
          "form": "맞어요ㅜㅠㅠ",
          "original_form": "맞어요ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 22:45"
        },
        {
          "id": "MDRW2100001064.1.27",
          "form": "제일 좋아하는 캐릭터도 아이언맨이에요",
          "original_form": "제일 좋아하는 캐릭터도 아이언맨이에요",
          "speaker_id": "1",
          "time": "20210722 22:46"
        },
        {
          "id": "MDRW2100001064.1.28",
          "form": "동굴에서 첨 슈트 만들때 넘 멋져요",
          "original_form": "동굴에서 첨 슈트 만들때 넘 멋져요",
          "speaker_id": "2",
          "time": "20210722 22:46"
        },
        {
          "id": "MDRW2100001064.1.29",
          "form": "하 그니까요",
          "original_form": "하 그니까요",
          "speaker_id": "1",
          "time": "20210722 22:46"
        },
        {
          "id": "MDRW2100001064.1.30",
          "form": "ㅎㅎㅎ전 이만 잡니다",
          "original_form": "ㅎㅎㅎ전 이만 잡니다",
          "speaker_id": "2",
          "time": "20210722 22:46"
        },
        {
          "id": "MDRW2100001064.1.31",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "2",
          "time": "20210722 22:46"
        },
        {
          "id": "MDRW2100001064.1.32",
          "form": "네! 잘자요 저도 재밌었습니다!",
          "original_form": "네! 잘자요 저도 재밌었습니다!",
          "speaker_id": "1",
