      "utterance": [
        {
          "id": "MDRW2100005339.1.1",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005339.1.2",
          "form": "오전에 갑자기 쏟아지더라",
          "original_form": "오전에 갑자기 쏟아지더라",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005339.1.3",
          "form": "비 너무 많이 왔어",
          "original_form": "비 너무 많이 왔어",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005339.1.4",
          "form": "알바 가는 시간이었나?",
          "original_form": "알바 가는 시간이었나?",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005339.1.5",
          "form": "엉..ㅠ",
          "original_form": "엉..ㅠ",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005339.1.6",
          "form": "나 갈 때 엄청 쏟아지고",
          "original_form": "나 갈 때 엄청 쏟아지고",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005339.1.7",
          "form": "알바 중에 ㄱ그치더라",
          "original_form": "알바 중에 ㄱ그치더라",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005339.1.8",
          "form": "아이고 난 오랜만에 좀 잤어",
          "original_form": "아이고 난 오랜만에 좀 잤어",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005339.1.9",
          "form": "잘했어",
          "original_form": "잘했어",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005339.1.10",
          "form": "비오니까 공사 안 해서 드디어 조용하더라고",
          "original_form": "비오니까 공사 안 해서 드디어 조용하더라고",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005339.1.11",
          "form": "그저께 수강신청 때 아침에 일어나서 그런가 나는 8시에 눈 떠져서 다시 잠이 안 옴...",
          "original_form": "그저께 수강신청 때 아침에 일어나서 그런가 나는 8시에 눈 떠져서 다시 잠이 안 옴...",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005339.1.12",
          "form": "4일만에 충분히 잔 듯",
          "original_form": "4일만에 충분히 잔 듯",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005339.1.13",
          "form": "3일동안 거의 3시간 씩만 잠",
          "original_form": "3일동안 거의 3시간 씩만 잠",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005339.1.14",
          "form": "먼 하수도인지 도로에 뭐 한다고 밤새 시끄러워서",
          "original_form": "먼 하수도인지 도로에 뭐 한다고 밤새 시끄러워서",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005339.1.15",
          "form": "헐 ㅠ 개피곤하깄다",
          "original_form": "헐 ㅠ 개피곤하깄다",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005339.1.16",
          "form": "그래도 오늘 자서 좀 괜찮아",
          "original_form": "그래도 오늘 자서 좀 괜찮아",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005339.1.17",
          "form": "좀 무서워서 오늘은 꼭 자려고 했어",
          "original_form": "좀 무서워서 오늘은 꼭 자려고 했어",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005339.1.18",
          "form": "응 ㅠ 다행이니",
          "original_form": "응 ㅠ 다행이니",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005339.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005339.1.20",
          "form": "그래도 낮에 일어나서 생활패턴은 못 고침",
          "original_form": "그래도 낮에 일어나서 생활패턴은 못 고침",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005339.1.21",
          "form": "ㅋㅋㅋㅋ곧 고칠 수 있을 거야",
          "original_form": "ㅋㅋㅋㅋ곧 고칠 수 있을 거야",
          "speaker_id": "1",
          "time": "20210821 22:10"
        }
      ]
