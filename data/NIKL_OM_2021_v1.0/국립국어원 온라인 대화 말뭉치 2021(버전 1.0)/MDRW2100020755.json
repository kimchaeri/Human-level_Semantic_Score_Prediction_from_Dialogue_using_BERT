      "utterance": [
        {
          "id": "MDRW2100020755.1.1",
          "form": "직장일 하면서 살림까지 힘든점이 많으시겠어요",
          "original_form": "직장일 하면서 살림까지 힘든점이 많으시겠어요",
          "speaker_id": "1",
          "time": "20210828 12:07"
        },
        {
          "id": "MDRW2100020755.1.2",
          "form": "아이들 어릴때 보단 많이 편해졌어요",
          "original_form": "아이들 어릴때 보단 많이 편해졌어요",
          "speaker_id": "2",
          "time": "20210828 12:10"
        },
        {
          "id": "MDRW2100020755.1.3",
          "form": "맞아요 어릴때 손이 많이 가지요",
          "original_form": "맞아요 어릴때 손이 많이 가지요",
          "speaker_id": "1",
          "time": "20210828 12:10"
        },
        {
          "id": "MDRW2100020755.1.4",
          "form": "이젠 배고프면 혼자서 챙겨 먹으니까 좋아요",
          "original_form": "이젠 배고프면 혼자서 챙겨 먹으니까 좋아요",
          "speaker_id": "2",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100020755.1.5",
          "form": "아이들이 기특하네요",
          "original_form": "아이들이 기특하네요",
          "speaker_id": "1",
          "time": "20210828 12:11"
        },
        {
          "id": "MDRW2100020755.1.6",
          "form": "가사와 직장 병행이 참 힘든일인데",
          "original_form": "가사와 직장 병행이 참 힘든일인데",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020755.1.7",
          "form": "아이들이 그렇게라도 해주면 엄마 입장에서 참 고맙죠",
          "original_form": "아이들이 그렇게라도 해주면 엄마 입장에서 참 고맙죠",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020755.1.8",
          "form": "자녀분들은 다 성인이시면 다 독립하셨나요?",
          "original_form": "자녀분들은 다 성인이시면 다 독립하셨나요?",
          "speaker_id": "2",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100020755.1.9",
          "form": "네 각자 자취하고 있어요",
          "original_form": "네 각자 자취하고 있어요",
          "speaker_id": "1",
          "time": "20210828 12:25"
        },
        {
          "id": "MDRW2100020755.1.10",
          "form": "엄마  고마운 걸 이제야   알더라구요",
          "original_form": "엄마  고마운 걸 이제야   알더라구요",
          "speaker_id": "1",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100020755.1.11",
          "form": "살림을 직접해봐야   느껴지나봐요",
          "original_form": "살림을 직접해봐야   느껴지나봐요",
          "speaker_id": "1",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100020755.1.12",
          "form": "아무래도 나가살면 생활용품 하나하나 다 사야하고 청소 빨래 등 혼자해야 하니까 그렇겠죠",
          "original_form": "아무래도 나가살면 생활용품 하나하나 다 사야하고 청소 빨래 등 혼자해야 하니까 그렇겠죠",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020755.1.13",
          "form": "울 아이들도 독립시키고 싶어요 ㅎㅎ",
          "original_form": "울 아이들도 독립시키고 싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020755.1.14",
          "form": "금방 그런 때가 올 거에요",
          "original_form": "금방 그런 때가 올 거에요",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020755.1.15",
          "form": "세상 편하고 홀가분하다가도",
          "original_form": "세상 편하고 홀가분하다가도",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020755.1.16",
          "form": "또 애들 생각에 쓸쓸해지면서",
          "original_form": "또 애들 생각에 쓸쓸해지면서",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020755.1.17",
          "form": "옆에 끼고 복작복작 살던 때가 그리워지기도하고 그러네요",
          "original_form": "옆에 끼고 복작복작 살던 때가 그리워지기도하고 그러네요",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020755.1.18",
          "form": "그러실거 같아요.. 허전하고 걱정되고..",
          "original_form": "그러실거 같아요.. 허전하고 걱정되고..",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020755.1.19",
          "form": "그게 엄마 마음인데 자식들이 알아주나요ㅡㅡ",
          "original_form": "그게 엄마 마음인데 자식들이 알아주나요ㅡㅡ",
          "speaker_id": "2",
          "time": "20210828 12:38"
        },
        {
          "id": "MDRW2100020755.1.20",
          "form": "그렇죠 우리들끼리만 아는거지",
          "original_form": "그렇죠 우리들끼리만 아는거지",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020755.1.21",
          "form": "자기들도  부모되어봐야",
          "original_form": "자기들도  부모되어봐야",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020755.1.22",
          "form": "조금 느끼고",
          "original_form": "조금 느끼고",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020755.1.23",
          "form": "나도 그랬었으니까요",
          "original_form": "나도 그랬었으니까요",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100020755.1.24",
          "form": "철이 들면 알아주겠죠^^\n지금은 다 잔소리라고 하는 것들이요",
          "original_form": "철이 들면 알아주겠죠^^\n지금은 다 잔소리라고 하는 것들이요",
          "speaker_id": "2",
          "time": "20210828 12:40"
        },
        {
          "id": "MDRW2100020755.1.25",
          "form": "지금은 남편분이랑  두 분 만 생활하시겠어요",
          "original_form": "지금은 남편분이랑  두 분 만 생활하시겠어요",
          "speaker_id": "2",
          "time": "20210828 12:41"
        },
        {
          "id": "MDRW2100020755.1.26",
          "form": "네 둘이서 있어요",
          "original_form": "네 둘이서 있어요",
          "speaker_id": "1",
          "time": "20210828 12:45"
        },
        {
          "id": "MDRW2100020755.1.27",
          "form": "좋기도 싫기도 하네요",
          "original_form": "좋기도 싫기도 하네요",
          "speaker_id": "1",
          "time": "20210828 12:46"
        },
        {
          "id": "MDRW2100020755.1.28",
          "form": "애들은 뭘주든 잘 먹었는데",
          "original_form": "애들은 뭘주든 잘 먹었는데",
          "speaker_id": "1",
          "time": "20210828 12:46"
        },
        {
          "id": "MDRW2100020755.1.29",
          "form": "입이 까탈스러워서",
          "original_form": "입이 까탈스러워서",
          "speaker_id": "1",
          "time": "20210828 12:46"
        }
      ]
