        {
          "id": "MDRW2100045785.1.2",
          "form": "하트시그널 봤어?",
          "original_form": "하트시그널 봤어?",
          "speaker_id": "1",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045785.1.3",
          "form": "뭔지는 알아",
          "original_form": "뭔지는 알아",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045785.1.4",
          "form": "나는 시즌 2,3은 봤어",
          "original_form": "나는 시즌 2,3은 봤어",
          "speaker_id": "1",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045785.1.5",
          "form": "시즌 1은 안봤구나",
          "original_form": "시즌 1은 안봤구나",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045785.1.6",
          "form": "응응 그건 흥미가 안가서 ㅋㅋ",
          "original_form": "응응 그건 흥미가 안가서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100045785.1.7",
          "form": "2랑 3중에 뭐가 더 재밌어",
          "original_form": "2랑 3중에 뭐가 더 재밌어",
          "speaker_id": "2",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100045785.1.8",
          "form": "시즌 2가 레전드야",
          "original_form": "시즌 2가 레전드야",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045785.1.9",
          "form": "ㆍ2가 핫하긴 했지",
          "original_form": "ㆍ2가 핫하긴 했지",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045785.1.10",
          "form": "출연자들 다 매력있어서",
          "original_form": "출연자들 다 매력있어서",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045785.1.11",
          "form": "맞아 나도 그거는 알아 ㅋㅋㅋ",
          "original_form": "맞아 나도 그거는 알아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045785.1.12",
          "form": "시즌 3도 재밌긴한데 다시볼정도는 아니라서",
          "original_form": "시즌 3도 재밌긴한데 다시볼정도는 아니라서",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045785.1.13",
          "form": "시즌2는 다시봐?",
