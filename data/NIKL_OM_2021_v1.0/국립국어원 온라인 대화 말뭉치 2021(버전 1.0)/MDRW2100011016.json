      "utterance": [
        {
          "id": "MDRW2100011016.1.1",
          "form": "너 며칠전에 보니까 뭘 한가득 사서 가던데",
          "original_form": "너 며칠전에 보니까 뭘 한가득 사서 가던데",
          "speaker_id": "1",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011016.1.2",
          "form": "뭘 산거야?",
          "original_form": "뭘 산거야?",
          "speaker_id": "1",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011016.1.3",
          "form": "아 그때 나 다이소에서 플랙스 했잖아",
          "original_form": "아 그때 나 다이소에서 플랙스 했잖아",
          "speaker_id": "2",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011016.1.4",
          "form": "청소용품들을 좀 샀지",
          "original_form": "청소용품들을 좀 샀지",
          "speaker_id": "2",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011016.1.5",
          "form": "플렉스.. 멋있구나",
          "original_form": "플렉스.. 멋있구나",
          "speaker_id": "1",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011016.1.6",
          "form": "너 청소를 좋아하는거야?",
          "original_form": "너 청소를 좋아하는거야?",
          "speaker_id": "1",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011016.1.7",
          "form": "청소용품을 한가득 샀다니 대단한걸?",
          "original_form": "청소용품을 한가득 샀다니 대단한걸?",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011016.1.8",
          "form": "아니..어쩔수없이 샀어",
          "original_form": "아니..어쩔수없이 샀어",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011016.1.9",
          "form": "가득사도 뭔가 즐겁지는 않더라",
          "original_form": "가득사도 뭔가 즐겁지는 않더라",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011016.1.10",
          "form": "아아..",
          "original_form": "아아..",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011016.1.11",
          "form": "청소용품이 하나도 없던거야?",
          "original_form": "청소용품이 하나도 없던거야?",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011016.1.12",
          "form": "너 그럼 그동안 청소를 안했니??!?!!?",
          "original_form": "너 그럼 그동안 청소를 안했니??!?!!?",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011016.1.13",
          "form": "응 아무래도 원래 청소를 안하니까!",
          "original_form": "응 아무래도 원래 청소를 안하니까!",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011016.1.14",
          "form": "그래도 그냥 살만해",
          "original_form": "그래도 그냥 살만해",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011016.1.15",
          "form": "정말 다양한 청소도구들이 너를 살렸구나",
          "original_form": "정말 다양한 청소도구들이 너를 살렸구나",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011016.1.16",
          "form": "다행이다.",
          "original_form": "다행이다.",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100011016.1.17",
          "form": "변기솔은 산거야?",
          "original_form": "변기솔은 산거야?",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100011016.1.18",
          "form": "음 아니 그걸 구매해야겟다",
          "original_form": "음 아니 그걸 구매해야겟다",
          "speaker_id": "2",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100011016.1.19",
          "form": "나는 최소한의 청소를 추구해.",
          "original_form": "나는 최소한의 청소를 추구해.",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100011016.1.20",
          "form": "그래. 필수란다.",
          "original_form": "그래. 필수란다.",
          "speaker_id": "1",
          "time": "20210823 23:54"
        }
      ]
