      "utterance": [
        {
          "id": "MDRW2100046211.1.1",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046211.1.2",
          "form": "며칠뒤에 알림와서보니",
          "original_form": "며칠뒤에 알림와서보니",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046211.1.3",
          "form": "사진은 같고",
          "original_form": "사진은 같고",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046211.1.4",
          "form": "아뒤는 다르고 ㅋ",
          "original_form": "아뒤는 다르고 ㅋ",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046211.1.5",
          "form": "또 사기인가요",
          "original_form": "또 사기인가요",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046211.1.6",
          "form": "댓글로 사기꾼이라고",
          "original_form": "댓글로 사기꾼이라고",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046211.1.7",
          "form": "ㅋㅋㅋㅋ 징하네요",
          "original_form": "ㅋㅋㅋㅋ 징하네요",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046211.1.8",
          "form": "적고싶더라구요",
          "original_form": "적고싶더라구요",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046211.1.9",
          "form": "신랑이 전화번호 집주소 다 넘겨놨기땜에 참으라고 ㅋ",
          "original_form": "신랑이 전화번호 집주소 다 넘겨놨기땜에 참으라고 ㅋ",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046211.1.10",
          "form": "아 이미 집주소는 보내놓으셨구나 ㅠㅠ",
          "original_form": "아 이미 집주소는 보내놓으셨구나 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046211.1.11",
          "form": "아이쿠 진짜 돈 안보내셔서 다행이에요",
          "original_form": "아이쿠 진짜 돈 안보내셔서 다행이에요",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046211.1.12",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046211.1.13",
          "form": "간담이 서늘했어요",
          "original_form": "간담이 서늘했어요",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046211.1.14",
          "form": "버즈 같은거 쓰세요?",
          "original_form": "버즈 같은거 쓰세요?",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046211.1.15",
          "form": "저 밖에서 음악 잘 안들어요",
          "original_form": "저 밖에서 음악 잘 안들어요",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046211.1.16",
          "form": "어지러워서 ㅎㅎ",
          "original_form": "어지러워서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046211.1.17",
          "form": "ㅎㅎ 저도 안쓰다가",
          "original_form": "ㅎㅎ 저도 안쓰다가",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046211.1.18",
          "form": "새폰에는 이어폰잭이 없더라구요",
          "original_form": "새폰에는 이어폰잭이 없더라구요",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046211.1.19",
          "form": "맞아요 요즘껀 없더라고요",
          "original_form": "맞아요 요즘껀 없더라고요",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100046211.1.20",
          "form": "그래서 필요해여 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 필요해여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046211.1.21",
          "form": "삼실에서",
          "original_form": "삼실에서",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046211.1.22",
          "form": "한번씩 유투브보려면",
          "original_form": "한번씩 유투브보려면",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046211.1.23",
          "form": "필요해서 샀어요",
          "original_form": "필요해서 샀어요",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046211.1.24",
          "form": "ㅋㅋㅋ 아 필요하네여",
          "original_form": "ㅋㅋㅋ 아 필요하네여",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046211.1.25",
          "form": "사무실용이라면!!",
          "original_form": "사무실용이라면!!",
          "speaker_id": "2",
          "time": "20210908 11:25"
        }
      ]
