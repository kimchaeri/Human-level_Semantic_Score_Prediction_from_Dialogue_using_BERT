      "utterance": [
        {
          "id": "MDRW2100039859.1.1",
          "form": "저 가을옷 지를까 말까 고민즁이에요",
          "original_form": "저 가을옷 지를까 말까 고민즁이에요",
          "speaker_id": "2",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039859.1.2",
          "form": "지르세요",
          "original_form": "지르세요",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039859.1.3",
          "form": "저도 어제 질렀어요",
          "original_form": "저도 어제 질렀어요",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039859.1.4",
          "form": "우와 뭐사셨나요",
          "original_form": "우와 뭐사셨나요",
          "speaker_id": "2",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039859.1.5",
          "form": "우선 니트 2벌이랑 가방 구매했어요 ㅎㅎ",
          "original_form": "우선 니트 2벌이랑 가방 구매했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039859.1.6",
          "form": "어떤거 고민이세요?",
          "original_form": "어떤거 고민이세요?",
          "speaker_id": "1",
          "time": "20210903 22:13"
        },
        {
          "id": "MDRW2100039859.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039859.1.8",
          "form": "저는 신발 흰바지 검은니트요ㅠㅠ",
          "original_form": "저는 신발 흰바지 검은니트요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100039859.1.9",
          "form": "헐 완전 기본 템 아닌가요?ㅠㅠ",
          "original_form": "헐 완전 기본 템 아닌가요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039859.1.10",
          "form": "지르세요 행복해요..ㅋㅋ큐",
          "original_form": "지르세요 행복해요..ㅋㅋ큐",
          "speaker_id": "1",
          "time": "20210903 22:17"
        },
        {
          "id": "MDRW2100039859.1.11",
          "form": "아 정말요 ㅠㅠ",
          "original_form": "아 정말요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039859.1.12",
          "form": "닮은 옷이 많아 고민이에요",
          "original_form": "닮은 옷이 많아 고민이에요",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039859.1.13",
          "form": "스타일 바꾸고 싶네용 ㅋㅋ",
          "original_form": "스타일 바꾸고 싶네용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039859.1.14",
          "form": "아 인정이요",
          "original_form": "아 인정이요",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039859.1.15",
          "form": "고민이죠 항상ㅋㅋ",
          "original_form": "고민이죠 항상ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039859.1.16",
          "form": "옷 사면 머리도 바꾸고 싶어요 ㅋㅋㅋ",
          "original_form": "옷 사면 머리도 바꾸고 싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039859.1.17",
          "form": "그쵸 사고나면 또 내옷중 하나 ㅋㅋ",
          "original_form": "그쵸 사고나면 또 내옷중 하나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039859.1.18",
          "form": "전 머리를 확 바꿨어요",
          "original_form": "전 머리를 확 바꿨어요",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039859.1.19",
          "form": "왕 짧은 단발",
          "original_form": "왕 짧은 단발",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039859.1.20",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039859.1.21",
          "form": "다들 말리지 않았어요?",
          "original_form": "다들 말리지 않았어요?",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039859.1.22",
          "form": "시크를 추구했는데",
          "original_form": "시크를 추구했는데",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039859.1.23",
          "form": "순박해져서 울고싶어요",
          "original_form": "순박해져서 울고싶어요",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039859.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ엌ㅋㅋㅋ큐ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ엌ㅋㅋㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039859.1.25",
          "form": "스모키한 화장은 어떠세요,,?ㅠㅠ",
          "original_form": "스모키한 화장은 어떠세요,,?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039859.1.26",
          "form": "앜ㅋㅋㅋㅋ 도전해볼게요",
          "original_form": "앜ㅋㅋㅋㅋ 도전해볼게요",
          "speaker_id": "2",
