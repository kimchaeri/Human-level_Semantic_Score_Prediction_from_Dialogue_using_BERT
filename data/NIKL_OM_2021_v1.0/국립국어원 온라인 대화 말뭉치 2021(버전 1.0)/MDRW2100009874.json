      "utterance": [
        {
          "id": "MDRW2100009874.1.1",
          "form": "한때 야구를 좋아했는데",
          "original_form": "한때 야구를 좋아했는데",
          "speaker_id": "1",
          "time": "20210823 11:09"
        },
        {
          "id": "MDRW2100009874.1.2",
          "form": "지금은 대구에 살지 않아서 열정이 좀 줄어들었네요 ㅎㅎ",
          "original_form": "지금은 대구에 살지 않아서 열정이 좀 줄어들었네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:09"
        },
        {
          "id": "MDRW2100009874.1.3",
          "form": "전 가끔 구겅하는편이에요ㅎ",
          "original_form": "전 가끔 구겅하는편이에요ㅎ",
          "speaker_id": "2",
          "time": "20210823 11:17"
        },
        {
          "id": "MDRW2100009874.1.4",
          "form": "야구장에서 치맥은 좋던데요",
          "original_form": "야구장에서 치맥은 좋던데요",
          "speaker_id": "2",
          "time": "20210823 11:17"
        },
        {
          "id": "MDRW2100009874.1.5",
          "form": "전 한창 팬일땐 야구장 안가도 티비로 매일 경기 보고했던 기억이 나요 ㅎㅎ",
          "original_form": "전 한창 팬일땐 야구장 안가도 티비로 매일 경기 보고했던 기억이 나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:20"
        },
        {
          "id": "MDRW2100009874.1.6",
          "form": "아 잘챙겨보셧나봐요ㅎ",
          "original_form": "아 잘챙겨보셧나봐요ㅎ",
          "speaker_id": "2",
          "time": "20210823 11:54"
        },
        {
          "id": "MDRW2100009874.1.7",
          "form": "전 그냥 티비나오면 슬쩍ㅎ",
          "original_form": "전 그냥 티비나오면 슬쩍ㅎ",
          "speaker_id": "2",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009874.1.8",
          "form": "전 삼성 라이온즈 팬이었어요",
          "original_form": "전 삼성 라이온즈 팬이었어요",
          "speaker_id": "1",
          "time": "20210823 11:58"
        },
        {
          "id": "MDRW2100009874.1.9",
          "form": "양준혁선수 아시나요",
          "original_form": "양준혁선수 아시나요",
          "speaker_id": "1",
          "time": "20210823 11:58"
        },
        {
          "id": "MDRW2100009874.1.10",
          "form": "넵ㅎㅎ티비로봐서요",
          "original_form": "넵ㅎㅎ티비로봐서요",
          "speaker_id": "2",
          "time": "20210823 12:07"
        },
        {
          "id": "MDRW2100009874.1.11",
          "form": "한때 되게 응원했었던기억나요 은퇴전에 안타 2000개달성 기원하고 이러면서 ㅋ",
          "original_form": "한때 되게 응원했었던기억나요 은퇴전에 안타 2000개달성 기원하고 이러면서 ㅋ",
          "speaker_id": "1",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009874.1.12",
          "form": "오ㅋㅋ전 예능에서나 봣엇거든요",
          "original_form": "오ㅋㅋ전 예능에서나 봣엇거든요",
          "speaker_id": "2",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009874.1.13",
          "form": "엄청 유명했다고는 들엇어요ㅎ",
          "original_form": "엄청 유명했다고는 들엇어요ㅎ",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100009874.1.14",
          "form": "근데 한때 야구선수들 인성문제가 팡팡터지면서",
          "original_form": "근데 한때 야구선수들 인성문제가 팡팡터지면서",
          "speaker_id": "1",
          "time": "20210823 14:02"
        },
        {
          "id": "MDRW2100009874.1.15",
          "form": "뭔가 실망했달까 ㅋㅋ",
          "original_form": "뭔가 실망했달까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:02"
        },
        {
          "id": "MDRW2100009874.1.16",
          "form": "ㅋㅋ스포츠인들이 좀 그런가봐요",
          "original_form": "ㅋㅋ스포츠인들이 좀 그런가봐요",
          "speaker_id": "2",
          "time": "20210823 14:02"
        },
        {
          "id": "MDRW2100009874.1.17",
          "form": "뭔가 그들에게 제 돈과 열정을 쓰는게 아까워졌지뭐에요",
          "original_form": "뭔가 그들에게 제 돈과 열정을 쓰는게 아까워졌지뭐에요",
          "speaker_id": "1",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100009874.1.18",
          "form": "이 타락한 인간들! ㅋㅋㅋ",
          "original_form": "이 타락한 인간들! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100009874.1.19",
          "form": "ㅋㅋ돈과열정까지나!!",
          "original_form": "ㅋㅋ돈과열정까지나!!",
          "speaker_id": "2",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100009874.1.20",
          "form": "전 경기보다 응원이 재밋던ㅋㅋ",
          "original_form": "전 경기보다 응원이 재밋던ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100009874.1.21",
          "form": "전 그냥 야구 자체가 좋았던거 같아요",
          "original_form": "전 그냥 야구 자체가 좋았던거 같아요",
          "speaker_id": "1",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100009874.1.22",
          "form": "끝날때까지 끝난게아니고",
          "original_form": "끝날때까지 끝난게아니고",
          "speaker_id": "1",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100009874.1.23",
          "form": "9회말 2아웃에도 모른다는 ㅋ",
          "original_form": "9회말 2아웃에도 모른다는 ㅋ",
          "speaker_id": "1",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100009874.1.24",
          "form": "ㅎ맞아요",
          "original_form": "ㅎ맞아요",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
