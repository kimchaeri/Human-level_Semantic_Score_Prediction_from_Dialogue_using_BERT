      "utterance": [
        {
          "id": "MDRW2100018387.1.1",
          "form": "여행도 다녀왔으니 이젠 일상에 다시 집중~",
          "original_form": "여행도 다녀왔으니 이젠 일상에 다시 집중~",
          "speaker_id": "1",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018387.1.2",
          "form": "그래요ㅋㅋ 다시 일상으로 돌아와야죠",
          "original_form": "그래요ㅋㅋ 다시 일상으로 돌아와야죠",
          "speaker_id": "2",
          "time": "20210827 02:26"
        },
        {
          "id": "MDRW2100018387.1.3",
          "form": "아까 따님 있으시다고 하셨던거 같은데",
          "original_form": "아까 따님 있으시다고 하셨던거 같은데",
          "speaker_id": "1",
          "time": "20210827 02:27"
        },
        {
          "id": "MDRW2100018387.1.4",
          "form": "맞나요? 따님은 몇살이에영?",
          "original_form": "맞나요? 따님은 몇살이에영?",
          "speaker_id": "1",
          "time": "20210827 02:27"
        },
        {
          "id": "MDRW2100018387.1.5",
          "form": "야채도 먹어주셔야지요 ㅋㅋ 몸에 좋습니다",
          "original_form": "야채도 먹어주셔야지요 ㅋㅋ 몸에 좋습니다",
          "speaker_id": "1",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018387.1.6",
          "form": "넵 알겠습니다 ㅎㅎ 야채는 저보다 제 오빠가 더 좋아했었어요",
          "original_form": "넵 알겠습니다 ㅎㅎ 야채는 저보다 제 오빠가 더 좋아했었어요",
          "speaker_id": "2",
          "time": "20210827 02:42"
        },
        {
          "id": "MDRW2100018387.1.7",
          "form": "아 오빠 있으셨구나",
          "original_form": "아 오빠 있으셨구나",
          "speaker_id": "1",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018387.1.8",
          "form": "오빠분은 몇살이세요?",
          "original_form": "오빠분은 몇살이세요?",
          "speaker_id": "1",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018387.1.9",
          "form": "저랑 1살 차이나요",
          "original_form": "저랑 1살 차이나요",
          "speaker_id": "2",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018387.1.10",
          "form": "지금은 군대에 있습니다 곧 전역해요",
          "original_form": "지금은 군대에 있습니다 곧 전역해요",
          "speaker_id": "2",
          "time": "20210827 02:43"
        },
        {
          "id": "MDRW2100018387.1.11",
          "form": "아하 군대에 있다니 힘들겠어요...",
          "original_form": "아하 군대에 있다니 힘들겠어요...",
          "speaker_id": "1",
          "time": "20210827 02:44"
        },
        {
          "id": "MDRW2100018387.1.12",
          "form": "걱정도 되시겠네요 군대 힘들잖아요ㅠㅠ",
          "original_form": "걱정도 되시겠네요 군대 힘들잖아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:44"
        },
        {
          "id": "MDRW2100018387.1.13",
          "form": "맞아요 군대가서 다치지만 않았으면 좋겠어요",
          "original_form": "맞아요 군대가서 다치지만 않았으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210827 02:44"
        },
        {
          "id": "MDRW2100018387.1.14",
          "form": "그래도 잘 살고 있더라구요ㅋㅋㅋ 요즘은 휴대폰도 된다 하더라구용",
          "original_form": "그래도 잘 살고 있더라구요ㅋㅋㅋ 요즘은 휴대폰도 된다 하더라구용",
          "speaker_id": "2",
          "time": "20210827 02:45"
        },
        {
          "id": "MDRW2100018387.1.15",
          "form": "ㅋㅋㅋㅋㅋ 다행이네요 어디 사단이에요?",
          "original_form": "ㅋㅋㅋㅋㅋ 다행이네요 어디 사단이에요?",
          "speaker_id": "1",
          "time": "20210827 02:45"
        },
        {
          "id": "MDRW2100018387.1.16",
          "form": "아 저희 오빠는 군목이라서요!",
          "original_form": "아 저희 오빠는 군목이라서요!",
          "speaker_id": "2",
          "time": "20210827 02:45"
        },
        {
          "id": "MDRW2100018387.1.17",
          "form": "군목이면 신학생?",
          "original_form": "군목이면 신학생?",
          "speaker_id": "1",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100018387.1.18",
          "form": "네네 맞아요ㅎㅎ",
          "original_form": "네네 맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100018387.1.19",
          "form": "군대에도 교회가 있어서 거기 있어요",
          "original_form": "군대에도 교회가 있어서 거기 있어요",
          "speaker_id": "2",
          "time": "20210827 02:46"
        },
        {
          "id": "MDRW2100018387.1.20",
          "form": "훈련소까지는 똑같구요",
          "original_form": "훈련소까지는 똑같구요",
          "speaker_id": "2",
          "time": "20210827 02:47"
        },
        {
          "id": "MDRW2100018387.1.21",
          "form": "아하 그렇군요~~",
          "original_form": "아하 그렇군요~~",
          "speaker_id": "1",
          "time": "20210827 02:47"
        },
        {
          "id": "MDRW2100018387.1.22",
          "form": "언제 전역해요?",
          "original_form": "언제 전역해요?",
          "speaker_id": "1",
          "time": "20210827 02:47"
        },
        {
          "id": "MDRW2100018387.1.23",
          "form": "얼른 오빠 보고싶으시겠어요",
          "original_form": "얼른 오빠 보고싶으시겠어요",
          "speaker_id": "1",
          "time": "20210827 02:47"
        },
        {
          "id": "MDRW2100018387.1.24",
          "form": "전역은 내년 2월이에요",
          "original_form": "전역은 내년 2월이에요",
          "speaker_id": "2",
          "time": "20210827 02:48"
        },
        {
          "id": "MDRW2100018387.1.25",
          "form": "딱히 보고 싶지는 않아요...ㅋㅋㅋㅋㅋ",
          "original_form": "딱히 보고 싶지는 않아요...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:48"
        },
        {
          "id": "MDRW2100018387.1.26",
          "form": "장난이고 얼른 조심히 전역하고 돌아왔으면 싶네요",
          "original_form": "장난이고 얼른 조심히 전역하고 돌아왔으면 싶네요",
          "speaker_id": "2",
          "time": "20210827 02:48"
        }
      ]
    }
  ]
