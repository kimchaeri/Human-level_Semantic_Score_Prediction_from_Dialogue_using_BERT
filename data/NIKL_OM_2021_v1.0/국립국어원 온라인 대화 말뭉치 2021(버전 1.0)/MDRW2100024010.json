      "utterance": [
        {
          "id": "MDRW2100024010.1.1",
          "form": "맞아요 하",
          "original_form": "맞아요 하",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024010.1.2",
          "form": "한때 로제 떡볶이도 진짜  많이 먹었는데",
          "original_form": "한때 로제 떡볶이도 진짜  많이 먹었는데",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024010.1.3",
          "form": "아직 저녁 안먹었는데 음식얘기하니까 넘 배고프네요",
          "original_form": "아직 저녁 안먹었는데 음식얘기하니까 넘 배고프네요",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024010.1.4",
          "form": "로제떡볶이 어디 프랜차이즈 젤 좋아하세요?",
          "original_form": "로제떡볶이 어디 프랜차이즈 젤 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024010.1.5",
          "form": "헉 혹시 다이어트 중이세요?",
          "original_form": "헉 혹시 다이어트 중이세요?",
          "speaker_id": "1",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100024010.1.6",
          "form": "저는 그 떡참인가 하는 브랜드에 진심 이었어요 ㅋㅋㅋ",
          "original_form": "저는 그 떡참인가 하는 브랜드에 진심 이었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100024010.1.7",
          "form": "아니요 다이어트는 아닌데 귀찮아서 끼니를 제때 잘 안 먹어요 ㅋㅋㅋㅋ",
          "original_form": "아니요 다이어트는 아닌데 귀찮아서 끼니를 제때 잘 안 먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024010.1.8",
          "form": "아 떡참 맛있어보이더라구요",
          "original_form": "아 떡참 맛있어보이더라구요",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024010.1.9",
          "form": "전 배떡이랑 태리로제랑 명랑핫도그만 먹어봤어요",
          "original_form": "전 배떡이랑 태리로제랑 명랑핫도그만 먹어봤어요",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024010.1.10",
          "form": "신참 도 많이시켜먹었고",
          "original_form": "신참 도 많이시켜먹었고",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024010.1.11",
          "form": "아니 먹는게 귀찮으시다니!",
          "original_form": "아니 먹는게 귀찮으시다니!",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024010.1.12",
          "form": "저도 좀 그러고싶어요ㅜㅜ",
          "original_form": "저도 좀 그러고싶어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024010.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ맛있는게 많았으면 안귀찮을텐게",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맛있는게 많았으면 안귀찮을텐게",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024010.1.14",
          "form": "텐데",
          "original_form": "텐데",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024010.1.15",
          "form": "챙겨먹어야하니까 귀찮더라구요",
          "original_form": "챙겨먹어야하니까 귀찮더라구요",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024010.1.16",
          "form": "근데 이렇게 굶다가 막 몰아서 엄청 먹고 그래요 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 이렇게 굶다가 막 몰아서 엄청 먹고 그래요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024010.1.17",
          "form": "저는 배민 ㅜ 에돈을 너무 많이보태줘서",
          "original_form": "저는 배민 ㅜ 에돈을 너무 많이보태줘서",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024010.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024010.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저도 돈이 많았다면 그랬을 것 같아요 ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저도 돈이 많았다면 그랬을 것 같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024010.1.20",
          "form": "그돈으로 차라리 배민 주식을 살걸 싶을정도로 ㅋㅋ",
          "original_form": "그돈으로 차라리 배민 주식을 살걸 싶을정도로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024010.1.21",
          "form": "아이구 ㅋㅋㅋㅋ",
          "original_form": "아이구 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024010.1.22",
          "form": "근데 요즘 진짜 배달이 넘 잘돼있어서..",
          "original_form": "근데 요즘 진짜 배달이 넘 잘돼있어서..",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024010.1.23",
          "form": "아니 절대금액이라기보다",
          "original_form": "아니 절대금액이라기보다",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024010.1.24",
          "form": "배고파지면 배민 슬쩍 켜보게되는 것 같아요",
          "original_form": "배고파지면 배민 슬쩍 켜보게되는 것 같아요",
          "speaker_id": "2",
          "time": "20210830 21:31"
        }
      ]
    }
  ]
