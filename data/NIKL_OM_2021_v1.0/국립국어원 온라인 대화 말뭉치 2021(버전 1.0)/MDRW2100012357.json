      "utterance": [
        {
          "id": "MDRW2100012357.1.1",
          "form": "비빔면도 좋아해요",
          "original_form": "비빔면도 좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012357.1.2",
          "form": "면요리는 거의 안 가리는거 같아요",
          "original_form": "면요리는 거의 안 가리는거 같아요",
          "speaker_id": "2",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012357.1.3",
          "form": "파스타 토마토가 좋으세요 크림이 좋으세요?",
          "original_form": "파스타 토마토가 좋으세요 크림이 좋으세요?",
          "speaker_id": "1",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100012357.1.4",
          "form": "저는 토마토를 더 선호해요",
          "original_form": "저는 토마토를 더 선호해요",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012357.1.5",
          "form": "크림은 식으면 느끼하더라구요",
          "original_form": "크림은 식으면 느끼하더라구요",
          "speaker_id": "2",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012357.1.6",
          "form": "아 좀 그런거같네요 첫 몇입은 맛있느데 물려요",
          "original_form": "아 좀 그런거같네요 첫 몇입은 맛있느데 물려요",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012357.1.7",
          "form": "라면은 뭐 젤 좋아하시나요",
          "original_form": "라면은 뭐 젤 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100012357.1.8",
          "form": "라면은 클래식하게 신라면이랑 불닭 좋아해요",
          "original_form": "라면은 클래식하게 신라면이랑 불닭 좋아해요",
          "speaker_id": "2",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100012357.1.9",
          "form": "혹시 신라면 볶음면 드셔보셨나요?",
          "original_form": "혹시 신라면 볶음면 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100012357.1.10",
          "form": "오 저도 불닭 좋아해요",
          "original_form": "오 저도 불닭 좋아해요",
          "speaker_id": "1",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100012357.1.11",
          "form": "신라면 볶음면은 아직 안먹어봤는데",
          "original_form": "신라면 볶음면은 아직 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100012357.1.12",
          "form": "어떤가요??",
          "original_form": "어떤가요??",
          "speaker_id": "1",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100012357.1.13",
          "form": "저도 아직 못먹어봤어요ㅋㅋ",
          "original_form": "저도 아직 못먹어봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012357.1.14",
          "form": "다른 분한테 추천받았는데 꼭 봉지라면으로 먹으라고 하시더라구요",
          "original_form": "다른 분한테 추천받았는데 꼭 봉지라면으로 먹으라고 하시더라구요",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012357.1.15",
          "form": "헐 차이가 있나봐요",
          "original_form": "헐 차이가 있나봐요",
          "speaker_id": "1",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012357.1.16",
          "form": "그런가봐요... name1은 어떤 라면 좋아하세요?",
          "original_form": "그런가봐요... &name1&은 어떤 라면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012357.1.17",
          "form": "저 까르보불닭이랑 오모리 김치찌개라면 좋아하는데 아시나요 ㅋㅋㅋ",
          "original_form": "저 까르보불닭이랑 오모리 김치찌개라면 좋아하는데 아시나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012357.1.18",
          "form": "오모리 김치찌개라면은 말로만 들어봤어요",
          "original_form": "오모리 김치찌개라면은 말로만 들어봤어요",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012357.1.19",
          "form": "그거 진짜 맛있어요 추천드립니다",
          "original_form": "그거 진짜 맛있어요 추천드립니다",
          "speaker_id": "1",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100012357.1.20",
          "form": "ㅇㅣ건 봉 ㄹ 지말고 컵으로 드세요 ㅋㅋ",
          "original_form": "ㅇㅣ건 봉 ㄹ 지말고 컵으로 드세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100012357.1.21",
          "form": "알겠습니다ㅋㅋ",
          "original_form": "알겠습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:18"
        },
        {
          "id": "MDRW2100012357.1.22",
          "form": "오늘의 급식 메뉴는 뭔가요?",
          "original_form": "오늘의 급식 메뉴는 뭔가요?",
