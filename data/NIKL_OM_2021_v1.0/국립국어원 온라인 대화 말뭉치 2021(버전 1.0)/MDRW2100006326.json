      "utterance": [
        {
          "id": "MDRW2100006326.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006326.1.2",
          "form": "특수시즌때만요",
          "original_form": "특수시즌때만요",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006326.1.3",
          "form": "특수시즌이라 하면 올림픽 월드컵이요?",
          "original_form": "특수시즌이라 하면 올림픽 월드컵이요?",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100006326.1.4",
          "form": "네네ㅋㅋ",
          "original_form": "네네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006326.1.5",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006326.1.6",
          "form": "전 평소에도 야구 열심히 봐요ㅋㅋ",
          "original_form": "전 평소에도 야구 열심히 봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100006326.1.7",
          "form": "직관도 가요?",
          "original_form": "직관도 가요?",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100006326.1.8",
          "form": "직관은 못 간지 한 3년 됐어요",
          "original_form": "직관은 못 간지 한 3년 됐어요",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006326.1.9",
          "form": "그전에도 일년에 두어번 갔지만요ㅋㅋ 주로 중계방송으로 보구요",
          "original_form": "그전에도 일년에 두어번 갔지만요ㅋㅋ 주로 중계방송으로 보구요",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100006326.1.10",
          "form": "전 한번 ㅋㅋ 가봤어요",
          "original_form": "전 한번 ㅋㅋ 가봤어요",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100006326.1.11",
          "form": "어느팀 응원하세요?",
          "original_form": "어느팀 응원하세요?",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100006326.1.12",
          "form": "두산베어스 팬이에요",
          "original_form": "두산베어스 팬이에요",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100006326.1.13",
          "form": "두산 팬층 두껍더라고요",
          "original_form": "두산 팬층 두껍더라고요",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100006326.1.14",
          "form": "최근에 잘 하는 편이어서 젊은 팬들 많이 생겼죠ㅋㅋ",
          "original_form": "최근에 잘 하는 편이어서 젊은 팬들 많이 생겼죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100006326.1.15",
          "form": "잘하고있나봐요!",
          "original_form": "잘하고있나봐요!",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100006326.1.16",
          "form": "ㅋㅋ 근데 올해는 못하고 있어요. 이제 뭐 그럴 때도 되긴 했죠",
          "original_form": "ㅋㅋ 근데 올해는 못하고 있어요. 이제 뭐 그럴 때도 되긴 했죠",
