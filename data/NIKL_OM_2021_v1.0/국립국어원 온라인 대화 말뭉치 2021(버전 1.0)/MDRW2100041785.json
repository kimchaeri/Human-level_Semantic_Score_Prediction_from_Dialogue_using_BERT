        },
        {
          "id": "MDRW2100041785.1.2",
          "form": "재채기랑 콧물이 나온다 ㅠㅠ",
          "original_form": "재채기랑 콧물이 나온다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041785.1.3",
          "form": "빨리 약 먹어라",
          "original_form": "빨리 약 먹어라",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041785.1.4",
          "form": "웅웅..",
          "original_form": "웅웅..",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041785.1.5",
          "form": "무리하면 안 됐지",
          "original_form": "무리하면 안 됐지",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041785.1.6",
          "form": "웅.... ㅋㅋ 오랜만에 삘받아서 무리했네",
          "original_form": "웅.... ㅋㅋ 오랜만에 삘받아서 무리했네",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041785.1.7",
          "form": "그래 조금씩만 해라",
          "original_form": "그래 조금씩만 해라",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041785.1.8",
          "form": "넴..",
          "original_form": "넴..",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041785.1.9",
          "form": "건강이 최고다",
          "original_form": "건강이 최고다",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041785.1.10",
          "form": "웅 감기 넘 싫구만 ㅠ",
          "original_form": "웅 감기 넘 싫구만 ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041785.1.11",
          "form": "감기는 안 돼",
          "original_form": "감기는 안 돼",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041785.1.12",
          "form": "빨리 떨어져랏..",
          "original_form": "빨리 떨어져랏..",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041785.1.13",
          "form": "따뜻하게 입어라",
          "original_form": "따뜻하게 입어라",
