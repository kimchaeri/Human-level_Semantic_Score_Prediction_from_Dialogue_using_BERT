      "utterance": [
        {
          "id": "MDRW2100011375.1.1",
          "form": "모가디슈 재밌나요?",
          "original_form": "모가디슈 재밌나요?",
          "speaker_id": "2",
          "time": "20210824 11:53"
        },
        {
          "id": "MDRW2100011375.1.2",
          "form": "저도그거보고싶어요",
          "original_form": "저도그거보고싶어요",
          "speaker_id": "2",
          "time": "20210824 11:53"
        },
        {
          "id": "MDRW2100011375.1.3",
          "form": "저는 재밌게봤어요 보고나니 아프가니스탄 터져서 실감나더라구요",
          "original_form": "저는 재밌게봤어요 보고나니 아프가니스탄 터져서 실감나더라구요",
          "speaker_id": "1",
          "time": "20210824 11:55"
        },
        {
          "id": "MDRW2100011375.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 12:15"
        },
        {
          "id": "MDRW2100011375.1.5",
          "form": "아프간이난리죠ㅠ",
          "original_form": "아프간이난리죠ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:15"
        },
        {
          "id": "MDRW2100011375.1.6",
          "form": "싱크홀도 재난영화죠?",
          "original_form": "싱크홀도 재난영화죠?",
          "speaker_id": "2",
          "time": "20210824 12:15"
        },
        {
          "id": "MDRW2100011375.1.7",
          "form": "아 그건 못봤네요 개그라던데",
          "original_form": "아 그건 못봤네요 개그라던데",
          "speaker_id": "1",
          "time": "20210824 12:16"
        },
        {
          "id": "MDRW2100011375.1.8",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011375.1.9",
          "form": "싱크홀이요??",
          "original_form": "싱크홀이요??",
          "speaker_id": "2",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011375.1.10",
          "form": "재난영화라고 알고있었는데..!",
          "original_form": "재난영화라고 알고있었는데..!",
          "speaker_id": "2",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011375.1.11",
          "form": "재난소재인데 개그로풀었나봐요 저도 아직안봐서  몰겠네요",
          "original_form": "재난소재인데 개그로풀었나봐요 저도 아직안봐서  몰겠네요",
          "speaker_id": "1",
          "time": "20210824 12:24"
        },
        {
          "id": "MDRW2100011375.1.12",
          "form": "아 개그였어요?",
          "original_form": "아 개그였어요?",
          "speaker_id": "2",
          "time": "20210824 12:29"
        },
        {
          "id": "MDRW2100011375.1.13",
          "form": "저는 진지물인지알았네요ㅋㅋㅋ",
          "original_form": "저는 진지물인지알았네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:30"
        },
        {
          "id": "MDRW2100011375.1.14",
          "form": "싱크홀이 재밌다던데 제가 좋아하는 배우는 없어서 걸렀어요  누누님은 영화뭐보셨나요",
          "original_form": "싱크홀이 재밌다던데 제가 좋아하는 배우는 없어서 걸렀어요  누누님은 영화뭐보셨나요",
          "speaker_id": "1",
          "time": "20210824 12:32"
        },
        {
          "id": "MDRW2100011375.1.15",
          "form": "전 기억도안나네요ㅎㅎ 하도예전이라",
          "original_form": "전 기억도안나네요ㅎㅎ 하도예전이라",
          "speaker_id": "2",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100011375.1.16",
          "form": "공포물잘보시나요?",
          "original_form": "공포물잘보시나요?",
          "speaker_id": "2",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100011375.1.17",
          "form": "오우 노웅",
          "original_form": "오우 노웅",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011375.1.18",
          "form": "좀비물좋아해요",
          "original_form": "좀비물좋아해요",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011375.1.19",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210824 12:44"
        },
        {
          "id": "MDRW2100011375.1.20",
          "form": "안징그럽나요?",
          "original_form": "안징그럽나요?",
          "speaker_id": "2",
          "time": "20210824 12:44"
        },
        {
          "id": "MDRW2100011375.1.21",
          "form": "좀비는 그래도 사람이 이기잖아요",
          "original_form": "좀비는 그래도 사람이 이기잖아요",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100011375.1.22",
          "form": "ㅋㅋㅋ 저는",
          "original_form": "ㅋㅋㅋ 저는",
          "speaker_id": "2",
          "time": "20210824 12:52"
        },
        {
          "id": "MDRW2100011375.1.23",
          "form": "좀비물은 진짜못보거든요ㅠ",
          "original_form": "좀비물은 진짜못보거든요ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:52"
        }
      ]
