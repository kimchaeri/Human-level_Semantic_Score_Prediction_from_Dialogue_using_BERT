      "utterance": [
        {
          "id": "MDRW2100004050.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004050.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004050.1.3",
          "form": "구입 물품 있으신가요?",
          "original_form": "구입 물품 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004050.1.4",
          "form": "최근에는....",
          "original_form": "최근에는....",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004050.1.5",
          "form": "엄... 먹을것밖에 산게 없네요",
          "original_form": "엄... 먹을것밖에 산게 없네요",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004050.1.6",
          "form": "어 저두요 ㅋㅋ",
          "original_form": "어 저두요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004050.1.7",
          "form": "먹을 거만 사게 되더라구요",
          "original_form": "먹을 거만 사게 되더라구요",
          "speaker_id": "2",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004050.1.8",
          "form": "뭐 사셨어요?",
          "original_form": "뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004050.1.9",
          "form": "어제 파바에서 빵좀 사구",
          "original_form": "어제 파바에서 빵좀 사구",
          "speaker_id": "1",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004050.1.10",
          "form": "엔젤리너스에서 반미??",
          "original_form": "엔젤리너스에서 반미??",
          "speaker_id": "1",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004050.1.11",
          "form": "우와 반미도 파는군여",
          "original_form": "우와 반미도 파는군여",
          "speaker_id": "2",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004050.1.12",
          "form": "그것두 사서 오늘 먹었네요",
          "original_form": "그것두 사서 오늘 먹었네요",
          "speaker_id": "1",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004050.1.13",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004050.1.14",
          "form": "파바 빵 뭐 좋아하세요??!",
          "original_form": "파바 빵 뭐 좋아하세요??!",
          "speaker_id": "2",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004050.1.15",
          "form": "파바 슈요!",
          "original_form": "파바 슈요!",
          "speaker_id": "1",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004050.1.16",
          "form": "보통 뚜레쥬르가 빵 더 맛있는데",
          "original_form": "보통 뚜레쥬르가 빵 더 맛있는데",
          "speaker_id": "1",
          "time": "20210821 14:40"
        },
        {
          "id": "MDRW2100004050.1.17",
          "form": "슈는 파바가 더 맛있더라구요",
          "original_form": "슈는 파바가 더 맛있더라구요",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004050.1.18",
          "form": "name2님은 최근에 어떤걸 구매하셨나요?",
          "original_form": "&name2&님은 최근에 어떤걸 구매하셨나요?",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004050.1.19",
          "form": "슈는 파바다 기억하고",
          "original_form": "슈는 파바다 기억하고",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004050.1.20",
          "form": "나중에 먹어봐야겠어요",
          "original_form": "나중에 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004050.1.21",
          "form": "파바는 돌돌말이소세지인가도 유명하더라구요",
          "original_form": "파바는 돌돌말이소세지인가도 유명하더라구요",
          "speaker_id": "2",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100004050.1.22",
          "form": "맞아요 소세지도 유명하죠",
          "original_form": "맞아요 소세지도 유명하죠",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004050.1.23",
          "form": "근데 그건 취향은 아니라서",
          "original_form": "근데 그건 취향은 아니라서",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004050.1.24",
          "form": "잘 안먹게 되더라구요",
          "original_form": "잘 안먹게 되더라구요",
          "speaker_id": "1",
          "time": "20210821 14:42"
        },
        {
          "id": "MDRW2100004050.1.25",
          "form": "뚜레쥬르에서는 브라우니랑 치즈방앗간...",
          "original_form": "뚜레쥬르에서는 브라우니랑 치즈방앗간...",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004050.1.26",
          "form": "치즈방앗간 꼭 드세요",
          "original_form": "치즈방앗간 꼭 드세요",
          "speaker_id": "1",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100004050.1.27",
          "form": "치즈방앗간 유명하다고 들었는데",
          "original_form": "치즈방앗간 유명하다고 들었는데",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004050.1.28",
          "form": "그리 맛있나요?",
          "original_form": "그리 맛있나요?",
          "speaker_id": "2",
          "time": "20210821 14:44"
        },
        {
          "id": "MDRW2100004050.1.29",
          "form": "제가 좋아하는 요소가 다 있어요",
          "original_form": "제가 좋아하는 요소가 다 있어요",
          "speaker_id": "1",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004050.1.30",
          "form": "짭짤한 체다치즈맛 살짝 + 크림치즈 + 찹쌀",
          "original_form": "짭짤한 체다치즈맛 살짝 + 크림치즈 + 찹쌀",
          "speaker_id": "1",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004050.1.31",
          "form": "아 정말 맛이 궁금하네요",
          "original_form": "아 정말 맛이 궁금하네요",
          "speaker_id": "2",
          "time": "20210821 14:46"
        },
        {
          "id": "MDRW2100004050.1.32",
          "form": "먹고싶어요",
          "original_form": "먹고싶어요",
          "speaker_id": "2",
          "time": "20210821 14:46"
        },
        {
          "id": "MDRW2100004050.1.33",
          "form": "이렇게 맛없을 수 없는 구성..ㅠㅠㅠ",
          "original_form": "이렇게 맛없을 수 없는 구성..ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:46"
        }
      ]
