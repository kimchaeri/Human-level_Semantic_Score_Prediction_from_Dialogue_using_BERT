      "utterance": [
        {
          "id": "MDRW2100006534.1.1",
          "form": "음 살짝 있어",
          "original_form": "음 살짝 있어",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100006534.1.2",
          "form": "추천있어??",
          "original_form": "추천있어??",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100006534.1.3",
          "form": "음 흠 ㅋㅋㅋㅋㅋ근데 뭔가 남의 머리랔ㅋㅋ",
          "original_form": "음 흠 ㅋㅋㅋㅋㅋ근데 뭔가 남의 머리랔ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100006534.1.4",
          "form": "추천하기가 애매하다",
          "original_form": "추천하기가 애매하다",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100006534.1.5",
          "form": "그때 했던건 너무 어두웠지?",
          "original_form": "그때 했던건 너무 어두웠지?",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100006534.1.6",
          "form": "사실 그 띠 첫 염색이었는데 한 티가 안나서 아쉬웠었어ㅋㅋㅋㅋ",
          "original_form": "사실 그 띠 첫 염색이었는데 한 티가 안나서 아쉬웠었어ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100006534.1.7",
          "form": "엥 그때 너 나한테 엄청 뭐라 했자낳ㅎㅎ..ㅎㅎ",
          "original_form": "엥 그때 너 나한테 엄청 뭐라 했자낳ㅎㅎ..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100006534.1.8",
          "form": "뭐라했었지??",
          "original_form": "뭐라했었지??",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100006534.1.9",
          "form": "허허허 망했다구",
          "original_form": "허허허 망했다구",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100006534.1.10",
          "form": "책임지라구..ㅎ  그래서 내가 돈까지 줬었는뎅",
          "original_form": "책임지라구..ㅎ  그래서 내가 돈까지 줬었는뎅",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100006534.1.11",
          "form": "엥 아니야",
          "original_form": "엥 아니야",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100006534.1.12",
          "form": "아니다 난 기억 잘 안나니까 너가 맞겠지",
          "original_form": "아니다 난 기억 잘 안나니까 너가 맞겠지",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100006534.1.13",
          "form": "미안해 비싼거 살게..",
          "original_form": "미안해 비싼거 살게..",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100006534.1.14",
          "form": "ㅠㅠ 기억이 안나다니",
          "original_form": "ㅠㅠ 기억이 안나다니",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100006534.1.15",
          "form": "아냐 뭐 지난 일이니깧ㅎㅎ.. ㄴ",
          "original_form": "아냐 뭐 지난 일이니깧ㅎㅎ.. ㄴ",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100006534.1.16",
          "form": "돈 주지 말껄 ㅡㅡ 은혜도 모르고",
          "original_form": "돈 주지 말껄 ㅡㅡ 은혜도 모르고",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100006534.1.17",
          "form": "그때 아쉽긴했어",
          "original_form": "그때 아쉽긴했어",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
