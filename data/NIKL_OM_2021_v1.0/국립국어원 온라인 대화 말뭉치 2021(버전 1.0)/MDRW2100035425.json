      "utterance": [
        {
          "id": "MDRW2100035425.1.1",
          "form": "이제 나온다 그말",
          "original_form": "이제 나온다 그말",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100035425.1.2",
          "form": "돈은 없고 애는 공부시켜야 되고",
          "original_form": "돈은 없고 애는 공부시켜야 되고",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100035425.1.3",
          "form": "그러게 영화지만 현실이다",
          "original_form": "그러게 영화지만 현실이다",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100035425.1.4",
          "form": "현실이다. 돈 없으면 아무것도 못함ㅠ",
          "original_form": "현실이다. 돈 없으면 아무것도 못함ㅠ",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100035425.1.5",
          "form": "기회가 와서 영화처럼 가난하면 그 기회를 잡을수가 없으니",
          "original_form": "기회가 와서 영화처럼 가난하면 그 기회를 잡을수가 없으니",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100035425.1.6",
          "form": "애가 저래 발레를 좋아하네",
          "original_form": "애가 저래 발레를 좋아하네",
          "speaker_id": "1",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100035425.1.7",
          "form": "고향밖으로 나간적이 없단다",
          "original_form": "고향밖으로 나간적이 없단다",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100035425.1.8",
          "form": "학교 좋네",
          "original_form": "학교 좋네",
          "speaker_id": "1",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100035425.1.9",
          "form": "응 런던이잖아 ㅋㅋ",
          "original_form": "응 런던이잖아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100035425.1.10",
          "form": "확실히 틀리네",
          "original_form": "확실히 틀리네",
          "speaker_id": "1",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100035425.1.11",
          "form": "그렇지 거기서 주연 발레리노가 되는거니 성공한거지",
          "original_form": "그렇지 거기서 주연 발레리노가 되는거니 성공한거지",
          "speaker_id": "2",
          "time": "20210829 16:50"
        },
        {
          "id": "MDRW2100035425.1.12",
          "form": "완전 성공한거지",
          "original_form": "완전 성공한거지",
          "speaker_id": "1",
          "time": "20210829 16:51"
        },
        {
          "id": "MDRW2100035425.1.13",
          "form": "응 그래서 아빠가 울더라 공연때",
          "original_form": "응 그래서 아빠가 울더라 공연때",
