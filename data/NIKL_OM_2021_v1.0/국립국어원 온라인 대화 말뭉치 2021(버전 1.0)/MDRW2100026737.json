        {
          "id": "MDRW2100026737.1.1",
          "form": "암요",
          "original_form": "암요",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026737.1.2",
          "form": "앵무새 키워봤어요",
          "original_form": "앵무새 키워봤어요",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026737.1.3",
          "form": "말 따라함?",
          "original_form": "말 따라함?",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026737.1.4",
          "form": "안녕하세요 라고 따라합니다",
          "original_form": "안녕하세요 라고 따라합니다",
          "speaker_id": "2",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026737.1.5",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026737.1.6",
          "form": "오~신기하네요",
          "original_form": "오~신기하네요",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026737.1.7",
          "form": "날아가나요?",
          "original_form": "날아가나요?",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026737.1.8",
          "form": "사료는 따로 없고",
          "original_form": "사료는 따로 없고",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026737.1.9",
          "form": "물은?",
          "original_form": "물은?",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026737.1.10",
          "form": "새장에서 풀어 놓으면",
          "original_form": "새장에서 풀어 놓으면",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026737.1.11",
          "form": "오 나오나요?",
          "original_form": "오 나오나요?",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026737.1.12",
          "form": "막 날아다녀요",
          "original_form": "막 날아다녀요",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026737.1.13",
          "form": "신기방기",
          "original_form": "신기방기",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026737.1.14",
          "form": "완전 신기방기",
          "original_form": "완전 신기방기",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026737.1.15",
          "form": "손에 모이주면 손위로오나요?",
          "original_form": "손에 모이주면 손위로오나요?",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026737.1.16",
          "form": "근데 생각보다 커서 무서워요",
          "original_form": "근데 생각보다 커서 무서워요",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026737.1.17",
          "form": "쪼이면 아픕니다",
          "original_form": "쪼이면 아픕니다",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026737.1.18",
          "form": "목욕은?",
          "original_form": "목욕은?",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026737.1.19",
          "form": "안시켜요",
          "original_form": "안시켜요",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026737.1.20",
          "form": "아,,,",
          "original_form": "아,,,",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026737.1.21",
          "form": "새들은 알아서 지들이 해요",
          "original_form": "새들은 알아서 지들이 해요",
          "speaker_id": "2",
