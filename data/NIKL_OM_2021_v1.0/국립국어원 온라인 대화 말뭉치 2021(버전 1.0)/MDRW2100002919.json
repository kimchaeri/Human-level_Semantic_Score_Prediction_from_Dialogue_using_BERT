      "utterance": [
        {
          "id": "MDRW2100002919.1.1",
          "form": "배달음식 좋아하시나요?",
          "original_form": "배달음식 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002919.1.2",
          "form": "음식은 좋아하는데",
          "original_form": "음식은 좋아하는데",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002919.1.3",
          "form": "가격이 비싸서 잘 안사먹는 편이에요",
          "original_form": "가격이 비싸서 잘 안사먹는 편이에요",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002919.1.4",
          "form": "맞아요ㅠㅠ 배달비도 비싸고 요즘은 다비싼것같아요",
          "original_form": "맞아요ㅠㅠ 배달비도 비싸고 요즘은 다비싼것같아요",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002919.1.5",
          "form": "저는 시켜먹으면 떡볶이종류시켜먹어요ㅎㅎ",
          "original_form": "저는 시켜먹으면 떡볶이종류시켜먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002919.1.6",
          "form": "저도 떡볶이 좋아해요",
          "original_form": "저도 떡볶이 좋아해요",
          "speaker_id": "1",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002919.1.7",
          "form": "요즘은 걸작떡볶이 좋아해요",
          "original_form": "요즘은 걸작떡볶이 좋아해요",
          "speaker_id": "1",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002919.1.8",
          "form": "치킨이랑 떡볶이가 같이 와요",
          "original_form": "치킨이랑 떡볶이가 같이 와요",
          "speaker_id": "1",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002919.1.9",
          "form": "저도 걸작떡볶이 좋아해요! 특히 숯불떡볶이 좋아해용 순살치킨도 맛있더라구용",
          "original_form": "저도 걸작떡볶이 좋아해요! 특히 숯불떡볶이 좋아해용 순살치킨도 맛있더라구용",
          "speaker_id": "2",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002919.1.10",
          "form": "오 저는 여기서 기본이랑 순살떡닭세트만 시켜먹어봤어요",
          "original_form": "오 저는 여기서 기본이랑 순살떡닭세트만 시켜먹어봤어요",
          "speaker_id": "1",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002919.1.11",
          "form": "숯불도 먹어봐야겠네요",
          "original_form": "숯불도 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002919.1.12",
          "form": "정말 불맛이 나서 넘 맛나더라구요!",
          "original_form": "정말 불맛이 나서 넘 맛나더라구요!",
          "speaker_id": "2",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100002919.1.13",
          "form": "우와.. 넘 맛있을거 같아요",
          "original_form": "우와.. 넘 맛있을거 같아요",
          "speaker_id": "1",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100002919.1.14",
          "form": "다른맛 먹어볼 생각을 왜 못했지",
          "original_form": "다른맛 먹어볼 생각을 왜 못했지",
          "speaker_id": "1",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100002919.1.15",
          "form": "기본맛만 20번 넘게 시켜먹었어요",
          "original_form": "기본맛만 20번 넘게 시켜먹었어요",
          "speaker_id": "1",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100002919.1.16",
          "form": "저는 오히려 기본맛을 못먹어봤던것같아요!ㅎㅎ저는 다음에는 기본을 먹어봐야겠어용",
          "original_form": "저는 오히려 기본맛을 못먹어봤던것같아요!ㅎㅎ저는 다음에는 기본을 먹어봐야겠어용",
          "speaker_id": "2",
          "time": "20210820 16:40"
        },
        {
          "id": "MDRW2100002919.1.17",
          "form": "떡볶이 말고 다른 음식은 뭐 좋아하세요?",
          "original_form": "떡볶이 말고 다른 음식은 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002919.1.18",
          "form": "저는 피자도 좋아해요! 뭐좋아하세용?",
          "original_form": "저는 피자도 좋아해요! 뭐좋아하세용?",
          "speaker_id": "2",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002919.1.19",
          "form": "저도 피자 좋아해요",
          "original_form": "저도 피자 좋아해요",
          "speaker_id": "1",
          "time": "20210820 16:42"
        },
        {
          "id": "MDRW2100002919.1.20",
          "form": "도미노랑 미스터피자에",
          "original_form": "도미노랑 미스터피자에",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002919.1.21",
          "form": "새우 포테이토 고구마 등등 좋아해요",
          "original_form": "새우 포테이토 고구마 등등 좋아해요",
          "speaker_id": "1",
          "time": "20210820 16:43"
        }
      ]
    }
