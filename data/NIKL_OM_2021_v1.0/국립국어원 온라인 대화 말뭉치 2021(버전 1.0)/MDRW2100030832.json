      "utterance": [
        {
          "id": "MDRW2100030832.1.1",
          "form": "대나무 조그만걸로 그릇 되나",
          "original_form": "대나무 조그만걸로 그릇 되나",
          "speaker_id": "2",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030832.1.2",
          "form": "죽부인이던가??",
          "original_form": "죽부인이던가??",
          "speaker_id": "1",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030832.1.3",
          "form": "찻잔 정도는 될거같은데 잘 모르겠네요",
          "original_form": "찻잔 정도는 될거같은데 잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030832.1.4",
          "form": "장식품도 있었던거같은데",
          "original_form": "장식품도 있었던거같은데",
          "speaker_id": "2",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030832.1.5",
          "form": "대나무 회초리는 예전에 썼는데",
          "original_form": "대나무 회초리는 예전에 썼는데",
          "speaker_id": "1",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030832.1.6",
          "form": "저한테 쓰진 마시궁 ㅎㅠ ..",
          "original_form": "저한테 쓰진 마시궁 ㅎㅠ ..",
          "speaker_id": "2",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030832.1.7",
          "form": "단소밖에 없었던거 같기도 하고",
          "original_form": "단소밖에 없었던거 같기도 하고",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030832.1.8",
          "form": "기억이 잘 안나네",
          "original_form": "기억이 잘 안나네",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030832.1.9",
          "form": "다음에 가면 되죠",
          "original_form": "다음에 가면 되죠",
          "speaker_id": "2",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030832.1.10",
          "form": "아직 안망했으려나?",
          "original_form": "아직 안망했으려나?",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030832.1.11",
          "form": "코로나때문에 닫은곳도 많을까요?",
          "original_form": "코로나때문에 닫은곳도 많을까요?",
          "speaker_id": "2",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030832.1.12",
          "form": "그래도 그런곳 닫기는 아까운데...",
          "original_form": "그래도 그런곳 닫기는 아까운데...",
          "speaker_id": "2",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030832.1.13",
          "form": "학교에서도 단체 체험활동 가니까",
          "original_form": "학교에서도 단체 체험활동 가니까",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030832.1.14",
          "form": "근데 학교도 못가잖아요 요즘애들... 불쌍혀...",
          "original_form": "근데 학교도 못가잖아요 요즘애들... 불쌍혀...",
          "speaker_id": "2",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030832.1.15",
          "form": "이런저런 체험하고 보고다녀야 하는데 어릴때는",
          "original_form": "이런저런 체험하고 보고다녀야 하는데 어릴때는",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030832.1.16",
          "form": "시국이 문제다",
          "original_form": "시국이 문제다",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030832.1.17",
          "form": "맞아요 마스크 잘 끼고 다녀야만 ㅠ",
          "original_form": "맞아요 마스크 잘 끼고 다녀야만 ㅠ",
          "speaker_id": "2",
          "time": "20210904 13:14"
        }
      ]
