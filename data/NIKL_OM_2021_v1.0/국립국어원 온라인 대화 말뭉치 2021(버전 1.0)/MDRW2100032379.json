      "utterance": [
        {
          "id": "MDRW2100032379.1.1",
          "form": "요약해준다고 해서 봤는데ㅜㅜ",
          "original_form": "요약해준다고 해서 봤는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032379.1.2",
          "form": "왜 내 하루가 다 날라가니...",
          "original_form": "왜 내 하루가 다 날라가니...",
          "speaker_id": "2",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032379.1.3",
          "form": "컨텐츠 만드는 사람들 대단한거같아요 ㅋㅋ",
          "original_form": "컨텐츠 만드는 사람들 대단한거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:46"
        },
        {
          "id": "MDRW2100032379.1.4",
          "form": "ㅋㅋㅋㅋㅋ그거도 보는 사람이나 편하지",
          "original_form": "ㅋㅋㅋㅋㅋ그거도 보는 사람이나 편하지",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032379.1.5",
          "form": "편집은 힘들텐데 그죠?",
          "original_form": "편집은 힘들텐데 그죠?",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100032379.1.6",
          "form": "네 기획까지 해야하는거니까 더 그럴것 같아요",
          "original_form": "네 기획까지 해야하는거니까 더 그럴것 같아요",
          "speaker_id": "1",
          "time": "20210905 17:57"
        },
        {
          "id": "MDRW2100032379.1.7",
          "form": "ㅋㅋㅋㅋ요새는 운동 선수들도 유투브 다양하게 많으 하네요",
          "original_form": "ㅋㅋㅋㅋ요새는 운동 선수들도 유투브 다양하게 많으 하네요",
          "speaker_id": "2",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100032379.1.8",
          "form": "오 어떤 선수요?",
          "original_form": "오 어떤 선수요?",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032379.1.9",
          "form": "지금은 무슨 스톡킹?",
          "original_form": "지금은 무슨 스톡킹?",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032379.1.10",
          "form": "이라는데에서 손아섭 선수 나온거 보고왔어요ㅋㅋㅋㅋㅋ",
          "original_form": "이라는데에서 손아섭 선수 나온거 보고왔어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032379.1.11",
          "form": "선수들 재미있네요",
          "original_form": "선수들 재미있네요",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032379.1.12",
          "form": "운동 선수들 이야기 잘 못 접하니까 그런 컨텐츠 좋은 것 같아요",
          "original_form": "운동 선수들 이야기 잘 못 접하니까 그런 컨텐츠 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032379.1.13",
          "form": "그니까요 그냥 굳은 표정으로 게임만 하는줄 알았더니",
          "original_form": "그니까요 그냥 굳은 표정으로 게임만 하는줄 알았더니",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032379.1.14",
          "form": "되게 그 안에서 두뇌싸움도 치열하고 상대팀 선수들이랑도 재미있게 잘 지내고ㅋㅋㅋ",
          "original_form": "되게 그 안에서 두뇌싸움도 치열하고 상대팀 선수들이랑도 재미있게 잘 지내고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032379.1.15",
          "form": "재미있네요 좋아하는 스포츠? 있으세요",
          "original_form": "재미있네요 좋아하는 스포츠? 있으세요",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032379.1.16",
          "form": "저는 딱히 챙겨보는 리그 스포츠는 없고 가끔 일년에 한두번 직관 가고 그러는것 같아요 직접 가서 보는건 다 재밌으니 ㅋㅋ",
          "original_form": "저는 딱히 챙겨보는 리그 스포츠는 없고 가끔 일년에 한두번 직관 가고 그러는것 같아요 직접 가서 보는건 다 재밌으니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100032379.1.17",
          "form": "오 어뗜 종목 직관이요?",
          "original_form": "오 어뗜 종목 직관이요?",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100032379.1.18",
          "form": "배구 야구 축구.. 무엇이든지요 ㅋㅋ 농구장은 안 가봤어요 ㅋㅋ",
          "original_form": "배구 야구 축구.. 무엇이든지요 ㅋㅋ 농구장은 안 가봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100032379.1.19",
          "form": "오 저는 배구랑 농구는 대학교 대항전만 가봤어요ㅋㅋㅋㅋ",
          "original_form": "오 저는 배구랑 농구는 대학교 대항전만 가봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100032379.1.20",
          "form": "야구 축구는 저도 연에 한두번씩ㅋㅋㅋ진짜 재밋는듯합니다",
          "original_form": "야구 축구는 저도 연에 한두번씩ㅋㅋㅋ진짜 재밋는듯합니다",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100032379.1.21",
          "form": "응원하고 맥주랑 간식먹고 하는게",
          "original_form": "응원하고 맥주랑 간식먹고 하는게",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100032379.1.22",
          "form": "저는 배구 직관이 젤 재밌었어요 실내라 날씨 상관없이 볼 수 있고 ㅋㅋ",
          "original_form": "저는 배구 직관이 젤 재밌었어요 실내라 날씨 상관없이 볼 수 있고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100032379.1.23",
          "form": "으으 연경신 있을 때 꼭 직관가보고 싶었는데ㅜㅡ",
          "original_form": "으으 연경신 있을 때 꼭 직관가보고 싶었는데ㅜㅡ",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100032379.1.24",
          "form": "그쵸 연경신 ㅜㅜ",
          "original_form": "그쵸 연경신 ㅜㅜ",
