      "utterance": [
        {
          "id": "MDRW2100008280.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008280.1.2",
          "form": "헉 무교이기는 하지만 절에는 자주 놀러갔던거같아요",
          "original_form": "헉 무교이기는 하지만 절에는 자주 놀러갔던거같아요",
          "speaker_id": "2",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008280.1.3",
          "form": "혹시 종교 있으신가요?",
          "original_form": "혹시 종교 있으신가요?",
          "speaker_id": "2",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008280.1.4",
          "form": "저는 예전애 성당 다녔었어요",
          "original_form": "저는 예전애 성당 다녔었어요",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008280.1.5",
          "form": "지금은 안다니고 부모님 따라서요 ㅎㅎ",
          "original_form": "지금은 안다니고 부모님 따라서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008280.1.6",
          "form": "절은 어떤가요?",
          "original_form": "절은 어떤가요?",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008280.1.7",
          "form": "절은 좀 그 향이 맘을 편안하게 해주는 거 같아요",
          "original_form": "절은 좀 그 향이 맘을 편안하게 해주는 거 같아요",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008280.1.8",
          "form": "성당 오",
          "original_form": "성당 오",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008280.1.9",
          "form": "성당은 그 지역마다 가는 성당이 있다고 들었어요",
          "original_form": "성당은 그 지역마다 가는 성당이 있다고 들었어요",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008280.1.10",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008280.1.11",
          "form": "지역마다 하나씩있어서",
          "original_form": "지역마다 하나씩있어서",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008280.1.12",
          "form": "집에서 가까운 곳으로 가죠",
          "original_form": "집에서 가까운 곳으로 가죠",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008280.1.13",
          "form": "이사하면서 바꾸기도 하는데 계속 차타고 다니시는 분듷도 있다고 들었어요",
          "original_form": "이사하면서 바꾸기도 하는데 계속 차타고 다니시는 분듷도 있다고 들었어요",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008280.1.14",
          "form": "아 절향 참 마음 편하게 해주죠 ㅠ",
          "original_form": "아 절향 참 마음 편하게 해주죠 ㅠ",
          "speaker_id": "1",
          "time": "20210822 14:52"
        },
        {
          "id": "MDRW2100008280.1.15",
          "form": "근데 절은 가기 좀 힘들지않나요? 잘 몰라서",
          "original_form": "근데 절은 가기 좀 힘들지않나요? 잘 몰라서",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008280.1.16",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008280.1.17",
          "form": "근데 놀러다니는 사람이 많아서",
          "original_form": "근데 놀러다니는 사람이 많아서",
          "speaker_id": "2",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008280.1.18",
          "form": "그냥 들어갈 수는 있는데",
          "original_form": "그냥 들어갈 수는 있는데",
          "speaker_id": "2",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008280.1.19",
          "form": "그 찾아가기가 아주 난리납니다",
          "original_form": "그 찾아가기가 아주 난리납니다",
          "speaker_id": "2",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008280.1.20",
          "form": "차 무조건 필수",
          "original_form": "차 무조건 필수",
          "speaker_id": "2",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008280.1.21",
          "form": "와 진짜 힘들겠어요",
          "original_form": "와 진짜 힘들겠어요",
          "speaker_id": "1",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100008280.1.22",
          "form": "저도 어렸을때는 가봤던것같은데",
          "original_form": "저도 어렸을때는 가봤던것같은데",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008280.1.23",
          "form": "막 산같은 곳에 갔던 기억이 있어요",
          "original_form": "막 산같은 곳에 갔던 기억이 있어요",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008280.1.24",
          "form": "경치는 참 좋아요",
          "original_form": "경치는 참 좋아요",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008280.1.25",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008280.1.26",
          "form": "그 경치보러 가는거죠",
          "original_form": "그 경치보러 가는거죠",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008280.1.27",
          "form": "저는 그 명동성당 들어가봤었는데",
          "original_form": "저는 그 명동성당 들어가봤었는데",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008280.1.28",
          "form": "성당은 진짜 웅장함이 있는거같아요",
          "original_form": "성당은 진짜 웅장함이 있는거같아요",
          "speaker_id": "2",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008280.1.29",
          "form": "홀리해진다해야하나",
          "original_form": "홀리해진다해야하나",
          "speaker_id": "2",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008280.1.30",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008280.1.31",
          "form": "규모도 크고",
          "original_form": "규모도 크고",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008280.1.32",
          "form": "특히 창에 색들어간게 어릴적에는 그렇게 예뻐보이더라고요",
          "original_form": "특히 창에 색들어간게 어릴적에는 그렇게 예뻐보이더라고요",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008280.1.33",
          "form": "근데 성당 떡볶이가 진짜 맛있어요 ㅋㅋ",
          "original_form": "근데 성당 떡볶이가 진짜 맛있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008280.1.34",
          "form": "아니 성당 왜 요리를 잘하나요",
          "original_form": "아니 성당 왜 요리를 잘하나요",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008280.1.35",
          "form": "그 복음자리 잼 핫하잖아욬ㅋㅋㅋ",
          "original_form": "그 복음자리 잼 핫하잖아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008280.1.36",
          "form": "고수들의 모임장소같아요 ㅋㅋㅋ",
          "original_form": "고수들의 모임장소같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008280.1.37",
          "form": "성탄절에 달란트 데이같은거 하는데",
          "original_form": "성탄절에 달란트 데이같은거 하는데",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008280.1.38",
          "form": "어우 장난아니에요",
          "original_form": "어우 장난아니에요",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008280.1.39",
          "form": "클스마스때 가면 주나요",
          "original_form": "클스마스때 가면 주나요",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008280.1.40",
          "form": "약간 불순한 마음이라 안주나요",
          "original_form": "약간 불순한 마음이라 안주나요",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008280.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008280.1.42",
          "form": "ㅋㅋㅋㅋㅋ 줄걸요",
          "original_form": "ㅋㅋㅋㅋㅋ 줄걸요",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008280.1.43",
          "form": "오는 사람 안막더라고요",
          "original_form": "오는 사람 안막더라고요",
          "speaker_id": "1",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008280.1.44",
          "form": "홀리하다",
          "original_form": "홀리하다",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008280.1.45",
