      "utterance": [
        {
          "id": "MDRW2100020439.1.1",
          "form": "저도 여초 직장 다녔었어요!",
          "original_form": "저도 여초 직장 다녔었어요!",
          "speaker_id": "1",
          "time": "20210828 05:45"
        },
        {
          "id": "MDRW2100020439.1.2",
          "form": "지금은 퇴사했지만ㅋㅋ",
          "original_form": "지금은 퇴사했지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:45"
        },
        {
          "id": "MDRW2100020439.1.3",
          "form": "앗 퇴사하셨군요",
          "original_form": "앗 퇴사하셨군요",
          "speaker_id": "2",
          "time": "20210828 05:46"
        },
        {
          "id": "MDRW2100020439.1.4",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210828 05:46"
        },
        {
          "id": "MDRW2100020439.1.5",
          "form": "아핰ㅋㅋㅋㅋ",
          "original_form": "아핰ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:46"
        },
        {
          "id": "MDRW2100020439.1.6",
          "form": "여초 직장이 장점도 있는데",
          "original_form": "여초 직장이 장점도 있는데",
          "speaker_id": "1",
          "time": "20210828 05:46"
        },
        {
          "id": "MDRW2100020439.1.7",
          "form": "단점이 더 많았어요...주륵",
          "original_form": "단점이 더 많았어요...주륵",
          "speaker_id": "1",
          "time": "20210828 05:46"
        },
        {
          "id": "MDRW2100020439.1.8",
          "form": "맞아요 모든것엔 음양의 조화가 있어야 겠더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 모든것엔 음양의 조화가 있어야 겠더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:46"
        },
        {
          "id": "MDRW2100020439.1.9",
          "form": "네네 적절한 성비ㅋㅋㅋ",
          "original_form": "네네 적절한 성비ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:46"
        },
        {
          "id": "MDRW2100020439.1.10",
          "form": "여고를 나와서 여초의 단점을 잘 알았는데도",
          "original_form": "여고를 나와서 여초의 단점을 잘 알았는데도",
          "speaker_id": "2",
          "time": "20210828 05:46"
        },
        {
          "id": "MDRW2100020439.1.11",
          "form": "헐 저도 여대 나왔어욬ㅋㅋㅋ",
          "original_form": "헐 저도 여대 나왔어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:47"
        },
        {
          "id": "MDRW2100020439.1.12",
          "form": "결국은 여초에 와서.ㅎㅎㅎ..",
          "original_form": "결국은 여초에 와서.ㅎㅎㅎ..",
          "speaker_id": "2",
          "time": "20210828 05:47"
        },
        {
          "id": "MDRW2100020439.1.13",
          "form": "좀 나잇대 있으신분들 상대하는게 힘들었어요",
          "original_form": "좀 나잇대 있으신분들 상대하는게 힘들었어요",
          "speaker_id": "1",
          "time": "20210828 05:47"
        },
        {
          "id": "MDRW2100020439.1.14",
          "form": "기혼은 기혼대로 미혼은 미혼대로...",
          "original_form": "기혼은 기혼대로 미혼은 미혼대로...",
          "speaker_id": "1",
          "time": "20210828 05:47"
        },
        {
          "id": "MDRW2100020439.1.15",
          "form": "맞아요 뭔가 가치관도 좀 다르고",
          "original_form": "맞아요 뭔가 가치관도 좀 다르고",
          "speaker_id": "2",
          "time": "20210828 05:47"
        },
        {
          "id": "MDRW2100020439.1.16",
          "form": "ㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 05:47"
        },
        {
          "id": "MDRW2100020439.1.17",
          "form": "네네ㅠㅠ",
          "original_form": "네네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 05:47"
        },
        {
          "id": "MDRW2100020439.1.18",
          "form": "그리고 자꾸 제 사생활 영역을 침범당하는 느낌도 들구...",
          "original_form": "그리고 자꾸 제 사생활 영역을 침범당하는 느낌도 들구...",
          "speaker_id": "1",
          "time": "20210828 05:47"
        },
        {
          "id": "MDRW2100020439.1.19",
          "form": "맞아요 저 볼때마다 시집보내야 한다고",
          "original_form": "맞아요 저 볼때마다 시집보내야 한다고",
          "speaker_id": "2",
          "time": "20210828 05:47"
        },
        {
          "id": "MDRW2100020439.1.20",
          "form": "그래서 남자친구 생겨도 생겼다 말안했어요ㅋㅋㅋ 너무 캐서",
          "original_form": "그래서 남자친구 생겨도 생겼다 말안했어요ㅋㅋㅋ 너무 캐서",
          "speaker_id": "1",
          "time": "20210828 05:48"
        },
        {
          "id": "MDRW2100020439.1.21",
          "form": "ㅠㅠㅋㅋ일과 결혼할게여 하고",
          "original_form": "ㅠㅠㅋㅋ일과 결혼할게여 하고",
          "speaker_id": "2",
          "time": "20210828 05:48"
        },
        {
          "id": "MDRW2100020439.1.22",
          "form": "아휴ㅠㅠ 오지라퍼 많아여",
          "original_form": "아휴ㅠㅠ 오지라퍼 많아여",
          "speaker_id": "1",
          "time": "20210828 05:48"
        },
        {
          "id": "MDRW2100020439.1.23",
          "form": "저도 아직 말안했어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 아직 말안했어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:48"
        },
        {
          "id": "MDRW2100020439.1.24",
          "form": "교육계면 진짜 애들보다",
          "original_form": "교육계면 진짜 애들보다",
          "speaker_id": "1",
          "time": "20210828 05:51"
        },
        {
          "id": "MDRW2100020439.1.25",
          "form": "학부모 상대하는게 만만치 않으시겠어요",
          "original_form": "학부모 상대하는게 만만치 않으시겠어요",
          "speaker_id": "1",
          "time": "20210828 05:51"
        }
      ]
    }
