      "utterance": [
        {
          "id": "MDRW2100038862.1.1",
          "form": "그런거같아요 전 예전에 김과장이란 드라마에서 처음 봤거든요ㅋㅋ 그땐 그냥 어리버리였는데ㅎㅎ",
          "original_form": "그런거같아요 전 예전에 김과장이란 드라마에서 처음 봤거든요ㅋㅋ 그땐 그냥 어리버리였는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100038862.1.2",
          "form": "헉 저두요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헉 저두요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038862.1.3",
          "form": "김과장 너무 재밌었어요",
          "original_form": "김과장 너무 재밌었어요",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038862.1.4",
          "form": "그때 준호가 진짜 최고 귀엽고 그랬거든요",
          "original_form": "그때 준호가 진짜 최고 귀엽고 그랬거든요",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038862.1.5",
          "form": "우리집준호 되기전에ㅋㅋ",
          "original_form": "우리집준호 되기전에ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038862.1.6",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100038862.1.7",
          "form": "준호랑 남궁민 합이 좋았어요",
          "original_form": "준호랑 남궁민 합이 좋았어요",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100038862.1.8",
          "form": "ㅋㅋ아 진짜 좋았어요",
          "original_form": "ㅋㅋ아 진짜 좋았어요",
          "speaker_id": "2",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100038862.1.9",
          "form": "빈센조도 그정도로 끝날줄 알았는데 마피아라 넘나 잔인...ㅜㅋㅋ",
          "original_form": "빈센조도 그정도로 끝날줄 알았는데 마피아라 넘나 잔인...ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:28"
        },
        {
          "id": "MDRW2100038862.1.10",
          "form": "헉 빈센조 잔인하게 끝났어요?",
          "original_form": "헉 빈센조 잔인하게 끝났어요?",
          "speaker_id": "1",
          "time": "20210902 22:29"
        },
        {
          "id": "MDRW2100038862.1.11",
          "form": "네ㅜㅋㅋ 악역들 다 처형ㅜㅋㅋ",
          "original_form": "네ㅜㅋㅋ 악역들 다 처형ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:31"
        },
        {
          "id": "MDRW2100038862.1.12",
          "form": "어머 그렇군요",
          "original_form": "어머 그렇군요",
          "speaker_id": "1",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100038862.1.13",
          "form": "전 1화에서 하차해서...",
          "original_form": "전 1화에서 하차해서...",
          "speaker_id": "1",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100038862.1.14",
          "form": "유명한 드라마들 다 1회에서 하차해ㅛ어요... 슬의도..",
          "original_form": "유명한 드라마들 다 1회에서 하차해ㅛ어요... 슬의도..",
          "speaker_id": "1",
          "time": "20210902 22:32"
        },
        {
          "id": "MDRW2100038862.1.15",
          "form": "아 저도 진득하게 끝까지 못봐욕ㅋㅋ 기사나 인터넷 캡쳐로ㅋㅋ",
          "original_form": "아 저도 진득하게 끝까지 못봐욕ㅋㅋ 기사나 인터넷 캡쳐로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100038862.1.16",
          "form": "슬의는 1회에 한시간반이라힘들더라고요ㅠㅋㅋ",
          "original_form": "슬의는 1회에 한시간반이라힘들더라고요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100038862.1.17",
          "form": "게다가 시리즈..",
          "original_form": "게다가 시리즈..",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100038862.1.18",
          "form": "시그널2는 빨리 나왔으면 좋겠어요",
          "original_form": "시그널2는 빨리 나왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100038862.1.19",
          "form": "시그널 무섭지않아요??",
          "original_form": "시그널 무섭지않아요??",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100038862.1.20",
          "form": "좀 무서운 회차들 있었어요",
          "original_form": "좀 무서운 회차들 있었어요",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100038862.1.21",
          "form": "근데 진짜... 명작 띵작 갓띵작..",
          "original_form": "근데 진짜... 명작 띵작 갓띵작..",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038862.1.22",
          "form": "꼭봐주세요..",
          "original_form": "꼭봐주세요..",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100038862.1.23",
          "form": "꼭 보겠습니다!!! 라온마하고 시그널 쫌씩만 봤어요ㅎㅎ",
          "original_form": "꼭 보겠습니다!!! 라온마하고 시그널 쫌씩만 봤어요ㅎㅎ",
