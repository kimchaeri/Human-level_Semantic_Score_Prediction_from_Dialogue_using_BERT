      "utterance": [
        {
          "id": "MDRW2100024920.1.1",
          "form": "안녕하세요 커뮤 하시나요 ㅎㅎ",
          "original_form": "안녕하세요 커뮤 하시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:49"
        },
        {
          "id": "MDRW2100024920.1.2",
          "form": "카페하고있어요",
          "original_form": "카페하고있어요",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100024920.1.3",
          "form": "많이 하시나요?",
          "original_form": "많이 하시나요?",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100024920.1.4",
          "form": "아니요 저도 카페정도만 해요 ㅋㅋ",
          "original_form": "아니요 저도 카페정도만 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024920.1.5",
          "form": "카페면 다음하시나요 네이버하시나요?",
          "original_form": "카페면 다음하시나요 네이버하시나요?",
          "speaker_id": "1",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024920.1.6",
          "form": "네이버여",
          "original_form": "네이버여",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024920.1.7",
          "form": "정모도 해보셨나요?",
          "original_form": "정모도 해보셨나요?",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024920.1.8",
          "form": "아 그러시구나 저는 정모는 안해봤어요 해보셨나요? ㅎㅎ",
          "original_form": "아 그러시구나 저는 정모는 안해봤어요 해보셨나요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100024920.1.9",
          "form": "저도 아직ㅎ",
          "original_form": "저도 아직ㅎ",
          "speaker_id": "2",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100024920.1.10",
          "form": "ㅋㅋㅋ저는 낯을 가려서...ㅎㅎ..",
          "original_form": "ㅋㅋㅋ저는 낯을 가려서...ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210831 21:03"
        },
        {
          "id": "MDRW2100024920.1.11",
          "form": "네이버 카페면 사시는 지역 카페 하시나여??ㅋㅋ",
          "original_form": "네이버 카페면 사시는 지역 카페 하시나여??ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100024920.1.12",
          "form": "네 지역 카볘여",
          "original_form": "네 지역 카볘여",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024920.1.13",
          "form": "정모는 어떤 느낌일지ㅎ",
          "original_form": "정모는 어떤 느낌일지ㅎ",
          "speaker_id": "2",
          "time": "20210831 21:06"
        },
        {
          "id": "MDRW2100024920.1.14",
          "form": "오오 그러시구나 지역 정보 많이 얻을거같아여 ㅎㅎ",
          "original_form": "오오 그러시구나 지역 정보 많이 얻을거같아여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100024920.1.15",
          "form": "또래분들이면 재밌을거같아요 ㅎㅎ",
          "original_form": "또래분들이면 재밌을거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100024920.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100024920.1.17",
          "form": "하는분들은 꾸준히 하시나바요",
          "original_form": "하는분들은 꾸준히 하시나바요",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100024920.1.18",
          "form": "오 그렇게 동네친구 생기면 좋을거같아여ㅎㅎ",
          "original_form": "오 그렇게 동네친구 생기면 좋을거같아여ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100024920.1.19",
          "form": "어젠가 한번 해봐야겠어요ㅋ",
          "original_form": "어젠가 한번 해봐야겠어요ㅋ",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100024920.1.20",
          "form": "넵 제친구도 지역 모임 갖는데 재밌다 하더라구요 ㅎㅎ 저도 기회되면 해볼까봐요 ㅎㅎ",
          "original_form": "넵 제친구도 지역 모임 갖는데 재밌다 하더라구요 ㅎㅎ 저도 기회되면 해볼까봐요 ㅎㅎ",
