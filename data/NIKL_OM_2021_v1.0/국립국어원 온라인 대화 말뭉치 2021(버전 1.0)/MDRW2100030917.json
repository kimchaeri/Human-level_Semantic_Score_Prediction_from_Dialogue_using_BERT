      "utterance": [
        {
          "id": "MDRW2100030917.1.1",
          "form": "네네진짜이뻐요",
          "original_form": "네네진짜이뻐요",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030917.1.2",
          "form": "선캐쳐도 있나보네요",
          "original_form": "선캐쳐도 있나보네요",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030917.1.3",
          "form": "지르세요 ! ㅋㅋ",
          "original_form": "지르세요 ! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030917.1.4",
          "form": "원데이클래스도있다고해서",
          "original_form": "원데이클래스도있다고해서",
          "speaker_id": "1",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030917.1.5",
          "form": "와 배유러 가보세요 아내분이랑",
          "original_form": "와 배유러 가보세요 아내분이랑",
          "speaker_id": "2",
          "time": "20210904 14:15"
        },
        {
          "id": "MDRW2100030917.1.6",
          "form": "배워보고싶어요",
          "original_form": "배워보고싶어요",
          "speaker_id": "1",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030917.1.7",
          "form": "선캐쳐는 빨리만들것같아요",
          "original_form": "선캐쳐는 빨리만들것같아요",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030917.1.8",
          "form": "드라마 알고있지만에 송강이 저런걸 나비로 만들던데",
          "original_form": "드라마 알고있지만에 송강이 저런걸 나비로 만들던데",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030917.1.9",
          "form": "앗ㅎㅎ저 아직 미혼이긴한데ㅋㅋ만들러 가보려구요",
          "original_form": "앗ㅎㅎ저 아직 미혼이긴한데ㅋㅋ만들러 가보려구요",
          "speaker_id": "1",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030917.1.10",
          "form": "혼자면 뭐어때여~~",
          "original_form": "혼자면 뭐어때여~~",
          "speaker_id": "1",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030917.1.11",
          "form": "아 미혼이시구나 헷갈렸어요 너무 많은분과 대화하다가",
          "original_form": "아 미혼이시구나 헷갈렸어요 너무 많은분과 대화하다가",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030917.1.12",
          "form": "네네 당연하죠 다녀오셔요",
          "original_form": "네네 당연하죠 다녀오셔요",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030917.1.13",
          "form": "후기알려쥬세요",
          "original_form": "후기알려쥬세요",
          "speaker_id": "2",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030917.1.14",
          "form": "네ㅎㅎ나중에 가보게되면 추천해드릴게요!!",
          "original_form": "네ㅎㅎ나중에 가보게되면 추천해드릴게요!!",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100030917.1.15",
          "form": "원데이클래스도 신기한거많더라구요",
          "original_form": "원데이클래스도 신기한거많더라구요",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030917.1.16",
          "form": "조향하는것도 있구",
          "original_form": "조향하는것도 있구",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030917.1.17",
          "form": "룸스프레이 좋아하시잖아요",
          "original_form": "룸스프레이 좋아하시잖아요",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030917.1.18",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "1",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030917.1.19",
          "form": "원데이가 좋아요 여러개 해볼슈있어서",
          "original_form": "원데이가 좋아요 여러개 해볼슈있어서",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100030917.1.20",
          "form": "요새는 터프팅건?",
          "original_form": "요새는 터프팅건?",
          "speaker_id": "1",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100030917.1.21",
          "form": "유행하던데 가격이 꽤나가더라구요???",
          "original_form": "유행하던데 가격이 꽤나가더라구요???",
          "speaker_id": "1",
          "time": "20210904 14:20"
