          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040057.1.2",
          "form": "얼음 딜캐가 하나 더 있으면 좋을거같긴 한데...",
          "original_form": "얼음 딜캐가 하나 더 있으면 좋을거같긴 한데...",
          "speaker_id": "1",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040057.1.3",
          "form": "막판에",
          "original_form": "막판에",
          "speaker_id": "1",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040057.1.4",
          "form": "직접 부순건 누구야?",
          "original_form": "직접 부순건 누구야?",
          "speaker_id": "1",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040057.1.5",
          "form": "나 얼음캐ㅋ",
          "original_form": "나 얼음캐ㅋ",
          "speaker_id": "2",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040057.1.6",
          "form": "중운?",
          "original_form": "중운?",
          "speaker_id": "1",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040057.1.7",
          "form": "있는거",
          "original_form": "있는거",
          "speaker_id": "2",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040057.1.8",
          "form": "로자리아밖에ㅋ",
          "original_form": "로자리아밖에ㅋ",
          "speaker_id": "2",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040057.1.9",
          "form": "없어",
          "original_form": "없어",
          "speaker_id": "2",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040057.1.10",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040057.1.11",
          "form": "공략에선 누구로 부쉇어?",
          "original_form": "공략에선 누구로 부쉇어?",
          "speaker_id": "1",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040057.1.12",
          "form": "다시 봐볼게",
          "original_form": "다시 봐볼게",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100040057.1.13",
          "form": "중운 궁 쓰고",
          "original_form": "중운 궁 쓰고",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100040057.1.14",
          "form": "설탕으로 바로 바꾸는데",
          "original_form": "설탕으로 바로 바꾸는데",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100040057.1.15",
          "form": "뭘로 부숴진지는",
          "original_form": "뭘로 부숴진지는",
          "speaker_id": "2",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100040057.1.16",
          "form": "모르겠어",
          "original_form": "모르겠어",
          "speaker_id": "2",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100040057.1.17",
          "form": "막판에 누구로 끝나?",
          "original_form": "막판에 누구로 끝나?",
          "speaker_id": "1",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100040057.1.18",
          "form": "설탕",
          "original_form": "설탕",
          "speaker_id": "2",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100040057.1.19",
          "form": "아 그리고 설탕 쓰는게",
          "original_form": "아 그리고 설탕 쓰는게",
          "speaker_id": "2",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100040057.1.20",
          "form": "궁 모션이 디게 짧다",
          "original_form": "궁 모션이 디게 짧다",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040057.1.21",
          "form": "중운 궁을 뭘로 대체하지......",
          "original_form": "중운 궁을 뭘로 대체하지......",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040057.1.22",
          "form": "ㅎㅏ 일단",
          "original_form": "ㅎㅏ 일단",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040057.1.23",
          "form": "케이ㅣ아 설탕으로",
          "original_form": "케이ㅣ아 설탕으로",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040057.1.24",
          "form": "해볼게",
          "original_form": "해볼게",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040057.1.25",
          "form": "내껀 이지니까",
          "original_form": "내껀 이지니까",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040057.1.26",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040057.1.27",
          "form": "중운이 양손검캐라",
          "original_form": "중운이 양손검캐라",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040057.1.28",
          "form": "궁을 쓴거같은데",
          "original_form": "궁을 쓴거같은데",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040057.1.29",
          "form": "ㅇㅇ그리고 중운도 궁모션 좀",
          "original_form": "ㅇㅇ그리고 중운도 궁모션 좀",
          "speaker_id": "2",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100040057.1.30",
          "form": "짧은편인듯?",
          "original_form": "짧은편인듯?",
          "speaker_id": "2",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100040057.1.31",
          "form": "강하게 딜 넣을",
          "original_form": "강하게 딜 넣을",
          "speaker_id": "1",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100040057.1.32",
          "form": "양손검 없어?",
          "original_form": "양손검 없어?",
          "speaker_id": "1",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100040057.1.33",
          "form": "없지",
          "original_form": "없지",
          "speaker_id": "2",
          "time": "20210904 01:55"
        },
        {
          "id": "MDRW2100040057.1.34",
          "form": "라이덴도 서폿이고",
          "original_form": "라이덴도 서폿이고",
          "speaker_id": "1",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100040057.1.35",
          "form": "음....",
          "original_form": "음....",
          "speaker_id": "1",
          "time": "20210904 01:56"
        }
      ]
    }
