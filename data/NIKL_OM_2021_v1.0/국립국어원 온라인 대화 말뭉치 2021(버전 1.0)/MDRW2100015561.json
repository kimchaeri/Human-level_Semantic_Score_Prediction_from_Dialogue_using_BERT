      "utterance": [
        {
          "id": "MDRW2100015561.1.1",
          "form": "이북으로 보면 불편한 점은 없어요?",
          "original_form": "이북으로 보면 불편한 점은 없어요?",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015561.1.2",
          "form": "네 익숙해져가지고 ㅋㅋㅋ",
          "original_form": "네 익숙해져가지고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015561.1.3",
          "form": "로맨스판타지 장르 책 봐요",
          "original_form": "로맨스판타지 장르 책 봐요",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015561.1.4",
          "form": "와.. 저는 집중 안될 것 같아요 ㅜㅜ",
          "original_form": "와.. 저는 집중 안될 것 같아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015561.1.5",
          "form": "진짜 가벼운 ㅋㅋㅋ",
          "original_form": "진짜 가벼운 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015561.1.6",
          "form": "이북 혹시 읽어주는 기능 있나요?",
          "original_form": "이북 혹시 읽어주는 기능 있나요?",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015561.1.7",
          "form": "사실 소설들도 빌려보다가",
          "original_form": "사실 소설들도 빌려보다가",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015561.1.8",
          "form": "도서관에서",
          "original_form": "도서관에서",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015561.1.9",
          "form": "나중에는 아이패드에 다 대여해서 봤어요",
          "original_form": "나중에는 아이패드에 다 대여해서 봤어요",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015561.1.10",
          "form": "저도 탭 있어서 스마트도서관에서 빌릴 수 있떠라구요",
          "original_form": "저도 탭 있어서 스마트도서관에서 빌릴 수 있떠라구요",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015561.1.11",
          "form": "있는데 답답해요 ㅋㅋ",
          "original_form": "있는데 답답해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015561.1.12",
          "form": "스마트도서관 좋죠",
          "original_form": "스마트도서관 좋죠",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015561.1.13",
          "form": "아 어떤 점이 답답해요?",
          "original_form": "아 어떤 점이 답답해요?",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015561.1.14",
          "form": "진짜 편한데 신간이 별로 없어요",
          "original_form": "진짜 편한데 신간이 별로 없어요",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015561.1.15",
          "form": "그거 신청하면 해줘요!",
          "original_form": "그거 신청하면 해줘요!",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015561.1.16",
          "form": "아 그 확실히 가끔은 종이가 그리워질때도 있다는 뜻이요",
          "original_form": "아 그 확실히 가끔은 종이가 그리워질때도 있다는 뜻이요",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015561.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015561.1.18",
          "form": "요새 보니까 신간인지는 모르겠지만 따끈한 도서 많이 있던데요?",
          "original_form": "요새 보니까 신간인지는 모르겠지만 따끈한 도서 많이 있던데요?",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015561.1.19",
          "form": "신청해야겠어요",
          "original_form": "신청해야겠어요",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015561.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015561.1.21",
          "form": "도서관책 많이 읽었던건 1년전이라",
          "original_form": "도서관책 많이 읽었던건 1년전이라",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015561.1.22",
          "form": "저 스마트도서관에 있는 지 모르고 <기분이 태도가 되지 않게> 책 샀어요..ㅜㅜ",
          "original_form": "저 스마트도서관에 있는 지 모르고 <기분이 태도가 되지 않게> 책 샀어요..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015561.1.23",
          "form": "많이 발전했네요",
          "original_form": "많이 발전했네요",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015561.1.24",
          "form": "아 ㅋㅋㅋㅋ그럴때 아깝죠",
          "original_form": "아 ㅋㅋㅋㅋ그럴때 아깝죠",
