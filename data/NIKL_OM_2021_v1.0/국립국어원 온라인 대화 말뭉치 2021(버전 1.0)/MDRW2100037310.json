        },
        {
          "id": "MDRW2100037310.1.4",
          "form": "없어서 못 먹지",
          "original_form": "없어서 못 먹지",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100037310.1.5",
          "form": "좋아하는 맛이 있니",
          "original_form": "좋아하는 맛이 있니",
          "speaker_id": "1",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100037310.1.6",
          "form": "밀크",
          "original_form": "밀크",
          "speaker_id": "2",
          "time": "20210831 22:25"
        },
        {
          "id": "MDRW2100037310.1.7",
          "form": "크런키 맛이 있는데",
          "original_form": "크런키 맛이 있는데",
          "speaker_id": "1",
          "time": "20210831 22:26"
