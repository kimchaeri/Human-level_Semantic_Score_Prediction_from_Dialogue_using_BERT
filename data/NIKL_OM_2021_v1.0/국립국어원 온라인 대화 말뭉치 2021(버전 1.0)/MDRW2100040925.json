      "utterance": [
        {
          "id": "MDRW2100040925.1.1",
          "form": "최근에 관심 갖고 계신 물건이 있나요",
          "original_form": "최근에 관심 갖고 계신 물건이 있나요",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100040925.1.2",
          "form": "방수운동화랑 방수커버요",
          "original_form": "방수운동화랑 방수커버요",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100040925.1.3",
          "form": "오 비오는 날 필요한 거네요",
          "original_form": "오 비오는 날 필요한 거네요",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100040925.1.4",
          "form": "네 발이 젖는 게 싫어서요",
          "original_form": "네 발이 젖는 게 싫어서요",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100040925.1.5",
          "form": "맞아요 발 축축해지는 것 진짜 싫죠ㆍ",
          "original_form": "맞아요 발 축축해지는 것 진짜 싫죠ㆍ",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100040925.1.6",
          "form": "보신 물건이 있나요?",
          "original_form": "보신 물건이 있나요?",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100040925.1.7",
          "form": "스케쳐스 아식스요 커버는 일단 다이소요",
          "original_form": "스케쳐스 아식스요 커버는 일단 다이소요",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100040925.1.8",
          "form": "혹시 방수운동화나 방수커버 사용해 본 적 있으세요?",
          "original_form": "혹시 방수운동화나 방수커버 사용해 본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100040925.1.9",
          "form": "아아 스케쳐스 운동화 좋아하느뉘ㅣ",
          "original_form": "아아 스케쳐스 운동화 좋아하느뉘ㅣ",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100040925.1.10",
          "form": "하느뉘ㅣ",
          "original_form": "하느뉘ㅣ",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100040925.1.11",
          "form": "전 방수운동화는 처음 들어봤어요",
          "original_form": "전 방수운동화는 처음 들어봤어요",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100040925.1.12",
          "form": "이번에 나왔더라구요 ㅎㅎ",
          "original_form": "이번에 나왔더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100040925.1.13",
          "form": "name2님은 구매 예정 있으세요?",
          "original_form": "&name2&님은 구매 예정 있으세요?",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100040925.1.14",
          "form": "한번 찾아봐야겠어요",
          "original_form": "한번 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100040925.1.15",
          "form": "요즘 비가 자주오니",
          "original_form": "요즘 비가 자주오니",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100040925.1.16",
          "form": "유용할 것 같네요",
          "original_form": "유용할 것 같네요",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100040925.1.17",
          "form": "갑자기 비 많이 올 때 좋겠더라구요",
          "original_form": "갑자기 비 많이 올 때 좋겠더라구요",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100040925.1.18",
          "form": "네 솔깃하네요",
          "original_form": "네 솔깃하네요",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100040925.1.19",
          "form": "크록스 신었는데 발이 아파서 알아보게 됐어요",
          "original_form": "크록스 신었는데 발이 아파서 알아보게 됐어요",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100040925.1.20",
          "form": "발 아파요",
          "original_form": "발 아파요",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100040925.1.21",
          "form": "크록스 오래 신고 다니면",
          "original_form": "크록스 오래 신고 다니면",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100040925.1.22",
          "form": "네 ㅠ 그래서 방수로 바꾸려고요 비나 장마도 미리 대비할 겸 해서요",
          "original_form": "네 ㅠ 그래서 방수로 바꾸려고요 비나 장마도 미리 대비할 겸 해서요",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100040925.1.23",
          "form": "저는 요즘 스마트기기들이 사고싶더라고요",
          "original_form": "저는 요즘 스마트기기들이 사고싶더라고요",
