      "utterance": [
        {
          "id": "MDRW2100035513.1.1",
          "form": "저녁 뭐 드셨어요??",
          "original_form": "저녁 뭐 드셨어요??",
          "speaker_id": "1",
          "time": "20210829 19:10"
        },
        {
          "id": "MDRW2100035513.1.2",
          "form": "저 곱창전골 먹었어요!",
          "original_form": "저 곱창전골 먹었어요!",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100035513.1.3",
          "form": "곱창 잘 드세요?",
          "original_form": "곱창 잘 드세요?",
          "speaker_id": "2",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100035513.1.4",
          "form": "네 ㅠㅠ",
          "original_form": "네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100035513.1.5",
          "form": "저 곱창 좋아해요",
          "original_form": "저 곱창 좋아해요",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100035513.1.6",
          "form": "저도 엄청 좋아해요ㅠㅠ",
          "original_form": "저도 엄청 좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 18:02"
        },
        {
          "id": "MDRW2100035513.1.7",
          "form": "원랜 구워먹는 곱창을 더 좋아해요",
          "original_form": "원랜 구워먹는 곱창을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210831 18:02"
        },
        {
          "id": "MDRW2100035513.1.8",
          "form": "저두요 ㅎ",
          "original_form": "저두요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100035513.1.9",
          "form": "그래도 구워먹는게 좋죠",
          "original_form": "그래도 구워먹는게 좋죠",
          "speaker_id": "1",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100035513.1.10",
          "form": "맞아요ㅠㅠ구워먹는 곱창이 최고",
          "original_form": "맞아요ㅠㅠ구워먹는 곱창이 최고",
          "speaker_id": "2",
          "time": "20210831 20:45"
        },
        {
          "id": "MDRW2100035513.1.11",
          "form": "안먹은지 넘 오래됐어요",
          "original_form": "안먹은지 넘 오래됐어요",
          "speaker_id": "2",
          "time": "20210831 20:45"
        },
        {
          "id": "MDRW2100035513.1.12",
          "form": "아 ㅠ 저도여",
          "original_form": "아 ㅠ 저도여",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100035513.1.13",
          "form": "그런 곱창이",
          "original_form": "그런 곱창이",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100035513.1.14",
          "form": "은근 비싸거드뇨 ㅠ",
          "original_form": "은근 비싸거드뇨 ㅠ",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100035513.1.15",
          "form": "아 맞아요ㅠㅠ",
          "original_form": "아 맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100035513.1.16",
          "form": "여럿이 먹으면 10만원 금방..^^",
          "original_form": "여럿이 먹으면 10만원 금방..^^",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100035513.1.17",
          "form": "진짜 ㅠ 양은 적은데 금액은",
          "original_form": "진짜 ㅠ 양은 적은데 금액은",
          "speaker_id": "1",
