        },
        {
          "id": "MDRW2100010906.1.2",
          "form": "할 수 있는 요리가 뭐야",
          "original_form": "할 수 있는 요리가 뭐야",
          "speaker_id": "2",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010906.1.3",
          "form": "뭐좋아하세요",
          "original_form": "뭐좋아하세요",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010906.1.4",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010906.1.5",
          "form": "나는~~~~ 요리잘못해",
          "original_form": "나는~~~~ 요리잘못해",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010906.1.6",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010906.1.7",
          "form": "나는 조금 할 수 있어!",
          "original_form": "나는 조금 할 수 있어!",
          "speaker_id": "2",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010906.1.8",
          "form": "뭐 잘해?",
          "original_form": "뭐 잘해?",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010906.1.9",
          "form": "라면 끓이기!",
          "original_form": "라면 끓이기!",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010906.1.10",
          "form": "또?",
          "original_form": "또?",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010906.1.11",
          "form": "계란후라이?",
          "original_form": "계란후라이?",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010906.1.12",
          "form": "계란후라이는",
          "original_form": "계란후라이는",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010906.1.13",
          "form": "소금이좋아캐찹이좋아?",
          "original_form": "소금이좋아캐찹이좋아?",
          "speaker_id": "1",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010906.1.14",
          "form": "난 둘 다 좋아",
          "original_form": "난 둘 다 좋아",
          "speaker_id": "2",
          "time": "20210823 22:55"
        },
        {
          "id": "MDRW2100010906.1.15",
          "form": "나는 케찹이좋아",
          "original_form": "나는 케찹이좋아",
          "speaker_id": "1",
          "time": "20210823 22:55"
        },
        {
          "id": "MDRW2100010906.1.16",
          "form": "그러면 케찹으로 많이 먹겠네?",
          "original_form": "그러면 케찹으로 많이 먹겠네?",
          "speaker_id": "2",
          "time": "20210823 22:55"
        },
        {
          "id": "MDRW2100010906.1.17",
          "form": "응 나는 거의 맨날",
          "original_form": "응 나는 거의 맨날",
          "speaker_id": "1",
          "time": "20210823 22:56"
        },
