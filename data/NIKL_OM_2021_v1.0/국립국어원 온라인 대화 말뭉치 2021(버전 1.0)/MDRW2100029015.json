          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029015.1.4",
          "form": "뭔 군대",
          "original_form": "뭔 군대",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029015.1.5",
          "form": "군대간 동기 다 돌아왔다",
          "original_form": "군대간 동기 다 돌아왔다",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029015.1.6",
          "form": "ㅋㅋㅋㅋ군대간 동기들 죄다....",
          "original_form": "ㅋㅋㅋㅋ군대간 동기들 죄다....",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029015.1.7",
          "form": "거의 졸업한듯?",
          "original_form": "거의 졸업한듯?",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029015.1.8",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029015.1.9",
          "form": "이제4학년인가",
          "original_form": "이제4학년인가",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029015.1.10",
          "form": "거의 졸업했을걸요",
          "original_form": "거의 졸업했을걸요",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029015.1.11",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029015.1.12",
          "form": "name2님이 5학년이라서?",
          "original_form": "&name2&님이 5학년이라서?",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029015.1.13",
          "form": ".",
          "original_form": ".",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029015.1.14",
          "form": "짱나네",
          "original_form": "짱나네",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029015.1.15",
          "form": "저 듣는수업에 같은학번 3명이거든요",
          "original_form": "저 듣는수업에 같은학번 3명이거든요",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029015.1.16",
          "form": "근데 5학년은 아니지않을까요",
          "original_form": "근데 5학년은 아니지않을까요",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029015.1.17",
          "form": "어이없어",
          "original_form": "어이없어",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029015.1.18",
          "form": "이거군대얘기예요??ㅠ",
          "original_form": "이거군대얘기예요??ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029015.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029015.1.20",
          "form": "맞지...",
          "original_form": "맞지...",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029015.1.21",
          "form": "나군대안갔다왓다고",
          "original_form": "나군대안갔다왓다고",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029015.1.22",
          "form": "저 면제임ㅠ",
          "original_form": "저 면제임ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029015.1.23",
          "form": "군대(친구)얘기",
          "original_form": "군대(친구)얘기",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029015.1.24",
          "form": "요즘은 군대에서 온라인수업도 들을수있는거 같던데요",
          "original_form": "요즘은 군대에서 온라인수업도 들을수있는거 같던데요",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029015.1.25",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029015.1.26",
          "form": "신기하네요",
          "original_form": "신기하네요",
