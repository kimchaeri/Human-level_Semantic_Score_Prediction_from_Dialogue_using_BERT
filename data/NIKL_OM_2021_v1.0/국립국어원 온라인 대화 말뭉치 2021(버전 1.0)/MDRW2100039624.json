      "utterance": [
        {
          "id": "MDRW2100039624.1.1",
          "form": "우와 김장^^",
          "original_form": "우와 김장^^",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039624.1.2",
          "form": "너무맛있겠어요~~",
          "original_form": "너무맛있겠어요~~",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100039624.1.3",
          "form": "흰쌀밥에 올려먹으면 꿀꺽!ㅎㅎ",
          "original_form": "흰쌀밥에 올려먹으면 꿀꺽!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100039624.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 배추 김장 아니고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 배추 김장 아니고",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039624.1.5",
          "form": "초롱무라는게 있더라구요",
          "original_form": "초롱무라는게 있더라구요",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039624.1.6",
          "form": "그거로ㅎㅎ한단담았어요",
          "original_form": "그거로ㅎㅎ한단담았어요",
          "speaker_id": "2",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039624.1.7",
          "form": "오~ 그래도 너무 맛있겠어요 전 배추김치 무김치 생김치 익은김치  다~~~~~좋아해서요^^*",
          "original_form": "오~ 그래도 너무 맛있겠어요 전 배추김치 무김치 생김치 익은김치  다~~~~~좋아해서요^^*",
          "speaker_id": "1",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039624.1.8",
          "form": "오늘은 대신 원래 있던 김치로 김치찜?",
          "original_form": "오늘은 대신 원래 있던 김치로 김치찜?",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039624.1.9",
          "form": "돼지고기 넣고 해먹었어요",
          "original_form": "돼지고기 넣고 해먹었어요",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039624.1.10",
          "form": "아직 배부르네요",
          "original_form": "아직 배부르네요",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039624.1.11",
          "form": "너무맛있으셨겠어용~~",
          "original_form": "너무맛있으셨겠어용~~",
          "speaker_id": "1",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039624.1.12",
          "form": "저녁 잘먹었는데",
          "original_form": "저녁 잘먹었는데",
          "speaker_id": "1",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039624.1.13",
          "form": "저도먹고싶네요ㅋㅋㅋ ㅠㅠ",
          "original_form": "저도먹고싶네요ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039624.1.14",
          "form": "ㅋㅋㅋㅋㅋ저녁은 뭐드셨어요",
          "original_form": "ㅋㅋㅋㅋㅋ저녁은 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039624.1.15",
          "form": "저 국밥먹고왔어용",
          "original_form": "저 국밥먹고왔어용",
          "speaker_id": "1",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039624.1.16",
          "form": "오!! 어떤거요? 순대? 돼지?",
          "original_form": "오!! 어떤거요? 순대? 돼지?",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039624.1.17",
          "form": "고르기가 힘들어서",
          "original_form": "고르기가 힘들어서",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039624.1.18",
          "form": "모듬으로 먹고왔어요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "모듬으로 먹고왔어요ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039624.1.19",
          "form": "우오",
          "original_form": "우오",
          "speaker_id": "2",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039624.1.20",
          "form": "저 완전 국밥충ㅋㅋㅋ소리듣거든요",
          "original_form": "저 완전 국밥충ㅋㅋㅋ소리듣거든요",
          "speaker_id": "2",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039624.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무 좋아합니다...ㅜㅜ 부러워요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무 좋아합니다...ㅜㅜ 부러워요",
          "speaker_id": "2",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039624.1.22",
          "form": "전 김치찌개 김치찜 너무좋아하거든요 ㅋㅋㅋㅋ",
          "original_form": "전 김치찌개 김치찜 너무좋아하거든요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039624.1.23",
          "form": "ㅋㅋㅋ바꼈네요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ바꼈네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039624.1.24",
          "form": "집에서도 자주 해드세요?",
          "original_form": "집에서도 자주 해드세요?",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039624.1.25",
          "form": "네 근데 엄청 맛있는 저희 할머님 김치가",
          "original_form": "네 근데 엄청 맛있는 저희 할머님 김치가",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100039624.1.26",
          "form": "똑!떨어져서  ...",
          "original_form": "똑!떨어져서  ...",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100039624.1.27",
          "form": "사먹는 김치맛은 정말 아니더라구요.ㅠ 흑흑",
          "original_form": "사먹는 김치맛은 정말 아니더라구요.ㅠ 흑흑",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100039624.1.28",
          "form": "오 종갓집 김치 이런거도 그 맛이 안나요?",
          "original_form": "오 종갓집 김치 이런거도 그 맛이 안나요?",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100039624.1.29",
          "form": "네ㅠㅠ 저희 시댁 시골 김장이 정말별걸다 넣거든요 시골 지하수?에다 홍삼까지 넣어서 만드는데",
          "original_form": "네ㅠㅠ 저희 시댁 시골 김장이 정말별걸다 넣거든요 시골 지하수?에다 홍삼까지 넣어서 만드는데",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100039624.1.30",
          "form": "세상맛나욬ㅋㅋㅋㅋ",
          "original_form": "세상맛나욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100039624.1.31",
          "form": "홍삼이요?",
          "original_form": "홍삼이요?",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100039624.1.32",
          "form": "작년에 김장을 못하셔서.....",
          "original_form": "작년에 김장을 못하셔서.....",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100039624.1.33",
          "form": "와ㅋㅋㅋㅋ엄청 정성들여서 만드시네요ㅋㅋㅋㅋㅋ",
          "original_form": "와ㅋㅋㅋㅋ엄청 정성들여서 만드시네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100039624.1.34",
          "form": "네 별걸다넣죠ㅋㅋㅋ",
          "original_form": "네 별걸다넣죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100039624.1.35",
          "form": "근데 홍삼맛은안나요^^;",
          "original_form": "근데 홍삼맛은안나요^^;",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
