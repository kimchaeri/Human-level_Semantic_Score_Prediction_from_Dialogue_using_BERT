      "utterance": [
        {
          "id": "MDRW2100018452.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 02:09"
        },
        {
          "id": "MDRW2100018452.1.2",
          "form": "날이 선선해져서 좋네요",
          "original_form": "날이 선선해져서 좋네요",
          "speaker_id": "1",
          "time": "20210827 02:10"
        },
        {
          "id": "MDRW2100018452.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210827 02:10"
        },
        {
          "id": "MDRW2100018452.1.4",
          "form": "맞아요 신기하게 날이 선선해졌어요",
          "original_form": "맞아요 신기하게 날이 선선해졌어요",
          "speaker_id": "2",
          "time": "20210827 02:10"
        },
        {
          "id": "MDRW2100018452.1.5",
          "form": "밤엔 시원하더라구요",
          "original_form": "밤엔 시원하더라구요",
          "speaker_id": "2",
          "time": "20210827 02:11"
        },
        {
          "id": "MDRW2100018452.1.6",
          "form": "진짜 너무 좋아요",
          "original_form": "진짜 너무 좋아요",
          "speaker_id": "1",
          "time": "20210827 02:11"
        },
        {
          "id": "MDRW2100018452.1.7",
          "form": "처서 지나면 시원하다더니 딱이네요",
          "original_form": "처서 지나면 시원하다더니 딱이네요",
          "speaker_id": "1",
          "time": "20210827 02:11"
        },
        {
          "id": "MDRW2100018452.1.8",
          "form": "어우.. 맞아요. 그래도 올해는 다른 해보다 덜더웠던거 같아요",
          "original_form": "어우.. 맞아요. 그래도 올해는 다른 해보다 덜더웠던거 같아요",
          "speaker_id": "2",
          "time": "20210827 02:13"
        },
        {
          "id": "MDRW2100018452.1.9",
          "form": "그러게요!",
          "original_form": "그러게요!",
          "speaker_id": "1",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018452.1.10",
          "form": "작년엔 비도 많이오고 덥고 ㅠㅠ",
          "original_form": "작년엔 비도 많이오고 덥고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018452.1.11",
          "form": "얼마나 더울까 걱정했는데 다행이에요",
          "original_form": "얼마나 더울까 걱정했는데 다행이에요",
          "speaker_id": "1",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018452.1.12",
          "form": "아 비도 많이 왔었죠~",
          "original_form": "아 비도 많이 왔었죠~",
          "speaker_id": "1",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018452.1.13",
          "form": "장마가 너무 길었어요 ㅠㅠㅠ",
          "original_form": "장마가 너무 길었어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018452.1.14",
          "form": "맞아요 진짜 장마란 이런 것이다",
          "original_form": "맞아요 진짜 장마란 이런 것이다",
          "speaker_id": "1",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018452.1.15",
          "form": "저 그렇게 길게 내리는거 살며 첨 봤어요ㅋㅋㅋㅋ",
          "original_form": "저 그렇게 길게 내리는거 살며 첨 봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:15"
        },
        {
          "id": "MDRW2100018452.1.16",
          "form": "올해는 왔다가 그쳤다가 반복이네요",
          "original_form": "올해는 왔다가 그쳤다가 반복이네요",
          "speaker_id": "1",
          "time": "20210827 02:15"
        },
        {
          "id": "MDRW2100018452.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 여기가 울릉도인가 궁금해지는 양이었죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 여기가 울릉도인가 궁금해지는 양이었죠",
          "speaker_id": "1",
          "time": "20210827 02:15"
        },
        {
          "id": "MDRW2100018452.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ진짜 기상청 예보에 비 밖에 없었던듯",
          "original_form": "ㅋㅋㅋㅋㅋㅋ진짜 기상청 예보에 비 밖에 없었던듯",
          "speaker_id": "2",
          "time": "20210827 02:16"
        },
        {
          "id": "MDRW2100018452.1.19",
          "form": "아 그런데 기상청 요즘 너무 자주 틀리지 않나요 ㅠㅠ",
          "original_form": "아 그런데 기상청 요즘 너무 자주 틀리지 않나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018452.1.20",
          "form": "기상청 체육대회...에도 비가 온다던 말이 생각나네요ㅋㅋㅋ",
          "original_form": "기상청 체육대회...에도 비가 온다던 말이 생각나네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018452.1.21",
          "form": "스콜성 기후라더니 그런 건가 봐요",
          "original_form": "스콜성 기후라더니 그런 건가 봐요",
          "speaker_id": "1",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018452.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ헉ㅋㅋㅋ 그거 일부러 노린거 아닌가요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ헉ㅋㅋㅋ 그거 일부러 노린거 아닌가요?",
          "speaker_id": "2",
          "time": "20210827 02:21"
        },
        {
          "id": "MDRW2100018452.1.23",
          "form": "체육대회를 일찍 마치기 위한...",
          "original_form": "체육대회를 일찍 마치기 위한...",
          "speaker_id": "2",
