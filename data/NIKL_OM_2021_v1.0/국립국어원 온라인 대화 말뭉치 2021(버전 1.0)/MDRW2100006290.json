        },
        {
          "id": "MDRW2100006290.1.2",
          "form": "저건 무슨 종인가",
          "original_form": "저건 무슨 종인가",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006290.1.3",
          "form": "그러게요 모르겠어요",
          "original_form": "그러게요 모르겠어요",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006290.1.4",
          "form": "잡종인가",
          "original_form": "잡종인가",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006290.1.5",
          "form": "원래 똥강아지가 귀엽지",
          "original_form": "원래 똥강아지가 귀엽지",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006290.1.6",
          "form": "잡종이 귀여워영",
          "original_form": "잡종이 귀여워영",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006290.1.7",
          "form": "시골에서 키우는 개들이 귀엽긴 해",
          "original_form": "시골에서 키우는 개들이 귀엽긴 해",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006290.1.8",
          "form": "묶여있지만 않으면 ㅠㅠ",
          "original_form": "묶여있지만 않으면 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006290.1.9",
          "form": "시골은 원래 다 그렇게 키우지",
          "original_form": "시골은 원래 다 그렇게 키우지",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006290.1.10",
          "form": "그런가봐요 불땽해",
          "original_form": "그런가봐요 불땽해",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006290.1.11",
          "form": "그래도 걔네들은 흙에서 구름이 즐거울 거야",
          "original_form": "그래도 걔네들은 흙에서 구름이 즐거울 거야",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006290.1.12",
          "form": "환경이 참 좋져!!",
          "original_form": "환경이 참 좋져!!",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006290.1.13",
          "form": "산책도 많이 다닐 수 있지",
          "original_form": "산책도 많이 다닐 수 있지",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100006290.1.14",
          "form": "그러니까요",
