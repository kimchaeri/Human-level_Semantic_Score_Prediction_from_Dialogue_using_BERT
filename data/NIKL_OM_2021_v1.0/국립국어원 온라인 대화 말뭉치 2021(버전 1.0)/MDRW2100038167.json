        },
        {
          "id": "MDRW2100038167.1.3",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "2",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100038167.1.4",
          "form": "구피키우고있어요",
          "original_form": "구피키우고있어요",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038167.1.5",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038167.1.6",
          "form": "저는 키워본적이 아예 없어요",
          "original_form": "저는 키워본적이 아예 없어요",
          "speaker_id": "2",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038167.1.7",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038167.1.8",
          "form": "키울생각도없으세요?ㅋ",
          "original_form": "키울생각도없으세요?ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100038167.1.9",
          "form": "키우고는 싶은데 부모님이 반대하셔서요",
          "original_form": "키우고는 싶은데 부모님이 반대하셔서요",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100038167.1.10",
          "form": "웰시코기 키우고 싶어요",
          "original_form": "웰시코기 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100038167.1.11",
          "form": "웰시코기 너무 귀엽죠ㅜㅜ",
          "original_form": "웰시코기 너무 귀엽죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100038167.1.12",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "1",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100038167.1.13",
          "form": "웃는 얼굴이 너무 매력적이에요",
          "original_form": "웃는 얼굴이 너무 매력적이에요",
          "speaker_id": "2",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100038167.1.14",
          "form": "네네ㅠㅜ짧은다리랑ㅋㅋ",
          "original_form": "네네ㅠㅜ짧은다리랑ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:46"
        },
        {
          "id": "MDRW2100038167.1.15",
          "form": "너무귀여워요.ㅋ",
          "original_form": "너무귀여워요.ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:46"
        },
        {
          "id": "MDRW2100038167.1.16",
          "form": "맞아요ㅠ 예전에는 동네에서 자주 만났는데",
          "original_form": "맞아요ㅠ 예전에는 동네에서 자주 만났는데",
          "speaker_id": "2",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100038167.1.17",
          "form": "이젠 그럴일도 없네요 나가질 못해서",
          "original_form": "이젠 그럴일도 없네요 나가질 못해서",
          "speaker_id": "2",
          "time": "20210902 12:47"
        },
        {
          "id": "MDRW2100038167.1.18",
