      "utterance": [
        {
          "id": "MDRW2100005195.1.1",
          "form": "퇴근은 몇시에 하세요??",
          "original_form": "퇴근은 몇시에 하세요??",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005195.1.2",
          "form": "6시반이요",
          "original_form": "6시반이요",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005195.1.3",
          "form": "출근은 9시쯤인가요??",
          "original_form": "출근은 9시쯤인가요??",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005195.1.4",
          "form": "9시반이죠 ㅎㅎ",
          "original_form": "9시반이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005195.1.5",
          "form": "점심은 12시반이고요",
          "original_form": "점심은 12시반이고요",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005195.1.6",
          "form": "오 그렇군요!!",
          "original_form": "오 그렇군요!!",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005195.1.7",
          "form": "아직은 알바 조금밖에 안 해봐서",
          "original_form": "아직은 알바 조금밖에 안 해봐서",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005195.1.8",
          "form": "직장생활 관해서는 하나도 모르겠어요",
          "original_form": "직장생활 관해서는 하나도 모르겠어요",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005195.1.9",
          "form": "ㅎㅎ 알바랑은 확실히 차이가 있는 것 같아요",
          "original_form": "ㅎㅎ 알바랑은 확실히 차이가 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210822 10:43"
        },
        {
          "id": "MDRW2100005195.1.10",
          "form": "그렇죠ㅠ",
          "original_form": "그렇죠ㅠ",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100005195.1.11",
          "form": "쫓기는게 좀 덜한달까",
          "original_form": "쫓기는게 좀 덜한달까",
          "speaker_id": "2",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100005195.1.12",
          "form": "직장생활이 더 편하신가요?",
          "original_form": "직장생활이 더 편하신가요?",
          "speaker_id": "1",
          "time": "20210822 16:42"
        },
        {
          "id": "MDRW2100005195.1.13",
          "form": "네 전 알바를 많이 안해보긴했지만 직장생활이 훨 편해요",
          "original_form": "네 전 알바를 많이 안해보긴했지만 직장생활이 훨 편해요",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100005195.1.14",
          "form": "업무에 있어서 주도적 느낌",
          "original_form": "업무에 있어서 주도적 느낌",
          "speaker_id": "2",
          "time": "20210822 16:44"
        },
        {
          "id": "MDRW2100005195.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100005195.1.16",
          "form": "직장생활이 힘들다는 얘기를 많이 들어서 새롭게 다가와요",
          "original_form": "직장생활이 힘들다는 얘기를 많이 들어서 새롭게 다가와요",
          "speaker_id": "1",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100005195.1.17",
          "form": "힘들긴하죠 주도적인만큼 책임이 따르니까요",
          "original_form": "힘들긴하죠 주도적인만큼 책임이 따르니까요",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100005195.1.18",
          "form": "그렇죠ㅜ 책임이란 게 가장 무서운 거 같아요",
          "original_form": "그렇죠ㅜ 책임이란 게 가장 무서운 거 같아요",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100005195.1.19",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100005195.1.20",
          "form": "그래도 업무시간 중간 차한잔 여유 이런건 좋아요",
          "original_form": "그래도 업무시간 중간 차한잔 여유 이런건 좋아요",
