        },
        {
          "id": "MDRW2100035711.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100035711.1.3",
          "form": "헐 아린이도 멍멍이상",
          "original_form": "헐 아린이도 멍멍이상",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100035711.1.4",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ 아니에요?",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ 아니에요?",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100035711.1.5",
          "form": "데뷔초때 진짜",
          "original_form": "데뷔초때 진짜",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100035711.1.6",
          "form": "아기강아지 그자체...",
          "original_form": "아기강아지 그자체...",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100035711.1.7",
          "form": "그쵸그쵸ㅋㅋㅋㅋㅋ 그룹 겹친게 용하지만...",
          "original_form": "그쵸그쵸ㅋㅋㅋㅋㅋ 그룹 겹친게 용하지만...",
          "speaker_id": "2",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100035711.1.8",
          "form": "거의 겸상불가라고 봐야죠",
          "original_form": "거의 겸상불가라고 봐야죠",
          "speaker_id": "2",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100035711.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100035711.1.10",
          "form": "아 겸상불갘ㅋㅋㅋㅋ",
          "original_form": "아 겸상불갘ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100035711.1.11",
          "form": "아니 근데 친구끼린 보통 안겹치더라구요.. ?",
          "original_form": "아니 근데 친구끼린 보통 안겹치더라구요.. ?",
          "speaker_id": "1",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100035711.1.12",
          "form": "물론 저도 모두 좋아합니다만ㅋㅋㅋㅋㅋㅋ",
          "original_form": "물론 저도 모두 좋아합니다만ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100035711.1.13",
          "form": "저는 살면서 단 한번도",
          "original_form": "저는 살면서 단 한번도",
          "speaker_id": "1",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100035711.1.14",
          "form": "그러니까요 신기해요",
          "original_form": "그러니까요 신기해요",
          "speaker_id": "2",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100035711.1.15",
          "form": "같은 그룹을 좋아한적은 있지만",
          "original_form": "같은 그룹을 좋아한적은 있지만",
          "speaker_id": "1",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100035711.1.16",
          "form": "최애가 겹친적은 없어요 ㅋㅋ",
          "original_form": "최애가 겹친적은 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100035711.1.17",
          "form": "어 그러게요 약간 진화의 신비 이런 건가",
          "original_form": "어 그러게요 약간 진화의 신비 이런 건가",
          "speaker_id": "2",
          "time": "20210829 23:11"
        },
        {
          "id": "MDRW2100035711.1.18",
          "form": "친한 친구끼리 취향 겹치명 약간 현실에서 곤란해 질 수도 있으니까요..?",
          "original_form": "친한 친구끼리 취향 겹치명 약간 현실에서 곤란해 질 수도 있으니까요..?",
          "speaker_id": "2",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100035711.1.19",
          "form": "그런건가욬ㅋㅋ 친구끼리 싸우지말라고 ...?",
          "original_form": "그런건가욬ㅋㅋ 친구끼리 싸우지말라고 ...?",
          "speaker_id": "1",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100035711.1.20",
          "form": "근데 겹치면 궁금하긴 해요",
          "original_form": "근데 겹치면 궁금하긴 해요",
          "speaker_id": "1",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100035711.1.21",
          "form": "서로 자기 할말만 할지 .. 공감하면서 덕질할지 ㅋㅋㅋ",
          "original_form": "서로 자기 할말만 할지 .. 공감하면서 덕질할지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100035711.1.22",
          "form": "생각해보니 즁고등학교 때도 그런 적 한번도 없었어요 ㅋㅋㅋㅋ",
          "original_form": "생각해보니 즁고등학교 때도 그런 적 한번도 없었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
