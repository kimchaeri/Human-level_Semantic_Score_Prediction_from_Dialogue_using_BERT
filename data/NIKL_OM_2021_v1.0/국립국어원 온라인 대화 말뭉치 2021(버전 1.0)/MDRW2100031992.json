      "utterance": [
        {
          "id": "MDRW2100031992.1.1",
          "form": "오늘 저녁은 뭐드실거에요?",
          "original_form": "오늘 저녁은 뭐드실거에요?",
          "speaker_id": "1",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031992.1.2",
          "form": "아직 이르지만ㅋㅋㅋㅋㅋ",
          "original_form": "아직 이르지만ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031992.1.3",
          "form": "저는 주말에 맛난거먹는재미로 살아요",
          "original_form": "저는 주말에 맛난거먹는재미로 살아요",
          "speaker_id": "1",
          "time": "20210905 10:31"
        },
        {
          "id": "MDRW2100031992.1.4",
          "form": "저녁은 ㅋㅋㅋ 피자가 먹고 싶어요",
          "original_form": "저녁은 ㅋㅋㅋ 피자가 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031992.1.5",
          "form": "어제 저녁에 다른분과 이야기하는데 완전 피자이야기때문에",
          "original_form": "어제 저녁에 다른분과 이야기하는데 완전 피자이야기때문에",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031992.1.6",
          "form": "관통당했거든요",
          "original_form": "관통당했거든요",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031992.1.7",
          "form": "새벽에 피자이야기... ㅎㅎ",
          "original_form": "새벽에 피자이야기... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031992.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 10:35"
        },
        {
          "id": "MDRW2100031992.1.9",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ아 어떤 피자 드세요?",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ아 어떤 피자 드세요?",
          "speaker_id": "1",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031992.1.10",
          "form": "저도 비슷한거 먹으러가요",
          "original_form": "저도 비슷한거 먹으러가요",
          "speaker_id": "1",
          "time": "20210905 10:36"
        },
        {
          "id": "MDRW2100031992.1.11",
          "form": "수제버거요 ㅋㅋㅋㅋㅋ",
          "original_form": "수제버거요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031992.1.12",
          "form": "몸에 좋은거 먹어야되는데 이런게 맛있어요.....ㅋㅋㅋ",
          "original_form": "몸에 좋은거 먹어야되는데 이런게 맛있어요.....ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031992.1.13",
          "form": "ㅋㅋㅋ 그런거 먹으려고 돈벌고 사는걸요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 그런거 먹으려고 돈벌고 사는걸요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100031992.1.14",
          "form": "수제버거! 맛있겠다~",
          "original_form": "수제버거! 맛있겠다~",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031992.1.15",
          "form": "피자는 따로 가리는게 없어서",
          "original_form": "피자는 따로 가리는게 없어서",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031992.1.16",
          "form": "가족들이 좋아하는걸 먹을것 같아요",
          "original_form": "가족들이 좋아하는걸 먹을것 같아요",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031992.1.17",
          "form": "누구랑 먹으러 가시나요?",
          "original_form": "누구랑 먹으러 가시나요?",
          "speaker_id": "2",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031992.1.18",
          "form": "친구랑요 ㅎㅎ",
          "original_form": "친구랑요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:38"
        },
        {
          "id": "MDRW2100031992.1.19",
          "form": "피자 브랜드도 무지 많잖아요",
          "original_form": "피자 브랜드도 무지 많잖아요",
          "speaker_id": "1",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031992.1.20",
          "form": "저는 서오릉피자 궁금하더라구요",
          "original_form": "저는 서오릉피자 궁금하더라구요",
          "speaker_id": "1",
          "time": "20210905 10:39"
        },
        {
          "id": "MDRW2100031992.1.21",
          "form": "친구랑~ 좋겠어요 ㅠㅠ",
          "original_form": "친구랑~ 좋겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031992.1.22",
          "form": "서오릉 피자는 저도 먹어보지 못했어요",
          "original_form": "서오릉 피자는 저도 먹어보지 못했어요",
          "speaker_id": "2",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031992.1.23",
          "form": "알볼로가 젤 좋아요 저는",
          "original_form": "알볼로가 젤 좋아요 저는",
          "speaker_id": "1",
          "time": "20210905 10:42"
        },
        {
          "id": "MDRW2100031992.1.24",
          "form": "어제 알볼로 이야기가 나왔었는데",
          "original_form": "어제 알볼로 이야기가 나왔었는데",
          "speaker_id": "2",
          "time": "20210905 10:42"
        },
        {
          "id": "MDRW2100031992.1.25",
          "form": "그.. 어깨피자?",
          "original_form": "그.. 어깨피자?",
          "speaker_id": "2",
          "time": "20210905 10:42"
        },
        {
          "id": "MDRW2100031992.1.26",
          "form": "그렇게 맛있다던데 그런가요?",
          "original_form": "그렇게 맛있다던데 그런가요?",
          "speaker_id": "2",
          "time": "20210905 10:42"
        },
        {
          "id": "MDRW2100031992.1.27",
          "form": "먹어본것중 토핑이 제일 풍부해서 ㅎㅎ",
          "original_form": "먹어본것중 토핑이 제일 풍부해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:43"
        },
        {
          "id": "MDRW2100031992.1.28",
          "form": "저는 꿈을피자도 맛나게 먹었어요",
          "original_form": "저는 꿈을피자도 맛나게 먹었어요",
          "speaker_id": "1",
          "time": "20210905 10:43"
        },
        {
          "id": "MDRW2100031992.1.29",
          "form": "진ㅉㅑ 맛있어요",
          "original_form": "진ㅉㅑ 맛있어요",
          "speaker_id": "1",
          "time": "20210905 10:43"
        },
        {
          "id": "MDRW2100031992.1.30",
          "form": "피자 좋아하시는군요!",
          "original_form": "피자 좋아하시는군요!",
          "speaker_id": "2",
          "time": "20210905 10:44"
        },
        {
          "id": "MDRW2100031992.1.31",
          "form": "브랜드별로 다양하게 먹어보지는 못했거든요 ㅠ",
          "original_form": "브랜드별로 다양하게 먹어보지는 못했거든요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 10:44"
        },
        {
          "id": "MDRW2100031992.1.32",
          "form": "ㅋㅋㅋ자주는 안먹는데 먹으면 알볼로 먹어요 :)",
          "original_form": "ㅋㅋㅋ자주는 안먹는데 먹으면 알볼로 먹어요 :)",
          "speaker_id": "1",
          "time": "20210905 10:44"
        },
        {
          "id": "MDRW2100031992.1.33",
          "form": "오.. 그정도군요! 진짜.. 주변에 매장 있나.. ㅠ",
          "original_form": "오.. 그정도군요! 진짜.. 주변에 매장 있나.. ㅠ",
          "speaker_id": "2",
          "time": "20210905 10:44"
        },
        {
          "id": "MDRW2100031992.1.34",
          "form": "있음 꼭 시켜먹어야겠어요 ㅠㅠ",
          "original_form": "있음 꼭 시켜먹어야겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 10:44"
        },
