      "utterance": [
        {
          "id": "MDRW2100012137.1.1",
          "form": "사실 최근 군대랑 관련해서는 안좋은 일이 많아서..",
          "original_form": "사실 최근 군대랑 관련해서는 안좋은 일이 많아서..",
          "speaker_id": "2",
          "time": "20210825 09:57"
        },
        {
          "id": "MDRW2100012137.1.2",
          "form": "ㅇㅇ이번에 또 났더라 미투",
          "original_form": "ㅇㅇ이번에 또 났더라 미투",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100012137.1.3",
          "form": "음 우리나라도 미국처럼 바뀌는건어떻게생각함?",
          "original_form": "음 우리나라도 미국처럼 바뀌는건어떻게생각함?",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100012137.1.4",
          "form": "근데 미국도 아예 없는건아니래",
          "original_form": "근데 미국도 아예 없는건아니래",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100012137.1.5",
          "form": "오 진짜요?",
          "original_form": "오 진짜요?",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100012137.1.6",
          "form": "어떤경우?",
          "original_form": "어떤경우?",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100012137.1.7",
          "form": "남녀 군인간의 성추행건이 나온댕 쩔수없나봐",
          "original_form": "남녀 군인간의 성추행건이 나온댕 쩔수없나봐",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100012137.1.8",
          "form": "이래서 여자들 왜 군대 안가냐 하는건 의미 없는듯",
          "original_form": "이래서 여자들 왜 군대 안가냐 하는건 의미 없는듯",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100012137.1.9",
          "form": "갈수있는 환경이안돼",
          "original_form": "갈수있는 환경이안돼",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100012137.1.10",
          "form": "웅 많이 안알려지니까 더 인식 개선이 안됌ㅜ",
          "original_form": "웅 많이 안알려지니까 더 인식 개선이 안됌ㅜ",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100012137.1.11",
          "form": "군대라는 조직자체가 폐쇄적일수밖에 없나봄",
          "original_form": "군대라는 조직자체가 폐쇄적일수밖에 없나봄",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100012137.1.12",
          "form": "그러고보니 의문사도 많음",
          "original_form": "그러고보니 의문사도 많음",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100012137.1.13",
          "form": "그래도 나은 방향을 위해 꾸준히 개선해나가야하는데 의욕없어보임ㅜ",
          "original_form": "그래도 나은 방향을 위해 꾸준히 개선해나가야하는데 의욕없어보임ㅜ",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100012137.1.14",
          "form": "마자 의문사",
          "original_form": "마자 의문사",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100012137.1.15",
          "form": "와 그거 인정 뭔가 그쪽 장군분들이 현상 유지 원하는듯함",
          "original_form": "와 그거 인정 뭔가 그쪽 장군분들이 현상 유지 원하는듯함",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100012137.1.16",
          "form": "내 어릴때 윗층오빠 군대가서 의문사당함",
          "original_form": "내 어릴때 윗층오빠 군대가서 의문사당함",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100012137.1.17",
          "form": "앗 나도 동네 슈퍼집 아들",
          "original_form": "앗 나도 동네 슈퍼집 아들",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100012137.1.18",
          "form": "홀",
          "original_form": "홀",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100012137.1.19",
          "form": "많네 주위에",
          "original_form": "많네 주위에",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100012137.1.20",
          "form": "은근 많아서 더 소름돋아",
          "original_form": "은근 많아서 더 소름돋아",
