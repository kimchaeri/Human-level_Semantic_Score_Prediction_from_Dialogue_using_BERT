      "utterance": [
        {
          "id": "MDRW2100035412.1.1",
          "form": "생각하는거 맞음 ㅋ",
          "original_form": "생각하는거 맞음 ㅋ",
          "speaker_id": "2",
          "time": "20210829 16:31"
        },
        {
          "id": "MDRW2100035412.1.2",
          "form": "아니였네 ㅋㅋ 실제네 ㅋㅋ",
          "original_form": "아니였네 ㅋㅋ 실제네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:31"
        },
        {
          "id": "MDRW2100035412.1.3",
          "form": "식탁춤을 여기서 안추네",
          "original_form": "식탁춤을 여기서 안추네",
          "speaker_id": "1",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100035412.1.4",
          "form": "그럼 나의 기억은 뭐지 ㅋㅋ",
          "original_form": "그럼 나의 기억은 뭐지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100035412.1.5",
          "form": "ㅋㅋㅋ 글쎄",
          "original_form": "ㅋㅋㅋ 글쎄",
          "speaker_id": "1",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100035412.1.6",
          "form": "에궁 뗄감으로 쓰네",
          "original_form": "에궁 뗄감으로 쓰네",
          "speaker_id": "2",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100035412.1.7",
          "form": "안그래도 가난한데 파업한다고 일을 안하니 더 가난",
          "original_form": "안그래도 가난한데 파업한다고 일을 안하니 더 가난",
          "speaker_id": "2",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100035412.1.8",
          "form": "저래 뿌시고 크리스마스는 한다",
          "original_form": "저래 뿌시고 크리스마스는 한다",
          "speaker_id": "1",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100035412.1.9",
          "form": "크리스마스는 저기 저사라들한테 명절이니",
          "original_form": "크리스마스는 저기 저사라들한테 명절이니",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100035412.1.10",
          "form": "그러네.  암만 난리 부르스를 쳐도",
          "original_form": "그러네.  암만 난리 부르스를 쳐도",
          "speaker_id": "1",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100035412.1.11",
          "form": "저 남자애가 동 성 애 자다",
          "original_form": "저 남자애가 동 성 애 자다",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100035412.1.12",
          "form": "눈빛이 틀리네",
          "original_form": "눈빛이 틀리네",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100035412.1.13",
          "form": "ㅋㅋ 빌리를 좋아한다",
          "original_form": "ㅋㅋ 빌리를 좋아한다",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100035412.1.14",
          "form": "그런거 같네",
          "original_form": "그런거 같네",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100035412.1.15",
          "form": "ㅋㅋ 여자 발레복 준다",
          "original_form": "ㅋㅋ 여자 발레복 준다",
