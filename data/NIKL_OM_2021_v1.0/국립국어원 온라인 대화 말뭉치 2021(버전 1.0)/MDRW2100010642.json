      "utterance": [
        {
          "id": "MDRW2100010642.1.1",
          "form": "오 이런 서비스가 있다고 뉴스같은 곳에서 들은것같긴해요 ㅋ",
          "original_form": "오 이런 서비스가 있다고 뉴스같은 곳에서 들은것같긴해요 ㅋ",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010642.1.2",
          "form": "홈트같은건가요?",
          "original_form": "홈트같은건가요?",
          "speaker_id": "2",
          "time": "20210823 20:48"
        },
        {
          "id": "MDRW2100010642.1.3",
          "form": "한번 검색 가볍게 해보고 오겠어요ㅋㅋ",
          "original_form": "한번 검색 가볍게 해보고 오겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:49"
        },
        {
          "id": "MDRW2100010642.1.4",
          "form": "어떤 서비스인건지",
          "original_form": "어떤 서비스인건지",
          "speaker_id": "1",
          "time": "20210823 20:50"
        },
        {
          "id": "MDRW2100010642.1.5",
          "form": "넵 저도 알려주세ㅇᆞ",
          "original_form": "넵 저도 알려주세ㅇᆞ",
          "speaker_id": "2",
          "time": "20210823 20:58"
        },
        {
          "id": "MDRW2100010642.1.6",
          "form": "음 비대면 피티인가봐요",
          "original_form": "음 비대면 피티인가봐요",
          "speaker_id": "1",
          "time": "20210823 21:19"
        },
        {
          "id": "MDRW2100010642.1.7",
          "form": "아ㅎㅎ그런것도 요금은 똑같이 받나봐요?",
          "original_form": "아ㅎㅎ그런것도 요금은 똑같이 받나봐요?",
          "speaker_id": "2",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100010642.1.8",
          "form": "그러게요 가격은 왜 안나와있죠 ㅋㅋ",
          "original_form": "그러게요 가격은 왜 안나와있죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:02"
        },
        {
          "id": "MDRW2100010642.1.9",
          "form": "아직 좀 대중화 전 같아요",
          "original_form": "아직 좀 대중화 전 같아요",
          "speaker_id": "1",
          "time": "20210823 22:02"
        },
        {
          "id": "MDRW2100010642.1.10",
          "form": "비대면 피티면 자세잡아주는것도 힘들꺼같아ㅇᆞ",
          "original_form": "비대면 피티면 자세잡아주는것도 힘들꺼같아ㅇᆞ",
          "speaker_id": "2",
          "time": "20210823 22:03"
        },
        {
          "id": "MDRW2100010642.1.11",
          "form": "뭔가 영상통화로 피티받는 그정도 일거같아요 ㅋㄱ",
          "original_form": "뭔가 영상통화로 피티받는 그정도 일거같아요 ㅋㄱ",
          "speaker_id": "1",
          "time": "20210823 22:04"
        },
        {
          "id": "MDRW2100010642.1.12",
          "form": "말만 거창한거같기도",
          "original_form": "말만 거창한거같기도",
          "speaker_id": "1",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010642.1.13",
          "form": "아직 갈길이 머네ㅇᆢㅎ",
          "original_form": "아직 갈길이 머네ㅇᆢㅎ",
          "speaker_id": "2",
          "time": "20210823 22:05"
        },
        {
          "id": "MDRW2100010642.1.14",
          "form": "근데 그거하난좋을거같아요",
          "original_form": "근데 그거하난좋을거같아요",
          "speaker_id": "1",
          "time": "20210823 22:06"
        },
        {
          "id": "MDRW2100010642.1.15",
          "form": "마스크 안껴도되는거?",
          "original_form": "마스크 안껴도되는거?",
          "speaker_id": "1",
          "time": "20210823 22:06"
        },
        {
          "id": "MDRW2100010642.1.16",
          "form": "그거 하나만요ㅋ",
          "original_form": "그거 하나만요ㅋ",
          "speaker_id": "2",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010642.1.17",
          "form": "가격이 진짜 싸다면 또 좀 고민해볼거같기도요 ㅋㅋ",
          "original_form": "가격이 진짜 싸다면 또 좀 고민해볼거같기도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010642.1.18",
          "form": "온라인이면 시간대도 맞추긴 편하겟어요ㅎ",
          "original_form": "온라인이면 시간대도 맞추긴 편하겟어요ㅎ",
