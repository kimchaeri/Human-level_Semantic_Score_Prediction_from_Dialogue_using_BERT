      "utterance": [
        {
          "id": "MDRW2100014579.1.1",
          "form": "안뇽하세요",
          "original_form": "안뇽하세요",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014579.1.2",
          "form": "게임 좋아하시나요",
          "original_form": "게임 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014579.1.3",
          "form": "네 좋아해요 뭐하세요??",
          "original_form": "네 좋아해요 뭐하세요??",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014579.1.4",
          "form": "저는 이것저것 많이 하긴하는데",
          "original_form": "저는 이것저것 많이 하긴하는데",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014579.1.5",
          "form": "요즘엔 롤 많이해요",
          "original_form": "요즘엔 롤 많이해요",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014579.1.6",
          "form": "아 전 롤은 안해봤고",
          "original_form": "아 전 롤은 안해봤고",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014579.1.7",
          "form": "폰겜은 쿠키런하고",
          "original_form": "폰겜은 쿠키런하고",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014579.1.8",
          "form": "피씨는 오버워치랑 배그해봤네요",
          "original_form": "피씨는 오버워치랑 배그해봤네요",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014579.1.9",
          "form": "쿠키런 킹덤하시는건가요??",
          "original_form": "쿠키런 킹덤하시는건가요??",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014579.1.10",
          "form": "전 배그는 안해봣지만 오버워치는 많이했었어요 ㅋㅋ",
          "original_form": "전 배그는 안해봣지만 오버워치는 많이했었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014579.1.11",
          "form": "네 킹덤이요",
          "original_form": "네 킹덤이요",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014579.1.12",
          "form": "요즘 인기많더라고요",
          "original_form": "요즘 인기많더라고요",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014579.1.13",
          "form": "옵치는 쉬다가 다시하고있어요",
          "original_form": "옵치는 쉬다가 다시하고있어요",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014579.1.14",
          "form": "주로 무슨 캐릭터 하시나요?",
          "original_form": "주로 무슨 캐릭터 하시나요?",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014579.1.15",
          "form": "라인이나 자리야 메이  아님 루시우 토르비욘 그냥 많이 해요",
          "original_form": "라인이나 자리야 메이  아님 루시우 토르비욘 그냥 많이 해요",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014579.1.16",
          "form": "신캐는 잘모르겠더라고요",
          "original_form": "신캐는 잘모르겠더라고요",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014579.1.17",
          "form": "저도 루시우랑 디바 많이 했어요",
          "original_form": "저도 루시우랑 디바 많이 했어요",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014579.1.18",
          "form": "신캐는 점점 많이 생겨나는데",
          "original_form": "신캐는 점점 많이 생겨나는데",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014579.1.19",
          "form": "아 디바도 많이 했어요",
          "original_form": "아 디바도 많이 했어요",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014579.1.20",
          "form": "제가 적응할 시간을 안주더라고요",
          "original_form": "제가 적응할 시간을 안주더라고요",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014579.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014579.1.22",
          "form": "맞아요 ㅋㅋ근데 왜 요즘은 인기떨어졌을까요??",
          "original_form": "맞아요 ㅋㅋ근데 왜 요즘은 인기떨어졌을까요??",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014579.1.23",
          "form": "몇년전엔 진짜 많이했었는데",
          "original_form": "몇년전엔 진짜 많이했었는데",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014579.1.24",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014579.1.25",
          "form": "배그가 나오면서 거품이 빠진것 같기도해요",
          "original_form": "배그가 나오면서 거품이 빠진것 같기도해요",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014579.1.26",
          "form": "롤은 스테디같긴해요",
          "original_form": "롤은 스테디같긴해요",
          "speaker_id": "2",
          "time": "20210825 20:28"
        }
      ]
    }
  ]
