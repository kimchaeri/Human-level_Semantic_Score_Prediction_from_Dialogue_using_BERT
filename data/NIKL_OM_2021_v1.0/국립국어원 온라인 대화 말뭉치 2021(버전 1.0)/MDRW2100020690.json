      "utterance": [
        {
          "id": "MDRW2100020690.1.1",
          "form": "후하핫",
          "original_form": "후하핫",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020690.1.2",
          "form": "뭐 갖고 싶은 거 있으세요?",
          "original_form": "뭐 갖고 싶은 거 있으세요?",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020690.1.3",
          "form": "오 눈치를 채신건가요 ? 저 반지 사고 싶어요",
          "original_form": "오 눈치를 채신건가요 ? 저 반지 사고 싶어요",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020690.1.4",
          "form": "그래서 어제 새벽에 급발진으로 호수 재고 왔어요",
          "original_form": "그래서 어제 새벽에 급발진으로 호수 재고 왔어요",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020690.1.5",
          "form": "멋지죠 제 행동력",
          "original_form": "멋지죠 제 행동력",
          "speaker_id": "1",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020690.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020690.1.7",
          "form": "무슨 반지요",
          "original_form": "무슨 반지요",
          "speaker_id": "2",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020690.1.8",
          "form": "실반지요",
          "original_form": "실반지요",
          "speaker_id": "1",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020690.1.9",
          "form": "금은방가서 쟀어요?",
          "original_form": "금은방가서 쟀어요?",
          "speaker_id": "2",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020690.1.10",
          "form": "실반지 노간지.",
          "original_form": "실반지 노간지.",
          "speaker_id": "2",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020690.1.11",
          "form": "아니요 집에서 쟀는데요 ?",
          "original_form": "아니요 집에서 쟀는데요 ?",
          "speaker_id": "1",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020690.1.12",
          "form": "테이프 돌돌말아서",
          "original_form": "테이프 돌돌말아서",
          "speaker_id": "1",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020690.1.13",
          "form": "재고 왔다고 하셔서 직접 갔다오신줄 알앗네요",
          "original_form": "재고 왔다고 하셔서 직접 갔다오신줄 알앗네요",
          "speaker_id": "2",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020690.1.14",
          "form": "반지 어디서 사시게요?",
          "original_form": "반지 어디서 사시게요?",
          "speaker_id": "2",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020690.1.15",
          "form": "인털넷이요",
          "original_form": "인털넷이요",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020690.1.16",
          "form": "어제 열심히 봤거든요",
          "original_form": "어제 열심히 봤거든요",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020690.1.17",
          "form": "저근데 반지 호수가 생각보다 작네요 8호래요",
          "original_form": "저근데 반지 호수가 생각보다 작네요 8호래요",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020690.1.18",
          "form": "아닌가 큰건가요",
          "original_form": "아닌가 큰건가요",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020690.1.19",
          "form": "약지요?",
          "original_form": "약지요?",
          "speaker_id": "2",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020690.1.20",
          "form": "중지요",
          "original_form": "중지요",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020690.1.21",
          "form": "전 반지 중지에다 끼거든요",
          "original_form": "전 반지 중지에다 끼거든요",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020690.1.22",
          "form": "작은데요",
          "original_form": "작은데요",
          "speaker_id": "2",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020690.1.23",
          "form": "그쵸 작은거 맞죠 ?",
          "original_form": "그쵸 작은거 맞죠 ?",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020690.1.24",
          "form": "님 손가락 원랴 얇았나요",
          "original_form": "님 손가락 원랴 얇았나요",
          "speaker_id": "2",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020690.1.25",
          "form": "님도 반지 끼세요 ?",
          "original_form": "님도 반지 끼세요 ?",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020690.1.26",
          "form": "네 저 얇은편이긴 해요",
          "original_form": "네 저 얇은편이긴 해요",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100020690.1.27",
          "form": "반지 자주 끼죠",
          "original_form": "반지 자주 끼죠",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020690.1.28",
          "form": "전 검지랑 중지에 맞는 반지가 많아요",
          "original_form": "전 검지랑 중지에 맞는 반지가 많아요",
          "speaker_id": "2",
          "time": "20210828 11:18"
        }
      ]
    }
  ]
