      "utterance": [
        {
          "id": "MDRW2100014751.1.1",
          "form": "오늘 강아지들 산책 갔어용?",
          "original_form": "오늘 강아지들 산책 갔어용?",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014751.1.2",
          "form": "오늘 강아지들 산책갔다 왔나ㅕ",
          "original_form": "오늘 강아지들 산책갔다 왔나ㅕ",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014751.1.3",
          "form": "오늘 강아지들 산책 갔다 왔낭",
          "original_form": "오늘 강아지들 산책 갔다 왔낭",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014751.1.4",
          "form": "오늘 비가 와서 못갔지",
          "original_form": "오늘 비가 와서 못갔지",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014751.1.5",
          "form": "그랬구나 ㅠㅠ 애들 가고싶어하겠다",
          "original_form": "그랬구나 ㅠㅠ 애들 가고싶어하겠다",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014751.1.6",
          "form": "아니다 다이소 갔다가 왔네",
          "original_form": "아니다 다이소 갔다가 왔네",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014751.1.7",
          "form": "강아지들 델꼬?",
          "original_form": "강아지들 델꼬?",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014751.1.8",
          "form": "두 마리 다 데리고",
          "original_form": "두 마리 다 데리고",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014751.1.9",
          "form": "비가 주룩주룩 내렸는데",
          "original_form": "비가 주룩주룩 내렸는데",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014751.1.10",
          "form": "그랬구만",
          "original_form": "그랬구만",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014751.1.11",
          "form": "그래도 갔다가 오니까",
          "original_form": "그래도 갔다가 오니까",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014751.1.12",
          "form": "운동도 하고 좋다",
          "original_form": "운동도 하고 좋다",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014751.1.13",
          "form": "글쿤 좋넴ㅋㅋ",
          "original_form": "글쿤 좋넴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014751.1.14",
          "form": "나두 가야겠다",
          "original_form": "나두 가야겠다",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014751.1.15",
          "form": "온유도 데리고 갔다",
          "original_form": "온유도 데리고 갔다",
          "speaker_id": "2",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014751.1.16",
          "form": "좋아했겠군..! 비는 안맞았어용?",
          "original_form": "좋아했겠군..! 비는 안맞았어용?",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014751.1.17",
          "form": "치유 도 갔다",
          "original_form": "치유 도 갔다",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014751.1.18",
