      "utterance": [
        {
          "id": "MDRW2100022315.1.1",
          "form": "순살 시키는게 평화를 지키는 일이에요..",
          "original_form": "순살 시키는게 평화를 지키는 일이에요..",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022315.1.2",
          "form": "혹시 치킨 무슨 소스 찍어 드세요?",
          "original_form": "혹시 치킨 무슨 소스 찍어 드세요?",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022315.1.3",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022315.1.4",
          "form": "전 후라이드 그냥 먹어요",
          "original_form": "전 후라이드 그냥 먹어요",
          "speaker_id": "1",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022315.1.5",
          "form": "아 후라이드만 드세요?",
          "original_form": "아 후라이드만 드세요?",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022315.1.6",
          "form": "뭐 안찍고 후라이드 자체맛이 좋아서",
          "original_form": "뭐 안찍고 후라이드 자체맛이 좋아서",
          "speaker_id": "1",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022315.1.7",
          "form": "후라이드는 어디 브랜드가 잘하나요",
          "original_form": "후라이드는 어디 브랜드가 잘하나요",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022315.1.8",
          "form": "자담치킨 후라이드 맛있어요",
          "original_form": "자담치킨 후라이드 맛있어요",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022315.1.9",
          "form": "아! 자담 치킨은 안먹어봤네요 ㅠㅠ",
          "original_form": "아! 자담 치킨은 안먹어봤네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022315.1.10",
          "form": "비비큐나 비에이치씨도 좋고",
          "original_form": "비비큐나 비에이치씨도 좋고",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022315.1.11",
          "form": "역시 비비큐 나올 줄 알았어요 ㅎㅎ",
          "original_form": "역시 비비큐 나올 줄 알았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022315.1.12",
          "form": "거긴 약간 크리스피한 튀김이라",
          "original_form": "거긴 약간 크리스피한 튀김이라",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022315.1.13",
          "form": "튀김옷 있는게 좋아요 업는게 좋아요?",
          "original_form": "튀김옷 있는게 좋아요 업는게 좋아요?",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022315.1.14",
          "form": "매력있더라구요",
          "original_form": "매력있더라구요",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022315.1.15",
          "form": "있는게 좋긴 한데 .. 음",
          "original_form": "있는게 좋긴 한데 .. 음",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022315.1.16",
          "form": "후참잘도 크리스피한 스탈이에요 ㅎㅎ",
          "original_form": "후참잘도 크리스피한 스탈이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022315.1.17",
          "form": "너무 두꺼운건 싫어요",
          "original_form": "너무 두꺼운건 싫어요",
