      "utterance": [
        {
          "id": "MDRW2100011940.1.1",
          "form": "온라인 pt 어떤것 같아?",
          "original_form": "온라인 pt 어떤것 같아?",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011940.1.2",
          "form": "음....생각해본적이없는데",
          "original_form": "음....생각해본적이없는데",
          "speaker_id": "2",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011940.1.3",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011940.1.4",
          "form": "근데 나는",
          "original_form": "근데 나는",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011940.1.5",
          "form": "줌같은거로 가르쳐쥬는건가?",
          "original_form": "줌같은거로 가르쳐쥬는건가?",
          "speaker_id": "2",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011940.1.6",
          "form": "아마 그럴껄",
          "original_form": "아마 그럴껄",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011940.1.7",
          "form": "카톡 같은걸로",
          "original_form": "카톡 같은걸로",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011940.1.8",
          "form": "식단 짜주고",
          "original_form": "식단 짜주고",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011940.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011940.1.10",
          "form": "운동 루틴 짜줄것 같은데",
          "original_form": "운동 루틴 짜줄것 같은데",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100011940.1.11",
          "form": "식단이나 루틴짜주고 이런건 괜찮은거같은데",
          "original_form": "식단이나 루틴짜주고 이런건 괜찮은거같은데",
          "speaker_id": "2",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011940.1.12",
          "form": "운동은 자제 조금만 잘못해도",
          "original_form": "운동은 자제 조금만 잘못해도",
          "speaker_id": "1",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011940.1.13",
          "form": "난 피티 장점이",
          "original_form": "난 피티 장점이",
          "speaker_id": "2",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011940.1.14",
          "form": "아픈곳이 많을것 같아서",
          "original_form": "아픈곳이 많을것 같아서",
          "speaker_id": "1",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011940.1.15",
          "form": "일대일로 자세 잡아주고 이런거라 생각해서",
          "original_form": "일대일로 자세 잡아주고 이런거라 생각해서",
          "speaker_id": "2",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011940.1.16",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011940.1.17",
          "form": "장점이?",
          "original_form": "장점이?",
          "speaker_id": "1",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011940.1.18",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011940.1.19",
          "form": "온라인으로 하면 효과가 좀 덜할거같은디",
          "original_form": "온라인으로 하면 효과가 좀 덜할거같은디",
          "speaker_id": "2",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011940.1.20",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011940.1.21",
          "form": "그리고 그냥 틀어놓으면",
          "original_form": "그리고 그냥 틀어놓으면",
          "speaker_id": "1",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100011940.1.22",
          "form": "좀 무의미하니까",
          "original_form": "좀 무의미하니까",
          "speaker_id": "1",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100011940.1.23",
          "form": "근데 아이디어는 좋다",
          "original_form": "근데 아이디어는 좋다",
          "speaker_id": "1",
          "time": "20210824 17:39"
        }
      ]
