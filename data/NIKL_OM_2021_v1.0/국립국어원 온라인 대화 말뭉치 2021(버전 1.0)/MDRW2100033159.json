      "utterance": [
        {
          "id": "MDRW2100033159.1.1",
          "form": "오늘저녁메뉴먼가요?",
          "original_form": "오늘저녁메뉴먼가요?",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100033159.1.2",
          "form": "치킨너겟에 흰쌀밥 먹을려고 합니다!",
          "original_form": "치킨너겟에 흰쌀밥 먹을려고 합니다!",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100033159.1.3",
          "form": "치킨너겟!!!",
          "original_form": "치킨너겟!!!",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100033159.1.4",
          "form": "저는 홈플치킨먹을 예정이에요",
          "original_form": "저는 홈플치킨먹을 예정이에요",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100033159.1.5",
          "form": "소스는KFC꺼요",
          "original_form": "소스는KFC꺼요",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100033159.1.6",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033159.1.7",
          "form": "맛있겠네요!",
          "original_form": "맛있겠네요!",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033159.1.8",
          "form": "너무 배고파요 빨리먹고싶어요",
          "original_form": "너무 배고파요 빨리먹고싶어요",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100033159.1.9",
          "form": "치킨너겟도맛있어요",
          "original_form": "치킨너겟도맛있어요",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100033159.1.10",
          "form": "얼른 드세요!",
          "original_form": "얼른 드세요!",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100033159.1.11",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100033159.1.12",
          "form": "그중에서 저는 용가리치킨먹어봤나요?",
          "original_form": "그중에서 저는 용가리치킨먹어봤나요?",
          "speaker_id": "1",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100033159.1.13",
          "form": "질문이이상하네ㅇᆢ",
          "original_form": "질문이이상하네ㅇᆢ",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100033159.1.14",
          "form": "저는 용가리너겟",
          "original_form": "저는 용가리너겟",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100033159.1.15",
          "form": "먹어봤어요",
          "original_form": "먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100033159.1.16",
          "form": "배고파서 말이 헛나오네요",
          "original_form": "배고파서 말이 헛나오네요",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100033159.1.17",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100033159.1.18",
          "form": "ㅋㅋㅋㅋㅋ용가리치킨도 먹어봤져",
          "original_form": "ㅋㅋㅋㅋㅋ용가리치킨도 먹어봤져",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100033159.1.19",
          "form": "맛있어요!ㅠㅠ",
          "original_form": "맛있어요!ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100033159.1.20",
          "form": "용가리치킨 너겟 맞죠?",
          "original_form": "용가리치킨 너겟 맞죠?",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100033159.1.21",
          "form": "그거 되게 유명한거자나요",
          "original_form": "그거 되게 유명한거자나요",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100033159.1.22",
          "form": "너겟얘기하길래",
          "original_form": "너겟얘기하길래",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100033159.1.23",
          "form": "용가리가생각이나서",
          "original_form": "용가리가생각이나서",
          "speaker_id": "1",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100033159.1.24",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100033159.1.25",
          "form": "용가리도 참 맛있는데 요세 주변에 안팔아서..",
          "original_form": "용가리도 참 맛있는데 요세 주변에 안팔아서..",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100033159.1.26",
          "form": "인터넷에팔아요",
          "original_form": "인터넷에팔아요",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100033159.1.27",
          "form": "저는 그걸로 먹었어요",
          "original_form": "저는 그걸로 먹었어요",
          "speaker_id": "1",
          "time": "20210906 18:26"
        },
        {
          "id": "MDRW2100033159.1.28",
          "form": "오.. 진짜요?!!",
          "original_form": "오.. 진짜요?!!",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033159.1.29",
          "form": "11번가 켜야겠어요..ㅋㅋㅋㅋ",
          "original_form": "11번가 켜야겠어요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033159.1.30",
          "form": "네 있어요ㅋㅋㅋ",
          "original_form": "네 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100033159.1.31",
          "form": "온라인주문이 대세자나요",
          "original_form": "온라인주문이 대세자나요",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100033159.1.32",
          "form": "없는게없어요",
          "original_form": "없는게없어요",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100033159.1.33",
