      "utterance": [
        {
          "id": "MDRW2100021629.1.1",
          "form": "재택근무 해봤어요?",
          "original_form": "재택근무 해봤어요?",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021629.1.2",
          "form": "아니요 ㅜㅜ 하고계신가요?",
          "original_form": "아니요 ㅜㅜ 하고계신가요?",
          "speaker_id": "2",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021629.1.3",
          "form": "지금은 안하고 있는데 해본 적은 있어요 ㅎ",
          "original_form": "지금은 안하고 있는데 해본 적은 있어요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021629.1.4",
          "form": "오 어떤가요?? 전 해보고 싶어요 ㅠ",
          "original_form": "오 어떤가요?? 전 해보고 싶어요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021629.1.5",
          "form": "해보니까 장점도 있는데 단점도 있더라구요 ㅜㅜ",
          "original_form": "해보니까 장점도 있는데 단점도 있더라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021629.1.6",
          "form": "단점은 어떤건가요 ㅠㅠ ?",
          "original_form": "단점은 어떤건가요 ㅠㅠ ?",
          "speaker_id": "2",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021629.1.7",
          "form": "일단 팀원들간 소통이 좀 잘안돼요",
          "original_form": "일단 팀원들간 소통이 좀 잘안돼요",
          "speaker_id": "1",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100021629.1.8",
          "form": "그래서 일처리가 느린거..?",
          "original_form": "그래서 일처리가 느린거..?",
          "speaker_id": "1",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100021629.1.9",
          "form": "아아 그렇겠어요 ㅠㅠ",
          "original_form": "아아 그렇겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100021629.1.10",
          "form": "막 회의도 화상회의로 해야되죠?",
          "original_form": "막 회의도 화상회의로 해야되죠?",
          "speaker_id": "2",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021629.1.11",
          "form": "헉 ㅋㅋㅋ 맞아요 줌이라고 아시나요....",
          "original_form": "헉 ㅋㅋㅋ 맞아요 줌이라고 아시나요....",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021629.1.12",
          "form": "네네 알아요 ! 요즘 많이 쓰던데ㅜㅜ",
          "original_form": "네네 알아요 ! 요즘 많이 쓰던데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021629.1.13",
          "form": "줌 중국꺼라면서요 ㅠㅠ 쓰기 깨름칙해요",
          "original_form": "줌 중국꺼라면서요 ㅠㅠ 쓰기 깨름칙해요",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021629.1.14",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021629.1.15",
          "form": "얼굴 다 털릴거같은 느낌인데요 ㅠㅠ",
          "original_form": "얼굴 다 털릴거같은 느낌인데요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100021629.1.16",
          "form": "그래서 줌 할 때마다 마스크 끼고 있었는데",
          "original_form": "그래서 줌 할 때마다 마스크 끼고 있었는데",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021629.1.17",
          "form": "사람들이 왜 마스크 끼고 있냐고 벗으라고 하네요 ㅠㅠ",
          "original_form": "사람들이 왜 마스크 끼고 있냐고 벗으라고 하네요 ㅠㅠ",
