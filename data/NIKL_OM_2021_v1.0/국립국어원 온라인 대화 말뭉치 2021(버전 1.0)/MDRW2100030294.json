        },
        {
          "id": "MDRW2100030294.1.2",
          "form": "식음료?",
          "original_form": "식음료?",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030294.1.3",
          "form": "저녁 먹었니?",
          "original_form": "저녁 먹었니?",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030294.1.4",
          "form": "응응 방금 먹었어",
          "original_form": "응응 방금 먹었어",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030294.1.5",
          "form": "간장게장 먹었어!",
          "original_form": "간장게장 먹었어!",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030294.1.6",
          "form": "완전 맛있음",
          "original_form": "완전 맛있음",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030294.1.7",
          "form": "와 난 게장 ...먹은지 오래되서",
          "original_form": "와 난 게장 ...먹은지 오래되서",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030294.1.8",
          "form": "뭔 맛인지 모르겠어",
          "original_form": "뭔 맛인지 모르겠어",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030294.1.9",
          "form": "비린 맛이지ㅋㅋㅋㅋ",
          "original_form": "비린 맛이지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030294.1.10",
          "form": "그래도 오랜만에 먹으니까 맛있더라",
          "original_form": "그래도 오랜만에 먹으니까 맛있더라",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030294.1.11",
          "form": "밥 한공기 뚝딱",
          "original_form": "밥 한공기 뚝딱",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030294.1.12",
          "form": "너는 저녁 뭐 먹었어?",
          "original_form": "너는 저녁 뭐 먹었어?",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030294.1.13",
          "form": "나는 불고기!",
          "original_form": "나는 불고기!",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030294.1.14",
          "form": "학원 가기 전에 먹어야 해서 7교시 수업시간에 먹음 ㅋㅋㅋㅋ",
          "original_form": "학원 가기 전에 먹어야 해서 7교시 수업시간에 먹음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030294.1.15",
          "form": "불고기 완전 맛있었겠다",
          "original_form": "불고기 완전 맛있었겠다",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030294.1.16",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030294.1.17",
          "form": "ㅋㅋㅋㅋㅋ온클 하면서?",
          "original_form": "ㅋㅋㅋㅋㅋ온클 하면서?",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030294.1.18",
          "form": "아니 줌 ㅋㅋㅋㅋ",
          "original_form": "아니 줌 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030294.1.19",
          "form": "나는 게장 비리다면",
          "original_form": "나는 게장 비리다면",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030294.1.20",
          "form": "못먹겟네ㅠㅠㅠ",
          "original_form": "못먹겟네ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030294.1.21",
          "form": "옛날부터 비린거 못먹어서..",
          "original_form": "옛날부터 비린거 못먹어서..",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030294.1.22",
          "form": "우리 언제 고깃집 가지?",
          "original_form": "우리 언제 고깃집 가지?",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030294.1.23",
          "form": "한번 만나서 고기 먹으러 가야 하는데",
          "original_form": "한번 만나서 고기 먹으러 가야 하는데",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030294.1.24",
          "form": "허억 맞아!!",
          "original_form": "허억 맞아!!",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030294.1.25",
          "form": "내일 어때?",
          "original_form": "내일 어때?",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030294.1.26",
          "form": "내일? 오 빠르네ㅋㅋㅋㅋ",
          "original_form": "내일? 오 빠르네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030294.1.27",
          "form": "ㅋㅋㅋㅋㅋ 이왕 말 나온 김에 빠르게 잡아야지 고기 약속인데",
          "original_form": "ㅋㅋㅋㅋㅋ 이왕 말 나온 김에 빠르게 잡아야지 고기 약속인데",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030294.1.28",
          "form": "어디서 먹을거야?",
          "original_form": "어디서 먹을거야?",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030294.1.29",
          "form": "우리 전에 시험 끝나고 닷던",
          "original_form": "우리 전에 시험 끝나고 닷던",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030294.1.30",
          "form": "명륜진사갈비집?",
          "original_form": "명륜진사갈비집?",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030294.1.31",
          "form": "명륜진사갈비?",
          "original_form": "명륜진사갈비?",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030294.1.32",
          "form": "오 좋아",
          "original_form": "오 좋아",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030294.1.33",
          "form": "그럼 지금부터 아무것도 먹지 말고",
          "original_form": "그럼 지금부터 아무것도 먹지 말고",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030294.1.34",
          "form": "굶어야겠다",
          "original_form": "굶어야겠다",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030294.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030294.1.36",
          "form": "근데 고기먹으면 고기냄새 날텐데ㅠㅠㅠ",
          "original_form": "근데 고기먹으면 고기냄새 날텐데ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030294.1.37",
          "form": "페브리즈 챙겨야 하나",
          "original_form": "페브리즈 챙겨야 하나",
          "speaker_id": "1",
          "time": "20210903 23:46"
        }
      ]
