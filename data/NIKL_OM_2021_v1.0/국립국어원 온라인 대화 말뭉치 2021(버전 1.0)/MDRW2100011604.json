      "utterance": [
        {
          "id": "MDRW2100011604.1.1",
          "form": "안녕하세요 좋아하는 음식이 어떻게 되세요?",
          "original_form": "안녕하세요 좋아하는 음식이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011604.1.2",
          "form": "음 싫어하는게 없어요",
          "original_form": "음 싫어하는게 없어요",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011604.1.3",
          "form": "빵과 밥중에는 빵이 좋아요 ㅋㅋ",
          "original_form": "빵과 밥중에는 빵이 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011604.1.4",
          "form": "아 음식 가리지않고 다 좋아하시는군요 ㅎㅎ",
          "original_form": "아 음식 가리지않고 다 좋아하시는군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100011604.1.5",
          "form": "빵은 어떤 빵 좋아하시나요",
          "original_form": "빵은 어떤 빵 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100011604.1.6",
          "form": "피자도 좋고",
          "original_form": "피자도 좋고",
          "speaker_id": "2",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100011604.1.7",
          "form": "빵은 음 달달한건 다 좋아해요",
          "original_form": "빵은 음 달달한건 다 좋아해요",
          "speaker_id": "2",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100011604.1.8",
          "form": "치즈빵 초코빵 크림빵같은",
          "original_form": "치즈빵 초코빵 크림빵같은",
          "speaker_id": "2",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100011604.1.9",
          "form": "그러시구나 전 스콘이랑 크로아상 좋아해요 ㅎㅎ",
          "original_form": "그러시구나 전 스콘이랑 크로아상 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100011604.1.10",
          "form": "오 저희동네에 크라상점 이라는곳이 생겼더라고요",
          "original_form": "오 저희동네에 크라상점 이라는곳이 생겼더라고요",
          "speaker_id": "2",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100011604.1.11",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100011604.1.12",
          "form": "아뇨 첨 들어봤어요 가게이름인가요? 오",
          "original_form": "아뇨 첨 들어봤어요 가게이름인가요? 오",
          "speaker_id": "1",
          "time": "20210824 14:31"
        },
        {
          "id": "MDRW2100011604.1.13",
          "form": "계신곳 있으신가 검색 한번해보세요",
          "original_form": "계신곳 있으신가 검색 한번해보세요",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011604.1.14",
          "form": "크로와상이 10개 9900원!",
          "original_form": "크로와상이 10개 9900원!",
          "speaker_id": "2",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011604.1.15",
          "form": "헐 엄청 싸네요 충격적인 가격 ㅋㅋㅋ",
          "original_form": "헐 엄청 싸네요 충격적인 가격 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100011604.1.16",
          "form": "찾아보니까 버스타고 가야 하는 거리네요 담에 지나갈때 한번 봐야겠어요",
          "original_form": "찾아보니까 버스타고 가야 하는 거리네요 담에 지나갈때 한번 봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100011604.1.17",
          "form": "지나갈일 있으면 한번 사보는거 정도?는 괜찮은거 같더라고요 ㅋㅋ",
          "original_form": "지나갈일 있으면 한번 사보는거 정도?는 괜찮은거 같더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:36"
        },
        {
          "id": "MDRW2100011604.1.18",
          "form": "네 이름 기억해놔야겠어요 ㅎㅎ",
          "original_form": "네 이름 기억해놔야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011604.1.19",
          "form": "커피는 좋아하시나요?",
          "original_form": "커피는 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 17:52"
        },
        {
          "id": "MDRW2100011604.1.20",
          "form": "막 좋아하는편은 아니었는데 최근에는 많이 사먹었어요 ㅎㅎ",
          "original_form": "막 좋아하는편은 아니었는데 최근에는 많이 사먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:26"
        },
        {
          "id": "MDRW2100011604.1.21",
          "form": "여름엔 커피 잘 사먹게 되는거 같아요 ㅎㅎ",
          "original_form": "여름엔 커피 잘 사먹게 되는거 같아요 ㅎㅎ",
