        },
        {
          "id": "MDRW2100044133.1.4",
          "form": "라면볶이 아니",
          "original_form": "라면볶이 아니",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044133.1.5",
          "form": "응 라면볶이 컵라면으로 먹어본 기억이 나",
          "original_form": "응 라면볶이 컵라면으로 먹어본 기억이 나",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044133.1.6",
          "form": "마카로니였나",
          "original_form": "마카로니였나",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044133.1.7",
          "form": "그거 가격이 아직도 1050원인거 아니",
          "original_form": "그거 가격이 아직도 1050원인거 아니",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044133.1.8",
          "form": "뭐 진짜? 드럽게 안 팔리나",
          "original_form": "뭐 진짜? 드럽게 안 팔리나",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044133.1.9",
          "form": "마카로니면이라고...?",
          "original_form": "마카로니면이라고...?",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044133.1.10",
          "form": "왜 그런대",
          "original_form": "왜 그런대",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044133.1.11",
          "form": "아니 마카로니가 아주 조금 들어앴었어",
          "original_form": "아니 마카로니가 아주 조금 들어앴었어",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044133.1.12",
          "form": "아!!!!!!!!!그거",
          "original_form": "아!!!!!!!!!그거",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044133.1.13",
          "form": "다른맛일걸",
          "original_form": "다른맛일걸",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044133.1.14",
          "form": "라면볶이말고",
          "original_form": "라면볶이말고",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044133.1.15",
          "form": "콘옥수수있는거",
          "original_form": "콘옥수수있는거",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044133.1.16",
          "form": "뭐 진짜????",
          "original_form": "뭐 진짜????",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044133.1.17",
          "form": "다른거라고?",
          "original_form": "다른거라고?",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044133.1.18",
          "form": "라면볶이 약간 케찹맛 나는 거 아니야?",
          "original_form": "라면볶이 약간 케찹맛 나는 거 아니야?",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044133.1.19",
          "form": "응 라면볶이는 고추장맛나",
          "original_form": "응 라면볶이는 고추장맛나",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044133.1.20",
          "form": "그거 먹었더니 무지개같은 마카로니 들어있던데",
          "original_form": "그거 먹었더니 무지개같은 마카로니 들어있던데",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044133.1.21",
          "form": "헐 뭐지",
          "original_form": "헐 뭐지",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044133.1.22",
          "form": "나 뭐 먹었노",
          "original_form": "나 뭐 먹었노",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044133.1.23",
          "form": "네...무지개 마카로니요..?",
          "original_form": "네...무지개 마카로니요..?",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044133.1.24",
          "form": "무지개 마카로니 먹으면",
