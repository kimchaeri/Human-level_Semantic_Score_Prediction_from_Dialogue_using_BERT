      "utterance": [
        {
          "id": "MDRW2100019103.1.1",
          "form": "치킨 좋아하세요?",
          "original_form": "치킨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019103.1.2",
          "form": "네 어제 치킨시켜먹었어요",
          "original_form": "네 어제 치킨시켜먹었어요",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019103.1.3",
          "form": "오 어떤 거 시켜드셨나요?",
          "original_form": "오 어떤 거 시켜드셨나요?",
          "speaker_id": "1",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019103.1.4",
          "form": "bhc 치킨 시켜먹었습니다",
          "original_form": "bhc 치킨 시켜먹었습니다",
          "speaker_id": "2",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019103.1.5",
          "form": "저도 bhc 좋아하는데 맛있죠ㅎㅎ",
          "original_form": "저도 bhc 좋아하는데 맛있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:23"
        },
        {
          "id": "MDRW2100019103.1.6",
          "form": "치킨 어디브랜드 좋아하시나요?",
          "original_form": "치킨 어디브랜드 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100019103.1.7",
          "form": "저는 교촌이랑 처갓집 자주 시켜먹어요!",
          "original_form": "저는 교촌이랑 처갓집 자주 시켜먹어요!",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019103.1.8",
          "form": "교촌 처갓집 맛있죠",
          "original_form": "교촌 처갓집 맛있죠",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019103.1.9",
          "form": "처갓집은 역시 양념이죠",
          "original_form": "처갓집은 역시 양념이죠",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019103.1.10",
          "form": "맞아요 슈퍼슈프림 젤 좋아해요 ㅎㅎ",
          "original_form": "맞아요 슈퍼슈프림 젤 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019103.1.11",
          "form": "양념이 달달해서 맛있더라구요",
          "original_form": "양념이 달달해서 맛있더라구요",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019103.1.12",
          "form": "예전에 트러플슈프림?",
          "original_form": "예전에 트러플슈프림?",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019103.1.13",
          "form": "한번 먹어봤었는데",
          "original_form": "한번 먹어봤었는데",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019103.1.14",
          "form": "그건 좀 다른 느낌이었던거같아요",
          "original_form": "그건 좀 다른 느낌이었던거같아요",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019103.1.15",
          "form": "오 그건 안먹어봤어요 별로였나요...?",
          "original_form": "오 그건 안먹어봤어요 별로였나요...?",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019103.1.16",
          "form": "약간 특이한맛이었어요",
          "original_form": "약간 특이한맛이었어요",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019103.1.17",
          "form": "역시 처갓집은 슈프림이 젤 무난한거같아요",
          "original_form": "역시 처갓집은 슈프림이 젤 무난한거같아요",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019103.1.18",
          "form": "교촌은 어떤 메뉴 먹으시나요",
          "original_form": "교촌은 어떤 메뉴 먹으시나요",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019103.1.19",
          "form": "트러플이라 그랬었나봐요 ..!",
          "original_form": "트러플이라 그랬었나봐요 ..!",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019103.1.20",
          "form": "교촌은 허니콤보자주 먹고 트리플순살..? 새로나온것도 맛있어서 자주 먹었어용",
          "original_form": "교촌은 허니콤보자주 먹고 트리플순살..? 새로나온것도 맛있어서 자주 먹었어용",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019103.1.21",
          "form": "트리플순살은 뭐죠? 처음 들어봐요",
          "original_form": "트리플순살은 뭐죠? 처음 들어봐요",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019103.1.22",
          "form": "약간 트러플맛 나는 치즈시즈닝치킨인데 맛있더라구요!",
          "original_form": "약간 트러플맛 나는 치즈시즈닝치킨인데 맛있더라구요!",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019103.1.23",
          "form": "아 그렇군요 저는 교촌은 허니콤보랑 레드콤보",
          "original_form": "아 그렇군요 저는 교촌은 허니콤보랑 레드콤보",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019103.1.24",
          "form": "이거말곤 먹어본적이없어요",
          "original_form": "이거말곤 먹어본적이없어요",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019103.1.25",
          "form": "그 두개가 제일 무난하죠",
          "original_form": "그 두개가 제일 무난하죠",
          "speaker_id": "1",
          "time": "20210827 15:34"
        },
        {
          "id": "MDRW2100019103.1.26",
          "form": "후라이드도 먹어보고싶었는데 아직 못먹어봤네요",
          "original_form": "후라이드도 먹어보고싶었는데 아직 못먹어봤네요",
          "speaker_id": "1",
          "time": "20210827 15:35"
        }
      ]
