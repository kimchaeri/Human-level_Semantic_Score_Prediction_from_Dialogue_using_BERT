      "utterance": [
        {
          "id": "MDRW2100044691.1.1",
          "form": "진짜 나재민 어디서 데러온건지..?",
          "original_form": "진짜 나재민 어디서 데러온건지..?",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044691.1.2",
          "form": "진짜 얼굴 완전 미쳣어요",
          "original_form": "진짜 얼굴 완전 미쳣어요",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044691.1.3",
          "form": "나이 먹으면서 진짜 더 잘생겨지는기 느껴져요",
          "original_form": "나이 먹으면서 진짜 더 잘생겨지는기 느껴져요",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044691.1.4",
          "form": "한 3년후쯤 진짜 리즈일거같은 느낌 ㅠ",
          "original_form": "한 3년후쯤 진짜 리즈일거같은 느낌 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044691.1.5",
          "form": "정재현 나와",
          "original_form": "정재현 나와",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044691.1.6",
          "form": "왜냐면 제가 재현 얼굴에ㅜ대환장",
          "original_form": "왜냐면 제가 재현 얼굴에ㅜ대환장",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044691.1.7",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044691.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아웃겨요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아웃겨요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044691.1.9",
          "form": "정재현 인정합니다",
          "original_form": "정재현 인정합니다",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044691.1.10",
          "form": "진짜 어케 생일도 발렌타인데이",
          "original_form": "진짜 어케 생일도 발렌타인데이",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044691.1.11",
          "form": "인소 주인공도 저런 설정 쓰면 욕먹어요 ;;",
          "original_form": "인소 주인공도 저런 설정 쓰면 욕먹어요 ;;",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044691.1.12",
          "form": "그니가요 ㅠ",
          "original_form": "그니가요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044691.1.13",
          "form": "심지어 강남 8학군 출신",
          "original_form": "심지어 강남 8학군 출신",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044691.1.14",
          "form": "그리고...전 사실 입덕전에 재현이.. 얼굴마담인줄만 알았는데..",
          "original_form": "그리고...전 사실 입덕전에 재현이.. 얼굴마담인줄만 알았는데..",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
