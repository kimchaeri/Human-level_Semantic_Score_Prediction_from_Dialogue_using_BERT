        },
        {
          "id": "MDRW2100006766.1.2",
          "form": "요즘 마라탕 홀릭이에요ㅋ",
          "original_form": "요즘 마라탕 홀릭이에요ㅋ",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006766.1.3",
          "form": "로제매운맛이랑ㅋ",
          "original_form": "로제매운맛이랑ㅋ",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100006766.1.4",
          "form": "우아 ㅜㅜ 로제 너무 좋죠 ㅋㅋㅋ",
          "original_form": "우아 ㅜㅜ 로제 너무 좋죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100006766.1.5",
          "form": "로제 떡볶이랑 파스타",
          "original_form": "로제 떡볶이랑 파스타",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100006766.1.6",
          "form": "사람들이 좋아하는덴 이유가 있더라고요ㅋ",
          "original_form": "사람들이 좋아하는덴 이유가 있더라고요ㅋ",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006766.1.7",
          "form": "칼로리가 높다던데ㄱㅋ",
          "original_form": "칼로리가 높다던데ㄱㅋ",
          "speaker_id": "2",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006766.1.8",
          "form": "와 칼로리 생각못햇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "와 칼로리 생각못햇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100006766.1.9",
          "form": "생각하지 않는게 행복^^",
          "original_form": "생각하지 않는게 행복^^",
          "speaker_id": "2",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100006766.1.10",
          "form": "ㅋㅋㅋㅋ 배달 자주 시켜드세요?",
          "original_form": "ㅋㅋㅋㅋ 배달 자주 시켜드세요?",
