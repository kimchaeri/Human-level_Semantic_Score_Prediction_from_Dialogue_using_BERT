      "utterance": [
        {
          "id": "MDRW2100019184.1.1",
          "form": "군대가봤니",
          "original_form": "군대가봤니",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019184.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니",
          "speaker_id": "2",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019184.1.3",
          "form": "아 예전에 언제였지 고딩땐가",
          "original_form": "아 예전에 언제였지 고딩땐가",
          "speaker_id": "2",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019184.1.4",
          "form": "군부대 체험 이런거 가본듯",
          "original_form": "군부대 체험 이런거 가본듯",
          "speaker_id": "2",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019184.1.5",
          "form": "흐에엥",
          "original_form": "흐에엥",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019184.1.6",
          "form": "뭐야 그런체험이있어?",
          "original_form": "뭐야 그런체험이있어?",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019184.1.7",
          "form": "엉 대학이었나 고딩때였나 가물한데",
          "original_form": "엉 대학이었나 고딩때였나 가물한데",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019184.1.8",
          "form": "그냥 가서 부대 둘러보고 거기 밥도 먹고 그런거 ㅇㅇ",
          "original_form": "그냥 가서 부대 둘러보고 거기 밥도 먹고 그런거 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019184.1.9",
          "form": "거기가 해군부대라 잠수정 안에도 들어가봤음",
          "original_form": "거기가 해군부대라 잠수정 안에도 들어가봤음",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019184.1.10",
          "form": "와대박",
          "original_form": "와대박",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019184.1.11",
          "form": "찐군인들 활동하는걸 구경해본거야?",
          "original_form": "찐군인들 활동하는걸 구경해본거야?",
          "speaker_id": "1",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100019184.1.12",
          "form": "활동하는겅 직접 보진 않았고",
          "original_form": "활동하는겅 직접 보진 않았고",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100019184.1.13",
          "form": "그냥 그 생활공간이랑 훈련공간 둘러보는 정도?",
          "original_form": "그냥 그 생활공간이랑 훈련공간 둘러보는 정도?",
          "speaker_id": "2",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100019184.1.14",
          "form": "아하 군인들을 본건아니구나",
          "original_form": "아하 군인들을 본건아니구나",
          "speaker_id": "1",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100019184.1.15",
          "form": "훈련도 약식으로 받아봤니?",
          "original_form": "훈련도 약식으로 받아봤니?",
          "speaker_id": "1",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100019184.1.16",
          "form": "군인을 보긴 봤음 ㅋㅋㅋ",
          "original_form": "군인을 보긴 봤음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019184.1.17",
          "form": "ㄴㄴ 훈련같은건 전혀 안 받았어",
          "original_form": "ㄴㄴ 훈련같은건 전혀 안 받았어",
          "speaker_id": "2",
          "time": "20210827 16:33"
        },
        {
          "id": "MDRW2100019184.1.18",
          "form": "ㅋㅋㅋㅋ밥맛은 어땠어",
          "original_form": "ㅋㅋㅋㅋ밥맛은 어땠어",
          "speaker_id": "1",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019184.1.19",
          "form": "군대밥이 글케 구리다던데",
          "original_form": "군대밥이 글케 구리다던데",
          "speaker_id": "1",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019184.1.20",
          "form": "밥...구리긴 했어",
          "original_form": "밥...구리긴 했어",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019184.1.21",
          "form": "ㅋㅋㅋ메뉴도 기억나?",
          "original_form": "ㅋㅋㅋ메뉴도 기억나?",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019184.1.22",
          "form": "하나는 오리불고기였나 소불고기였나..? 그런거랑 김치랑 급식같은거였는데",
          "original_form": "하나는 오리불고기였나 소불고기였나..? 그런거랑 김치랑 급식같은거였는데",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019184.1.23",
          "form": "급식이랑 엇비슷했던가?",
          "original_form": "급식이랑 엇비슷했던가?",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019184.1.24",
          "form": "어..급식수준이라니",
          "original_form": "어..급식수준이라니",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019184.1.25",
          "form": "대충감이온다",
          "original_form": "대충감이온다",
          "speaker_id": "1",
          "time": "20210827 16:44"
        }
      ]
    }
