      "utterance": [
        {
          "id": "MDRW2100031063.1.1",
          "form": "첵스초코도 아니야",
          "original_form": "첵스초코도 아니야",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100031063.1.2",
          "form": "무슨 시리얼일까 맞춰봐 으하ㅓ",
          "original_form": "무슨 시리얼일까 맞춰봐 으하ㅓ",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100031063.1.3",
          "form": "너 혹시 그래놀라 이딴거먹니",
          "original_form": "너 혹시 그래놀라 이딴거먹니",
          "speaker_id": "2",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100031063.1.4",
          "form": "이딴거라니 정말 너무하네",
          "original_form": "이딴거라니 정말 너무하네",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100031063.1.5",
          "form": "정답이야",
          "original_form": "정답이야",
          "speaker_id": "1",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100031063.1.6",
          "form": "그건 악마의 각질이야",
          "original_form": "그건 악마의 각질이야",
          "speaker_id": "2",
          "time": "20210904 16:03"
        },
        {
          "id": "MDRW2100031063.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031063.1.8",
          "form": "악마의 각질을 아침으로 먹었단다",
          "original_form": "악마의 각질을 아침으로 먹었단다",
          "speaker_id": "1",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031063.1.9",
          "form": "그런거 먹으면 안돼",
          "original_form": "그런거 먹으면 안돼",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031063.1.10",
          "form": "맛있는걸 먹어야지",
          "original_form": "맛있는걸 먹어야지",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031063.1.11",
          "form": "왜 안돼 그래놀라의 어디가 이상한데!",
          "original_form": "왜 안돼 그래놀라의 어디가 이상한데!",
          "speaker_id": "1",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031063.1.12",
          "form": "그래놀라는 이름부터 별로야",
          "original_form": "그래놀라는 이름부터 별로야",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031063.1.13",
          "form": "아저씨 개그 나오잖아",
          "original_form": "아저씨 개그 나오잖아",
          "speaker_id": "2",
          "time": "20210904 16:04"
        },
        {
          "id": "MDRW2100031063.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ너 또 드립치면 궁디 꼬집어줄거야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ너 또 드립치면 궁디 꼬집어줄거야",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100031063.1.15",
          "form": "그래놀라 싫어하니?",
          "original_form": "그래놀라 싫어하니?",
          "speaker_id": "1",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100031063.1.16",
          "form": "허허하하 곡물원형이",
          "original_form": "허허하하 곡물원형이",
          "speaker_id": "2",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100031063.1.17",
          "form": "싫어",
          "original_form": "싫어",
          "speaker_id": "2",
          "time": "20210904 16:05"
        },
        {
          "id": "MDRW2100031063.1.18",
          "form": "이럴수가 너 곡물이랑 안 친하구나",
          "original_form": "이럴수가 너 곡물이랑 안 친하구나",
          "speaker_id": "1",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100031063.1.19",
          "form": "아몬드는 양념된건 먹겠는데",
          "original_form": "아몬드는 양념된건 먹겠는데",
          "speaker_id": "2",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100031063.1.20",
          "form": "난 밥에 곡물 넣는것도 싫더라",
          "original_form": "난 밥에 곡물 넣는것도 싫더라",
          "speaker_id": "2",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100031063.1.21",
          "form": "미숫가루도 싫어하니?",
          "original_form": "미숫가루도 싫어하니?",
          "speaker_id": "1",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100031063.1.22",
          "form": "잣 대추 콩 등등의 악마의 코딱지들",
          "original_form": "잣 대추 콩 등등의 악마의 코딱지들",
          "speaker_id": "2",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100031063.1.23",
          "form": "그런애들은 원형이 아니라서",
          "original_form": "그런애들은 원형이 아니라서",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100031063.1.24",
          "form": "원형이 아닌애들은 괜찮더라",
          "original_form": "원형이 아닌애들은 괜찮더라",
          "speaker_id": "2",
          "time": "20210904 16:07"
        }
      ]
    }
  ]
