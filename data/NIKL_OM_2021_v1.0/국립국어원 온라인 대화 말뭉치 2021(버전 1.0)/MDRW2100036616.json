      "utterance": [
        {
          "id": "MDRW2100036616.1.1",
          "form": "고소한맛이 좋은 참깨라면 저도좋아해요 ㅎㅎ",
          "original_form": "고소한맛이 좋은 참깨라면 저도좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 02:22"
        },
        {
          "id": "MDRW2100036616.1.2",
          "form": "부가적으로 넣는재료있나요",
          "original_form": "부가적으로 넣는재료있나요",
          "speaker_id": "2",
          "time": "20210831 02:22"
        },
        {
          "id": "MDRW2100036616.1.3",
          "form": "계란 넣고",
          "original_form": "계란 넣고",
          "speaker_id": "1",
          "time": "20210831 02:23"
        },
        {
          "id": "MDRW2100036616.1.4",
          "form": "아주 가끔 떡이나 콩나물 넣어요ㅋㅋㅋ",
          "original_form": "아주 가끔 떡이나 콩나물 넣어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:23"
        },
        {
          "id": "MDRW2100036616.1.5",
          "form": "ㅋㅋㅋ저도계란은 필수로넣구여",
          "original_form": "ㅋㅋㅋ저도계란은 필수로넣구여",
          "speaker_id": "2",
          "time": "20210831 02:24"
        },
        {
          "id": "MDRW2100036616.1.6",
          "form": "콩나물 집에있음넣고 없으면 걍 그대로먹어요",
          "original_form": "콩나물 집에있음넣고 없으면 걍 그대로먹어요",
          "speaker_id": "2",
          "time": "20210831 02:24"
        },
        {
          "id": "MDRW2100036616.1.7",
          "form": "국물없는 라면중엔어떤거 좋아하세요",
          "original_form": "국물없는 라면중엔어떤거 좋아하세요",
          "speaker_id": "2",
          "time": "20210831 02:24"
        },
        {
          "id": "MDRW2100036616.1.8",
          "form": "참깨라면은 계란블럭부터 기름까지 다 갖춰있어서",
          "original_form": "참깨라면은 계란블럭부터 기름까지 다 갖춰있어서",
          "speaker_id": "1",
          "time": "20210831 02:24"
        },
        {
          "id": "MDRW2100036616.1.9",
          "form": "더 추가하면 투머치해지더라구요",
          "original_form": "더 추가하면 투머치해지더라구요",
          "speaker_id": "1",
          "time": "20210831 02:25"
        },
        {
          "id": "MDRW2100036616.1.10",
          "form": "저 삼양 열무비빔면 좋아해요",
          "original_form": "저 삼양 열무비빔면 좋아해요",
          "speaker_id": "1",
          "time": "20210831 02:25"
        },
        {
          "id": "MDRW2100036616.1.11",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210831 02:25"
        },
        {
          "id": "MDRW2100036616.1.12",
          "form": "삼양 열무비빔 좀 맵던데",
          "original_form": "삼양 열무비빔 좀 맵던데",
          "speaker_id": "2",
          "time": "20210831 02:25"
        },
        {
          "id": "MDRW2100036616.1.13",
          "form": "매운거 잘드시나봐요",
          "original_form": "매운거 잘드시나봐요",
          "speaker_id": "2",
          "time": "20210831 02:25"
        },
        {
          "id": "MDRW2100036616.1.14",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210831 02:25"
        },
        {
          "id": "MDRW2100036616.1.15",
          "form": "저 매운거 진짜 못먹는편이라",
          "original_form": "저 매운거 진짜 못먹는편이라",
          "speaker_id": "1",
          "time": "20210831 02:25"
        },
        {
          "id": "MDRW2100036616.1.16",
          "form": "네..ㅜ전 비빔면들 왜케들 매운지",
          "original_form": "네..ㅜ전 비빔면들 왜케들 매운지",
          "speaker_id": "2",
          "time": "20210831 02:25"
        },
        {
          "id": "MDRW2100036616.1.17",
          "form": "불닭볶음면도 못먹어요ㅋㅋㅋㅋ",
          "original_form": "불닭볶음면도 못먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:26"
        },
        {
          "id": "MDRW2100036616.1.18",
          "form": "ㅋㅋㅋㅋ저두요ㅜ ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ저두요ㅜ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:26"
        },
        {
          "id": "MDRW2100036616.1.19",
          "form": "제일 매웠던게 진비빔면이었어요",
          "original_form": "제일 매웠던게 진비빔면이었어요",
          "speaker_id": "1",
          "time": "20210831 02:26"
        },
        {
          "id": "MDRW2100036616.1.20",
          "form": "글보기만해도 침나와요 ㅋㅋㅋㅋ",
          "original_form": "글보기만해도 침나와요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:26"
        },
        {
          "id": "MDRW2100036616.1.21",
          "form": "이게 가장 자극적!",
          "original_form": "이게 가장 자극적!",
          "speaker_id": "1",
          "time": "20210831 02:26"
        },
        {
          "id": "MDRW2100036616.1.22",
          "form": "전 짜파게티가 가장좋습니다..",
          "original_form": "전 짜파게티가 가장좋습니다..",
          "speaker_id": "2",
          "time": "20210831 02:26"
        },
        {
          "id": "MDRW2100036616.1.23",
          "form": "배홍동이 가장 순하고 시원한 맛이었어요",
          "original_form": "배홍동이 가장 순하고 시원한 맛이었어요",
          "speaker_id": "1",
          "time": "20210831 02:26"
        }
      ]
    }
