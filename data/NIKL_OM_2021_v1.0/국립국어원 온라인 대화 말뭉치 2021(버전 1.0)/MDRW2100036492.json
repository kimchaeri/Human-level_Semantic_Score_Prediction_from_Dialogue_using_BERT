      "utterance": [
        {
          "id": "MDRW2100036492.1.1",
          "form": "엉 티비에서 봤다",
          "original_form": "엉 티비에서 봤다",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036492.1.2",
          "form": "ㅋㅋ 나도 세비야 대성당은 티비에서 봤다",
          "original_form": "ㅋㅋ 나도 세비야 대성당은 티비에서 봤다",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036492.1.3",
          "form": "안가본 사람은 많지",
          "original_form": "안가본 사람은 많지",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036492.1.4",
          "form": "엉 바티칸 성 베드로 대성당이랑",
          "original_form": "엉 바티칸 성 베드로 대성당이랑",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036492.1.5",
          "form": "바티칸?",
          "original_form": "바티칸?",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036492.1.6",
          "form": "응 그리고 런던의 세인트 폴 대성당 다음으로 세번째로 크단다",
          "original_form": "응 그리고 런던의 세인트 폴 대성당 다음으로 세번째로 크단다",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036492.1.7",
          "form": "ㅋㅋ그게뭐야",
          "original_form": "ㅋㅋ그게뭐야",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036492.1.8",
          "form": "세번째이긴 하지만 다 양식이 틀리다는데",
          "original_form": "세번째이긴 하지만 다 양식이 틀리다는데",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036492.1.9",
          "form": "그래 세비야는 무슨 양식인덕",
          "original_form": "그래 세비야는 무슨 양식인덕",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036492.1.10",
          "form": "고딕양식으로 이 양식으로는 젤 크단다",
          "original_form": "고딕양식으로 이 양식으로는 젤 크단다",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036492.1.11",
          "form": "고딕양식 난 잘 모르겠지만",
          "original_form": "고딕양식 난 잘 모르겠지만",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036492.1.12",
          "form": "근데 여기에 속설이 있단다",
          "original_form": "근데 여기에 속설이 있단다",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036492.1.13",
          "form": "속설이 웃기네 뭔데",
          "original_form": "속설이 웃기네 뭔데",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036492.1.14",
          "form": "묘가 있는데 카스티야 왕의 발을 만지면 부자가 된단다",
          "original_form": "묘가 있는데 카스티야 왕의 발을 만지면 부자가 된단다",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036492.1.15",
          "form": "부자 그럼 꼭 만 져 야 겠다",
          "original_form": "부자 그럼 꼭 만 져 야 겠다",
          "speaker_id": "2",
          "time": "20210830 23:52"
        },
