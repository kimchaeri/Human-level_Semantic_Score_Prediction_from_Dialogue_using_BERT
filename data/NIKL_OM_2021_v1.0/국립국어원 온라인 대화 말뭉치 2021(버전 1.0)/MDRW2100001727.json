      "utterance": [
        {
          "id": "MDRW2100001727.1.1",
          "form": "너는 집안일 요즘 머해?",
          "original_form": "너는 집안일 요즘 머해?",
          "speaker_id": "1",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001727.1.2",
          "form": "나 요즘 집안일 잘안해",
          "original_form": "나 요즘 집안일 잘안해",
          "speaker_id": "2",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001727.1.3",
          "form": "그려???",
          "original_form": "그려???",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001727.1.4",
          "form": "빨래 개기나 이런것도??",
          "original_form": "빨래 개기나 이런것도??",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001727.1.5",
          "form": "엄청엄청 쉬운것도??",
          "original_form": "엄청엄청 쉬운것도??",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001727.1.6",
          "form": "빨래개기는 가끔하지",
          "original_form": "빨래개기는 가끔하지",
          "speaker_id": "2",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001727.1.7",
          "form": "그리고 밥상차리는것도",
          "original_form": "그리고 밥상차리는것도",
          "speaker_id": "2",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001727.1.8",
          "form": "엄청 자주 돕네",
          "original_form": "엄청 자주 돕네",
          "speaker_id": "2",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001727.1.9",
          "form": "생각해보니까",
          "original_form": "생각해보니까",
          "speaker_id": "2",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001727.1.10",
          "form": "진짜로???",
          "original_form": "진짜로???",
          "speaker_id": "1",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001727.1.11",
          "form": "그래? 좋구만!!",
          "original_form": "그래? 좋구만!!",
          "speaker_id": "1",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001727.1.12",
          "form": "요즘에 아빠는 뭐해?",
          "original_form": "요즘에 아빠는 뭐해?",
          "speaker_id": "1",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001727.1.13",
          "form": "요즘에 아빠 서울가서 일하지",
          "original_form": "요즘에 아빠 서울가서 일하지",
          "speaker_id": "2",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001727.1.14",
          "form": "그래서 아빠는 완전 가끔 집에 오잖아",
          "original_form": "그래서 아빠는 완전 가끔 집에 오잖아",
          "speaker_id": "2",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001727.1.15",
          "form": "아~~",
          "original_form": "아~~",
          "speaker_id": "1",
          "time": "20210819 18:30"
        },
        {
          "id": "MDRW2100001727.1.16",
          "form": "아빠는 언제 우리집 온대?",
          "original_form": "아빠는 언제 우리집 온대?",
          "speaker_id": "1",
          "time": "20210819 18:31"
        },
        {
          "id": "MDRW2100001727.1.17",
          "form": "한 한달뒤?",
          "original_form": "한 한달뒤?",
          "speaker_id": "2",
          "time": "20210819 18:31"
        },
        {
          "id": "MDRW2100001727.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001727.1.19",
          "form": "요즘엔 아빠가 전화도 안하더라",
          "original_form": "요즘엔 아빠가 전화도 안하더라",
          "speaker_id": "1",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001727.1.20",
          "form": "바쁜가봐",
          "original_form": "바쁜가봐",
          "speaker_id": "1",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001727.1.21",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001727.1.22",
          "form": "우리 이제 집안일좀 하자",
          "original_form": "우리 이제 집안일좀 하자",
          "speaker_id": "2",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001727.1.23",
          "form": "그래 좋은 생각이다!",
          "original_form": "그래 좋은 생각이다!",
          "speaker_id": "1",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001727.1.24",
          "form": "나도 설거지해야지",
          "original_form": "나도 설거지해야지",
          "speaker_id": "1",
          "time": "20210819 18:32"
        },
        {
          "id": "MDRW2100001727.1.25",
          "form": "너는 머할래?",
          "original_form": "너는 머할래?",
          "speaker_id": "1",
          "time": "20210819 18:33"
        },
        {
          "id": "MDRW2100001727.1.26",
          "form": "나는 빨래 널고 걷기",
          "original_form": "나는 빨래 널고 걷기",
          "speaker_id": "2",
          "time": "20210819 18:33"
        },
        {
          "id": "MDRW2100001727.1.27",
          "form": "개기",
          "original_form": "개기",
          "speaker_id": "2",
          "time": "20210819 18:33"
        },
        {
          "id": "MDRW2100001727.1.28",
          "form": "이런거",
          "original_form": "이런거",
          "speaker_id": "2",
          "time": "20210819 18:33"
        },
        {
          "id": "MDRW2100001727.1.29",
          "form": "하려고",
          "original_form": "하려고",
          "speaker_id": "2",
          "time": "20210819 18:33"
        },
        {
          "id": "MDRW2100001727.1.30",
          "form": "우리 이제 집안일 나눠서 해보자",
          "original_form": "우리 이제 집안일 나눠서 해보자",
          "speaker_id": "1",
          "time": "20210819 18:33"
        },
        {
          "id": "MDRW2100001727.1.31",
          "form": "오키?",
          "original_form": "오키?",
          "speaker_id": "1",
          "time": "20210819 18:33"
        },
        {
          "id": "MDRW2100001727.1.32",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210819 18:33"
        }
      ]
    }
