      "utterance": [
        {
          "id": "MDRW2100023430.1.1",
          "form": "ㅎㅎㅎㅎ 밤새면안됩니다 내일 일해야죠",
          "original_form": "ㅎㅎㅎㅎ 밤새면안됩니다 내일 일해야죠",
          "speaker_id": "2",
          "time": "20210830 02:15"
        },
        {
          "id": "MDRW2100023430.1.2",
          "form": "네네 ㅎ",
          "original_form": "네네 ㅎ",
          "speaker_id": "1",
          "time": "20210830 02:15"
        },
        {
          "id": "MDRW2100023430.1.3",
          "form": "차량을 바꾸고 싶어요 ㅣㅎ",
          "original_form": "차량을 바꾸고 싶어요 ㅣㅎ",
          "speaker_id": "2",
          "time": "20210830 02:15"
        },
        {
          "id": "MDRW2100023430.1.4",
          "form": "어떤거로요",
          "original_form": "어떤거로요",
          "speaker_id": "1",
          "time": "20210830 02:15"
        },
        {
          "id": "MDRW2100023430.1.5",
          "form": "근데 지금은 안되니 ㅎ",
          "original_form": "근데 지금은 안되니 ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023430.1.6",
          "form": "나중에 이혼하면 바꾸려구요ㅎ",
          "original_form": "나중에 이혼하면 바꾸려구요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023430.1.7",
          "form": "지금 아반떼인데ㅎ 현대 스타리아?",
          "original_form": "지금 아반떼인데ㅎ 현대 스타리아?",
          "speaker_id": "2",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023430.1.8",
          "form": "아아  연애인차 같은!!",
          "original_form": "아아  연애인차 같은!!",
          "speaker_id": "1",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023430.1.9",
          "form": "그걸로 바꺼서 아이랑 같이 여행도가고 놀고 그렇고 싶어요ㅎ",
          "original_form": "그걸로 바꺼서 아이랑 같이 여행도가고 놀고 그렇고 싶어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023430.1.10",
          "form": "요즘 코로나라 놀때갚마땅치 않아서ㅎ",
          "original_form": "요즘 코로나라 놀때갚마땅치 않아서ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023430.1.11",
          "form": "아이를 위해 작은 놀이공간읊만들어주고싶어요ㅎ",
          "original_form": "아이를 위해 작은 놀이공간읊만들어주고싶어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023430.1.12",
          "form": "전 날씨좋음 캠핑해여 ㅎㅎ",
          "original_form": "전 날씨좋음 캠핑해여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023430.1.13",
          "form": "여행도갈수도있고 놀수도 있는ㅎ 그런차요ㅎㅎ 그래서 이벤트로 차량을 가지고 싶은거ㅎㅎ",
          "original_form": "여행도갈수도있고 놀수도 있는ㅎ 그런차요ㅎㅎ 그래서 이벤트로 차량을 가지고 싶은거ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023430.1.14",
          "form": "캠핑도 너무 좋아하더라구요ㅎ",
          "original_form": "캠핑도 너무 좋아하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023430.1.15",
          "form": "오오 큰차가 좋긴 함여 ㅎㅎ",
          "original_form": "오오 큰차가 좋긴 함여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023430.1.16",
          "form": "저 캠핑장비 샀는데ㅎ 그것도 뭐 싸게 샀긴했지만ㅎ",
          "original_form": "저 캠핑장비 샀는데ㅎ 그것도 뭐 싸게 샀긴했지만ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023430.1.17",
          "form": "코로나라 못나가니깐 집에다가 해줬아요ㅎㅎ",
          "original_form": "코로나라 못나가니깐 집에다가 해줬아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023430.1.18",
          "form": "ㅎㅎ  좋죠~",
          "original_form": "ㅎㅎ  좋죠~",
          "speaker_id": "1",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023430.1.19",
          "form": "엄청좋아하더라구요ㅎ 테이블에다가 짜장만들어주고 하니깐",
          "original_form": "엄청좋아하더라구요ㅎ 테이블에다가 짜장만들어주고 하니깐",
          "speaker_id": "2",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023430.1.20",
          "form": "아주 업되서는ㅎㅎ 뽀뽀해주고 난리였아요ㅎㅎ",
          "original_form": "아주 업되서는ㅎㅎ 뽀뽀해주고 난리였아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023430.1.21",
          "form": "애들은  그냥 저냥 다좋은 ㅋ",
          "original_form": "애들은  그냥 저냥 다좋은 ㅋ",
          "speaker_id": "1",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023430.1.22",
          "form": "아아님도 나중에 한번해보세요ㅎ",
          "original_form": "아아님도 나중에 한번해보세요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023430.1.23",
          "form": "맞아요ㅎ 엄마아빠랑 하는 모든것이 좋을 꺼애요ㅎ",
          "original_form": "맞아요ㅎ 엄마아빠랑 하는 모든것이 좋을 꺼애요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:19"
        },
        {
          "id": "MDRW2100023430.1.24",
          "form": "ㅋ 집에서 이미 다햇죠ㅠㅋ",
          "original_form": "ㅋ 집에서 이미 다햇죠ㅠㅋ",
          "speaker_id": "1",
          "time": "20210830 02:19"
        },
        {
          "id": "MDRW2100023430.1.25",
          "form": "아아ㅎㅎ 잠은안자봤져?",
          "original_form": "아아ㅎㅎ 잠은안자봤져?",
          "speaker_id": "2",
          "time": "20210830 02:19"
        },
        {
          "id": "MDRW2100023430.1.26",
          "form": "텐트치고?ㅎ",
          "original_form": "텐트치고?ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:19"
        },
        {
          "id": "MDRW2100023430.1.27",
          "form": "잠은 안자봤어요",
          "original_form": "잠은 안자봤어요",
          "speaker_id": "1",
          "time": "20210830 02:19"
        },
        {
          "id": "MDRW2100023430.1.28",
          "form": "불끄고 텐트 위에다가 램프달아놓고 자면",
          "original_form": "불끄고 텐트 위에다가 램프달아놓고 자면",
          "speaker_id": "2",
          "time": "20210830 02:20"
        },
        {
          "id": "MDRW2100023430.1.29",
          "form": "좋아해요ㅎㅎ",
          "original_form": "좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:20"
        },
        {
          "id": "MDRW2100023430.1.30",
          "form": "아 방에서요?",
          "original_form": "아 방에서요?",
          "speaker_id": "1",
          "time": "20210830 02:20"
        },
        {
          "id": "MDRW2100023430.1.31",
          "form": "근데 늦게자니깐 그건 감안하셔야해요ㅎㅎ",
          "original_form": "근데 늦게자니깐 그건 감안하셔야해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:20"
        },
        {
          "id": "MDRW2100023430.1.32",
          "form": "거실에서요ㅎ",
          "original_form": "거실에서요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:20"
        },
        {
          "id": "MDRW2100023430.1.33",
          "form": "밖에서 캠핑생각했어요 ㅋ",
          "original_form": "밖에서 캠핑생각했어요 ㅋ",
          "speaker_id": "1",
          "time": "20210830 02:21"
        },
        {
          "id": "MDRW2100023430.1.34",
          "form": "이렇게 시강리 잘가다니.후훗",
          "original_form": "이렇게 시강리 잘가다니.후훗",
          "speaker_id": "1",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023430.1.35",
          "form": "그러게요ㅎ",
          "original_form": "그러게요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023430.1.36",
          "form": "수다쟁이라ㅎ 그런가봐요 ㅎ",
          "original_form": "수다쟁이라ㅎ 그런가봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023430.1.37",
          "form": "오늘은 요기까지.내일더 달려요 ^^",
          "original_form": "오늘은 요기까지.내일더 달려요 ^^",
          "speaker_id": "1",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023430.1.38",
          "form": "어제도그렇고 오늘도 그렇고 매일 수고하시네요ㅎ",
          "original_form": "어제도그렇고 오늘도 그렇고 매일 수고하시네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023430.1.39",
          "form": "으잉 왜요",
          "original_form": "으잉 왜요",
          "speaker_id": "1",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023430.1.40",
          "form": "네넹~^^안녕히주무셔요ㅎ",
          "original_form": "네넹~^^안녕히주무셔요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023430.1.41",
          "form": "ㅎㅎ 굿밤이요",
          "original_form": "ㅎㅎ 굿밤이요",
          "speaker_id": "1",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023430.1.42",
          "form": "ㅎㅎ 피곤하실까봐ㅎ",
          "original_form": "ㅎㅎ 피곤하실까봐ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:23"
        },
        {
