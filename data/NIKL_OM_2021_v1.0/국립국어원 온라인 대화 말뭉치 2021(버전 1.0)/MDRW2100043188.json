      "utterance": [
        {
          "id": "MDRW2100043188.1.1",
          "form": "냉장고에 케이크 빨리 먹어",
          "original_form": "냉장고에 케이크 빨리 먹어",
          "speaker_id": "2",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100043188.1.2",
          "form": "좀 있다 먹을거야",
          "original_form": "좀 있다 먹을거야",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043188.1.3",
          "form": "냉장고 자리 없어서 빨리 먹어서 자리 만들어야돼",
          "original_form": "냉장고 자리 없어서 빨리 먹어서 자리 만들어야돼",
          "speaker_id": "2",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043188.1.4",
          "form": "엄마는 파이나 빨리 먹어",
          "original_form": "엄마는 파이나 빨리 먹어",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043188.1.5",
          "form": "안 먹어",
          "original_form": "안 먹어",
          "speaker_id": "2",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043188.1.6",
          "form": "배부르다",
          "original_form": "배부르다",
          "speaker_id": "2",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100043188.1.7",
          "form": "그러면서 나는 빨리 먹으래ㅡㅡ",
          "original_form": "그러면서 나는 빨리 먹으래ㅡㅡ",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043188.1.8",
          "form": "빨리 먹어야지 다른",
          "original_form": "빨리 먹어야지 다른",
          "speaker_id": "2",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043188.1.9",
          "form": "거",
          "original_form": "거",
          "speaker_id": "2",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043188.1.10",
          "form": "넣",
          "original_form": "넣",
          "speaker_id": "2",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043188.1.11",
          "form": "을",
          "original_form": "을",
          "speaker_id": "2",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043188.1.12",
          "form": "자리가 생기지",
          "original_form": "자리가 생기지",
          "speaker_id": "2",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043188.1.13",
          "form": "아 그렇긴한데",
          "original_form": "아 그렇긴한데",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043188.1.14",
          "form": "지금 별로 입맛없는데",
          "original_form": "지금 별로 입맛없는데",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043188.1.15",
          "form": "희진이 먹으라고해",
          "original_form": "희진이 먹으라고해",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043188.1.16",
          "form": "걔 오늘 먹음",
          "original_form": "걔 오늘 먹음",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043188.1.17",
          "form": "초코라 그런가 엄청 잘먹는다",
          "original_form": "초코라 그런가 엄청 잘먹는다",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043188.1.18",
          "form": "파이도 먹으라고해",
          "original_form": "파이도 먹으라고해",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043188.1.19",
          "form": "이따 먹는대 그것도",
          "original_form": "이따 먹는대 그것도",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043188.1.20",
          "form": "잘먹는다 진짜",
          "original_form": "잘먹는다 진짜",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043188.1.21",
          "form": "ㅇㅇ군것질 별로 안하나 싶더니",
          "original_form": "ㅇㅇ군것질 별로 안하나 싶더니",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100043188.1.22",
          "form": "지취향이 아니었나봐",
          "original_form": "지취향이 아니었나봐",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100043188.1.23",
          "form": "그냥 생리때매 그때 컨디션 안 좋았던거 아니야?",
          "original_form": "그냥 생리때매 그때 컨디션 안 좋았던거 아니야?",
          "speaker_id": "2",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100043188.1.24",
          "form": "그건 아닌거같고",
          "original_form": "그건 아닌거같고",
          "speaker_id": "1",
          "time": "20210907 15:51"
        },
        {
          "id": "MDRW2100043188.1.25",
          "form": "낮에 군것질 안할라고 하는것같은데 저녁먹고 엄청 먹더라",
          "original_form": "낮에 군것질 안할라고 하는것같은데 저녁먹고 엄청 먹더라",
          "speaker_id": "1",
          "time": "20210907 15:51"
        },
        {
          "id": "MDRW2100043188.1.26",
          "form": "참다 입터지나?",
          "original_form": "참다 입터지나?",
          "speaker_id": "1",
          "time": "20210907 15:51"
        },
        {
          "id": "MDRW2100043188.1.27",
          "form": "버릇이 안좋게 들었어",
          "original_form": "버릇이 안좋게 들었어",
          "speaker_id": "2",
          "time": "20210907 15:51"
        },
        {
          "id": "MDRW2100043188.1.28",
          "form": "ㅇㅇ낮에는 뭐 먹으라고해도 안먹는대",
          "original_form": "ㅇㅇ낮에는 뭐 먹으라고해도 안먹는대",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043188.1.29",
          "form": "아껴서 자기전에 먹고 배부르게 잠자는듯..",
          "original_form": "아껴서 자기전에 먹고 배부르게 잠자는듯..",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043188.1.30",
          "form": "그러니까 살오르지 문제다",
          "original_form": "그러니까 살오르지 문제다",
          "speaker_id": "2",
          "time": "20210907 15:52"
        }
      ]
    }
