      "utterance": [
        {
          "id": "MDRW2100033336.1.1",
          "form": "비오니 맥주나 한 잔 했으면 좋겠다",
          "original_form": "비오니 맥주나 한 잔 했으면 좋겠다",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100033336.1.2",
          "form": "ㅎㅎ 다이어트 한다며",
          "original_form": "ㅎㅎ 다이어트 한다며",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100033336.1.3",
          "form": "곰표 맥주 맛있던데 집 앞 편의점에서 하나 사올까 ㅎㅎ",
          "original_form": "곰표 맥주 맛있던데 집 앞 편의점에서 하나 사올까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100033336.1.4",
          "form": "무슨 맛인데?",
          "original_form": "무슨 맛인데?",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100033336.1.5",
          "form": "상큼한 맛이야 ㅎㅎ",
          "original_form": "상큼한 맛이야 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100033336.1.6",
          "form": "느끼한 음식이랑 먹기 좋겠네",
          "original_form": "느끼한 음식이랑 먹기 좋겠네",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033336.1.7",
          "form": "ㅇㅇ 상큼하고 향긋해서 맛있음",
          "original_form": "ㅇㅇ 상큼하고 향긋해서 맛있음",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033336.1.8",
          "form": "신랑은 파울라너 같이 깊은 맛 나는 맥주 좋아하는 줄 알았는데",
          "original_form": "신랑은 파울라너 같이 깊은 맛 나는 맥주 좋아하는 줄 알았는데",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033336.1.9",
          "form": "가볍게 먹기에는 곰표 맥주도 맛있더라고 ㅎㅎ",
          "original_form": "가볍게 먹기에는 곰표 맥주도 맛있더라고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100033336.1.10",
          "form": "무슨 맛인지 먹어보고 싶네",
          "original_form": "무슨 맛인지 먹어보고 싶네",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100033336.1.11",
          "form": "내가 사오면 색시도 한모금 해봐 ㅎㅎ",
          "original_form": "내가 사오면 색시도 한모금 해봐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100033336.1.12",
          "form": "그럴까? 과자랑 같이 마시면 너무 맛있겠다",
          "original_form": "그럴까? 과자랑 같이 마시면 너무 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033336.1.13",
          "form": "ㅎㅎ 맥주랑 과자도 같이 사올께",
          "original_form": "ㅎㅎ 맥주랑 과자도 같이 사올께",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033336.1.14",
          "form": "오오 고마웡~ 비오는데 조심히 다녀와",
