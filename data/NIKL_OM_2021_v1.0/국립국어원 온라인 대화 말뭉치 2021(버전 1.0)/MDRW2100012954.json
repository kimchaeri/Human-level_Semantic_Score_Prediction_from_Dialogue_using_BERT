      "utterance": [
        {
          "id": "MDRW2100012954.1.1",
          "form": "출근길이이요 9시반출근이라",
          "original_form": "출근길이이요 9시반출근이라",
          "speaker_id": "2",
          "time": "20210825 09:27"
        },
        {
          "id": "MDRW2100012954.1.2",
          "form": "오 출근이 늦네요 ㅎㅎ",
          "original_form": "오 출근이 늦네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:28"
        },
        {
          "id": "MDRW2100012954.1.3",
          "form": "8시부터 30분격으로 본인이 결정하는거라",
          "original_form": "8시부터 30분격으로 본인이 결정하는거라",
          "speaker_id": "2",
          "time": "20210825 09:29"
        },
        {
          "id": "MDRW2100012954.1.4",
          "form": "밤새신건가요?ㅎ",
          "original_form": "밤새신건가요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 09:29"
        },
        {
          "id": "MDRW2100012954.1.5",
          "form": "아니요 ㅎㅎ 잤죠",
          "original_form": "아니요 ㅎㅎ 잤죠",
          "speaker_id": "1",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012954.1.6",
          "form": "아침에 주식 파느라 ㅎㅎ",
          "original_form": "아침에 주식 파느라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012954.1.7",
          "form": "오 ㅎ 주식하는군요ㅎ",
          "original_form": "오 ㅎ 주식하는군요ㅎ",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012954.1.8",
          "form": "많이버셨나요?",
          "original_form": "많이버셨나요?",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012954.1.9",
          "form": "차라리 안하는게 좋았을...ㅋㅋ",
          "original_form": "차라리 안하는게 좋았을...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012954.1.10",
          "form": "한번 발들이는순간 뺄수없는게 주식~",
          "original_form": "한번 발들이는순간 뺄수없는게 주식~",
          "speaker_id": "2",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012954.1.11",
          "form": "손절하지않는이상",
          "original_form": "손절하지않는이상",
          "speaker_id": "2",
          "time": "20210825 09:32"
        },
        {
          "id": "MDRW2100012954.1.12",
          "form": "ㅋㅋ손절 몇번쳤죠ㅠ",
          "original_form": "ㅋㅋ손절 몇번쳤죠ㅠ",
          "speaker_id": "1",
          "time": "20210825 09:33"
        },
        {
          "id": "MDRW2100012954.1.13",
          "form": "ㅎㅎ 우량주위주로 사셔야죠",
          "original_form": "ㅎㅎ 우량주위주로 사셔야죠",
          "speaker_id": "2",
          "time": "20210825 09:34"
        },
        {
          "id": "MDRW2100012954.1.14",
          "form": "요즘 공모주 많이들하던데",
          "original_form": "요즘 공모주 많이들하던데",
          "speaker_id": "2",
          "time": "20210825 09:34"
        },
        {
          "id": "MDRW2100012954.1.15",
          "form": "ㅋㅋ요새는 우량주로..",
          "original_form": "ㅋㅋ요새는 우량주로..",
          "speaker_id": "1",
          "time": "20210825 09:51"
        },
        {
          "id": "MDRW2100012954.1.16",
          "form": "우량주면 존버하면 언젠가 플러스올때가 있어요ㅎㅎ",
          "original_form": "우량주면 존버하면 언젠가 플러스올때가 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:55"
        },
        {
          "id": "MDRW2100012954.1.17",
          "form": "네 기다리고 있어요 ㅎㅎ",
          "original_form": "네 기다리고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100012954.1.18",
          "form": "곧 점심시간이군요",
          "original_form": "곧 점심시간이군요",
          "speaker_id": "1",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100012954.1.19",
          "form": "오늘도 이사님과?",
          "original_form": "오늘도 이사님과?",
          "speaker_id": "1",
          "time": "20210825 11:36"
        },
        {
          "id": "MDRW2100012954.1.20",
          "form": "네네 점심 먹으러 가는중이요ㅎ",
          "original_form": "네네 점심 먹으러 가는중이요ㅎ",
          "speaker_id": "2",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100012954.1.21",
          "form": "직화오징어로",
          "original_form": "직화오징어로",
          "speaker_id": "2",
          "time": "20210825 11:48"
        },
        {
          "id": "MDRW2100012954.1.22",
          "form": "캬 맛있겠네요",
          "original_form": "캬 맛있겠네요",
          "speaker_id": "1",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100012954.1.23",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100012954.1.24",
          "form": "오늘은 아예 출근안하시는건가요?",
          "original_form": "오늘은 아예 출근안하시는건가요?",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100012954.1.25",
          "form": "아직이요 ㅎㅎ",
          "original_form": "아직이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100012954.1.26",
          "form": "네 내일 오전에 출근해요!!",
          "original_form": "네 내일 오전에 출근해요!!",
          "speaker_id": "1",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100012954.1.27",
          "form": "정상출근인가보네요ㅎ",
          "original_form": "정상출근인가보네요ㅎ",
          "speaker_id": "2",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100012954.1.28",
          "form": "주야비휴가 계속 반복되는거예요?",
          "original_form": "주야비휴가 계속 반복되는거예요?",
          "speaker_id": "2",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100012954.1.29",
          "form": "주주야비비 뭐이렇게 섞이는건아닌가ㅋㅋ",
          "original_form": "주주야비비 뭐이렇게 섞이는건아닌가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:59"
        },
        {
          "id": "MDRW2100012954.1.30",
          "form": "ㅋㅋ4조2교대라서 주야비휴 반복입니다",
          "original_form": "ㅋㅋ4조2교대라서 주야비휴 반복입니다",
          "speaker_id": "1",
          "time": "20210825 12:16"
        }
      ]
