      "utterance": [
        {
          "id": "MDRW2100036924.1.1",
          "form": "안녕하새요.",
          "original_form": "안녕하새요.",
          "speaker_id": "1",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100036924.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036924.1.3",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "2",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036924.1.4",
          "form": "네..라면으로 간단하게 먹엇어요 ㅎㅎ\r\nname2님은요?",
          "original_form": "네..라면으로 간단하게 먹엇어요 ㅎㅎ\r\n&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036924.1.5",
          "form": "전 짜장밥먹었어요",
          "original_form": "전 짜장밥먹었어요",
          "speaker_id": "2",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036924.1.6",
          "form": "어떤라면드셨어요",
          "original_form": "어떤라면드셨어요",
          "speaker_id": "2",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036924.1.7",
          "form": "오 너무맛잇엇겟다..\r\n저는 스낵면이용",
          "original_form": "오 너무맛잇엇겟다..\r\n저는 스낵면이용",
          "speaker_id": "1",
          "time": "20210831 15:04"
        },
        {
          "id": "MDRW2100036924.1.8",
          "form": "저는 짜장밥 너무 좋아해요 ㅎㅎ\r\n짬뽕국물도 같이 나와서 ㅎㅎ",
          "original_form": "저는 짜장밥 너무 좋아해요 ㅎㅎ\r\n짬뽕국물도 같이 나와서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100036924.1.9",
          "form": "스낵면도넘맛있져",
          "original_form": "스낵면도넘맛있져",
          "speaker_id": "2",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100036924.1.10",
          "form": "요새는 짬뽕국물안주더라구여ㅜ",
          "original_form": "요새는 짬뽕국물안주더라구여ㅜ",
          "speaker_id": "2",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100036924.1.11",
          "form": "거기는주나봐요",
          "original_form": "거기는주나봐요",
          "speaker_id": "2",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100036924.1.12",
          "form": "저 그것때문에 주문하는데 ㅜ",
          "original_form": "저 그것때문에 주문하는데 ㅜ",
          "speaker_id": "1",
          "time": "20210831 15:06"
        },
        {
          "id": "MDRW2100036924.1.13",
          "form": "저는 직접찾아갓을때 먹어봣고둔요 ㅜ\r\n매장마다 다른가봐용",
          "original_form": "저는 직접찾아갓을때 먹어봣고둔요 ㅜ\r\n매장마다 다른가봐용",
          "speaker_id": "1",
          "time": "20210831 15:06"
        },
        {
          "id": "MDRW2100036924.1.14",
          "form": "ㅜㅜㅜ짬뽕국물 돈받고팔아여",
          "original_form": "ㅜㅜㅜ짬뽕국물 돈받고팔아여",
          "speaker_id": "2",
          "time": "20210831 15:06"
        },
        {
          "id": "MDRW2100036924.1.15",
          "form": "헐...얼만데요? 와..ㅜㅜㅜㅜ",
          "original_form": "헐...얼만데요? 와..ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 15:07"
        },
        {
          "id": "MDRW2100036924.1.16",
          "form": "그 막 술국 그런건아니죠? 만원받더라고요 ㅋㅋㅋ",
          "original_form": "그 막 술국 그런건아니죠? 만원받더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:07"
        },
        {
          "id": "MDRW2100036924.1.17",
          "form": "아예 짬뽕가격으로파는데도있구여 한3천원받는데도있어요",
          "original_form": "아예 짬뽕가격으로파는데도있구여 한3천원받는데도있어요",
          "speaker_id": "2",
          "time": "20210831 15:07"
        },
        {
          "id": "MDRW2100036924.1.18",
          "form": "헐..... 너무하네요....\r\n만들기힘든건아는데 흑흑",
          "original_form": "헐..... 너무하네요....\r\n만들기힘든건아는데 흑흑",
          "speaker_id": "1",
          "time": "20210831 15:07"
        },
        {
          "id": "MDRW2100036924.1.19",
          "form": "어쩔수없죠ㅜㅜ뭐 그래서",
          "original_form": "어쩔수없죠ㅜㅜ뭐 그래서",
          "speaker_id": "2",
          "time": "20210831 15:08"
        },
        {
          "id": "MDRW2100036924.1.20",
          "form": "저는 청정원 짬뽕 사서햐봣는데 아쉽더라고요 ㅎㅎㅎㅎ",
          "original_form": "저는 청정원 짬뽕 사서햐봣는데 아쉽더라고요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 15:08"
        },
        {
          "id": "MDRW2100036924.1.21",
          "form": "넹넹",
