      "utterance": [
        {
          "id": "MDRW2100011581.1.1",
          "form": "네 여기도 지금 엄청 쏟아져요",
          "original_form": "네 여기도 지금 엄청 쏟아져요",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011581.1.2",
          "form": "네 내일 뮤지컬 보러 갈건데",
          "original_form": "네 내일 뮤지컬 보러 갈건데",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011581.1.3",
          "form": "내일까지 오더라구요 ㅠㅠ",
          "original_form": "내일까지 오더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011581.1.4",
          "form": "내일 가세요?",
          "original_form": "내일 가세요?",
          "speaker_id": "2",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011581.1.5",
          "form": "내일 태풍올 수도 있다던데..",
          "original_form": "내일 태풍올 수도 있다던데..",
          "speaker_id": "2",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011581.1.6",
          "form": "네 저도 오늘 내일 태풍 올 수 있다고 들은 것 같아요",
          "original_form": "네 저도 오늘 내일 태풍 올 수 있다고 들은 것 같아요",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011581.1.7",
          "form": "근데 내일이 딱 제가 원하는 캐스팅이라 ㅜㅜ",
          "original_form": "근데 내일이 딱 제가 원하는 캐스팅이라 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011581.1.8",
          "form": "아이구 조심하세요",
          "original_form": "아이구 조심하세요",
          "speaker_id": "2",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011581.1.9",
          "form": "내일은 비가 좀 그치면 좋을텐데요",
          "original_form": "내일은 비가 좀 그치면 좋을텐데요",
          "speaker_id": "2",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011581.1.10",
          "form": "네 ㅜㅜ 비오면 아무래도 이동하기도 너무 번잡스럽고..",
          "original_form": "네 ㅜㅜ 비오면 아무래도 이동하기도 너무 번잡스럽고..",
          "speaker_id": "1",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011581.1.11",
          "form": "네 그래도 시원하긴 한 것 같아요",
          "original_form": "네 그래도 시원하긴 한 것 같아요",
          "speaker_id": "2",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011581.1.12",
          "form": "아 네 맞아요",
          "original_form": "아 네 맞아요",
          "speaker_id": "1",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011581.1.13",
          "form": "확 시원해졌죠",
          "original_form": "확 시원해졌죠",
          "speaker_id": "1",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011581.1.14",
          "form": "가을같아요",
          "original_form": "가을같아요",
          "speaker_id": "1",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011581.1.15",
          "form": "네 이제 정말 이불 필수더라구요",
          "original_form": "네 이제 정말 이불 필수더라구요",
          "speaker_id": "2",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011581.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011581.1.17",
          "form": "저도 어제는 이불 꼭 덮고 잤어요",
          "original_form": "저도 어제는 이불 꼭 덮고 잤어요",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011581.1.18",
          "form": "이제 좀 있으면 두꺼운 이불로 바꿔줘야 할 것 같아요",
          "original_form": "이제 좀 있으면 두꺼운 이불로 바꿔줘야 할 것 같아요",
          "speaker_id": "2",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011581.1.19",
          "form": "네 근데 또 날이 시원해지니까 모기도 슬슬 나타나는 것 같아요",
          "original_form": "네 근데 또 날이 시원해지니까 모기도 슬슬 나타나는 것 같아요",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011581.1.20",
          "form": "네 시원해져서 좋긴한데",
          "original_form": "네 시원해져서 좋긴한데",
          "speaker_id": "2",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011581.1.21",
          "form": "어쩔 수 없죠",
          "original_form": "어쩔 수 없죠",
          "speaker_id": "2",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011581.1.22",
          "form": "네 내일은 좀 맑아졌으면 좋겠네요 ㅠㅠ",
          "original_form": "네 내일은 좀 맑아졌으면 좋겠네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011581.1.23",
          "form": "오늘도 비가 많이 올 줄 알았는데",
          "original_form": "오늘도 비가 많이 올 줄 알았는데",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100011581.1.24",
          "form": "지금은 안 오네요",
          "original_form": "지금은 안 오네요",
          "speaker_id": "1",
          "time": "20210825 13:19"
        }
      ]
    }
