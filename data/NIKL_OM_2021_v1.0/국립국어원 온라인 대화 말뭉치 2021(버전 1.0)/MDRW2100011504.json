      "utterance": [
        {
          "id": "MDRW2100011504.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 13:20"
        },
        {
          "id": "MDRW2100011504.1.2",
          "form": "네 학생이신가요?",
          "original_form": "네 학생이신가요?",
          "speaker_id": "2",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100011504.1.3",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "1",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011504.1.4",
          "form": "대학생이세요?",
          "original_form": "대학생이세요?",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011504.1.5",
          "form": "저는졸업한지 엄청오래되었어요ㅋㅋ",
          "original_form": "저는졸업한지 엄청오래되었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011504.1.6",
          "form": "네네 대학생입니다ㅎㅎ",
          "original_form": "네네 대학생입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011504.1.7",
          "form": "과제가많은과이신가요?",
          "original_form": "과제가많은과이신가요?",
          "speaker_id": "2",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011504.1.8",
          "form": "저는 과제폭탄과여서 힘들었던기억이..",
          "original_form": "저는 과제폭탄과여서 힘들었던기억이..",
          "speaker_id": "2",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011504.1.9",
          "form": "과제 많아요ㅠㅠ 교수님 제발...",
          "original_form": "과제 많아요ㅠㅠ 교수님 제발...",
          "speaker_id": "1",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100011504.1.10",
          "form": "시험도 과제로줘서 증말 밤새서했어요ㅋㅋ",
          "original_form": "시험도 과제로줘서 증말 밤새서했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100011504.1.11",
          "form": "차라리 시험치고싶다며",
          "original_form": "차라리 시험치고싶다며",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100011504.1.12",
          "form": "아 엄청 괴롭겠네요ㅠ",
          "original_form": "아 엄청 괴롭겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011504.1.13",
          "form": "지금은방학이신가요?",
          "original_form": "지금은방학이신가요?",
          "speaker_id": "2",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011504.1.14",
          "form": "저는 방학때도 학교갓어욬ㅋ끔찍",
          "original_form": "저는 방학때도 학교갓어욬ㅋ끔찍",
          "speaker_id": "2",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011504.1.15",
          "form": "근데이젠다시가고싶기도해요",
          "original_form": "근데이젠다시가고싶기도해요",
          "speaker_id": "2",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011504.1.16",
          "form": "와 엄청 고생하셨네요...",
          "original_form": "와 엄청 고생하셨네요...",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011504.1.17",
          "form": "이젠과제가아닌실전이라ㅋㅋ",
          "original_form": "이젠과제가아닌실전이라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100011504.1.18",
          "form": "차라리과제를하고싶어요",
          "original_form": "차라리과제를하고싶어요",
          "speaker_id": "2",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100011504.1.19",
          "form": "아 맞아요 그립기도 하실것같아요",
          "original_form": "아 맞아요 그립기도 하실것같아요",
          "speaker_id": "1",
          "time": "20210824 13:32"
        }
      ]
