      "utterance": [
        {
          "id": "MDRW2100042044.1.1",
          "form": "임현주 넘 이쁘죠",
          "original_form": "임현주 넘 이쁘죠",
          "speaker_id": "1",
          "time": "20210906 19:04"
        },
        {
          "id": "MDRW2100042044.1.2",
          "form": "다이어트 하세요??",
          "original_form": "다이어트 하세요??",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042044.1.3",
          "form": "아니용 살이 여름에 입맛없어서",
          "original_form": "아니용 살이 여름에 입맛없어서",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042044.1.4",
          "form": "5키로빠졌어요ㅠㅠ",
          "original_form": "5키로빠졌어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042044.1.5",
          "form": "다엇트 하세여?",
          "original_form": "다엇트 하세여?",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042044.1.6",
          "form": "저도 살이 빠져서 걱정이예요 ㅠㅠ",
          "original_form": "저도 살이 빠져서 걱정이예요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042044.1.7",
          "form": "운동을 해여하는대",
          "original_form": "운동을 해여하는대",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042044.1.8",
          "form": "근력운동을 해야 할거같아요",
          "original_form": "근력운동을 해야 할거같아요",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042044.1.9",
          "form": "맞아요 저도 근력 8키로 모자라대요",
          "original_form": "맞아요 저도 근력 8키로 모자라대요",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042044.1.10",
          "form": "저 인바디 재면 항상 내장비만이래요 ㅋㅋㅋ",
          "original_form": "저 인바디 재면 항상 내장비만이래요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042044.1.11",
          "form": "아 저랑 같아요 저도 마른비만이래요",
          "original_form": "아 저랑 같아요 저도 마른비만이래요",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100042044.1.12",
          "form": "내장비만은 운동을 엄청 해야 하나봐요 ㅠㅠ",
          "original_form": "내장비만은 운동을 엄청 해야 하나봐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 11:06"
        },
        {
          "id": "MDRW2100042044.1.13",
          "form": "구쵸 유산소는 마니 하는데",
          "original_form": "구쵸 유산소는 마니 하는데",
          "speaker_id": "1",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042044.1.14",
          "form": "유산소 어떤거 하세용",
          "original_form": "유산소 어떤거 하세용",
          "speaker_id": "2",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100042044.1.15",
          "form": "유산소 걷기랑 산책 달리기,  댄스 레슨 받아",
          "original_form": "유산소 걷기랑 산책 달리기,  댄스 레슨 받아",
          "speaker_id": "1",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042044.1.16",
          "form": "오오 댄스 레슨 !! 어떤 장르 추세요?",
          "original_form": "오오 댄스 레슨 !! 어떤 장르 추세요?",
          "speaker_id": "2",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100042044.1.17",
          "form": "주로 아이돌 댄스 합니다",
          "original_form": "주로 아이돌 댄스 합니다",
