      "utterance": [
        {
          "id": "MDRW2100044427.1.1",
          "form": "메인한테 잘 보여서",
          "original_form": "메인한테 잘 보여서",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044427.1.2",
          "form": "좋은자리 얻으려는 게 넘 웃기다",
          "original_form": "좋은자리 얻으려는 게 넘 웃기다",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044427.1.3",
          "form": "ㅎㅎ다들 좋은자리 하고싶어서",
          "original_form": "ㅎㅎ다들 좋은자리 하고싶어서",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044427.1.4",
          "form": "응 글고 메인댄서가",
          "original_form": "응 글고 메인댄서가",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044427.1.5",
          "form": "그러고 뒤에서 얘기하고",
          "original_form": "그러고 뒤에서 얘기하고",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044427.1.6",
          "form": "저 계급의 워스트댄서를 고르는거야",
          "original_form": "저 계급의 워스트댄서를 고르는거야",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044427.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044427.1.8",
          "form": "다 이미 대본이 있는거라긴 하지만",
          "original_form": "다 이미 대본이 있는거라긴 하지만",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044427.1.9",
          "form": "그치 저런 것도 다 대본이 있다더라",
          "original_form": "그치 저런 것도 다 대본이 있다더라",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044427.1.10",
          "form": "응 막 갈등이나 그런거",
          "original_form": "응 막 갈등이나 그런거",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044427.1.11",
          "form": "다 짜준대",
          "original_form": "다 짜준대",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044427.1.12",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044427.1.13",
          "form": "슈퍼맨 애기들도 대본 있다는데 뭐",
          "original_form": "슈퍼맨 애기들도 대본 있다는데 뭐",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044427.1.14",
          "form": "응 그렇겠지",
          "original_form": "응 그렇겠지",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044427.1.15",
          "form": "티비프로그램은 대본이 없을 수 없어",
          "original_form": "티비프로그램은 대본이 없을 수 없어",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044427.1.16",
          "form": "엊그제 옛날 리얼리티 쇼에서 설수현씨가 엄청 아들만 감싸는 그런 클립이 다시 나와서",
          "original_form": "엊그제 옛날 리얼리티 쇼에서 설수현씨가 엄청 아들만 감싸는 그런 클립이 다시 나와서",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044427.1.17",
          "form": "엄청 설왕설래했는데",
          "original_form": "엄청 설왕설래했는데",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044427.1.18",
          "form": "아 막내아들만",
          "original_form": "아 막내아들만",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044427.1.19",
          "form": "그때 보고서도 이상하다 생각했어",
          "original_form": "그때 보고서도 이상하다 생각했어",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044427.1.20",
          "form": "그 딸이 직접 대본 올리면서 대본대로 연기한거라고 했어",
          "original_form": "그 딸이 직접 대본 올리면서 대본대로 연기한거라고 했어",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044427.1.21",
          "form": "어 그거 봤던 기억나?",
          "original_form": "어 그거 봤던 기억나?",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044427.1.22",
          "form": "오 다들 춤 잘춘다",
          "original_form": "오 다들 춤 잘춘다",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044427.1.23",
          "form": "멋있구만",
          "original_form": "멋있구만",
          "speaker_id": "2",
          "time": "20210907 22:47"
        }
      ]
