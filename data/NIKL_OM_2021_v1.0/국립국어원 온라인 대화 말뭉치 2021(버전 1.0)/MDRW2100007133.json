      "utterance": [
        {
          "id": "MDRW2100007133.1.1",
          "form": "최근에 구매한 물건 있으세요?",
          "original_form": "최근에 구매한 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007133.1.2",
          "form": "저는 대관령 놀러가서 양 인형 샀네요 ㅎㅎ",
          "original_form": "저는 대관령 놀러가서 양 인형 샀네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007133.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007133.1.4",
          "form": "저는 마스크를 삿어요!",
          "original_form": "저는 마스크를 삿어요!",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007133.1.5",
          "form": "다써서 삿는데 정말 빨리오더라구요",
          "original_form": "다써서 삿는데 정말 빨리오더라구요",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007133.1.6",
          "form": "강릉 가셨구나~",
          "original_form": "강릉 가셨구나~",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007133.1.7",
          "form": "아니요 평창 다녀왔어요~~",
          "original_form": "아니요 평창 다녀왔어요~~",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007133.1.8",
          "form": "좋더라고요 추천드려요!",
          "original_form": "좋더라고요 추천드려요!",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007133.1.9",
          "form": "ㅋㅋㅋㅋ 기회가 된다면!!",
          "original_form": "ㅋㅋㅋㅋ 기회가 된다면!!",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007133.1.10",
          "form": "가서 또 뭘 구매하셨나요?",
          "original_form": "가서 또 뭘 구매하셨나요?",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007133.1.11",
          "form": "메밀 국수도 샀어요 ㅎㅎㅎ",
          "original_form": "메밀 국수도 샀어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007133.1.12",
          "form": "외! 맛있겠다!!!",
          "original_form": "외! 맛있겠다!!!",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007133.1.13",
          "form": "저도 면 샀아요!",
          "original_form": "저도 면 샀아요!",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007133.1.14",
          "form": "컵라면 ㅋㅋ",
          "original_form": "컵라면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007133.1.15",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007133.1.16",
          "form": "웃기다 뭐 사셨어요?",
          "original_form": "웃기다 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007133.1.17",
          "form": "감자면 아세요?",
          "original_form": "감자면 아세요?",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007133.1.18",
          "form": "오 봤던 것 같아요 좋아하시나요?",
          "original_form": "오 봤던 것 같아요 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007133.1.19",
          "form": "국물 있는거 맞죠?",
          "original_form": "국물 있는거 맞죠?",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007133.1.20",
          "form": "네네 담백하면서 시원해서 좋아해요",
          "original_form": "네네 담백하면서 시원해서 좋아해요",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007133.1.21",
          "form": "저도 한 번 먹어봐야겠네요 ㅎㅎㅎㅎㅎ",
          "original_form": "저도 한 번 먹어봐야겠네요 ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007133.1.22",
          "form": "저는 이제 가을옷을 사고 싶어요",
          "original_form": "저는 이제 가을옷을 사고 싶어요",
          "speaker_id": "1",
