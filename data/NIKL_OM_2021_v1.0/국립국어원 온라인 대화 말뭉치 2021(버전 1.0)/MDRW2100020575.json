        {
          "id": "MDRW2100020575.1.1",
          "form": "하 애플민트까진 허용 가능하신가요",
          "original_form": "하 애플민트까진 허용 가능하신가요",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020575.1.2",
          "form": "어 전 그거 좋아해요",
          "original_form": "어 전 그거 좋아해요",
          "speaker_id": "1",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020575.1.3",
          "form": "고기먹고 입가심으로 딱인데",
          "original_form": "고기먹고 입가심으로 딱인데",
          "speaker_id": "2",
          "time": "20210828 10:03"
        },
        {
          "id": "MDRW2100020575.1.4",
          "form": "그거 하프갤런에 한통 담아왔어요",
          "original_form": "그거 하프갤런에 한통 담아왔어요",
          "speaker_id": "1",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020575.1.5",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020575.1.6",
          "form": "애플민트는 맛있으니까요",
          "original_form": "애플민트는 맛있으니까요",
          "speaker_id": "1",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020575.1.7",
          "form": "와 애플민트로만요?",
          "original_form": "와 애플민트로만요?",
          "speaker_id": "2",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020575.1.8",
          "form": "네 저 약간",
          "original_form": "네 저 약간",
          "speaker_id": "1",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020575.1.9",
          "form": "짱이다 플랙스",
          "original_form": "짱이다 플랙스",
          "speaker_id": "2",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020575.1.10",
          "form": "광기있어서 한통에 하나만 담아먹어요",
          "original_form": "광기있어서 한통에 하나만 담아먹어요",
          "speaker_id": "1",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020575.1.11",
          "form": "와와 진짜 먹잘알 고수를 보는 느낌이에요",
          "original_form": "와와 진짜 먹잘알 고수를 보는 느낌이에요",
          "speaker_id": "2",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020575.1.12",
          "form": "ㅋㅋㅋㅋ 맛섞이는게 너무싫어서",
          "original_form": "ㅋㅋㅋㅋ 맛섞이는게 너무싫어서",
          "speaker_id": "1",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020575.1.13",
          "form": "배라 가시면 민초드세요,",
          "original_form": "배라 가시면 민초드세요,",
          "speaker_id": "1",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020575.1.14",
          "form": "일단 민트류는 꼭 들어갑니다",
          "original_form": "일단 민트류는 꼭 들어갑니다",
