      "utterance": [
        {
          "id": "MDRW2100020907.1.1",
          "form": "꽃에이드에 직접 꽃잎도 넣나요?",
          "original_form": "꽃에이드에 직접 꽃잎도 넣나요?",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020907.1.2",
          "form": "예전에는 있었는데 오늘은 없네요.. 아쉽군요",
          "original_form": "예전에는 있었는데 오늘은 없네요.. 아쉽군요",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100020907.1.3",
          "form": "요새 트렌드인가봐요 꽃차, 꽃에이드",
          "original_form": "요새 트렌드인가봐요 꽃차, 꽃에이드",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100020907.1.4",
          "form": "아 국화차나 장미차 에이드 버전인가 했는데",
          "original_form": "아 국화차나 장미차 에이드 버전인가 했는데",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100020907.1.5",
          "form": "꽃이 안 들어가 아쉽네요",
          "original_form": "꽃이 안 들어가 아쉽네요",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100020907.1.6",
          "form": "그래도 향이 너무 좋겠어요",
          "original_form": "그래도 향이 너무 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100020907.1.7",
          "form": "그쵸.. 아쉬워요 네 향은 좋답니다!",
          "original_form": "그쵸.. 아쉬워요 네 향은 좋답니다!",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020907.1.8",
          "form": "나중에 새 메뉴 도전하실 때 드셔보세요 ㅎㅎ",
          "original_form": "나중에 새 메뉴 도전하실 때 드셔보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020907.1.9",
          "form": "네네 그래야겠어요!",
          "original_form": "네네 그래야겠어요!",
          "speaker_id": "1",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020907.1.10",
          "form": "여름 가을에 잘 어울리는 것 같아요",
          "original_form": "여름 가을에 잘 어울리는 것 같아요",
          "speaker_id": "1",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020907.1.11",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020907.1.12",
          "form": "본인만의 맛있는 레시피 있나요?",
          "original_form": "본인만의 맛있는 레시피 있나요?",
          "speaker_id": "2",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100020907.1.13",
          "form": "요알못이라ㅜ 그냥 배달이 최고예요",
          "original_form": "요알못이라ㅜ 그냥 배달이 최고예요",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020907.1.14",
          "form": "라면만 어레인지해요",
          "original_form": "라면만 어레인지해요",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020907.1.15",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020907.1.16",
          "form": "카야잼 아세요?",
          "original_form": "카야잼 아세요?",
          "speaker_id": "2",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100020907.1.17",
          "form": "그냥 식빵 구워서 카야잼바르고 휘핑크림 좋아하시면 뿌려드세요",
          "original_form": "그냥 식빵 구워서 카야잼바르고 휘핑크림 좋아하시면 뿌려드세요",
          "speaker_id": "2",
          "time": "20210829 11:02"
        },
        {
          "id": "MDRW2100020907.1.18",
          "form": "저 진짜 언젠가 카페를 차리면 그 메뉴를 넣을까 생각했거든요",
          "original_form": "저 진짜 언젠가 카페를 차리면 그 메뉴를 넣을까 생각했거든요",
          "speaker_id": "2",
          "time": "20210829 11:02"
        },
        {
          "id": "MDRW2100020907.1.19",
          "form": "카야잼은 버터와만 먹어봤어오",
          "original_form": "카야잼은 버터와만 먹어봤어오",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100020907.1.20",
          "form": "휘핑크림 잘 어울릴것 같아요!!",
          "original_form": "휘핑크림 잘 어울릴것 같아요!!",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100020907.1.21",
          "form": "우와 버터랑은 어떤가요? 완전 맛있을 것 같네요",
          "original_form": "우와 버터랑은 어떤가요? 완전 맛있을 것 같네요",
          "speaker_id": "2",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100020907.1.22",
          "form": "그 앙버터같은 조합이에요",
          "original_form": "그 앙버터같은 조합이에요",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100020907.1.23",
          "form": "팥 대신 카야잼., 그리고 버터는 앙버터처럼 슬라이해서 끼운 건데",
          "original_form": "팥 대신 카야잼., 그리고 버터는 앙버터처럼 슬라이해서 끼운 건데",
          "speaker_id": "1",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100020907.1.24",
          "form": "앙버터서 팥의 묵직함을 걷은 느낌?  어울려요",
          "original_form": "앙버터서 팥의 묵직함을 걷은 느낌?  어울려요",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100020907.1.25",
          "form": "아 그 버터구나!!! 맛있겠네요.. 참고 하겠어요 하하하",
          "original_form": "아 그 버터구나!!! 맛있겠네요.. 참고 하겠어요 하하하",
          "speaker_id": "2",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100020907.1.26",
          "form": "오늘의 점심은 무엇인가요?",
          "original_form": "오늘의 점심은 무엇인가요?",
          "speaker_id": "2",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100020907.1.27",
          "form": "저는 고기를 구워먹을까 생각 중입니다",
          "original_form": "저는 고기를 구워먹을까 생각 중입니다",
          "speaker_id": "2",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100020907.1.28",
          "form": "네네ㅎ 전 앙버터보다 얘를 먼저 알았어요",
          "original_form": "네네ㅎ 전 앙버터보다 얘를 먼저 알았어요",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100020907.1.29",
          "form": "아앗 부러워요ㅜ",
          "original_form": "아앗 부러워요ㅜ",
          "speaker_id": "1",
          "time": "20210829 11:19"
        }
      ]
