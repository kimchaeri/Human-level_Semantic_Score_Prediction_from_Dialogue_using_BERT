      "utterance": [
        {
          "id": "MDRW2100006028.1.1",
          "form": "가고 싶은 여행지는?",
          "original_form": "가고 싶은 여행지는?",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100006028.1.2",
          "form": "패러글라이딩 탈거야",
          "original_form": "패러글라이딩 탈거야",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100006028.1.3",
          "form": "여행지인데 왜 패러글라이딩 탄다고 답변한거야?",
          "original_form": "여행지인데 왜 패러글라이딩 탄다고 답변한거야?",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100006028.1.4",
          "form": "패러글라이딩도 여행지지 ㅡㅡ",
          "original_form": "패러글라이딩도 여행지지 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100006028.1.5",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006028.1.6",
          "form": "할 때 제일 편안 해 지는 건 뭐야?",
          "original_form": "할 때 제일 편안 해 지는 건 뭐야?",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006028.1.7",
          "form": "잠자는거지 당빠",
          "original_form": "잠자는거지 당빠",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006028.1.8",
          "form": "회사 퇴사하고 모스크바 기차여행 갈래",
          "original_form": "회사 퇴사하고 모스크바 기차여행 갈래",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006028.1.9",
          "form": "우왕 모스크바 좋다 나두 기차여행",
          "original_form": "우왕 모스크바 좋다 나두 기차여행",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100006028.1.10",
          "form": "ㅋㅋ 엄마 데리고 안갈건데?",
          "original_form": "ㅋㅋ 엄마 데리고 안갈건데?",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100006028.1.11",
          "form": "기차여행은 혼자가야 제맛이지",
          "original_form": "기차여행은 혼자가야 제맛이지",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100006028.1.12",
          "form": "난 친구랑 갈거야",
          "original_form": "난 친구랑 갈거야",
          "speaker_id": "1",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100006028.1.13",
          "form": "엄마 친구들 다 바쁘잖나",
          "original_form": "엄마 친구들 다 바쁘잖나",
          "speaker_id": "2",
          "time": "20210907 19:24"
        },
        {
          "id": "MDRW2100006028.1.14",
          "form": "어차피 지금은 어렵고 몇 년 후에 도전 희망인거지",
          "original_form": "어차피 지금은 어렵고 몇 년 후에 도전 희망인거지",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100006028.1.15",
          "form": "그렇긴하지",
          "original_form": "그렇긴하지",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100006028.1.16",
          "form": "참한 남자 중에 나랑 결혼하는 척만 해줬으면 좋겠다",
          "original_form": "참한 남자 중에 나랑 결혼하는 척만 해줬으면 좋겠다",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100006028.1.17",
          "form": "신혼여행인척 휴가쓰고 여행가게",
          "original_form": "신혼여행인척 휴가쓰고 여행가게",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100006028.1.18",
          "form": "헉 신혼여행인척???  그냥 좀 더  지나서 휴가를 모아모아서는 어때??",
          "original_form": "헉 신혼여행인척???  그냥 좀 더  지나서 휴가를 모아모아서는 어때??",
          "speaker_id": "1",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100006028.1.19",
          "form": "아무리 모아도 일주일이상 쓸수가없잖아",
          "original_form": "아무리 모아도 일주일이상 쓸수가없잖아",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100006028.1.20",
          "form": "눈치보여!",
          "original_form": "눈치보여!",
          "speaker_id": "2",
          "time": "20210907 19:27"
        }
      ]
