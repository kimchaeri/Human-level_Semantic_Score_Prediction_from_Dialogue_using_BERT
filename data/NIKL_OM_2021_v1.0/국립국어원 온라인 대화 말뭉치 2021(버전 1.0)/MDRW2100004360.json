      "utterance": [
        {
          "id": "MDRW2100004360.1.1",
          "form": "전 평소에 스포츠 경기 안보고 이번 올림픽 때 줄창 봤는데 name2님은 평소에 관람하는 경기 있으세요?",
          "original_form": "전 평소에 스포츠 경기 안보고 이번 올림픽 때 줄창 봤는데 &name2&님은 평소에 관람하는 경기 있으세요?",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004360.1.2",
          "form": "저도 똑같아요!",
          "original_form": "저도 똑같아요!",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004360.1.3",
          "form": "평소에 스포츠를 그렇게 챙겨보는 편은 아닌데",
          "original_form": "평소에 스포츠를 그렇게 챙겨보는 편은 아닌데",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004360.1.4",
          "form": "올림픽때는 홀린듯이",
          "original_form": "올림픽때는 홀린듯이",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004360.1.5",
          "form": "다챙겨봤네요",
          "original_form": "다챙겨봤네요",
          "speaker_id": "2",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004360.1.6",
          "form": "그쵸그쵸 ㅎㅎ 야구장 티켓도 한 번도 예약 안해봤는데",
          "original_form": "그쵸그쵸 ㅎㅎ 야구장 티켓도 한 번도 예약 안해봤는데",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004360.1.7",
          "form": "재밌게 볼 줄 알았으면 평창 올림픽 때 직관도 가볼걸 그랬어요",
          "original_form": "재밌게 볼 줄 알았으면 평창 올림픽 때 직관도 가볼걸 그랬어요",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004360.1.8",
          "form": "그쵸ㅠㅠㅠ",
          "original_form": "그쵸ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004360.1.9",
          "form": "저는 야구장이나 축구장 한번씩은 가봤는데",
          "original_form": "저는 야구장이나 축구장 한번씩은 가봤는데",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004360.1.10",
          "form": "뭔가 직관은",
          "original_form": "뭔가 직관은",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004360.1.11",
          "form": "애정 없는팀이면 보기 힘들더라구요",
          "original_form": "애정 없는팀이면 보기 힘들더라구요",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004360.1.12",
          "form": "해설 없이 보니까",
          "original_form": "해설 없이 보니까",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004360.1.13",
          "form": "뭐가 뭔지도 모르겠구",
          "original_form": "뭐가 뭔지도 모르겠구",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004360.1.14",
          "form": "이기는건지 지는건지...",
          "original_form": "이기는건지 지는건지...",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004360.1.15",
          "form": "평소에 관심 없어서 규칙같은거 잘 모르면 더 그럴 것 같아요ㅠ",
          "original_form": "평소에 관심 없어서 규칙같은거 잘 모르면 더 그럴 것 같아요ㅠ",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004360.1.16",
          "form": "리그 운영 방식도 잘 모르겠더라구요ㅎㅎㅎ",
          "original_form": "리그 운영 방식도 잘 모르겠더라구요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004360.1.17",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004360.1.18",
          "form": "역도 보는데",
          "original_form": "역도 보는데",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004360.1.19",
          "form": "친구들이랑 저게 도대체 룰이 뭐지",
          "original_form": "친구들이랑 저게 도대체 룰이 뭐지",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004360.1.20",
          "form": "왜저러는거지",
          "original_form": "왜저러는거지",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004360.1.21",
          "form": "이런 말만 10번했던것같아요",
          "original_form": "이런 말만 10번했던것같아요",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004360.1.22",
          "form": "사실 야구장에 관람은 아니고 알바하러 가서 슬쩍슬쩍 보는데",
          "original_form": "사실 야구장에 관람은 아니고 알바하러 가서 슬쩍슬쩍 보는데",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004360.1.23",
          "form": "응원가 같이 따라하는거 그것만 신나더라구요",
          "original_form": "응원가 같이 따라하는거 그것만 신나더라구요",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004360.1.24",
          "form": "연고지 팀이 있으면 같이 묻어갈 수 있는것 같아요",
          "original_form": "연고지 팀이 있으면 같이 묻어갈 수 있는것 같아요",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004360.1.25",
          "form": "마자요 마자요",
          "original_form": "마자요 마자요",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004360.1.26",
          "form": "응원 분위기는 좋죠",
          "original_form": "응원 분위기는 좋죠",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004360.1.27",
          "form": "다같이 신나게 즐기는 그 분위기가 참 좋은것같아요",
          "original_form": "다같이 신나게 즐기는 그 분위기가 참 좋은것같아요",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004360.1.28",
          "form": "그래서 친구들이랑 같이 직관가보고 싶었는데 그게 정말 쉽지 않네요ㅠ",
          "original_form": "그래서 친구들이랑 같이 직관가보고 싶었는데 그게 정말 쉽지 않네요ㅠ",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004360.1.29",
          "form": "ㅠㅠㅠㅠ 그러니까요",
          "original_form": "ㅠㅠㅠㅠ 그러니까요",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004360.1.30",
          "form": "코로나 끝나는건 기대도 안하고",
          "original_form": "코로나 끝나는건 기대도 안하고",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004360.1.31",
          "form": "백신이나 빨리 맞고싶네요",
          "original_form": "백신이나 빨리 맞고싶네요",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004360.1.32",
          "form": "혹시 올림픽 보면서 해보고 싶은 스포츠 경기는 없으셨어요?",
          "original_form": "혹시 올림픽 보면서 해보고 싶은 스포츠 경기는 없으셨어요?",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004360.1.33",
          "form": "저는 테니스요!",
          "original_form": "저는 테니스요!",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004360.1.34",
          "form": "올림픽에 테니스가 있었는지...",
          "original_form": "올림픽에 테니스가 있었는지...",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004360.1.35",
          "form": "는 기억이 안나긴하지만",
          "original_form": "는 기억이 안나긴하지만",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004360.1.36",
          "form": "아무튼 베드민턴이나 테니스?",
          "original_form": "아무튼 베드민턴이나 테니스?",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004360.1.37",
          "form": "하고싶더라구요",
          "original_form": "하고싶더라구요",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004360.1.38",
          "form": "오 아무래도 직접 하기에 가까운 운동이긴 하죠",
          "original_form": "오 아무래도 직접 하기에 가까운 운동이긴 하죠",
          "speaker_id": "1",
          "time": "20210821 18:12"
        }
      ]
    }
