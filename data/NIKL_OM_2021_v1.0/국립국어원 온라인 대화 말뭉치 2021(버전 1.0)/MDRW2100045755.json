      "utterance": [
        {
          "id": "MDRW2100045755.1.1",
          "form": "그쵸 ㅎㅎ 욕이 쫌있지만",
          "original_form": "그쵸 ㅎㅎ 욕이 쫌있지만",
          "speaker_id": "2",
          "time": "20210908 06:41"
        },
        {
          "id": "MDRW2100045755.1.2",
          "form": "렙업도 필요한가봐요",
          "original_form": "렙업도 필요한가봐요",
          "speaker_id": "1",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045755.1.3",
          "form": "티비나 책읽으면돼요",
          "original_form": "티비나 책읽으면돼요",
          "speaker_id": "2",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045755.1.4",
          "form": "머가 이리 힘든지",
          "original_form": "머가 이리 힘든지",
          "speaker_id": "1",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045755.1.5",
          "form": "계속 컨텐츠를 추가했나 봐요",
          "original_form": "계속 컨텐츠를 추가했나 봐요",
          "speaker_id": "2",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045755.1.6",
          "form": "그럴 만하다는",
          "original_form": "그럴 만하다는",
          "speaker_id": "1",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045755.1.7",
          "form": "할 게 되게 많아요",
          "original_form": "할 게 되게 많아요",
          "speaker_id": "2",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045755.1.8",
          "form": "떡 상할 만하네요",
          "original_form": "떡 상할 만하네요",
          "speaker_id": "1",
          "time": "20210908 06:43"
        },
        {
          "id": "MDRW2100045755.1.9",
          "form": "보라색 자동차 예쁘네요",
          "original_form": "보라색 자동차 예쁘네요",
          "speaker_id": "2",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045755.1.10",
          "form": "정비소 도 만드신 듯",
          "original_form": "정비소 도 만드신 듯",
          "speaker_id": "1",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045755.1.11",
          "form": "짐이 무거워 보여요",
          "original_form": "짐이 무거워 보여요",
          "speaker_id": "2",
          "time": "20210908 06:45"
        },
        {
          "id": "MDRW2100045755.1.12",
