      "utterance": [
        {
          "id": "MDRW2100005417.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005417.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005417.1.3",
          "form": "가족분들과 같이 사시나요?",
          "original_form": "가족분들과 같이 사시나요?",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005417.1.4",
          "form": "아뇨! 저는 자취하면서 반려동물 키우고있어요",
          "original_form": "아뇨! 저는 자취하면서 반려동물 키우고있어요",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005417.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005417.1.6",
          "form": "지금은 가족들이랑 사는데 조금 있으면 기숙사로 가요",
          "original_form": "지금은 가족들이랑 사는데 조금 있으면 기숙사로 가요",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005417.1.7",
          "form": "자취 부러워요",
          "original_form": "자취 부러워요",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005417.1.8",
          "form": "어떤 동물 키우세요?",
          "original_form": "어떤 동물 키우세요?",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005417.1.9",
          "form": "앗 대학생이신가봐요!ㅎㅎ",
          "original_form": "앗 대학생이신가봐요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005417.1.10",
          "form": "저는 강아지 키우고있어요!ㅎㅎ",
          "original_form": "저는 강아지 키우고있어요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005417.1.11",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005417.1.12",
          "form": "강아지 한 마리랑 둘이 사시는거예요?",
          "original_form": "강아지 한 마리랑 둘이 사시는거예요?",
          "speaker_id": "1",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005417.1.13",
          "form": "완전 워너비네요",
          "original_form": "완전 워너비네요",
          "speaker_id": "1",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005417.1.14",
          "form": "네네 강아지랑 저랑 둘이 살아요!ㅎㅎㅎ",
          "original_form": "네네 강아지랑 저랑 둘이 살아요!ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005417.1.15",
          "form": "그쵸 저도 나름 만족하고있어요!",
          "original_form": "그쵸 저도 나름 만족하고있어요!",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005417.1.16",
          "form": "자취하다보니 한번씩 외로울때도 있는데",
          "original_form": "자취하다보니 한번씩 외로울때도 있는데",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005417.1.17",
          "form": "강아지가 있어서 그나마 덜 외롭더라구요",
          "original_form": "강아지가 있어서 그나마 덜 외롭더라구요",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005417.1.18",
          "form": "그러겠어요 제가 아는 사람도 둘리 님이랑 똑같은데 둘이 서로한테 분리불안있다고 하더라고요ㅋㅋ",
          "original_form": "그러겠어요 제가 아는 사람도 둘리 님이랑 똑같은데 둘이 서로한테 분리불안있다고 하더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005417.1.19",
          "form": "앗ㅋㅋㅋㅋ 맞아요",
          "original_form": "앗ㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005417.1.20",
          "form": "서로서로 분리불안 있는 느낌이에요ㅋㅋ",
          "original_form": "서로서로 분리불안 있는 느낌이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005417.1.21",
          "form": "저도 집에 오래있는편이라 강아지도 제가 없으면 심심하고 그런가보더라구요",
          "original_form": "저도 집에 오래있는편이라 강아지도 제가 없으면 심심하고 그런가보더라구요",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005417.1.22",
          "form": "저는 고양이 좋아해서 언젠가 돈 많이 벌면 고양이랑 둘이 살고 싶다고 생각했었는데",
          "original_form": "저는 고양이 좋아해서 언젠가 돈 많이 벌면 고양이랑 둘이 살고 싶다고 생각했었는데",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005417.1.23",
          "form": "요즘엔 그런 생각 없어졌어요 전 저 스스로나 잘 키워야 할 것 같아서ㅋㅋ",
          "original_form": "요즘엔 그런 생각 없어졌어요 전 저 스스로나 잘 키워야 할 것 같아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005417.1.24",
          "form": "앗ㅋㅋㅋㅋㅋ 그것도 그래요",
          "original_form": "앗ㅋㅋㅋㅋㅋ 그것도 그래요",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005417.1.25",
          "form": "저도 강아지 키우지만",
          "original_form": "저도 강아지 키우지만",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005417.1.26",
          "form": "제 몸하나 간수하기 힘들더라구요 ㅋㅋㅋㅋ",
          "original_form": "제 몸하나 간수하기 힘들더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005417.1.27",
          "form": "고양이 키우는것도",
          "original_form": "고양이 키우는것도",
          "speaker_id": "2",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005417.1.28",
          "form": "정말 로망이긴하네요",
          "original_form": "정말 로망이긴하네요",
          "speaker_id": "2",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005417.1.29",
          "form": "제가 다니는 학교에 고양이들이 많거든요",
          "original_form": "제가 다니는 학교에 고양이들이 많거든요",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005417.1.30",
          "form": "걔네들 없으면 학교생활 못 해요",
          "original_form": "걔네들 없으면 학교생활 못 해요",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005417.1.31",
          "form": "오오ㅎㅎ",
          "original_form": "오오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:50"
        }
      ]
    }
