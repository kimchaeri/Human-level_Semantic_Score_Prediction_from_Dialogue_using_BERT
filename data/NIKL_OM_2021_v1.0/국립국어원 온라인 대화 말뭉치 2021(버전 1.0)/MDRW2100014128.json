      "utterance": [
        {
          "id": "MDRW2100014128.1.1",
          "form": "비가 너무 많이 와",
          "original_form": "비가 너무 많이 와",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014128.1.2",
          "form": "장마라서 그래",
          "original_form": "장마라서 그래",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014128.1.3",
          "form": "태풍이 또 올라올거 같아",
          "original_form": "태풍이 또 올라올거 같아",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014128.1.4",
          "form": "끙 태풍이랑 장마랑 바통터치를 했어",
          "original_form": "끙 태풍이랑 장마랑 바통터치를 했어",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014128.1.5",
          "form": "근데 태풍이 또 온다고?",
          "original_form": "근데 태풍이 또 온다고?",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014128.1.6",
          "form": "동남아 날씨처럼 우리나라도",
          "original_form": "동남아 날씨처럼 우리나라도",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014128.1.7",
          "form": "변한거야",
          "original_form": "변한거야",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014128.1.8",
          "form": "태풍은 또 오겠지",
          "original_form": "태풍은 또 오겠지",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014128.1.9",
          "form": "안오면 정말 좋겠다",
          "original_form": "안오면 정말 좋겠다",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014128.1.10",
          "form": "한창 더울 여름에 좀 오지",
          "original_form": "한창 더울 여름에 좀 오지",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014128.1.11",
          "form": "장마로는 비상근무 안서나?",
          "original_form": "장마로는 비상근무 안서나?",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014128.1.12",
          "form": "강수량 기준이 있어.",
          "original_form": "강수량 기준이 있어.",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014128.1.13",
          "form": "근데 태풍급이 아니면 보통 비상근무를 안서지",
          "original_form": "근데 태풍급이 아니면 보통 비상근무를 안서지",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014128.1.14",
          "form": "비오는 날은 집에서 라면 끓여먹는게 좋은데",
          "original_form": "비오는 날은 집에서 라면 끓여먹는게 좋은데",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014128.1.15",
          "form": "부산은 태풍 때만큼 비가 오는 거 같아",
          "original_form": "부산은 태풍 때만큼 비가 오는 거 같아",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014128.1.16",
          "form": "오늘 날씨에 밖에 다니면 습해서 옷이 땀에 젖을거 같아",
          "original_form": "오늘 날씨에 밖에 다니면 습해서 옷이 땀에 젖을거 같아",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014128.1.17",
          "form": "부산은 많이 온다고 뉴스났다",
          "original_form": "부산은 많이 온다고 뉴스났다",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014128.1.18",
          "form": "응 비는 많이 오는데 시원하진 않은듯",
          "original_form": "응 비는 많이 오는데 시원하진 않은듯",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014128.1.19",
          "form": "부산 며칠 전에도 잠겼는데 또 잠기겠다",
          "original_form": "부산 며칠 전에도 잠겼는데 또 잠기겠다",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014128.1.20",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014128.1.21",
          "form": "울산도 태풍와서 태화시장 침수됐다",
          "original_form": "울산도 태풍와서 태화시장 침수됐다",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014128.1.22",
          "form": "상습 침수지약이야",
          "original_form": "상습 침수지약이야",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014128.1.23",
          "form": "거기 뉴스나왔던데",
          "original_form": "거기 뉴스나왔던데",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014128.1.24",
          "form": "지금은 비가 그치고 하늘이 노랗다",
          "original_form": "지금은 비가 그치고 하늘이 노랗다",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014128.1.25",
          "form": "하늘이 노랗군",
          "original_form": "하늘이 노랗군",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014128.1.26",
          "form": "빨간 건가 노란 건가",
          "original_form": "빨간 건가 노란 건가",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014128.1.27",
          "form": "노랗다",
          "original_form": "노랗다",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014128.1.28",
          "form": "오늘 넘 피곤하군",
          "original_form": "오늘 넘 피곤하군",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014128.1.29",
          "form": "화가 난다",
          "original_form": "화가 난다",
          "speaker_id": "2",
          "time": "20210825 19:06"
        }
      ]
    }
