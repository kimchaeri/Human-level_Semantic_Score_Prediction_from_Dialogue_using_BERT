      "utterance": [
        {
          "id": "MDRW2100021454.1.1",
          "form": "입추도 지났는데 오늘 엄청 더웠어",
          "original_form": "입추도 지났는데 오늘 엄청 더웠어",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021454.1.2",
          "form": "나 오늘 잠깐 나갔다왔는데 땀범벅",
          "original_form": "나 오늘 잠깐 나갔다왔는데 땀범벅",
          "speaker_id": "2",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021454.1.3",
          "form": "그치 무슨 해가...울산시 캘리포니아인줄...뜨거워 죽음",
          "original_form": "그치 무슨 해가...울산시 캘리포니아인줄...뜨거워 죽음",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021454.1.4",
          "form": "아니 가을 아니냐고 이제ㅠㅠㅠㅠ",
          "original_form": "아니 가을 아니냐고 이제ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100021454.1.5",
          "form": "그러니까..아직도 엄청 습해ㅠ",
          "original_form": "그러니까..아직도 엄청 습해ㅠ",
          "speaker_id": "1",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100021454.1.6",
          "form": "ㅠ에어컨 그만 틀고싶어요...",
          "original_form": "ㅠ에어컨 그만 틀고싶어요...",
          "speaker_id": "2",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100021454.1.7",
          "form": "맞아 안틀면 덥고 틀면추운거 알지",
          "original_form": "맞아 안틀면 덥고 틀면추운거 알지",
          "speaker_id": "1",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100021454.1.8",
          "form": "맞아 안틀면 더운데 틀고있으면 추워 또ㅠㅠㅠ 너무 싫어",
          "original_form": "맞아 안틀면 더운데 틀고있으면 추워 또ㅠㅠㅠ 너무 싫어",
          "speaker_id": "2",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100021454.1.9",
          "form": "맞아 ㅠㅠ살기좋은 봄이랑 가을으느 너무 짧고 여름겨울만 길어졌어",
          "original_form": "맞아 ㅠㅠ살기좋은 봄이랑 가을으느 너무 짧고 여름겨울만 길어졌어",
          "speaker_id": "1",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100021454.1.10",
          "form": "맞아 봄이랑 가을이 길었으면 좋겠어 진짜로",
          "original_form": "맞아 봄이랑 가을이 길었으면 좋겠어 진짜로",
          "speaker_id": "2",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100021454.1.11",
          "form": "언니는 봄이 제일 좋아?",
          "original_form": "언니는 봄이 제일 좋아?",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021454.1.12",
          "form": "그치그치 여름이랑 겨울은 한 3주만 있으면 좋을거같아",
          "original_form": "그치그치 여름이랑 겨울은 한 3주만 있으면 좋을거같아",
          "speaker_id": "1",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021454.1.13",
          "form": "응응 난 봄이 좋아",
          "original_form": "응응 난 봄이 좋아",
          "speaker_id": "1",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021454.1.14",
          "form": "맞아 맞아!! 봄 가을이 길어야해",
          "original_form": "맞아 맞아!! 봄 가을이 길어야해",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021454.1.15",
          "form": "맞아 시원한 바람불때가 좋아",
          "original_form": "맞아 시원한 바람불때가 좋아",
          "speaker_id": "1",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021454.1.16",
          "form": "요즘 날씨가 너무 덥고 습해",
          "original_form": "요즘 날씨가 너무 덥고 습해",
          "speaker_id": "1",
