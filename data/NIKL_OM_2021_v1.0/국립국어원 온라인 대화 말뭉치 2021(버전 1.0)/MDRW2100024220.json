      "utterance": [
        {
          "id": "MDRW2100024220.1.1",
          "form": "샐러드 원래 자주 드세요?",
          "original_form": "샐러드 원래 자주 드세요?",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024220.1.2",
          "form": "최근에는 꽤 자주 먹는 것 같아요",
          "original_form": "최근에는 꽤 자주 먹는 것 같아요",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024220.1.3",
          "form": "건강검진 받고 나니까. 딱히 나쁘다고 말이 나오지도 않았는대",
          "original_form": "건강검진 받고 나니까. 딱히 나쁘다고 말이 나오지도 않았는대",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024220.1.4",
          "form": "전 원래 강력한 육시파였는데",
          "original_form": "전 원래 강력한 육시파였는데",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024220.1.5",
          "form": "먹어야할 것 같은 마음이 드는거있죠",
          "original_form": "먹어야할 것 같은 마음이 드는거있죠",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024220.1.6",
          "form": "어느순간 좀 더부룩하고 그래서",
          "original_form": "어느순간 좀 더부룩하고 그래서",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024220.1.7",
          "form": "샐러드를 찾게 되더라고요..",
          "original_form": "샐러드를 찾게 되더라고요..",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024220.1.8",
          "form": "오오 뭔지 알아요ㅋㅋㅋㅋ",
          "original_form": "오오 뭔지 알아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024220.1.9",
          "form": "와 저두요저두요...",
          "original_form": "와 저두요저두요...",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024220.1.10",
          "form": "위장이 이쯤에서 채소가 들어오지 않는다면 파업하겠다!하고 협박하는 것 같아요...",
          "original_form": "위장이 이쯤에서 채소가 들어오지 않는다면 파업하겠다!하고 협박하는 것 같아요...",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024220.1.11",
          "form": "근데 의사들마다 말이 달라요",
          "original_form": "근데 의사들마다 말이 달라요",
          "speaker_id": "1",
          "time": "20210831 00:23"
        },
        {
          "id": "MDRW2100024220.1.12",
          "form": "또 생야채가 위에 부담된다는 말도 있고..",
          "original_form": "또 생야채가 위에 부담된다는 말도 있고..",
          "speaker_id": "1",
          "time": "20210831 00:23"
        },
        {
          "id": "MDRW2100024220.1.13",
          "form": "아 맞아요!익힌 야채가 좋다고 하긴하더라구요",
          "original_form": "아 맞아요!익힌 야채가 좋다고 하긴하더라구요",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024220.1.14",
          "form": "근데 안먹는거보다는...낫지 않을까요?",
          "original_form": "근데 안먹는거보다는...낫지 않을까요?",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024220.1.15",
          "form": "그...렇겠죠?",
          "original_form": "그...렇겠죠?",
          "speaker_id": "1",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024220.1.16",
          "form": "물에 빠진 야채 별로예요ㅠ",
          "original_form": "물에 빠진 야채 별로예요ㅠ",
          "speaker_id": "1",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024220.1.17",
          "form": "물컹거리고",
          "original_form": "물컹거리고",
          "speaker_id": "1",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024220.1.18",
          "form": "그러면 볶아먹어야하는데 ㅋㅋㅋ그럼또 기름쓰고",
          "original_form": "그러면 볶아먹어야하는데 ㅋㅋㅋ그럼또 기름쓰고",
          "speaker_id": "1",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024220.1.19",
          "form": "맞아요!!기름에ㅜ지지는게 더 몸에 안 좋을 것 같은데 ㅠ",
          "original_form": "맞아요!!기름에ㅜ지지는게 더 몸에 안 좋을 것 같은데 ㅠ",
          "speaker_id": "2",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100024220.1.20",
          "form": "데쳐서 먹는건",
          "original_form": "데쳐서 먹는건",
          "speaker_id": "2",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100024220.1.21",
          "form": "브로콜리만 좋아요",
          "original_form": "브로콜리만 좋아요",
          "speaker_id": "2",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100024220.1.22",
          "form": "으악ㅋㅋㅋㅋ저는 당근 좋아요",
          "original_form": "으악ㅋㅋㅋㅋ저는 당근 좋아요",
          "speaker_id": "1",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100024220.1.23",
          "form": "김밥에 당근이랑 계란 왕창들어가면 존맛",
          "original_form": "김밥에 당근이랑 계란 왕창들어가면 존맛",
          "speaker_id": "1",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024220.1.24",
          "form": "헐헐 너무 맛있겠어요",
          "original_form": "헐헐 너무 맛있겠어요",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024220.1.25",
          "form": "야밤에 김밥 먹구 싶어요...",
          "original_form": "야밤에 김밥 먹구 싶어요...",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024220.1.26",
          "form": "김밥 좋아하세요?",
          "original_form": "김밥 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100024220.1.27",
          "form": "전 진짜 좋아해요",
          "original_form": "전 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100024220.1.28",
          "form": "세줄도 먹을수있어요 배터지지만",
          "original_form": "세줄도 먹을수있어요 배터지지만",
          "speaker_id": "1",
          "time": "20210831 00:32"
        }
      ]
    }
