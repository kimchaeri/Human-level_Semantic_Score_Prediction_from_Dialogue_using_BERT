      "utterance": [
        {
          "id": "MDRW2100035014.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100035014.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100035014.1.3",
          "form": "마스크스트랩은 6천원인데\n배송비가 또 붙더라고요.\n\n별로 안사고싶엇어요",
          "original_form": "마스크스트랩은 6천원인데\n배송비가 또 붙더라고요.\n\n별로 안사고싶엇어요",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100035014.1.4",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100035014.1.5",
          "form": "실리콘재질이라 너무 아끼면 안되겟더라고요.",
          "original_form": "실리콘재질이라 너무 아끼면 안되겟더라고요.",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100035014.1.6",
          "form": "홀맨이 아니더라도\n다른 마스크스트랩들도 넘 비싼거같아요",
          "original_form": "홀맨이 아니더라도\n다른 마스크스트랩들도 넘 비싼거같아요",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100035014.1.7",
          "form": "열심히 씁시당.",
          "original_form": "열심히 씁시당.",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100035014.1.8",
          "form": "그래야겠어요\n사실 물건 아껴도 나중에 쓰지 않더라구요\n있을때 바로 쓰는게 제일 좋은거같아요",
          "original_form": "그래야겠어요\n사실 물건 아껴도 나중에 쓰지 않더라구요\n있을때 바로 쓰는게 제일 좋은거같아요",
          "speaker_id": "2",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100035014.1.9",
          "form": "저는 실리콘재질이면 얼른써버리는편이에요",
          "original_form": "저는 실리콘재질이면 얼른써버리는편이에요",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100035014.1.10",
          "form": "실리콘재질은 빨리 쓰느게 정답이군요",
          "original_form": "실리콘재질은 빨리 쓰느게 정답이군요",
          "speaker_id": "2",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100035014.1.11",
          "form": "나중에 기름같은게 뜨더라거여\n 동생이 너무 아끼길래 얼른 뜯으라고 했엇어요.",
          "original_form": "나중에 기름같은게 뜨더라거여\n 동생이 너무 아끼길래 얼른 뜯으라고 했엇어요.",
          "speaker_id": "1",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100035014.1.12",
          "form": "저도 그냥 꺼내써야겠네요\n아낄 필요가 없겠어요",
          "original_form": "저도 그냥 꺼내써야겠네요\n아낄 필요가 없겠어요",
          "speaker_id": "2",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100035014.1.13",
          "form": "ㅋㅋㅋㅋ 그리거 볼펜도 열심히쓰세여..\nㅋㅋㅋ 잘나오고 귀엽고 좋아요",
          "original_form": "ㅋㅋㅋㅋ 그리거 볼펜도 열심히쓰세여..\nㅋㅋㅋ 잘나오고 귀엽고 좋아요",
          "speaker_id": "1",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100035014.1.14",
          "form": "벌써 써보셨군요!",
          "original_form": "벌써 써보셨군요!",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100035014.1.15",
          "form": "나중에 안나오면 맘아파요..",
          "original_form": "나중에 안나오면 맘아파요..",
          "speaker_id": "1",
          "time": "20210829 00:14"
        }
      ]
