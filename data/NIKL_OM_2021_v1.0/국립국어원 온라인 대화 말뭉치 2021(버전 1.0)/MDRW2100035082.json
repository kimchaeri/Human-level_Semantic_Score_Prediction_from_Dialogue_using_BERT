      "utterance": [
        {
          "id": "MDRW2100035082.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210829 02:13"
        },
        {
          "id": "MDRW2100035082.1.2",
          "form": "내가 보는 고양이가 아기를 낳았어",
          "original_form": "내가 보는 고양이가 아기를 낳았어",
          "speaker_id": "1",
          "time": "20210829 02:14"
        },
        {
          "id": "MDRW2100035082.1.3",
          "form": "아기 고양이 귀엽겠다",
          "original_form": "아기 고양이 귀엽겠다",
          "speaker_id": "2",
          "time": "20210829 02:14"
        },
        {
          "id": "MDRW2100035082.1.4",
          "form": "그래서 유튜브에서 인기가 엄청 많아",
          "original_form": "그래서 유튜브에서 인기가 엄청 많아",
          "speaker_id": "1",
          "time": "20210829 02:14"
        },
        {
          "id": "MDRW2100035082.1.5",
          "form": "어떤 유튜브 인데",
          "original_form": "어떤 유튜브 인데",
          "speaker_id": "2",
          "time": "20210829 02:14"
        },
        {
          "id": "MDRW2100035082.1.6",
          "form": "하하하라는 유튜번데 시골에 사는 사람이야",
          "original_form": "하하하라는 유튜번데 시골에 사는 사람이야",
          "speaker_id": "1",
          "time": "20210829 02:14"
        },
        {
          "id": "MDRW2100035082.1.7",
          "form": "무슨 색깔 고양이야",
          "original_form": "무슨 색깔 고양이야",
          "speaker_id": "2",
          "time": "20210829 02:15"
        },
        {
          "id": "MDRW2100035082.1.8",
          "form": "일단 여러마린데 엄마는 삼색이거든",
          "original_form": "일단 여러마린데 엄마는 삼색이거든",
          "speaker_id": "1",
          "time": "20210829 02:15"
        },
        {
          "id": "MDRW2100035082.1.9",
          "form": "어 그렇구나 많이 나왔네",
          "original_form": "어 그렇구나 많이 나왔네",
          "speaker_id": "2",
          "time": "20210829 02:15"
        },
        {
          "id": "MDRW2100035082.1.10",
          "form": "애들이 그래서 색이 무척 다양해 궁금하지",
          "original_form": "애들이 그래서 색이 무척 다양해 궁금하지",
          "speaker_id": "1",
          "time": "20210829 02:15"
        },
        {
          "id": "MDRW2100035082.1.11",
          "form": "삼색이 자식이면 삼재 기가 많이 있나",
          "original_form": "삼색이 자식이면 삼재 기가 많이 있나",
          "speaker_id": "2",
          "time": "20210829 02:15"
        },
        {
          "id": "MDRW2100035082.1.12",
          "form": "나도 그럴줄 알았는데 흰색이 있더라니까",
          "original_form": "나도 그럴줄 알았는데 흰색이 있더라니까",
          "speaker_id": "1",
          "time": "20210829 02:15"
        },
        {
          "id": "MDRW2100035082.1.13",
          "form": "흰색 고양이가 있어?",
          "original_form": "흰색 고양이가 있어?",
          "speaker_id": "2",
          "time": "20210829 02:16"
        },
        {
          "id": "MDRW2100035082.1.14",
          "form": "심지어 노란 치즈냥이도 있어",
          "original_form": "심지어 노란 치즈냥이도 있어",
          "speaker_id": "1",
          "time": "20210829 02:16"
        },
        {
          "id": "MDRW2100035082.1.15",
          "form": "치주 낭이 너무 귀여워",
          "original_form": "치주 낭이 너무 귀여워",
          "speaker_id": "2",
          "time": "20210829 02:16"
        },
        {
          "id": "MDRW2100035082.1.16",
