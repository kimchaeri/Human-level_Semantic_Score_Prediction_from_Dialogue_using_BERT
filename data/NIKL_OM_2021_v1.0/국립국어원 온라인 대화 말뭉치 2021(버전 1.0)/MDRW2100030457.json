      "utterance": [
        {
          "id": "MDRW2100030457.1.1",
          "form": "남자가 봐도 그런가요ㅋㅋㅋㅋ",
          "original_form": "남자가 봐도 그런가요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030457.1.2",
          "form": "그 남자가 어딜 봐서 40 일까요...서른이래도 믿겠구만ㅋㅋ",
          "original_form": "그 남자가 어딜 봐서 40 일까요...서른이래도 믿겠구만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030457.1.3",
          "form": "진짜 연기 너무 잘해요",
          "original_form": "진짜 연기 너무 잘해요",
          "speaker_id": "1",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100030457.1.4",
          "form": "깐족 대마왕",
          "original_form": "깐족 대마왕",
          "speaker_id": "1",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100030457.1.5",
          "form": "오늘 밤에 꼭 정주행 하겠어요!ㅋㅋ",
          "original_form": "오늘 밤에 꼭 정주행 하겠어요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:38"
        },
        {
          "id": "MDRW2100030457.1.6",
          "form": "네 보면 킬링파트 하나 있어요",
          "original_form": "네 보면 킬링파트 하나 있어요",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030457.1.7",
          "form": "바로 아실꺼에요",
          "original_form": "바로 아실꺼에요",
          "speaker_id": "1",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100030457.1.8",
          "form": "디피 1편까지 봤는데 구교환 머리카락 한올 안나오네여ㅋㅋㅋㅋ",
          "original_form": "디피 1편까지 봤는데 구교환 머리카락 한올 안나오네여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:05"
        },
        {
          "id": "MDRW2100030457.1.9",
          "form": "2편을 보시면",
          "original_form": "2편을 보시면",
          "speaker_id": "1",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100030457.1.10",
          "form": "나오겠쥬",
          "original_form": "나오겠쥬",
          "speaker_id": "1",
          "time": "20210904 21:40"
        },
        {
          "id": "MDRW2100030457.1.11",
          "form": "2편 구교환 첫등장 강렬하네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "2편 구교환 첫등장 강렬하네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100030457.1.12",
          "form": "ㅋㅋ2편인가요",
          "original_form": "ㅋㅋ2편인가요",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100030457.1.13",
          "form": "한!마!디!",
          "original_form": "한!마!디!",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100030457.1.14",
          "form": "깐족깐족ㅋㅋㅋㅋㅋ",
          "original_form": "깐족깐족ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:21"
        },
        {
          "id": "MDRW2100030457.1.15",
          "form": "한마디",
          "original_form": "한마디",
          "speaker_id": "1",
          "time": "20210905 00:23"
        },
        {
          "id": "MDRW2100030457.1.16",
          "form": "너무 매력터지죠ㅋㅋ",
          "original_form": "너무 매력터지죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100030457.1.17",
          "form": "구교환한테 빠져서 정해인은 눈에 들어오지도 않았어요ㅋㅋ",
          "original_form": "구교환한테 빠져서 정해인은 눈에 들어오지도 않았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:29"
        },
        {
          "id": "MDRW2100030457.1.18",
          "form": "상병한~호여ㄹ!",
          "original_form": "상병한~호여ㄹ!",
          "speaker_id": "1",
          "time": "20210905 00:37"
        },
        {
          "id": "MDRW2100030457.1.19",
          "form": "호열이 얄미운데 귀여워요ㅋㅋㅋㅋ",
          "original_form": "호열이 얄미운데 귀여워요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100030457.1.20",
          "form": "ㅋㅋ진짜 역대급 캐릭터 찍은듯요",
          "original_form": "ㅋㅋ진짜 역대급 캐릭터 찍은듯요",
