      "utterance": [
        {
          "id": "MDRW2100036875.1.1",
          "form": "어떤게 주로 거슬리세요",
          "original_form": "어떤게 주로 거슬리세요",
          "speaker_id": "2",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100036875.1.2",
          "form": "생긴거요",
          "original_form": "생긴거요",
          "speaker_id": "1",
          "time": "20210831 14:20"
        },
        {
          "id": "MDRW2100036875.1.3",
          "form": "외모",
          "original_form": "외모",
          "speaker_id": "1",
          "time": "20210831 14:20"
        },
        {
          "id": "MDRW2100036875.1.4",
          "form": "그리고 먹는 모습",
          "original_form": "그리고 먹는 모습",
          "speaker_id": "1",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100036875.1.5",
          "form": "식탐많은거싫어요",
          "original_form": "식탐많은거싫어요",
          "speaker_id": "1",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100036875.1.6",
          "form": "음침한 눈빛도 싫어요",
          "original_form": "음침한 눈빛도 싫어요",
          "speaker_id": "1",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100036875.1.7",
          "form": "외모...중요하죠",
          "original_form": "외모...중요하죠",
          "speaker_id": "2",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100036875.1.8",
          "form": "ㅋㅋㅋ 눈빛.ㅠㅠ 하...참",
          "original_form": "ㅋㅋㅋ 눈빛.ㅠㅠ 하...참",
          "speaker_id": "2",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100036875.1.9",
          "form": "못생기면 눈빛도 이상해요",
          "original_form": "못생기면 눈빛도 이상해요",
          "speaker_id": "2",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100036875.1.10",
          "form": "성격도 중요해요 저는 나대는 사람이 싫어요",
          "original_form": "성격도 중요해요 저는 나대는 사람이 싫어요",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036875.1.11",
          "form": "너무 직진하는 사람도 별로",
          "original_form": "너무 직진하는 사람도 별로",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036875.1.12",
          "form": "어디서 그런 근자감이 나오는지요..?",
          "original_form": "어디서 그런 근자감이 나오는지요..?",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036875.1.13",
          "form": "어우 그니까요!!!!",
          "original_form": "어우 그니까요!!!!",
          "speaker_id": "2",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036875.1.14",
          "form": "상대생각안하고 직진 ㅎㅎㄹ",
          "original_form": "상대생각안하고 직진 ㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036875.1.15",
          "form": "얼굴이라도...잘생겼으면...모르겠네요...",
          "original_form": "얼굴이라도...잘생겼으면...모르겠네요...",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036875.1.16",
          "form": "정말 한숨이 푹 나온답니다",
          "original_form": "정말 한숨이 푹 나온답니다",
          "speaker_id": "1",
          "time": "20210831 14:28"
        },
        {
          "id": "MDRW2100036875.1.17",
          "form": "표정도요...휴",
          "original_form": "표정도요...휴",
          "speaker_id": "2",
          "time": "20210831 16:18"
        },
        {
          "id": "MDRW2100036875.1.18",
          "form": "느끼한표정",
          "original_form": "느끼한표정",
          "speaker_id": "2",
          "time": "20210831 16:18"
        },
        {
          "id": "MDRW2100036875.1.19",
          "form": "근데 눈빛 별로다 했는데 알고보니 공황장애여서 약먹고 계셨던 ...사연있는 분도 있더라구요.. 그건 좀 안타까웠죠",
          "original_form": "근데 눈빛 별로다 했는데 알고보니 공황장애여서 약먹고 계셨던 ...사연있는 분도 있더라구요.. 그건 좀 안타까웠죠",
          "speaker_id": "1",
          "time": "20210831 16:24"
        },
        {
          "id": "MDRW2100036875.1.20",
          "form": "사연있는분들이 많죠...",
          "original_form": "사연있는분들이 많죠...",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100036875.1.21",
          "form": "만나시는분이 공황장애로 약먹고 있다하면 기분이 어떨것같아요?? 위로해주실건가요??",
          "original_form": "만나시는분이 공황장애로 약먹고 있다하면 기분이 어떨것같아요?? 위로해주실건가요??",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036875.1.22",
          "form": "음...제가 그걸 잘 몰라서요",
          "original_form": "음...제가 그걸 잘 몰라서요",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036875.1.23",
          "form": "그럼 공황장애아니라",
          "original_form": "그럼 공황장애아니라",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036875.1.24",
          "form": "제가 감당할수 있으면..??",
          "original_form": "제가 감당할수 있으면..??",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036875.1.25",
          "form": "어떤병이든 정신과에서 처방받는다면요",
          "original_form": "어떤병이든 정신과에서 처방받는다면요",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036875.1.26",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036875.1.27",
          "form": "포용력있으시네요...^^",
          "original_form": "포용력있으시네요...^^",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036875.1.28",
          "form": "아.. 음..워낙 흔해서...",
          "original_form": "아.. 음..워낙 흔해서...",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036875.1.29",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036875.1.30",
          "form": "저는 저도 힘들어서 못받아줄거같거든요",
          "original_form": "저는 저도 힘들어서 못받아줄거같거든요",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036875.1.31",
          "form": "경미한 우울증정돈..",
          "original_form": "경미한 우울증정돈..",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036875.1.32",
          "form": "우울증 싫긴하지만 너무 하이텐션인것도 싫고~~",
          "original_form": "우울증 싫긴하지만 너무 하이텐션인것도 싫고~~",
          "speaker_id": "1",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100036875.1.33",
          "form": "저도 뭔가를 해주진 못하니까",
          "original_form": "저도 뭔가를 해주진 못하니까",
          "speaker_id": "2",
          "time": "20210831 16:50"
        },
        {
