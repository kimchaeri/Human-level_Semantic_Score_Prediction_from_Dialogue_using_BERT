      "utterance": [
        {
          "id": "MDRW2100045263.1.1",
          "form": "드큘은 제가 류드큘을 안봐서",
          "original_form": "드큘은 제가 류드큘을 안봐서",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045263.1.2",
          "form": "류드큘을 안본게 너무 아쉬우ㅝ...",
          "original_form": "류드큘을 안본게 너무 아쉬우ㅝ...",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045263.1.3",
          "form": "드큘은 저도.. 넘버 제목조차도 모르겠..",
          "original_form": "드큘은 저도.. 넘버 제목조차도 모르겠..",
          "speaker_id": "1",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045263.1.4",
          "form": "볼때마다 류님과",
          "original_form": "볼때마다 류님과",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045263.1.5",
          "form": "샤가 같은 캐슷이라는게",
          "original_form": "샤가 같은 캐슷이라는게",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045263.1.6",
          "form": "말이됨?",
          "original_form": "말이됨?",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045263.1.7",
          "form": "홍이면몰라도",
          "original_form": "홍이면몰라도",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045263.1.8",
          "form": "에바지",
          "original_form": "에바지",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045263.1.9",
          "form": "아니 애초에",
          "original_form": "아니 애초에",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045263.1.10",
          "form": "그 뭐냐",
          "original_form": "그 뭐냐",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045263.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045263.1.12",
          "form": "배역 넘버에 따른 음역대라는게 있는데",
          "original_form": "배역 넘버에 따른 음역대라는게 있는데",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045263.1.13",
          "form": "그게 안맞을거같은데",
          "original_form": "그게 안맞을거같은데",
          "speaker_id": "1",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045263.1.14",
          "form": "류님은 화내야제맛이지..",
          "original_form": "류님은 화내야제맛이지..",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100045263.1.15",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.16",
          "form": "모차만봐도",
          "original_form": "모차만봐도",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.17",
          "form": "박은태한테 개발리는데",
          "original_form": "박은태한테 개발리는데",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.18",
          "form": "샤는 너무 높지 않나...?",
          "original_form": "샤는 너무 높지 않나...?",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.19",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.20",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.21",
          "form": "진성으로 몬내",
          "original_form": "진성으로 몬내",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.22",
          "form": "못내",
          "original_form": "못내",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.23",
          "form": "박은태 연기는 못봐서 모르겠지만 노래로는",
          "original_form": "박은태 연기는 못봐서 모르겠지만 노래로는",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.24",
          "form": "은태는 연기를...",
          "original_form": "은태는 연기를...",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.25",
          "form": "샤 그냥 털거같은데",
          "original_form": "샤 그냥 털거같은데",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.26",
          "form": "너무못해",
          "original_form": "너무못해",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.28",
          "form": "갖다댈수가없어",
          "original_form": "갖다댈수가없어",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045263.1.29",
          "form": "한때 박은태가 보컬만 4명한테 수업을 받는다그랬거든",
          "original_form": "한때 박은태가 보컬만 4명한테 수업을 받는다그랬거든",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045263.1.30",
          "form": "샤 겟세마네 안되지?",
          "original_form": "샤 겟세마네 안되지?",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045263.1.31",
          "form": "히익",
          "original_form": "히익",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045263.1.32",
          "form": "안되지...",
          "original_form": "안되지...",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045263.1.33",
          "form": "근데 내가 연극보고",
          "original_form": "근데 내가 연극보고",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045263.1.34",
          "form": "은태야 1명만 빼고",
          "original_form": "은태야 1명만 빼고",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045263.1.35",
          "form": "노래대신 연기수업좀...",
          "original_form": "노래대신 연기수업좀...",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045263.1.36",
          "form": "했는데",
          "original_form": "했는데",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045263.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋ겟세마네가 하이g라그랬나..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ겟세마네가 하이g라그랬나..",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045263.1.38",
          "form": "샤가 될리가...",
          "original_form": "샤가 될리가...",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045263.1.39",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045263.1.40",
          "form": "지금쯤이면 연기가.. 늘었길...",
          "original_form": "지금쯤이면 연기가.. 늘었길...",
          "speaker_id": "1",
          "time": "20210908 01:37"
        }
      ]
    }
