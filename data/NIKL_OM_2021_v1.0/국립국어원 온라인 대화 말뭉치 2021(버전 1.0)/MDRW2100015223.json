      "utterance": [
        {
          "id": "MDRW2100015223.1.1",
          "form": "싸이월드 복원하셨어요?",
          "original_form": "싸이월드 복원하셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015223.1.2",
          "form": "아뇨 전 시도 조차 안 해봤어요",
          "original_form": "아뇨 전 시도 조차 안 해봤어요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015223.1.3",
          "form": "아이디도 기억 안 나요ㅋㅋ",
          "original_form": "아이디도 기억 안 나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015223.1.4",
          "form": "저 해봤는대 안되더라구요ㅎㅎㅎ",
          "original_form": "저 해봤는대 안되더라구요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015223.1.5",
          "form": "2015년 이후로 된대요ㅎㅎ",
          "original_form": "2015년 이후로 된대요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015223.1.6",
          "form": "2015년 이후로 로그인 했어야 되는 거예요?",
          "original_form": "2015년 이후로 로그인 했어야 되는 거예요?",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015223.1.7",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015223.1.8",
          "form": "2015년 이후 가입자만 복원되고 그이전은 나중에 차근차근 된다고해ㅛ어요ㅎㅎ",
          "original_form": "2015년 이후 가입자만 복원되고 그이전은 나중에 차근차근 된다고해ㅛ어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015223.1.9",
          "form": "아 그 뜻이었군요. 그럼 한참 걸리겠네요",
          "original_form": "아 그 뜻이었군요. 그럼 한참 걸리겠네요",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015223.1.10",
          "form": "아무래도 데이터가 많으니깐 그렇지 않을까요ㅎ",
          "original_form": "아무래도 데이터가 많으니깐 그렇지 않을까요ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015223.1.11",
          "form": "그당시는 엄청 많이했잖아요ㅎㅎ",
          "original_form": "그당시는 엄청 많이했잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015223.1.12",
          "form": "그니까요. 갑자기 밀려났어요",
          "original_form": "그니까요. 갑자기 밀려났어요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015223.1.13",
          "form": "근데 복원되는 게 좋은걸까요?",
          "original_form": "근데 복원되는 게 좋은걸까요?",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015223.1.14",
          "form": "흑역사가 많겠죠?ㅎㅎㅎ",
          "original_form": "흑역사가 많겠죠?ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015223.1.15",
          "form": "나는 가끔..ㅎ",
          "original_form": "나는 가끔..ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015223.1.16",
          "form": "뭐이런거 있지않을까요? 사진이랑?",
          "original_form": "뭐이런거 있지않을까요? 사진이랑?",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015223.1.17",
          "form": "ㅋㅋㅋ 눈물을 흘린다?ㅋㅋ",
          "original_form": "ㅋㅋㅋ 눈물을 흘린다?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015223.1.18",
          "form": "ㅎㅎㅎ 그때는 대학생때셨어요?",
          "original_form": "ㅎㅎㅎ 그때는 대학생때셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015223.1.19",
          "form": "싸이월드하실때?",
          "original_form": "싸이월드하실때?",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015223.1.20",
          "form": "전 대학때라 지우고싶은기억이ㅠㅠㅍ",
          "original_form": "전 대학때라 지우고싶은기억이ㅠㅠㅍ",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015223.1.21",
          "form": "저도 대학 때 주로",
          "original_form": "저도 대학 때 주로",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015223.1.22",
          "form": "글은 별로 안 썼고 사진은 꽤 있을 거에요",
          "original_form": "글은 별로 안 썼고 사진은 꽤 있을 거에요",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015223.1.23",
          "form": "ㅎㅎ 그때가 리즈시절이었겟죠ㅎㅎ",
          "original_form": "ㅎㅎ 그때가 리즈시절이었겟죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015223.1.24",
          "form": "저도 그때로 돌아가고싶네요ㅠ",
          "original_form": "저도 그때로 돌아가고싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:32"
        }
      ]
    }
  ]
