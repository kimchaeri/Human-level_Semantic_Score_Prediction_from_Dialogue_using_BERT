      "utterance": [
        {
          "id": "MDRW2100029931.1.1",
          "form": "막 카톡할때 친구들이 이상하게 써서 보내면 고쳐주고 싶죠?",
          "original_form": "막 카톡할때 친구들이 이상하게 써서 보내면 고쳐주고 싶죠?",
          "speaker_id": "2",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029931.1.2",
          "form": "윽",
          "original_form": "윽",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029931.1.3",
          "form": "아녀요",
          "original_form": "아녀요",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029931.1.4",
          "form": "시간 외 근무는 안한다는 주의라",
          "original_form": "시간 외 근무는 안한다는 주의라",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029931.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029931.1.6",
          "form": "그리고 저는 편집보다는 작가 쪽에 가까워서",
          "original_form": "그리고 저는 편집보다는 작가 쪽에 가까워서",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029931.1.7",
          "form": "우와 더 멋진데요?",
          "original_form": "우와 더 멋진데요?",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029931.1.8",
          "form": "작가..",
          "original_form": "작가..",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029931.1.9",
          "form": "멋있어요!!",
          "original_form": "멋있어요!!",
          "speaker_id": "2",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100029931.1.10",
          "form": "혹시 그런데는 급여가 어떤지 물어봐도 될까요?",
          "original_form": "혹시 그런데는 급여가 어떤지 물어봐도 될까요?",
          "speaker_id": "2",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100029931.1.11",
          "form": "아 작가보다는 저자인가..",
          "original_form": "아 작가보다는 저자인가..",
          "speaker_id": "1",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100029931.1.12",
          "form": "급여는 음...",
          "original_form": "급여는 음...",
          "speaker_id": "1",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029931.1.13",
          "form": "말씀하시기 어려우시면 안해주셔도 돼요~~",
          "original_form": "말씀하시기 어려우시면 안해주셔도 돼요~~",
          "speaker_id": "2",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029931.1.14",
          "form": "신기해요 책쓰셔도 되겠어요",
          "original_form": "신기해요 책쓰셔도 되겠어요",
          "speaker_id": "2",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029931.1.15",
          "form": "저는 글재주가 없어서",
          "original_form": "저는 글재주가 없어서",
          "speaker_id": "2",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100029931.1.16",
          "form": "말재주도 없지만",
          "original_form": "말재주도 없지만",
          "speaker_id": "2",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100029931.1.17",
          "form": "부러워요ㅎㅎ",
          "original_form": "부러워요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100029931.1.18",
          "form": "중소기업청년대출은 안되는데 재난지원금은 받는정도...?",
          "original_form": "중소기업청년대출은 안되는데 재난지원금은 받는정도...?",
          "speaker_id": "1",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029931.1.19",
          "form": "이 정도면 직접적이진 않겠죠...",
          "original_form": "이 정도면 직접적이진 않겠죠...",
          "speaker_id": "1",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029931.1.20",
          "form": "사실 교재용 글 솜씨는",
          "original_form": "사실 교재용 글 솜씨는",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029931.1.21",
          "form": "기술(?)에 가까워서...",
          "original_form": "기술(?)에 가까워서...",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029931.1.22",
          "form": "네네 굉장히 안직접적이에요ㅋㅋㅋㅋ",
          "original_form": "네네 굉장히 안직접적이에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029931.1.23",
          "form": "기술?",
          "original_form": "기술?",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029931.1.24",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029931.1.25",
          "form": "대단합니다 그냥..",
          "original_form": "대단합니다 그냥..",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029931.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029931.1.27",
          "form": "아니 그 기술이란게",
          "original_form": "아니 그 기술이란게",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029931.1.28",
          "form": "1문단에 어떤 내용 배치",
          "original_form": "1문단에 어떤 내용 배치",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029931.1.29",
          "form": "2문단에 어떻게 받아서 어디까지 설명",
          "original_form": "2문단에 어떻게 받아서 어디까지 설명",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029931.1.30",
          "form": "이런 느낌이라;;",
          "original_form": "이런 느낌이라;;",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029931.1.31",
          "form": "오~ 몇년 정도 하신거예요??",
          "original_form": "오~ 몇년 정도 하신거예요??",
          "speaker_id": "2",
          "time": "20210903 16:51"
