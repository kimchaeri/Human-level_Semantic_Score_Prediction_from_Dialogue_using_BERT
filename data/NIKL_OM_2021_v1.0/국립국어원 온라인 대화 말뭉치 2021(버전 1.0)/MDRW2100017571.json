      "utterance": [
        {
          "id": "MDRW2100017571.1.1",
          "form": "최근에 산 것 중에 맘에 드는 거 있으세요",
          "original_form": "최근에 산 것 중에 맘에 드는 거 있으세요",
          "speaker_id": "1",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017571.1.2",
          "form": "저 폼롤러 사서 요긴하게 쓰고잇어요!",
          "original_form": "저 폼롤러 사서 요긴하게 쓰고잇어요!",
          "speaker_id": "2",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100017571.1.3",
          "form": "아 폼롤러 진짜 하고있으면 피로가 풀리는 느낌이죠",
          "original_form": "아 폼롤러 진짜 하고있으면 피로가 풀리는 느낌이죠",
          "speaker_id": "1",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017571.1.4",
          "form": "네 아픈데 너무 시원해요",
          "original_form": "네 아픈데 너무 시원해요",
          "speaker_id": "2",
          "time": "20210826 20:10"
        },
        {
          "id": "MDRW2100017571.1.5",
          "form": "name1은 최근에 구매한 물건 잇나요?",
          "original_form": "&name1&은 최근에 구매한 물건 잇나요?",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017571.1.6",
          "form": "저는 최근에 가을을 맞이하여 구두를 샀어요",
          "original_form": "저는 최근에 가을을 맞이하여 구두를 샀어요",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017571.1.7",
          "form": "아직 한여름인데",
          "original_form": "아직 한여름인데",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017571.1.8",
          "form": "금방 가을 올거예요 ㅋㅋㅋㅋ",
          "original_form": "금방 가을 올거예요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017571.1.9",
          "form": "무슨색 구두예요?",
          "original_form": "무슨색 구두예요?",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017571.1.10",
          "form": "검은 구두를 샀어요!",
          "original_form": "검은 구두를 샀어요!",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017571.1.11",
          "form": "의외로 검은 구두가 없더라구요 제가",
          "original_form": "의외로 검은 구두가 없더라구요 제가",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017571.1.12",
          "form": "맞아요 저도 없어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 저도 없어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017571.1.13",
          "form": "기본템인데 의외로 없음ㅋㅋㅋㅋ",
          "original_form": "기본템인데 의외로 없음ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017571.1.14",
          "form": "그러니까요 그래서 사야했죠",
          "original_form": "그러니까요 그래서 사야했죠",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017571.1.15",
          "form": "현재는 물욕이 없어서 장바구니 다 비워놨어요",
          "original_form": "현재는 물욕이 없어서 장바구니 다 비워놨어요",
          "speaker_id": "1",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017571.1.16",
          "form": "갑자기 물욕이 없어진 계기가 있나요?",
          "original_form": "갑자기 물욕이 없어진 계기가 있나요?",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100017571.1.17",
          "form": "그냥 사고싶은 물건을 아직 못만나서 그럴까요",
          "original_form": "그냥 사고싶은 물건을 아직 못만나서 그럴까요",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017571.1.18",
          "form": "가을 옷 쇼핑하는데 뭔가 딱 운명처럼 다가오는게 없어서",
          "original_form": "가을 옷 쇼핑하는데 뭔가 딱 운명처럼 다가오는게 없어서",
          "speaker_id": "1",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017571.1.19",
          "form": "맞아요 저도 겨울옷욕심은 많은데 이상하게 가을옷은 안끌려요 ㅋㅋㅋ",
          "original_form": "맞아요 저도 겨울옷욕심은 많은데 이상하게 가을옷은 안끌려요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017571.1.20",
          "form": "그쵸? 얼마 못 입기도 하고...",
          "original_form": "그쵸? 얼마 못 입기도 하고...",
          "speaker_id": "1",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017571.1.21",
          "form": "맞아요 ㅋㅋㅋㅋ어차피 가을 금방지나갈텐데 라는 생각이 들어서 그런가",
          "original_form": "맞아요 ㅋㅋㅋㅋ어차피 가을 금방지나갈텐데 라는 생각이 들어서 그런가",
          "speaker_id": "2",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017571.1.22",
          "form": "네 그래서 요즘은 먹을 거만 많이 사는 중이네요",
          "original_form": "네 그래서 요즘은 먹을 거만 많이 사는 중이네요",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017571.1.23",
          "form": "먹을거 뭐사셧어요? ㅋㅋ",
          "original_form": "먹을거 뭐사셧어요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017571.1.24",
          "form": "저 최근에 군옥수수 아몬드랑 허니버터 아몬드",
          "original_form": "저 최근에 군옥수수 아몬드랑 허니버터 아몬드",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017571.1.25",
          "form": "군옥수수 아몬드 너무 맛잇죠 ㅋㅋㅋㅋ",
          "original_form": "군옥수수 아몬드 너무 맛잇죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017571.1.26",
          "form": "이가 나갈것같은데도 멈출수없어욬ㅋㅋㅋㅋㅋ",
          "original_form": "이가 나갈것같은데도 멈출수없어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017571.1.27",
          "form": "아 그 매력을 아시는군요ㅋㅋㅋㅋㅋ",
          "original_form": "아 그 매력을 아시는군요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017571.1.28",
          "form": "너무 좋아요 이 부서질거같지만",
          "original_form": "너무 좋아요 이 부서질거같지만",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017571.1.29",
          "form": "짭쪼름한데 너무 맛잇어요 ㅋㅋㅋㅋ",
          "original_form": "짭쪼름한데 너무 맛잇어요 ㅋㅋㅋㅋ",
