      "utterance": [
        {
          "id": "MDRW2100005221.1.1",
          "form": "안녕하새요~",
          "original_form": "안녕하새요~",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005221.1.2",
          "form": "안녕하세요~!",
          "original_form": "안녕하세요~!",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005221.1.3",
          "form": "요즘 운동하시나요?",
          "original_form": "요즘 운동하시나요?",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005221.1.4",
          "form": "네 요즘 운동 열심히 하고있어요",
          "original_form": "네 요즘 운동 열심히 하고있어요",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005221.1.5",
          "form": "헬스장가서 근육 열심히 키우고 있답니다",
          "original_form": "헬스장가서 근육 열심히 키우고 있답니다",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005221.1.6",
          "form": "헬스장 가시는 군요 저는 홈트로 하고있어요!",
          "original_form": "헬스장 가시는 군요 저는 홈트로 하고있어요!",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005221.1.7",
          "form": "다이어트하세요? 아니면 건강목적으로 하시는 건가요?",
          "original_form": "다이어트하세요? 아니면 건강목적으로 하시는 건가요?",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005221.1.8",
          "form": "건강목적으로 다이어트 시작했죠 ㅎ",
          "original_form": "건강목적으로 다이어트 시작했죠 ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005221.1.9",
          "form": "운동하니까 확실히 체력이 쑥쑥 오르더라고요",
          "original_form": "운동하니까 확실히 체력이 쑥쑥 오르더라고요",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005221.1.10",
          "form": "홈트는 보통 뭐하시나요?",
          "original_form": "홈트는 보통 뭐하시나요?",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005221.1.11",
          "form": "유튜브로 보면서 간단한 운동 하고있어요 !",
          "original_form": "유튜브로 보면서 간단한 운동 하고있어요 !",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005221.1.12",
          "form": "체력이 워낙 부족해서 조금씩 하면서 늘려가고있네요..",
          "original_form": "체력이 워낙 부족해서 조금씩 하면서 늘려가고있네요..",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005221.1.13",
          "form": "저도 체력이 엄청 안좋았어서 공감도ㅑ요 퓨",
          "original_form": "저도 체력이 엄청 안좋았어서 공감도ㅑ요 퓨",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005221.1.14",
          "form": "요즘은 날도 선선해져서 산책하시는 것도 좋겠어요",
          "original_form": "요즘은 날도 선선해져서 산책하시는 것도 좋겠어요",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005221.1.15",
          "form": "맞아요 오늘 비가 와서 그런지 바람도 불고 날씨가 많이 선선해졌더라구요",
          "original_form": "맞아요 오늘 비가 와서 그런지 바람도 불고 날씨가 많이 선선해졌더라구요",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005221.1.16",
          "form": "다음주에는 산책나가봐야되겠어요",
          "original_form": "다음주에는 산책나가봐야되겠어요",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005221.1.17",
          "form": "지금이 산책하기애는 딱 적기죠",
          "original_form": "지금이 산책하기애는 딱 적기죠",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005221.1.18",
          "form": "가을날씨는 참 좋아요",
          "original_form": "가을날씨는 참 좋아요",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005221.1.19",
          "form": "강아지들도 많이보이고 좋더라고요 ㅎㅎ",
          "original_form": "강아지들도 많이보이고 좋더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005221.1.20",
          "form": "맞아요 산책로가면 귀여운 강아지 많이 만날 수 있죠!!",
          "original_form": "맞아요 산책로가면 귀여운 강아지 많이 만날 수 있죠!!",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005221.1.21",
          "form": "식단도 챙겨드시나요?",
          "original_form": "식단도 챙겨드시나요?",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005221.1.22",
          "form": "네 ㅎㅎ 딱 상상하시는 다이어트식 먹어요",
          "original_form": "네 ㅎㅎ 딱 상상하시는 다이어트식 먹어요",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005221.1.23",
          "form": "닭가 슴살애..",
          "original_form": "닭가 슴살애..",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005221.1.24",
          "form": "다이어트하면 역시 닭가슴살을 빼놓을 수 없죠..",
          "original_form": "다이어트하면 역시 닭가슴살을 빼놓을 수 없죠..",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005221.1.25",
          "form": "닭가슴살 맛이 다양하더라구요 ㅎㅎ",
          "original_form": "닭가슴살 맛이 다양하더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005221.1.26",
          "form": "그쳐 ㅎㅎ 이번에 바베큐맛 먹어봤는데",
          "original_form": "그쳐 ㅎㅎ 이번에 바베큐맛 먹어봤는데",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005221.1.27",
          "form": "정말 끝내주게 맛있더라고요",
          "original_form": "정말 끝내주게 맛있더라고요",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005221.1.28",
          "form": "맞아요 맛이 점점 여러가지로 늘어나서",
          "original_form": "맞아요 맛이 점점 여러가지로 늘어나서",
          "speaker_id": "2",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005221.1.29",
          "form": "다이어트를 좀 더 힘나게 할 수 있어요 ..ㅠㅠ!!",
          "original_form": "다이어트를 좀 더 힘나게 할 수 있어요 ..ㅠㅠ!!",
