      "utterance": [
        {
          "id": "MDRW2100011595.1.1",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011595.1.2",
          "form": "좋아하긴 하는데",
          "original_form": "좋아하긴 하는데",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011595.1.3",
          "form": "요즘은 자주 못 읽어여",
          "original_form": "요즘은 자주 못 읽어여",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011595.1.4",
          "form": "좋아하는 책 있으세요?",
          "original_form": "좋아하는 책 있으세요?",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011595.1.5",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011595.1.6",
          "form": "저는 소년이 온다 떠오르네요",
          "original_form": "저는 소년이 온다 떠오르네요",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011595.1.7",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011595.1.8",
          "form": "네 한강 작가",
          "original_form": "네 한강 작가",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011595.1.9",
          "form": "저 그거 보고 정말",
          "original_form": "저 그거 보고 정말",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011595.1.10",
          "form": "펑펑 울었어요",
          "original_form": "펑펑 울었어요",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011595.1.11",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011595.1.12",
          "form": "너무 슬프죠",
          "original_form": "너무 슬프죠",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011595.1.13",
          "form": "네 진짜 너무 마음 아파서 ㅜㅜ",
          "original_form": "네 진짜 너무 마음 아파서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011595.1.14",
          "form": "저 카페에서 읽었는데",
          "original_form": "저 카페에서 읽었는데",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011595.1.15",
          "form": "마지막에 너무 울어서",
          "original_form": "마지막에 너무 울어서",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011595.1.16",
          "form": "결국 화장실가서 울고왔어요",
          "original_form": "결국 화장실가서 울고왔어요",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011595.1.17",
          "form": "그 정도군요 ㅎㄹ",
          "original_form": "그 정도군요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011595.1.18",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011595.1.19",
          "form": "저도 많이 울었네요",
          "original_form": "저도 많이 울었네요",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011595.1.20",
          "form": "마지막 어머니 부분..",
          "original_form": "마지막 어머니 부분..",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011595.1.21",
          "form": "아 네 맞아요",
          "original_form": "아 네 맞아요",
          "speaker_id": "1",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011595.1.22",
          "form": "그 부분 진짜 끕끕 소리나게 울음 터져가지고 ㅜㅜ",
          "original_form": "그 부분 진짜 끕끕 소리나게 울음 터져가지고 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011595.1.23",
          "form": "네 근데 너무 슬퍼서 두번은 못 읽겠더라구요",
          "original_form": "네 근데 너무 슬퍼서 두번은 못 읽겠더라구요",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011595.1.24",
          "form": "저도 완전 인생책 중의 하나인데",
          "original_form": "저도 완전 인생책 중의 하나인데",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011595.1.25",
          "form": "다신 못 읽을 것 같긴 해요",
          "original_form": "다신 못 읽을 것 같긴 해요",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011595.1.26",
          "form": "채식주의자도 보셨어요?",
          "original_form": "채식주의자도 보셨어요?",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011595.1.27",
          "form": "아 네",
          "original_form": "아 네",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011595.1.28",
          "form": "그건 저는 그냥 그랬던 것 같아요",
          "original_form": "그건 저는 그냥 그랬던 것 같아요",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011595.1.29",
          "form": "앗 저두요",
          "original_form": "앗 저두요",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011595.1.30",
          "form": "그건 완전 별로",
          "original_form": "그건 완전 별로",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011595.1.31",
          "form": "뭔 내용인지 모르겠어요",
          "original_form": "뭔 내용인지 모르겠어요",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011595.1.32",
          "form": "ㅎㅎ 저도요",
          "original_form": "ㅎㅎ 저도요",
          "speaker_id": "2",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011595.1.33",
          "form": "그거 먼저 보고 소년이 온다 봤는데",
          "original_form": "그거 먼저 보고 소년이 온다 봤는데",
          "speaker_id": "2",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011595.1.34",
          "form": "완전 다른 느낌이었어요",
          "original_form": "완전 다른 느낌이었어요",
          "speaker_id": "2",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011595.1.35",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011595.1.36",
          "form": "그건 진짜 등장인물들도 다 이해 안가고 ㅋㅋ",
          "original_form": "그건 진짜 등장인물들도 다 이해 안가고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011595.1.37",
          "form": "맨부커상 받았다던데..",
          "original_form": "맨부커상 받았다던데..",
          "speaker_id": "1",
          "time": "20210824 14:26"
        }
      ]
    }
