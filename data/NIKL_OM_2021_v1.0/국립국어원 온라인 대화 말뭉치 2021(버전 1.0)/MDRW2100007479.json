      "utterance": [
        {
          "id": "MDRW2100007479.1.1",
          "form": "내가 할 줄 아는건 진짜",
          "original_form": "내가 할 줄 아는건 진짜",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007479.1.2",
          "form": "제목 내용 넣는거 밖에 없는데",
          "original_form": "제목 내용 넣는거 밖에 없는데",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007479.1.3",
          "form": "다들 어디서 그런 간지나는 폰트랑 사진이랑 디자인 실력으로",
          "original_form": "다들 어디서 그런 간지나는 폰트랑 사진이랑 디자인 실력으로",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007479.1.4",
          "form": "간지나는 피피티를 만들오오눈지...ㅠㅠㅠ",
          "original_form": "간지나는 피피티를 만들오오눈지...ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007479.1.5",
          "form": "저는 열심히 해서 줬다고 생각하는데",
          "original_form": "저는 열심히 해서 줬다고 생각하는데",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007479.1.6",
          "form": "그 미묘한 공기....",
          "original_form": "그 미묘한 공기....",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007479.1.7",
          "form": "아 네...",
          "original_form": "아 네...",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007479.1.8",
          "form": "완성본이죠 ....?",
          "original_form": "완성본이죠 ....?",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007479.1.9",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007479.1.10",
          "form": "좋은거같아요...",
          "original_form": "좋은거같아요...",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007479.1.11",
          "form": "으악...차라리",
          "original_form": "으악...차라리",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007479.1.12",
          "form": "이게 별로라고 해줘 ㅠㅠㅠㅠㅠ",
          "original_form": "이게 별로라고 해줘 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007479.1.13",
          "form": "그 어색함 느끼고 싶지 않아..",
          "original_form": "그 어색함 느끼고 싶지 않아..",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007479.1.14",
          "form": "진짜 수정사항 알려주면",
          "original_form": "진짜 수정사항 알려주면",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007479.1.15",
          "form": "수정할건데 ㅠㅠㅠㅠㅠ",
          "original_form": "수정할건데 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007479.1.16",
          "form": "근데 또 반대입장 되면",
          "original_form": "근데 또 반대입장 되면",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007479.1.17",
          "form": "수정부분 쉽게 못 말하게 되잖아요ㅋㅋㅋㅋㅋ",
          "original_form": "수정부분 쉽게 못 말하게 되잖아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007479.1.18",
          "form": "그쵸 ㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "그쵸 ㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007479.1.19",
          "form": "그냥 조금만 손보고...",
          "original_form": "그냥 조금만 손보고...",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007479.1.20",
          "form": "이러한 점에서 팀플은 진짜",
          "original_form": "이러한 점에서 팀플은 진짜",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007479.1.21",
          "form": "피할 수 있으면 피하는게",
          "original_form": "피할 수 있으면 피하는게",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007479.1.22",
          "form": "좋은거 같아요..",
          "original_form": "좋은거 같아요..",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007479.1.23",
          "form": "교수님들 여러명 지목해서 팀플로 강의하시게 해야해요 진짜 ㅠㅠ",
          "original_form": "교수님들 여러명 지목해서 팀플로 강의하시게 해야해요 진짜 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007479.1.24",
          "form": "어우 그거 안돼요",
          "original_form": "어우 그거 안돼요",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007479.1.25",
          "form": "저 그거 저번학기에 들어봤는데",
          "original_form": "저 그거 저번학기에 들어봤는데",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007479.1.26",
          "form": "공지가 한번에 절대 안나와요",
          "original_form": "공지가 한번에 절대 안나와요",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007479.1.27",
          "form": "아....?????",
          "original_form": "아....?????",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007479.1.28",
          "form": "세상에....",
          "original_form": "세상에....",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007479.1.29",
          "form": "그냥 과제만 시켜야하나....",
          "original_form": "그냥 과제만 시켜야하나....",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007479.1.30",
          "form": "한 강의에 교수님이 9분인가",
          "original_form": "한 강의에 교수님이 9분인가",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007479.1.31",
          "form": "그랬는데",
          "original_form": "그랬는데",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007479.1.32",
          "form": "한분은 공지를 냈는데 한분은 안내",
          "original_form": "한분은 공지를 냈는데 한분은 안내",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007479.1.33",
          "form": "시험 공지를 안내주시면",
          "original_form": "시험 공지를 안내주시면",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007479.1.34",
          "form": "어떡하냐고요",
          "original_form": "어떡하냐고요",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007479.1.35",
          "form": "교수님...?",
          "original_form": "교수님...?",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007479.1.36",
          "form": "시험 하루전에 공지 받은거 같아요...ㅋㅋㅋ",
          "original_form": "시험 하루전에 공지 받은거 같아요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007479.1.37",
          "form": "진짜 그렇다고 말걸기도 그렇고",
          "original_form": "진짜 그렇다고 말걸기도 그렇고",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007479.1.38",
          "form": "엄청 애매하겠어요 ㅠㅠㅠ",
          "original_form": "엄청 애매하겠어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007479.1.39",
          "form": "차라리 대면이면 조심스럽게",
          "original_form": "차라리 대면이면 조심스럽게",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007479.1.40",
          "form": "여쭤보면 되는데",
          "original_form": "여쭤보면 되는데",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007479.1.41",
          "form": "하필 그 강의가 비대면이라",
          "original_form": "하필 그 강의가 비대면이라",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007479.1.42",
          "form": "문의... 못 넣었죠",
          "original_form": "문의... 못 넣었죠",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007479.1.43",
          "form": "아.....ㅠㅠㅠ 비대면 은근 말하기 힘들어요 ㅠㅠㅠ",
          "original_form": "아.....ㅠㅠㅠ 비대면 은근 말하기 힘들어요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007479.1.44",
          "form": "교수님이 채팅 읽씹하시면",
          "original_form": "교수님이 채팅 읽씹하시면",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007479.1.45",
          "form": "그때부터 생각 많아져요..",
          "original_form": "그때부터 생각 많아져요..",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007479.1.46",
          "form": "나 뭐 잘못보냈나...?",
          "original_form": "나 뭐 잘못보냈나...?",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007479.1.47",
          "form": "이 말을 쓰면 안됐을까",
          "original_form": "이 말을 쓰면 안됐을까",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007479.1.48",
          "form": "진짜 저희는 공휴일 수업 관련해서 채팅 문의 넣었는데",
          "original_form": "진짜 저희는 공휴일 수업 관련해서 채팅 문의 넣었는데",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007479.1.49",
          "form": "안읽고 할말만 하시고 끄셔서",
          "original_form": "안읽고 할말만 하시고 끄셔서",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007479.1.50",
          "form": "그 다음주 수업에 들어갔다가 휴강이란걸 알아서.....",
          "original_form": "그 다음주 수업에 들어갔다가 휴강이란걸 알아서.....",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007479.1.51",
          "form": "교수님 제정신이신지",
          "original_form": "교수님 제정신이신지",
          "speaker_id": "2",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007479.1.52",
          "form": "월급도 주는 줄 알고 있다가",
          "original_form": "월급도 주는 줄 알고 있다가",
          "speaker_id": "2",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007479.1.53",
          "form": "안 들어와봐야 정신 드시는지",
          "original_form": "안 들어와봐야 정신 드시는지",
          "speaker_id": "2",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007479.1.54",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 교수님 정신줄...!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 교수님 정신줄...!",
          "speaker_id": "1",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007479.1.55",
          "form": "교수님 정신줄 챙겨...!",
          "original_form": "교수님 정신줄 챙겨...!",
          "speaker_id": "2",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007479.1.56",
          "form": "학교 학식은 맛있나요???",
          "original_form": "학교 학식은 맛있나요???",
          "speaker_id": "2",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007479.1.57",
          "form": "학식 맛없으면 학교 갈 맛 안나는데",
          "original_form": "학식 맛없으면 학교 갈 맛 안나는데",
          "speaker_id": "2",
          "time": "20210822 02:39"
        }
      ]
    }
  ]
