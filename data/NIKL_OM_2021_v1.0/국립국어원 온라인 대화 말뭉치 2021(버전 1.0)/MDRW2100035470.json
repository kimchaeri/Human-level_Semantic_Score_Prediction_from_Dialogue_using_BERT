      "utterance": [
        {
          "id": "MDRW2100035470.1.1",
          "form": "여름 휴가는 다녀오셨어요?",
          "original_form": "여름 휴가는 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100035470.1.2",
          "form": "아직요ㅠㅠ",
          "original_form": "아직요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:15"
        },
        {
          "id": "MDRW2100035470.1.3",
          "form": "휴가를아직 못 썼어요",
          "original_form": "휴가를아직 못 썼어요",
          "speaker_id": "2",
          "time": "20210829 23:15"
        },
        {
          "id": "MDRW2100035470.1.4",
          "form": "바쁘셨구나ㅜ",
          "original_form": "바쁘셨구나ㅜ",
          "speaker_id": "1",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100035470.1.5",
          "form": "저는 시국이 코시국이라서 부산 가려다 말았내요ㅜ",
          "original_form": "저는 시국이 코시국이라서 부산 가려다 말았내요ㅜ",
          "speaker_id": "1",
          "time": "20210830 11:55"
        },
        {
          "id": "MDRW2100035470.1.6",
          "form": "그죠ㅠㅠ 코로나 때문에 안그래도 여행 잘 안다니는데 더 못가고 있네요ㅠ",
          "original_form": "그죠ㅠㅠ 코로나 때문에 안그래도 여행 잘 안다니는데 더 못가고 있네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 14:38"
        },
        {
          "id": "MDRW2100035470.1.7",
          "form": "동네를 열심히 돌아다니고있어요 대신 ㅋㅋ",
          "original_form": "동네를 열심히 돌아다니고있어요 대신 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:26"
        },
        {
          "id": "MDRW2100035470.1.8",
          "form": "어디로요?동네에도 명소가있나여?",
          "original_form": "어디로요?동네에도 명소가있나여?",
          "speaker_id": "2",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100035470.1.9",
          "form": "그냥 동네 아무곳이나걸어요",
          "original_form": "그냥 동네 아무곳이나걸어요",
          "speaker_id": "1",
          "time": "20210830 17:28"
        },
        {
          "id": "MDRW2100035470.1.10",
          "form": "요새 산책하기좋게 공원 만들어둔곳이라던가",
          "original_form": "요새 산책하기좋게 공원 만들어둔곳이라던가",
          "speaker_id": "1",
          "time": "20210830 17:28"
        },
        {
          "id": "MDRW2100035470.1.11",
          "form": "정말 요즘은 동네 공원도 잘 되어있잖아여 저도 애랑 오전에 나갔다왔어요",
          "original_form": "정말 요즘은 동네 공원도 잘 되어있잖아여 저도 애랑 오전에 나갔다왔어요",
          "speaker_id": "2",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100035470.1.12",
          "form": "굳이 헬스장 아니라도 운동기구놓인곳도 많고요",
          "original_form": "굳이 헬스장 아니라도 운동기구놓인곳도 많고요",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100035470.1.13",
          "form": "아 제가 바닷가 살아서 바다근처도 많이걸었어요",
          "original_form": "아 제가 바닷가 살아서 바다근처도 많이걸었어요",
          "speaker_id": "1",
          "time": "20210830 21:16"
        },
        {
          "id": "MDRW2100035470.1.14",
          "form": "와 바다근처 사시는구나 좋으시겠어요 ㅠㅠ 애들은 바다 가자고 난린데ㅠㅠ",
          "original_form": "와 바다근처 사시는구나 좋으시겠어요 ㅠㅠ 애들은 바다 가자고 난린데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:17"
        },
        {
          "id": "MDRW2100035470.1.15",
          "form": "유명한 해수욕장은 아니고 그냥 동네 주민들만 가는 그런 해수욕장있거든요",
          "original_form": "유명한 해수욕장은 아니고 그냥 동네 주민들만 가는 그런 해수욕장있거든요",
          "speaker_id": "1",
          "time": "20210830 21:18"
        },
        {
          "id": "MDRW2100035470.1.16",
          "form": "지금 해수욕장들 다 닫았다고 전해주세요 ㅋㅋ",
          "original_form": "지금 해수욕장들 다 닫았다고 전해주세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100035470.1.17",
          "form": "맙소사 ㅠㅠ",
          "original_form": "맙소사 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:21"
        },
        {
          "id": "MDRW2100035470.1.18",
          "form": "원래 8/31까진 하는데",
          "original_form": "원래 8/31까진 하는데",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100035470.1.19",
          "form": "코로나라서 20일에 조기폐장했어요",
          "original_form": "코로나라서 20일에 조기폐장했어요",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100035470.1.20",
          "form": "아 근데 뭐 사실가서 발만 담궈도 바다는 좋잖아요",
          "original_form": "아 근데 뭐 사실가서 발만 담궈도 바다는 좋잖아요",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100035470.1.21",
          "form": "음 요새 전부 막 그 테이프같은거 둘러놨어요 ㅋㅋ해변에 ㅋㅋㅋ",
          "original_form": "음 요새 전부 막 그 테이프같은거 둘러놨어요 ㅋㅋ해변에 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:43"
        }
      ]
    }
