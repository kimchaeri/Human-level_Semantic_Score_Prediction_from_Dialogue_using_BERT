      "utterance": [
        {
          "id": "MDRW2100003577.1.1",
          "form": "이번에 도쿄올림픽 보셨어요?",
          "original_form": "이번에 도쿄올림픽 보셨어요?",
          "speaker_id": "2",
          "time": "20210821 02:33"
        },
        {
          "id": "MDRW2100003577.1.2",
          "form": "어떤게 제일 재미있었나요?",
          "original_form": "어떤게 제일 재미있었나요?",
          "speaker_id": "2",
          "time": "20210821 02:33"
        },
        {
          "id": "MDRW2100003577.1.3",
          "form": "이번에 양궁 너무 재미있었어요!",
          "original_form": "이번에 양궁 너무 재미있었어요!",
          "speaker_id": "1",
          "time": "20210821 02:34"
        },
        {
          "id": "MDRW2100003577.1.4",
          "form": "저두요 양궁 진짜 재미있었어요",
          "original_form": "저두요 양궁 진짜 재미있었어요",
          "speaker_id": "2",
          "time": "20210821 02:34"
        },
        {
          "id": "MDRW2100003577.1.5",
          "form": "유퀴즈에 양궁선수들 나오는거 봐야하는데",
          "original_form": "유퀴즈에 양궁선수들 나오는거 봐야하는데",
          "speaker_id": "2",
          "time": "20210821 02:34"
        },
        {
          "id": "MDRW2100003577.1.6",
          "form": "진짜 양궁선수들 너무 멋있어요 ㅠㅠ",
          "original_form": "진짜 양궁선수들 너무 멋있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 02:34"
        },
        {
          "id": "MDRW2100003577.1.7",
          "form": "오 유퀴즈에 나왔나요?",
          "original_form": "오 유퀴즈에 나왔나요?",
          "speaker_id": "1",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003577.1.8",
          "form": "네 유퀴즈에 양궁선수들 나왔대요!",
          "original_form": "네 유퀴즈에 양궁선수들 나왔대요!",
          "speaker_id": "2",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003577.1.9",
          "form": "헉 챙겨봐야겠어요",
          "original_form": "헉 챙겨봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003577.1.10",
          "form": "전 초반에는",
          "original_form": "전 초반에는",
          "speaker_id": "1",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003577.1.11",
          "form": "저 안산선수 금메달 딸 때 너무 재미있었어요",
          "original_form": "저 안산선수 금메달 딸 때 너무 재미있었어요",
          "speaker_id": "2",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003577.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210821 02:35"
        },
        {
          "id": "MDRW2100003577.1.13",
          "form": "도쿄올림픽 관심 없었어서 딱히 안챙겨봤는데",
          "original_form": "도쿄올림픽 관심 없었어서 딱히 안챙겨봤는데",
          "speaker_id": "1",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003577.1.14",
          "form": "그래서 그 혼성 메달 따는거 실시간으로 못봤어서 아쉬워요",
          "original_form": "그래서 그 혼성 메달 따는거 실시간으로 못봤어서 아쉬워요",
          "speaker_id": "1",
          "time": "20210821 02:36"
        },
        {
          "id": "MDRW2100003577.1.15",
          "form": "저두요 ㅠㅠ 실시간으로 못챙겨봤어요",
          "original_form": "저두요 ㅠㅠ 실시간으로 못챙겨봤어요",
          "speaker_id": "2",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003577.1.16",
          "form": "집에 티비가없어서..너무 아쉬웠어요",
          "original_form": "집에 티비가없어서..너무 아쉬웠어요",
          "speaker_id": "2",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003577.1.17",
          "form": "아 못보셨군요ㅠㅠ",
          "original_form": "아 못보셨군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003577.1.18",
          "form": "그래도 안산선수 개인전은 챙겨봤어요!",
          "original_form": "그래도 안산선수 개인전은 챙겨봤어요!",
          "speaker_id": "1",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003577.1.19",
          "form": "아 부러워요 ㅠㅠ",
          "original_form": "아 부러워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 02:37"
        },
        {
          "id": "MDRW2100003577.1.20",
          "form": "전 거의 다 실시간으로 못챙겨봤어요..",
          "original_form": "전 거의 다 실시간으로 못챙겨봤어요..",
          "speaker_id": "2",
          "time": "20210821 02:38"
        },
        {
          "id": "MDRW2100003577.1.21",
          "form": "앗ㅠㅠ",
          "original_form": "앗ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:38"
        },
        {
          "id": "MDRW2100003577.1.22",
          "form": "네이버에서도 실시간 중계하더라구요",
          "original_form": "네이버에서도 실시간 중계하더라구요",
          "speaker_id": "1",
          "time": "20210821 02:39"
        },
        {
          "id": "MDRW2100003577.1.23",
          "form": "그거 보고 그랬어요",
          "original_form": "그거 보고 그랬어요",
          "speaker_id": "1",
          "time": "20210821 02:39"
        },
        {
          "id": "MDRW2100003577.1.24",
          "form": "ㅠㅠ 아쉽다 저는 뒤늦게 뒷북으로 좋아하고그랬어요",
          "original_form": "ㅠㅠ 아쉽다 저는 뒤늦게 뒷북으로 좋아하고그랬어요",
          "speaker_id": "2",
          "time": "20210821 02:39"
        },
        {
          "id": "MDRW2100003577.1.25",
          "form": "앗 저도 그랬었는데ㅠㅠ",
          "original_form": "앗 저도 그랬었는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:40"
        },
        {
          "id": "MDRW2100003577.1.26",
          "form": "챙겨보니까 재밌더라구요",
          "original_form": "챙겨보니까 재밌더라구요",
          "speaker_id": "1",
