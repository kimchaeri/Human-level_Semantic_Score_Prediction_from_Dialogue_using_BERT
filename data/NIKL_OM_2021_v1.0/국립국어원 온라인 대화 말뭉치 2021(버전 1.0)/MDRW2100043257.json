      "utterance": [
        {
          "id": "MDRW2100043257.1.1",
          "form": "요즘 뭐 챙겨보시는거 잇으신가욥",
          "original_form": "요즘 뭐 챙겨보시는거 잇으신가욥",
          "speaker_id": "1",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043257.1.2",
          "form": "펜하요",
          "original_form": "펜하요",
          "speaker_id": "2",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043257.1.3",
          "form": "펜트하우스 보시나여?ㅋ",
          "original_form": "펜트하우스 보시나여?ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100043257.1.4",
          "form": "1화보고 때려쳣어요ㅋㅋㅋㅋ",
          "original_form": "1화보고 때려쳣어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043257.1.5",
          "form": "막장은 안맞더라구용",
          "original_form": "막장은 안맞더라구용",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043257.1.6",
          "form": "그럼대탈출?",
          "original_form": "그럼대탈출?",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100043257.1.7",
          "form": "저 지금 보는건",
          "original_form": "저 지금 보는건",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100043257.1.8",
          "form": "환승연애밖에엄써요",
          "original_form": "환승연애밖에엄써요",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100043257.1.9",
          "form": "막챙겨보는프로가 없는거같아여ㅋㅋ",
          "original_form": "막챙겨보는프로가 없는거같아여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100043257.1.10",
          "form": "드라마도안보시남요",
          "original_form": "드라마도안보시남요",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043257.1.11",
          "form": "네네ㅋㅋ",
          "original_form": "네네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043257.1.12",
          "form": "펜하보시나여",
          "original_form": "펜하보시나여",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043257.1.13",
          "form": "광자매 보신적없으신가용",
          "original_form": "광자매 보신적없으신가용",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043257.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100043257.1.15",
          "form": "쭉ㅋㅋ챙겨봤어욤",
          "original_form": "쭉ㅋㅋ챙겨봤어욤",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100043257.1.16",
          "form": "광자매가 머에여?",
          "original_form": "광자매가 머에여?",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100043257.1.17",
          "form": "김소연 귀뜯는짤보고",
          "original_form": "김소연 귀뜯는짤보고",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100043257.1.18",
          "form": "식겁햇어여ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "식겁햇어여ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100043257.1.19",
          "form": "어우 징그럽더라구요ㅠ",
          "original_form": "어우 징그럽더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100043257.1.20",
          "form": "반전은 그것조차도연기였다는게",
          "original_form": "반전은 그것조차도연기였다는게",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100043257.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100043257.1.22",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100043257.1.23",
