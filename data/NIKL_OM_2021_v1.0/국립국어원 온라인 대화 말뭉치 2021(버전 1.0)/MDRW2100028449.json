      "utterance": [
        {
          "id": "MDRW2100028449.1.1",
          "form": "실습은 차라리 좋았어",
          "original_form": "실습은 차라리 좋았어",
          "speaker_id": "2",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028449.1.2",
          "form": "전공이고 사람들도 좋고",
          "original_form": "전공이고 사람들도 좋고",
          "speaker_id": "2",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028449.1.3",
          "form": "뭣보다 경험도 꽤 좋지 않았어?",
          "original_form": "뭣보다 경험도 꽤 좋지 않았어?",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028449.1.4",
          "form": "힘든거도 있지만 좋은거도 많았던거로 아는데",
          "original_form": "힘든거도 있지만 좋은거도 많았던거로 아는데",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100028449.1.5",
          "form": "그건 그랴",
          "original_form": "그건 그랴",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028449.1.6",
          "form": "일단 사람들이 좋고 담당자가 열심히니까",
          "original_form": "일단 사람들이 좋고 담당자가 열심히니까",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028449.1.7",
          "form": "우리 실수 안하게 많이 도와줬지",
          "original_form": "우리 실수 안하게 많이 도와줬지",
          "speaker_id": "2",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028449.1.8",
          "form": "그런 분위기는 좋았던거같아",
          "original_form": "그런 분위기는 좋았던거같아",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028449.1.9",
          "form": "노동측면에서 힘든거도 많았지만",
          "original_form": "노동측면에서 힘든거도 많았지만",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028449.1.10",
          "form": "사실 근데 진짜 인턴들은 더 힘들겠지?",
          "original_form": "사실 근데 진짜 인턴들은 더 힘들겠지?",
          "speaker_id": "2",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028449.1.11",
          "form": "나는 그냥 실습생이었으니까",
          "original_form": "나는 그냥 실습생이었으니까",
          "speaker_id": "2",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028449.1.12",
          "form": "실습생이나 인턴이나 돈안받고 일하는건 비슷한데",
          "original_form": "실습생이나 인턴이나 돈안받고 일하는건 비슷한데",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028449.1.13",
          "form": "인턴이 더 힘들겠지?",
          "original_form": "인턴이 더 힘들겠지?",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028449.1.14",
          "form": "아 인턴도 돈은 주려나...",
          "original_form": "아 인턴도 돈은 주려나...",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028449.1.15",
          "form": "인턴은 돈 주지 않을까?",
          "original_form": "인턴은 돈 주지 않을까?",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028449.1.16",
          "form": "우린 학교실습이었으니까",
          "original_form": "우린 학교실습이었으니까",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028449.1.17",
          "form": "수업에 가까웠고",
          "original_form": "수업에 가까웠고",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028449.1.18",
          "form": "그리고 학생인데 힘든 일 시키거나 하진 않을거니까",
          "original_form": "그리고 학생인데 힘든 일 시키거나 하진 않을거니까",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028449.1.19",
          "form": "보고 배워라지",
          "original_form": "보고 배워라지",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028449.1.20",
          "form": "근데 지금 하는 일은...힘들어",
          "original_form": "근데 지금 하는 일은...힘들어",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028449.1.21",
          "form": "그럴만도...",
          "original_form": "그럴만도...",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028449.1.22",
          "form": "봉사겸 나갔는데 걍 일시키는 꼴이잖아",
          "original_form": "봉사겸 나갔는데 걍 일시키는 꼴이잖아",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028449.1.23",
          "form": "그게 뭐얏",
          "original_form": "그게 뭐얏",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028449.1.24",
          "form": "업무지 사실...",
          "original_form": "업무지 사실...",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028449.1.25",
          "form": "서류작업은 그나마 이제 적응했어",
          "original_form": "서류작업은 그나마 이제 적응했어",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028449.1.26",
          "form": "아니 사실 못함",
          "original_form": "아니 사실 못함",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028449.1.27",
          "form": "서류 무슨 작업해?",
          "original_form": "서류 무슨 작업해?",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028449.1.28",
          "form": "월간 보고서나 필요하다고 하는 서류들",
          "original_form": "월간 보고서나 필요하다고 하는 서류들",
          "speaker_id": "2",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028449.1.29",
          "form": "기획서도 수정하고",
          "original_form": "기획서도 수정하고",
          "speaker_id": "2",
          "time": "20210902 20:21"
        }
      ]
    }
  ]
