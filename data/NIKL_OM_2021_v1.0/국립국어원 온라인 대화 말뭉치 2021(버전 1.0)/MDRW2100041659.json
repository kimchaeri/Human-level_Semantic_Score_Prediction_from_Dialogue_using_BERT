      "utterance": [
        {
          "id": "MDRW2100041659.1.1",
          "form": "삼양라면도 맛있고 불닭볶음면도 맛있고",
          "original_form": "삼양라면도 맛있고 불닭볶음면도 맛있고",
          "speaker_id": "2",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100041659.1.2",
          "form": "비빔면에 동치미육수 부어먹으면 맛있어요",
          "original_form": "비빔면에 동치미육수 부어먹으면 맛있어요",
          "speaker_id": "1",
          "time": "20210906 14:03"
        },
        {
          "id": "MDRW2100041659.1.3",
          "form": "혹시 불닭시리즈 좋아하세여?",
          "original_form": "혹시 불닭시리즈 좋아하세여?",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041659.1.4",
          "form": "와 맛있겠어요",
          "original_form": "와 맛있겠어요",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041659.1.5",
          "form": "엇 저도 불닭좋아해요ㅋ",
          "original_form": "엇 저도 불닭좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100041659.1.6",
          "form": "불닭에서 어떤맛 좋아하세여",
          "original_form": "불닭에서 어떤맛 좋아하세여",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100041659.1.7",
          "form": "저는 그 까르보 소스도 샀어요",
          "original_form": "저는 그 까르보 소스도 샀어요",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100041659.1.8",
          "form": "ㅋㅋㅋ 근데 막상 사니까 안먹네옄ㅋㅋ",
          "original_form": "ㅋㅋㅋ 근데 막상 사니까 안먹네옄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100041659.1.9",
          "form": "크림까르보도 괜찮더라고요",
          "original_form": "크림까르보도 괜찮더라고요",
          "speaker_id": "1",
          "time": "20210906 14:08"
        },
        {
          "id": "MDRW2100041659.1.10",
          "form": "크림이라도 넘매워서ㅋㅋ",
          "original_form": "크림이라도 넘매워서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100041659.1.11",
          "form": "맞아여 저는 맵찔인데도 사서 먹어요ㅋㅋㅋㅋ",
          "original_form": "맞아여 저는 맵찔인데도 사서 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100041659.1.12",
          "form": "저도요 떨면서 먹져ㅋㅋ",
          "original_form": "저도요 떨면서 먹져ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100041659.1.13",
          "form": "그래놓고 매번 너무 맵다 이러면서 먹는게 취미",
          "original_form": "그래놓고 매번 너무 맵다 이러면서 먹는게 취미",
          "speaker_id": "2",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100041659.1.14",
          "form": "ㅋㅋㅋㅋ네 그런재미",
          "original_form": "ㅋㅋㅋㅋ네 그런재미",
          "speaker_id": "2",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100041659.1.15",
          "form": "매운맛은 중독인거같아요",
          "original_form": "매운맛은 중독인거같아요",
          "speaker_id": "1",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100041659.1.16",
          "form": "또 좋아하는 라면 있으세여?",
          "original_form": "또 좋아하는 라면 있으세여?",
          "speaker_id": "2",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100041659.1.17",
          "form": "그러니까여",
          "original_form": "그러니까여",
          "speaker_id": "2",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100041659.1.18",
          "form": "저 짜파게티 종류도 다 좋아해요",
          "original_form": "저 짜파게티 종류도 다 좋아해요",
          "speaker_id": "2",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100041659.1.19",
          "form": "짜파게티불닭 섞으면 맛나요",
          "original_form": "짜파게티불닭 섞으면 맛나요",
          "speaker_id": "1",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100041659.1.20",
          "form": "전 짜파게티 종류는 회사 안가려요ㅋㅋㅋㅋㅋ",
          "original_form": "전 짜파게티 종류는 회사 안가려요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:13"
        },
        {
          "id": "MDRW2100041659.1.21",
          "form": "그러니까여 맛있더라고욬ㅋㅋㅋ",
          "original_form": "그러니까여 맛있더라고욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100041659.1.22",
          "form": "전 짜파게티만 먹어요\n그게 너무 익숙해서 딴건 안먹게되요",
          "original_form": "전 짜파게티만 먹어요\n그게 너무 익숙해서 딴건 안먹게되요",
          "speaker_id": "1",
          "time": "20210906 14:14"
        }
      ]
    }
