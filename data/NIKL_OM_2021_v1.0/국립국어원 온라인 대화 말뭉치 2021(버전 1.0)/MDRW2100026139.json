      "utterance": [
        {
          "id": "MDRW2100026139.1.1",
          "form": "저녁 먹었어?",
          "original_form": "저녁 먹었어?",
          "speaker_id": "1",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026139.1.2",
          "form": "엉... 저녁 먹었지",
          "original_form": "엉... 저녁 먹었지",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026139.1.3",
          "form": "굴비랑",
          "original_form": "굴비랑",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026139.1.4",
          "form": "소고기국이랑 밥",
          "original_form": "소고기국이랑 밥",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026139.1.5",
          "form": "콩나물무침... 등등 먹었어",
          "original_form": "콩나물무침... 등등 먹었어",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026139.1.6",
          "form": "그리고 빠지지 않는 김치",
          "original_form": "그리고 빠지지 않는 김치",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026139.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026139.1.8",
          "form": "완전 제대로 된 식단이다",
          "original_form": "완전 제대로 된 식단이다",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026139.1.9",
          "form": "그렇지 ㅋㅋ",
          "original_form": "그렇지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026139.1.10",
          "form": "한식 먹었어",
          "original_form": "한식 먹었어",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026139.1.11",
          "form": "나는... 그냥 며칠째 먹다 남은 고기..ㅋㅋ",
          "original_form": "나는... 그냥 며칠째 먹다 남은 고기..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026139.1.12",
          "form": "으악 ㅋㅋ 기름져",
          "original_form": "으악 ㅋㅋ 기름져",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026139.1.13",
          "form": "느끼 폭탄",
          "original_form": "느끼 폭탄",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026139.1.14",
          "form": "생선 오랜만에 먹으니까 맛있더라고",
          "original_form": "생선 오랜만에 먹으니까 맛있더라고",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026139.1.15",
          "form": "굴비 뼈많아서 싫어했는데",
          "original_form": "굴비 뼈많아서 싫어했는데",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026139.1.16",
          "form": "오늘 먹은건 잔가시가 많이 없어서 너무 편했어",
          "original_form": "오늘 먹은건 잔가시가 많이 없어서 너무 편했어",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026139.1.17",
          "form": "굴비 밥도둑이지",
          "original_form": "굴비 밥도둑이지",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026139.1.18",
          "form": "진짜 뜨끈한 밥이랑 먹으면...",
          "original_form": "진짜 뜨끈한 밥이랑 먹으면...",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026139.1.19",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026139.1.20",
          "form": "넌 저녁에 뭐 먹은 거야",
          "original_form": "넌 저녁에 뭐 먹은 거야",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026139.1.21",
          "form": "먹다 남은 대패삼겹살?",
          "original_form": "먹다 남은 대패삼겹살?",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026139.1.22",
          "form": "저녁 아직 안 먹었어ㅠㅠ",
          "original_form": "저녁 아직 안 먹었어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 20:07"
        },
        {
          "id": "MDRW2100026139.1.23",
          "form": "메뉴 추천받습니다!",
          "original_form": "메뉴 추천받습니다!",
          "speaker_id": "1",
          "time": "20210901 20:07"
        },
        {
          "id": "MDRW2100026139.1.24",
          "form": "헐 배고프겠다",
          "original_form": "헐 배고프겠다",
          "speaker_id": "2",
          "time": "20210901 20:07"
        },
        {
          "id": "MDRW2100026139.1.25",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210901 20:07"
        },
        {
          "id": "MDRW2100026139.1.26",
          "form": "뭐 먹고 싶은데??",
          "original_form": "뭐 먹고 싶은데??",
          "speaker_id": "2",
          "time": "20210901 20:07"
        },
        {
          "id": "MDRW2100026139.1.27",
          "form": "음... 냉장고에 든 건 많은데",
          "original_form": "음... 냉장고에 든 건 많은데",
          "speaker_id": "1",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026139.1.28",
          "form": "알리오올리오 만들 재료 있고 볶음밥이나 라면도 있고",
          "original_form": "알리오올리오 만들 재료 있고 볶음밥이나 라면도 있고",
          "speaker_id": "1",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026139.1.29",
          "form": "그냥 밥이랑 김도 있고",
          "original_form": "그냥 밥이랑 김도 있고",
          "speaker_id": "1",
          "time": "20210901 20:09"
        }
      ]
    }
  ]
