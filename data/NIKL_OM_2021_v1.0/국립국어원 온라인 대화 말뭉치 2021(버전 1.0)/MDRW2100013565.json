      "utterance": [
        {
          "id": "MDRW2100013565.1.1",
          "form": "성형 관심 많으신가요?",
          "original_form": "성형 관심 많으신가요?",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013565.1.2",
          "form": "쌍수했고 코 하고싶어요 ㅎㅎ name1님은요?",
          "original_form": "쌍수했고 코 하고싶어요 ㅎㅎ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013565.1.3",
          "form": "저는 지방흡입이요..ㅋㅋ 뱃살",
          "original_form": "저는 지방흡입이요..ㅋㅋ 뱃살",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013565.1.4",
          "form": "코만해도 인상 확 바뀌죠",
          "original_form": "코만해도 인상 확 바뀌죠",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013565.1.5",
          "form": "ㅋㅋㅋㅋㅋ 오옹 !! 지흡 효과있었나요?",
          "original_form": "ㅋㅋㅋㅋㅋ 오옹 !! 지흡 효과있었나요?",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013565.1.6",
          "form": "저는 눈 앞트임 같은거 안하고 절개만했어용 tmi",
          "original_form": "저는 눈 앞트임 같은거 안하고 절개만했어용 tmi",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013565.1.7",
          "form": "절개랑 트임이랑 다른거에요?",
          "original_form": "절개랑 트임이랑 다른거에요?",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013565.1.8",
          "form": "앞트임은 눈 앞머리를 찢는(?)거고",
          "original_form": "앞트임은 눈 앞머리를 찢는(?)거고",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013565.1.9",
          "form": "절개는 그냥 쌍꺼풀 선따라 자르는거에욥 ㅎㄷㄷ",
          "original_form": "절개는 그냥 쌍꺼풀 선따라 자르는거에욥 ㅎㄷㄷ",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013565.1.10",
          "form": "쌍수=절개?",
          "original_form": "쌍수=절개?",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013565.1.11",
          "form": "절개법 매몰법 할때 그건가요?",
          "original_form": "절개법 매몰법 할때 그건가요?",
          "speaker_id": "1",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013565.1.12",
          "form": "네네 그거예용",
          "original_form": "네네 그거예용",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013565.1.13",
          "form": "절개가 있고",
          "original_form": "절개가 있고",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013565.1.14",
          "form": "매몰이있는데",
          "original_form": "매몰이있는데",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013565.1.15",
          "form": "매몰이 좀 더 티 안나고 붓기도 빨리빠져요",
          "original_form": "매몰이 좀 더 티 안나고 붓기도 빨리빠져요",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013565.1.16",
          "form": "대신 풀릴 가능성도 있대요!",
          "original_form": "대신 풀릴 가능성도 있대요!",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100013565.1.17",
          "form": "아.. 이상하게 항상 장단점이있음..",
          "original_form": "아.. 이상하게 항상 장단점이있음..",
          "speaker_id": "1",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100013565.1.18",
          "form": "왜 한장에 완벽한건 없을까여",
          "original_form": "왜 한장에 완벽한건 없을까여",
