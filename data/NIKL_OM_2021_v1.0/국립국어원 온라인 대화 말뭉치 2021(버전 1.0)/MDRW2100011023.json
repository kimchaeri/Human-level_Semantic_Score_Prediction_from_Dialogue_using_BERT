      "utterance": [
        {
          "id": "MDRW2100011023.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 23:49"
        },
        {
          "id": "MDRW2100011023.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011023.1.3",
          "form": "완전좋아해용",
          "original_form": "완전좋아해용",
          "speaker_id": "2",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011023.1.4",
          "form": "해외여행 좋아하시요?",
          "original_form": "해외여행 좋아하시요?",
          "speaker_id": "1",
          "time": "20210823 23:52"
        },
        {
          "id": "MDRW2100011023.1.5",
          "form": "해외여행을 좋아합니다",
          "original_form": "해외여행을 좋아합니다",
          "speaker_id": "2",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100011023.1.6",
          "form": "님도 해외여행 좋아하ㅛㅣ나욜",
          "original_form": "님도 해외여행 좋아하ㅛㅣ나욜",
          "speaker_id": "2",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100011023.1.7",
          "form": "넹 저는 국내 해외 다 좋아해요",
          "original_form": "넹 저는 국내 해외 다 좋아해요",
          "speaker_id": "1",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011023.1.8",
          "form": "어디가 제일 좋으셨어요?",
          "original_form": "어디가 제일 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011023.1.9",
          "form": "저는 해외는 거의 다 좋았는데",
          "original_form": "저는 해외는 거의 다 좋았는데",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011023.1.10",
          "form": "쿠바가 제일 마지막여행이라 기억남라요",
          "original_form": "쿠바가 제일 마지막여행이라 기억남라요",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011023.1.11",
          "form": "오 쿠바 어때요?",
          "original_form": "오 쿠바 어때요?",
          "speaker_id": "1",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100011023.1.12",
          "form": "전 아직 미국이랑 남미쪽은 못가봤어요",
          "original_form": "전 아직 미국이랑 남미쪽은 못가봤어요",
          "speaker_id": "1",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011023.1.13",
          "form": "쿠바 너무 좋아여",
          "original_form": "쿠바 너무 좋아여",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011023.1.14",
          "form": "멕시코랑 같이 다녀왔는데 또 가고싶어요",
          "original_form": "멕시코랑 같이 다녀왔는데 또 가고싶어요",
          "speaker_id": "2",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100011023.1.15",
          "form": "님은 어디가 좋으셨나용",
          "original_form": "님은 어디가 좋으셨나용",
          "speaker_id": "2",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100011023.1.16",
          "form": "저는 스페인이 좋았어요",
          "original_form": "저는 스페인이 좋았어요",
          "speaker_id": "1",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011023.1.17",
          "form": "스페인 가보셨나요?",
          "original_form": "스페인 가보셨나요?",
          "speaker_id": "1",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100011023.1.18",
          "form": "네 ㅎㅎ 스페인가봤어요",
          "original_form": "네 ㅎㅎ 스페인가봤어요",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011023.1.19",
          "form": "어디어디가셨나여 저는 마드리드 바르셀로나 어디 지방이랑 갔었는데 까먹었어용 ㅜ",
          "original_form": "어디어디가셨나여 저는 마드리드 바르셀로나 어디 지방이랑 갔었는데 까먹었어용 ㅜ",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100011023.1.20",
          "form": "저두여! 저는 세비야 까지 다녀왔어요",
          "original_form": "저두여! 저는 세비야 까지 다녀왔어요",
          "speaker_id": "1",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011023.1.21",
          "form": "세비아 좋더라구여 ㅎㅎ",
          "original_form": "세비아 좋더라구여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100011023.1.22",
          "form": "아아 세비야",
          "original_form": "아아 세비야",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011023.1.23",
          "form": "저두 가봤어요 세비야 ㅎㅎ 언제또 갈수있을까요",
          "original_form": "저두 가봤어요 세비야 ㅎㅎ 언제또 갈수있을까요",
          "speaker_id": "2",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100011023.1.24",
          "form": "그러니까요 ㅠㅠ 너무 가고싶어요",
          "original_form": "그러니까요 ㅠㅠ 너무 가고싶어요",
          "speaker_id": "1",
          "time": "20210824 00:04"
        }
      ]
