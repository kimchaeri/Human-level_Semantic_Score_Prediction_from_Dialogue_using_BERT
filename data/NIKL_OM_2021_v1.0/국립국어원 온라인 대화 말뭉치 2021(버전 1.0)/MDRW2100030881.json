      "utterance": [
        {
          "id": "MDRW2100030881.1.1",
          "form": "혹시 뱀을 피워 본 적 있나요",
          "original_form": "혹시 뱀을 피워 본 적 있나요",
          "speaker_id": "2",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030881.1.2",
          "form": "키워",
          "original_form": "키워",
          "speaker_id": "2",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030881.1.3",
          "form": "뱀을 어떻게 키우죠",
          "original_form": "뱀을 어떻게 키우죠",
          "speaker_id": "1",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030881.1.4",
          "form": "지렁이도 뭐 키워 봤어요",
          "original_form": "지렁이도 뭐 키워 봤어요",
          "speaker_id": "1",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030881.1.5",
          "form": "못",
          "original_form": "못",
          "speaker_id": "1",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030881.1.6",
          "form": "뱀을 애완동물로 삼는 사람도 많습니다",
          "original_form": "뱀을 애완동물로 삼는 사람도 많습니다",
          "speaker_id": "2",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030881.1.7",
          "form": "아주 시원하대요",
          "original_form": "아주 시원하대요",
          "speaker_id": "2",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030881.1.8",
          "form": "파충류도 사랑스럽대요",
          "original_form": "파충류도 사랑스럽대요",
          "speaker_id": "1",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030881.1.9",
          "form": "전에 불가사리 방에서 보았는데 어떤 뱀이 사람만큼 컸거든요",
          "original_form": "전에 불가사리 방에서 보았는데 어떤 뱀이 사람만큼 컸거든요",
          "speaker_id": "2",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030881.1.10",
          "form": "악어 키우는 사람도 있잖아요",
          "original_form": "악어 키우는 사람도 있잖아요",
          "speaker_id": "1",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100030881.1.11",
          "form": "잡아 먹었나요",
          "original_form": "잡아 먹었나요",
          "speaker_id": "1",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030881.1.12",
          "form": "음식을 안 먹어서 걱정을 했는데 알고보니 그 뱀이 사람을 잡아 먹으려고 했던 거 같다는  기억이 나네요",
          "original_form": "음식을 안 먹어서 걱정을 했는데 알고보니 그 뱀이 사람을 잡아 먹으려고 했던 거 같다는  기억이 나네요",
          "speaker_id": "2",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030881.1.13",
          "form": "무섭네요 생각만 해도",
          "original_form": "무섭네요 생각만 해도",
          "speaker_id": "1",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030881.1.14",
          "form": "충분히 그럴 듯하네요",
          "original_form": "충분히 그럴 듯하네요",
          "speaker_id": "1",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030881.1.15",
          "form": "곤충을 무서워 하거든요",
          "original_form": "곤충을 무서워 하거든요",
          "speaker_id": "2",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030881.1.16",
          "form": "동물은 동물이에요",
          "original_form": "동물은 동물이에요",
          "speaker_id": "1",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030881.1.17",
          "form": "사람이 될 수 없어요",
          "original_form": "사람이 될 수 없어요",
          "speaker_id": "1",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030881.1.18",
          "form": "맞아요 사람의 마음을 알아주는 건 힘들지 않을까요",
          "original_form": "맞아요 사람의 마음을 알아주는 건 힘들지 않을까요",
          "speaker_id": "2",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030881.1.19",
          "form": "저도 곤충은 싫어요",
          "original_form": "저도 곤충은 싫어요",
          "speaker_id": "1",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030881.1.20",
          "form": "강아지도 사람을 많이 물 줘",
          "original_form": "강아지도 사람을 많이 물 줘",
          "speaker_id": "1",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030881.1.21",
          "form": "곤충을 애완으로 키우는 사람은 정말 대단한 것 같아요",
          "original_form": "곤충을 애완으로 키우는 사람은 정말 대단한 것 같아요",
          "speaker_id": "2",
          "time": "20210904 13:51"
        },
        {
          "id": "MDRW2100030881.1.22",
          "form": "아기랑 강아지랑 같이 두면 안 될 것 같아요 둘이서만 둘이서만",
          "original_form": "아기랑 강아지랑 같이 두면 안 될 것 같아요 둘이서만 둘이서만",
          "speaker_id": "2",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100030881.1.23",
          "form": "어느 것이든 중독되면 그럴 수밖에 없나 봐요",
          "original_form": "어느 것이든 중독되면 그럴 수밖에 없나 봐요",
          "speaker_id": "1",
          "time": "20210904 13:52"
        }
      ]
    }
