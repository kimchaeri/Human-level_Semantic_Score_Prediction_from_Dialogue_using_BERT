      "utterance": [
        {
          "id": "MDRW2100019024.1.1",
          "form": "지금 하시는 동호회 있으신가요?",
          "original_form": "지금 하시는 동호회 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019024.1.2",
          "form": "아뇨 아직 없어요",
          "original_form": "아뇨 아직 없어요",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019024.1.3",
          "form": "동호회 하시나요?",
          "original_form": "동호회 하시나요?",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019024.1.4",
          "form": "아니요 ! 저도 없네요 ㅎㅎ",
          "original_form": "아니요 ! 저도 없네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019024.1.5",
          "form": "동네 사람이 없어요 ㅋㅋ",
          "original_form": "동네 사람이 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019024.1.6",
          "form": "동호회는 왠지 연령대 좀 있으신 분들이 할거같다는",
          "original_form": "동호회는 왠지 연령대 좀 있으신 분들이 할거같다는",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019024.1.7",
          "form": "아 맞아요 ㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019024.1.8",
          "form": "선입견이 있어서 선뜻 하기 어려워요 ㅋㅋㅋ",
          "original_form": "선입견이 있어서 선뜻 하기 어려워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019024.1.9",
          "form": "산악회 ! 이런 느낌이죠 ㅋㅋ",
          "original_form": "산악회 ! 이런 느낌이죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019024.1.10",
          "form": "맞아요 산악회나 조기축구회 같은거 ㅋㅋㅋ",
          "original_form": "맞아요 산악회나 조기축구회 같은거 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019024.1.11",
          "form": "아 맞아요 ㅋㅋ 조기축구회",
          "original_form": "아 맞아요 ㅋㅋ 조기축구회",
          "speaker_id": "2",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019024.1.12",
          "form": "저희 아빠는 종종 나가시더라구요",
          "original_form": "저희 아빠는 종종 나가시더라구요",
          "speaker_id": "2",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019024.1.13",
          "form": "와 건강에는 좋을거같아요",
          "original_form": "와 건강에는 좋을거같아요",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019024.1.14",
          "form": "근데 저번에 하다가 다치셨어요 .. ㅎㅎ",
          "original_form": "근데 저번에 하다가 다치셨어요 .. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019024.1.15",
          "form": "건강에 좋은거같은데 다치면 무소용 ㅋㅋ 이에요",
          "original_form": "건강에 좋은거같은데 다치면 무소용 ㅋㅋ 이에요",
          "speaker_id": "2",
