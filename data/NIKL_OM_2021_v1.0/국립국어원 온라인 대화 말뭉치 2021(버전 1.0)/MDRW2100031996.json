      "utterance": [
        {
          "id": "MDRW2100031996.1.1",
          "form": "너희 가족은 오늘 뭐하는데?",
          "original_form": "너희 가족은 오늘 뭐하는데?",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100031996.1.2",
          "form": "name3이는 명륜진사가서 갈비 먹고싶단다",
          "original_form": "&name3&이는 명륜진사가서 갈비 먹고싶단다",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100031996.1.3",
          "form": "가서먹어도 안위험하나",
          "original_form": "가서먹어도 안위험하나",
          "speaker_id": "1",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100031996.1.4",
          "form": "우린 이따책사러갔다가 밥먹고들어올까한다",
          "original_form": "우린 이따책사러갔다가 밥먹고들어올까한다",
          "speaker_id": "1",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100031996.1.5",
          "form": "무슨 밥 먹게??",
          "original_form": "무슨 밥 먹게??",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100031996.1.6",
          "form": "나는 너거형부랑 냉전중이라ㅜname3 명륜진사 안가면 학교도 안간단다",
          "original_form": "나는 너거형부랑 냉전중이라ㅜ&name3& 명륜진사 안가면 학교도 안간단다",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100031996.1.7",
          "form": "또싸웠나",
          "original_form": "또싸웠나",
          "speaker_id": "1",
          "time": "20210905 14:40"
        },
        {
          "id": "MDRW2100031996.1.8",
          "form": "성격 이상하다ㅜ",
          "original_form": "성격 이상하다ㅜ",
          "speaker_id": "2",
          "time": "20210905 14:40"
        },
        {
          "id": "MDRW2100031996.1.9",
          "form": "왜 또",
          "original_form": "왜 또",
          "speaker_id": "1",
          "time": "20210905 14:40"
        },
        {
          "id": "MDRW2100031996.1.10",
          "form": "얘들땜에",
          "original_form": "얘들땜에",
          "speaker_id": "2",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100031996.1.11",
          "form": "너희는 뭐 먹을라고?",
          "original_form": "너희는 뭐 먹을라고?",
          "speaker_id": "2",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100031996.1.12",
          "form": "몸은 괜찮나?",
          "original_form": "몸은 괜찮나?",
          "speaker_id": "2",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100031996.1.13",
          "form": "애들이왜 싸웠나",
          "original_form": "애들이왜 싸웠나",
          "speaker_id": "1",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100031996.1.14",
          "form": "ㅇㅇ몸은가볍다",
          "original_form": "ㅇㅇ몸은가볍다",
          "speaker_id": "1",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100031996.1.15",
          "form": "다행이네ㅜname4가 티비 딴데 틀었거든 근데 name3이가 버릇없이 어디 트냐고",
          "original_form": "다행이네ㅜ&name4&가 티비 딴데 틀었거든 근데 &name3&이가 버릇없이 어디 트냐고",
          "speaker_id": "2",
          "time": "20210905 14:43"
        },
        {
          "id": "MDRW2100031996.1.16",
          "form": "그래서 내가 너거 형부한테 오빠랑 똑같이 말하네 이캤다고 막 화냄",
          "original_form": "그래서 내가 너거 형부한테 오빠랑 똑같이 말하네 이캤다고 막 화냄",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100031996.1.17",
          "form": "신경질내더나",
          "original_form": "신경질내더나",
          "speaker_id": "1",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100031996.1.18",
          "form": "형부 갱년기인가",
          "original_form": "형부 갱년기인가",
          "speaker_id": "1",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100031996.1.19",
          "form": "응 몰라ㅜ",
          "original_form": "응 몰라ㅜ",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100031996.1.20",
          "form": "팀장이 스트레스 주기는 함",
          "original_form": "팀장이 스트레스 주기는 함",
          "speaker_id": "2",
          "time": "20210905 14:44"
        },
        {
          "id": "MDRW2100031996.1.21",
          "form": "원래 화도 잘내고",
          "original_form": "원래 화도 잘내고",
          "speaker_id": "2",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100031996.1.22",
          "form": "그것땜에 예민한거같다",
          "original_form": "그것땜에 예민한거같다",
          "speaker_id": "1",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100031996.1.23",
          "form": "회사일이그러면 짜증나지 우울하고 ㅜ",
          "original_form": "회사일이그러면 짜증나지 우울하고 ㅜ",
          "speaker_id": "1",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100031996.1.24",
          "form": "회사일 안 풀리면 짜증 많이내긴한다",
          "original_form": "회사일 안 풀리면 짜증 많이내긴한다",
          "speaker_id": "2",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100031996.1.25",
          "form": "근데 본인만 회사 다니나?",
          "original_form": "근데 본인만 회사 다니나?",
          "speaker_id": "2",
          "time": "20210905 14:45"
        },
        {
          "id": "MDRW2100031996.1.26",
          "form": "ㅜㅜ 팀장이왜",
          "original_form": "ㅜㅜ 팀장이왜",
          "speaker_id": "1",
          "time": "20210905 14:46"
        },
        {
          "id": "MDRW2100031996.1.27",
          "form": "팀장 조울증",
          "original_form": "팀장 조울증",
          "speaker_id": "2",
          "time": "20210905 14:46"
        }
      ]
    }
