      "utterance": [
        {
          "id": "MDRW2100007446.1.1",
          "form": "최근에 다이어트 조리법 찾아보다가 요즘 당류 제한해서 먹는게 유행이더라고요",
          "original_form": "최근에 다이어트 조리법 찾아보다가 요즘 당류 제한해서 먹는게 유행이더라고요",
          "speaker_id": "2",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007446.1.2",
          "form": "액상과당 진짜 문제긴 하더라구요",
          "original_form": "액상과당 진짜 문제긴 하더라구요",
          "speaker_id": "1",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007446.1.3",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007446.1.4",
          "form": "그 대체설탕?",
          "original_form": "그 대체설탕?",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007446.1.5",
          "form": "먹어봤어요?",
          "original_form": "먹어봤어요?",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007446.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007446.1.7",
          "form": "스테비아는 별로고",
          "original_form": "스테비아는 별로고",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007446.1.8",
          "form": "스테비아 알룰로스",
          "original_form": "스테비아 알룰로스",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007446.1.9",
          "form": "약간 단만이 덜하다고는 들었는데",
          "original_form": "약간 단만이 덜하다고는 들었는데",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007446.1.10",
          "form": "에리스리톨이",
          "original_form": "에리스리톨이",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007446.1.11",
          "form": "더 좋은것같아요",
          "original_form": "더 좋은것같아요",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.12",
          "form": "어떤점이 좋았어요",
          "original_form": "어떤점이 좋았어요",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.13",
          "form": "그게 설탕이랑 가장 비슷한 것 같어요",
          "original_form": "그게 설탕이랑 가장 비슷한 것 같어요",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.14",
          "form": "맛이 좀 더 풍부한가..?",
          "original_form": "맛이 좀 더 풍부한가..?",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.15",
          "form": "스테비아는",
          "original_form": "스테비아는",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.17",
          "form": "좀 쓴 맛이 있어요",
          "original_form": "좀 쓴 맛이 있어요",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.18",
          "form": "그렇다더라구요",
          "original_form": "그렇다더라구요",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.19",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.20",
          "form": "근데 제일 괜찮은건",
          "original_form": "근데 제일 괜찮은건",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.21",
          "form": "스테비아는 토마토 유명해서 먹어보긴했는데",
          "original_form": "스테비아는 토마토 유명해서 먹어보긴했는데",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.22",
          "form": "알룰로스더라고요",
          "original_form": "알룰로스더라고요",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.23",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.24",
          "form": "단가요 많이",
          "original_form": "단가요 많이",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.25",
          "form": "알룰로스 기억할게요",
          "original_form": "알룰로스 기억할게요",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007446.1.26",
          "form": "겁나 달아요 미친듯이",
          "original_form": "겁나 달아요 미친듯이",
          "speaker_id": "1",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007446.1.27",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007446.1.28",
          "form": "초당 옥수수인가",
          "original_form": "초당 옥수수인가",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007446.1.29",
          "form": "이게 대체품이 아니면 당뇨걸리겠다 생각들정도",
          "original_form": "이게 대체품이 아니면 당뇨걸리겠다 생각들정도",
          "speaker_id": "1",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007446.1.30",
          "form": "그것도 설탕대체품 먹인거라던데",
          "original_form": "그것도 설탕대체품 먹인거라던데",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007446.1.31",
          "form": "맞아요 그것도 합성식품",
          "original_form": "맞아요 그것도 합성식품",
          "speaker_id": "1",
          "time": "20210822 02:22"
        }
      ]
