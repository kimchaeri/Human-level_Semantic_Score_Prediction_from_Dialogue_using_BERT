        {
          "id": "MDRW2100027231.1.1",
          "form": "일해요?",
          "original_form": "일해요?",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027231.1.2",
          "form": "반직장인 반학생입니다",
          "original_form": "반직장인 반학생입니다",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027231.1.3",
          "form": "대학원생이라서요",
          "original_form": "대학원생이라서요",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027231.1.4",
          "form": "전 직장인이여ㅛ",
          "original_form": "전 직장인이여ㅛ",
          "speaker_id": "1",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100027231.1.5",
          "form": "요즘 재택하세요?",
          "original_form": "요즘 재택하세요?",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100027231.1.6",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100027231.1.7",
          "form": "직장 동료들은 괜찮나요",
          "original_form": "직장 동료들은 괜찮나요",
          "speaker_id": "2",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100027231.1.8",
          "form": "힘들어요",
          "original_form": "힘들어요",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100027231.1.9",
          "form": "아..그래도 휴일엔 일 안하시죠?",
          "original_form": "아..그래도 휴일엔 일 안하시죠?",
          "speaker_id": "2",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100027231.1.10",
          "form": "쉬어요",
          "original_form": "쉬어요",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100027231.1.11",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100027231.1.12",
          "form": "가끔 나갈째더",
          "original_form": "가끔 나갈째더",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100027231.1.13",
          "form": "전 거의.일하는 것 같아요 ㅠ",
          "original_form": "전 거의.일하는 것 같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100027231.1.14",
          "form": "휴일에도 ㅠ",
          "original_form": "휴일에도 ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100027231.1.15",
          "form": "피곤한몸을 이끌고있는",
          "original_form": "피곤한몸을 이끌고있는",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100027231.1.16",
          "form": "네네 일하는 사람들은 다 그런것 같아요",
          "original_form": "네네 일하는 사람들은 다 그런것 같아요",
