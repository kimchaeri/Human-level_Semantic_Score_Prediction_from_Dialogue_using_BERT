      "utterance": [
        {
          "id": "MDRW2100032753.1.1",
          "form": "블로그와 트위터를 했는데",
          "original_form": "블로그와 트위터를 했는데",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032753.1.2",
          "form": "사실 원래 블로그에 사진이 있었거든요",
          "original_form": "사실 원래 블로그에 사진이 있었거든요",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032753.1.3",
          "form": "!!!",
          "original_form": "!!!",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032753.1.4",
          "form": "최근에 블로그 챌린지라는게 생기면서",
          "original_form": "최근에 블로그 챌린지라는게 생기면서",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032753.1.5",
          "form": "저도 블로그합니닷(수줍)",
          "original_form": "저도 블로그합니닷(수줍)",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032753.1.6",
          "form": "저의 모든 흑역사들이 보여서",
          "original_form": "저의 모든 흑역사들이 보여서",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032753.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032753.1.8",
          "form": "싹 한번 밀어버렸... 허허",
          "original_form": "싹 한번 밀어버렸... 허허",
          "speaker_id": "1",
          "time": "20210905 23:08"
        },
        {
          "id": "MDRW2100032753.1.9",
          "form": "어머 블로그 하세요?",
          "original_form": "어머 블로그 하세요?",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032753.1.10",
          "form": "아깝네요....",
          "original_form": "아깝네요....",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032753.1.11",
          "form": "지금은 책 리뷰를 하는 블로그로 사용 중이에요 ㅎㅎ",
          "original_form": "지금은 책 리뷰를 하는 블로그로 사용 중이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032753.1.12",
          "form": "비공개도아니고 레알 없애버리셨나요",
          "original_form": "비공개도아니고 레알 없애버리셨나요",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032753.1.13",
          "form": "name2님께서는요??",
          "original_form": "&name2&님께서는요??",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032753.1.14",
          "form": "저는 모든것의 블로그에요",
          "original_form": "저는 모든것의 블로그에요",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032753.1.15",
          "form": "완전 진짜 다급하게 싹 없앴어요  ㅠ",
          "original_form": "완전 진짜 다급하게 싹 없앴어요  ㅠ",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032753.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032753.1.17",
          "form": "비공개는 생각도 못했답니다",
          "original_form": "비공개는 생각도 못했답니다",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032753.1.18",
          "form": "아까워오 ㅜ",
          "original_form": "아까워오 ㅜ",
          "speaker_id": "2",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100032753.1.19",
          "form": "맛집 공부 일상",
          "original_form": "맛집 공부 일상",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032753.1.20",
          "form": "그러게요 지금 생각하니까 캡쳐라도 해둘걸 너무 아까운거 있죠 ㅠ",
          "original_form": "그러게요 지금 생각하니까 캡쳐라도 해둘걸 너무 아까운거 있죠 ㅠ",
          "speaker_id": "1",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032753.1.21",
          "form": "다 있어요",
          "original_form": "다 있어요",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032753.1.22",
          "form": "ㅋㄱㅋ",
          "original_form": "ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032753.1.23",
          "form": "재테크공부 맛집 쇼핑후기 이런거요 ㅎㅎ",
          "original_form": "재테크공부 맛집 쇼핑후기 이런거요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032753.1.24",
          "form": "어디 다른 서버에 저장안돼있나요 넘 아까워요 ㅜ",
          "original_form": "어디 다른 서버에 저장안돼있나요 넘 아까워요 ㅜ",
          "speaker_id": "2",
          "time": "20210905 23:10"
        },
        {
          "id": "MDRW2100032753.1.25",
          "form": "우와 정말 다양하게 남겨두시는군요!",
          "original_form": "우와 정말 다양하게 남겨두시는군요!",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032753.1.26",
          "form": "맛집 정보 정말 탐이 나요!",
          "original_form": "맛집 정보 정말 탐이 나요!",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032753.1.27",
          "form": "재태크라면 혹시 주식도 하시나요?",
          "original_form": "재태크라면 혹시 주식도 하시나요?",
          "speaker_id": "1",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032753.1.28",
          "form": "넵:)",
          "original_form": "넵:)",
          "speaker_id": "2",
          "time": "20210905 23:12"
        },
        {
          "id": "MDRW2100032753.1.29",
          "form": "망테크인지 재테크인지몰라도",
          "original_form": "망테크인지 재테크인지몰라도",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100032753.1.30",
          "form": "매우 열심히 공부는 합니다",
          "original_form": "매우 열심히 공부는 합니다",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100032753.1.31",
          "form": "허허",
          "original_form": "허허",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100032753.1.32",
          "form": "앜ㅋㅋㅋㅋㅋ 저는 최근에 주식 처음 시작했어요",
          "original_form": "앜ㅋㅋㅋㅋㅋ 저는 최근에 주식 처음 시작했어요",
          "speaker_id": "1",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100032753.1.33",
          "form": "흐름 타기엔 좀 늦게 시작해버렸죠 ㅠ",
          "original_form": "흐름 타기엔 좀 늦게 시작해버렸죠 ㅠ",
          "speaker_id": "1",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100032753.1.34",
          "form": "맛집정보가 탐나신다면 초록창 블로그에 챠밍정을 검색해주세요",
          "original_form": "맛집정보가 탐나신다면 초록창 블로그에 챠밍정을 검색해주세요",
          "speaker_id": "2",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032753.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ조심스런 블로그 이웃 신청",
          "original_form": "ㅋㅋㅋㅋㅋㅋ조심스런 블로그 이웃 신청",
          "speaker_id": "2",
