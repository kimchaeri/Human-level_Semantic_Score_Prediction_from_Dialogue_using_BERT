      "utterance": [
        {
          "id": "MDRW2100035932.1.1",
          "form": "돼지불고기요!",
          "original_form": "돼지불고기요!",
          "speaker_id": "2",
          "time": "20210830 12:01"
        },
        {
          "id": "MDRW2100035932.1.2",
          "form": "오 맛있겠어요 ! 밖에 나가서 드시나요?",
          "original_form": "오 맛있겠어요 ! 밖에 나가서 드시나요?",
          "speaker_id": "1",
          "time": "20210830 12:05"
        },
        {
          "id": "MDRW2100035932.1.3",
          "form": "집에 있는 걸 데워 먹으려고요!",
          "original_form": "집에 있는 걸 데워 먹으려고요!",
          "speaker_id": "2",
          "time": "20210830 12:07"
        },
        {
          "id": "MDRW2100035932.1.4",
          "form": "아 어디꺼에요 ??!",
          "original_form": "아 어디꺼에요 ??!",
          "speaker_id": "1",
          "time": "20210830 12:08"
        },
        {
          "id": "MDRW2100035932.1.5",
          "form": "엄마표예요!ㅎㅎ",
          "original_form": "엄마표예요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 12:09"
        },
        {
          "id": "MDRW2100035932.1.6",
          "form": "오 !! 아 집불고기 맛있는데 ! ㅠㅠ",
          "original_form": "오 !! 아 집불고기 맛있는데 ! ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100035932.1.7",
          "form": "저도 먹고싶네요 ㅎㅎ",
          "original_form": "저도 먹고싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100035932.1.8",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 12:27"
        },
        {
          "id": "MDRW2100035932.1.9",
          "form": "네 이제 먹으려구요!",
          "original_form": "네 이제 먹으려구요!",
          "speaker_id": "1",
          "time": "20210830 12:29"
        },
        {
          "id": "MDRW2100035932.1.10",
          "form": "메뉴 정하셨나요?",
          "original_form": "메뉴 정하셨나요?",
          "speaker_id": "2",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100035932.1.11",
          "form": "전 라면 먹으려구요 ㅎㅎ !",
          "original_form": "전 라면 먹으려구요 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210830 12:36"
        },
        {
          "id": "MDRW2100035932.1.12",
          "form": "어떤 라면 좋아하시나요?",
          "original_form": "어떤 라면 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100035932.1.13",
          "form": "저는 볶음류 좋아해요 ㅎㅎ",
          "original_form": "저는 볶음류 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100035932.1.14",
          "form": "불닭 짜파게티 같은거요 !",
          "original_form": "불닭 짜파게티 같은거요 !",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100035932.1.15",
          "form": "매운걸 잘 드시나봐요!",
          "original_form": "매운걸 잘 드시나봐요!",
          "speaker_id": "2",
