      "utterance": [
        {
          "id": "MDRW2100017433.1.1",
          "form": "다 게임이야 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "다 게임이야 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017433.1.2",
          "form": "그래서 못 넘어간",
          "original_form": "그래서 못 넘어간",
          "speaker_id": "2",
          "time": "20210901 17:15"
        },
        {
          "id": "MDRW2100017433.1.3",
          "form": "공략 필요해?",
          "original_form": "공략 필요해?",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100017433.1.4",
          "form": "지도가 안열려서",
          "original_form": "지도가 안열려서",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100017433.1.5",
          "form": "아 지도 안열려도",
          "original_form": "아 지도 안열려도",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100017433.1.6",
          "form": "걍 갈수있어",
          "original_form": "걍 갈수있어",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100017433.1.7",
          "form": "빨간 라인 바깥만 아니면",
          "original_form": "빨간 라인 바깥만 아니면",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100017433.1.8",
          "form": "아 그래? 막 바위 올라가고 그러길래",
          "original_form": "아 그래? 막 바위 올라가고 그러길래",
          "speaker_id": "2",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100017433.1.9",
          "form": "바위 올라가다가 기력 다해서(?",
          "original_form": "바위 올라가다가 기력 다해서(?",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100017433.1.10",
          "form": "ㅋㅋㅋ큐",
          "original_form": "ㅋㅋㅋ큐",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100017433.1.11",
          "form": "지금 머야 새로 패치된거 까는 중이라",
          "original_form": "지금 머야 새로 패치된거 까는 중이라",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100017433.1.12",
          "form": "아직 못 들어가봤어 어제 들어가고",
          "original_form": "아직 못 들어가봤어 어제 들어가고",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100017433.1.13",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100017433.1.14",
          "form": "그거는",
          "original_form": "그거는",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100017433.1.15",
          "form": "어쩔수업서.....",
          "original_form": "어쩔수업서.....",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100017433.1.16",
          "form": "스테미너 모자란건 그냥 신의눈을 많이 모아다 바치는 수밖엔....",
          "original_form": "스테미너 모자란건 그냥 신의눈을 많이 모아다 바치는 수밖엔....",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100017433.1.17",
          "form": "ㅇㅎ...",
          "original_form": "ㅇㅎ...",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100017433.1.18",
          "form": "그럼 다른 퀘부터 깨는게 나으려나",
          "original_form": "그럼 다른 퀘부터 깨는게 나으려나",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100017433.1.19",
          "form": "뭔가 다 레벨에 막히고 기력에 막히고",
          "original_form": "뭔가 다 레벨에 막히고 기력에 막히고",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100017433.1.20",
          "form": "비행 4개까진 고리 통과해봤는데 이게 먼가 그 머지 경로가 어디어디 있는지 잘 모르니까 결국 다시 낙하산 되더라고...",
          "original_form": "비행 4개까진 고리 통과해봤는데 이게 먼가 그 머지 경로가 어디어디 있는지 잘 모르니까 결국 다시 낙하산 되더라고...",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100017433.1.21",
          "form": "이것마저 공략을 찾아봐야하나 자괴감이 들고",
          "original_form": "이것마저 공략을 찾아봐야하나 자괴감이 들고",
          "speaker_id": "2",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100017433.1.22",
          "form": "아녀 나도",
          "original_form": "아녀 나도",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100017433.1.23",
          "form": "공략 찾아보고했어",
          "original_form": "공략 찾아보고했어",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100017433.1.24",
          "form": "난 보스도 혼자깨기 무섭다고",
          "original_form": "난 보스도 혼자깨기 무섭다고",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100017433.1.25",
          "form": "맛언니랑 같이할수있을때까지",
          "original_form": "맛언니랑 같이할수있을때까지",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100017433.1.26",
          "form": "미뤘는걸",
          "original_form": "미뤘는걸",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100017433.1.27",
          "form": "이거는 컨트롤 문제가 아니라 고리 위치 외우는거 문제라",
          "original_form": "이거는 컨트롤 문제가 아니라 고리 위치 외우는거 문제라",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100017433.1.28",
          "form": "걍 공략 보고하는게 맘 편해",
          "original_form": "걍 공략 보고하는게 맘 편해",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100017433.1.29",
          "form": "유튜브에 원신 비행시험",
          "original_form": "유튜브에 원신 비행시험",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100017433.1.30",
          "form": "검색해보면",
          "original_form": "검색해보면",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100017433.1.31",
          "form": "다 나와",
          "original_form": "다 나와",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100017433.1.32",
