      "utterance": [
        {
          "id": "MDRW2100007484.1.1",
          "form": "불면증  극복은  어찌해야될까요?",
          "original_form": "불면증  극복은  어찌해야될까요?",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007484.1.2",
          "form": "요즘 잠을  넘 늦게자요",
          "original_form": "요즘 잠을  넘 늦게자요",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007484.1.3",
          "form": "습관이실듯해요",
          "original_form": "습관이실듯해요",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007484.1.4",
          "form": "자꾸 늦게 자는 습관을 들이시니 자꾸늦어지는듯해요",
          "original_form": "자꾸 늦게 자는 습관을 들이시니 자꾸늦어지는듯해요",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007484.1.5",
          "form": "그런거같아요 출근시간도 늦으니",
          "original_form": "그런거같아요 출근시간도 늦으니",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007484.1.6",
          "form": "폰보는 시간을 줄여야되는데 잘안되네요",
          "original_form": "폰보는 시간을 줄여야되는데 잘안되네요",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007484.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007484.1.8",
          "form": "저도 잘안되는게 폰보는시간줄이는거요 ㅜㅠ",
          "original_form": "저도 잘안되는게 폰보는시간줄이는거요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007484.1.9",
          "form": "건강에 안좋긴해요",
          "original_form": "건강에 안좋긴해요",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007484.1.10",
          "form": "눈도침침해지고",
          "original_form": "눈도침침해지고",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007484.1.11",
          "form": "맞아요 그리고 이러다",
          "original_form": "맞아요 그리고 이러다",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007484.1.12",
          "form": "놓치는것도 있는거같기도하구요",
          "original_form": "놓치는것도 있는거같기도하구요",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007484.1.13",
          "form": "내려놓음 편할건데 맘처럼 안되네요ㅎㅎ",
          "original_form": "내려놓음 편할건데 맘처럼 안되네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007484.1.14",
          "form": "이러다 손목터널증후군 올거같아요",
          "original_form": "이러다 손목터널증후군 올거같아요",
          "speaker_id": "1",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007484.1.15",
          "form": "내려놔야하는데 그러질못하니 ㅎㅎ",
          "original_form": "내려놔야하는데 그러질못하니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007484.1.16",
          "form": "어디 더 안좋아지시면 큰일이니 건강챙기시면서 하세요",
          "original_form": "어디 더 안좋아지시면 큰일이니 건강챙기시면서 하세요",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007484.1.17",
          "form": "건강 잃으면 다 소용없으니까요",
          "original_form": "건강 잃으면 다 소용없으니까요",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007484.1.18",
          "form": "그래야겠어요 더나이먹음 여기저기 아플듯요^^",
          "original_form": "그래야겠어요 더나이먹음 여기저기 아플듯요^^",
          "speaker_id": "1",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007484.1.19",
          "form": "늦은시간인데  안주무세요?  name2님도 불면즘?",
          "original_form": "늦은시간인데  안주무세요?  &name2&님도 불면즘?",
          "speaker_id": "1",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007484.1.20",
          "form": "내일이 일요일이니 좀 무리중이예요ㅎ",
          "original_form": "내일이 일요일이니 좀 무리중이예요ㅎ",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007484.1.21",
          "form": "자는시간이 아까워요 ㅋㅋ",
          "original_form": "자는시간이 아까워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007484.1.22",
          "form": "하루쯤은 괜찮죠",
          "original_form": "하루쯤은 괜찮죠",
          "speaker_id": "1",
          "time": "20210822 02:45"
        },
        {
          "id": "MDRW2100007484.1.23",
          "form": "name2님은 루테인 드시나요?",
          "original_form": "&name2&님은 루테인 드시나요?",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007484.1.24",
          "form": "아뇨 그건안먹어요",
          "original_form": "아뇨 그건안먹어요",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007484.1.25",
          "form": "눈건강에 좋대서 먹어볼까하는데 종류가 너무 많네요",
          "original_form": "눈건강에 좋대서 먹어볼까하는데 종류가 너무 많네요",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007484.1.26",
          "form": "약들은 종류가 많아서",
          "original_form": "약들은 종류가 많아서",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007484.1.27",
          "form": "지인들한테물어보고 먹어요 ㅎ",
          "original_form": "지인들한테물어보고 먹어요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007484.1.28",
          "form": "저도 그래야겠네요",
          "original_form": "저도 그래야겠네요",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007484.1.29",
          "form": "유산균은 랏토핏 많이먹던데",
          "original_form": "유산균은 랏토핏 많이먹던데",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007484.1.30",
          "form": "저도 유산균은 그거먹어요",
          "original_form": "저도 유산균은 그거먹어요",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007484.1.31",
          "form": "노란색이랑 주황색이 또다른가봐요",
          "original_form": "노란색이랑 주황색이 또다른가봐요",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007484.1.32",
          "form": "주황이 뚱보균유전자를 막아주는건지",
          "original_form": "주황이 뚱보균유전자를 막아주는건지",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007484.1.33",
          "form": "언니가 한통줘서 먹어봤네요",
          "original_form": "언니가 한통줘서 먹어봤네요",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007484.1.34",
          "form": "저도 주황먹어요",
          "original_form": "저도 주황먹어요",
          "speaker_id": "2",
          "time": "20210822 02:56"
        },
        {
          "id": "MDRW2100007484.1.35",
