      "utterance": [
        {
          "id": "MDRW2100003812.1.1",
          "form": "너는 어디 치킨 브랜드 좋아해?",
          "original_form": "너는 어디 치킨 브랜드 좋아해?",
          "speaker_id": "2",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003812.1.2",
          "form": "나는 페리카나나 처갓집!",
          "original_form": "나는 페리카나나 처갓집!",
          "speaker_id": "1",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003812.1.3",
          "form": "거기 양념 맛있지",
          "original_form": "거기 양념 맛있지",
          "speaker_id": "2",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003812.1.4",
          "form": "응 진짜 정말정말 맛있어",
          "original_form": "응 진짜 정말정말 맛있어",
          "speaker_id": "1",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003812.1.5",
          "form": "우리 동네에는 없어서 못 시켜먹어",
          "original_form": "우리 동네에는 없어서 못 시켜먹어",
          "speaker_id": "2",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003812.1.6",
          "form": "아쉽다",
          "original_form": "아쉽다",
          "speaker_id": "1",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003812.1.7",
          "form": "먹고 싶다",
          "original_form": "먹고 싶다",
          "speaker_id": "2",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003812.1.8",
          "form": "나도 ㅋㅋㅋ",
          "original_form": "나도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003812.1.9",
          "form": "예전에 한강 갔을 때 친구가 시켜서 먹었어",
          "original_form": "예전에 한강 갔을 때 친구가 시켜서 먹었어",
          "speaker_id": "2",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003812.1.10",
          "form": "한강 치맥 좋지 ㅋㅋ",
          "original_form": "한강 치맥 좋지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003812.1.11",
          "form": "술은 안먹었어 ㅋㅋ",
          "original_form": "술은 안먹었어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003812.1.12",
          "form": "그럼 치콜 ㅋㅋㅋ",
          "original_form": "그럼 치콜 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:28"
        },
        {
          "id": "MDRW2100003812.1.13",
          "form": "맞아 ㅋㅋㅋ",
          "original_form": "맞아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:28"
        },
        {
          "id": "MDRW2100003812.1.14",
          "form": "아 나 저번에 삼촌이 치킨 가져다주셨는데",
          "original_form": "아 나 저번에 삼촌이 치킨 가져다주셨는데",
