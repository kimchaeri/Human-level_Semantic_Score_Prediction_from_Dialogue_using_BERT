      "utterance": [
        {
          "id": "MDRW2100032396.1.1",
          "form": "봉사하시는거 있으세요?",
          "original_form": "봉사하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100032396.1.2",
          "form": "저는 아직 없어요 name1님은 있으신가요?",
          "original_form": "저는 아직 없어요 &name1&님은 있으신가요?",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100032396.1.3",
          "form": "저는 가끔 유기견 센터 가요!",
          "original_form": "저는 가끔 유기견 센터 가요!",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100032396.1.4",
          "form": "우와 유기견 센터!! 강아지 좋아하시나봐요!",
          "original_form": "우와 유기견 센터!! 강아지 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032396.1.5",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032396.1.6",
          "form": "아하 저 강아지 키우거든요!!",
          "original_form": "아하 저 강아지 키우거든요!!",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032396.1.7",
          "form": "그래서 가끔 시간 빌 때 가용",
          "original_form": "그래서 가끔 시간 빌 때 가용",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100032396.1.8",
          "form": "오오 정말 좋은 일 하시네요ㅠㅠ 대단해요!",
          "original_form": "오오 정말 좋은 일 하시네요ㅠㅠ 대단해요!",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032396.1.9",
          "form": "유기견 센터 봉사가면 주로 뭐하시나요??",
          "original_form": "유기견 센터 봉사가면 주로 뭐하시나요??",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032396.1.10",
          "form": "거기서 그냥 청소하구 애들 밥주구 그래요!",
          "original_form": "거기서 그냥 청소하구 애들 밥주구 그래요!",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100032396.1.11",
          "form": "별거 없죠ㅋㅋㅋ",
          "original_form": "별거 없죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100032396.1.12",
          "form": "너무 별거죠!!! 사랑이 아니면 할 수 없는 일이라고 생각해요",
          "original_form": "너무 별거죠!!! 사랑이 아니면 할 수 없는 일이라고 생각해요",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032396.1.13",
          "form": "맞아요ㅠㅠ 좋아서 하는 일이어서 하는 거지",
          "original_form": "맞아요ㅠㅠ 좋아서 하는 일이어서 하는 거지",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100032396.1.14",
          "form": "아니면 안할 거 같아요!",
          "original_form": "아니면 안할 거 같아요!",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100032396.1.15",
          "form": "그러니까요 저는 동물을 무서워해서 그런 봉사는 못해봤어요ㅠㅠ",
          "original_form": "그러니까요 저는 동물을 무서워해서 그런 봉사는 못해봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032396.1.16",
          "form": "아하ㅠㅠ 근데 진짜 자기에게 맞는 봉사가 최곤거 같아요",
          "original_form": "아하ㅠㅠ 근데 진짜 자기에게 맞는 봉사가 최곤거 같아요",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032396.1.17",
          "form": "저는 동물 다 좋아하거든용!!!",
          "original_form": "저는 동물 다 좋아하거든용!!!",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032396.1.18",
          "form": "맨날 쓰레기줍기 환경미화 응급조치교육봉사(?) 이런 것만 해봤어요",
          "original_form": "맨날 쓰레기줍기 환경미화 응급조치교육봉사(?) 이런 것만 해봤어요",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032396.1.19",
          "form": "오오 동물!! 그럼 또 키워보고 싶은 동물 있으세요??",
          "original_form": "오오 동물!! 그럼 또 키워보고 싶은 동물 있으세요??",
          "speaker_id": "2",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032396.1.20",
          "form": "우와 그런 봉사도 좋죠!",
          "original_form": "우와 그런 봉사도 좋죠!",
          "speaker_id": "1",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032396.1.21",
          "form": "저는 푸들 색깔별로 키우고 싶어요 ㅋㅋㅋ",
          "original_form": "저는 푸들 색깔별로 키우고 싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032396.1.22",
