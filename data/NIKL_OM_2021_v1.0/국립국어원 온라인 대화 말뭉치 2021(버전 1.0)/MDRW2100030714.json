      "utterance": [
        {
          "id": "MDRW2100030714.1.1",
          "form": "국민지원금 받으면",
          "original_form": "국민지원금 받으면",
          "speaker_id": "1",
          "time": "20210904 11:41"
        },
        {
          "id": "MDRW2100030714.1.2",
          "form": "뭐 사고 싶냐",
          "original_form": "뭐 사고 싶냐",
          "speaker_id": "1",
          "time": "20210904 11:41"
        },
        {
          "id": "MDRW2100030714.1.3",
          "form": "요즘 사고싶은거 있어?",
          "original_form": "요즘 사고싶은거 있어?",
          "speaker_id": "2",
          "time": "20210904 12:09"
        },
        {
          "id": "MDRW2100030714.1.4",
          "form": "나 밴앤제리 아이스크림.",
          "original_form": "나 밴앤제리 아이스크림.",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030714.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030714.1.6",
          "form": "제발 사라",
          "original_form": "제발 사라",
          "speaker_id": "2",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030714.1.7",
          "form": "신용카드사에서 이용가능 가맹점 검색도 되서 그거 보고",
          "original_form": "신용카드사에서 이용가능 가맹점 검색도 되서 그거 보고",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030714.1.8",
          "form": "이용 가능한 곳에서",
          "original_form": "이용 가능한 곳에서",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030714.1.9",
          "form": "25만원 탕진예정",
          "original_form": "25만원 탕진예정",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030714.1.10",
          "form": "난 사먹는거에 많이쓸것같아",
          "original_form": "난 사먹는거에 많이쓸것같아",
          "speaker_id": "2",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030714.1.11",
          "form": "난 집앞에 유가네 있는 넘이야",
          "original_form": "난 집앞에 유가네 있는 넘이야",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030714.1.12",
          "form": "집앞에서 다 해결한다",
          "original_form": "집앞에서 다 해결한다",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030714.1.13",
          "form": "이갈 위해서 외출 가능",
          "original_form": "이갈 위해서 외출 가능",
          "speaker_id": "1",
          "time": "20210904 12:11"
        },
        {
          "id": "MDRW2100030714.1.14",
          "form": "부럽다... 난 집앞에 음식점들 거의없는데",
          "original_form": "부럽다... 난 집앞에 음식점들 거의없는데",
          "speaker_id": "2",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030714.1.15",
          "form": "집 위치가 좋다",
          "original_form": "집 위치가 좋다",
          "speaker_id": "1",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030714.1.16",
          "form": "옷도 살 수 있음 좋겠는데",
          "original_form": "옷도 살 수 있음 좋겠는데",
          "speaker_id": "1",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030714.1.17",
          "form": "쇼핑하고싶어~",
          "original_form": "쇼핑하고싶어~",
          "speaker_id": "2",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030714.1.18",
          "form": "부츠.",
          "original_form": "부츠.",
          "speaker_id": "1",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030714.1.19",
          "form": "신발광공",
          "original_form": "신발광공",
          "speaker_id": "2",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030714.1.20",
          "form": "아 카페도 가야함",
