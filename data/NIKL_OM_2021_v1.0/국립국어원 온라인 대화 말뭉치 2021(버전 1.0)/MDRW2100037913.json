      "utterance": [
        {
          "id": "MDRW2100037913.1.1",
          "form": "식음료는 참 딱 생각나는게 없네요",
          "original_form": "식음료는 참 딱 생각나는게 없네요",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037913.1.2",
          "form": "물을 젤 많이 마셔요",
          "original_form": "물을 젤 많이 마셔요",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037913.1.3",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037913.1.4",
          "form": "저도 음료는 그냥 물 제일 많이 마시는 거 같아요",
          "original_form": "저도 음료는 그냥 물 제일 많이 마시는 거 같아요",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100037913.1.5",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100037913.1.6",
          "form": "네 오늘 칼국수 뜨끈하게 먹었습니다ㅎㅎ",
          "original_form": "네 오늘 칼국수 뜨끈하게 먹었습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037913.1.7",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037913.1.8",
          "form": "와 너무 맛있었을 거 같아요",
          "original_form": "와 너무 맛있었을 거 같아요",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037913.1.9",
          "form": "저는 미역국 먹었어요",
          "original_form": "저는 미역국 먹었어요",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037913.1.10",
          "form": "직접 만들어 드셨나요?",
          "original_form": "직접 만들어 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037913.1.11",
          "form": "미역국도 맛나죠~",
          "original_form": "미역국도 맛나죠~",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037913.1.12",
          "form": "아뇨 사먹었어요 ㅋㅋㅋ 김치가 국산이라 더 리필했습니당",
          "original_form": "아뇨 사먹었어요 ㅋㅋㅋ 김치가 국산이라 더 리필했습니당",
          "speaker_id": "1",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037913.1.13",
          "form": "요즘 쌀쌀해서 칼국수가 너무 땡겨요",
          "original_form": "요즘 쌀쌀해서 칼국수가 너무 땡겨요",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100037913.1.14",
          "form": "그쵸ㅠㅠ 뜨거운 국물이 필요해요",
          "original_form": "그쵸ㅠㅠ 뜨거운 국물이 필요해요",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037913.1.15",
          "form": "한식 너무 좋아요",
          "original_form": "한식 너무 좋아요",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037913.1.16",
          "form": "요리 자주 하시는 편인가요?",
          "original_form": "요리 자주 하시는 편인가요?",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037913.1.17",
          "form": "맞아요 한식 너무 좋아요",
          "original_form": "맞아요 한식 너무 좋아요",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037913.1.18",
          "form": "저 혼자 있으면 잘 안해요",
          "original_form": "저 혼자 있으면 잘 안해요",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037913.1.19",
          "form": "누구 있음 배고프다 하면 해주구 그래요 ㅋㅋㅋ",
          "original_form": "누구 있음 배고프다 하면 해주구 그래요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037913.1.20",
          "form": "요리 너무 귀찮아요ㅋㅋㅋㅋㅋ",
          "original_form": "요리 너무 귀찮아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037913.1.21",
          "form": "주로 사서 드시나요?",
          "original_form": "주로 사서 드시나요?",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100037913.1.22",
          "form": "약간 반반이에요~ 요즘은 워낙 밖에서 못 먹어서ㅠㅠ",
          "original_form": "약간 반반이에요~ 요즘은 워낙 밖에서 못 먹어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100037913.1.23",
          "form": "코로나 끝나야 편히 먹으러 갈 것 같아요",
          "original_form": "코로나 끝나야 편히 먹으러 갈 것 같아요",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100037913.1.24",
          "form": "맞아요ㅠㅠ 저도 외식은 거의 안 하고 배달만 좀 시키는 거 같아요",
          "original_form": "맞아요ㅠㅠ 저도 외식은 거의 안 하고 배달만 좀 시키는 거 같아요",
          "speaker_id": "2",
          "time": "20210901 20:48"
        }
      ]
