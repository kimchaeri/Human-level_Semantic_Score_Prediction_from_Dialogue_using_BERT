        },
        {
          "id": "MDRW2100013411.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013411.1.3",
          "form": "최근에 읽은책이 있나요?",
          "original_form": "최근에 읽은책이 있나요?",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013411.1.4",
          "form": "아니요 최근에는 책을 읽지 않았어요",
          "original_form": "아니요 최근에는 책을 읽지 않았어요",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013411.1.5",
          "form": "가장 기억에남는 책 있어요?",
          "original_form": "가장 기억에남는 책 있어요?",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013411.1.6",
          "form": "최근에 읽은 헝거게임이라는 책이 기억에남네요",
          "original_form": "최근에 읽은 헝거게임이라는 책이 기억에남네요",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013411.1.7",
          "form": "영화로도 만들어졌어요",
          "original_form": "영화로도 만들어졌어요",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013411.1.8",
          "form": "만화책은 좋아하세요?",
          "original_form": "만화책은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013411.1.9",
          "form": "만화책도 요즘엔 읽지 않았네요.",
          "original_form": "만화책도 요즘엔 읽지 않았네요.",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013411.1.10",
          "form": "베스트셀러에 있는 책들 제목만 본적있어요",
          "original_form": "베스트셀러에 있는 책들 제목만 본적있어요",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013411.1.11",
          "form": "책 자주 읽으시나요?",
          "original_form": "책 자주 읽으시나요?",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013411.1.12",
          "form": "가끔씩 읽어요",
          "original_form": "가끔씩 읽어요",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013411.1.13",
          "form": "요즘은 히가시노 게이고의 책을 읽고 있어요",
          "original_form": "요즘은 히가시노 게이고의 책을 읽고 있어요",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013411.1.14",
          "form": "잔잔하지만 역시 지루하네요",
          "original_form": "잔잔하지만 역시 지루하네요",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013411.1.15",
          "form": "주로 어떤시간에 책을 읽으세요?",
          "original_form": "주로 어떤시간에 책을 읽으세요?",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013411.1.16",
          "form": "저는 업무도중 남는시간에 읽어요",
          "original_form": "저는 업무도중 남는시간에 읽어요",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013411.1.17",
          "form": "오 업무시간에 책도 읽으시고 부지런하시네요.",
          "original_form": "오 업무시간에 책도 읽으시고 부지런하시네요.",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013411.1.18",
          "form": "저는 오디오북을 가끔 듣기도해요 ㅋㅋㅋ",
          "original_form": "저는 오디오북을 가끔 듣기도해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013411.1.19",
          "form": "오디오북은 한번도 들어본적없어요",
          "original_form": "오디오북은 한번도 들어본적없어요",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013411.1.20",
          "form": "어디에서 들을수있어요?",
          "original_form": "어디에서 들을수있어요?",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100013411.1.21",
          "form": "저는 밀리의서재에서 듣고있어요",
          "original_form": "저는 밀리의서재에서 듣고있어요",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013411.1.22",
          "form": "이동할때 들으면 좋더라구요",
          "original_form": "이동할때 들으면 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013411.1.23",
          "form": "오프라인 서점이 별로 없기도 하고..ㅠ",
          "original_form": "오프라인 서점이 별로 없기도 하고..ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013411.1.24",
          "form": "아하 밀리의 서재 들어보기만했는데 나중에 한번 봐야겠네요",
          "original_form": "아하 밀리의 서재 들어보기만했는데 나중에 한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013411.1.25",
          "form": "유료 결제죠?",
          "original_form": "유료 결제죠?",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013411.1.26",
          "form": "네 맞아요. 저는 무료 이용권이 있어서 체험중이에요",
          "original_form": "네 맞아요. 저는 무료 이용권이 있어서 체험중이에요",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013411.1.27",
          "form": "종이책을 읽는거랑은 또 다른 느낌이에요",
          "original_form": "종이책을 읽는거랑은 또 다른 느낌이에요",
          "speaker_id": "1",
          "time": "20210825 14:38"
        }
      ]
    }
