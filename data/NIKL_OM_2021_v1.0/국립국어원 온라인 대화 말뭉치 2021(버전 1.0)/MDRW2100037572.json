      "utterance": [
        {
          "id": "MDRW2100037572.1.1",
          "form": "저도ㅋㅋㅋㅋ어우 점심 너무 맛있었어요",
          "original_form": "저도ㅋㅋㅋㅋ어우 점심 너무 맛있었어요",
          "speaker_id": "2",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100037572.1.2",
          "form": "ㅋㅋ오 오늘점심 어떤거였어요?",
          "original_form": "ㅋㅋ오 오늘점심 어떤거였어요?",
          "speaker_id": "1",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100037572.1.3",
          "form": "매달 1일마다 생일상 나오는데",
          "original_form": "매달 1일마다 생일상 나오는데",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037572.1.4",
          "form": "미역국이랑 비빔만두랑 궁중떡볶이요ㅎㅎㅎ",
          "original_form": "미역국이랑 비빔만두랑 궁중떡볶이요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037572.1.5",
          "form": "헐 대박!!!!! 매달 생일 챙겨주는거 너무 좋아요!!!",
          "original_form": "헐 대박!!!!! 매달 생일 챙겨주는거 너무 좋아요!!!",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037572.1.6",
          "form": "와 미역국 비빔만두 떡볶이 메뉴 정말 짱이었네요 ㅋㅋㅋ",
          "original_form": "와 미역국 비빔만두 떡볶이 메뉴 정말 짱이었네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037572.1.7",
          "form": "그쵸 뭔가 집에서 이런 메뉴 해먹으려면",
          "original_form": "그쵸 뭔가 집에서 이런 메뉴 해먹으려면",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100037572.1.8",
          "form": "진짜 손 많이 가잖아요",
          "original_form": "진짜 손 많이 가잖아요",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100037572.1.9",
          "form": "맞아요 ㅋㅋ진짜 집에서 해먹기 귀찮은데 맛있는 음식들! ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ진짜 집에서 해먹기 귀찮은데 맛있는 음식들! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100037572.1.10",
          "form": "회사에서 생일에 케익도 챙겨주나요?",
          "original_form": "회사에서 생일에 케익도 챙겨주나요?",
          "speaker_id": "1",
          "time": "20210901 16:38"
        },
        {
          "id": "MDRW2100037572.1.11",
          "form": "아니요ㅋㅋㅋㅋ 그런건 잘 안해요ㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋ 그런건 잘 안해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037572.1.12",
          "form": "아항 그대신 1일마다 생일메뉴로 챙겨주는군요!",
          "original_form": "아항 그대신 1일마다 생일메뉴로 챙겨주는군요!",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037572.1.13",
          "form": "일과시간에 간식은 안드셨나요?",
          "original_form": "일과시간에 간식은 안드셨나요?",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037572.1.14",
          "form": "간식 제가 사놨는데 부장님이 다른 분들에게 선물해줬어요ㅋㅋㅋㅋㅋ",
          "original_form": "간식 제가 사놨는데 부장님이 다른 분들에게 선물해줬어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100037572.1.15",
          "form": "헐ㅋㅋㅋ부장님ㅋㅋㅋㅋ 어떤거 사두셨나요?",
          "original_form": "헐ㅋㅋㅋ부장님ㅋㅋㅋㅋ 어떤거 사두셨나요?",
          "speaker_id": "1",
          "time": "20210902 08:15"
        },
        {
          "id": "MDRW2100037572.1.16",
          "form": "엄마손이 원플원하길래 소소하게 사와봤거든요ㅋㅋㅋㅋ",
          "original_form": "엄마손이 원플원하길래 소소하게 사와봤거든요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100037572.1.17",
          "form": "헐 저 엄마손쿠키 진짜 좋아하는데 ㅋㅋㅋ 어디서 원플원했나요!!",
          "original_form": "헐 저 엄마손쿠키 진짜 좋아하는데 ㅋㅋㅋ 어디서 원플원했나요!!",
          "speaker_id": "1",
          "time": "20210902 09:53"
        },
        {
          "id": "MDRW2100037572.1.18",
          "form": "이마트몰에서 샀어요ㅋㅋㅋㅋ",
          "original_form": "이마트몰에서 샀어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100037572.1.19",
          "form": "오 저도 사야겠어요 ㅋㅋㅋ",
          "original_form": "오 저도 사야겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100037572.1.20",
          "form": "엄마손파이가 작게 포장되어있어서 한번에 먹기도좋고",
          "original_form": "엄마손파이가 작게 포장되어있어서 한번에 먹기도좋고",
          "speaker_id": "1",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100037572.1.21",
          "form": "맛도 깔끔 달달해서 자꾸 끌리더라구요",
          "original_form": "맛도 깔끔 달달해서 자꾸 끌리더라구요",
          "speaker_id": "1",
          "time": "20210902 12:16"
        }
      ]
    }
