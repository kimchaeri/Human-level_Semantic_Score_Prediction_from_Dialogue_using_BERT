      "utterance": [
        {
          "id": "MDRW2100023454.1.1",
          "form": "근데 대신에 짤주머니",
          "original_form": "근데 대신에 짤주머니",
          "speaker_id": "2",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.2",
          "form": "요리용붓",
          "original_form": "요리용붓",
          "speaker_id": "2",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.3",
          "form": "나 다이소에서",
          "original_form": "나 다이소에서",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.4",
          "form": "마니산단말야",
          "original_form": "마니산단말야",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.5",
          "form": "청소도구",
          "original_form": "청소도구",
          "speaker_id": "2",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.6",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.7",
          "form": "이런건 유용해",
          "original_form": "이런건 유용해",
          "speaker_id": "2",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.8",
          "form": "베이킹도구들",
          "original_form": "베이킹도구들",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.9",
          "form": "렌즈통도 거기서사고",
          "original_form": "렌즈통도 거기서사고",
          "speaker_id": "2",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.10",
          "form": "다이소에서 많이 사는구나",
          "original_form": "다이소에서 많이 사는구나",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.11",
          "form": "플라스틱컵",
          "original_form": "플라스틱컵",
          "speaker_id": "2",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.12",
          "form": "렌즈통은 렌즈가게가서",
          "original_form": "렌즈통은 렌즈가게가서",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.13",
          "form": "공짜로 달라고해",
          "original_form": "공짜로 달라고해",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.14",
          "form": "도 거기서자주사",
          "original_form": "도 거기서자주사",
          "speaker_id": "2",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.15",
          "form": "아...또 공짜를 언급해버렸다",
          "original_form": "아...또 공짜를 언급해버렸다",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.16",
          "form": "렌즈가게에서 뭐 살게없는데도?",
          "original_form": "렌즈가게에서 뭐 살게없는데도?",
          "speaker_id": "2",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.17",
          "form": "아아ㅡ",
          "original_form": "아아ㅡ",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.18",
          "form": "그거 500원주고사는거잖아",
          "original_form": "그거 500원주고사는거잖아",
          "speaker_id": "2",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.19",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.20",
          "form": "나는 왜 공짜를 이캐 좋아할까!",
          "original_form": "나는 왜 공짜를 이캐 좋아할까!",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.21",
          "form": "아니 렌즈가게가서",
          "original_form": "아니 렌즈가게가서",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.22",
          "form": "싼거하나 사면서",
          "original_form": "싼거하나 사면서",
          "speaker_id": "1",
          "time": "20210830 03:29"
        },
        {
          "id": "MDRW2100023454.1.23",
          "form": "3 4개씩 얻어오는거지",
          "original_form": "3 4개씩 얻어오는거지",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.24",
          "form": "그냥 다이소에서 천원주고",
          "original_form": "그냥 다이소에서 천원주고",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.25",
          "form": "그럼 오래 쓸수잇자나",
          "original_form": "그럼 오래 쓸수잇자나",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.26",
          "form": "바보야",
          "original_form": "바보야",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.27",
          "form": "왼쪽오른쪽구분되는거살래",
          "original_form": "왼쪽오른쪽구분되는거살래",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.28",
          "form": "공짜로받은거는",
          "original_form": "공짜로받은거는",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.29",
          "form": "공짜도",
          "original_form": "공짜도",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.30",
          "form": "왼오구분이애매해",
          "original_form": "왼오구분이애매해",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.31",
          "form": "왼오구분돼ㅡㅡ",
          "original_form": "왼오구분돼ㅡㅡ",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.32",
          "form": "바보야",
          "original_form": "바보야",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.33",
          "form": "공짜좋아하는대머리야",
          "original_form": "공짜좋아하는대머리야",
          "speaker_id": "2",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.34",
          "form": "난 대머리 아니야",
          "original_form": "난 대머리 아니야",
          "speaker_id": "1",
          "time": "20210830 03:30"
        },
        {
          "id": "MDRW2100023454.1.35",
          "form": "그리고 다이소 거보다",
          "original_form": "그리고 다이소 거보다",
          "speaker_id": "1",
          "time": "20210830 03:30"
        }
      ]
