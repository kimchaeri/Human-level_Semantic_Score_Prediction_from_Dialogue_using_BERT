      "utterance": [
        {
          "id": "MDRW2100040621.1.1",
          "form": "한때 맥도날드 해피밀 열심히 사먹었어요 ㅎㅎ",
          "original_form": "한때 맥도날드 해피밀 열심히 사먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100040621.1.2",
          "form": "저도 해피밀 은근히 자주 사먹었어요 ㅎㅎ",
          "original_form": "저도 해피밀 은근히 자주 사먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100040621.1.3",
          "form": "근데 아침메뉴 핫케익",
          "original_form": "근데 아침메뉴 핫케익",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040621.1.4",
          "form": "그거 맛있는데 그나마 가까워도 잘 안가게 되더라고요",
          "original_form": "그거 맛있는데 그나마 가까워도 잘 안가게 되더라고요",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040621.1.5",
          "form": "저도요 아침엔 발길이 잘 안떨어져요",
          "original_form": "저도요 아침엔 발길이 잘 안떨어져요",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040621.1.6",
          "form": "맥모닝을 먹고 싶은데 먹을일이 읎어요",
          "original_form": "맥모닝을 먹고 싶은데 먹을일이 읎어요",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040621.1.7",
          "form": "그것만 사먹으러 가기엔 주말은 늦잠자야되고",
          "original_form": "그것만 사먹으러 가기엔 주말은 늦잠자야되고",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040621.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040621.1.9",
          "form": "ㅠㅠ ㅎㅎ 그죠",
          "original_form": "ㅠㅠ ㅎㅎ 그죠",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040621.1.10",
          "form": "배달시키기엔 금액이 안맞고.",
          "original_form": "배달시키기엔 금액이 안맞고.",
          "speaker_id": "1",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100040621.1.11",
          "form": "네 애매하잖아요 ㅋㅋㅋ",
          "original_form": "네 애매하잖아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100040621.1.12",
          "form": "맥도날드 말고 좋아하는 햄버거 브랜드 어디신가여??",
          "original_form": "맥도날드 말고 좋아하는 햄버거 브랜드 어디신가여??",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100040621.1.13",
          "form": "롯데리아 맛있던데요",
          "original_form": "롯데리아 맛있던데요",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040621.1.14",
          "form": "양상추가 통째로 든 느낌인게 좋았어요",
          "original_form": "양상추가 통째로 든 느낌인게 좋았어요",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040621.1.15",
          "form": "롯데리아 요즘 맛이 나아졌다더니 맛있나보네요",
          "original_form": "롯데리아 요즘 맛이 나아졌다더니 맛있나보네요",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040621.1.16",
          "form": "저는 버거킹이 좋아요",
          "original_form": "저는 버거킹이 좋아요",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040621.1.17",
          "form": "생양파!",
          "original_form": "생양파!",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040621.1.18",
          "form": "버거킹도 맛있죠 비싸서 잘 못먹는거죠",
          "original_form": "버거킹도 맛있죠 비싸서 잘 못먹는거죠",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040621.1.19",
          "form": "근데 롯데리아도 비싸더라구요",
          "original_form": "근데 롯데리아도 비싸더라구요",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040621.1.20",
          "form": "맞아옄ㅋㅋ",
          "original_form": "맞아옄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040621.1.21",
          "form": "할인 안받고는 잘 안먹어요",
          "original_form": "할인 안받고는 잘 안먹어요",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040621.1.22",
          "form": "ㅋㅋㅋ 아까워서 차라리 밥먹지",
          "original_form": "ㅋㅋㅋ 아까워서 차라리 밥먹지",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040621.1.23",
          "form": "그렇더라구요 가격이 생각보다 꽤나가요",
          "original_form": "그렇더라구요 가격이 생각보다 꽤나가요",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040621.1.24",
          "form": "수제버거가 요즘은 차라리 정가대비 괜찮다.느껴지더라고요",
          "original_form": "수제버거가 요즘은 차라리 정가대비 괜찮다.느껴지더라고요",
          "speaker_id": "2",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100040621.1.25",
          "form": "사람 생각이 다 비슷하네요",
          "original_form": "사람 생각이 다 비슷하네요",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040621.1.26",
          "form": "저도그런 생각했었어요",
          "original_form": "저도그런 생각했었어요",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040621.1.27",
          "form": "그쵸?? 이삭버거도 먹어보고 싶어욬ㅋㅋ",
          "original_form": "그쵸?? 이삭버거도 먹어보고 싶어욬ㅋㅋ",
