      "utterance": [
        {
          "id": "MDRW2100018261.1.1",
          "form": "선크림을 잘 발랐어야지",
          "original_form": "선크림을 잘 발랐어야지",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018261.1.2",
          "form": "그러게 ㅜㅜ",
          "original_form": "그러게 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018261.1.3",
          "form": "자외선 피부에 안좋다",
          "original_form": "자외선 피부에 안좋다",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018261.1.4",
          "form": "그렇징 ㅠ 피부에 진짜 안좋다잖아 ㅋ 그래도 양산도 샀는데",
          "original_form": "그렇징 ㅠ 피부에 진짜 안좋다잖아 ㅋ 그래도 양산도 샀는데",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018261.1.5",
          "form": "그래 양산 써라",
          "original_form": "그래 양산 써라",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018261.1.6",
          "form": "계속 까먹는당 ㅠㅋㅋㅋ",
          "original_form": "계속 까먹는당 ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018261.1.7",
          "form": "선크림 바르는 습관을 들어야지",
          "original_form": "선크림 바르는 습관을 들어야지",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018261.1.8",
          "form": "그르게.. 피부 한번 상하면 끝인디",
          "original_form": "그르게.. 피부 한번 상하면 끝인디",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018261.1.9",
          "form": "그래 잘 관리해라",
          "original_form": "그래 잘 관리해라",
          "speaker_id": "2",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018261.1.10",
          "form": "옹옹 주름 안생기게 ㅜㅋㅋ",
          "original_form": "옹옹 주름 안생기게 ㅜㅋㅋ",
