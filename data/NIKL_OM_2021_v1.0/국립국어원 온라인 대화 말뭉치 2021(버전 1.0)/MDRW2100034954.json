      "utterance": [
        {
          "id": "MDRW2100034954.1.1",
          "form": "이마트엔 요새 사람많나요?",
          "original_form": "이마트엔 요새 사람많나요?",
          "speaker_id": "1",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100034954.1.2",
          "form": "저희 동네는 늘 사람이 많아요",
          "original_form": "저희 동네는 늘 사람이 많아요",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100034954.1.3",
          "form": "롯데마트 이마트 또 다른 큰 마트 있는데",
          "original_form": "롯데마트 이마트 또 다른 큰 마트 있는데",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100034954.1.4",
          "form": "아하 많군요",
          "original_form": "아하 많군요",
          "speaker_id": "1",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100034954.1.5",
          "form": "제일 많아용",
          "original_form": "제일 많아용",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100034954.1.6",
          "form": "저희동네도 이마트 롯데마트 코스트코 홈플 다양하게있어요",
          "original_form": "저희동네도 이마트 롯데마트 코스트코 홈플 다양하게있어요",
          "speaker_id": "1",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100034954.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100034954.1.8",
          "form": "어디가 더 많은진 몰겠지만",
          "original_form": "어디가 더 많은진 몰겠지만",
          "speaker_id": "1",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100034954.1.9",
          "form": "좋은동네 사시나봐요!",
          "original_form": "좋은동네 사시나봐요!",
          "speaker_id": "2",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100034954.1.10",
          "form": "코스트코 옆동네에 있는데",
          "original_form": "코스트코 옆동네에 있는데",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.11",
          "form": "신상이 나오면 거기로가욯ㅎㅎ",
          "original_form": "신상이 나오면 거기로가욯ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.12",
          "form": "평일 낮에가도 밖까지 줄서요ㅋㅋㅋ",
          "original_form": "평일 낮에가도 밖까지 줄서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.13",
          "form": "아니요 전 가난하고 다른사람이 잘살아요",
          "original_form": "아니요 전 가난하고 다른사람이 잘살아요",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.14",
          "form": "와우!",
          "original_form": "와우!",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.15",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 아니에옄ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 아니에옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.16",
          "form": "코로나가 무색하네요",
          "original_form": "코로나가 무색하네요",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.17",
          "form": "네네ㅜㅠㅠ",
          "original_form": "네네ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.18",
          "form": "코스트코는 항상 대량으로 팔아서 그런지",
          "original_form": "코스트코는 항상 대량으로 팔아서 그런지",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.19",
          "form": "차타고 많이 코스트고가더래요",
          "original_form": "차타고 많이 코스트고가더래요",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.20",
          "form": "4인 가족 모여살때는",
          "original_form": "4인 가족 모여살때는",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.21",
          "form": "코스트코 자주갔었는데",
          "original_form": "코스트코 자주갔었는데",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.22",
          "form": "주차장들어가는 줄이 많았어요",
          "original_form": "주차장들어가는 줄이 많았어요",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034954.1.23",
          "form": "독립하고 나서는 처치곤란이라 안가요",
          "original_form": "독립하고 나서는 처치곤란이라 안가요",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034954.1.24",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034954.1.25",
          "form": "차가 모두 줄서있어",
          "original_form": "차가 모두 줄서있어",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034954.1.26",
          "form": "ㅋㅋㅋ ㅋ",
          "original_form": "ㅋㅋㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100034954.1.27",
          "form": "다 차 끌고 오드라구요",
          "original_form": "다 차 끌고 오드라구요",
          "speaker_id": "2",
          "time": "20210828 22:51"
        }
      ]
    }
