      "utterance": [
        {
          "id": "MDRW2100041587.1.1",
          "form": "전 스벅 쿠키도 좋아해요! 엄청 두툼",
          "original_form": "전 스벅 쿠키도 좋아해요! 엄청 두툼",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041587.1.2",
          "form": "스벅 쿠키는 안 먹어봤는데1",
          "original_form": "스벅 쿠키는 안 먹어봤는데1",
          "speaker_id": "1",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041587.1.3",
          "form": "특히 뭐가 맛있나요",
          "original_form": "특히 뭐가 맛있나요",
          "speaker_id": "1",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041587.1.4",
          "form": "저 다크초코릿 카우보이쿠키요",
          "original_form": "저 다크초코릿 카우보이쿠키요",
          "speaker_id": "2",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100041587.1.5",
          "form": "다들 케이크랑 샌드위치만 먹더라고요ㅋㅋ",
          "original_form": "다들 케이크랑 샌드위치만 먹더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:57"
        },
        {
          "id": "MDRW2100041587.1.6",
          "form": "다크초코면 단맛은 조금 약한가요?!",
          "original_form": "다크초코면 단맛은 조금 약한가요?!",
          "speaker_id": "1",
          "time": "20210906 11:57"
        },
        {
          "id": "MDRW2100041587.1.7",
          "form": "아뇨ㅋㅋ 진한 아메랑 먹어야해요 쫀득한 식감이고 커요!",
          "original_form": "아뇨ㅋㅋ 진한 아메랑 먹어야해요 쫀득한 식감이고 커요!",
          "speaker_id": "2",
          "time": "20210906 12:22"
        },
        {
          "id": "MDRW2100041587.1.8",
          "form": "오오 듣기만해도 좋네요ㅎ",
          "original_form": "오오 듣기만해도 좋네요ㅎ",
          "speaker_id": "1",
          "time": "20210906 12:42"
        },
        {
          "id": "MDRW2100041587.1.9",
          "form": "식후에 디저트로 딱일듯!",
          "original_form": "식후에 디저트로 딱일듯!",
          "speaker_id": "1",
          "time": "20210906 12:42"
        },
        {
          "id": "MDRW2100041587.1.10",
          "form": "서브웨이보다 크고 고급스런 맛이랄까요",
          "original_form": "서브웨이보다 크고 고급스런 맛이랄까요",
          "speaker_id": "2",
          "time": "20210906 13:15"
        },
        {
          "id": "MDRW2100041587.1.11",
          "form": "어제 저녁 드셨나요??",
          "original_form": "어제 저녁 드셨나요??",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100041587.1.12",
          "form": "네! 장어 시켜먹었어요",
          "original_form": "네! 장어 시켜먹었어요",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100041587.1.13",
          "form": "오오!! 고급!!!",
          "original_form": "오오!! 고급!!!",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100041587.1.14",
          "form": "님은 뭐 드셨나요?",
          "original_form": "님은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100041587.1.15",
          "form": "전 비와서 라면!",
          "original_form": "전 비와서 라면!",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100041587.1.16",
          "form": "비올땐 국물 요리죠ㅎㅎ",
          "original_form": "비올땐 국물 요리죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100041587.1.17",
          "form": "무슨 라면인가요?",
          "original_form": "무슨 라면인가요?",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100041587.1.18",
          "form": "진매 먹었습니다",
          "original_form": "진매 먹었습니다",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100041587.1.19",
          "form": "맛잘알ㅎㅎ",
          "original_form": "맛잘알ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100041587.1.20",
          "form": "진순과는 겸상 안합니다ㅎㅎ",
          "original_form": "진순과는 겸상 안합니다ㅎㅎ",
