      "utterance": [
        {
          "id": "MDRW2100040840.1.1",
          "form": "건강을 위해 먹는 영양제 있나요?",
          "original_form": "건강을 위해 먹는 영양제 있나요?",
          "speaker_id": "1",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100040840.1.2",
          "form": "백신 접종하셨나요",
          "original_form": "백신 접종하셨나요",
          "speaker_id": "2",
          "time": "20210905 09:34"
        },
        {
          "id": "MDRW2100040840.1.3",
          "form": "아 저는 비타민이랑 밀크시슬 먹어요",
          "original_form": "아 저는 비타민이랑 밀크시슬 먹어요",
          "speaker_id": "2",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100040840.1.4",
          "form": "밀크시슬은 어디에 좋나요?",
          "original_form": "밀크시슬은 어디에 좋나요?",
          "speaker_id": "1",
          "time": "20210905 09:37"
        },
        {
          "id": "MDRW2100040840.1.5",
          "form": "간에 좋다더라고요",
          "original_form": "간에 좋다더라고요",
          "speaker_id": "2",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100040840.1.6",
          "form": "저는 얼굴에 열이 잘 올랐는데 밀크시슬 먹음 좋다고 해서",
          "original_form": "저는 얼굴에 열이 잘 올랐는데 밀크시슬 먹음 좋다고 해서",
          "speaker_id": "2",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100040840.1.7",
          "form": "먹기 시작했어요",
          "original_form": "먹기 시작했어요",
          "speaker_id": "2",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100040840.1.8",
          "form": "간에 좋은 거군요 알아봐야겠네요",
          "original_form": "간에 좋은 거군요 알아봐야겠네요",
          "speaker_id": "1",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100040840.1.9",
          "form": "저는 칼슘 마그네슘",
          "original_form": "저는 칼슘 마그네슘",
          "speaker_id": "1",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100040840.1.10",
          "form": "비타민 씨랑 디 먹어요",
          "original_form": "비타민 씨랑 디 먹어요",
          "speaker_id": "1",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100040840.1.11",
          "form": "오 다양하게 챙겨드시는군요",
          "original_form": "오 다양하게 챙겨드시는군요",
          "speaker_id": "2",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100040840.1.12",
          "form": "뼈건강이랑 감기예방을 위해서요",
          "original_form": "뼈건강이랑 감기예방을 위해서요",
          "speaker_id": "1",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100040840.1.13",
          "form": "건강관리 열심히 하시네요",
          "original_form": "건강관리 열심히 하시네요",
          "speaker_id": "2",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100040840.1.14",
          "form": "이게 저도 신경안썼었는데",
          "original_form": "이게 저도 신경안썼었는데",
          "speaker_id": "1",
          "time": "20210905 09:44"
        },
        {
          "id": "MDRW2100040840.1.15",
          "form": "코로나 이후로 건강한것이 얼마나 중요한지 느끼게 돼서",
          "original_form": "코로나 이후로 건강한것이 얼마나 중요한지 느끼게 돼서",
          "speaker_id": "1",
          "time": "20210905 09:45"
        },
        {
          "id": "MDRW2100040840.1.16",
          "form": "최근에 챙겨먹기 시작했어요",
          "original_form": "최근에 챙겨먹기 시작했어요",
          "speaker_id": "1",
          "time": "20210905 09:45"
        },
        {
          "id": "MDRW2100040840.1.17",
          "form": "장건강을 위해 유산균도 알아보는 중이에요",
          "original_form": "장건강을 위해 유산균도 알아보는 중이에요",
          "speaker_id": "1",
          "time": "20210905 09:45"
        },
        {
          "id": "MDRW2100040840.1.18",
          "form": "유산균도 많이들 챙겨 먹더라고요",
          "original_form": "유산균도 많이들 챙겨 먹더라고요",
          "speaker_id": "2",
          "time": "20210905 09:45"
        },
        {
          "id": "MDRW2100040840.1.19",
          "form": "네 근데 종류가 많아서 아직 정착하지 못했어요",
          "original_form": "네 근데 종류가 많아서 아직 정착하지 못했어요",
          "speaker_id": "1",
          "time": "20210905 09:50"
        },
        {
          "id": "MDRW2100040840.1.20",
          "form": "요즘 영양제도 워낙 다양해서",
          "original_form": "요즘 영양제도 워낙 다양해서",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100040840.1.21",
          "form": "선택하기 쉽지 않더라고요",
          "original_form": "선택하기 쉽지 않더라고요",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100040840.1.22",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100040840.1.23",
          "form": "무조건 비싸다고 좋은 것도 아니고",
          "original_form": "무조건 비싸다고 좋은 것도 아니고",
          "speaker_id": "1",
          "time": "20210905 09:54"
        },
        {
          "id": "MDRW2100040840.1.24",
          "form": "유명하다고 좋은 것도 아니고",
          "original_form": "유명하다고 좋은 것도 아니고",
          "speaker_id": "1",
          "time": "20210905 09:54"
        },
        {
          "id": "MDRW2100040840.1.25",
          "form": "나한테 잘 맞는 제품 찾는게 쉽지 않아요",
          "original_form": "나한테 잘 맞는 제품 찾는게 쉽지 않아요",
          "speaker_id": "1",
          "time": "20210905 09:54"
        }
      ]
    }
