      "utterance": [
        {
          "id": "MDRW2100004669.1.1",
          "form": "비가 많이 오더니 지금 좀 그쳤ㄴ네",
          "original_form": "비가 많이 오더니 지금 좀 그쳤ㄴ네",
          "speaker_id": "1",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004669.1.2",
          "form": "엉 아까는 하늘에 구멍 뚫린줄 알았다",
          "original_form": "엉 아까는 하늘에 구멍 뚫린줄 알았다",
          "speaker_id": "2",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004669.1.3",
          "form": "그러게 너무 많이 쏟아지더라",
          "original_form": "그러게 너무 많이 쏟아지더라",
          "speaker_id": "1",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004669.1.4",
          "form": "다행히 비 덜 올때 나갔다왔어",
          "original_form": "다행히 비 덜 올때 나갔다왔어",
          "speaker_id": "2",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004669.1.5",
          "form": "급한게 아니면 나가지 말지",
          "original_form": "급한게 아니면 나가지 말지",
          "speaker_id": "1",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004669.1.6",
          "form": "장화신고 금방 슈퍼갔다옴",
          "original_form": "장화신고 금방 슈퍼갔다옴",
          "speaker_id": "2",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004669.1.7",
          "form": "장화가 편하나?",
          "original_form": "장화가 편하나?",
          "speaker_id": "1",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004669.1.8",
          "form": "발이  물에 안젖으니 난 좋더라",
          "original_form": "발이  물에 안젖으니 난 좋더라",
          "speaker_id": "2",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004669.1.9",
          "form": "그래 질퍽한 느낌은 없는데 장화가 딱딱하니 불편하잖아",
          "original_form": "그래 질퍽한 느낌은 없는데 장화가 딱딱하니 불편하잖아",
          "speaker_id": "1",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004669.1.10",
          "form": "첨에는 그러는데 익숙해지면 괜찮응",
          "original_form": "첨에는 그러는데 익숙해지면 괜찮응",
          "speaker_id": "2",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004669.1.11",
          "form": "난 장화사놓고는 몇년째 방치중이다",
          "original_form": "난 장화사놓고는 몇년째 방치중이다",
          "speaker_id": "1",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004669.1.12",
          "form": "안실을꺼면 나줘",
          "original_form": "안실을꺼면 나줘",
          "speaker_id": "2",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004669.1.13",
          "form": "너두 있잖아",
          "original_form": "너두 있잖아",
          "speaker_id": "1",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004669.1.14",
          "form": "16인가 17년도에 산거다",
          "original_form": "16인가 17년도에 산거다",
          "speaker_id": "2",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004669.1.15",
          "form": "오래되었네 좀 바꾸어줘야겠다",
          "original_form": "오래되었네 좀 바꾸어줘야겠다",
          "speaker_id": "1",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004669.1.16",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
