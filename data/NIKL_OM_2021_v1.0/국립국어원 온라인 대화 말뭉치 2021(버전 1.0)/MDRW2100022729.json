      "utterance": [
        {
          "id": "MDRW2100022729.1.1",
          "form": "일주일 이상 지났는데 문제 없길래",
          "original_form": "일주일 이상 지났는데 문제 없길래",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022729.1.2",
          "form": "타이레놀도 안먹었어요!",
          "original_form": "타이레놀도 안먹었어요!",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022729.1.3",
          "form": "1차때는",
          "original_form": "1차때는",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022729.1.4",
          "form": "저도 팔근육 멍든거처럼 아프던데",
          "original_form": "저도 팔근육 멍든거처럼 아프던데",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022729.1.5",
          "form": "2차때도 비슷했군요",
          "original_form": "2차때도 비슷했군요",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022729.1.6",
          "form": "잠결에 옆으로 누웠다가 아파서 깬 적도 있습니다...^^",
          "original_form": "잠결에 옆으로 누웠다가 아파서 깬 적도 있습니다...^^",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022729.1.7",
          "form": "벌써 2차 접종 맞으신건",
          "original_form": "벌써 2차 접종 맞으신건",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022729.1.8",
          "form": "잔여백신 성공해서 마치신건가요?",
          "original_form": "잔여백신 성공해서 마치신건가요?",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022729.1.9",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022729.1.10",
          "form": "어떻게 잘 해서 맞았어요",
          "original_form": "어떻게 잘 해서 맞았어요",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022729.1.11",
          "form": "저도 빨리 2차접종 맞고 걱정안하고싶네요 ㅋㅋ",
          "original_form": "저도 빨리 2차접종 맞고 걱정안하고싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022729.1.12",
          "form": "6주나 기다려야해서",
          "original_form": "6주나 기다려야해서",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022729.1.13",
          "form": "아 저희 엄마도 뉴스 보면서 그렇게 말씀하시더라구요 ㅋㅋㅋㅋ",
          "original_form": "아 저희 엄마도 뉴스 보면서 그렇게 말씀하시더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022729.1.14",
          "form": "너는 접종 끝나서 걱정 안해도 되겠다고 ㅋㅋ",
          "original_form": "너는 접종 끝나서 걱정 안해도 되겠다고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022729.1.15",
          "form": "name2님은",
          "original_form": "&name2&님은",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022729.1.16",
          "form": "몇주정도 후에 2차 맞으셨어요?",
          "original_form": "몇주정도 후에 2차 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022729.1.17",
          "form": "한 달 정도요!",
          "original_form": "한 달 정도요!",
          "speaker_id": "2",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022729.1.18",
          "form": "주위에 2차 접종 끝낸 지인들있으신가요?",
          "original_form": "주위에 2차 접종 끝낸 지인들있으신가요?",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022729.1.19",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022729.1.20",
          "form": "그분들은 부작용 안심했데요?",
          "original_form": "그분들은 부작용 안심했데요?",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022729.1.21",
          "form": "엄청 심하다는 얘기는 카더라로만 들어서",
          "original_form": "엄청 심하다는 얘기는 카더라로만 들어서",
          "speaker_id": "2",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022729.1.22",
          "form": "뉴스보면 심근염에",
          "original_form": "뉴스보면 심근염에",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022729.1.23",
          "form": "두통에 발열 그런분들 많아보여서",
          "original_form": "두통에 발열 그런분들 많아보여서",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022729.1.24",
          "form": "젊은 사람들한테 부작용이 더 심하게 온다더라구여",
          "original_form": "젊은 사람들한테 부작용이 더 심하게 온다더라구여",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022729.1.25",
          "form": "저도 그래서 걱정 많이 했는데 많이 아프진 않더라구요...!",
          "original_form": "저도 그래서 걱정 많이 했는데 많이 아프진 않더라구요...!",
          "speaker_id": "2",
          "time": "20210829 17:37"
        }
      ]
