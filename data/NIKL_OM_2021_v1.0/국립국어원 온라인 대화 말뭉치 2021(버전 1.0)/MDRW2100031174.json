      "utterance": [
        {
          "id": "MDRW2100031174.1.1",
          "form": "저는 잘 모르겠네요",
          "original_form": "저는 잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100031174.1.2",
          "form": "그냥 많으면 많을수록 좋은 거 같아요",
          "original_form": "그냥 많으면 많을수록 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100031174.1.3",
          "form": "그런데 무엇에 대한 수단 요",
          "original_form": "그런데 무엇에 대한 수단 요",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100031174.1.4",
          "form": "돈이 있어야 먹고 살 수 있고",
          "original_form": "돈이 있어야 먹고 살 수 있고",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100031174.1.5",
          "form": "가족을 부양할 수 있고",
          "original_form": "가족을 부양할 수 있고",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100031174.1.6",
          "form": "내  집 마련 도 해야 하니까요",
          "original_form": "내  집 마련 도 해야 하니까요",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031174.1.7",
          "form": "돈의 마법이라는 책을 한번 읽어 보세요",
          "original_form": "돈의 마법이라는 책을 한번 읽어 보세요",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031174.1.8",
          "form": "노후준비도 해야 하구요",
          "original_form": "노후준비도 해야 하구요",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031174.1.9",
          "form": "집은 없나요",
          "original_form": "집은 없나요",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031174.1.10",
          "form": "잊지만 전세에요",
          "original_form": "잊지만 전세에요",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031174.1.11",
          "form": "그럼 큰 걱정거리는 없지 않나요",
          "original_form": "그럼 큰 걱정거리는 없지 않나요",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031174.1.12",
          "form": "국민연금도 내야 해요",
          "original_form": "국민연금도 내야 해요",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031174.1.13",
          "form": "돈을 더 벌면 행복해질까요",
          "original_form": "돈을 더 벌면 행복해질까요",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031174.1.14",
          "form": "글쎄요 그렇지 않을까요",
          "original_form": "글쎄요 그렇지 않을까요",
          "speaker_id": "1",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100031174.1.15",
          "form": "돈은 많아도 걱정 적어도 걱정이라고 하잖아요",
          "original_form": "돈은 많아도 걱정 적어도 걱정이라고 하잖아요",
          "speaker_id": "2",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031174.1.16",
          "form": "그래도 많았으면 좋겠네요",
          "original_form": "그래도 많았으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210904 17:53"
        },
        {
          "id": "MDRW2100031174.1.17",
          "form": "그건 저도 그렇네요",
          "original_form": "그건 저도 그렇네요",
          "speaker_id": "2",
          "time": "20210904 17:53"
        }
      ]
