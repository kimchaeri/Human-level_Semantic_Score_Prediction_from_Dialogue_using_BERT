      "utterance": [
        {
          "id": "MDRW2100018709.1.1",
          "form": "캣휠 사보셧나요?",
          "original_form": "캣휠 사보셧나요?",
          "speaker_id": "1",
          "time": "20210827 11:13"
        },
        {
          "id": "MDRW2100018709.1.2",
          "form": "네! 집에 있어요",
          "original_form": "네! 집에 있어요",
          "speaker_id": "2",
          "time": "20210827 11:13"
        },
        {
          "id": "MDRW2100018709.1.3",
          "form": "잘 타나요? 캣바캣이라던데..",
          "original_form": "잘 타나요? 캣바캣이라던데..",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018709.1.4",
          "form": "가끔 안볼때만 타요..",
          "original_form": "가끔 안볼때만 타요..",
          "speaker_id": "2",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018709.1.5",
          "form": "초반에는 타더니 안타네요ㅠ",
          "original_form": "초반에는 타더니 안타네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 11:18"
        },
        {
          "id": "MDRW2100018709.1.6",
          "form": "더 사고 싶은 아이템있어요?",
          "original_form": "더 사고 싶은 아이템있어요?",
          "speaker_id": "1",
          "time": "20210827 11:19"
        },
        {
          "id": "MDRW2100018709.1.7",
          "form": "최근에는 방석이요!",
          "original_form": "최근에는 방석이요!",
          "speaker_id": "2",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100018709.1.8",
          "form": "? 어떤방석이요?",
          "original_form": "? 어떤방석이요?",
          "speaker_id": "1",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100018709.1.9",
          "form": "마약쿠션이요!",
          "original_form": "마약쿠션이요!",
          "speaker_id": "2",
          "time": "20210827 11:21"
        },
        {
          "id": "MDRW2100018709.1.10",
          "form": "진짜인지 궁금해요ㅋㅋ",
          "original_form": "진짜인지 궁금해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:21"
        },
        {
          "id": "MDRW2100018709.1.11",
          "form": "마약배게 이런것도 있더라구요",
          "original_form": "마약배게 이런것도 있더라구요",
          "speaker_id": "2",
          "time": "20210827 11:21"
        },
        {
          "id": "MDRW2100018709.1.12",
          "form": "그거 맞으면 진짜 효과 좋다던데요..",
          "original_form": "그거 맞으면 진짜 효과 좋다던데요..",
          "speaker_id": "1",
          "time": "20210827 11:22"
        },
        {
          "id": "MDRW2100018709.1.13",
          "form": "써보신거 있으신가요?",
          "original_form": "써보신거 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 11:43"
        },
        {
          "id": "MDRW2100018709.1.14",
          "form": "친구가 사봤는데 만족한다고 하더라구요.. \r\n정확한 제품은 몰겠어요..",
          "original_form": "친구가 사봤는데 만족한다고 하더라구요.. \r\n정확한 제품은 몰겠어요..",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018709.1.15",
          "form": "전 난중에 캣타워 엄청 큰거 사고싶어요..",
          "original_form": "전 난중에 캣타워 엄청 큰거 사고싶어요..",
          "speaker_id": "1",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018709.1.16",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018709.1.17",
          "form": "오 얼마나 큰거요?",
          "original_form": "오 얼마나 큰거요?",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018709.1.18",
          "form": "사람이랑 같이 올라가서 놀 수 있는? ㅋㅋㅋ",
          "original_form": "사람이랑 같이 올라가서 놀 수 있는? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018709.1.19",
          "form": "타잔이랑 치타처럼..",
          "original_form": "타잔이랑 치타처럼..",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018709.1.20",
          "form": "우와ㅋㅋ 멋지네요",
          "original_form": "우와ㅋㅋ 멋지네요",
          "speaker_id": "2",
          "time": "20210827 11:47"
        }
      ]
