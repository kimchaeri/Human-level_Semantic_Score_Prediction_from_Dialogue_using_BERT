      "utterance": [
        {
          "id": "MDRW2100045931.1.1",
          "form": "안녕하세요. 쇼핑 하시는 거 좋아하시나요?",
          "original_form": "안녕하세요. 쇼핑 하시는 거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045931.1.2",
          "form": "네 보는 것도사는거도 좋아해요",
          "original_form": "네 보는 것도사는거도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045931.1.3",
          "form": "주로 어디서 쇼핑 하시나요?",
          "original_form": "주로 어디서 쇼핑 하시나요?",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045931.1.4",
          "form": "인터넷인 것 가타요",
          "original_form": "인터넷인 것 가타요",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045931.1.5",
          "form": "쇼핑몰은 주로 어디를 이용하세요??",
          "original_form": "쇼핑몰은 주로 어디를 이용하세요??",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045931.1.6",
          "form": "네이버 쇼핑이 젤 간편해서 그거 자주 써요",
          "original_form": "네이버 쇼핑이 젤 간편해서 그거 자주 써요",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045931.1.7",
          "form": "저두요! 그래서 네이버 멤버십도 가입했어요ㅎㅎ",
          "original_form": "저두요! 그래서 네이버 멤버십도 가입했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045931.1.8",
          "form": "적립되는게 꽤 쏠쏠하더라구요",
          "original_form": "적립되는게 꽤 쏠쏠하더라구요",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045931.1.9",
          "form": "멤버십 데이? 그런 거 이용하면 혜택도 더 많고요 ㅎㅎ",
          "original_form": "멤버십 데이? 그런 거 이용하면 혜택도 더 많고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045931.1.10",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100045931.1.11",
          "form": "그래서 급하지 않은 건 장바구니에 담아 뒀다가 멤버십데이 때 사는 거 같아요",
          "original_form": "그래서 급하지 않은 건 장바구니에 담아 뒀다가 멤버십데이 때 사는 거 같아요",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045931.1.12",
          "form": "오 머 사셨나요?",
          "original_form": "오 머 사셨나요?",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045931.1.13",
          "form": "저는 추석 선물 샀어요!",
          "original_form": "저는 추석 선물 샀어요!",
          "speaker_id": "1",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100045931.1.14",
          "form": "오 한우요?ㅋㅋㅋㅋㅋ",
          "original_form": "오 한우요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100045931.1.15",
          "form": "아니요 한우는 비싸서 못 사고 평소에 좋아하던 잼이 추석 선물 패키지로 나왔길래 여기저기 보냈어요!",
          "original_form": "아니요 한우는 비싸서 못 사고 평소에 좋아하던 잼이 추석 선물 패키지로 나왔길래 여기저기 보냈어요!",
          "speaker_id": "1",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100045931.1.16",
          "form": "와 잼이요? 저한테 바이럴해주시면 안돼요?",
          "original_form": "와 잼이요? 저한테 바이럴해주시면 안돼요?",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100045931.1.17",
          "form": "당연히 알려 드릴 수 있죠!",
          "original_form": "당연히 알려 드릴 수 있죠!",
          "speaker_id": "1",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045931.1.18",
          "form": "와아 좋아요ㅋㅋㅋㅋ",
          "original_form": "와아 좋아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045931.1.19",
          "form": "오마스마르멜라덴이라고 하는데",
          "original_form": "오마스마르멜라덴이라고 하는데",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045931.1.20",
          "form": "독일 수제 잼이에요!",
          "original_form": "독일 수제 잼이에요!",
          "speaker_id": "1",
          "time": "20210908 09:05"
        }
      ]
