      "utterance": [
        {
          "id": "MDRW2100022850.1.1",
          "form": "저도 키우고싶은데 엄마가 알러지가 있어서 ㅠㅠ",
          "original_form": "저도 키우고싶은데 엄마가 알러지가 있어서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:31"
        },
        {
          "id": "MDRW2100022850.1.2",
          "form": "아.. 그러시구나",
          "original_form": "아.. 그러시구나",
          "speaker_id": "2",
          "time": "20210829 18:32"
        },
        {
          "id": "MDRW2100022850.1.3",
          "form": "네 ㅎㅎ 그래서 부러워요 강아지 키우는집이",
          "original_form": "네 ㅎㅎ 그래서 부러워요 강아지 키우는집이",
          "speaker_id": "1",
          "time": "20210829 18:33"
        },
        {
          "id": "MDRW2100022850.1.4",
          "form": "미니핀도 털 많이 빠지나요?",
          "original_form": "미니핀도 털 많이 빠지나요?",
          "speaker_id": "1",
          "time": "20210829 18:34"
        },
        {
          "id": "MDRW2100022850.1.5",
          "form": "엄청 빠져요\r\n단모가 더 빠진대요",
          "original_form": "엄청 빠져요\r\n단모가 더 빠진대요",
          "speaker_id": "2",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022850.1.6",
          "form": "헐 그렇군요 ??",
          "original_form": "헐 그렇군요 ??",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022850.1.7",
          "form": "털 치우는것도 일이겠네요 ㅠㅜ",
          "original_form": "털 치우는것도 일이겠네요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022850.1.8",
          "form": "털도 까만데 엄청 빠지니 집이 엉망이예요",
          "original_form": "털도 까만데 엄청 빠지니 집이 엉망이예요",
          "speaker_id": "2",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022850.1.9",
          "form": "아 흰티는 절대 못입겠는걸요 ㅜㅜ",
          "original_form": "아 흰티는 절대 못입겠는걸요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 18:43"
        },
        {
          "id": "MDRW2100022850.1.10",
          "form": "우리 아이들은 흰옷이든 신발이든 까미털은 사랑이래요.ㅎ 별로 신경들 안쓰는데 전 힘들어요",
          "original_form": "우리 아이들은 흰옷이든 신발이든 까미털은 사랑이래요.ㅎ 별로 신경들 안쓰는데 전 힘들어요",
          "speaker_id": "2",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022850.1.11",
          "form": "ㅋㅋㅜㅜ 원래 치우는 사람이 젤 힘들잖아요 ㅠㅠ",
          "original_form": "ㅋㅋㅜㅜ 원래 치우는 사람이 젤 힘들잖아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022850.1.12",
          "form": "그래도 키우는게 좋으시죠 ???!",
          "original_form": "그래도 키우는게 좋으시죠 ???!",
          "speaker_id": "1",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022850.1.13",
          "form": "맞아요 ㅎ\r\n그래도 우리에겐 사랑이예요",
          "original_form": "맞아요 ㅎ\r\n그래도 우리에겐 사랑이예요",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022850.1.14",
          "form": "ㅎㅎ 미니핀도 애교쟁인가요 ?",
          "original_form": "ㅎㅎ 미니핀도 애교쟁인가요 ?",
          "speaker_id": "1",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100022850.1.15",
          "form": "애교보다는.. 보호본능 자극요",
          "original_form": "애교보다는.. 보호본능 자극요",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022850.1.16",
          "form": "겁도많고 예민하고..",
          "original_form": "겁도많고 예민하고..",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022850.1.17",
          "form": "아아 지켜줘야할 소중한 애기군요 ㅋㅋㅋ",
          "original_form": "아아 지켜줘야할 소중한 애기군요 ㅋㅋㅋ",
