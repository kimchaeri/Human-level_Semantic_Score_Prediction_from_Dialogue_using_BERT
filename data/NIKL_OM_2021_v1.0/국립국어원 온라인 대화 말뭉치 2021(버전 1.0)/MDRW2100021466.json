      "utterance": [
        {
          "id": "MDRW2100021466.1.1",
          "form": "안녕하세용 저녁 뭐 드셨어용?",
          "original_form": "안녕하세용 저녁 뭐 드셨어용?",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021466.1.2",
          "form": "저 저녁 무려 회시켰어요",
          "original_form": "저 저녁 무려 회시켰어요",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021466.1.3",
          "form": "행복합니다~^^",
          "original_form": "행복합니다~^^",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021466.1.4",
          "form": "우와 오늘 특별한 날이신가요?",
          "original_form": "우와 오늘 특별한 날이신가요?",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021466.1.5",
          "form": "회라니 너무 먹고싶어요",
          "original_form": "회라니 너무 먹고싶어요",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021466.1.6",
          "form": "다이어트 중인데 \r\n주변에서 협조를 안해주는 날이 있잖아요",
          "original_form": "다이어트 중인데 \r\n주변에서 협조를 안해주는 날이 있잖아요",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021466.1.7",
          "form": "그런 날인것같아요",
          "original_form": "그런 날인것같아요",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021466.1.8",
          "form": "아ㅋㅋㅋㅋ 맞아요ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ 맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021466.1.9",
          "form": "저도 빵먹어서",
          "original_form": "저도 빵먹어서",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021466.1.10",
          "form": "또 밀가루를 먹고야 말았네요ㅠㅠ",
          "original_form": "또 밀가루를 먹고야 말았네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021466.1.11",
          "form": "탄수화물은 죄악인데",
          "original_form": "탄수화물은 죄악인데",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021466.1.12",
          "form": "맛있어서 포기할수없어요",
          "original_form": "맛있어서 포기할수없어요",
          "speaker_id": "2",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021466.1.13",
          "form": "다이어트하려면 밀가루 끊어야하는데",
          "original_form": "다이어트하려면 밀가루 끊어야하는데",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021466.1.14",
          "form": "저노 오늘 마카롱 먹었어요ㅋㅋㅋ",
          "original_form": "저노 오늘 마카롱 먹었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021466.1.15",
          "form": "밀가루면 다 좋아서ㅠㅠㅋㅋ",
          "original_form": "밀가루면 다 좋아서ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021466.1.16",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021466.1.17",
          "form": "무슨 맛이었어요?",
          "original_form": "무슨 맛이었어요?",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021466.1.18",
          "form": "홍차라떼맛이랑 치즈맛야ㅡ",
          "original_form": "홍차라떼맛이랑 치즈맛야ㅡ",
          "speaker_id": "2",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021466.1.19",
          "form": "맛이요",
          "original_form": "맛이요",
          "speaker_id": "2",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021466.1.20",
          "form": "마카롱은 무한대로 먹을수 있을것같아요",
          "original_form": "마카롱은 무한대로 먹을수 있을것같아요",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021466.1.21",
          "form": "마카롱은 죄책감만 없다면",
          "original_form": "마카롱은 죄책감만 없다면",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021466.1.22",
          "form": "백개도 먹을수있어욬ㅋㅋㅋ",
          "original_form": "백개도 먹을수있어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021466.1.23",
          "form": "홍차 좋아하는데ㅠㅠ",
          "original_form": "홍차 좋아하는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021466.1.24",
          "form": "name1은 저녁 뭐드셨어요??",
          "original_form": "&name1&은 저녁 뭐드셨어요??",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021466.1.25",
          "form": "빵이랑 우유 먹었어용ㅎㅎ",
          "original_form": "빵이랑 우유 먹었어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021466.1.26",
          "form": "헉...빵 맛있죠 \r\n빵먹을때 우유 국룰이잖아요",
          "original_form": "헉...빵 맛있죠 \r\n빵먹을때 우유 국룰이잖아요",
