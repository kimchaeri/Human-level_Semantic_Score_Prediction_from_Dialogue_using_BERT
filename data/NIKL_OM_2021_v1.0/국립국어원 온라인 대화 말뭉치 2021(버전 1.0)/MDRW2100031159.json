          "original_form": "잘해",
          "speaker_id": "1",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100031159.1.29",
          "form": "오늘선발은 아니지만",
          "original_form": "오늘선발은 아니지만",
          "speaker_id": "1",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100031159.1.30",
          "form": "그릭구 귀여워",
          "original_form": "그릭구 귀여워",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031159.1.31",
          "form": "그래?? 나도 야구 안본지 오래돼서",
          "original_form": "그래?? 나도 야구 안본지 오래돼서",
          "speaker_id": "2",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031159.1.32",
          "form": "우리 미누도 귀엽지만",
          "original_form": "우리 미누도 귀엽지만",
          "speaker_id": "1",
          "time": "20210904 17:44"
