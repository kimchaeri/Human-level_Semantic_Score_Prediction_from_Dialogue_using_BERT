        },
        {
          "id": "MDRW2100031159.1.2",
          "form": "우리 미뉴미누 선발인데 ",
          "original_form": "우리 미뉴미누 선발인데 ",
          "speaker_id": "1",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031159.1.3",
          "form": "어때 최강 한화야?",
          "original_form": "어때 최강 한화야?",
          "speaker_id": "2",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031159.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031159.1.5",
          "form": "1점 뺏겼으 ...ㅎ",
          "original_form": "1점 뺏겼으 ...ㅎ",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031159.1.6",
          "form": "아니아까 잔루만류 넘아쉽",
          "original_form": "아니아까 잔루만류 넘아쉽",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031159.1.7",
          "form": "이열ㄹㄹㄹ",
          "original_form": "이열ㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031159.1.8",
          "form": "좀 야구팬 같습니다?",
          "original_form": "좀 야구팬 같습니다?",
          "speaker_id": "2",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031159.1.9",
          "form": "용어가 막ㅎ",
          "original_form": "용어가 막ㅎ",
          "speaker_id": "2",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031159.1.10",
          "form": "진짜 정은원이붙터 1아웃에 잔루만루였는데 ",
          "original_form": "진짜 정은원이붙터 1아웃에 잔루만루였는데 ",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031159.1.11",
          "form": "멋있네",
          "original_form": "멋있네",
          "speaker_id": "2",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031159.1.12",
          "form": "까비,,,",
          "original_form": "까비,,,",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031159.1.13",
          "form": "ㅎㅎㅎㅎ쓰포츠세계 잼",
          "original_form": "ㅎㅎㅎㅎ쓰포츠세계 잼",
          "speaker_id": "1",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031159.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ멋있구만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ멋있구만",
          "speaker_id": "2",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031159.1.15",
          "form": "기아랑 하네",
          "original_form": "기아랑 하네",
          "speaker_id": "2",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031159.1.16",
          "form": "맏쟈",
          "original_form": "맏쟈",
          "speaker_id": "1",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031159.1.17",
          "form": "요즘 넝무 아깝게 져",
          "original_form": "요즘 넝무 아깝게 져",
          "speaker_id": "1",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031159.1.18",
          "form": "어때 오늘 이길 수 있어?ㅋㅋㅋㅋㅋ",
          "original_form": "어때 오늘 이길 수 있어?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031159.1.19",
          "form": "개인적으로는ㅠ",
          "original_form": "개인적으로는ㅠ",
          "speaker_id": "1",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031159.1.20",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031159.1.21",
          "form": "응...",
          "original_form": "응...",
          "speaker_id": "1",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031159.1.22",
          "form": "져도 돼 괜차나",
          "original_form": "져도 돼 괜차나",
          "speaker_id": "1",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100031159.1.23",
          "form": "근데 아깝다는거지",
          "original_form": "근데 아깝다는거지",
          "speaker_id": "1",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100031159.1.24",
          "form": "그치그리",
          "original_form": "그치그리",
          "speaker_id": "2",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100031159.1.25",
          "form": "기아정도는 잡고 가줘야지",
          "original_form": "기아정도는 잡고 가줘야지",
          "speaker_id": "2",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100031159.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ웅?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ웅?",
