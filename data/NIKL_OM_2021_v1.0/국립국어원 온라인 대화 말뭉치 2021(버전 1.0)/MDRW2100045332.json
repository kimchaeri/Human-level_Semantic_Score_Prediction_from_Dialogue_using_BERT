      "utterance": [
        {
          "id": "MDRW2100045332.1.1",
          "form": "로티보이알지?",
          "original_form": "로티보이알지?",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045332.1.2",
          "form": "나모르는데?!!",
          "original_form": "나모르는데?!!",
          "speaker_id": "2",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045332.1.3",
          "form": "번파는곳이잖아 남목에도 있었지 한때광풍이었다구",
          "original_form": "번파는곳이잖아 남목에도 있었지 한때광풍이었다구",
          "speaker_id": "1",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045332.1.4",
          "form": "로티보이",
          "original_form": "로티보이",
          "speaker_id": "1",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045332.1.5",
          "form": "이친구 모르는게 왜이리많아",
          "original_form": "이친구 모르는게 왜이리많아",
          "speaker_id": "1",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045332.1.6",
          "form": "내말이! 왜케 모르는게많아 우리동네에 있었다니",
          "original_form": "내말이! 왜케 모르는게많아 우리동네에 있었다니",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045332.1.7",
          "form": "번은 알아?",
          "original_form": "번은 알아?",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045332.1.8",
          "form": "빵종류중에",
          "original_form": "빵종류중에",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045332.1.9",
          "form": "커피번 이런거",
          "original_form": "커피번 이런거",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045332.1.10",
          "form": "알아알아좋아해!",
          "original_form": "알아알아좋아해!",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045332.1.11",
          "form": "그거파는 전문점이었다구",
          "original_form": "그거파는 전문점이었다구",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045332.1.12",
          "form": "헐 언제있었어?",
          "original_form": "헐 언제있었어?",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045332.1.13",
          "form": "지금 버정에 쌈지 유플 ㅋㅋㅋ 그자리에있었음",
          "original_form": "지금 버정에 쌈지 유플 ㅋㅋㅋ 그자리에있었음",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100045332.1.14",
          "form": "우리 중학교까지는있었을걸",
          "original_form": "우리 중학교까지는있었을걸",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045332.1.15",
          "form": "나안그래도 커피번 먹고싶어서 내일사야지했음",
          "original_form": "나안그래도 커피번 먹고싶어서 내일사야지했음",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045332.1.16",
          "form": "그러다가 그 옆에 정항우생기고 그랬는데",
          "original_form": "그러다가 그 옆에 정항우생기고 그랬는데",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045332.1.17",
          "form": "지금껏 먹은 커피번중 로티보이를 따라갈 곳이 없어",
          "original_form": "지금껏 먹은 커피번중 로티보이를 따라갈 곳이 없어",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045332.1.18",
          "form": "아.....그때의 나는 뭘하고있었지",
          "original_form": "아.....그때의 나는 뭘하고있었지",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045332.1.19",
          "form": "진짜 뭐했냐 그거안먹고...",
          "original_form": "진짜 뭐했냐 그거안먹고...",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045332.1.20",
          "form": "초딩중딩때 난리였는데....",
          "original_form": "초딩중딩때 난리였는데....",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045332.1.21",
          "form": "떡볶이나 먹었겠지.....샌드위치랑...",
          "original_form": "떡볶이나 먹었겠지.....샌드위치랑...",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045332.1.22",
          "form": "그앞에 지나가기만해도 냄새 대박이었다구",
          "original_form": "그앞에 지나가기만해도 냄새 대박이었다구",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045332.1.23",
          "form": "그앞에",
          "original_form": "그앞에",
          "speaker_id": "1",
          "time": "20210908 02:01"
        }
      ]
    }
