      "utterance": [
        {
          "id": "MDRW2100000322.1.1",
          "form": "오늘은 땅콩 처리해야해",
          "original_form": "오늘은 땅콩 처리해야해",
          "speaker_id": "1",
          "time": "20210818 15:17"
        },
        {
          "id": "MDRW2100000322.1.2",
          "form": "지난 설에 집에서 받아온건데",
          "original_form": "지난 설에 집에서 받아온건데",
          "speaker_id": "1",
          "time": "20210818 15:17"
        },
        {
          "id": "MDRW2100000322.1.3",
          "form": "아직도 안먹었어",
          "original_form": "아직도 안먹었어",
          "speaker_id": "1",
          "time": "20210818 15:17"
        },
        {
          "id": "MDRW2100000322.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ너무했네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ너무했네",
          "speaker_id": "2",
          "time": "20210818 15:17"
        },
        {
          "id": "MDRW2100000322.1.5",
          "form": "얼렁 볶아서 먹엉",
          "original_form": "얼렁 볶아서 먹엉",
          "speaker_id": "2",
          "time": "20210818 15:17"
        },
        {
          "id": "MDRW2100000322.1.6",
          "form": "견과류 아침저녁으로 먹으몀 좋대자나",
          "original_form": "견과류 아침저녁으로 먹으몀 좋대자나",
          "speaker_id": "2",
          "time": "20210818 15:17"
        },
        {
          "id": "MDRW2100000322.1.7",
          "form": "나는 간헐적 단식 하고있어",
          "original_form": "나는 간헐적 단식 하고있어",
          "speaker_id": "2",
          "time": "20210818 15:17"
        },
        {
          "id": "MDRW2100000322.1.8",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210818 15:18"
        },
        {
          "id": "MDRW2100000322.1.9",
          "form": "저녁7시 부터 다음날 12시까지",
          "original_form": "저녁7시 부터 다음날 12시까지",
          "speaker_id": "2",
          "time": "20210818 15:18"
        },
        {
          "id": "MDRW2100000322.1.10",
          "form": "살좀뺄까하고",
          "original_form": "살좀뺄까하고",
          "speaker_id": "2",
          "time": "20210818 15:18"
        },
        {
          "id": "MDRW2100000322.1.11",
          "form": "빠지다가",
          "original_form": "빠지다가",
          "speaker_id": "2",
          "time": "20210818 15:18"
        },
        {
          "id": "MDRW2100000322.1.12",
          "form": "정체기가와서",
          "original_form": "정체기가와서",
          "speaker_id": "2",
          "time": "20210818 15:18"
        },
        {
          "id": "MDRW2100000322.1.13",
          "form": "간헐적단식을 추가했어",
          "original_form": "간헐적단식을 추가했어",
          "speaker_id": "2",
          "time": "20210818 15:18"
        },
        {
          "id": "MDRW2100000322.1.14",
          "form": "운동은?",
          "original_form": "운동은?",
          "speaker_id": "1",
          "time": "20210818 15:18"
        },
        {
          "id": "MDRW2100000322.1.15",
          "form": "운동 할 시간이 없어",
          "original_form": "운동 할 시간이 없어",
          "speaker_id": "2",
          "time": "20210818 15:19"
        },
        {
          "id": "MDRW2100000322.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210818 15:19"
        },
        {
          "id": "MDRW2100000322.1.17",
          "form": "요새도 9시되야 퇴근이야..",
          "original_form": "요새도 9시되야 퇴근이야..",
          "speaker_id": "2",
          "time": "20210818 15:19"
        },
        {
          "id": "MDRW2100000322.1.18",
          "form": "집가면 열시다되가가...",
          "original_form": "집가면 열시다되가가...",
          "speaker_id": "2",
          "time": "20210818 15:19"
        },
        {
          "id": "MDRW2100000322.1.19",
          "form": "운동가봐 레깅스 쭉빵이들 있는데",
          "original_form": "운동가봐 레깅스 쭉빵이들 있는데",
          "speaker_id": "1",
          "time": "20210818 15:19"
        },
        {
          "id": "MDRW2100000322.1.20",
          "form": "로잉머신을 집에 하나 들여놀까",
          "original_form": "로잉머신을 집에 하나 들여놀까",
          "speaker_id": "2",
          "time": "20210818 15:19"
        },
        {
          "id": "MDRW2100000322.1.21",
          "form": "힘들겠다",
          "original_form": "힘들겠다",
          "speaker_id": "1",
          "time": "20210818 15:19"
        },
        {
          "id": "MDRW2100000322.1.22",
          "form": "필라테스를 다녀볼까",
          "original_form": "필라테스를 다녀볼까",
          "speaker_id": "2",
          "time": "20210818 15:19"
        },
        {
          "id": "MDRW2100000322.1.23",
          "form": "고민중이야 허리가 쪼금 불편해다지고",
          "original_form": "고민중이야 허리가 쪼금 불편해다지고",
          "speaker_id": "2",
          "time": "20210818 15:20"
        },
        {
          "id": "MDRW2100000322.1.24",
          "form": "근데 우리나라 필라테스 강사들 실력이 많이 부족하대",
          "original_form": "근데 우리나라 필라테스 강사들 실력이 많이 부족하대",
          "speaker_id": "1",
          "time": "20210818 15:20"
        }
      ]
