      "utterance": [
        {
          "id": "MDRW2100027271.1.1",
          "form": "코로나때문에못간지",
          "original_form": "코로나때문에못간지",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027271.1.2",
          "form": "오래됐어요",
          "original_form": "오래됐어요",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027271.1.3",
          "form": "맥주집도요",
          "original_form": "맥주집도요",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027271.1.4",
          "form": "하나 차리시죠",
          "original_form": "하나 차리시죠",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027271.1.5",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027271.1.6",
          "form": "칭따오랑 어울려요",
          "original_form": "칭따오랑 어울려요",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027271.1.7",
          "form": "맥주는 혼맥좋죠",
          "original_form": "맥주는 혼맥좋죠",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027271.1.8",
          "form": "저녁에 한캔땁니다",
          "original_form": "저녁에 한캔땁니다",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027271.1.9",
          "form": "데스페라도나 타이거맥주 마십니다",
          "original_form": "데스페라도나 타이거맥주 마십니다",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027271.1.10",
          "form": "칭따오는 양꼬치에 좋지요",
          "original_form": "칭따오는 양꼬치에 좋지요",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027271.1.11",
          "form": "타이거",
          "original_form": "타이거",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027271.1.12",
          "form": "레몬도맛났는데",
          "original_form": "레몬도맛났는데",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027271.1.13",
          "form": "자몽도대존맛",
          "original_form": "자몽도대존맛",
          "speaker_id": "2",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027271.1.14",
          "form": "라거도 제취향",
          "original_form": "라거도 제취향",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.15",
          "form": "맛이 다양하군요",
          "original_form": "맛이 다양하군요",
          "speaker_id": "1",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.16",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.17",
          "form": "와인도좋아해요",
          "original_form": "와인도좋아해요",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.18",
          "form": "기본만 마셔봤어요",
          "original_form": "기본만 마셔봤어요",
          "speaker_id": "1",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.19",
          "form": "씨유에서 만원이상 와인사면",
          "original_form": "씨유에서 만원이상 와인사면",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.20",
          "form": "와인은 무스카토 좋아요",
          "original_form": "와인은 무스카토 좋아요",
          "speaker_id": "1",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.21",
          "form": "천원할인쿠폰주는거",
          "original_form": "천원할인쿠폰주는거",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.22",
          "form": "근처에서 행사해서",
          "original_form": "근처에서 행사해서",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.23",
          "form": "씨유에서 와인도 파나요",
          "original_form": "씨유에서 와인도 파나요",
          "speaker_id": "1",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.24",
          "form": "써봤어요",
          "original_form": "써봤어요",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.25",
          "form": "싼가격에 겟해서 개이득",
          "original_form": "싼가격에 겟해서 개이득",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100027271.1.26",
          "form": "와인이 저렴하군요",
