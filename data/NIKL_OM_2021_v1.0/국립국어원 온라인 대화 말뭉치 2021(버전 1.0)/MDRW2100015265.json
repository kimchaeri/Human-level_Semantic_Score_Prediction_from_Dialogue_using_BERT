      "utterance": [
        {
          "id": "MDRW2100015265.1.1",
          "form": "나 곰표 맥주 아직도 못 먹어봄.....",
          "original_form": "나 곰표 맥주 아직도 못 먹어봄.....",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.2",
          "form": "짠 이거 흥미로운 주제같아서",
          "original_form": "짠 이거 흥미로운 주제같아서",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.3",
          "form": "왜냐면 오늘 곰표 팝콘 먹음",
          "original_form": "왜냐면 오늘 곰표 팝콘 먹음",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.4",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.5",
          "form": "아직도?",
          "original_form": "아직도?",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.6",
          "form": "세상에.. 맛 괜찮던데",
          "original_form": "세상에.. 맛 괜찮던데",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.7",
          "form": "아직도.....",
          "original_form": "아직도.....",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.8",
          "form": "편의점에 안 보이기도 하고",
          "original_form": "편의점에 안 보이기도 하고",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.9",
          "form": "근데 난 테라가 더 맛있는것 같기도 해",
          "original_form": "근데 난 테라가 더 맛있는것 같기도 해",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.10",
          "form": "요즘 맥주를 안 마셔서",
          "original_form": "요즘 맥주를 안 마셔서",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.11",
          "form": "아그래...?",
          "original_form": "아그래...?",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.12",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.13",
          "form": "곰표 팝콘은 맛있어?",
          "original_form": "곰표 팝콘은 맛있어?",
          "speaker_id": "2",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015265.1.14",
          "form": "그럼 그냥 안마셔서 그런거아냐??ㅋㅋㅋ",
          "original_form": "그럼 그냥 안마셔서 그런거아냐??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015265.1.15",
          "form": "가성비 괜찮아 곰표 팝콘!!",
          "original_form": "가성비 괜찮아 곰표 팝콘!!",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015265.1.16",
          "form": "근데 유통기한이 넉넉한걸로 사야할듯 내년 3월까진거 먹었는데 약간 눅눅쓰? 렌지 돌리니까 괜찮더라",
          "original_form": "근데 유통기한이 넉넉한걸로 사야할듯 내년 3월까진거 먹었는데 약간 눅눅쓰? 렌지 돌리니까 괜찮더라",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015265.1.17",
          "form": "근데 막 너무 무분별하게 젤리같은것도 많이 나오는듯",
          "original_form": "근데 막 너무 무분별하게 젤리같은것도 많이 나오는듯",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015265.1.18",
          "form": "맞아 콜라보 너무 이것저것 많이 나오더라",
          "original_form": "맞아 콜라보 너무 이것저것 많이 나오더라",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015265.1.19",
          "form": "팝콘 맥주정도까지가 좋았어",
          "original_form": "팝콘 맥주정도까지가 좋았어",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015265.1.20",
          "form": "곰표 두꺼비 정도는 좋은데",
          "original_form": "곰표 두꺼비 정도는 좋은데",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015265.1.21",
          "form": "구두약이나 마카 콜라보는 왜 한지 모르겠음",
          "original_form": "구두약이나 마카 콜라보는 왜 한지 모르겠음",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015265.1.22",
          "form": "말표 ㅋㅋㅋ",
          "original_form": "말표 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015265.1.23",
