      "utterance": [
        {
          "id": "MDRW2100024491.1.1",
          "form": "새내기 시절에 대부분 많이 하지 않나요?",
          "original_form": "새내기 시절에 대부분 많이 하지 않나요?",
          "speaker_id": "2",
          "time": "20210831 11:42"
        },
        {
          "id": "MDRW2100024491.1.2",
          "form": "그치요ㅋㅋ 근데 전 흠.. 외로움도 잘 안타는 편인데다가",
          "original_form": "그치요ㅋㅋ 근데 전 흠.. 외로움도 잘 안타는 편인데다가",
          "speaker_id": "1",
          "time": "20210831 11:46"
        },
        {
          "id": "MDRW2100024491.1.3",
          "form": "뭔가 연애 자체를 하고 싶단 생각 자체는 잘 안들어서 그런지?",
          "original_form": "뭔가 연애 자체를 하고 싶단 생각 자체는 잘 안들어서 그런지?",
          "speaker_id": "1",
          "time": "20210831 11:46"
        },
        {
          "id": "MDRW2100024491.1.4",
          "form": "누군가가 자연스럽게 좋아지지 않는 이상 귀찮더라구요 그런 자리들이ㅋㅋ",
          "original_form": "누군가가 자연스럽게 좋아지지 않는 이상 귀찮더라구요 그런 자리들이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:47"
        },
        {
          "id": "MDRW2100024491.1.5",
          "form": "자만추이시군요ㅎㅎ 소개팅 같은 자리가 어색할 수도 있죠",
          "original_form": "자만추이시군요ㅎㅎ 소개팅 같은 자리가 어색할 수도 있죠",
          "speaker_id": "2",
          "time": "20210831 11:49"
        },
        {
          "id": "MDRW2100024491.1.6",
          "form": "연애가 필수인 세상도 아니구요",
          "original_form": "연애가 필수인 세상도 아니구요",
          "speaker_id": "2",
          "time": "20210831 11:49"
        },
        {
          "id": "MDRW2100024491.1.7",
          "form": "그렇죠ㅎㅎ 사실 전 애인보다는 친구가 더 좋아요",
          "original_form": "그렇죠ㅎㅎ 사실 전 애인보다는 친구가 더 좋아요",
          "speaker_id": "1",
          "time": "20210831 11:50"
        },
        {
          "id": "MDRW2100024491.1.8",
          "form": "그게 훨씬 편하고 좋죠",
          "original_form": "그게 훨씬 편하고 좋죠",
          "speaker_id": "2",
          "time": "20210831 11:51"
        },
        {
          "id": "MDRW2100024491.1.9",
          "form": "그럴지도요ㅋㅋ name2님은 꾸준히 연애하시는 편인가요?",
          "original_form": "그럴지도요ㅋㅋ &name2&님은 꾸준히 연애하시는 편인가요?",
          "speaker_id": "1",
          "time": "20210831 11:52"
        },
        {
          "id": "MDRW2100024491.1.10",
          "form": "현재진행형이에요",
          "original_form": "현재진행형이에요",
          "speaker_id": "2",
          "time": "20210831 11:54"
        },
        {
          "id": "MDRW2100024491.1.11",
          "form": "고등학교 친구랑 사귀고 있어요",
          "original_form": "고등학교 친구랑 사귀고 있어요",
          "speaker_id": "2",
          "time": "20210831 11:54"
        },
        {
          "id": "MDRW2100024491.1.12",
          "form": "오오ㅋㅋ 알고보니 키크고 귀여운건 그 친구 얘기 아닌가요??",
          "original_form": "오오ㅋㅋ 알고보니 키크고 귀여운건 그 친구 얘기 아닌가요??",
          "speaker_id": "1",
          "time": "20210831 11:55"
        },
        {
          "id": "MDRW2100024491.1.13",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 11:55"
        },
        {
          "id": "MDRW2100024491.1.14",
          "form": "그건 또 아닌거 같습니다...ㅋㅋ",
          "original_form": "그건 또 아닌거 같습니다...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:55"
        },
        {
          "id": "MDRW2100024491.1.15",
          "form": "물론 키는 커요",
          "original_form": "물론 키는 커요",
          "speaker_id": "2",
          "time": "20210831 11:56"
        },
        {
          "id": "MDRW2100024491.1.16",
          "form": "ㅋㅋ 그럼 그분은 왜 좋아하게 됐어요? 귀여움 말고 다른 매력인가요",
          "original_form": "ㅋㅋ 그럼 그분은 왜 좋아하게 됐어요? 귀여움 말고 다른 매력인가요",
          "speaker_id": "1",
          "time": "20210831 11:57"
        },
        {
          "id": "MDRW2100024491.1.17",
          "form": "오래 알고 지내다보니 편했어요ㅋㅋ",
          "original_form": "오래 알고 지내다보니 편했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:57"
        },
        {
          "id": "MDRW2100024491.1.18",
          "form": "그런게 최고죠!",
          "original_form": "그런게 최고죠!",
          "speaker_id": "1",
          "time": "20210831 11:58"
        },
        {
          "id": "MDRW2100024491.1.19",
          "form": "대신 더이상 친구 사이는 아니에요",
          "original_form": "대신 더이상 친구 사이는 아니에요",
          "speaker_id": "2",
          "time": "20210831 11:58"
        },
        {
          "id": "MDRW2100024491.1.20",
          "form": "다시 돌아갈 수도 없구요",
          "original_form": "다시 돌아갈 수도 없구요",
          "speaker_id": "2",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100024491.1.21",
          "form": "ㅋㅋㅋㅋㅋ확실하네요",
          "original_form": "ㅋㅋㅋㅋㅋ확실하네요",
          "speaker_id": "1",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100024491.1.22",
          "form": "어떤 것 같아요??",
          "original_form": "어떤 것 같아요??",
          "speaker_id": "1",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100024491.1.23",
          "form": "지금이 더 좋은거죠?",
          "original_form": "지금이 더 좋은거죠?",
