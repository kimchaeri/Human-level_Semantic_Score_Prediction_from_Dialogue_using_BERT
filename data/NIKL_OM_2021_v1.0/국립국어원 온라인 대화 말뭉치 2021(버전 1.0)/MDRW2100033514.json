      "utterance": [
        {
          "id": "MDRW2100033514.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100033514.1.2",
          "form": "아침 드셨어요",
          "original_form": "아침 드셨어요",
          "speaker_id": "2",
          "time": "20210824 10:30"
        },
        {
          "id": "MDRW2100033514.1.3",
          "form": "대충 빵으로 떼웠어요^^",
          "original_form": "대충 빵으로 떼웠어요^^",
          "speaker_id": "1",
          "time": "20210824 10:30"
        },
        {
          "id": "MDRW2100033514.1.4",
          "form": "저는 현미 누룽지 과자 먹었는데 맛있었어요",
          "original_form": "저는 현미 누룽지 과자 먹었는데 맛있었어요",
          "speaker_id": "2",
          "time": "20210824 10:42"
        },
        {
          "id": "MDRW2100033514.1.5",
          "form": "추천 ㅋㅋ",
          "original_form": "추천 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:42"
        },
        {
          "id": "MDRW2100033514.1.6",
          "form": "와 맛있겠어요 과자 이름이모에여?",
          "original_form": "와 맛있겠어요 과자 이름이모에여?",
          "speaker_id": "1",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100033514.1.7",
          "form": "다이어트 때 먹기좋은거 같아요",
          "original_form": "다이어트 때 먹기좋은거 같아요",
          "speaker_id": "2",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100033514.1.8",
          "form": "현미랑 물만 넣었대요 오직",
          "original_form": "현미랑 물만 넣었대요 오직",
          "speaker_id": "2",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100033514.1.9",
          "form": "과자라도 열량이 적나요? 와",
          "original_form": "과자라도 열량이 적나요? 와",
          "speaker_id": "1",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100033514.1.10",
          "form": "음 이름이 그냥 넘 평범해서",
          "original_form": "음 이름이 그냥 넘 평범해서",
          "speaker_id": "2",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100033514.1.11",
          "form": "현미 누룽지?",
          "original_form": "현미 누룽지?",
          "speaker_id": "2",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100033514.1.12",
          "form": "네이버에 참좋은 누룽지인가",
          "original_form": "네이버에 참좋은 누룽지인가",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100033514.1.13",
          "form": "늘 좋은 누룽지인가 치면 구매가능하다던데 ㅋㅋ",
          "original_form": "늘 좋은 누룽지인가 치면 구매가능하다던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100033514.1.14",
          "form": "나중에 한번 검색해볼께요~^^ 좋은정보네요",
          "original_form": "나중에 한번 검색해볼께요~^^ 좋은정보네요",
          "speaker_id": "1",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100033514.1.15",
          "form": "지금 밖이라 갑자기 생각이 안나네요 ㅋㄱ",
          "original_form": "지금 밖이라 갑자기 생각이 안나네요 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100033514.1.16",
          "form": "현미랑 물만 넣은 말그대로 현미누룽지인데",
          "original_form": "현미랑 물만 넣은 말그대로 현미누룽지인데",
          "speaker_id": "2",
          "time": "20210824 11:33"
        },
        {
          "id": "MDRW2100033514.1.17",
          "form": "특수기계로 완전 얇게 눌렀더라고요",
          "original_form": "특수기계로 완전 얇게 눌렀더라고요",
          "speaker_id": "2",
          "time": "20210824 11:33"
        },
        {
          "id": "MDRW2100033514.1.18",
          "form": "손바닥 사이즈로",
          "original_form": "손바닥 사이즈로",
          "speaker_id": "2",
          "time": "20210824 11:33"
        },
        {
          "id": "MDRW2100033514.1.19",
          "form": "근데 넘맛있었어요ㅜ",
          "original_form": "근데 넘맛있었어요ㅜ",
          "speaker_id": "2",
          "time": "20210824 11:33"
        },
        {
          "id": "MDRW2100033514.1.20",
          "form": "걍 과자였음ㅜㅜ",
          "original_form": "걍 과자였음ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100033514.1.21",
          "form": "와~~~~~ 신기하네요 누룽지는. 얇아야 맛있죠",
          "original_form": "와~~~~~ 신기하네요 누룽지는. 얇아야 맛있죠",
          "speaker_id": "1",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100033514.1.22",
          "form": "다이어트 어제부터 시작해서 ㅋㅋ",
          "original_form": "다이어트 어제부터 시작해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100033514.1.23",
          "form": "뭔가 과자 너무 먹고싶어서 먹었네요",
          "original_form": "뭔가 과자 너무 먹고싶어서 먹었네요",
          "speaker_id": "2",
          "time": "20210824 11:35"
        },
        {
          "id": "MDRW2100033514.1.24",
          "form": "근데 넘 마니 먹었어요ㅜ",
          "original_form": "근데 넘 마니 먹었어요ㅜ",
          "speaker_id": "2",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100033514.1.25",
          "form": "이런ㅋ 그래도 누룽지니ㅋㅋ",
