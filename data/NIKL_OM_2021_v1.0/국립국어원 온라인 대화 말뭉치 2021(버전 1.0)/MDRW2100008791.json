      "utterance": [
        {
          "id": "MDRW2100008791.1.1",
          "form": "오늘 저녁은 뭐드시나요?",
          "original_form": "오늘 저녁은 뭐드시나요?",
          "speaker_id": "1",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008791.1.2",
          "form": "김치찌개로 간단히 ㅋㅋㅋ",
          "original_form": "김치찌개로 간단히 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008791.1.3",
          "form": "아이들도 같이 김치찌개먹나요?!",
          "original_form": "아이들도 같이 김치찌개먹나요?!",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008791.1.4",
          "form": "아니요~ 신랑이 매운걸 좋아해서",
          "original_form": "아니요~ 신랑이 매운걸 좋아해서",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008791.1.5",
          "form": "아이는 소세지 구워주면 그거에 밥 뚝딱",
          "original_form": "아이는 소세지 구워주면 그거에 밥 뚝딱",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008791.1.6",
          "form": "오 저도오늘 비엔나소세지구워요 요즘 뽀로로 얼굴 프린팅된 비엔나가 팔더라구요ㅎ",
          "original_form": "오 저도오늘 비엔나소세지구워요 요즘 뽀로로 얼굴 프린팅된 비엔나가 팔더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008791.1.7",
          "form": "오~ 애들이 좋아할만한 비엔나!!",
          "original_form": "오~ 애들이 좋아할만한 비엔나!!",
          "speaker_id": "2",
          "time": "20210822 18:40"
        },
        {
          "id": "MDRW2100008791.1.8",
          "form": "요즘에는 식탁에 기름에 굽거나 튀긴음식 없으면 왜 맛있는거 없냐고...",
          "original_form": "요즘에는 식탁에 기름에 굽거나 튀긴음식 없으면 왜 맛있는거 없냐고...",
          "speaker_id": "2",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100008791.1.9",
          "form": "ㅋㅋㅋ 맛을 알아차렸네요ㅋㅋ 기름이 들어가야 맛나쥬ㅋ",
          "original_form": "ㅋㅋㅋ 맛을 알아차렸네요ㅋㅋ 기름이 들어가야 맛나쥬ㅋ",
          "speaker_id": "1",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008791.1.10",
          "form": "그래서 매번 뭐 구워주고 튀겨주고",
          "original_form": "그래서 매번 뭐 구워주고 튀겨주고",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008791.1.11",
          "form": "근데 구워주는게 진짜 편하지 않아요?? 튀기는건어떤거 해주세요?",
          "original_form": "근데 구워주는게 진짜 편하지 않아요?? 튀기는건어떤거 해주세요?",
          "speaker_id": "1",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008791.1.12",
          "form": "생선가스, 치즈스틱",
          "original_form": "생선가스, 치즈스틱",
          "speaker_id": "2",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008791.1.13",
          "form": "여거 좋아해요",
          "original_form": "여거 좋아해요",
          "speaker_id": "2",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008791.1.14",
          "form": "어디꺼 사서해주세요~~?",
          "original_form": "어디꺼 사서해주세요~~?",
          "speaker_id": "1",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008791.1.15",
          "form": "마트갔을때 사오는데",
          "original_form": "마트갔을때 사오는데",
          "speaker_id": "2",
          "time": "20210822 19:11"
        },
        {
          "id": "MDRW2100008791.1.16",
          "form": "흰살 생선까스라고 써 있네요",
          "original_form": "흰살 생선까스라고 써 있네요",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008791.1.17",
          "form": "가시는 안나오나요??",
          "original_form": "가시는 안나오나요??",
          "speaker_id": "1",
          "time": "20210822 19:15"
