      "utterance": [
        {
          "id": "MDRW2100015798.1.1",
          "form": "혹시 음식도 상큼한 종류 좋아하시나용?",
          "original_form": "혹시 음식도 상큼한 종류 좋아하시나용?",
          "speaker_id": "1",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015798.1.2",
          "form": "네 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "네 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015798.1.3",
          "form": "사실 음식은...",
          "original_form": "사실 음식은...",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015798.1.4",
          "form": "다좋아해요...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다좋아해요...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015798.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저더여...! 딱히 가리는음식이 없는것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저더여...! 딱히 가리는음식이 없는것 같아요",
          "speaker_id": "1",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015798.1.6",
          "form": "달콤한 향 좋아하는거처럼 달달한걸 좋아하는 편이긴해요!",
          "original_form": "달콤한 향 좋아하는거처럼 달달한걸 좋아하는 편이긴해요!",
          "speaker_id": "1",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015798.1.7",
          "form": "이쯤에서 배스킨라빈스 취향이 궁금해집니다..",
          "original_form": "이쯤에서 배스킨라빈스 취향이 궁금해집니다..",
          "speaker_id": "1",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015798.1.8",
          "form": "달달한거 뭐 좋아하세요?",
          "original_form": "달달한거 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015798.1.9",
          "form": "과일이나 디저트?",
          "original_form": "과일이나 디저트?",
          "speaker_id": "2",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015798.1.10",
          "form": "저는 일단 배스킨라빈스에서 기본 초콜렛, 초코칩박힌초코... 를 좋아합니다 ㅋㅋㅋ",
          "original_form": "저는 일단 배스킨라빈스에서 기본 초콜렛, 초코칩박힌초코... 를 좋아합니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015798.1.11",
          "form": "근데 유일하게 좋아하는 상큼한 맛이 하나 있어요!!",
          "original_form": "근데 유일하게 좋아하는 상큼한 맛이 하나 있어요!!",
          "speaker_id": "1",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015798.1.12",
          "form": "궁금궁금 ㅋㅋㅋㅋ",
          "original_form": "궁금궁금 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015798.1.13",
          "form": "두구두구두구 ㅋㅋㅋㅋ",
          "original_form": "두구두구두구 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:19"
        },
        {
          "id": "MDRW2100015798.1.14",
          "form": "맞춰보세요!",
          "original_form": "맞춰보세요!",
          "speaker_id": "1",
          "time": "20210826 01:19"
        },
        {
          "id": "MDRW2100015798.1.15",
          "form": "음... 자두?",
          "original_form": "음... 자두?",
          "speaker_id": "2",
          "time": "20210826 01:21"
        },
        {
          "id": "MDRW2100015798.1.16",
          "form": "아아.... 배스킨라빈스 맛중에요!",
          "original_form": "아아.... 배스킨라빈스 맛중에요!",
          "speaker_id": "1",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015798.1.17",
          "form": "앗 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015798.1.18",
          "form": "배스킨라빈스 맛 아는게 많이 없는데...",
          "original_form": "배스킨라빈스 맛 아는게 많이 없는데...",
          "speaker_id": "2",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015798.1.19",
          "form": "레인보우 샤베트?",
          "original_form": "레인보우 샤베트?",
          "speaker_id": "2",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015798.1.20",
          "form": "음 ㅜㅜ 단순해요 ㅋㅋㅋ 요거트31 입니다!",
          "original_form": "음 ㅜㅜ 단순해요 ㅋㅋㅋ 요거트31 입니다!",
          "speaker_id": "1",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015798.1.21",
          "form": "요거 드셔보셨어용??",
          "original_form": "요거 드셔보셨어용??",
          "speaker_id": "1",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015798.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아하 단순하게생각할걸",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아하 단순하게생각할걸",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015798.1.23",
          "form": "네 좋아하는 맛이에요",
          "original_form": "네 좋아하는 맛이에요",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015798.1.24",
          "form": "ㅋㅋㅋㅋ 이거 진짜 맛있더라구요",
          "original_form": "ㅋㅋㅋㅋ 이거 진짜 맛있더라구요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015798.1.25",
          "form": "초콜렛 사랑에빠진딸기 말고 유일하게 골라먹는 상큼한맛이에옼ㅋㅋ",
          "original_form": "초콜렛 사랑에빠진딸기 말고 유일하게 골라먹는 상큼한맛이에옼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015798.1.26",
          "form": "어떤 맛 좋아하세요?!",
          "original_form": "어떤 맛 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015798.1.27",
          "form": "전 아이스크림은 할매입맛이에요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 아이스크림은 할매입맛이에요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015798.1.28",
          "form": "친구들이 질색팔색해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "친구들이 질색팔색해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015798.1.29",
          "form": "같이먹을때 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "같이먹을때 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015798.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015798.1.31",
          "form": "뭘까여...",
          "original_form": "뭘까여...",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015798.1.32",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015798.1.33",
          "form": "스트로베리?",
          "original_form": "스트로베리?",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015798.1.34",
          "form": "피스타치오아몬드나",
          "original_form": "피스타치오아몬드나",
          "speaker_id": "2",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015798.1.35",
          "form": "녹차맛...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "녹차맛...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015798.1.36",
          "form": "아아~~ 아니 이게 왜! 할매맛이에요!",
          "original_form": "아아~~ 아니 이게 왜! 할매맛이에요!",
          "speaker_id": "1",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015798.1.37",
          "form": "피스타치오아몬드는 가끔씩 먹으면 맛있어요...ㅋㅋㅋㅋ",
          "original_form": "피스타치오아몬드는 가끔씩 먹으면 맛있어요...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015798.1.38",
          "form": "그냥 바닐라맛...",
          "original_form": "그냥 바닐라맛...",
          "speaker_id": "2",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015798.1.39",
          "form": "녹차맛은... 저는 안먹..",
          "original_form": "녹차맛은... 저는 안먹..",
          "speaker_id": "1",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015798.1.40",
          "form": "배스킨라빈스에서 누가 저런거 먹냐몈ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "배스킨라빈스에서 누가 저런거 먹냐몈ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015798.1.41",
          "form": "배스킨에서 그냥 바...닐라..",
          "original_form": "배스킨에서 그냥 바...닐라..",
          "speaker_id": "1",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015798.1.42",
          "form": "그르게욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그르게욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015798.1.43",
          "form": "아니 아이스크림 선택 무슨일이죠 ㅋㅋㅋ",
          "original_form": "아니 아이스크림 선택 무슨일이죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015798.1.44",
          "form": "친구들이 같이 먹을때 질색팔색하는 이유를 알... 것... 같...아.......요........",
          "original_form": "친구들이 같이 먹을때 질색팔색하는 이유를 알... 것... 같...아.......요........",
