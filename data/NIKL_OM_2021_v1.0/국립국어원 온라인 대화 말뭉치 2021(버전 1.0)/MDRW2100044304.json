      "utterance": [
        {
          "id": "MDRW2100044304.1.1",
          "form": "티비프로 뭐 보시나요",
          "original_form": "티비프로 뭐 보시나요",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044304.1.2",
          "form": "음 저는 요새 대탈출이랑",
          "original_form": "음 저는 요새 대탈출이랑",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044304.1.3",
          "form": "환승연애 봅니다!",
          "original_form": "환승연애 봅니다!",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044304.1.4",
          "form": "전 예능 몇가지봐요",
          "original_form": "전 예능 몇가지봐요",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044304.1.5",
          "form": "오 뭐뭐 보세요?",
          "original_form": "오 뭐뭐 보세요?",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044304.1.6",
          "form": "놀면뭐하니 msg워너비 보다가 요즘 안보고 있구요",
          "original_form": "놀면뭐하니 msg워너비 보다가 요즘 안보고 있구요",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044304.1.7",
          "form": "아ㅎㅎ네네",
          "original_form": "아ㅎㅎ네네",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044304.1.8",
          "form": "골목식당, 맛남의광장 가끔 보고",
          "original_form": "골목식당, 맛남의광장 가끔 보고",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044304.1.9",
          "form": "나혼산 전참시도 가끙 봐요ㅋㅋ",
          "original_form": "나혼산 전참시도 가끙 봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044304.1.10",
          "form": "아 ㅎㅎㅎ저도 스쳐지나가면서 조금씩 봤던 프로그램들이네요!",
          "original_form": "아 ㅎㅎㅎ저도 스쳐지나가면서 조금씩 봤던 프로그램들이네요!",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044304.1.11",
          "form": "msg워너비 노래 나온거 들어보셨어요?:)",
          "original_form": "msg워너비 노래 나온거 들어보셨어요?:)",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044304.1.12",
          "form": "네 바라만본다 좋아해요",
          "original_form": "네 바라만본다 좋아해요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044304.1.13",
          "form": "저는 kcm이 노래를 그렇게 잘하시는줄 몰랐어요 ㅎㅎ",
          "original_form": "저는 kcm이 노래를 그렇게 잘하시는줄 몰랐어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044304.1.14",
          "form": "저두 예전에 관심없었는데 잘하더라구요",
          "original_form": "저두 예전에 관심없었는데 잘하더라구요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044304.1.15",
          "form": "확실히 음색이 특이해서 같이 노래하니까 감초 같았던것같아요",
          "original_form": "확실히 음색이 특이해서 같이 노래하니까 감초 같았던것같아요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044304.1.16",
          "form": "원슈타인에 빠져서 그노래만 요즘 들어요ㅋ",
          "original_form": "원슈타인에 빠져서 그노래만 요즘 들어요ㅋ",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044304.1.17",
          "form": "유튜브 보시는거 있으세요?",
          "original_form": "유튜브 보시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044304.1.18",
          "form": "아 원슈타인 좋아하시는군요! 음색 너무 좋죠 ㅎㅎ",
          "original_form": "아 원슈타인 좋아하시는군요! 음색 너무 좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044304.1.19",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044304.1.20",
          "form": "네 저 좀 다양하게 보는편인데 최근에는 주로 동물 테크 시사 보는것같아요",
          "original_form": "네 저 좀 다양하게 보는편인데 최근에는 주로 동물 테크 시사 보는것같아요",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044304.1.21",
          "form": "전 우연히 비보이 브루스리티비 봤는데 빠졌어요ㅋ 세계여행 하는 컨텐츠가 재밌더라구요",
          "original_form": "전 우연히 비보이 브루스리티비 봤는데 빠졌어요ㅋ 세계여행 하는 컨텐츠가 재밌더라구요",
          "speaker_id": "1",
          "time": "20210907 22:20"
        }
      ]
