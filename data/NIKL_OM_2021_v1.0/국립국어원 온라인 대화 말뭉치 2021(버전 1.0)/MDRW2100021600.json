      "utterance": [
        {
          "id": "MDRW2100021600.1.1",
          "form": "집안 분위기는 어떤 편이세요?",
          "original_form": "집안 분위기는 어떤 편이세요?",
          "speaker_id": "1",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021600.1.2",
          "form": "좋은 편이면서도 각자 따로 놀아욬ㅋㅋㅋ",
          "original_form": "좋은 편이면서도 각자 따로 놀아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021600.1.3",
          "form": "앗 ㅋㅋㅋ 그래요? 자유분방해요?",
          "original_form": "앗 ㅋㅋㅋ 그래요? 자유분방해요?",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021600.1.4",
          "form": "아 네네 맞아요 ㅋㅋㅋ",
          "original_form": "아 네네 맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021600.1.5",
          "form": "각자 관심사가 달라서 ㅋㄱㅋㅋ 주말에 각자 취미생활 하러  흩어지는 것 같아요",
          "original_form": "각자 관심사가 달라서 ㅋㄱㅋㅋ 주말에 각자 취미생활 하러  흩어지는 것 같아요",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021600.1.6",
          "form": "name1 집안은 어떠세요?",
          "original_form": "&name1& 집안은 어떠세요?",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021600.1.7",
          "form": "저희집도 자유분방한 편인데 아빠가 좀 보수적이세요",
          "original_form": "저희집도 자유분방한 편인데 아빠가 좀 보수적이세요",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021600.1.8",
          "form": "그래서 가끔 답답해요 ㅜ",
          "original_form": "그래서 가끔 답답해요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021600.1.9",
          "form": "아 그러시구나 ㅜ",
          "original_form": "아 그러시구나 ㅜ",
          "speaker_id": "2",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100021600.1.10",
          "form": "평소에 집안일은 뭐하세요?",
          "original_form": "평소에 집안일은 뭐하세요?",
          "speaker_id": "2",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021600.1.11",
          "form": "청소 빨래 요리 설거지 이 4종 자주해요",
          "original_form": "청소 빨래 요리 설거지 이 4종 자주해요",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021600.1.12",
          "form": "오 요리 잘하세요??",
          "original_form": "오 요리 잘하세요??",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021600.1.13",
          "form": "네 요리는 잘하는 편인데 에어프라이기 덕분인 것 같아요",
          "original_form": "네 요리는 잘하는 편인데 에어프라이기 덕분인 것 같아요",
          "speaker_id": "1",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021600.1.14",
          "form": "와 그럼 보통 뭐해먹으세요?",
          "original_form": "와 그럼 보통 뭐해먹으세요?",
          "speaker_id": "2",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100021600.1.15",
          "form": "다양하게 먹긴하는데 치킨,피자 자주 먹을 때도 있구요",
          "original_form": "다양하게 먹긴하는데 치킨,피자 자주 먹을 때도 있구요",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100021600.1.16",
          "form": "파스타나 라자냐 먹을 때도 있고, 참치회덮밥이나 리조또 등등 먹기도 해요",
          "original_form": "파스타나 라자냐 먹을 때도 있고, 참치회덮밥이나 리조또 등등 먹기도 해요",
          "speaker_id": "1",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100021600.1.17",
          "form": "우와 잘해드시네요",
          "original_form": "우와 잘해드시네요",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021600.1.18",
          "form": "요리하고 나면 뒷정리가 힘들지 않으세요??",
          "original_form": "요리하고 나면 뒷정리가 힘들지 않으세요??",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021600.1.19",
          "form": "저는 정리를 잘 못하는 편이라 요리도 정말 간단한것만 해먹거든요 ㅋㅋ",
          "original_form": "저는 정리를 잘 못하는 편이라 요리도 정말 간단한것만 해먹거든요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100021600.1.20",
          "form": "요리하면서 뒷정리도 같이 해요 ㅎㅎ",
          "original_form": "요리하면서 뒷정리도 같이 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100021600.1.21",
          "form": "근데 다 먹고 배부르고 기분도 좋을 때 설거지 하긴 싫어요 ㅜ",
          "original_form": "근데 다 먹고 배부르고 기분도 좋을 때 설거지 하긴 싫어요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100021600.1.22",
          "form": "집안일 중에 젤 하기 싫은게 뭐에요?",
          "original_form": "집안일 중에 젤 하기 싫은게 뭐에요?",
          "speaker_id": "1",
          "time": "20210828 21:21"
        }
      ]
