      "utterance": [
        {
          "id": "MDRW2100014629.1.1",
          "form": "화면이 빨게요?",
          "original_form": "화면이 빨게요?",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014629.1.2",
          "form": "번인땜에 그런거같은데 전 잘몰겧어요",
          "original_form": "번인땜에 그런거같은데 전 잘몰겧어요",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014629.1.3",
          "form": "근데 다른사람꺼비교함 벌거스름ㅣㄱ",
          "original_form": "근데 다른사람꺼비교함 벌거스름ㅣㄱ",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014629.1.4",
          "form": "제발좀 바꾸라고 다들ㅋㅋ 당사자는 괜찮다는데",
          "original_form": "제발좀 바꾸라고 다들ㅋㅋ 당사자는 괜찮다는데",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014629.1.5",
          "form": "막상 쓰는사람은 모르죠",
          "original_form": "막상 쓰는사람은 모르죠",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014629.1.6",
          "form": "ㅋㅋ 번인이 심하긴하거든요 자판이 다보이니ㅋ",
          "original_form": "ㅋㅋ 번인이 심하긴하거든요 자판이 다보이니ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014629.1.7",
          "form": "눈더 안좋아지시는거아니세요?",
          "original_form": "눈더 안좋아지시는거아니세요?",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014629.1.8",
          "form": "화면에 앱배치한게 다 보일정도ㅋ",
          "original_form": "화면에 앱배치한게 다 보일정도ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014629.1.9",
          "form": "그래서 눈이 침침한가ㅡㅡ",
          "original_form": "그래서 눈이 침침한가ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014629.1.10",
          "form": "새폰써보세요 ㅎㅎ",
          "original_form": "새폰써보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014629.1.11",
          "form": "가격좀 떨어짐 사고싶긴한데",
          "original_form": "가격좀 떨어짐 사고싶긴한데",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014629.1.12",
          "form": "그럼 다른걸 아실듯요 ㅎ",
          "original_form": "그럼 다른걸 아실듯요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014629.1.13",
          "form": "앱테크한다고 넘막쓰니까",
          "original_form": "앱테크한다고 넘막쓰니까",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014629.1.14",
          "form": "새폰사도 또이렇게 될듯해요",
          "original_form": "새폰사도 또이렇게 될듯해요",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014629.1.15",
          "form": "앱테크는 끊을수가 없어요 ㅎ",
          "original_form": "앱테크는 끊을수가 없어요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014629.1.16",
          "form": "근데 막상사며 며칠내로",
          "original_form": "근데 막상사며 며칠내로",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014629.1.17",
          "form": "그니까요ㅎ",
          "original_form": "그니까요ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014629.1.18",
          "form": "기존폰처럼 쓰게되요 ㅎ",
          "original_form": "기존폰처럼 쓰게되요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014629.1.19",
          "form": "폰성지에 문의해보세요",
          "original_form": "폰성지에 문의해보세요",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014629.1.20",
          "form": "저도 아마 똑같이 깔아놓고 쓸듯요",
          "original_form": "저도 아마 똑같이 깔아놓고 쓸듯요",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014629.1.21",
          "form": "생각보다 쌀수도 있어요",
          "original_form": "생각보다 쌀수도 있어요",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014629.1.22",
          "form": "원래 언니회사서 지원금주면 싸게바꾸고했거든요",
          "original_form": "원래 언니회사서 지원금주면 싸게바꾸고했거든요",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014629.1.23",
          "form": "그래서 s7도 100원에샀는데",
          "original_form": "그래서 s7도 100원에샀는데",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014629.1.24",
          "form": "지금비싼가봐요",
          "original_form": "지금비싼가봐요",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014629.1.25",
          "form": "100원이요?",
          "original_form": "100원이요?",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014629.1.26",
          "form": "거저네요 ㅎ",
          "original_form": "거저네요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014629.1.27",
          "form": "언닌 좀주고사고 번호이동이 쌌을때라",
          "original_form": "언닌 좀주고사고 번호이동이 쌌을때라",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014629.1.28",
          "form": "선택약정할인도되고",
          "original_form": "선택약정할인도되고",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014629.1.29",
          "form": "맛가면 바꿔야죠모",
          "original_form": "맛가면 바꿔야죠모",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014629.1.30",
          "form": "그래용 그전에 백업 잘해놓으시구요",
          "original_form": "그래용 그전에 백업 잘해놓으시구요",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014629.1.31",
          "form": "저는 애가 책읽어달라고해서 가유 ㅎ",
          "original_form": "저는 애가 책읽어달라고해서 가유 ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014629.1.32",
          "form": "네네 ㅎ",
          "original_form": "네네 ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:11"
        }
      ]
