          "speaker_id": "1",
          "time": "20210905 19:46"
        },
        {
          "id": "MDRW2100032506.1.5",
          "form": "산책삼아서",
          "original_form": "산책삼아서",
          "speaker_id": "2",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.6",
          "form": "걷기는해~",
          "original_form": "걷기는해~",
          "speaker_id": "2",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.8",
          "form": "보통몇분걸어",
          "original_form": "보통몇분걸어",
          "speaker_id": "1",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.9",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.10",
          "form": "40분?",
          "original_form": "40분?",
          "speaker_id": "2",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.11",
          "form": "많이걷네~",
          "original_form": "많이걷네~",
          "speaker_id": "1",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.12",
          "form": "한 5000보 되겠다",
          "original_form": "한 5000보 되겠다",
          "speaker_id": "1",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.13",
          "form": "응!",
          "original_form": "응!",
          "speaker_id": "2",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.14",
          "form": "나도걸어야하는데",
          "original_form": "나도걸어야하는데",
          "speaker_id": "1",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.15",
          "form": "걸으면 기분 좋아져서ㅎㅎ",
          "original_form": "걸으면 기분 좋아져서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.16",
          "form": "요즘그냥집밖을안나가게되네...ㅜㅠ.ㅠ.ㅠ.ㅠ",
          "original_form": "요즘그냥집밖을안나가게되네...ㅜㅠ.ㅠ.ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.17",
          "form": "맞아 좋아지더랑",
          "original_form": "맞아 좋아지더랑",
          "speaker_id": "1",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.18",
          "form": "아앗ㅠㅠㅜㅜㅠㅜㅜㅠ",
          "original_form": "아앗ㅠㅠㅜㅜㅠㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032506.1.19",
          "form": "넘귀찮지않냐",
          "original_form": "넘귀찮지않냐",
          "speaker_id": "1",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032506.1.20",
          "form": "마저ㅋㅋㄱㅋㄱㄱㄱ",
          "original_form": "마저ㅋㅋㄱㅋㄱㄱㄱ",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032506.1.21",
          "form": "name2은 그럼 걸을때",
          "original_form": "&name2&은 그럼 걸을때",
          "speaker_id": "1",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032506.1.22",
          "form": "산책로같은데를 걸어",
          "original_form": "산책로같은데를 걸어",
          "speaker_id": "1",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032506.1.23",
          "form": "아니면그냥 집주변",
          "original_form": "아니면그냥 집주변",
          "speaker_id": "1",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032506.1.24",
          "form": "공원같은?!",
          "original_form": "공원같은?!",
          "speaker_id": "1",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032506.1.25",
          "form": "집주변!",
          "original_form": "집주변!",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032506.1.26",
          "form": "오오옹",
          "original_form": "오오옹",
          "speaker_id": "1",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032506.1.27",
          "form": "안질려?",
          "original_form": "안질려?",
          "speaker_id": "1",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032506.1.28",
          "form": "난뭔가 질리던뎈큐ㅠㅠ",
          "original_form": "난뭔가 질리던뎈큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100032506.1.29",
          "form": "질리지만...",
          "original_form": "질리지만...",
          "speaker_id": "2",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032506.1.30",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032506.1.31",
          "form": "엄마가 다른데 가는건 힘들다셔서ㅠㅠ",
          "original_form": "엄마가 다른데 가는건 힘들다셔서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032506.1.32",
          "form": "헉거ㅓㄱ",
          "original_form": "헉거ㅓㄱ",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032506.1.33",
          "form": "어머님이랑 같이 가는구나",
          "original_form": "어머님이랑 같이 가는구나",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032506.1.34",
          "form": "그럼 심심하젠않겠당",
          "original_form": "그럼 심심하젠않겠당",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032506.1.35",
          "form": "난 노래들으면서가는데 맨날 똑같은노래를들으니까...",
          "original_form": "난 노래들으면서가는데 맨날 똑같은노래를들으니까...",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032506.1.36",
          "form": "그것도 질리더라구...ㅠㅠ",
          "original_form": "그것도 질리더라구...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032506.1.37",
          "form": "응응!!",
          "original_form": "응응!!",
          "speaker_id": "2",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032506.1.38",
          "form": "아앗...ㅠㅠ",
          "original_form": "아앗...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 19:49"
        },
