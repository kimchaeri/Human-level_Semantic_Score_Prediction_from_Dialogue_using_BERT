      "utterance": [
        {
          "id": "MDRW2100011979.1.1",
          "form": "안녕하세요 집안일 자주하세요?",
          "original_form": "안녕하세요 집안일 자주하세요?",
          "speaker_id": "1",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011979.1.2",
          "form": "자주하긴 하는데 좋아하진 않아요ㅜ",
          "original_form": "자주하긴 하는데 좋아하진 않아요ㅜ",
          "speaker_id": "2",
          "time": "20210824 18:02"
        },
        {
          "id": "MDRW2100011979.1.3",
          "form": "저두요ㅜㅠ저는 특히 설거지 너무 싫어요ㅠㅠ",
          "original_form": "저두요ㅜㅠ저는 특히 설거지 너무 싫어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 18:03"
        },
        {
          "id": "MDRW2100011979.1.4",
          "form": "저는 방 닦는 거ㅋㅋㅋㅋ",
          "original_form": "저는 방 닦는 거ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:03"
        },
        {
          "id": "MDRW2100011979.1.5",
          "form": "허리도 아프고ㅜㅜ",
          "original_form": "허리도 아프고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011979.1.6",
          "form": "맞아요ㅋㅋ 저는 그래서 물 걸 레 청소기 사고 싶어요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ 저는 그래서 물 걸 레 청소기 사고 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:04"
        },
        {
          "id": "MDRW2100011979.1.7",
          "form": "와 그런 것도 있어요?",
          "original_form": "와 그런 것도 있어요?",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011979.1.8",
          "form": "세상 좋다",
          "original_form": "세상 좋다",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011979.1.9",
          "form": "네 점전 편리해지네요ㅎㅎ 건조기도 그렇구ㅎㅎ",
          "original_form": "네 점전 편리해지네요ㅎㅎ 건조기도 그렇구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:06"
        },
        {
          "id": "MDRW2100011979.1.10",
          "form": "돈만 많으면...ㅜㅜ",
          "original_form": "돈만 많으면...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 18:07"
        },
        {
          "id": "MDRW2100011979.1.11",
          "form": "맞아요ㅠㅠ그런 장비가 많아지면 집안일도 재밌게 할수있을텐데ㅜㅜㅋㅋ",
          "original_form": "맞아요ㅠㅠ그런 장비가 많아지면 집안일도 재밌게 할수있을텐데ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100011979.1.12",
          "form": "역시 청소는 장비빨...",
          "original_form": "역시 청소는 장비빨...",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011979.1.13",
          "form": "맞아요ㅋㅋ점점 느껴요 힘들어서ㅋㅋ",
          "original_form": "맞아요ㅋㅋ점점 느껴요 힘들어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011979.1.14",
