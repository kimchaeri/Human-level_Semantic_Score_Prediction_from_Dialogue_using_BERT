      "utterance": [
        {
          "id": "MDRW2100025637.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100025637.1.2",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100025637.1.3",
          "form": "네 잔치국수 먹었습니다",
          "original_form": "네 잔치국수 먹었습니다",
          "speaker_id": "2",
          "time": "20210901 14:48"
        },
        {
          "id": "MDRW2100025637.1.4",
          "form": "면 좋아하시나요?",
          "original_form": "면 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025637.1.5",
          "form": "네 근데 면보다는 밥이 더 좋아요 ㅋㅋ",
          "original_form": "네 근데 면보다는 밥이 더 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025637.1.6",
          "form": "저도 밥이 속이 더 편해서 좋아요",
          "original_form": "저도 밥이 속이 더 편해서 좋아요",
          "speaker_id": "1",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100025637.1.7",
          "form": "볶음밥은 좋아하시나요? 저는 자주해먹어요",
          "original_form": "볶음밥은 좋아하시나요? 저는 자주해먹어요",
          "speaker_id": "1",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100025637.1.8",
          "form": "ㅋㅋ김치볶음밥 좋아하긴하는데 전 거의 시켜먹는 편이라서 잘 안먹어요 ㅜ뭔가 볶음밥 시켜먹기엔 돈이 아깝더라구요",
          "original_form": "ㅋㅋ김치볶음밥 좋아하긴하는데 전 거의 시켜먹는 편이라서 잘 안먹어요 ㅜ뭔가 볶음밥 시켜먹기엔 돈이 아깝더라구요",
          "speaker_id": "2",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025637.1.9",
          "form": "배달음식 자주 드세요?",
          "original_form": "배달음식 자주 드세요?",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025637.1.10",
          "form": "거의 일주일에 다섯번은 먹는 것 같아요 ㅠ",
          "original_form": "거의 일주일에 다섯번은 먹는 것 같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025637.1.11",
          "form": "어떤거 자주드시나요?",
          "original_form": "어떤거 자주드시나요?",
          "speaker_id": "1",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100025637.1.12",
          "form": "거의 족발이나 치킨이요 ㅋㅋ",
          "original_form": "거의 족발이나 치킨이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100025637.1.13",
          "form": "남긴음식 다시먹기좋아서 그런가요?",
          "original_form": "남긴음식 다시먹기좋아서 그런가요?",
          "speaker_id": "1",
          "time": "20210901 16:20"
        },
        {
          "id": "MDRW2100025637.1.14",
          "form": "아뇨 단지 그냥 좋아서요 ㅋㅋㄱ",
          "original_form": "아뇨 단지 그냥 좋아서요 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100025637.1.15",
          "form": "취향이시구나ㅋㅋㅋㅋ고기좋아하나보네요",
          "original_form": "취향이시구나ㅋㅋㅋㅋ고기좋아하나보네요",
          "speaker_id": "1",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025637.1.16",
          "form": "ㅋㅋㅋㄱ네 이러다 병 생길까봐 걱정이에요",
