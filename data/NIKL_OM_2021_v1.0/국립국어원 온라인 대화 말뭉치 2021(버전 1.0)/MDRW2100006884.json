      "utterance": [
        {
          "id": "MDRW2100006884.1.1",
          "form": "소스는 어떻게요",
          "original_form": "소스는 어떻게요",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100006884.1.2",
          "form": "간장이랑 물엿정도만 넣어도 돼요",
          "original_form": "간장이랑 물엿정도만 넣어도 돼요",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100006884.1.3",
          "form": "팽이버섯 넣응좋겠다",
          "original_form": "팽이버섯 넣응좋겠다",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100006884.1.4",
          "form": "네 소불고기처럼 양파, 파, 버섯 좋아요",
          "original_form": "네 소불고기처럼 양파, 파, 버섯 좋아요",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100006884.1.5",
          "form": "덮밥도 좋겠구요",
          "original_form": "덮밥도 좋겠구요",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100006884.1.6",
          "form": "맞아요 듬뿍올려먹으면 한끼배부르죠",
          "original_form": "맞아요 듬뿍올려먹으면 한끼배부르죠",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006884.1.7",
          "form": "잡채볶음도 좋아요",
          "original_form": "잡채볶음도 좋아요",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006884.1.8",
          "form": "거기에 자장소스 올리면 중식 부럽지않아요",
          "original_form": "거기에 자장소스 올리면 중식 부럽지않아요",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006884.1.9",
          "form": "맞아요 계란도 올리고",
          "original_form": "맞아요 계란도 올리고",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006884.1.10",
          "form": "찜닭이나 닭갈비 먹고나서 볶음밥",
          "original_form": "찜닭이나 닭갈비 먹고나서 볶음밥",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006884.1.11",
          "form": "떡복이나 샤브샤브두요",
          "original_form": "떡복이나 샤브샤브두요",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100006884.1.12",
          "form": "남은 소스에 볶는건 국롤이져",
          "original_form": "남은 소스에 볶는건 국롤이져",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100006884.1.13",
          "form": "소스만있음 김가루넣구요",
          "original_form": "소스만있음 김가루넣구요",
          "speaker_id": "2",
