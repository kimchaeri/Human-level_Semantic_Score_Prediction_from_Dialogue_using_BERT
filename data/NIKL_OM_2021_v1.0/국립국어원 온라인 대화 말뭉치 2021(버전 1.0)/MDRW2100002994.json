      "utterance": [
        {
          "id": "MDRW2100002994.1.1",
          "form": "점심 드셨나용?",
          "original_form": "점심 드셨나용?",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002994.1.2",
          "form": "네 많이 먹었어요! 마라탕 먹었습니다",
          "original_form": "네 많이 먹었어요! 마라탕 먹었습니다",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002994.1.3",
          "form": "오 마라탕 맛있었겠네요",
          "original_form": "오 마라탕 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002994.1.4",
          "form": "마라탕 좋아하시나요?",
          "original_form": "마라탕 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002994.1.5",
          "form": "마라탕 혀얼얼한 그맛이 좋더라구요 마라탕 자주드세요?",
          "original_form": "마라탕 혀얼얼한 그맛이 좋더라구요 마라탕 자주드세요?",
          "speaker_id": "2",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002994.1.6",
          "form": "한달에 한번 정도 먹는것 같아요~ 가까운곳에 마라탕집이 생겨서",
          "original_form": "한달에 한번 정도 먹는것 같아요~ 가까운곳에 마라탕집이 생겨서",
          "speaker_id": "1",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002994.1.7",
          "form": "오늘 저녁은 뭐 드세요?",
          "original_form": "오늘 저녁은 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210820 17:24"
        },
        {
          "id": "MDRW2100002994.1.8",
          "form": "오 그래도 자주드시네요 오늘 저녁은 가볍게 샐러드 먹으려구요ㅠ",
          "original_form": "오 그래도 자주드시네요 오늘 저녁은 가볍게 샐러드 먹으려구요ㅠ",
          "speaker_id": "2",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002994.1.9",
          "form": "저도 그런거 먹어야하는데.. 마라탕이 살찌기 좋대요",
          "original_form": "저도 그런거 먹어야하는데.. 마라탕이 살찌기 좋대요",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002994.1.10",
          "form": "마자요 듣긴했어요 마라탕이  국물류라 그런걸까용ㅎㅎ",
          "original_form": "마자요 듣긴했어요 마라탕이  국물류라 그런걸까용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002994.1.11",
          "form": "기름이 엄청 들어간대요!",
          "original_form": "기름이 엄청 들어간대요!",
          "speaker_id": "1",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100002994.1.12",
          "form": "중국에선 마라탕 먹을때 원래 건더기만 먹는대요",
          "original_form": "중국에선 마라탕 먹을때 원래 건더기만 먹는대요",
          "speaker_id": "1",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100002994.1.13",
          "form": "아 기름이 들어가서ㅜㅋㅋㅋㅋ국물도 넘 맛있는데!! 꿔바로우도 같이먹으면 넘 맛있드라구요",
          "original_form": "아 기름이 들어가서ㅜㅋㅋㅋㅋ국물도 넘 맛있는데!! 꿔바로우도 같이먹으면 넘 맛있드라구요",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002994.1.14",
          "form": "꿔바로우가 뭐였지 탕수육 같은거였나요?",
          "original_form": "꿔바로우가 뭐였지 탕수육 같은거였나요?",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002994.1.15",
          "form": "네 마자요 찹쌀탕수육같은건데 좀 더 넙쩍하고 길쭉한 느낌의 탕수육이요",
          "original_form": "네 마자요 찹쌀탕수육같은건데 좀 더 넙쩍하고 길쭉한 느낌의 탕수육이요",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100002994.1.16",
          "form": "오 먹어본적 있는거 같은데 기억이.. 한번더 먹어봐야겠네요",
          "original_form": "오 먹어본적 있는거 같은데 기억이.. 한번더 먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210820 17:31"
        }
      ]
