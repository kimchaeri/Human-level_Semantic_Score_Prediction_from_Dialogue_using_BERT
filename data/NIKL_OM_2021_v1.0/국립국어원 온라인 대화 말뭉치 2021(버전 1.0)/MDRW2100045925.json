      "utterance": [
        {
          "id": "MDRW2100045925.1.1",
          "form": "아침 드셧나요",
          "original_form": "아침 드셧나요",
          "speaker_id": "1",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100045925.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045925.1.3",
          "form": "저는 도넛먹고왔어요",
          "original_form": "저는 도넛먹고왔어요",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045925.1.4",
          "form": "드셨어요?",
          "original_form": "드셨어요?",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045925.1.5",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045925.1.6",
          "form": "단거 좋아하세요?",
          "original_form": "단거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045925.1.7",
          "form": "단거는 좋아하는데 ㅎ",
          "original_form": "단거는 좋아하는데 ㅎ",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045925.1.8",
          "form": "도넛은 오랫만에 먹었네요",
          "original_form": "도넛은 오랫만에 먹었네요",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045925.1.9",
          "form": "어제 해피앱에서 미니도넛팩 천원할인쿠폰주길래",
          "original_form": "어제 해피앱에서 미니도넛팩 천원할인쿠폰주길래",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045925.1.10",
          "form": "사봤어요",
          "original_form": "사봤어요",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045925.1.11",
          "form": "오오 너무 좋아요 ㅋㅋ",
          "original_form": "오오 너무 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:45"
        },
        {
          "id": "MDRW2100045925.1.12",
          "form": "커피도 드세요?",
          "original_form": "커피도 드세요?",
          "speaker_id": "1",
          "time": "20210908 08:45"
        },
        {
          "id": "MDRW2100045925.1.13",
          "form": "같이 먹었어요 ㅎ",
          "original_form": "같이 먹었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 08:45"
        },
        {
          "id": "MDRW2100045925.1.14",
          "form": "저는 도넛팩이라길래 비싸다생각했는데",
          "original_form": "저는 도넛팩이라길래 비싸다생각했는데",
          "speaker_id": "2",
          "time": "20210908 08:45"
        },
        {
          "id": "MDRW2100045925.1.15",
          "form": "3300원이더라구요",
          "original_form": "3300원이더라구요",
          "speaker_id": "2",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100045925.1.16",
          "form": "거기에 천원할쿠 적용하니",
          "original_form": "거기에 천원할쿠 적용하니",
          "speaker_id": "2",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100045925.1.17",
          "form": "2300원에 사왔어요",
          "original_form": "2300원에 사왔어요",
          "speaker_id": "2",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100045925.1.18",
          "form": "오오 딱 좋네요!!!!",
          "original_form": "오오 딱 좋네요!!!!",
          "speaker_id": "1",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100045925.1.19",
          "form": "도넛 손바닥만하긴한데",
          "original_form": "도넛 손바닥만하긴한데",
          "speaker_id": "2",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100045925.1.20",
          "form": "6개나들었구요",
          "original_form": "6개나들었구요",
          "speaker_id": "2",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100045925.1.21",
          "form": "네넼ㅋㅋ 우아",
          "original_form": "네넼ㅋㅋ 우아",
          "speaker_id": "1",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100045925.1.22",
          "form": "파바가서 빵사면 만원후딱인데 ㅎ",
          "original_form": "파바가서 빵사면 만원후딱인데 ㅎ",
          "speaker_id": "2",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100045925.1.23",
          "form": "여섯개 다 드셧나요??",
          "original_form": "여섯개 다 드셧나요??",
          "speaker_id": "1",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100045925.1.24",
          "form": "아뇨 ㅎ",
          "original_form": "아뇨 ㅎ",
          "speaker_id": "2",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100045925.1.25",
          "form": "2개먹고 나머지는 아이들이 먹었어요",
          "original_form": "2개먹고 나머지는 아이들이 먹었어요",
          "speaker_id": "2",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100045925.1.26",
          "form": "우아 아이들이 도넛 좋아하겟어요",
          "original_form": "우아 아이들이 도넛 좋아하겟어요",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100045925.1.27",
          "form": "달달하니 잘먹더라구요",
          "original_form": "달달하니 잘먹더라구요",
