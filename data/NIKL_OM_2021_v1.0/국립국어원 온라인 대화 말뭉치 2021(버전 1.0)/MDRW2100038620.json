      "utterance": [
        {
          "id": "MDRW2100038620.1.1",
          "form": "요리하는거 좋아하세요?",
          "original_form": "요리하는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100038620.1.2",
          "form": "좋아하는건 아니구 생존을 위해 해요",
          "original_form": "좋아하는건 아니구 생존을 위해 해요",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100038620.1.3",
          "form": "전 간을 잘 못맞춰서 힘들어요",
          "original_form": "전 간을 잘 못맞춰서 힘들어요",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038620.1.4",
          "form": "간맞추기 힘들죠",
          "original_form": "간맞추기 힘들죠",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038620.1.5",
          "form": "저는 그래서 가끔 싱겁게 먹기도 해요",
          "original_form": "저는 그래서 가끔 싱겁게 먹기도 해요",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038620.1.6",
          "form": "힘들어요 그래서 자꾸 밀키트같은거 사먹게 되요",
          "original_form": "힘들어요 그래서 자꾸 밀키트같은거 사먹게 되요",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038620.1.7",
          "form": "저두요ㅋㅋ 밀키트는 따로 간 맞출 필요도 없죠",
          "original_form": "저두요ㅋㅋ 밀키트는 따로 간 맞출 필요도 없죠",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038620.1.8",
          "form": "네 그래서 계속 그런거 쟁여놔요ㅋ",
          "original_form": "네 그래서 계속 그런거 쟁여놔요ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038620.1.9",
          "form": "무슨 밀키트 제일 좋아하세요?",
          "original_form": "무슨 밀키트 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038620.1.10",
          "form": "곱창전골이랑 쭈꾸미볶음요",
          "original_form": "곱창전골이랑 쭈꾸미볶음요",
          "speaker_id": "1",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100038620.1.11",
          "form": "와 전 그런건 안사먹어 봤어요",
          "original_form": "와 전 그런건 안사먹어 봤어요",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100038620.1.12",
          "form": "간편하게 볶음밥같은거도 사고요",
          "original_form": "간편하게 볶음밥같은거도 사고요",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100038620.1.13",
          "form": "볶음밥도 잘 나오나요?",
          "original_form": "볶음밥도 잘 나오나요?",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100038620.1.14",
          "form": "밀키트가 하도 다양해서 못먹어본게 많네요",
          "original_form": "밀키트가 하도 다양해서 못먹어본게 많네요",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038620.1.15",
          "form": "전 제가한거 아님 다 맛있더라고요ㅋ",
          "original_form": "전 제가한거 아님 다 맛있더라고요ㅋ",
