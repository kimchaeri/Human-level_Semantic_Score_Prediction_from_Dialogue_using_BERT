      "utterance": [
        {
          "id": "MDRW2100017311.1.1",
          "form": "사서 운동은 꾸준히 하고있어요",
          "original_form": "사서 운동은 꾸준히 하고있어요",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017311.1.2",
          "form": "어떤게 가장 만족스러워요",
          "original_form": "어떤게 가장 만족스러워요",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017311.1.3",
          "form": "홈트용품 좋은 게 많더라구요",
          "original_form": "홈트용품 좋은 게 많더라구요",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017311.1.4",
          "form": "쇼핑한 물건을",
          "original_form": "쇼핑한 물건을",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017311.1.5",
          "form": "저도 추천해주세요",
          "original_form": "저도 추천해주세요",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017311.1.6",
          "form": "내가 진짜 활용하고있다",
          "original_form": "내가 진짜 활용하고있다",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017311.1.7",
          "form": "이 느낌이 좋아요",
          "original_form": "이 느낌이 좋아요",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017311.1.8",
          "form": "무게조절덤벨이라고",
          "original_form": "무게조절덤벨이라고",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017311.1.9",
          "form": "아령 하나하나 사지 않아도",
          "original_form": "아령 하나하나 사지 않아도",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017311.1.10",
          "form": "하나 사면 그걸로 48kg까지 자유자재로",
          "original_form": "하나 사면 그걸로 48kg까지 자유자재로",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017311.1.11",
          "form": "그런게 있다니",
          "original_form": "그런게 있다니",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017311.1.12",
          "form": "대박적",
          "original_form": "대박적",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017311.1.13",
          "form": "무개조절되는 덤벨이 있어요",
          "original_form": "무개조절되는 덤벨이 있어요",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017311.1.14",
          "form": "그거 추천 드리구",
          "original_form": "그거 추천 드리구",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017311.1.15",
          "form": "방 좁으시면 문틀철봉?",
          "original_form": "방 좁으시면 문틀철봉?",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017311.1.16",
          "form": "철봉?",
          "original_form": "철봉?",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017311.1.17",
          "form": "근데 name1님은 허스키 키우시니까",
          "original_form": "근데 &name1&님은 허스키 키우시니까",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017311.1.18",
          "form": "산책이 운동이네요 ㅎㅎ",
          "original_form": "산책이 운동이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017311.1.19",
          "form": "그거 설치하려면 문 뚫어야하죠?",
          "original_form": "그거 설치하려면 문 뚫어야하죠?",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017311.1.20",
          "form": "굳이 홈트용품까지...",
          "original_form": "굳이 홈트용품까지...",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017311.1.21",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017311.1.22",
          "form": "뚫으면 더 안전",
          "original_form": "뚫으면 더 안전",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017311.1.23",
          "form": "아니요 산책은 운동이 아닙니다 (진지)",
          "original_form": "아니요 산책은 운동이 아닙니다 (진지)",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017311.1.24",
          "form": "안뚫고 가능해요?",
          "original_form": "안뚫고 가능해요?",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017311.1.25",
          "form": "안뚫으면 살짝 위험 하지만 대체로 오케이",
          "original_form": "안뚫으면 살짝 위험 하지만 대체로 오케이",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017311.1.26",
          "form": "내 돌려서 꽉 끼우면 돼요",
          "original_form": "내 돌려서 꽉 끼우면 돼요",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017311.1.27",
          "form": "신기하다 알아봐야지",
          "original_form": "신기하다 알아봐야지",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017311.1.28",
          "form": "아하 이렇게 고르는거구나",
          "original_form": "아하 이렇게 고르는거구나",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017311.1.29",
          "form": "근데 문틀철봉은",
          "original_form": "근데 문틀철봉은",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017311.1.30",
          "form": "남자분이면 뚫으면 좋져",
          "original_form": "남자분이면 뚫으면 좋져",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017311.1.31",
          "form": "몸집이 허스키신 분이라면",
          "original_form": "몸집이 허스키신 분이라면",
          "speaker_id": "2",
          "time": "20210826 18:42"
        }
      ]
    }
