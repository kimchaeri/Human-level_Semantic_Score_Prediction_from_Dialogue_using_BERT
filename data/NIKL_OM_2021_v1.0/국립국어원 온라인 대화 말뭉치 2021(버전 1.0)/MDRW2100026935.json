      "utterance": [
        {
          "id": "MDRW2100026935.1.1",
          "form": "안녕하세요? 요즘 운동하세요?",
          "original_form": "안녕하세요? 요즘 운동하세요?",
          "speaker_id": "1",
          "time": "20210902 01:26"
        },
        {
          "id": "MDRW2100026935.1.2",
          "form": "숨쉬기 운동만 합니다ㅎ",
          "original_form": "숨쉬기 운동만 합니다ㅎ",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026935.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026935.1.4",
          "form": "저도 요즘 숨쉬기운동이랑 위 소화운동만해요ㅋㅋ",
          "original_form": "저도 요즘 숨쉬기운동이랑 위 소화운동만해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026935.1.5",
          "form": "ㅎㅎ 태어날 때부터 운동하셨군요ㅋㅋㅋ",
          "original_form": "ㅎㅎ 태어날 때부터 운동하셨군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100026935.1.6",
          "form": "저도 요새 위운동은 좀 격하게 하네요ㅎ",
          "original_form": "저도 요새 위운동은 좀 격하게 하네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100026935.1.7",
          "form": "태어날때부터 운동했지만 근육은 하나도 안늘었답니다ㅋㅋㅋ",
          "original_form": "태어날때부터 운동했지만 근육은 하나도 안늘었답니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100026935.1.8",
          "form": "오히려 열심히 근육만 사라지고 있네요",
          "original_form": "오히려 열심히 근육만 사라지고 있네요",
          "speaker_id": "1",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100026935.1.9",
          "form": "아ㅎㅎ",
          "original_form": "아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026935.1.10",
          "form": "사라질 근육이 있으시군요!!!",
          "original_form": "사라질 근육이 있으시군요!!!",
          "speaker_id": "2",
          "time": "20210902 01:33"
        },
        {
          "id": "MDRW2100026935.1.11",
          "form": "건강관리를 위해 하는건 있나요",
          "original_form": "건강관리를 위해 하는건 있나요",
          "speaker_id": "2",
          "time": "20210902 01:35"
        },
        {
          "id": "MDRW2100026935.1.12",
          "form": "먹는거에 그나마 신경을 쓰려고 노력하고 있어요",
          "original_form": "먹는거에 그나마 신경을 쓰려고 노력하고 있어요",
          "speaker_id": "1",
          "time": "20210902 01:38"
        },
        {
          "id": "MDRW2100026935.1.13",
          "form": "조금더 밀가루를 줄일려고 하는데 쉽지 않네요ㅠ",
          "original_form": "조금더 밀가루를 줄일려고 하는데 쉽지 않네요ㅠ",
          "speaker_id": "1",
          "time": "20210902 01:38"
        },
        {
          "id": "MDRW2100026935.1.14",
          "form": "아 어떤 식으로요?",
          "original_form": "아 어떤 식으로요?",
          "speaker_id": "2",
          "time": "20210902 01:39"
        },
        {
          "id": "MDRW2100026935.1.15",
          "form": "아아 밀가루 끊기 어렵죠ㅜ",
          "original_form": "아아 밀가루 끊기 어렵죠ㅜ",
          "speaker_id": "2",
          "time": "20210902 01:39"
        },
        {
          "id": "MDRW2100026935.1.16",
          "form": "과자류가 거진 밀가루니까 간식을 줄이고 있는데",
          "original_form": "과자류가 거진 밀가루니까 간식을 줄이고 있는데",
          "speaker_id": "1",
          "time": "20210902 01:41"
        },
        {
          "id": "MDRW2100026935.1.17",
          "form": "간식 끊는건...너무 힘들어요",
          "original_form": "간식 끊는건...너무 힘들어요",
          "speaker_id": "1",
          "time": "20210902 01:41"
        },
        {
          "id": "MDRW2100026935.1.18",
          "form": "헉 전 라면같은 것만 생각했는데ㅜ",
          "original_form": "헉 전 라면같은 것만 생각했는데ㅜ",
          "speaker_id": "2",
          "time": "20210902 01:43"
        },
        {
          "id": "MDRW2100026935.1.19",
          "form": "과자ㄷㄷㄷ",
          "original_form": "과자ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210902 01:43"
        },
        {
          "id": "MDRW2100026935.1.20",
          "form": "미션 임파서블이네요ㅜㅇ",
          "original_form": "미션 임파서블이네요ㅜㅇ",
          "speaker_id": "2",
          "time": "20210902 01:43"
        },
        {
          "id": "MDRW2100026935.1.21",
          "form": "라면은 끊을 생각도 안했어요ㅋㅋㅋ",
          "original_form": "라면은 끊을 생각도 안했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:46"
        },
        {
          "id": "MDRW2100026935.1.22",
          "form": "ㅎㅎ 밀가루는 과자만 줄이신 거군요ㅋㅋ",
          "original_form": "ㅎㅎ 밀가루는 과자만 줄이신 거군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:47"
        },
        {
          "id": "MDRW2100026935.1.23",
          "form": "요리하기 귀찮을때 라면조차 못먹으면",
          "original_form": "요리하기 귀찮을때 라면조차 못먹으면",
          "speaker_id": "1",
          "time": "20210902 01:47"
        },
        {
          "id": "MDRW2100026935.1.24",
          "form": "저같아도 양자택일하면 라면을 살리죠",
          "original_form": "저같아도 양자택일하면 라면을 살리죠",
          "speaker_id": "2",
          "time": "20210902 01:47"
        },
        {
          "id": "MDRW2100026935.1.25",
          "form": "밀가루 줄이기가 아니라 단식이 되버릴거에요ㅋㅋ",
          "original_form": "밀가루 줄이기가 아니라 단식이 되버릴거에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:48"
        },
        {
          "id": "MDRW2100026935.1.26",
          "form": "아니ㅋ 저도 부정을 못하겠네요ㅋㅋ",
          "original_form": "아니ㅋ 저도 부정을 못하겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:49"
        }
      ]
