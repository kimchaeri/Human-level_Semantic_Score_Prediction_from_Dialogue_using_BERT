      "utterance": [
        {
          "id": "MDRW2100039527.1.1",
          "form": "모가디슈이후로 영화관못갔네",
          "original_form": "모가디슈이후로 영화관못갔네",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039527.1.2",
          "form": "난 뭐 언제 갔는지 기억도 없다 언니",
          "original_form": "난 뭐 언제 갔는지 기억도 없다 언니",
          "speaker_id": "2",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039527.1.3",
          "form": "코로나 이후로는 한 번도 안 간 듯",
          "original_form": "코로나 이후로는 한 번도 안 간 듯",
          "speaker_id": "2",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100039527.1.4",
          "form": "영화 보러 가고 싶네",
          "original_form": "영화 보러 가고 싶네",
          "speaker_id": "2",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100039527.1.5",
          "form": "그래 영화관가서보는거랑다르지",
          "original_form": "그래 영화관가서보는거랑다르지",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039527.1.6",
          "form": "코로나땜에 더힘들긴하다",
          "original_form": "코로나땜에 더힘들긴하다",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039527.1.7",
          "form": "이번에 이정재 뭐찍은것도 잼있을라나",
          "original_form": "이번에 이정재 뭐찍은것도 잼있을라나",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039527.1.8",
          "form": "이정재??",
          "original_form": "이정재??",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039527.1.9",
          "form": "제목이 뭐야??",
          "original_form": "제목이 뭐야??",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039527.1.10",
          "form": "뭔 게임이던데",
          "original_form": "뭔 게임이던데",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039527.1.11",
          "form": "암튼ㅋㅋ",
          "original_form": "암튼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039527.1.12",
          "form": "이제육퇴",
          "original_form": "이제육퇴",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039527.1.13",
          "form": "설거지하고 영화하나봐야긋다",
          "original_form": "설거지하고 영화하나봐야긋다",
          "speaker_id": "1",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039527.1.14",
          "form": "좋구만",
          "original_form": "좋구만",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039527.1.15",
          "form": "벌써 육 테라니",
          "original_form": "벌써 육 테라니",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039527.1.16",
          "form": "즐거운 영화 보슈",
          "original_form": "즐거운 영화 보슈",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039527.1.17",
          "form": "오냐 니는 아직?",
          "original_form": "오냐 니는 아직?",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039527.1.18",
          "form": "오늘불금인데ㅋ",
          "original_form": "오늘불금인데ㅋ",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039527.1.19",
          "form": "금욜이라 좀 늦게재우려고",
          "original_form": "금욜이라 좀 늦게재우려고",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039527.1.20",
          "form": "이제 불 끌려고 합니다",
          "original_form": "이제 불 끌려고 합니다",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039527.1.21",
          "form": "굳ㅎㅎ",
          "original_form": "굳ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100039527.1.22",
          "form": "난 영화고르고있음",
          "original_form": "난 영화고르고있음",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100039527.1.23",
          "form": "ㅋㅋㅋㅋㅋ 아까 그거 본다며??",
          "original_form": "ㅋㅋㅋㅋㅋ 아까 그거 본다며??",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100039527.1.24",
          "form": "이정재나오는거",
          "original_form": "이정재나오는거",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100039527.1.25",
          "form": "그거 이제개봉작",
          "original_form": "그거 이제개봉작",
          "speaker_id": "1",
          "time": "20210904 10:16"
        },
        {
          "id": "MDRW2100039527.1.26",
          "form": "아직못본다ㅋ",
          "original_form": "아직못본다ㅋ",
          "speaker_id": "1",
          "time": "20210904 10:16"
        },
        {
          "id": "MDRW2100039527.1.27",
          "form": "영화관가야재 그거ㆍ",
          "original_form": "영화관가야재 그거ㆍ",
          "speaker_id": "1",
          "time": "20210904 10:16"
        },
        {
          "id": "MDRW2100039527.1.28",
          "form": "그래서 결국 뭐 봤슈??",
          "original_form": "그래서 결국 뭐 봤슈??",
          "speaker_id": "2",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100039527.1.29",
          "form": "똑똑",
          "original_form": "똑똑",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100039527.1.30",
          "form": "일어나슈 ㅋㅋ",
          "original_form": "일어나슈 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:22"
        },
        {
          "id": "MDRW2100039527.1.31",
          "form": "상담잘했나?",
          "original_form": "상담잘했나?",
          "speaker_id": "1",
          "time": "20210904 21:59"
        },
        {
          "id": "MDRW2100039527.1.32",
          "form": "오늘 엄청정신없었다이ㅡㅡ",
          "original_form": "오늘 엄청정신없었다이ㅡㅡ",
          "speaker_id": "1",
          "time": "20210904 21:59"
        },
        {
          "id": "MDRW2100039527.1.33",
          "form": "뭐그래 바쁘노 언니",
          "original_form": "뭐그래 바쁘노 언니",
          "speaker_id": "2",
          "time": "20210904 22:04"
        }
      ]
