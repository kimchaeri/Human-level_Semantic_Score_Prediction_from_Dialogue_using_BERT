      "utterance": [
        {
          "id": "MDRW2100021484.1.1",
          "form": "오리 좋아하시는 분은 근데 처음 봤어요 ㅋㅋㅋㅋ",
          "original_form": "오리 좋아하시는 분은 근데 처음 봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100021484.1.2",
          "form": "아 저 그 새덕후도 봐요 혹시 아시나요?",
          "original_form": "아 저 그 새덕후도 봐요 혹시 아시나요?",
          "speaker_id": "1",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021484.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021484.1.4",
          "form": "저 이분도 구독했어요!",
          "original_form": "저 이분도 구독했어요!",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021484.1.5",
          "form": "와 저랑 겹치시네요ㅋㅋㅋ",
          "original_form": "와 저랑 겹치시네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021484.1.6",
          "form": "주변에 군인 있으세요?? ㅋㅋㅋ",
          "original_form": "주변에 군인 있으세요?? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100021484.1.7",
          "form": "음 제가 나이가",
          "original_form": "음 제가 나이가",
          "speaker_id": "2",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100021484.1.8",
          "form": "20대후반이라",
          "original_form": "20대후반이라",
          "speaker_id": "2",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100021484.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:33"
        },
        {
          "id": "MDRW2100021484.1.10",
          "form": "군대 다녀온 친구들은 많아용",
          "original_form": "군대 다녀온 친구들은 많아용",
          "speaker_id": "2",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100021484.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 저는 직업군인이 몇 있어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 저는 직업군인이 몇 있어요",
          "speaker_id": "1",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100021484.1.12",
          "form": "오오 그럼 그 군인아파트 이런곳에서 사시나요?",
          "original_form": "오오 그럼 그 군인아파트 이런곳에서 사시나요?",
          "speaker_id": "2",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100021484.1.13",
          "form": "네 ㅋㅋ 관사 살더라구요",
          "original_form": "네 ㅋㅋ 관사 살더라구요",
          "speaker_id": "1",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100021484.1.14",
          "form": "우왕 예전에 소개팅했던 남자분이",
          "original_form": "우왕 예전에 소개팅했던 남자분이",
          "speaker_id": "2",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100021484.1.15",
          "form": "군사학과 나와서 직업 군인 준비하던데",
          "original_form": "군사학과 나와서 직업 군인 준비하던데",
          "speaker_id": "2",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100021484.1.16",
          "form": "체력이 장난 아니더라구요ㅋㅋㅋ",
          "original_form": "체력이 장난 아니더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100021484.1.17",
          "form": "아하 제 친구들은 rotc 나와서 쭉.. 하더라고요 ㅋㅋㅋ",
          "original_form": "아하 제 친구들은 rotc 나와서 쭉.. 하더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100021484.1.18",
          "form": "우왕 군인하면 엄청 강인한 이미지잖아용",
          "original_form": "우왕 군인하면 엄청 강인한 이미지잖아용",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100021484.1.19",
          "form": "실제로도 그런 느낌인가요?",
          "original_form": "실제로도 그런 느낌인가요?",
          "speaker_id": "2",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100021484.1.20",
          "form": "엄청 그렇진 않았어요 ㅋㅋㅋ",
          "original_form": "엄청 그렇진 않았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100021484.1.21",
          "form": "여군도 있고 남군도 있는데",
          "original_form": "여군도 있고 남군도 있는데",
          "speaker_id": "1",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100021484.1.22",
          "form": "와 여군 너무 멋져요",
          "original_form": "와 여군 너무 멋져요",
          "speaker_id": "2",
          "time": "20210828 20:40"
        },
        {
          "id": "MDRW2100021484.1.23",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210828 20:40"
        },
        {
          "id": "MDRW2100021484.1.24",
          "form": "정말 멋있어요",
          "original_form": "정말 멋있어요",
          "speaker_id": "2",
          "time": "20210828 20:40"
        },
        {
          "id": "MDRW2100021484.1.25",
          "form": "옆에서 준비하는 거부터 봤는데",
          "original_form": "옆에서 준비하는 거부터 봤는데",
          "speaker_id": "1",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021484.1.26",
          "form": "진짜 열심히하구 그렇더라구요",
          "original_form": "진짜 열심히하구 그렇더라구요",
          "speaker_id": "1",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021484.1.27",
          "form": "똑똑해야하고 체력도 좋아야하고",
          "original_form": "똑똑해야하고 체력도 좋아야하고",
          "speaker_id": "2",
          "time": "20210828 20:42"
        },
        {
          "id": "MDRW2100021484.1.28",
          "form": "끈기도 있어야하는것같아요",
          "original_form": "끈기도 있어야하는것같아요",
          "speaker_id": "2",
          "time": "20210828 20:43"
        },
        {
          "id": "MDRW2100021484.1.29",
          "form": "맞아요 다 필요해요",
          "original_form": "맞아요 다 필요해요",
          "speaker_id": "1",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100021484.1.30",
          "form": "군인될 때 시험도 엄청 보더라구요",
