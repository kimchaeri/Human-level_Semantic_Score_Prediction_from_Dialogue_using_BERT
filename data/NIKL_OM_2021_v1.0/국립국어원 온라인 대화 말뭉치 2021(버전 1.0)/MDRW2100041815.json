      "utterance": [
        {
          "id": "MDRW2100041815.1.1",
          "form": "매일 모할지가 걱정이져",
          "original_form": "매일 모할지가 걱정이져",
          "speaker_id": "2",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100041815.1.2",
          "form": "왜 엄마들이 점심먹으면서 저녁걱정햇는지 알겠어여",
          "original_form": "왜 엄마들이 점심먹으면서 저녁걱정햇는지 알겠어여",
          "speaker_id": "2",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100041815.1.3",
          "form": "ㅎㅎㅎ 다 똑같나보네여",
          "original_form": "ㅎㅎㅎ 다 똑같나보네여",
          "speaker_id": "1",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100041815.1.4",
          "form": "맞아요 진짜",
          "original_form": "맞아요 진짜",
          "speaker_id": "1",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100041815.1.5",
          "form": "밥해먹는거 귀찮아욬ㅋㅋㅋㅋ",
          "original_form": "밥해먹는거 귀찮아욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100041815.1.6",
          "form": "너무나요",
          "original_form": "너무나요",
          "speaker_id": "2",
          "time": "20210906 16:12"
        },
        {
          "id": "MDRW2100041815.1.7",
          "form": "제일 맛난밥은 남이 해준 밥",
          "original_form": "제일 맛난밥은 남이 해준 밥",
          "speaker_id": "2",
          "time": "20210906 16:13"
        },
        {
          "id": "MDRW2100041815.1.8",
          "form": "달다님도 사먹는 밥이 최고??",
          "original_form": "달다님도 사먹는 밥이 최고??",
          "speaker_id": "1",
          "time": "20210906 16:13"
        },
        {
          "id": "MDRW2100041815.1.9",
          "form": "역싴ㅋㅋ",
          "original_form": "역싴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:13"
        },
        {
          "id": "MDRW2100041815.1.10",
          "form": "내가 만든건 다 맛없고 ㅋㅋㅋ",
          "original_form": "내가 만든건 다 맛없고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:13"
        },
        {
          "id": "MDRW2100041815.1.11",
          "form": "ㅋㅋㅋㅋ 저도 제가 만들기 시작하니까 그마음을 알게됐어욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 저도 제가 만들기 시작하니까 그마음을 알게됐어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:13"
        },
        {
          "id": "MDRW2100041815.1.12",
          "form": "다시다를 넣어도 맛없어요ㅋ",
          "original_form": "다시다를 넣어도 맛없어요ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100041815.1.13",
          "form": "뭐만 해도 별로",
          "original_form": "뭐만 해도 별로",
          "speaker_id": "1",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100041815.1.14",
          "form": "ㅋㅋㅋㅋ 맞아여",
          "original_form": "ㅋㅋㅋㅋ 맞아여",
          "speaker_id": "1",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100041815.1.15",
          "form": "전 요즘 라면스프를 활용합니다",
          "original_form": "전 요즘 라면스프를 활용합니다",
          "speaker_id": "2",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100041815.1.16",
          "form": "찌개 국에 라면스프넣어옄ㅋ",
          "original_form": "찌개 국에 라면스프넣어옄ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100041815.1.17",
          "form": "맛이 나은거 같으신가여?ㅋㅋㅋㅋ",
          "original_form": "맛이 나은거 같으신가여?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100041815.1.18",
          "form": "안넣은것보단 나은거같아여ㅋ",
          "original_form": "안넣은것보단 나은거같아여ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100041815.1.19",
          "form": "다행입니다 ㅋㅋㅋㅋㅋ",
          "original_form": "다행입니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100041815.1.20",
          "form": "그래도 역시 남이해준건 못따라가져",
          "original_form": "그래도 역시 남이해준건 못따라가져",
          "speaker_id": "2",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100041815.1.21",
          "form": "그 하얀라면 스프 괜찮을지도 모르겠어욬ㅋㅋㅋ",
          "original_form": "그 하얀라면 스프 괜찮을지도 모르겠어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100041815.1.22",
          "form": "물론 남이 해준밥 최고ㅎㅎㅎㄹ",
          "original_form": "물론 남이 해준밥 최고ㅎㅎㅎㄹ",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100041815.1.23",
          "form": "전에 티비에서봣는데요\r\n김치볶음에도 라면스프 넣더라고요ㅋㅋㅋ",
          "original_form": "전에 티비에서봣는데요\r\n김치볶음에도 라면스프 넣더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100041815.1.24",
          "form": "진짜요??",
          "original_form": "진짜요??",
          "speaker_id": "1",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100041815.1.25",
          "form": "밖에서 김치볶음 그렇게 하나봐요",
          "original_form": "밖에서 김치볶음 그렇게 하나봐요",
          "speaker_id": "1",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100041815.1.26",
          "form": "생각해보니까 색깔이 라면스프 색이었어욬ㅋㅋㅋ",
          "original_form": "생각해보니까 색깔이 라면스프 색이었어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:18"
        }
      ]
    }
  ]
