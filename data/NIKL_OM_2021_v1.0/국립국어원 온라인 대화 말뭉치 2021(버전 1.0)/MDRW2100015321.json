      },
      "utterance": [
        {
          "id": "MDRW2100015321.1.1",
          "form": "나도 재택근무 하고싶어",
          "original_form": "나도 재택근무 하고싶어",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015321.1.2",
          "form": "저두여!!!!",
          "original_form": "저두여!!!!",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015321.1.3",
          "form": "진짜 하고싶다 재택근무",
          "original_form": "진짜 하고싶다 재택근무",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015321.1.4",
          "form": "친구남친 재택하는데 너무부러움",
          "original_form": "친구남친 재택하는데 너무부러움",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015321.1.5",
          "form": "코로나 심해지고 다들 재택 한다는데",
          "original_form": "코로나 심해지고 다들 재택 한다는데",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015321.1.6",
          "form": "나만 못함",
          "original_form": "나만 못함",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015321.1.7",
          "form": "그니까여... 부들부들..",
          "original_form": "그니까여... 부들부들..",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015321.1.8",
          "form": "사무직이 아닌거의 한인가",
          "original_form": "사무직이 아닌거의 한인가",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015321.1.9",
          "form": "다들 재택 하니깐 전철에 사람은 없어서 좋더라",
          "original_form": "다들 재택 하니깐 전철에 사람은 없어서 좋더라",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015321.1.10",
          "form": "취업 목표를 재택 할 수 있는 일자리 얻기로 해야 할까봐",
          "original_form": "취업 목표를 재택 할 수 있는 일자리 얻기로 해야 할까봐",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015321.1.11",
          "form": "앗ㅋㅋㅋ 장점이네..",
          "original_form": "앗ㅋㅋㅋ 장점이네..",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015321.1.12",
          "form": "재택.. 음...",
          "original_form": "재택.. 음...",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015321.1.13",
          "form": "뭐 있냐?",
          "original_form": "뭐 있냐?",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015321.1.14",
          "form": "국가직 공무원도 보면 약사 자리도 은근 있던데",
          "original_form": "국가직 공무원도 보면 약사 자리도 은근 있던데",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015321.1.15",
          "form": "근데 공무원 재택 못하나??",
          "original_form": "근데 공무원 재택 못하나??",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015321.1.16",
          "form": "공무원은 재택이긴 할듯",
          "original_form": "공무원은 재택이긴 할듯",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015321.1.17",
          "form": "몇부제 한다 그랬는데",
          "original_form": "몇부제 한다 그랬는데",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015321.1.18",
          "form": "아항... 그렇담 부럽군....",
          "original_form": "아항... 그렇담 부럽군....",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015321.1.19",
          "form": "재택 하다보면 다시 출근 못하겠다고 그러더라 사람들이",
          "original_form": "재택 하다보면 다시 출근 못하겠다고 그러더라 사람들이",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015321.1.20",
          "form": "그니까 ㅋㅋ 근데 사실 집에서 하면 효율이 훨 좋을껄",
          "original_form": "그니까 ㅋㅋ 근데 사실 집에서 하면 효율이 훨 좋을껄",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015321.1.21",
          "form": "쓸데없는거 안해듀되고",
          "original_form": "쓸데없는거 안해듀되고",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015321.1.22",
          "form": "출퇴근에 체력 안뺏기고",
          "original_form": "출퇴근에 체력 안뺏기고",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015321.1.23",
          "form": "출퇴근 하는 시간이 너무 아까움",
          "original_form": "출퇴근 하는 시간이 너무 아까움",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015321.1.24",
          "form": "맞아 이동시간이 크지...",
          "original_form": "맞아 이동시간이 크지...",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015321.1.25",
          "form": "근데 이제 말로 하면 빠를걸 메신저로 하면 세월아 네월아 걸리기도 하겠지",
          "original_form": "근데 이제 말로 하면 빠를걸 메신저로 하면 세월아 네월아 걸리기도 하겠지",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015321.1.26",
          "form": "친구 남친도 보면 10분 일하고 30분 쉬고 그러던데",
          "original_form": "친구 남친도 보면 10분 일하고 30분 쉬고 그러던데",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015321.1.27",
          "form": "줌으로 미팅하던데?? 그래서 필요한 말만 해서 회의도 줄었단 얘기가...",
          "original_form": "줌으로 미팅하던데?? 그래서 필요한 말만 해서 회의도 줄었단 얘기가...",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015321.1.28",
          "form": "오.....",
          "original_form": "오.....",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015321.1.29",
          "form": "완전 좋네 부럽다",
          "original_form": "완전 좋네 부럽다",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015321.1.30",
          "form": "실험도 재택으로 하고싶다",
          "original_form": "실험도 재택으로 하고싶다",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015321.1.31",
          "form": "그렇게 하이웨이라는 썰을 어디서 봄",
          "original_form": "그렇게 하이웨이라는 썰을 어디서 봄",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015321.1.32",
          "form": "실험 재택 ㅋㅋㅋㅋㅋ 획기적이네",
          "original_form": "실험 재택 ㅋㅋㅋㅋㅋ 획기적이네",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015321.1.33",
          "form": "실험 대신 해 주는 로봇 만들어서",
          "original_form": "실험 대신 해 주는 로봇 만들어서",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015321.1.34",
          "form": "재택으로 실험하면",
          "original_form": "재택으로 실험하면",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015321.1.35",
          "form": "실험 오차도 줄고 좋지 않을까",
          "original_form": "실험 오차도 줄고 좋지 않을까",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015321.1.36",
          "form": "원격 조종 하는거야?",
          "original_form": "원격 조종 하는거야?",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015321.1.37",
          "form": "이론상으론 괜찮네",
          "original_form": "이론상으론 괜찮네",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015321.1.38",
          "form": "코로나 종식쯤엔 얘기가 나올수도 있겠네",
          "original_form": "코로나 종식쯤엔 얘기가 나올수도 있겠네",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
