      "utterance": [
        {
          "id": "MDRW2100032624.1.1",
          "form": "최근에 올림픽보셨나요?",
          "original_form": "최근에 올림픽보셨나요?",
          "speaker_id": "1",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100032624.1.2",
          "form": "네 봤어요",
          "original_form": "네 봤어요",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032624.1.3",
          "form": "name1님도 보셨나요?",
          "original_form": "&name1&님도 보셨나요?",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032624.1.4",
          "form": "넵ㅎㅎ양궁멋있었어요",
          "original_form": "넵ㅎㅎ양궁멋있었어요",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100032624.1.5",
          "form": "양궁 멋있었죠",
          "original_form": "양궁 멋있었죠",
          "speaker_id": "2",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100032624.1.6",
          "form": "스포츠 평소에 좋아하시나요?",
          "original_form": "스포츠 평소에 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 21:38"
        },
        {
          "id": "MDRW2100032624.1.7",
          "form": "음 보면 멋있는데 챙겨보진 않아요",
          "original_form": "음 보면 멋있는데 챙겨보진 않아요",
          "speaker_id": "1",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100032624.1.8",
          "form": "좋아하시죠??",
          "original_form": "좋아하시죠??",
          "speaker_id": "1",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100032624.1.9",
          "form": "아니요ㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100032624.1.10",
          "form": "그럼 스포츠 직관 가신 적 있으신가요?",
          "original_form": "그럼 스포츠 직관 가신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100032624.1.11",
          "form": "앗그래요ㅋㅋㅋㅋ?ㅎㅎ",
          "original_form": "앗그래요ㅋㅋㅋㅋ?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:42"
        },
        {
          "id": "MDRW2100032624.1.12",
          "form": "저는야구요!",
          "original_form": "저는야구요!",
          "speaker_id": "1",
          "time": "20210905 21:42"
        },
        {
          "id": "MDRW2100032624.1.13",
          "form": "언제 가셨나요?",
          "original_form": "언제 가셨나요?",
          "speaker_id": "2",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032624.1.14",
          "form": "코로나전에요ㅎㅎㅎ",
          "original_form": "코로나전에요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032624.1.15",
          "form": "직관가신적있으세요?",
          "original_form": "직관가신적있으세요?",
          "speaker_id": "1",
          "time": "20210905 21:44"
        },
        {
          "id": "MDRW2100032624.1.16",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032624.1.17",
          "form": "직접 티켓팅 해서 가신건가요?",
          "original_form": "직접 티켓팅 해서 가신건가요?",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032624.1.18",
          "form": "ㅎㅎ같이갔던친구가끊었어요",
          "original_form": "ㅎㅎ같이갔던친구가끊었어요",
          "speaker_id": "1",
          "time": "20210905 21:46"
        },
        {
          "id": "MDRW2100032624.1.19",
          "form": "친구분은 스포츠 좋아하셨나요?",
          "original_form": "친구분은 스포츠 좋아하셨나요?",
          "speaker_id": "2",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100032624.1.20",
          "form": "그친구는야구축구좋아했어요",
          "original_form": "그친구는야구축구좋아했어요",
          "speaker_id": "1",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032624.1.21",
          "form": "가서보니재밌더라고요",
          "original_form": "가서보니재밌더라고요",
          "speaker_id": "1",
          "time": "20210905 21:49"
        }
