      "utterance": [
        {
          "id": "MDRW2100027506.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 13:14"
        },
        {
          "id": "MDRW2100027506.1.2",
          "form": "네 좋아하죠",
          "original_form": "네 좋아하죠",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027506.1.3",
          "form": "최근에어디 다녀오셨어요",
          "original_form": "최근에어디 다녀오셨어요",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027506.1.4",
          "form": "여행은 아니고 호캉스로",
          "original_form": "여행은 아니고 호캉스로",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100027506.1.5",
          "form": "코로나가 빨리사라지길",
          "original_form": "코로나가 빨리사라지길",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100027506.1.6",
          "form": "호캉스도좋죠 어디다녀오셨나요",
          "original_form": "호캉스도좋죠 어디다녀오셨나요",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100027506.1.7",
          "form": "대전에 신세계백화점과 호텔이 생겨서 거기요..",
          "original_form": "대전에 신세계백화점과 호텔이 생겨서 거기요..",
          "speaker_id": "1",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027506.1.8",
          "form": "아 좋네요 쇼핑도하구요",
          "original_form": "아 좋네요 쇼핑도하구요",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100027506.1.9",
          "form": "연결되어있어서 편해요",
          "original_form": "연결되어있어서 편해요",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027506.1.10",
          "form": "전 제주도 신화리조트다녀왔었어요 2달전에요",
          "original_form": "전 제주도 신화리조트다녀왔었어요 2달전에요",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027506.1.11",
          "form": "제주도 좋네요",
          "original_form": "제주도 좋네요",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027506.1.12",
          "form": "네 너무좋았어요 다시가거싶네요 ㅠ",
          "original_form": "네 너무좋았어요 다시가거싶네요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027506.1.13",
          "form": "저도 제주도 가면 가봐야겠네요",
          "original_form": "저도 제주도 가면 가봐야겠네요",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027506.1.14",
          "form": "네 스카이수영장엔 사람도 나눠받아서 많지않아좋았어요",
          "original_form": "네 스카이수영장엔 사람도 나눠받아서 많지않아좋았어요",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027506.1.15",
          "form": "아이들도 좋아하겠어요",
          "original_form": "아이들도 좋아하겠어요",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027506.1.16",
          "form": "네 너무좋아했어요 가고싶다고 노래를불렀거든요",
          "original_form": "네 너무좋아했어요 가고싶다고 노래를불렀거든요",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027506.1.17",
          "form": "더 가고 싶네요..",
          "original_form": "더 가고 싶네요..",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027506.1.18",
          "form": "지금은 확진자가 너무많아서요  ㅠ",
          "original_form": "지금은 확진자가 너무많아서요  ㅠ",
          "speaker_id": "2",
          "time": "20210902 13:28"
        }
      ]
