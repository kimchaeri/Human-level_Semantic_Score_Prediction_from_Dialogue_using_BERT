      "utterance": [
        {
          "id": "MDRW2100033954.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100033954.1.2",
          "form": "못먹는 음식은 없어요ㅎ",
          "original_form": "못먹는 음식은 없어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100033954.1.3",
          "form": "단음식은 많이 못 먹기는 합니다",
          "original_form": "단음식은 많이 못 먹기는 합니다",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100033954.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100033954.1.5",
          "form": "저도 다 좋아해요ㅋㅋㅋㅋ",
          "original_form": "저도 다 좋아해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100033954.1.6",
          "form": "와 그런사람 드문데 세상의 먹거리 행복을 다 가지셨군요",
          "original_form": "와 그런사람 드문데 세상의 먹거리 행복을 다 가지셨군요",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100033954.1.7",
          "form": "제 주변에는 고기 못먹는 사람 해산물 못먹는 사람 이런 경우도 있어요ㅜ",
          "original_form": "제 주변에는 고기 못먹는 사람 해산물 못먹는 사람 이런 경우도 있어요ㅜ",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100033954.1.8",
          "form": "이분들과 같이 모이면 정말 식사메뉴 답이 없음",
          "original_form": "이분들과 같이 모이면 정말 식사메뉴 답이 없음",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100033954.1.9",
          "form": "ㅋㅋㅋㅋ 전 고기도먹고",
          "original_form": "ㅋㅋㅋㅋ 전 고기도먹고",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100033954.1.10",
          "form": "해산물도먹는답니다",
          "original_form": "해산물도먹는답니다",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100033954.1.11",
          "form": "다만 닭발 해삼 이런건",
          "original_form": "다만 닭발 해삼 이런건",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100033954.1.12",
          "form": "못먹어요",
          "original_form": "못먹어요",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100033954.1.13",
          "form": "아니 못먹는 음식이 없는 게 아니잖아요ㅎ",
          "original_form": "아니 못먹는 음식이 없는 게 아니잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100033954.1.14",
          "form": "해삼 말고 멍게는 드시나요",
          "original_form": "해삼 말고 멍게는 드시나요",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100033954.1.15",
          "form": "ㅋㅋㅋ저정도는 다들 못먹긴하죠",
          "original_form": "ㅋㅋㅋ저정도는 다들 못먹긴하죠",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100033954.1.16",
          "form": "멍게도 싫어요!",
          "original_form": "멍게도 싫어요!",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100033954.1.17",
          "form": "개불도 안 드시겠지요...?",
          "original_form": "개불도 안 드시겠지요...?",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100033954.1.18",
          "form": "굴은요?",
          "original_form": "굴은요?",
          "speaker_id": "2",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100033954.1.19",
          "form": "개불은 안먹죠ㅋㅋㅋ",
          "original_form": "개불은 안먹죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100033954.1.20",
          "form": "굴은 먹는데요",
          "original_form": "굴은 먹는데요",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100033954.1.21",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100033954.1.22",
          "form": "노로바이러스땜에 잘안먹어요ㅎㅎ...",
          "original_form": "노로바이러스땜에 잘안먹어요ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100033954.1.23",
          "form": "...ㅎ 어디가서 못먹는 거 없다고 하지 마세욧!",
          "original_form": "...ㅎ 어디가서 못먹는 거 없다고 하지 마세욧!",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100033954.1.24",
          "form": "제 주변 친구가 그말 하면 식도락메이트 생겼다고 기뻐하다 좌절했을 거예요ㅜ",
          "original_form": "제 주변 친구가 그말 하면 식도락메이트 생겼다고 기뻐하다 좌절했을 거예요ㅜ",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100033954.1.25",
          "form": "제가 그 해삼 멍게 개불을 없어서 목 먹는 사람이라서요ㅜ",
          "original_form": "제가 그 해삼 멍게 개불을 없어서 목 먹는 사람이라서요ㅜ",
          "speaker_id": "2",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100033954.1.26",
          "form": "ㅋㅋㅋㅋ 헐 다 잘드시나요?",
          "original_form": "ㅋㅋㅋㅋ 헐 다 잘드시나요?",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100033954.1.27",
          "form": "천엽은요? 홍어는요?!!",
          "original_form": "천엽은요? 홍어는요?!!",
          "speaker_id": "1",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100033954.1.28",
          "form": "천엽 간 곱창 대창 다 좋아해요",
          "original_form": "천엽 간 곱창 대창 다 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100033954.1.29",
          "form": "홍어도 거부감은 없더라고요",
          "original_form": "홍어도 거부감은 없더라고요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100033954.1.30",
          "form": "부모님이 불가사리라고ㅎ",
          "original_form": "부모님이 불가사리라고ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100033954.1.31",
          "form": "어...그럼 천엽도 안 드시나요?",
          "original_form": "어...그럼 천엽도 안 드시나요?",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100033954.1.32",
          "form": "와우..",
          "original_form": "와우..",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100033954.1.33",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100033954.1.34",
          "form": "와우..",
          "original_form": "와우..",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100033954.1.35",
          "form": "거의 뭐 만렙자시네요?",
          "original_form": "거의 뭐 만렙자시네요?",
