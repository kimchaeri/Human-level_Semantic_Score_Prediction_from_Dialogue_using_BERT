      "utterance": [
        {
          "id": "MDRW2100037147.1.1",
          "form": "감히 권해드립니다 ㅋㅋㅋ",
          "original_form": "감히 권해드립니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:39"
        },
        {
          "id": "MDRW2100037147.1.2",
          "form": "두유를 좋아하신다고 하니...!!",
          "original_form": "두유를 좋아하신다고 하니...!!",
          "speaker_id": "1",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100037147.1.3",
          "form": "저는 스타벅스 가면 보통 차이티라떼 마셔요",
          "original_form": "저는 스타벅스 가면 보통 차이티라떼 마셔요",
          "speaker_id": "2",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100037147.1.4",
          "form": "두유라떼도 궁금하네요 ㅋㅋ",
          "original_form": "두유라떼도 궁금하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100037147.1.5",
          "form": "차이~~",
          "original_form": "차이~~",
          "speaker_id": "1",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100037147.1.6",
          "form": "아! 제가 그 진짜 옛날에",
          "original_form": "아! 제가 그 진짜 옛날에",
          "speaker_id": "1",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100037147.1.7",
          "form": "거의ㅜ8~9년전에 스벅에서 소이라떼 먹고 니맛도 내맛돜ㅋㅋㅋ 아닌맛에 놀랬던적이 있거든요!!!!!",
          "original_form": "거의ㅜ8~9년전에 스벅에서 소이라떼 먹고 니맛도 내맛돜ㅋㅋㅋ 아닌맛에 놀랬던적이 있거든요!!!!!",
          "speaker_id": "1",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100037147.1.8",
          "form": "그때가 진짜 추운겨울이라 따뜻해보이는거 선택했었는데 노맛에 놀랐던기억이.",
          "original_form": "그때가 진짜 추운겨울이라 따뜻해보이는거 선택했었는데 노맛에 놀랐던기억이.",
          "speaker_id": "1",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100037147.1.9",
          "form": "그때는 제가 참입맛이 아니었나봐요...",
          "original_form": "그때는 제가 참입맛이 아니었나봐요...",
          "speaker_id": "1",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100037147.1.10",
          "form": "입맛도 조금씩 변하더라구요 ㅋㅋ",
          "original_form": "입맛도 조금씩 변하더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037147.1.11",
          "form": "아니면 그 때랑 레시피가 달라졌거나 직원분의 솜씨 차이일지도?",
          "original_form": "아니면 그 때랑 레시피가 달라졌거나 직원분의 솜씨 차이일지도?",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100037147.1.12",
          "form": "아니면... 지금 스벅에서 주문해온건아니라... 브랜드맛이 다른걸까요  ㅎㅎㅎㅎ",
          "original_form": "아니면... 지금 스벅에서 주문해온건아니라... 브랜드맛이 다른걸까요  ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:43"
        },
        {
          "id": "MDRW2100037147.1.13",
          "form": "그곳만의 비법이 있나봐요 ㅎㅎ",
          "original_form": "그곳만의 비법이 있나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 20:46"
        },
        {
          "id": "MDRW2100037147.1.14",
          "form": "그런걸까요...? 다른곳에서도 한번 시도해봐야할것 같아요",
          "original_form": "그런걸까요...? 다른곳에서도 한번 시도해봐야할것 같아요",
          "speaker_id": "1",
          "time": "20210831 20:47"
        },
        {
          "id": "MDRW2100037147.1.15",
          "form": "한동안 두유라떼 많이 드시겠는데요?ㅋㅋ",
          "original_form": "한동안 두유라떼 많이 드시겠는데요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100037147.1.16",
          "form": "그럴것 같아요 ㅋㅋㅋㅋ",
          "original_form": "그럴것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:49"
        },
        {
          "id": "MDRW2100037147.1.17",
          "form": "저는 꽂히면 하나만 계속 먹는 스타일이랔ㅋ",
          "original_form": "저는 꽂히면 하나만 계속 먹는 스타일이랔ㅋ",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100037147.1.18",
          "form": "또 꽂혔던 음식 있나요?",
          "original_form": "또 꽂혔던 음식 있나요?",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100037147.1.19",
          "form": "저는 버거킹 와퍼...",
          "original_form": "저는 버거킹 와퍼...",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100037147.1.20",
          "form": "라떼는 n년째 현재진행형..ㅋㅋㅋㅋ",
          "original_form": "라떼는 n년째 현재진행형..ㅋㅋㅋㅋ",
