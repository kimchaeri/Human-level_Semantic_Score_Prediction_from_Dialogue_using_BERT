      "utterance": [
        {
          "id": "MDRW2100013955.1.1",
          "form": "대학생이신가요?",
          "original_form": "대학생이신가요?",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013955.1.2",
          "form": "저는 10년 전에 학교생활을 했습죠",
          "original_form": "저는 10년 전에 학교생활을 했습죠",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013955.1.3",
          "form": "대학생입니까",
          "original_form": "대학생입니까",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013955.1.4",
          "form": "넵 지금은 휴학생이에요",
          "original_form": "넵 지금은 휴학생이에요",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013955.1.5",
          "form": "크흐 좋을 때네요",
          "original_form": "크흐 좋을 때네요",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013955.1.6",
          "form": "요새는 다 비대면 아닌가요?",
          "original_form": "요새는 다 비대면 아닌가요?",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013955.1.7",
          "form": "저번까진 그랬는데 이번엔 대면도 많이 섞여있어서",
          "original_form": "저번까진 그랬는데 이번엔 대면도 많이 섞여있어서",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013955.1.8",
          "form": "어케 될지 모르겠네요",
          "original_form": "어케 될지 모르겠네요",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013955.1.9",
          "form": "저 졸업한 학교는 비대면 공지 떴더라고용",
          "original_form": "저 졸업한 학교는 비대면 공지 떴더라고용",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013955.1.10",
          "form": "요즘 확진자가 워낙 많아서 그럴만하죠",
          "original_form": "요즘 확진자가 워낙 많아서 그럴만하죠",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013955.1.11",
          "form": "무슨과셨나요?",
          "original_form": "무슨과셨나요?",
          "speaker_id": "1",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013955.1.12",
          "form": "저 국문학과요",
          "original_form": "저 국문학과요",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013955.1.13",
          "form": "님은 어떤 학과 다니세요?",
          "original_form": "님은 어떤 학과 다니세요?",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013955.1.14",
          "form": "저는 컴공이요",
          "original_form": "저는 컴공이요",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013955.1.15",
          "form": "국문이시구나 인문쪽 수업 재밌죠",
          "original_form": "국문이시구나 인문쪽 수업 재밌죠",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013955.1.16",
          "form": "오 저희 언니도 컴공 나와서 개발자 하고 있는뎅",
          "original_form": "오 저희 언니도 컴공 나와서 개발자 하고 있는뎅",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013955.1.17",
          "form": "수업 그냥 그래요..ㅎ",
          "original_form": "수업 그냥 그래요..ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013955.1.18",
          "form": "앗 그런가요.. ㅎㅎ 우와 멋있네요 저도 개발자 되려고 컴공으로 전과했는데 역시 힘들긴 하더라구요 개발자분들 대단해요",
          "original_form": "앗 그런가요.. ㅎㅎ 우와 멋있네요 저도 개발자 되려고 컴공으로 전과했는데 역시 힘들긴 하더라구요 개발자분들 대단해요",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013955.1.19",
          "form": "아직 학생이시면",
          "original_form": "아직 학생이시면",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013955.1.20",
          "form": "학점관리 힘드시겠어요",
          "original_form": "학점관리 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013955.1.21",
          "form": "네 조금,,",
          "original_form": "네 조금,,",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013955.1.22",
          "form": "비대면이라서 그나마 연명하고 있어요",
          "original_form": "비대면이라서 그나마 연명하고 있어요",
          "speaker_id": "1",
