      "utterance": [
        {
          "id": "MDRW2100022056.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210829 01:38"
        },
        {
          "id": "MDRW2100022056.1.2",
          "form": "안녕하세염",
          "original_form": "안녕하세염",
          "speaker_id": "2",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022056.1.3",
          "form": "최근에 산 물건 뭐잇어요?",
          "original_form": "최근에 산 물건 뭐잇어요?",
          "speaker_id": "2",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100022056.1.4",
          "form": "저는 겨울을 위해 구스다운을 샀어요",
          "original_form": "저는 겨울을 위해 구스다운을 샀어요",
          "speaker_id": "1",
          "time": "20210829 01:42"
        },
        {
          "id": "MDRW2100022056.1.5",
          "form": "패딩인가요?",
          "original_form": "패딩인가요?",
          "speaker_id": "2",
          "time": "20210829 01:43"
        },
        {
          "id": "MDRW2100022056.1.6",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210829 01:43"
        },
        {
          "id": "MDRW2100022056.1.7",
          "form": "거위털들어간 패딩인데\n이월상품이요 ㅋㅋ",
          "original_form": "거위털들어간 패딩인데\n이월상품이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022056.1.8",
          "form": "아하 그렇군용",
          "original_form": "아하 그렇군용",
          "speaker_id": "2",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022056.1.9",
          "form": "이월상품으로 사면 저렴하게 살수잇됴",
          "original_form": "이월상품으로 사면 저렴하게 살수잇됴",
          "speaker_id": "2",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022056.1.10",
          "form": "와 무슨 68만원짜리가 8만원이되니까\n안살수가없더라고요",
          "original_form": "와 무슨 68만원짜리가 8만원이되니까\n안살수가없더라고요",
          "speaker_id": "1",
          "time": "20210829 01:44"
        },
        {
          "id": "MDRW2100022056.1.11",
          "form": "와우 이건 무조건 사야죠 ㅋㅋㅋㅋㅋ",
          "original_form": "와우 이건 무조건 사야죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:45"
        },
        {
          "id": "MDRW2100022056.1.12",
          "form": "안사면 손해보는 기분 일거같아요",
          "original_form": "안사면 손해보는 기분 일거같아요",
          "speaker_id": "2",
          "time": "20210829 01:45"
        },
        {
          "id": "MDRW2100022056.1.13",
          "form": "동생이 사라고 강요해서 삿엇죠..\n엄마가 입어봣던옷이엇던것같은데\n제가 3년만에삿더라고요 ㅎ",
          "original_form": "동생이 사라고 강요해서 삿엇죠..\n엄마가 입어봣던옷이엇던것같은데\n제가 3년만에삿더라고요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:46"
        },
        {
          "id": "MDRW2100022056.1.14",
          "form": "오오 디자인은 어때요?",
          "original_form": "오오 디자인은 어때요?",
          "speaker_id": "2",
          "time": "20210829 01:47"
        },
        {
          "id": "MDRW2100022056.1.15",
          "form": "저는 마음에들엇어요!!",
          "original_form": "저는 마음에들엇어요!!",
          "speaker_id": "1",
          "time": "20210829 01:50"
        },
        {
          "id": "MDRW2100022056.1.16",
          "form": "오 다행이네용",
          "original_form": "오 다행이네용",
          "speaker_id": "2",
          "time": "20210829 01:50"
        },
        {
          "id": "MDRW2100022056.1.17",
          "form": "완전 더울때와서 입어보고 더워 죽는줄알앗어요 ㅋㅋㅋ",
          "original_form": "완전 더울때와서 입어보고 더워 죽는줄알앗어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:52"
        },
        {
          "id": "MDRW2100022056.1.18",
          "form": "아 ㅋㅋㅋㅋㅌㅌㅋㅌ 잘 모셔두고 이제 겨울에 입으시면 되겟네요",
          "original_form": "아 ㅋㅋㅋㅋㅌㅌㅋㅌ 잘 모셔두고 이제 겨울에 입으시면 되겟네요",
          "speaker_id": "2",
          "time": "20210829 01:52"
        },
        {
          "id": "MDRW2100022056.1.19",
          "form": "케이스같은걸 사야겟어요.. ㅋㅋ",
          "original_form": "케이스같은걸 사야겟어요.. ㅋㅋ",
          "speaker_id": "1",
