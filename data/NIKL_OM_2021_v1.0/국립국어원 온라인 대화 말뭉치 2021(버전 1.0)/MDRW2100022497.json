      "utterance": [
        {
          "id": "MDRW2100022497.1.1",
          "form": "반려동물 키우세요??",
          "original_form": "반려동물 키우세요??",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022497.1.2",
          "form": "아뇨... \n뭐 키우시나요?",
          "original_form": "아뇨... \n뭐 키우시나요?",
          "speaker_id": "1",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022497.1.3",
          "form": "저도...안키워요 ㅋㅋ",
          "original_form": "저도...안키워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022497.1.4",
          "form": "동물은 좋아하시나여?",
          "original_form": "동물은 좋아하시나여?",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022497.1.5",
          "form": "어우,, 사람이외에 움직이는 것들을 다 싫어해여 ㅎ",
          "original_form": "어우,, 사람이외에 움직이는 것들을 다 싫어해여 ㅎ",
          "speaker_id": "2",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022497.1.6",
          "form": "헉.. 진짜유?",
          "original_form": "헉.. 진짜유?",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022497.1.7",
          "form": "강아지.. 고양이.. 호랑이.. 전부다?",
          "original_form": "강아지.. 고양이.. 호랑이.. 전부다?",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022497.1.8",
          "form": "네 보는건 좋아요 ㅋ 가까이 오면....ㄷ ㄷ",
          "original_form": "네 보는건 좋아요 ㅋ 가까이 오면....ㄷ ㄷ",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022497.1.9",
          "form": "만지는거 자체가 힘든거에요?",
          "original_form": "만지는거 자체가 힘든거에요?",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022497.1.10",
          "form": "무서워요ㅠㅠ 강아지 옆에오면 ㄷ ㄷ",
          "original_form": "무서워요ㅠㅠ 강아지 옆에오면 ㄷ ㄷ",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022497.1.11",
          "form": "어릴때 트라우마에요?",
          "original_form": "어릴때 트라우마에요?",
          "speaker_id": "1",
          "time": "20210829 16:00"
        },
        {
          "id": "MDRW2100022497.1.12",
          "form": "딱히 그런건 없었던것 같아여ㅠ",
          "original_form": "딱히 그런건 없었던것 같아여ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:01"
        },
        {
          "id": "MDRW2100022497.1.13",
          "form": "그.폭신폭신하고 따뜻하고 꼬릿꼬릿한 품 너무 좋은데.. ㅋㅋㅋ",
          "original_form": "그.폭신폭신하고 따뜻하고 꼬릿꼬릿한 품 너무 좋은데.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022497.1.14",
          "form": "그럼 파충류는 더 싫겟어요?",
          "original_form": "그럼 파충류는 더 싫겟어요?",
          "speaker_id": "1",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022497.1.15",
          "form": "세상에나.. 극혐..이에여 ㅋ",
          "original_form": "세상에나.. 극혐..이에여 ㅋ",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022497.1.16",
          "form": "파충류...ㅋㅋ",
          "original_form": "파충류...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022497.1.17",
          "form": "벌레,곤충...제일싫어해여ㅠㅠ",
          "original_form": "벌레,곤충...제일싫어해여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022497.1.18",
          "form": "ㅋㅋㅋ 사람은 키우시나여?",
          "original_form": "ㅋㅋㅋ 사람은 키우시나여?",
          "speaker_id": "1",
          "time": "20210829 16:06"
        },
        {
          "id": "MDRW2100022497.1.19",
