      "utterance": [
        {
          "id": "MDRW2100002452.1.1",
          "form": "날씨가 참좋아서 창문을 열고사는거 같아요",
          "original_form": "날씨가 참좋아서 창문을 열고사는거 같아요",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002452.1.2",
          "form": "맞아요 가을이라 선선하네요",
          "original_form": "맞아요 가을이라 선선하네요",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002452.1.3",
          "form": "어떤계절을 좋아하시나요??",
          "original_form": "어떤계절을 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002452.1.4",
          "form": "가을을 참 좋아해요",
          "original_form": "가을을 참 좋아해요",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002452.1.5",
          "form": "시원하고 풍경도 멋지고",
          "original_form": "시원하고 풍경도 멋지고",
          "speaker_id": "2",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002452.1.6",
          "form": "저도 가을 참 좋아해요",
          "original_form": "저도 가을 참 좋아해요",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002452.1.7",
          "form": "지금이 딱이네요",
          "original_form": "지금이 딱이네요",
          "speaker_id": "1",
          "time": "20210820 12:46"
        },
        {
          "id": "MDRW2100002452.1.8",
          "form": "미세먼지도 덜하고 지금이 행복하네요",
          "original_form": "미세먼지도 덜하고 지금이 행복하네요",
          "speaker_id": "2",
          "time": "20210820 12:47"
        },
        {
          "id": "MDRW2100002452.1.9",
          "form": "미세먼지 없는 날씨가 좋긴하죠",
          "original_form": "미세먼지 없는 날씨가 좋긴하죠",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002452.1.10",
          "form": "놀러가고 싶은 날씨에요",
          "original_form": "놀러가고 싶은 날씨에요",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002452.1.11",
          "form": "이런날씨엔 연차써서 어디가고 싶죠",
          "original_form": "이런날씨엔 연차써서 어디가고 싶죠",
          "speaker_id": "2",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002452.1.12",
          "form": "네 그러고싶네요 환절기 조심하세요",
          "original_form": "네 그러고싶네요 환절기 조심하세요",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002452.1.13",
          "form": "맞아요 감기걸리기 딱 좋은날씨",
          "original_form": "맞아요 감기걸리기 딱 좋은날씨",
          "speaker_id": "2",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100002452.1.14",
          "form": "이번주에는 비가올까요??",
          "original_form": "이번주에는 비가올까요??",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002452.1.15",
          "form": "온다구만 하고 안오네요",
          "original_form": "온다구만 하고 안오네요",
          "speaker_id": "1",
          "time": "20210820 12:50"
        },
        {
          "id": "MDRW2100002452.1.16",
          "form": "비가계속오네요",
          "original_form": "비가계속오네요",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100002452.1.17",
          "form": "가을 장마 인가봐요",
          "original_form": "가을 장마 인가봐요",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100002452.1.18",
          "form": "네 장마라구 하더라구요",
          "original_form": "네 장마라구 하더라구요",
          "speaker_id": "1",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100002452.1.19",
          "form": "그래서그런지 춥네요",
          "original_form": "그래서그런지 춥네요",
          "speaker_id": "2",
          "time": "20210902 14:05"
        }
      ]
