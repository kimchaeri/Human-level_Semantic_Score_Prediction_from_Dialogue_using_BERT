      "utterance": [
        {
          "id": "MDRW2100040122.1.1",
          "form": "서브퀘 이것저것 건드려보고 있는데",
          "original_form": "서브퀘 이것저것 건드려보고 있는데",
          "speaker_id": "1",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100040122.1.2",
          "form": "퍼즐 짜증나...",
          "original_form": "퍼즐 짜증나...",
          "speaker_id": "1",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100040122.1.3",
          "form": "서브퀘는 공략 좀 나오면 손대려구......",
          "original_form": "서브퀘는 공략 좀 나오면 손대려구......",
          "speaker_id": "2",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100040122.1.4",
          "form": "멘스는 오늘 저녁에나 밀듯",
          "original_form": "멘스는 오늘 저녁에나 밀듯",
          "speaker_id": "2",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100040122.1.5",
          "form": "난 이따 빨래돌려놓고 멘스 남은거 다 밀건데",
          "original_form": "난 이따 빨래돌려놓고 멘스 남은거 다 밀건데",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100040122.1.6",
          "form": "대체 왜 결말이 후루룩 말아먹는 전개가 되는건지",
          "original_form": "대체 왜 결말이 후루룩 말아먹는 전개가 되는건지",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100040122.1.7",
          "form": "알 수 업다..",
          "original_form": "알 수 업다..",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100040122.1.8",
          "form": "난 밥 좀 먹고",
          "original_form": "난 밥 좀 먹고",
          "speaker_id": "2",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100040122.1.9",
          "form": "자야겠어......",
          "original_form": "자야겠어......",
          "speaker_id": "2",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100040122.1.10",
          "form": "잠을 못잤네",
          "original_form": "잠을 못잤네",
          "speaker_id": "2",
          "time": "20210904 11:00"
        },
        {
          "id": "MDRW2100040122.1.11",
          "form": "ㅇㅇ....",
          "original_form": "ㅇㅇ....",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100040122.1.12",
          "form": "어쩐지 아침부터 있더라니 ㅋㅋㅋ",
          "original_form": "어쩐지 아침부터 있더라니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100040122.1.13",
          "form": "그렇게 되엇읍니다...",
          "original_form": "그렇게 되엇읍니다...",
          "speaker_id": "2",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100040122.1.14",
          "form": "아 그리고",
          "original_form": "아 그리고",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100040122.1.15",
          "form": "이번 업뎃에서 제일 충격적인거",
          "original_form": "이번 업뎃에서 제일 충격적인거",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100040122.1.16",
          "form": "멘스에서 세이라이섬을 아예 안감",
          "original_form": "멘스에서 세이라이섬을 아예 안감",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100040122.1.17",
          "form": "아니 그럼ㄹㅇ",
          "original_form": "아니 그럼ㄹㅇ",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100040122.1.18",
          "form": "저 전기박쥐만을 위해",
          "original_form": "저 전기박쥐만을 위해",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100040122.1.19",
          "form": "있는곳이야??",
          "original_form": "있는곳이야??",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100040122.1.20",
          "form": "왜 그런 맵낭비를.....?",
          "original_form": "왜 그런 맵낭비를.....?",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100040122.1.21",
          "form": "그것까진 몰겟지만",
          "original_form": "그것까진 몰겟지만",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040122.1.22",
          "form": "보통 스토리 진행하다보면 원래",
          "original_form": "보통 스토리 진행하다보면 원래",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040122.1.23",
          "form": "맵 전부 열게 되어있잖아?",
          "original_form": "맵 전부 열게 되어있잖아?",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040122.1.24",
          "form": "얜 아님 ㅋㅋㅋ",
          "original_form": "얜 아님 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100040122.1.25",
          "form": "ㅇㅎ이나즈마 성",
          "original_form": "ㅇㅎ이나즈마 성",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040122.1.26",
          "form": "왜 있는것도없으면서",
          "original_form": "왜 있는것도없으면서",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040122.1.27",
          "form": "쓰잘떼기없이 넓게 디자인했나 했더니",
          "original_form": "쓰잘떼기없이 넓게 디자인했나 했더니",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040122.1.28",
          "form": "거기가 시뇨라 자리구나",
          "original_form": "거기가 시뇨라 자리구나",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040122.1.29",
          "form": "시뇨라 만남?",
          "original_form": "시뇨라 만남?",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100040122.1.30",
          "form": "이러고 빨래널러갓어",
          "original_form": "이러고 빨래널러갓어",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040122.1.31",
          "form": "이제 만나러 갈듯",
          "original_form": "이제 만나러 갈듯",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040122.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040122.1.33",
          "form": "시뇨라 전투햇어?",
          "original_form": "시뇨라 전투햇어?",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040122.1.34",
          "form": "ㅇㅓ",
          "original_form": "ㅇㅓ",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040122.1.35",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040122.1.36",
          "form": "멍때리고잇엇어....",
          "original_form": "멍때리고잇엇어....",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040122.1.37",
          "form": "체력만 잘 채우면",
          "original_form": "체력만 잘 채우면",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040122.1.38",
          "form": "긴장돼......",
          "original_form": "긴장돼......",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100040122.1.39",
          "form": "밥 160개로",
          "original_form": "밥 160개로",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100040122.1.40",
          "form": "잡는건 글케 안어려움",
          "original_form": "잡는건 글케 안어려움",
          "speaker_id": "1",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100040122.1.41",
          "form": "괜찮겠지?",
          "original_form": "괜찮겠지?",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100040122.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋ 그거 다 먹기전에 포만도 아웃될듯",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그거 다 먹기전에 포만도 아웃될듯",
          "speaker_id": "1",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100040122.1.43",
