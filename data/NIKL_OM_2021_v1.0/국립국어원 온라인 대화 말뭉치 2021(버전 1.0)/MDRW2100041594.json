      "utterance": [
        {
          "id": "MDRW2100041594.1.1",
          "form": "햄버거먹기로했어",
          "original_form": "햄버거먹기로했어",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041594.1.2",
          "form": "버거킹 먹을 건데",
          "original_form": "버거킹 먹을 건데",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041594.1.3",
          "form": "어디햄버거?",
          "original_form": "어디햄버거?",
          "speaker_id": "1",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041594.1.4",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041594.1.5",
          "form": "메뉴 추천해줘",
          "original_form": "메뉴 추천해줘",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041594.1.6",
          "form": "주니어로 먹을거야",
          "original_form": "주니어로 먹을거야",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041594.1.7",
          "form": "와퍼가 젤 무난하지 않아?",
          "original_form": "와퍼가 젤 무난하지 않아?",
          "speaker_id": "1",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041594.1.8",
          "form": "콰트로나",
          "original_form": "콰트로나",
          "speaker_id": "1",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041594.1.9",
          "form": "콰트로를 주로 먹어",
          "original_form": "콰트로를 주로 먹어",
          "speaker_id": "2",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041594.1.10",
          "form": "콰트로가 우리나라에서 시작한 뭔가의",
          "original_form": "콰트로가 우리나라에서 시작한 뭔가의",
          "speaker_id": "1",
          "time": "20210906 12:01"
        },
        {
          "id": "MDRW2100041594.1.11",
          "form": "버거라던데",
          "original_form": "버거라던데",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.12",
          "form": "뭔가의?",
          "original_form": "뭔가의?",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.13",
          "form": "스테이크 뭐시기",
          "original_form": "스테이크 뭐시기",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.15",
          "form": "있지않나",
          "original_form": "있지않나",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.16",
          "form": "글로벌로 뻗어간?",
          "original_form": "글로벌로 뻗어간?",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.17",
          "form": "한국에서 대충 흥하면",
          "original_form": "한국에서 대충 흥하면",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.18",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.19",
          "form": "해외에서도 흥한다는",
          "original_form": "해외에서도 흥한다는",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.20",
          "form": "공식",
          "original_form": "공식",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.21",
          "form": "국뽕영상을 봤어",
          "original_form": "국뽕영상을 봤어",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ국뽕",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ국뽕",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.23",
          "form": "그건 먹어봣어?",
          "original_form": "그건 먹어봣어?",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.24",
          "form": "그 예를들어",
          "original_form": "그 예를들어",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.25",
          "form": "고추장",
          "original_form": "고추장",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.26",
          "form": "모",
          "original_form": "모",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041594.1.27",
          "form": "아니 몬지몰라",
          "original_form": "아니 몬지몰라",
          "speaker_id": "1",
          "time": "20210906 12:02"
        },
