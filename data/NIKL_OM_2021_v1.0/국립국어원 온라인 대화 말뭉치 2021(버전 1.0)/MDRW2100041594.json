          "speaker_id": "2",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041594.1.33",
          "form": "예를들어",
          "original_form": "예를들어",
          "speaker_id": "2",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041594.1.34",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041594.1.35",
          "form": "라고 했잖아",
          "original_form": "라고 했잖아",
          "speaker_id": "2",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041594.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041594.1.37",
          "form": "뭐야...왜그래...",
          "original_form": "뭐야...왜그래...",
          "speaker_id": "2",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041594.1.38",
          "form": "고추장 꺼내길래",
          "original_form": "고추장 꺼내길래",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041594.1.39",
          "form": "이것부터 듣고 말하랴그했지",
          "original_form": "이것부터 듣고 말하랴그했지",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041594.1.40",
          "form": "끝났잖아",
          "original_form": "끝났잖아",
          "speaker_id": "2",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041594.1.41",
          "form": "그게뭔지 가르쳐줄 줄 알았지",
          "original_form": "그게뭔지 가르쳐줄 줄 알았지",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041594.1.42",
          "form": "그 배라 아이스크림 케이크가 한국에서 시작했다는 말을 하려고 햇어",
          "original_form": "그 배라 아이스크림 케이크가 한국에서 시작했다는 말을 하려고 햇어",
          "speaker_id": "1",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041594.1.43",
          "form": "먹어봤느냐 (나는 궁금하다)>모름>에잉(아쉽구나)",
          "original_form": "먹어봤느냐 (나는 궁금하다)>모름>에잉(아쉽구나)",
          "speaker_id": "2",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041594.1.44",
