      "utterance": [
        {
          "id": "MDRW2100023932.1.1",
          "form": "다시 가지 얘기로 돌아와서...",
          "original_form": "다시 가지 얘기로 돌아와서...",
          "speaker_id": "1",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023932.1.2",
          "form": "저녁으로 가지구이를 드셨다고 했는데 에어프라이어에 구우셨나요?",
          "original_form": "저녁으로 가지구이를 드셨다고 했는데 에어프라이어에 구우셨나요?",
          "speaker_id": "1",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023932.1.3",
          "form": "아뇨 밀가루를 얇게 발라서 프라이팬에 구웠어요",
          "original_form": "아뇨 밀가루를 얇게 발라서 프라이팬에 구웠어요",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100023932.1.4",
          "form": "에프에 구워서 반을 긁어내고 거기 치즈와 토마토소스를 채워먹고 싶지만",
          "original_form": "에프에 구워서 반을 긁어내고 거기 치즈와 토마토소스를 채워먹고 싶지만",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100023932.1.5",
          "form": "우왕 에프 레시피 귀찮지만 넘 맛있을 것 같아요",
          "original_form": "우왕 에프 레시피 귀찮지만 넘 맛있을 것 같아요",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100023932.1.6",
          "form": "프라이팬에 구울 때 밀가루는 왜 바르는 건가요?",
          "original_form": "프라이팬에 구울 때 밀가루는 왜 바르는 건가요?",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023932.1.7",
          "form": "프라이팬에 구워도 돼요",
          "original_form": "프라이팬에 구워도 돼요",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023932.1.8",
          "form": "가지전이었어요 ㅋㅋ",
          "original_form": "가지전이었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023932.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023932.1.10",
          "form": "전 먼가 밀가루가 하는 기능이 있는 줄 알고...",
          "original_form": "전 먼가 밀가루가 하는 기능이 있는 줄 알고...",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023932.1.11",
          "form": "간은 소금으로 하셨나요?",
          "original_form": "간은 소금으로 하셨나요?",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023932.1.12",
          "form": "네 미원가진 필요없어서 그냥 소금만 쳤어요",
          "original_form": "네 미원가진 필요없어서 그냥 소금만 쳤어요",
          "speaker_id": "2",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023932.1.13",
          "form": "제가 라구 소스를 4키로를 샀는데",
          "original_form": "제가 라구 소스를 4키로를 샀는데",
          "speaker_id": "2",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023932.1.14",
          "form": "가지구이 라구소스도 꽤 먹었어요",
          "original_form": "가지구이 라구소스도 꽤 먹었어요",
          "speaker_id": "2",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100023932.1.15",
          "form": "아니 보통 500g 단위로 병 소스를 팔지 않나요",
          "original_form": "아니 보통 500g 단위로 병 소스를 팔지 않나요",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023932.1.16",
          "form": "갑자기 4키로...",
          "original_form": "갑자기 4키로...",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023932.1.17",
          "form": "4키로 산 거 다 드셨나요?",
          "original_form": "4키로 산 거 다 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023932.1.18",
          "form": "아니오 3키로는 냉동실에",
          "original_form": "아니오 3키로는 냉동실에",
          "speaker_id": "2",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023932.1.19",
          "form": "1키로는 제 뱃속에",
          "original_form": "1키로는 제 뱃속에",
          "speaker_id": "2",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100023932.1.20",
          "form": "ㅋㅋㅋㅋㅋ 남은 3키로도 얼른 드시길 바라요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 남은 3키로도 얼른 드시길 바라요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100023932.1.21",
          "form": "냉동실에 넣었으니 상하진 않겠지만",
          "original_form": "냉동실에 넣었으니 상하진 않겠지만",
          "speaker_id": "1",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100023932.1.22",
          "form": "필요하심 말씀하세요",
          "original_form": "필요하심 말씀하세요",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100023932.1.23",
          "form": "나눌수록 줄어드는 식재료",
          "original_form": "나눌수록 줄어드는 식재료",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100023932.1.24",
          "form": "아뇨 저는 됐어요 사놓은 식재료도 다 못 먹어서 매번 버려요ㅠㅠ",
          "original_form": "아뇨 저는 됐어요 사놓은 식재료도 다 못 먹어서 매번 버려요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100023932.1.25",
          "form": "냉동고를 사심 됩니다",
          "original_form": "냉동고를 사심 됩니다",
          "speaker_id": "2",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023932.1.26",
          "form": "냉동고를 사시면 양파도 뚝딱",
          "original_form": "냉동고를 사시면 양파도 뚝딱",
          "speaker_id": "2",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023932.1.27",
          "form": "다진양파 다진 대파 상태로 냉동하면",
          "original_form": "다진양파 다진 대파 상태로 냉동하면",
          "speaker_id": "2",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023932.1.28",
          "form": "파는 다지진 않고 썰어서 냉동 보관 해두긴 해요.",
          "original_form": "파는 다지진 않고 썰어서 냉동 보관 해두긴 해요.",
          "speaker_id": "1",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100023932.1.29",
          "form": "카레나 라면에 냉동 파 투하해서 먹음 맛있었어요ㅎㅎ",
          "original_form": "카레나 라면에 냉동 파 투하해서 먹음 맛있었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023932.1.30",
          "form": "전 다져요 그래야 볶음 밥 해먹기 편해요",
          "original_form": "전 다져요 그래야 볶음 밥 해먹기 편해요",
          "speaker_id": "2",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023932.1.31",
          "form": "저도 볶음밥 좋아해서 자주 먹는데",
          "original_form": "저도 볶음밥 좋아해서 자주 먹는데",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023932.1.32",
          "form": "걍 냉동 볶음밥 사서 볶아먹는 정도?",
          "original_form": "걍 냉동 볶음밥 사서 볶아먹는 정도?",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023932.1.33",
          "form": "파도 넣어 드시는군여...",
          "original_form": "파도 넣어 드시는군여...",
          "speaker_id": "1",
