      "utterance": [
        {
          "id": "MDRW2100020748.1.1",
          "form": "아니요 맘에안들어요 빨리 기르려구요ㅋㅋㅋ",
          "original_form": "아니요 맘에안들어요 빨리 기르려구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:02"
        },
        {
          "id": "MDRW2100020748.1.2",
          "form": "머리잘기르고있지요?",
          "original_form": "머리잘기르고있지요?",
          "speaker_id": "2",
          "time": "20210828 12:03"
        },
        {
          "id": "MDRW2100020748.1.3",
          "form": "헝 맘에안들어요?",
          "original_form": "헝 맘에안들어요?",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020748.1.4",
          "form": "저는 더 자를까싶은데",
          "original_form": "저는 더 자를까싶은데",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020748.1.5",
          "form": "헐 그때  기른다구했잖아요!  이ㅡ닌가..",
          "original_form": "헐 그때  기른다구했잖아요!  이ㅡ닌가..",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020748.1.6",
          "form": "사실너무너무편한데 긴게 더나은거같기도하곶..",
          "original_form": "사실너무너무편한데 긴게 더나은거같기도하곶..",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020748.1.7",
          "form": "긴머리가확실히이쁘죠",
          "original_form": "긴머리가확실히이쁘죠",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020748.1.8",
          "form": "하지만 단발 너무 편해",
          "original_form": "하지만 단발 너무 편해",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020748.1.9",
          "form": "편한걸로따지면 절대단발쓰 최고최고",
          "original_form": "편한걸로따지면 절대단발쓰{emoji:☆} 최고최고",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100020748.1.10",
          "form": "정말그래요 그냥기를까자를까",
          "original_form": "정말그래요 그냥기를까자를까",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100020748.1.11",
          "form": "근데name1잘어울려요단발쓰",
          "original_form": "근데&name1&잘어울려요단발쓰",
          "speaker_id": "2",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100020748.1.12",
          "form": "다행이에요 고민중",
          "original_form": "다행이에요 고민중",
          "speaker_id": "1",
          "time": "20210828 18:55"
        },
        {
          "id": "MDRW2100020748.1.13",
          "form": "그냥 단발도 며칠지나니 지겹고 그냥 매사 지겨워욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 단발도 며칠지나니 지겹고 그냥 매사 지겨워욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:56"
        },
        {
          "id": "MDRW2100020748.1.14",
          "form": "ㅋㅋㅋㄱㅋㅋㅋ매사지겨워요?ㅜㅜㄱㅋㅈㄱㅈㄱ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋ매사지겨워요?ㅜㅜㄱㅋㅈㄱㅈㄱ",
          "speaker_id": "2",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100020748.1.15",
          "form": "네 그래요 사는게지겹네요",
          "original_form": "네 그래요 사는게지겹네요",
          "speaker_id": "1",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100020748.1.16",
          "form": "그래서머리도자른거같아요 ㅋㅋㅋ",
          "original_form": "그래서머리도자른거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100020748.1.17",
          "form": "맞아어휴지겨워지겨워  키목소리재ㅇ생",
          "original_form": "맞아어휴지겨워지겨워  키목소리재ㅇ생",
          "speaker_id": "2",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100020748.1.18",
          "form": "ㅋㅋㅋㅋㅋ앜ㅋㅋㅋ맞다",
          "original_form": "ㅋㅋㅋㅋㅋ앜ㅋㅋㅋ맞다",
          "speaker_id": "1",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100020748.1.19",
          "form": "지겨운걸 귀엽게표현해줘서 좋아요",
          "original_form": "지겨운걸 귀엽게표현해줘서 좋아요",
          "speaker_id": "1",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100020748.1.20",
          "form": "ㄱㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄱㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:00"
        },
        {
          "id": "MDRW2100020748.1.21",
          "form": "진짜지겹긴해요",
          "original_form": "진짜지겹긴해요",
          "speaker_id": "2",
          "time": "20210828 19:00"
        },
        {
          "id": "MDRW2100020748.1.22",
          "form": "건강을 위해 운동을 해보아요",
