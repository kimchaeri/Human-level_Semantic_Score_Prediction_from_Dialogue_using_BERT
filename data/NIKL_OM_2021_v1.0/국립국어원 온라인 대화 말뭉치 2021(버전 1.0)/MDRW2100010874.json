      "utterance": [
        {
          "id": "MDRW2100010874.1.1",
          "form": "특히 초보들끼리 하면 기술이 없어서",
          "original_form": "특히 초보들끼리 하면 기술이 없어서",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010874.1.2",
          "form": "엄청 투닥거리거든요 ㅋㅋㅋ",
          "original_form": "엄청 투닥거리거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010874.1.3",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010874.1.4",
          "form": "ㅡ럴것 같아요",
          "original_form": "ㅡ럴것 같아요",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010874.1.5",
          "form": "막 온몸으로 몸씨름을 해요",
          "original_form": "막 온몸으로 몸씨름을 해요",
          "speaker_id": "2",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010874.1.6",
          "form": "근데 가면 남자분들이 대다수아닌가요?",
          "original_form": "근데 가면 남자분들이 대다수아닌가요?",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100010874.1.7",
          "form": "그게 좀 불편하더라고요 저는 ㅜㅜㅜ",
          "original_form": "그게 좀 불편하더라고요 저는 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 22:34"
        },
        {
          "id": "MDRW2100010874.1.8",
          "form": "요잠은 성비 비슷한 도장이 많아요!",
          "original_form": "요잠은 성비 비슷한 도장이 많아요!",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010874.1.9",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010874.1.10",
          "form": "저 다니던 곳은 여자가 더 많았어요",
          "original_form": "저 다니던 곳은 여자가 더 많았어요",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100010874.1.11",
          "form": "그리고 생각보다 남자랑 스파링 하는 게",
          "original_form": "그리고 생각보다 남자랑 스파링 하는 게",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010874.1.12",
          "form": "헐 진짱요?",
          "original_form": "헐 진짱요?",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010874.1.13",
          "form": "ㄴ0ㄴ0",
          "original_form": "ㄴ0ㄴ0",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010874.1.14",
          "form": "막 엄청 거부감들고 민망하고 그러지가 않더라구요",
          "original_form": "막 엄청 거부감들고 민망하고 그러지가 않더라구요",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010874.1.15",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010874.1.16",
          "form": "그런 생각이 들 정신마저도 없거든요",
          "original_form": "그런 생각이 들 정신마저도 없거든요",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010874.1.17",
          "form": "하 넘 재밋어 보이네요ㅜㅜ 꼭 배워봐야겟어요",
          "original_form": "하 넘 재밋어 보이네요ㅜㅜ 꼭 배워봐야겟어요",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100010874.1.18",
          "form": "거기도 띠 같은게 잇나요?",
          "original_form": "거기도 띠 같은게 잇나요?",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010874.1.19",
          "form": "네 있어요!! 벨트라고 부르더라구요",
          "original_form": "네 있어요!! 벨트라고 부르더라구요",
          "speaker_id": "2",
          "time": "20210823 22:37"
        }
      ]
