      "utterance": [
        {
          "id": "MDRW2100021790.1.1",
          "form": "좋아하는 연예인은 누구있나요?",
          "original_form": "좋아하는 연예인은 누구있나요?",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021790.1.2",
          "form": "제가 좋아했던 연예인은 범죄자가 돼서...",
          "original_form": "제가 좋아했던 연예인은 범죄자가 돼서...",
          "speaker_id": "2",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100021790.1.3",
          "form": "지금은 서현진 이도현 좋아하는것같아요",
          "original_form": "지금은 서현진 이도현 좋아하는것같아요",
          "speaker_id": "2",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100021790.1.4",
          "form": "누구 좋아하세요,",
          "original_form": "누구 좋아하세요,",
          "speaker_id": "2",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100021790.1.5",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "1",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100021790.1.6",
          "form": "요즘 세븐틴이 눈에 들어와요",
          "original_form": "요즘 세븐틴이 눈에 들어와요",
          "speaker_id": "1",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100021790.1.7",
          "form": "오 세븐틴!!",
          "original_form": "오 세븐틴!!",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100021790.1.8",
          "form": "부승관 너무 웃겨요 ...",
          "original_form": "부승관 너무 웃겨요 ...",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021790.1.9",
          "form": "가수인데 자꾸 웃겨야된다는 부담감 있는 거 넘 웃겨요 ㅋㅋㅋ",
          "original_form": "가수인데 자꾸 웃겨야된다는 부담감 있는 거 넘 웃겨요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021790.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 근데 진짜 재밌고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 근데 진짜 재밌고",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021790.1.11",
          "form": "본업도 잘하는것같더라고요!",
          "original_form": "본업도 잘하는것같더라고요!",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021790.1.12",
          "form": "맞ㅇㅏ요 이번에 신곡 넘 좋더라고요",
          "original_form": "맞ㅇㅏ요 이번에 신곡 넘 좋더라고요",
          "speaker_id": "1",
          "time": "20210828 22:49"
        },
        {
          "id": "MDRW2100021790.1.13",
          "form": "오 신곡이 나왔나요?",
          "original_form": "오 신곡이 나왔나요?",
          "speaker_id": "2",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021790.1.14",
          "form": "레디 투 럽 이요. 지금 또 신곡 작업 중이래요",
          "original_form": "레디 투 럽 이요. 지금 또 신곡 작업 중이래요",
          "speaker_id": "1",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021790.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021790.1.16",
          "form": "덕질하는입장에서 너무 즐거운 일이네요",
          "original_form": "덕질하는입장에서 너무 즐거운 일이네요",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021790.1.17",
          "form": "본업 너무 잘해서 기대되요",
          "original_form": "본업 너무 잘해서 기대되요",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100021790.1.18",
          "form": "또 좋아하는 연예인 있으세요?",
          "original_form": "또 좋아하는 연예인 있으세요?",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021790.1.19",
          "form": "온앤오프 좋아해요",
          "original_form": "온앤오프 좋아해요",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021790.1.20",
          "form": "노래 들어주세요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "노래 들어주세요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100021790.1.21",
          "form": "아이돌 좋아하시는구나!",
          "original_form": "아이돌 좋아하시는구나!",
          "speaker_id": "2",
          "time": "20210828 23:20"
        }
      ]
