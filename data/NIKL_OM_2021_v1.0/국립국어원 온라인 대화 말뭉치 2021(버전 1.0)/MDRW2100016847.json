      "utterance": [
        {
          "id": "MDRW2100016847.1.1",
          "form": "울신랑도2개",
          "original_form": "울신랑도2개",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016847.1.2",
          "form": "2개만되는가?균등이라?",
          "original_form": "2개만되는가?균등이라?",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016847.1.3",
          "form": "균등이요 ㅋㅋ 균등은 1~3개인듯요. ㅋㅋ",
          "original_form": "균등이요 ㅋㅋ 균등은 1~3개인듯요. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016847.1.4",
          "form": "제 친구는 1주됐대요.",
          "original_form": "제 친구는 1주됐대요.",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016847.1.5",
          "form": "그래?ㅎㅎ",
          "original_form": "그래?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016847.1.6",
          "form": "울 신랑은 우째 됐는고 모르겠네 ㅋㅋ",
          "original_form": "울 신랑은 우째 됐는고 모르겠네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016847.1.7",
          "form": "우리아이들 학원갔다",
          "original_form": "우리아이들 학원갔다",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016847.1.8",
          "form": "첫째도 친구집에서 놀다가 나가라고 했는데 ㅋㅋ 잘 도착했는지 모르겟네유 ㅋㅋ",
          "original_form": "첫째도 친구집에서 놀다가 나가라고 했는데 ㅋㅋ 잘 도착했는지 모르겟네유 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016847.1.9",
          "form": "같이 기다리고 있대요. ㅋㅋ",
          "original_form": "같이 기다리고 있대요. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016847.1.10",
          "form": "아ㅋ 그렇군ㅋㅋ",
          "original_form": "아ㅋ 그렇군ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016847.1.11",
          "form": "둘째오겠네",
          "original_form": "둘째오겠네",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016847.1.12",
          "form": "육아시작이군ㅎ",
          "original_form": "육아시작이군ㅎ",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016847.1.13",
          "form": "12시에 잠들었어요ㅜ ㅋㅋ전 이만자요ㅋ",
          "original_form": "12시에 잠들었어요ㅜ ㅋㅋ전 이만자요ㅋ",
          "speaker_id": "1",
          "time": "20210827 01:14"
        },
        {
          "id": "MDRW2100016847.1.14",
          "form": "이래늦게자노ㅜ",
          "original_form": "이래늦게자노ㅜ",
          "speaker_id": "2",
          "time": "20210827 07:34"
        },
        {
          "id": "MDRW2100016847.1.15",
          "form": "난 팔이 좀아프네ㅡㅡ",
          "original_form": "난 팔이 좀아프네ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 07:34"
        },
        {
          "id": "MDRW2100016847.1.16",
          "form": "슬기로운 의사생활본다구요. 그리고 중요한건 애들이 안잤어요ㅋㅋ",
          "original_form": "슬기로운 의사생활본다구요. 그리고 중요한건 애들이 안잤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:37"
        },
        {
          "id": "MDRW2100016847.1.17",
          "form": "그래 나도 슬의생봣다ㅋ",
          "original_form": "그래 나도 슬의생봣다ㅋ",
          "speaker_id": "2",
          "time": "20210827 09:04"
        },
        {
          "id": "MDRW2100016847.1.18",
          "form": "난 누워있음",
          "original_form": "난 누워있음",
          "speaker_id": "2",
          "time": "20210827 09:04"
        },
        {
          "id": "MDRW2100016847.1.19",
          "form": "집에왔나?",
          "original_form": "집에왔나?",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100016847.1.20",
          "form": "이제 저녁생각해야되네",
          "original_form": "이제 저녁생각해야되네",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100016847.1.21",
          "form": "응 일하고 와서 청소 좀 하구,, 집에서 쉬다가 ㅋㅋ 애들왔는데",
          "original_form": "응 일하고 와서 청소 좀 하구,, 집에서 쉬다가 ㅋㅋ 애들왔는데",
          "speaker_id": "1",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100016847.1.22",
          "form": "오늘 너무 피곤해서 아무것도 하기싫으네 ㅋㅋ 잠이 쏟아진다.",
          "original_form": "오늘 너무 피곤해서 아무것도 하기싫으네 ㅋㅋ 잠이 쏟아진다.",
          "speaker_id": "1",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100016847.1.23",
          "form": "아 언니네요 ㅋㅋㅋㅋㅋㅋㅋ친구인줄 ㅋㅋㅋㅋ",
          "original_form": "아 언니네요 ㅋㅋㅋㅋㅋㅋㅋ친구인줄 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100016847.1.24",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100016847.1.25",
          "form": "이거 윽시 헷갈리재?",
          "original_form": "이거 윽시 헷갈리재?",
          "speaker_id": "2",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100016847.1.26",
          "form": "오늘 니도 피곤한가보군ㅋ 우리오늘 진도가 많이안나갔네ㅋㅋ 육체피로로 인해ㅋㅋ",
          "original_form": "오늘 니도 피곤한가보군ㅋ 우리오늘 진도가 많이안나갔네ㅋㅋ 육체피로로 인해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100016847.1.27",
          "form": "ㅋㅋㅋㅋ얍얍ㅋㅋㅋ언니몸은 괜찮아요?",
          "original_form": "ㅋㅋㅋㅋ얍얍ㅋㅋㅋ언니몸은 괜찮아요?",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100016847.1.28",
          "form": "어ㅋ 하루지나니 괜찮네ㅋㅋ",
          "original_form": "어ㅋ 하루지나니 괜찮네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 07:17"
        },
        {
          "id": "MDRW2100016847.1.29",
          "form": "아침부터 난리부르스라 ㅜ울신랑출근하고",
          "original_form": "아침부터 난리부르스라 ㅜ울신랑출근하고",
          "speaker_id": "2",
          "time": "20210828 07:18"
        },
        {
          "id": "MDRW2100016847.1.30",
          "form": "ㅋㅋㅋ 다행이네요.ㅋㅋ 저는 콩나물밥했어요.ㅋㅋ",
          "original_form": "ㅋㅋㅋ 다행이네요.ㅋㅋ 저는 콩나물밥했어요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:05"
        }
      ]
