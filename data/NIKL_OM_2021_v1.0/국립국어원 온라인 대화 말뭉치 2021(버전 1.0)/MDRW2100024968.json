      "utterance": [
        {
          "id": "MDRW2100024968.1.1",
          "form": "나 프로틴 샀어",
          "original_form": "나 프로틴 샀어",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024968.1.2",
          "form": "아이스라떼맛",
          "original_form": "아이스라떼맛",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024968.1.3",
          "form": "지금 개봉한다",
          "original_form": "지금 개봉한다",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024968.1.4",
          "form": "대박 뭐라고?",
          "original_form": "대박 뭐라고?",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024968.1.5",
          "form": "그런 맛도 있다니 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런 맛도 있다니 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024968.1.6",
          "form": "어느 브랜드야?",
          "original_form": "어느 브랜드야?",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024968.1.7",
          "form": "마이프로틴이라는 해외브랜드?",
          "original_form": "마이프로틴이라는 해외브랜드?",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024968.1.8",
          "form": "맛이 엄청 많음",
          "original_form": "맛이 엄청 많음",
          "speaker_id": "2",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100024968.1.9",
          "form": "초콜렛 맛도 5~6개 있더라",
          "original_form": "초콜렛 맛도 5~6개 있더라",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024968.1.10",
          "form": "아니무슨일이야 맛이 왤케많아 ㅋㅋㅋㅋㅋ",
          "original_form": "아니무슨일이야 맛이 왤케많아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100024968.1.11",
          "form": "뭐랑 섞어마셔?",
          "original_form": "뭐랑 섞어마셔?",
          "speaker_id": "1",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100024968.1.12",
          "form": "ㅋㅋㅋ난 우유랑 마시려고",
          "original_form": "ㅋㅋㅋ난 우유랑 마시려고",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024968.1.13",
          "form": "물에 타먹는건 좀 무섭다",
          "original_form": "물에 타먹는건 좀 무섭다",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024968.1.14",
          "form": "아아 원래 물에도 먹는거야?",
          "original_form": "아아 원래 물에도 먹는거야?",
          "speaker_id": "1",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024968.1.15",
          "form": "원래 물에 타먹는 거일걸",
          "original_form": "원래 물에 타먹는 거일걸",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024968.1.16",
          "form": "근데 물에 타면 좀 밍밍한 믹스커피 맛이라고 해서",
          "original_form": "근데 물에 타면 좀 밍밍한 믹스커피 맛이라고 해서",
          "speaker_id": "2",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024968.1.17",
          "form": "아하 그럼 저지방우유에 타면 딱이내",
          "original_form": "아하 그럼 저지방우유에 타면 딱이내",
          "speaker_id": "1",
          "time": "20210831 21:37"
        },
        {
          "id": "MDRW2100024968.1.18",
          "form": "오 맛이 살짝 애매하면서",
          "original_form": "오 맛이 살짝 애매하면서",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024968.1.19",
          "form": "괜찮네 ㅋㅋㅋ",
          "original_form": "괜찮네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024968.1.20",
          "form": "오오 한번에 몇미리 마셔?",
          "original_form": "오오 한번에 몇미리 마셔?",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024968.1.21",
          "form": "근성장 간다...",
          "original_form": "근성장 간다...",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024968.1.22",
          "form": "25g 이 한스푼인데 우유 이번에 250정도 부어봄",
          "original_form": "25g 이 한스푼인데 우유 이번에 250정도 부어봄",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100024968.1.23",
          "form": "오 그렇군",
          "original_form": "오 그렇군",
          "speaker_id": "1",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100024968.1.24",
          "form": "그거를 한끼 대용으로??",
          "original_form": "그거를 한끼 대용으로??",
          "speaker_id": "1",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100024968.1.25",
          "form": "그건 아닌것 같고",
          "original_form": "그건 아닌것 같고",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024968.1.26",
          "form": "운동 끝나고 먹으라고 해서",
          "original_form": "운동 끝나고 먹으라고 해서",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024968.1.27",
          "form": "먹는 즁",
          "original_form": "먹는 즁",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024968.1.28",
          "form": "아하 그럼 진짜 단백질 보충너낌인가??",
          "original_form": "아하 그럼 진짜 단백질 보충너낌인가??",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100024968.1.29",
          "form": "예쓰 운동 끝나고 닭가슴살 먹기는 싫어서",
          "original_form": "예쓰 운동 끝나고 닭가슴살 먹기는 싫어서",
          "speaker_id": "2",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100024968.1.30",
          "form": "샀어 ㅋㅋ",
          "original_form": "샀어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:43"
        }
      ]
    }
  ]
