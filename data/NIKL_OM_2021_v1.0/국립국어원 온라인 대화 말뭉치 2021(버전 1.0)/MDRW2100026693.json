        {
          "id": "MDRW2100026693.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026693.1.3",
          "form": "엄청난 스트레스라던데ㅜㅠ",
          "original_form": "엄청난 스트레스라던데ㅜㅠ",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026693.1.4",
          "form": "극악무도하잖아요",
          "original_form": "극악무도하잖아요",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026693.1.5",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026693.1.6",
          "form": "저 화실기 C+이었어요",
          "original_form": "저 화실기 C+이었어요",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026693.1.7",
          "form": "근데 교수님이 피드백을 하나도 안해주셔서",
          "original_form": "근데 교수님이 피드백을 하나도 안해주셔서",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026693.1.8",
          "form": "그냥... 맨날 바닥 점수만 받고...ㅠ",
          "original_form": "그냥... 맨날 바닥 점수만 받고...ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026693.1.9",
          "form": "혹시name3 교수님...?",
          "original_form": "혹시{emoji:…}&name3& 교수님...?",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026693.1.10",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026693.1.11",
          "form": "딱 아시네요...",
          "original_form": "딱 아시네요...",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026693.1.12",
          "form": "제가 무기화학1을 name3수님한테 들었을 때",
          "original_form": "제가 무기화학1을 &name3&수님한테 들었을 때",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026693.1.13",
          "form": "A+ 받아서 교수님이 성적 잘 주시는 줄 알았어요ㅠㅋㅋㅋ",
          "original_form": "A+ 받아서 교수님이 성적 잘 주시는 줄 알았어요ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026693.1.14",
          "form": "그건 그냥 언니가 잘했던것이에요",
          "original_form": "그건 그냥 언니가 잘했던것이에요",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026693.1.15",
          "form": "장미칼로 유명하시던데",
          "original_form": "장미칼로 유명하시던데",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026693.1.16",
          "form": "진짜 화실기가 어렵나봐요...",
          "original_form": "진짜 화실기가 어렵나봐요...",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026693.1.17",
          "form": "진짜 몰랐어욬ㅋㅋㅋㅋㅋ 하....",
          "original_form": "진짜 몰랐어욬ㅋㅋㅋㅋㅋ 하....",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100026693.1.18",
          "form": "두렵네요 듣지말까ㅋㅋㅋㅋ",
          "original_form": "두렵네요 듣지말까ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026693.1.19",
          "form": "성적망하면 안되는데....",
          "original_form": "성적망하면 안되는데....",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026693.1.20",
          "form": "사실 저는 비추천합니다...",
          "original_form": "사실 저는 비추천합니다...",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026693.1.21",
          "form": "원래 화실기랑 기기분석 둘 다",
          "original_form": "원래 화실기랑 기기분석 둘 다",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026693.1.22",
          "form": "취업할 때 어필하기 좋은 과목인데",
          "original_form": "취업할 때 어필하기 좋은 과목인데",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026693.1.23",
          "form": "어디까지나 성적이 좋을 때 이야기예요...",
          "original_form": "어디까지나 성적이 좋을 때 이야기예요...",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026693.1.24",
          "form": "기기분석 들으면 화실기는 굳이들을필요 없을까요...",
          "original_form": "기기분석 들으면 화실기는 굳이들을필요 없을까요...",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100026693.1.25",
          "form": "이론이랑 실습차이긴한데...",
