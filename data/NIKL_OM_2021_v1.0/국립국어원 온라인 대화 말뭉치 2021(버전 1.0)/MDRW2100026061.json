      "utterance": [
        {
          "id": "MDRW2100026061.1.1",
          "form": "나 요즘 name3 간식 많이 안줘",
          "original_form": "나 요즘 &name3& 간식 많이 안줘",
          "speaker_id": "1",
          "time": "20210901 19:27"
        },
        {
          "id": "MDRW2100026061.1.2",
          "form": "그렇게 하는게 맞는거야",
          "original_form": "그렇게 하는게 맞는거야",
          "speaker_id": "2",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100026061.1.3",
          "form": "요즘 name3 살이 넘 많이 쪘어 ㅠ",
          "original_form": "요즘 &name3& 살이 넘 많이 쪘어 ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100026061.1.4",
          "form": "동물병원 의사가 3키로 초반이 제일 적당하댔어",
          "original_form": "동물병원 의사가 3키로 초반이 제일 적당하댔어",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026061.1.5",
          "form": "우리 name4는 몇키로였을까?",
          "original_form": "우리 &name4&는 몇키로였을까?",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100026061.1.6",
          "form": "글쎄 name4도 육개월지나선 그정도 한거 같아",
          "original_form": "글쎄 &name4&도 육개월지나선 그정도 한거 같아",
          "speaker_id": "2",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026061.1.7",
          "form": "오래도록 같이 살아야하니 건강관리를 해줘야해",
          "original_form": "오래도록 같이 살아야하니 건강관리를 해줘야해",
          "speaker_id": "2",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026061.1.8",
          "form": "나 name4는 기억이 하나도 안 나 name4도 간식 사먹였나?",
          "original_form": "나 &name4&는 기억이 하나도 안 나 &name4&도 간식 사먹였나?",
          "speaker_id": "1",
          "time": "20210901 19:30"
        },
        {
          "id": "MDRW2100026061.1.9",
          "form": "name4 사료도 비싼거먹이고 캔도 주고 그랬지 마당에서 안좋은것먀 먹지않았음 그렇게 빨리 안갔는데",
          "original_form": "&name4& 사료도 비싼거먹이고 캔도 주고 그랬지 마당에서 안좋은것먀 먹지않았음 그렇게 빨리 안갔는데",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026061.1.10",
          "form": "name4 참치캔 먹였었어?",
          "original_form": "&name4& 참치캔 먹였었어?",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100026061.1.11",
          "form": "참친진 기억이 안나",
          "original_form": "참친진 기억이 안나",
          "speaker_id": "2",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026061.1.12",
          "form": "name3는 건강하게 잘키우자",
          "original_form": "&name3&는 건강하게 잘키우자",
          "speaker_id": "2",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026061.1.13",
          "form": "반려동물이 있으니 집안이 활기차고 좋아",
          "original_form": "반려동물이 있으니 집안이 활기차고 좋아",
          "speaker_id": "2",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100026061.1.14",
          "form": "윈윈하는게 반려동물과 함께 사는것같아",
          "original_form": "윈윈하는게 반려동물과 함께 사는것같아",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100026061.1.15",
          "form": "우리 강아지는 백살까지 살거야",
          "original_form": "우리 강아지는 백살까지 살거야",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026061.1.16",
          "form": "그럼 너무 좋지요",
          "original_form": "그럼 너무 좋지요",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026061.1.17",
          "form": "개껌이 나쁜말이야?",
          "original_form": "개껌이 나쁜말이야?",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026061.1.18",
          "form": "아닐걸\r\n개껌 만들어야하는데",
          "original_form": "아닐걸\r\n개껌 만들어야하는데",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026061.1.19",
          "form": "뭘로 만들거야?",
          "original_form": "뭘로 만들거야?",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026061.1.20",
          "form": "우유랑 브로컬리 당근 글구 젤라틴으로 아주맛나게 맹글어야지",
          "original_form": "우유랑 브로컬리 당근 글구 젤라틴으로 아주맛나게 맹글어야지",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100026061.1.21",
          "form": "강아지 양치 시켜주라고",
          "original_form": "강아지 양치 시켜주라고",
          "speaker_id": "1",
          "time": "20210904 14:15"
        }
      ]
