      "utterance": [
        {
          "id": "MDRW2100017770.1.1",
          "form": "치킨 먹고싶어요",
          "original_form": "치킨 먹고싶어요",
          "speaker_id": "1",
          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017770.1.2",
          "form": "어떤 치킨을 좋아하세요?",
          "original_form": "어떤 치킨을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017770.1.3",
          "form": "블랙알리오나 양념많은 치킨좋아해요",
          "original_form": "블랙알리오나 양념많은 치킨좋아해요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017770.1.4",
          "form": "그럼 푸라닭 좋아하겠네요",
          "original_form": "그럼 푸라닭 좋아하겠네요",
          "speaker_id": "2",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017770.1.5",
          "form": "전 당연히 모든 치킨을 좋아하는데 부모님 취향대로 시켜요",
          "original_form": "전 당연히 모든 치킨을 좋아하는데 부모님 취향대로 시켜요",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017770.1.6",
          "form": "후라이드요 ㅋㅋㅋㅋㅋ",
          "original_form": "후라이드요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017770.1.7",
          "form": "맞아요 푸라닭좋아해요",
          "original_form": "맞아요 푸라닭좋아해요",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017770.1.8",
          "form": "후라이드 좋죠",
          "original_form": "후라이드 좋죠",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017770.1.9",
          "form": "딱 1인 기준이라던데 먹어 본 적은 없어요",
          "original_form": "딱 1인 기준이라던데 먹어 본 적은 없어요",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017770.1.10",
          "form": "주위에선 다 맛있다하긴 했어요",
          "original_form": "주위에선 다 맛있다하긴 했어요",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017770.1.11",
          "form": "요즘 치킨 브랜드가 많아서 선택 하기 힘들어요",
          "original_form": "요즘 치킨 브랜드가 많아서 선택 하기 힘들어요",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100017770.1.12",
          "form": "후라이드는 비비큐?",
          "original_form": "후라이드는 비비큐?",
          "speaker_id": "1",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100017770.1.13",
          "form": "네 원래 네네 시켰는데 이사하고 가까운데 시키고 있어요",
          "original_form": "네 원래 네네 시켰는데 이사하고 가까운데 시키고 있어요",
          "speaker_id": "2",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100017770.1.14",
          "form": "네네 맛있죠",
          "original_form": "네네 맛있죠",
          "speaker_id": "1",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100017770.1.15",
          "form": "치맥 좋아하나요? 치콜 좋아하나요?",
          "original_form": "치맥 좋아하나요? 치콜 좋아하나요?",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100017770.1.16",
          "form": "때때로 다른데 요즘은 치콜이요ㅋㅋ",
          "original_form": "때때로 다른데 요즘은 치콜이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100017770.1.17",
          "form": "치킨 가격이나 낮춰졌으면 좋겠어요 너무 비싸요",
          "original_form": "치킨 가격이나 낮춰졌으면 좋겠어요 너무 비싸요",
          "speaker_id": "2",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100017770.1.18",
          "form": "맞아요ㅜㅜ 배달비 붙으면 이만원 훌쩍이니.\n .",
          "original_form": "맞아요ㅜㅜ 배달비 붙으면 이만원 훌쩍이니.\n .",
          "speaker_id": "1",
          "time": "20210827 15:06"
        }
      ]
