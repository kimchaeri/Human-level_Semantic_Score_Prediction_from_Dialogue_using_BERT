      "utterance": [
        {
          "id": "MDRW2100001819.1.1",
          "form": "name2님 중고거래하세요?",
          "original_form": "&name2&님 중고거래하세요?",
          "speaker_id": "1",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001819.1.2",
          "form": "중고는 안해요",
          "original_form": "중고는 안해요",
          "speaker_id": "2",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001819.1.3",
          "form": "그냥 나눔하면했지 팔지를 못하겠어요",
          "original_form": "그냥 나눔하면했지 팔지를 못하겠어요",
          "speaker_id": "2",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001819.1.4",
          "form": "저도 그냥 기프티콘은 판매하는데  중고는 거래못하겠더라구요",
          "original_form": "저도 그냥 기프티콘은 판매하는데  중고는 거래못하겠더라구요",
          "speaker_id": "1",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100001819.1.5",
          "form": "저도요. 요즘엔 기프티콘도 다먹어서",
          "original_form": "저도요. 요즘엔 기프티콘도 다먹어서",
          "speaker_id": "2",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100001819.1.6",
          "form": "저는 스벅만 간간히 팔아요 중고나라는 요즘 안하고 당근을 주로 애용해요",
          "original_form": "저는 스벅만 간간히 팔아요 중고나라는 요즘 안하고 당근을 주로 애용해요",
          "speaker_id": "1",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001819.1.7",
          "form": "전 당근안해봤어요. 다들 안좋은것만 올려서 당근하면 진상만 생각나요",
          "original_form": "전 당근안해봤어요. 다들 안좋은것만 올려서 당근하면 진상만 생각나요",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001819.1.8",
          "form": "한번도 안해보셨어요? 전 아직 이상한사람은 못만나봤어요",
          "original_form": "한번도 안해보셨어요? 전 아직 이상한사람은 못만나봤어요",
          "speaker_id": "1",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001819.1.9",
          "form": "ㅎ 팔것도없고 거래를 못하겠어요",
          "original_form": "ㅎ 팔것도없고 거래를 못하겠어요",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001819.1.10",
          "form": "얘기하는도중 투썸 아메 사겠다고 연락왔어요ㅎ",
          "original_form": "얘기하는도중 투썸 아메 사겠다고 연락왔어요ㅎ",
          "speaker_id": "1",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001819.1.11",
          "form": "오~~~얼렁 팔고오세요",
          "original_form": "오~~~얼렁 팔고오세요",
          "speaker_id": "2",
          "time": "20210819 21:44"
        },
        {
          "id": "MDRW2100001819.1.12",
          "form": "방금 팔고왔어요ㅎ 어제 위메프에서 산거 후딱 팔았네요",
          "original_form": "방금 팔고왔어요ㅎ 어제 위메프에서 산거 후딱 팔았네요",
          "speaker_id": "1",
          "time": "20210819 21:45"
        },
        {
          "id": "MDRW2100001819.1.13",
          "form": "빠르시네요^^",
          "original_form": "빠르시네요^^",
          "speaker_id": "2",
          "time": "20210819 21:46"
        },
        {
          "id": "MDRW2100001819.1.14",
          "form": "입금을 엄청 초고속으로 해주길래 바로 알림보고 보내줬어요",
          "original_form": "입금을 엄청 초고속으로 해주길래 바로 알림보고 보내줬어요",
          "speaker_id": "1",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001819.1.15",
          "form": "쿨거래 좋네요",
          "original_form": "쿨거래 좋네요",
          "speaker_id": "2",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001819.1.16",
          "form": "이런분만있음 거래할맛 나겠어요",
          "original_form": "이런분만있음 거래할맛 나겠어요",
          "speaker_id": "2",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001819.1.17",
          "form": "그러게요 기프티스타 이런데 가끔 본인이쓰고 신고하는데 당근은 아직 그런분 못봤어요",
          "original_form": "그러게요 기프티스타 이런데 가끔 본인이쓰고 신고하는데 당근은 아직 그런분 못봤어요",
          "speaker_id": "1",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001819.1.18",
          "form": "진짜 왜 그러고사나 모르겠어요",
          "original_form": "진짜 왜 그러고사나 모르겠어요",
          "speaker_id": "2",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001819.1.19",
          "form": "그니까요 양심도 없는건지 이해불가요",
