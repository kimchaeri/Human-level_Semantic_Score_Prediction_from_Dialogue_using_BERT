      "utterance": [
        {
          "id": "MDRW2100018429.1.1",
          "form": "네네 하나도 안했어요..ㅋㅋ",
          "original_form": "네네 하나도 안했어요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018429.1.2",
          "form": "안타깝네요 참..",
          "original_form": "안타깝네요 참..",
          "speaker_id": "1",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018429.1.3",
          "form": "물론 저도 대학생활 대면으로는",
          "original_form": "물론 저도 대학생활 대면으로는",
          "speaker_id": "1",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018429.1.4",
          "form": "1년밖에 안해봤는데",
          "original_form": "1년밖에 안해봤는데",
          "speaker_id": "1",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018429.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018429.1.6",
          "form": "ㅋㅋㅋㅋㅋㅠ 3학년이시군요!",
          "original_form": "ㅋㅋㅋㅋㅋㅠ 3학년이시군요!",
          "speaker_id": "2",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018429.1.7",
          "form": "그 일년은 어떠셨나요 재밌으셨나요",
          "original_form": "그 일년은 어떠셨나요 재밌으셨나요",
          "speaker_id": "2",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018429.1.8",
          "form": "ㅋㅋㅋㅋ 어떻게 아셨죠",
          "original_form": "ㅋㅋㅋㅋ 어떻게 아셨죠",
          "speaker_id": "1",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018429.1.9",
          "form": "지금에 비하면",
          "original_form": "지금에 비하면",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018429.1.10",
          "form": "나름대로 재밌게 보냈었던거 같아요",
          "original_form": "나름대로 재밌게 보냈었던거 같아요",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018429.1.11",
          "form": "오오..",
          "original_form": "오오..",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018429.1.12",
          "form": "거의 모든 술자리와",
          "original_form": "거의 모든 술자리와",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018429.1.13",
          "form": "동아리활동",
          "original_form": "동아리활동",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018429.1.14",
          "form": "전체모임 그런건",
          "original_form": "전체모임 그런건",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018429.1.15",
          "form": "다 참석했었어서 ㅋㅋ",
          "original_form": "다 참석했었어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018429.1.16",
          "form": "우와ㅋㅋㅋ 재밌을것같아요",
          "original_form": "우와ㅋㅋㅋ 재밌을것같아요",
          "speaker_id": "2",
          "time": "20210827 01:43"
        },
        {
          "id": "MDRW2100018429.1.17",
          "form": "동아리는 뭐하셨어요?",
          "original_form": "동아리는 뭐하셨어요?",
          "speaker_id": "2",
          "time": "20210827 01:43"
        },
        {
          "id": "MDRW2100018429.1.18",
          "form": "저는 봉사동아리랑",
          "original_form": "저는 봉사동아리랑",
          "speaker_id": "1",
          "time": "20210827 01:43"
        },
        {
          "id": "MDRW2100018429.1.19",
          "form": "농구동아리 했었어요",
          "original_form": "농구동아리 했었어요",
          "speaker_id": "1",
          "time": "20210827 01:43"
        },
        {
          "id": "MDRW2100018429.1.20",
          "form": "오 저도 운동하는? 동아리 들어가고 싶어요",
          "original_form": "오 저도 운동하는? 동아리 들어가고 싶어요",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018429.1.21",
          "form": "운동 어떤거 좋아하시나요",
          "original_form": "운동 어떤거 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018429.1.22",
          "form": "재밌을 것 같아요ㅋㅋ",
          "original_form": "재밌을 것 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018429.1.23",
          "form": "음 저는 배드민턴??",
          "original_form": "음 저는 배드민턴??",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018429.1.24",
          "form": "근데 배울수있으면 축구 이런것도 배우거 싶어요ㅋㅋ",
          "original_form": "근데 배울수있으면 축구 이런것도 배우거 싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018429.1.25",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018429.1.26",
          "form": "배드민턴 동아리는",
          "original_form": "배드민턴 동아리는",
          "speaker_id": "1",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018429.1.27",
          "form": "정말 많았던거같아요",
          "original_form": "정말 많았던거같아요",
          "speaker_id": "1",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018429.1.28",
          "form": "그쵸 저희학교에도 있더라구요",
          "original_form": "그쵸 저희학교에도 있더라구요",
          "speaker_id": "2",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018429.1.29",
          "form": "저희는 교양중에 배드민턴기초라는 과목도 있어서",
          "original_form": "저희는 교양중에 배드민턴기초라는 과목도 있어서",
          "speaker_id": "1",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018429.1.30",
          "form": "배드민턴치는 교양이 있더라구여",
          "original_form": "배드민턴치는 교양이 있더라구여",
          "speaker_id": "1",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018429.1.31",
          "form": "우와 신기해요 ㅋㅋㅋ",
          "original_form": "우와 신기해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018429.1.32",
          "form": "잘찾아보면 운동관련 교양은",
          "original_form": "잘찾아보면 운동관련 교양은",
          "speaker_id": "1",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018429.1.33",
          "form": "모든 대학에 있을거에요 ㅋㅋㅋ",
          "original_form": "모든 대학에 있을거에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:46"
        }
      ]
    }
