      "utterance": [
        {
          "id": "MDRW2100003053.1.1",
          "form": "아니 안가봤는데",
          "original_form": "아니 안가봤는데",
          "speaker_id": "2",
          "time": "20210820 18:05"
        },
        {
          "id": "MDRW2100003053.1.2",
          "form": "이 축제가 빛축제인가봐",
          "original_form": "이 축제가 빛축제인가봐",
          "speaker_id": "1",
          "time": "20210820 18:06"
        },
        {
          "id": "MDRW2100003053.1.3",
          "form": "아 고신대축제처럼",
          "original_form": "아 고신대축제처럼",
          "speaker_id": "2",
          "time": "20210820 18:06"
        },
        {
          "id": "MDRW2100003053.1.4",
          "form": "맞아 포토존이 많아서 인생사진 찍기 좋음",
          "original_form": "맞아 포토존이 많아서 인생사진 찍기 좋음",
          "speaker_id": "1",
          "time": "20210820 18:07"
        },
        {
          "id": "MDRW2100003053.1.5",
          "form": "꾸미고 가야하는데 마스크때문에",
          "original_form": "꾸미고 가야하는데 마스크때문에",
          "speaker_id": "2",
          "time": "20210820 18:07"
        },
        {
          "id": "MDRW2100003053.1.6",
          "form": "그렇지 청도 핫플레이스란다",
          "original_form": "그렇지 청도 핫플레이스란다",
          "speaker_id": "1",
          "time": "20210820 18:07"
        },
        {
          "id": "MDRW2100003053.1.7",
          "form": "어디쯤이야?",
          "original_form": "어디쯤이야?",
          "speaker_id": "2",
          "time": "20210820 18:08"
        },
        {
          "id": "MDRW2100003053.1.8",
          "form": "남성현휴계소 근처야",
          "original_form": "남성현휴계소 근처야",
          "speaker_id": "1",
          "time": "20210820 18:09"
        },
        {
          "id": "MDRW2100003053.1.9",
          "form": "차가 있어야겠다 그렁",
          "original_form": "차가 있어야겠다 그렁",
          "speaker_id": "2",
          "time": "20210820 18:09"
        },
        {
          "id": "MDRW2100003053.1.10",
          "form": "가게 되면 같이 가자",
          "original_form": "가게 되면 같이 가자",
          "speaker_id": "1",
          "time": "20210820 18:10"
        },
        {
          "id": "MDRW2100003053.1.11",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210820 18:10"
        },
        {
          "id": "MDRW2100003053.1.12",
          "form": "소싸움경기장 건너편쪽이네",
          "original_form": "소싸움경기장 건너편쪽이네",
          "speaker_id": "1",
          "time": "20210820 18:10"
        },
        {
          "id": "MDRW2100003053.1.13",
          "form": "본전 있다",
          "original_form": "본전 있다",
          "speaker_id": "2",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100003053.1.14",
          "form": "포토존이 100개라는데 ㅋㅋ",
          "original_form": "포토존이 100개라는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100003053.1.15",
          "form": "포토존예서 다 찍겠어?",
          "original_form": "포토존예서 다 찍겠어?",
          "speaker_id": "2",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100003053.1.16",
          "form": "많으니 대기 이런건 없겠다",
          "original_form": "많으니 대기 이런건 없겠다",
          "speaker_id": "1",
          "time": "20210820 18:11"
        }
