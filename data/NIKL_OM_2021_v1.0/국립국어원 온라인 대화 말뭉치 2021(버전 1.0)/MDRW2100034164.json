      "utterance": [
        {
          "id": "MDRW2100034164.1.1",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100034164.1.2",
          "form": "저 지금 ㅜㅜ",
          "original_form": "저 지금 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100034164.1.3",
          "form": "불금이라",
          "original_form": "불금이라",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100034164.1.4",
          "form": "달리네요 ㅋㅋ",
          "original_form": "달리네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100034164.1.5",
          "form": "모듬전 시켰어요",
          "original_form": "모듬전 시켰어요",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100034164.1.6",
          "form": "저는 닭갈비 먹었어요ㅋㅋ",
          "original_form": "저는 닭갈비 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100034164.1.7",
          "form": "와 맛나겠어요 ㅎㅎ",
          "original_form": "와 맛나겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100034164.1.8",
          "form": "전 지금전왔어요 ㅎㅎ",
          "original_form": "전 지금전왔어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100034164.1.9",
          "form": "헉 맛이 너무 건강해요 ㅋㅋ",
          "original_form": "헉 맛이 너무 건강해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100034164.1.10",
          "form": "뭔 맛인지",
          "original_form": "뭔 맛인지",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100034164.1.11",
          "form": "저번에 다른솟은 맛났는데 ㅜㅜ",
          "original_form": "저번에 다른솟은 맛났는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100034164.1.12",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100034164.1.13",
          "form": "세상에.. 전이 건강하면 안되는데욧!!",
          "original_form": "세상에.. 전이 건강하면 안되는데욧!!",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100034164.1.14",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100034164.1.15",
          "form": "건강하면 흥이 안나요ㅠ",
          "original_form": "건강하면 흥이 안나요ㅠ",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100034164.1.16",
          "form": "소금을 안쳤네요",
          "original_form": "소금을 안쳤네요",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100034164.1.17",
          "form": "간장해도 소금으로 간해야 하는데",
          "original_form": "간장해도 소금으로 간해야 하는데",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100034164.1.18",
          "form": "불금",
          "original_form": "불금",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100034164.1.19",
          "form": "닭갈비 탁월한 선택",
          "original_form": "닭갈비 탁월한 선택",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100034164.1.20",
          "form": "입니다 ㅎㅎ",
          "original_form": "입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100034164.1.21",
          "form": "으윽 간이 다인데 말이죠 제가 다 아쉽네요ㅠ",
          "original_form": "으윽 간이 다인데 말이죠 제가 다 아쉽네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100034164.1.22",
          "form": "ㅎㅎ감사합니당",
          "original_form": "ㅎㅎ감사합니당",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100034164.1.23",
          "form": "친구들도 맛있다해줘서 기뻐용",
          "original_form": "친구들도 맛있다해줘서 기뻐용",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100034164.1.24",
          "form": "오 친구분도 맛났데요?",
          "original_form": "오 친구분도 맛났데요?",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100034164.1.25",
          "form": "좋은데요 역시",
          "original_form": "좋은데요 역시",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100034164.1.26",
          "form": "닭갈비 배신 안해요 ㅎㅎ",
          "original_form": "닭갈비 배신 안해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100034164.1.27",
          "form": "넹! 진짜 오랜만에 만나서 간거라 약간 떨렸는데 다행이였어오",
          "original_form": "넹! 진짜 오랜만에 만나서 간거라 약간 떨렸는데 다행이였어오",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100034164.1.28",
          "form": "맞아용 닭갈비 최고!",
          "original_form": "맞아용 닭갈비 최고!",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100034164.1.29",
          "form": "ㅎㅎ 가자고 했을때 맛나지 않음 당황해서 떨린데 맛났으니 다행이예요 ㅎㅎ",
          "original_form": "ㅎㅎ 가자고 했을때 맛나지 않음 당황해서 떨린데 맛났으니 다행이예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100034164.1.30",
          "form": "술도 했나요?",
          "original_form": "술도 했나요?",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100034164.1.31",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100034164.1.32",
          "form": "아쉽게도 안했어요ㅜㅠ",
          "original_form": "아쉽게도 안했어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100034164.1.33",
          "form": "전 술 잘못해서 ㅋㅋ 맥주로다가",
          "original_form": "전 술 잘못해서 ㅋㅋ 맥주로다가",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100034164.1.34",
          "form": "그래도 기쁜일이 있었어요!",
          "original_form": "그래도 기쁜일이 있었어요!",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100034164.1.35",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100034164.1.36",
          "form": "뭐요",
          "original_form": "뭐요",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100034164.1.37",
          "form": "맥주! 저도 맥주 마시고싶었는데ㅜㅡㅜ",
          "original_form": "맥주! 저도 맥주 마시고싶었는데ㅜㅡㅜ",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100034164.1.38",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100034164.1.39",
          "form": "닭갈비집 사장님이 저 알아보시고 음료수 서비스 주셨어요!",
          "original_form": "닭갈비집 사장님이 저 알아보시고 음료수 서비스 주셨어요!",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100034164.1.40",
          "form": "어머 어깨 뿜뿜 인데요",
          "original_form": "어머 어깨 뿜뿜 인데요",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100034164.1.41",
          "form": "진짜 센스 쟁이시다",
          "original_form": "진짜 센스 쟁이시다",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100034164.1.42",
          "form": "그래야 또가죠 ㅎㅎ",
          "original_form": "그래야 또가죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100034164.1.43",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100034164.1.44",
          "form": "넹 ㅎㅎ",
          "original_form": "넹 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100034164.1.45",
          "form": "맞아요ㅋㅋ 또 가야지! 싶었어용ㅋㅋ",
          "original_form": "맞아요ㅋㅋ 또 가야지! 싶었어용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:24"
        }
      ]
    }
