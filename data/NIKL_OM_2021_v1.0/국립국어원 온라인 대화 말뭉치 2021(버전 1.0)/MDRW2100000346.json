      "utterance": [
        {
          "id": "MDRW2100000346.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210819 15:27"
        },
        {
          "id": "MDRW2100000346.1.2",
          "form": "내다 내",
          "original_form": "내다 내",
          "speaker_id": "2",
          "time": "20210819 15:27"
        },
        {
          "id": "MDRW2100000346.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 15:28"
        },
        {
          "id": "MDRW2100000346.1.4",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:28"
        },
        {
          "id": "MDRW2100000346.1.5",
          "form": "나 요즘 살 많이 쪘다",
          "original_form": "나 요즘 살 많이 쪘다",
          "speaker_id": "1",
          "time": "20210819 15:29"
        },
        {
          "id": "MDRW2100000346.1.6",
          "form": "요요가 온건가",
          "original_form": "요요가 온건가",
          "speaker_id": "2",
          "time": "20210819 15:29"
        },
        {
          "id": "MDRW2100000346.1.7",
          "form": "백신땜에 약은 안먹지?",
          "original_form": "백신땜에 약은 안먹지?",
          "speaker_id": "2",
          "time": "20210819 15:29"
        },
        {
          "id": "MDRW2100000346.1.8",
          "form": "그건 아닌데",
          "original_form": "그건 아닌데",
          "speaker_id": "1",
          "time": "20210819 15:30"
        },
        {
          "id": "MDRW2100000346.1.9",
          "form": "내만하겠수?ㅋㅋㅋ",
          "original_form": "내만하겠수?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 15:30"
        },
        {
          "id": "MDRW2100000346.1.10",
          "form": "약삘이 떨어졌으ㅜ",
          "original_form": "약삘이 떨어졌으ㅜ",
          "speaker_id": "1",
          "time": "20210819 15:30"
        },
        {
          "id": "MDRW2100000346.1.11",
          "form": "ㅠㅠ유지약?",
          "original_form": "ㅠㅠ유지약?",
          "speaker_id": "2",
          "time": "20210819 15:30"
        },
        {
          "id": "MDRW2100000346.1.12",
          "form": "내성생겼나봐",
          "original_form": "내성생겼나봐",
          "speaker_id": "1",
          "time": "20210819 15:30"
        },
        {
          "id": "MDRW2100000346.1.13",
          "form": "아 ㅠ한계일수도이미",
          "original_form": "아 ㅠ한계일수도이미",
          "speaker_id": "2",
          "time": "20210819 15:31"
        },
        {
          "id": "MDRW2100000346.1.14",
          "form": "많이빠졌자나",
          "original_form": "많이빠졌자나",
          "speaker_id": "2",
          "time": "20210819 15:31"
        },
        {
          "id": "MDRW2100000346.1.15",
          "form": "백신주사 맞기 2주전엔 약 끊어야되는데",
          "original_form": "백신주사 맞기 2주전엔 약 끊어야되는데",
          "speaker_id": "1",
          "time": "20210819 15:31"
        },
        {
          "id": "MDRW2100000346.1.16",
          "form": "살찔까봐 무섭다ㅜㅜㅜ",
          "original_form": "살찔까봐 무섭다ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000346.1.17",
          "form": "살과의 전쟁",
          "original_form": "살과의 전쟁",
          "speaker_id": "2",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000346.1.18",
          "form": "언제까지 먹으려고?",
          "original_form": "언제까지 먹으려고?",
          "speaker_id": "2",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000346.1.19",
          "form": "우리 헬스나 좀 다녀보까",
          "original_form": "우리 헬스나 좀 다녀보까",
          "speaker_id": "1",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000346.1.20",
          "form": "2주전엔 끊어야지",
          "original_form": "2주전엔 끊어야지",
          "speaker_id": "1",
          "time": "20210819 15:32"
        },
        {
          "id": "MDRW2100000346.1.21",
          "form": "아니 맞고나서도",
          "original_form": "아니 맞고나서도",
          "speaker_id": "2",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000346.1.22",
          "form": "다시 먹으면",
          "original_form": "다시 먹으면",
          "speaker_id": "2",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000346.1.23",
          "form": "아 그냥그때까지?",
          "original_form": "아 그냥그때까지?",
          "speaker_id": "2",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000346.1.24",
          "form": "주사맞고도 2주쉬고 맞아야한대",
          "original_form": "주사맞고도 2주쉬고 맞아야한대",
          "speaker_id": "1",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000346.1.25",
          "form": "아니지 먹어야된대ㅋㅋ",
          "original_form": "아니지 먹어야된대ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000346.1.26",
          "form": "응 2차도있으니",
          "original_form": "응 2차도있으니",
          "speaker_id": "2",
          "time": "20210819 15:34"
        },
        {
          "id": "MDRW2100000346.1.27",
          "form": "그리고 연속으로 다이어트약 타먹어서",
          "original_form": "그리고 연속으로 다이어트약 타먹어서",
          "speaker_id": "1",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000346.1.28",
          "form": "조금 쉬어야한대",
          "original_form": "조금 쉬어야한대",
          "speaker_id": "1",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000346.1.29",
          "form": "그니까ㅜㅜ몸이",
          "original_form": "그니까ㅜㅜ몸이",
          "speaker_id": "2",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000346.1.30",
          "form": "무리가갈수있으니",
          "original_form": "무리가갈수있으니",
          "speaker_id": "2",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000346.1.31",
          "form": "나라에서 그니깐 규제를 하는거 같애",
          "original_form": "나라에서 그니깐 규제를 하는거 같애",
          "speaker_id": "1",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000346.1.32",
          "form": "아 그런것도있어?",
          "original_form": "아 그런것도있어?",
          "speaker_id": "2",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000346.1.33",
          "form": "응 그러니깐 3개월 뒤에 오라고 하네",
          "original_form": "응 그러니깐 3개월 뒤에 오라고 하네",
          "speaker_id": "1",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000346.1.34",
          "form": "아.\r\n뭐야그람 3갤뒤에찌믄 또 그거빼고ㅠㅠ",
          "original_form": "아.\r\n뭐야그람 3갤뒤에찌믄 또 그거빼고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000346.1.35",
          "form": "반복인가",
          "original_form": "반복인가",
          "speaker_id": "2",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000346.1.36",
          "form": "악순한이지ㅋ",
          "original_form": "악순한이지ㅋ",
          "speaker_id": "1",
          "time": "20210819 15:38"
        },
        {
          "id": "MDRW2100000346.1.37",
          "form": "ㅋㅋㅋ 이런",
          "original_form": "ㅋㅋㅋ 이런",
          "speaker_id": "2",
          "time": "20210819 15:38"
        }
      ]
    }
