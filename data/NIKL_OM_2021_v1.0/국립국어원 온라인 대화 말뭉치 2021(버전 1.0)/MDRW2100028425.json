      "utterance": [
        {
          "id": "MDRW2100028425.1.1",
          "form": "아이스티노를 아십니까",
          "original_form": "아이스티노를 아십니까",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100028425.1.2",
          "form": "저는 지금 마시고 있다",
          "original_form": "저는 지금 마시고 있다",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028425.1.3",
          "form": "정답! 아이스티+아메리카노",
          "original_form": "정답! 아이스티+아메리카노",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028425.1.4",
          "form": "맛있음??",
          "original_form": "맛있음??",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028425.1.5",
          "form": "첫인상: 불타버린 아이스티",
          "original_form": "첫인상: 불타버린 아이스티",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028425.1.6",
          "form": "근데 아이스티의 단맛과 아메리카노의 쓴맛이",
          "original_form": "근데 아이스티의 단맛과 아메리카노의 쓴맛이",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028425.1.7",
          "form": "묘한 조화를 이루어서",
          "original_form": "묘한 조화를 이루어서",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028425.1.8",
          "form": "완전히 다른 맛을 냄",
          "original_form": "완전히 다른 맛을 냄",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028425.1.9",
          "form": "정량화된 레시피가 없다는게 단점",
          "original_form": "정량화된 레시피가 없다는게 단점",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028425.1.10",
          "form": "오 ㄷㅂ",
          "original_form": "오 ㄷㅂ",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028425.1.11",
          "form": "그거 생각난다 우리 먹었던",
          "original_form": "그거 생각난다 우리 먹었던",
          "speaker_id": "2",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028425.1.12",
          "form": "그.................... 사이다커피?",
          "original_form": "그.................... 사이다커피?",
          "speaker_id": "2",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028425.1.13",
          "form": "근데 아이스티도 티백으로 우리는 아이스티 있는거 알아?",
          "original_form": "근데 아이스티도 티백으로 우리는 아이스티 있는거 알아?",
          "speaker_id": "2",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028425.1.14",
          "form": "그거 집에서 해먹으려니까",
          "original_form": "그거 집에서 해먹으려니까",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028425.1.15",
          "form": "진짜 맛없어서 그냥 괴식 됨",
          "original_form": "진짜 맛없어서 그냥 괴식 됨",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028425.1.16",
          "form": "그거 가루아이스티랑 걍차원이다른맛임",
          "original_form": "그거 가루아이스티랑 걍차원이다른맛임",
          "speaker_id": "2",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028425.1.17",
          "form": "아개웃겨",
          "original_form": "아개웃겨",
          "speaker_id": "2",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028425.1.18",
          "form": "헐 끌린다",
          "original_form": "헐 끌린다",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100028425.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
