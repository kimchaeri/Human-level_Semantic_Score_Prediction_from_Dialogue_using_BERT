      "utterance": [
        {
          "id": "MDRW2100002498.1.1",
          "form": "너 동물 길러본적 있어?",
          "original_form": "너 동물 길러본적 있어?",
          "speaker_id": "1",
          "time": "20210820 12:52"
        },
        {
          "id": "MDRW2100002498.1.2",
          "form": "지금도 기르고 있지",
          "original_form": "지금도 기르고 있지",
          "speaker_id": "2",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002498.1.3",
          "form": "오 뭐 기르고 있어?",
          "original_form": "오 뭐 기르고 있어?",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002498.1.4",
          "form": "고양이 기르고 있지",
          "original_form": "고양이 기르고 있지",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002498.1.5",
          "form": "키운지 한 3~4년됬어",
          "original_form": "키운지 한 3~4년됬어",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002498.1.6",
          "form": "무슨 종이야?",
          "original_form": "무슨 종이야?",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002498.1.7",
          "form": "삼색이라고 흔히부르는 종인데",
          "original_form": "삼색이라고 흔히부르는 종인데",
          "speaker_id": "2",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002498.1.8",
          "form": "자세한 종이름은 모름",
          "original_form": "자세한 종이름은 모름",
          "speaker_id": "2",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002498.1.9",
          "form": "아마 삼색코쇼?",
          "original_form": "아마 삼색코쇼?",
          "speaker_id": "2",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002498.1.10",
          "form": "아하 고양이 성격은 어때?",
          "original_form": "아하 고양이 성격은 어때?",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002498.1.11",
          "form": "장난기 많아?",
          "original_form": "장난기 많아?",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002498.1.12",
          "form": "엄청 예민하고 장난기 많음",
          "original_form": "엄청 예민하고 장난기 많음",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002498.1.13",
          "form": "그리고 잔머리가 엄청",
          "original_form": "그리고 잔머리가 엄청",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002498.1.14",
          "form": "높아서 간식먹고싶을때 엄마 일 방해하고 그럼",
          "original_form": "높아서 간식먹고싶을때 엄마 일 방해하고 그럼",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002498.1.15",
          "form": "햘퀴진 않아?",
          "original_form": "햘퀴진 않아?",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002498.1.16",
          "form": "거의 할퀴진 않음",
          "original_form": "거의 할퀴진 않음",
          "speaker_id": "2",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002498.1.17",
          "form": "가끔 장난도가 올라갈때",
          "original_form": "가끔 장난도가 올라갈때",
          "speaker_id": "2",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002498.1.18",
          "form": "할퀴는경우가 잇긴하지",
          "original_form": "할퀴는경우가 잇긴하지",
          "speaker_id": "2",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002498.1.19",
          "form": "아프고 흉터남을텐데 괜찮아?",
          "original_form": "아프고 흉터남을텐데 괜찮아?",
          "speaker_id": "1",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100002498.1.20",
          "form": "흉터는 금방 사라지고",
          "original_form": "흉터는 금방 사라지고",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002498.1.21",
          "form": "아픈것도 역시 금방없어져서",
          "original_form": "아픈것도 역시 금방없어져서",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002498.1.22",
          "form": "괜찮음",
          "original_form": "괜찮음",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002498.1.23",
          "form": "귀여운걸로 다 커버가 되버려서 허허",
          "original_form": "귀여운걸로 다 커버가 되버려서 허허",
          "speaker_id": "2",
          "time": "20210820 13:05"
        },
        {
          "id": "MDRW2100002498.1.24",
          "form": "귀여운게 최고긴해",
          "original_form": "귀여운게 최고긴해",
          "speaker_id": "1",
          "time": "20210820 13:05"
        }
      ]
