      "utterance": [
        {
          "id": "MDRW2100027083.1.1",
          "form": "아침은 챙겨먹는 편인가요?",
          "original_form": "아침은 챙겨먹는 편인가요?",
          "speaker_id": "1",
          "time": "20210902 09:17"
        },
        {
          "id": "MDRW2100027083.1.2",
          "form": "저는 아침챙겨먹어요",
          "original_form": "저는 아침챙겨먹어요",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027083.1.3",
          "form": "식단하는 중이라",
          "original_form": "식단하는 중이라",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027083.1.4",
          "form": "그렇군요.. 저희 식구들도 아침은 꼬박꼬박 먹죠..",
          "original_form": "그렇군요.. 저희 식구들도 아침은 꼬박꼬박 먹죠..",
          "speaker_id": "1",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100027083.1.5",
          "form": "그럼 점심은 사먹나요?",
          "original_form": "그럼 점심은 사먹나요?",
          "speaker_id": "1",
          "time": "20210902 18:09"
        },
        {
          "id": "MDRW2100027083.1.6",
          "form": "제가 보통 해먹어요!",
          "original_form": "제가 보통 해먹어요!",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100027083.1.7",
          "form": "제가 집에 있는 시간이 길어서",
          "original_form": "제가 집에 있는 시간이 길어서",
          "speaker_id": "2",
          "time": "20210902 18:55"
        },
        {
          "id": "MDRW2100027083.1.8",
          "form": "보통 해먹는편이에요",
          "original_form": "보통 해먹는편이에요",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100027083.1.9",
          "form": "나갈일있거나 해도 도시락싸서다니구요",
          "original_form": "나갈일있거나 해도 도시락싸서다니구요",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100027083.1.10",
          "form": "요리 좋아하는 군요! 저두요",
          "original_form": "요리 좋아하는 군요! 저두요",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100027083.1.11",
          "form": "외식보다는 집밥을 좋아하죠.. 주로 제가 요리하구요",
          "original_form": "외식보다는 집밥을 좋아하죠.. 주로 제가 요리하구요",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100027083.1.12",
          "form": "오오 저도 요리 좋아해요! 엄청 잘하지는 못하지만",
          "original_form": "오오 저도 요리 좋아해요! 엄청 잘하지는 못하지만",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100027083.1.13",
          "form": "손도 엄청 큰데 해놓고 다못먹으니까",
          "original_form": "손도 엄청 큰데 해놓고 다못먹으니까",
          "speaker_id": "2",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100027083.1.14",
          "form": "한번 요리할 때 주변에 나눠주가나 저장식품같은 걸 만들게 되더라구요",
          "original_form": "한번 요리할 때 주변에 나눠주가나 저장식품같은 걸 만들게 되더라구요",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100027083.1.15",
          "form": "그렇군요.. 저도 손이 큰 편인데..",
          "original_form": "그렇군요.. 저도 손이 큰 편인데..",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100027083.1.16",
          "form": "보통 손님 초대해서 해먹이는 거 좋아하는데",
          "original_form": "보통 손님 초대해서 해먹이는 거 좋아하는데",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100027083.1.17",
          "form": "작년부터 그걸 못하니 슬퍼요",
          "original_form": "작년부터 그걸 못하니 슬퍼요",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100027083.1.18",
          "form": "정말요 저도 만약 자취한다면",
          "original_form": "정말요 저도 만약 자취한다면",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100027083.1.19",
          "form": "손님초대해서 조촐한 홈파티하는게 꿈인데",
          "original_form": "손님초대해서 조촐한 홈파티하는게 꿈인데",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100027083.1.20",
          "form": "너무좋을것같아요",
          "original_form": "너무좋을것같아요",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100027083.1.21",
          "form": "사람 불러서 먹이는 그 재미가 장난아니죠..",
          "original_form": "사람 불러서 먹이는 그 재미가 장난아니죠..",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100027083.1.22",
          "form": "소소하게라도 모여서 교제하고 나눠먹는 그 재미",
          "original_form": "소소하게라도 모여서 교제하고 나눠먹는 그 재미",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100027083.1.23",
          "form": "지금 그걸 못하니 너무 아쉬워요",
          "original_form": "지금 그걸 못하니 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100027083.1.24",
          "form": "저두 술좋아해서",
          "original_form": "저두 술좋아해서",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100027083.1.25",
          "form": "좋아하는 사람들이랑 모여서 술마시는거좋아하는대",
          "original_form": "좋아하는 사람들이랑 모여서 술마시는거좋아하는대",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100027083.1.26",
          "form": "데",
          "original_form": "데",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100027083.1.27",
          "form": "요새 많이 못했죠...ㅍㅍ",
          "original_form": "요새 많이 못했죠...ㅍㅍ",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100027083.1.28",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100027083.1.29",
          "form": "코시국되고나서 담금주 만들었는데 아직도 못뜯었어요",
          "original_form": "코시국되고나서 담금주 만들었는데 아직도 못뜯었어요",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100027083.1.30",
          "form": "아이고.. 얼른 좋은 사람들과 나눠마셔야 할텐데요",
          "original_form": "아이고.. 얼른 좋은 사람들과 나눠마셔야 할텐데요",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100027083.1.31",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100027083.1.32",
          "form": "아휴",
          "original_form": "아휴",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100027083.1.33",
          "form": "저녁은 뭐드셨나요?",
          "original_form": "저녁은 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100027083.1.34",
          "form": "양념돼지갈비요",
          "original_form": "양념돼지갈비요",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100027083.1.35",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100027083.1.36",
          "form": "좋은거 드셨네요",
          "original_form": "좋은거 드셨네요",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100027083.1.37",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100027083.1.38",
          "form": "저는 냉동실에있던 미역국 꺼내서 다시 끓여먹었는디",
          "original_form": "저는 냉동실에있던 미역국 꺼내서 다시 끓여먹었는디",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100027083.1.39",
          "form": "미역국도 맛있죠",
          "original_form": "미역국도 맛있죠",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100027083.1.40",
          "form": "이번에 지인이 10키로를 보내주는 바람에 급작스레 구워먹게 됐답니다.",
          "original_form": "이번에 지인이 10키로를 보내주는 바람에 급작스레 구워먹게 됐답니다.",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100027083.1.41",
          "form": "와 좋은 사람을 주변에 두셨네요",
          "original_form": "와 좋은 사람을 주변에 두셨네요",
          "speaker_id": "2",
          "time": "20210903 00:26"
        },
        {
          "id": "MDRW2100027083.1.42",
          "form": "부러워요 고기선물해주는 지인",
          "original_form": "부러워요 고기선물해주는 지인",
