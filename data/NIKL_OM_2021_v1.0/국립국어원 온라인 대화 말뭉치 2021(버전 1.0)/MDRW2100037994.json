      "utterance": [
        {
          "id": "MDRW2100037994.1.1",
          "form": "연모라는 드라마도 사극이네",
          "original_form": "연모라는 드라마도 사극이네",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100037994.1.2",
          "form": "누구 나와요?",
          "original_form": "누구 나와요?",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037994.1.3",
          "form": "쌍둥이로 때어나 여아라는 이유만으로 버려진 여주",
          "original_form": "쌍둥이로 때어나 여아라는 이유만으로 버려진 여주",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037994.1.4",
          "form": "쌍둥이 하나는 남자일거고",
          "original_form": "쌍둥이 하나는 남자일거고",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037994.1.5",
          "form": "오라비 세손이 죽어서 남장을 통해 세자역할",
          "original_form": "오라비 세손이 죽어서 남장을 통해 세자역할",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037994.1.6",
          "form": "남장 왜 안 나오나 했네요",
          "original_form": "남장 왜 안 나오나 했네요",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037994.1.7",
          "form": "박은빈이 여주다",
          "original_form": "박은빈이 여주다",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037994.1.8",
          "form": "아 박은빈 괜찮은데 그럼 박은빈이 남장 하겠네요",
          "original_form": "아 박은빈 괜찮은데 그럼 박은빈이 남장 하겠네요",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037994.1.9",
          "form": "남주는 가수 로운이고",
          "original_form": "남주는 가수 로운이고",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037994.1.10",
          "form": "아 로운 난 얘 별로던데. 대개 밀어주네요",
          "original_form": "아 로운 난 얘 별로던데. 대개 밀어주네요",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037994.1.11",
          "form": "엉 박은빈이 남장하겠는데",
          "original_form": "엉 박은빈이 남장하겠는데",
          "speaker_id": "2",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037994.1.12",
          "form": "로운 때매 보기 싫어지네요ㅋ",
          "original_form": "로운 때매 보기 싫어지네요ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037994.1.13",
          "form": "ㅋㅋ연모 이 드라마는 만화가 원작",
          "original_form": "ㅋㅋ연모 이 드라마는 만화가 원작",
