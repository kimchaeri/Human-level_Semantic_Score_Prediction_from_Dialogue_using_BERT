      "utterance": [
        {
          "id": "MDRW2100042365.1.1",
          "form": "맞아요 버스커버스커 덕분에 아니 윈윈이죠",
          "original_form": "맞아요 버스커버스커 덕분에 아니 윈윈이죠",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042365.1.2",
          "form": "ㅎㅎ여수여행도 좋을 것 같아요",
          "original_form": "ㅎㅎ여수여행도 좋을 것 같아요",
          "speaker_id": "1",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042365.1.3",
          "form": "가신 김에 순천도 한번 방문해보세요",
          "original_form": "가신 김에 순천도 한번 방문해보세요",
          "speaker_id": "1",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042365.1.4",
          "form": "저는 순천만에서 보는 일몰이 참 멋있더라고요",
          "original_form": "저는 순천만에서 보는 일몰이 참 멋있더라고요",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042365.1.5",
          "form": "아 그렇대요! 여수 순천 이렇게...",
          "original_form": "아 그렇대요! 여수 순천 이렇게...",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042365.1.6",
          "form": "자가운전해야 전라도는 좋다해서요",
          "original_form": "자가운전해야 전라도는 좋다해서요",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042365.1.7",
          "form": "네 대중교통으로는 조금 불편할수도",
          "original_form": "네 대중교통으로는 조금 불편할수도",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042365.1.8",
          "form": "강릉이나 전주 이런데는 뚜벅이도 좋았어요ㅋㅋ",
          "original_form": "강릉이나 전주 이런데는 뚜벅이도 좋았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042365.1.9",
          "form": "네 강릉 전주 다 저도 뚜벅이로 다녔어요",
          "original_form": "네 강릉 전주 다 저도 뚜벅이로 다녔어요",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042365.1.10",
          "form": "가끔 택시 타기도 했고",
          "original_form": "가끔 택시 타기도 했고",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042365.1.11",
          "form": "먹거리가 참 좋았어요",
          "original_form": "먹거리가 참 좋았어요",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042365.1.12",
          "form": "네네 맛집도 많고",
          "original_form": "네네 맛집도 많고",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042365.1.13",
          "form": "코로나 좀 잠잠해지면 또 가고싶네요",
          "original_form": "코로나 좀 잠잠해지면 또 가고싶네요",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042365.1.14",
          "form": "강릉 전주",
          "original_form": "강릉 전주",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042365.1.15",
          "form": "대기가 힘들긴하지만요ㅋㅋ",
          "original_form": "대기가 힘들긴하지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042365.1.16",
          "form": "짬뽕순두부드셨나요?",
          "original_form": "짬뽕순두부드셨나요?",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042365.1.17",
          "form": "짬뽕순두부는 찾아보기만 하고",
          "original_form": "짬뽕순두부는 찾아보기만 하고",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042365.1.18",
          "form": "먹진 못했어요",
          "original_form": "먹진 못했어요",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042365.1.19",
          "form": "엄지네 포장마차 꼬막은 기다렸다 먹었네요",
          "original_form": "엄지네 포장마차 꼬막은 기다렸다 먹었네요",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042365.1.20",
          "form": "아 거기서 한시간인가... 기다렸어요...",
          "original_form": "아 거기서 한시간인가... 기다렸어요...",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042365.1.21",
          "form": "저도 한시간 넘게 기다린듯ㅠ",
          "original_form": "저도 한시간 넘게 기다린듯ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042365.1.22",
          "form": "너무나...ㅜㅜ 기다리다 시간 다...",
          "original_form": "너무나...ㅜㅜ 기다리다 시간 다...",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042365.1.23",
          "form": "맞아요 배고픈데 기다리려니ㅜ",
          "original_form": "맞아요 배고픈데 기다리려니ㅜ",
          "speaker_id": "1",
          "time": "20210906 22:03"
        }
      ]
