      "utterance": [
        {
          "id": "MDRW2100029908.1.1",
          "form": "책 좋아해요?",
          "original_form": "책 좋아해요?",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029908.1.2",
          "form": "어릴 땐 많이 읽었는데",
          "original_form": "어릴 땐 많이 읽었는데",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029908.1.3",
          "form": "요즘은 별로 못 봐요",
          "original_form": "요즘은 별로 못 봐요",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029908.1.4",
          "form": "사는게 바쁘죠",
          "original_form": "사는게 바쁘죠",
          "speaker_id": "1",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029908.1.5",
          "form": "그래도 틈나는 대로 항상 읽으려고 해요",
          "original_form": "그래도 틈나는 대로 항상 읽으려고 해요",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029908.1.6",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "1",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029908.1.7",
          "form": "아까 말씀하신 박완서 작가의 작품도 좋아해요",
          "original_form": "아까 말씀하신 박완서 작가의 작품도 좋아해요",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029908.1.8",
          "form": "어머 정말요 반갑네요",
          "original_form": "어머 정말요 반갑네요",
          "speaker_id": "1",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100029908.1.9",
          "form": "한말씀만하소서",
          "original_form": "한말씀만하소서",
          "speaker_id": "2",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100029908.1.10",
          "form": "이런 제목이었던 거 같은데",
          "original_form": "이런 제목이었던 거 같은데",
          "speaker_id": "2",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100029908.1.11",
          "form": "네네 수필집",
          "original_form": "네네 수필집",
          "speaker_id": "1",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100029908.1.12",
          "form": "에세이 잘 읽었어요",
          "original_form": "에세이 잘 읽었어요",
          "speaker_id": "2",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100029908.1.13",
          "form": "그렇게 표현을 할 수 있다는게 너무 신기해요",
          "original_form": "그렇게 표현을 할 수 있다는게 너무 신기해요",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029908.1.14",
          "form": "맞아요 작가분들의 표현력",
          "original_form": "맞아요 작가분들의 표현력",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029908.1.15",
          "form": "시각 신기해요",
          "original_form": "시각 신기해요",
          "speaker_id": "2",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029908.1.16",
          "form": "문장에서 따스함이 느껴지잖아요",
          "original_form": "문장에서 따스함이 느껴지잖아요",
          "speaker_id": "1",
          "time": "20210903 16:26"
