      "utterance": [
        {
          "id": "MDRW2100017268.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017268.1.2",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017268.1.3",
          "form": "안녕하세용 네네 좋아합니다 근데 못가봐서 유튜브로 랜선여행해요 ㅋㅋㅋㅋ",
          "original_form": "안녕하세용 네네 좋아합니다 근데 못가봐서 유튜브로 랜선여행해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017268.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017268.1.5",
          "form": "저두요즘에는 유튜브로 봐요ㅋㅋㅋㅋㅋ",
          "original_form": "저두요즘에는 유튜브로 봐요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017268.1.6",
          "form": "어디 가고싶은데 있으세요??",
          "original_form": "어디 가고싶은데 있으세요??",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017268.1.7",
          "form": "저는 스위스가 가보고싶어요 자연경관이 너무 예쁜게 보기만해도 힐링이거든요ㅠ",
          "original_form": "저는 스위스가 가보고싶어요 자연경관이 너무 예쁜게 보기만해도 힐링이거든요ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017268.1.8",
          "form": "아 맞아요 산너무 이쁘더라구요",
          "original_form": "아 맞아요 산너무 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017268.1.9",
          "form": "저는 일단 일본을 가보고싶어요",
          "original_form": "저는 일단 일본을 가보고싶어요",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017268.1.10",
          "form": "해외여행 한 번도 안 가봐서 가까운데 먼저 가보려구요ㅋㅋ",
          "original_form": "해외여행 한 번도 안 가봐서 가까운데 먼저 가보려구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017268.1.11",
          "form": "name2님은 가본데 있으세요??",
          "original_form": "&name2&님은 가본데 있으세요??",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017268.1.12",
          "form": "헐 저도 안가봤어요 저 여권도 없어요!ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐 저도 안가봤어요 저 여권도 없어요!ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017268.1.13",
          "form": "동지시네용",
          "original_form": "동지시네용",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100017268.1.14",
          "form": "오오ㅋㅋㅋㅋㅋㅋㅋ 그러게요",
          "original_form": "오오ㅋㅋㅋㅋㅋㅋㅋ 그러게요",
          "speaker_id": "1",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017268.1.15",
          "form": "저도 여권도 없어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 여권도 없어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100017268.1.16",
          "form": "국내여행은 어디 가본데 있으세요??",
          "original_form": "국내여행은 어디 가본데 있으세요??",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017268.1.17",
          "form": "아ㅋㅋㅋㅋ 저희는 코로나 끝나면 여권부터 만들어야 겠네요",
          "original_form": "아ㅋㅋㅋㅋ 저희는 코로나 끝나면 여권부터 만들어야 겠네요",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017268.1.18",
          "form": "아뇨 여유거 없었어서 여행다운 여행은 못해봤어요",
          "original_form": "아뇨 여유거 없었어서 여행다운 여행은 못해봤어요",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017268.1.19",
          "form": "기껏해야 그냥 나들이 정도로 잠깐 바람쐬는거?",
          "original_form": "기껏해야 그냥 나들이 정도로 잠깐 바람쐬는거?",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017268.1.20",
          "form": "집순이기도 하구요 ㅎㅎ",
          "original_form": "집순이기도 하구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017268.1.21",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017268.1.22",
          "form": "저는 바다 좋아해서 바다만 몇 번 가봤어요ㅎㅎ",
          "original_form": "저는 바다 좋아해서 바다만 몇 번 가봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017268.1.23",
          "form": "강릉이 바다가 이쁘더라구요",
          "original_form": "강릉이 바다가 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017268.1.24",
          "form": "바다 너무 좋아요 저는 물이 좋은가봐요ㅋㅋㅋ 회도 좋아요",
          "original_form": "바다 너무 좋아요 저는 물이 좋은가봐요ㅋㅋㅋ 회도 좋아요",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017268.1.25",
          "form": "바다 가시면 회 드시나용",
          "original_form": "바다 가시면 회 드시나용",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017268.1.26",
          "form": "회 진짜 너무 좋아하는데 돈이..ㅠㅠ",
          "original_form": "회 진짜 너무 좋아하는데 돈이..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017268.1.27",
          "form": "회는 잘 못 사먹어요ㅠㅠㅠ",
          "original_form": "회는 잘 못 사먹어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 18:44"
        },
        {
          "id": "MDRW2100017268.1.28",
          "form": "가족들이랑 갔을때만 먹는답니다ㅋㅋㅋㅋㅋ",
          "original_form": "가족들이랑 갔을때만 먹는답니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017268.1.29",
          "form": "아 ㅠㅠㅠㅠㅠㅠㅠㅠ 너무 비싸죠 맞아요 바닷가 근처는 진짜 너무 비싸요",
          "original_form": "아 ㅠㅠㅠㅠㅠㅠㅠㅠ 너무 비싸죠 맞아요 바닷가 근처는 진짜 너무 비싸요",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017268.1.30",
          "form": "바다 가면 수영도 하시나요?",
          "original_form": "바다 가면 수영도 하시나요?",
          "speaker_id": "2",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017268.1.31",
          "form": "아뇨  저는 발만 담궈요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨  저는 발만 담궈요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017268.1.32",
          "form": "바닷가는 모래 묻는게 싫어서",
          "original_form": "바닷가는 모래 묻는게 싫어서",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017268.1.33",
          "form": "잘 안 들어가요ㅋㅋㅋㅋㅋ",
          "original_form": "잘 안 들어가요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017268.1.34",
          "form": "name2님은 수영 좋아하세요?",
          "original_form": "&name2&님은 수영 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017268.1.35",
          "form": "아ㅋㅋㅋ 보는거만 좋아하시는군요 어 물을 좋아해요ㅋㅋㅋㅋ 수영을 못해요..ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ 보는거만 좋아하시는군요 어 물을 좋아해요ㅋㅋㅋㅋ 수영을 못해요..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017268.1.36",
          "form": "맥주병도 아니에요 뜰수가 없거든요 ㅠ",
          "original_form": "맥주병도 아니에요 뜰수가 없거든요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017268.1.37",
          "form": "엌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ\r\n저두 못하는것도 하나의 이유라..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ\r\n저두 못하는것도 하나의 이유라..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017268.1.38",
          "form": "아ㅋㅋㅋㅋㅋㅋ저는 그래서 배우고 싶어요",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ저는 그래서 배우고 싶어요",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017268.1.39",
          "form": "저두요ㅋㅋㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋㅋㅋ",
