      "utterance": [
        {
          "id": "MDRW2100044114.1.1",
          "form": "결혼은 하셨나요",
          "original_form": "결혼은 하셨나요",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044114.1.2",
          "form": "아니요 결혼하셨나요?",
          "original_form": "아니요 결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044114.1.3",
          "form": "네 했습니다",
          "original_form": "네 했습니다",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044114.1.4",
          "form": "그럼 연애중 이신가요",
          "original_form": "그럼 연애중 이신가요",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044114.1.5",
          "form": "아니요 쉬고있어요",
          "original_form": "아니요 쉬고있어요",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044114.1.6",
          "form": "운명의 상대를 만나셨군요",
          "original_form": "운명의 상대를 만나셨군요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044114.1.7",
          "form": "그렇겠죠",
          "original_form": "그렇겠죠",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044114.1.8",
          "form": "소개팅으로 만나셨어요?",
          "original_form": "소개팅으로 만나셨어요?",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044114.1.9",
          "form": "사촌오빠 결혼식에서 만났어요",
          "original_form": "사촌오빠 결혼식에서 만났어요",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044114.1.10",
          "form": "사촌 오빠 친구 거든요",
          "original_form": "사촌 오빠 친구 거든요",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044114.1.11",
          "form": "와 누가 먼저 맘에 들어하신거예요?",
          "original_form": "와 누가 먼저 맘에 들어하신거예요?",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044114.1.12",
          "form": "제가 먼저 오빠한테 소개 부탁했죠",
          "original_form": "제가 먼저 오빠한테 소개 부탁했죠",
          "speaker_id": "1",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044114.1.13",
          "form": "와 용기있으세요 상대분도 맘에 드신거군요",
          "original_form": "와 용기있으세요 상대분도 맘에 드신거군요",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044114.1.14",
          "form": "글쎄요",
          "original_form": "글쎄요",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044114.1.15",
          "form": "몇번만나고 결혼결심하셨어요?",
          "original_form": "몇번만나고 결혼결심하셨어요?",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044114.1.16",
          "form": "남편은 처음 맘에 든건 아닌것 같 더라고요",
          "original_form": "남편은 처음 맘에 든건 아닌것 같 더라고요",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044114.1.17",
          "form": "아 그럼어떤매력에 빠지신건가요",
          "original_form": "아 그럼어떤매력에 빠지신건가요",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044114.1.18",
          "form": "1년 사귀고 자연스럽게 결혼으로 진행 되었죠",
          "original_form": "1년 사귀고 자연스럽게 결혼으로 진행 되었죠",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044114.1.19",
          "form": "혹 이상형이 연예인중 있나요",
          "original_form": "혹 이상형이 연예인중 있나요",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044114.1.20",
          "form": "저는 박보검 송중기요",
          "original_form": "저는 박보검 송중기요",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044114.1.21",
          "form": "이상형은 이상형일뿐",
          "original_form": "이상형은 이상형일뿐",
          "speaker_id": "2",
