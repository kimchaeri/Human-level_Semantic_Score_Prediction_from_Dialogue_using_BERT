        {
          "id": "MDRW2100000627.1.2",
          "form": "반가워요~",
          "original_form": "반가워요~",
          "speaker_id": "2",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000627.1.3",
          "form": "반가워요 요즘 뭐 사고싶어요?",
          "original_form": "반가워요 요즘 뭐 사고싶어요?",
          "speaker_id": "1",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000627.1.4",
          "form": "가구 이쁜거 사서 방 꾸미고싶어요!!!!!",
          "original_form": "가구 이쁜거 사서 방 꾸미고싶어요!!!!!",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000627.1.5",
          "form": "저는 요즘 새로나온 갤럭시 워치4를 사고싶어요",
          "original_form": "저는 요즘 새로나온 갤럭시 워치4를 사고싶어요",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000627.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000627.1.7",
          "form": "저는 아이폰13나오면",
          "original_form": "저는 아이폰13나오면",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000627.1.8",
          "form": "아이폰13 살꺼예요",
          "original_form": "아이폰13 살꺼예요",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000627.1.9",
          "form": "언제 출시된데요?",
          "original_form": "언제 출시된데요?",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000627.1.10",
          "form": "저도 아이폰 고민 중이긴해요",
          "original_form": "저도 아이폰 고민 중이긴해요",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000627.1.11",
          "form": "올해 안에는 나온다고 하더라구요",
          "original_form": "올해 안에는 나온다고 하더라구요",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000627.1.12",
          "form": "한국은 10월이랬나..",
          "original_form": "한국은 10월이랬나..",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000627.1.13",
          "form": "보안은 아이폰이라더라고요",
          "original_form": "보안은 아이폰이라더라고요",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000627.1.14",
          "form": "고민이에요 AS냐 보안이냐",
          "original_form": "고민이에요 AS냐 보안이냐",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000627.1.15",
          "form": "크 마자요 진짜 근데 이쁜건 아이폰이고ㅠㅠ",
          "original_form": "크 마자요 진짜 근데 이쁜건 아이폰이고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000627.1.16",
          "form": "아이폰은 유독 액정이 잘나가더라고요",
          "original_form": "아이폰은 유독 액정이 잘나가더라고요",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000627.1.17",
          "form": "완전 덤벙대는데 사자마자 깨먹을것같아요ㅋㅋ",
          "original_form": "완전 덤벙대는데 사자마자 깨먹을것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:07"
        },
        {
          "id": "MDRW2100000627.1.18",
          "form": "그래도 아이폰 사면 애플워치까지 싹 살거예요",
          "original_form": "그래도 아이폰 사면 애플워치까지 싹 살거예요",
          "speaker_id": "2",
          "time": "20210819 18:08"
        },
        {
          "id": "MDRW2100000627.1.19",
          "form": "왜 아이폰 쓰는 친구들은 특히 여사친들 폰은 왜 액정이 깨져있을까요",
