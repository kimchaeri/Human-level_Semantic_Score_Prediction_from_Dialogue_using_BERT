      "utterance": [
        {
          "id": "MDRW2100030369.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030369.1.2",
          "form": "최근에 구매하신 굿즈는 뭐에요?",
          "original_form": "최근에 구매하신 굿즈는 뭐에요?",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100030369.1.3",
          "form": "어 뭐샀더라",
          "original_form": "어 뭐샀더라",
          "speaker_id": "2",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030369.1.4",
          "form": "모르는척 말건건데 췟... 그래서 머샀어",
          "original_form": "모르는척 말건건데 췟... 그래서 머샀어",
          "speaker_id": "1",
          "time": "20210904 00:24"
        },
        {
          "id": "MDRW2100030369.1.5",
          "form": "엽서 날아왓징",
          "original_form": "엽서 날아왓징",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030369.1.6",
          "form": "어떤거??",
          "original_form": "어떤거??",
          "speaker_id": "1",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030369.1.7",
          "form": "아까 카톡라이브에서 보신거 ㅋㅋㅋ",
          "original_form": "아까 카톡라이브에서 보신거 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030369.1.8",
          "form": "앜ㅋㅋㅋ 산거였어?",
          "original_form": "앜ㅋㅋㅋ 산거였어?",
          "speaker_id": "1",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100030369.1.9",
          "form": "엽서만 ㅋㅋ",
          "original_form": "엽서만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100030369.1.10",
          "form": "ㅇㅎ...",
          "original_form": "ㅇㅎ...",
          "speaker_id": "1",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100030369.1.11",
          "form": "또 살거 뭐 있어?",
          "original_form": "또 살거 뭐 있어?",
          "speaker_id": "1",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100030369.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100030369.1.13",
          "form": "왜 저한테",
          "original_form": "왜 저한테",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100030369.1.14",
          "form": "쇼핑리스트를 뜯어내세욬ㅋㅋㅋㅋ",
          "original_form": "쇼핑리스트를 뜯어내세욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030369.1.15",
          "form": "함 말해봐",
          "original_form": "함 말해봐",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030369.1.16",
          "form": "또 살거..?",
          "original_form": "또 살거..?",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030369.1.17",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030369.1.18",
          "form": "최근엔 닫고 살아서 말이얔ㅋ",
          "original_form": "최근엔 닫고 살아서 말이얔ㅋ",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030369.1.19",
          "form": "후회한건 잇는데",
          "original_form": "후회한건 잇는데",
          "speaker_id": "2",
          "time": "20210904 00:28"
        },
        {
          "id": "MDRW2100030369.1.20",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "1",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030369.1.21",
          "form": "이름언급되나(별로 하고 싶지 않았는데ㅋㅋ)",
          "original_form": "이름언급되나(별로 하고 싶지 않았는데ㅋㅋ)",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030369.1.22",
          "form": "팔각님 프박",
          "original_form": "팔각님 프박",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030369.1.23",
          "form": "리트윗으로 본거 몇개만 있는줄 알앗는데",
          "original_form": "리트윗으로 본거 몇개만 있는줄 알앗는데",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030369.1.24",
          "form": "그게 뭐더라",
          "original_form": "그게 뭐더라",
          "speaker_id": "1",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030369.1.25",
          "form": "끝나고보니 더 잇엇고..",
          "original_form": "끝나고보니 더 잇엇고..",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030369.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030369.1.27",
          "form": "이런거 안쳐준댔는데",
          "original_form": "이런거 안쳐준댔는데",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030369.1.28",
          "form": "그 또 다음날 보니 스급 단행본 일러 최근꺼 그려주신분인걸",
          "original_form": "그 또 다음날 보니 스급 단행본 일러 최근꺼 그려주신분인걸",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030369.1.29",
          "form": "늦게 알아챗고<ㅇ>",
          "original_form": "늦게 알아챗고<ㅇ>",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030369.1.30",
          "form": "팔각님 트위터에 쳐보면 나옵니당",
          "original_form": "팔각님 트위터에 쳐보면 나옵니당",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030369.1.31",
          "form": "ㅎ...  그렇겠죠",
          "original_form": "ㅎ...  그렇겠죠",
          "speaker_id": "1",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030369.1.32",
          "form": "중고거랜 잘 돼가시나요ㅋㅋ",
          "original_form": "중고거랜 잘 돼가시나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030369.1.33",
          "form": "안타깝게도 소식이 없...",
          "original_form": "안타깝게도 소식이 없...",
          "speaker_id": "1",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030369.1.34",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030369.1.35",
          "form": "저런..",
          "original_form": "저런..",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030369.1.36",
          "form": "뭉쳐서 팔만한건 없었니",
          "original_form": "뭉쳐서 팔만한건 없었니",
          "speaker_id": "2",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030369.1.37",
          "form": "그렇게 원치 않는걸 사게 하고 싶지 않아",
          "original_form": "그렇게 원치 않는걸 사게 하고 싶지 않아",
          "speaker_id": "1",
          "time": "20210904 00:43"
        }
      ]
    }
