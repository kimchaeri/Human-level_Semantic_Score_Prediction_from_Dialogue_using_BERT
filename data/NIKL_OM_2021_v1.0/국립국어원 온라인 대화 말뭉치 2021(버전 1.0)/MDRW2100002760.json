      "utterance": [
        {
          "id": "MDRW2100002760.1.1",
          "form": "영화봐요?",
          "original_form": "영화봐요?",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002760.1.2",
          "form": "영화 좋아하는데 요즘 극장가기 겁나서 못가고있어요",
          "original_form": "영화 좋아하는데 요즘 극장가기 겁나서 못가고있어요",
          "speaker_id": "2",
          "time": "20210820 15:23"
        },
        {
          "id": "MDRW2100002760.1.3",
          "form": "마지막으로 본게 기생충인거같아요 ㅎ",
          "original_form": "마지막으로 본게 기생충인거같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:23"
        },
        {
          "id": "MDRW2100002760.1.4",
          "form": "저도 그쭘",
          "original_form": "저도 그쭘",
          "speaker_id": "1",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002760.1.5",
          "form": "모가디슈 잼나다고하던데요 보셨나요?",
          "original_form": "모가디슈 잼나다고하던데요 보셨나요?",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002760.1.6",
          "form": "저도 들었어요",
          "original_form": "저도 들었어요",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002760.1.7",
          "form": "인질도 있고 싱크홀도 감동적이라고하고",
          "original_form": "인질도 있고 싱크홀도 감동적이라고하고",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002760.1.8",
          "form": "갑자기 보고싶은것들이 많이 나오는거같아요",
          "original_form": "갑자기 보고싶은것들이 많이 나오는거같아요",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002760.1.9",
          "form": "영화관 못 가서 슬퍼요",
          "original_form": "영화관 못 가서 슬퍼요",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002760.1.10",
          "form": "맞아요 그래도 가는사람들은 가시나봐요",
          "original_form": "맞아요 그래도 가는사람들은 가시나봐요",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002760.1.11",
          "form": "개봉을 하니",
          "original_form": "개봉을 하니",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002760.1.12",
          "form": "저는 찝찝해서 못가겠던데...\r\n나만 안가는게 맞는건지싶기도 하더라구요",
          "original_form": "저는 찝찝해서 못가겠던데...\r\n나만 안가는게 맞는건지싶기도 하더라구요",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002760.1.13",
          "form": "거리두기라도 불안",
          "original_form": "거리두기라도 불안",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002760.1.14",
          "form": "그렇다고 거리두기되는 샤롯데가기엔 비용이 만만찮고 ㅎㅎ",
          "original_form": "그렇다고 거리두기되는 샤롯데가기엔 비용이 만만찮고 ㅎㅎ",
          "speaker_id": "2",
