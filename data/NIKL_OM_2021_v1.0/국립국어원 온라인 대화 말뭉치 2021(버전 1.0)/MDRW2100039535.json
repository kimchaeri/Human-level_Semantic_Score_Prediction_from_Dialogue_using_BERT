      "utterance": [
        {
          "id": "MDRW2100039535.1.1",
          "form": "머리 빗는 빗중에?",
          "original_form": "머리 빗는 빗중에?",
          "speaker_id": "1",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100039535.1.2",
          "form": "빗중에 숱많은 빗있잖아",
          "original_form": "빗중에 숱많은 빗있잖아",
          "speaker_id": "2",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100039535.1.3",
          "form": "그 한쪽방향에 칫솔처럼 달린 거?",
          "original_form": "그 한쪽방향에 칫솔처럼 달린 거?",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039535.1.4",
          "form": "어 그거로 두드리느내ㅣ 좋대",
          "original_form": "어 그거로 두드리느내ㅣ 좋대",
          "speaker_id": "2",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100039535.1.5",
          "form": "아 니가생각하는 빗이랑",
          "original_form": "아 니가생각하는 빗이랑",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100039535.1.6",
          "form": "다른것같은데",
          "original_form": "다른것같은데",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100039535.1.7",
          "form": "그 끝에 동그란거 붙어이쏜",
          "original_form": "그 끝에 동그란거 붙어이쏜",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100039535.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ서로 다른 거 생각하고 있었나",
          "original_form": "ㅋㅋㅋㅋㅋㅋ서로 다른 거 생각하고 있었나",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039535.1.9",
          "form": "끝에 동그란 게 뭐지",
          "original_form": "끝에 동그란 게 뭐지",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039535.1.10",
          "form": "나는 칫솔브러쉬같이 생긴 거 떠올렸는데",
          "original_form": "나는 칫솔브러쉬같이 생긴 거 떠올렸는데",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039535.1.11",
          "form": "넌 어떤 거 생각한 거야",
          "original_form": "넌 어떤 거 생각한 거야",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039535.1.12",
          "form": "끝에 동그란 게 뭐지",
          "original_form": "끝에 동그란 게 뭐지",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039535.1.13",
          "form": "대머리아저씨들이",
          "original_form": "대머리아저씨들이",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039535.1.14",
          "form": "머리두드릴때 쓰는거",
          "original_form": "머리두드릴때 쓰는거",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039535.1.15",
          "form": "그런거",
          "original_form": "그런거",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039535.1.16",
          "form": "혹시 그 끈끈이 주걱같이 생긴 빗?",
          "original_form": "혹시 그 끈끈이 주걱같이 생긴 빗?",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039535.1.17",
          "form": "끈끈이 주걱이 뭔지 모르겠네",
          "original_form": "끈끈이 주걱이 뭔지 모르겠네",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039535.1.18",
          "form": "그냥 넘겨버려 대충 손끝으로",
          "original_form": "그냥 넘겨버려 대충 손끝으로",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039535.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ짱 웃겨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ짱 웃겨",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039535.1.20",
          "form": "서로 모름",
          "original_form": "서로 모름",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039535.1.21",
          "form": "두드려줘",
          "original_form": "두드려줘",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039535.1.22",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039535.1.23",
          "form": "넌 림프절 마사지 효과 봤어?",
          "original_form": "넌 림프절 마사지 효과 봤어?",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039535.1.24",
          "form": "사실은 잘모르겠어",
          "original_form": "사실은 잘모르겠어",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039535.1.25",
          "form": "한지 얼마안돼서",
          "original_form": "한지 얼마안돼서",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039535.1.26",
          "form": "나도 이번에 같이 시작해볼까",
          "original_form": "나도 이번에 같이 시작해볼까",
          "speaker_id": "1",
          "time": "20210903 19:35"
        },
        {
          "id": "MDRW2100039535.1.27",
          "form": "난 몸이 차니까 건강에 효과 있는지 알기 더 좋을 것 같은데",
          "original_form": "난 몸이 차니까 건강에 효과 있는지 알기 더 좋을 것 같은데",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039535.1.28",
          "form": "같이 해보자",
          "original_form": "같이 해보자",
          "speaker_id": "1",
          "time": "20210903 19:36"
        },
        {
          "id": "MDRW2100039535.1.29",
          "form": "그래그래 자주 해주는게 좋대",
          "original_form": "그래그래 자주 해주는게 좋대",
