      "utterance": [
        {
          "id": "MDRW2100038706.1.1",
          "form": "저녁 드셧어요?",
          "original_form": "저녁 드셧어요?",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100038706.1.2",
          "form": "네 저녁드셨나요?",
          "original_form": "네 저녁드셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100038706.1.3",
          "form": "넵 저는 돈가스 먹었습니당 !",
          "original_form": "넵 저는 돈가스 먹었습니당 !",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100038706.1.4",
          "form": "돈가스 맛있으셨겠다 밖에서 드셨나요?",
          "original_form": "돈가스 맛있으셨겠다 밖에서 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100038706.1.5",
          "form": "집에서 튀겨먹었어요!!!",
          "original_form": "집에서 튀겨먹었어요!!!",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038706.1.6",
          "form": "저녁 뭐 드셧어용?",
          "original_form": "저녁 뭐 드셧어용?",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038706.1.7",
          "form": "저는 고기 구워먹었어요",
          "original_form": "저는 고기 구워먹었어요",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038706.1.8",
          "form": "오 삼겹살?? 소고기?? 드셧나요??",
          "original_form": "오 삼겹살?? 소고기?? 드셧나요??",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038706.1.9",
          "form": "돼지앞다리살이요!",
          "original_form": "돼지앞다리살이요!",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038706.1.10",
          "form": "정육점아저씨가 추천하셔서",
          "original_form": "정육점아저씨가 추천하셔서",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038706.1.11",
          "form": "바로 사버렸어요",
          "original_form": "바로 사버렸어요",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038706.1.12",
          "form": "우앙 ㅜㅜ 고기랑 소주랑 먹고싶네요 ㅋㅋ",
          "original_form": "우앙 ㅜㅜ 고기랑 소주랑 먹고싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038706.1.13",
          "form": "술 좋아하셔요?",
          "original_form": "술 좋아하셔요?",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038706.1.14",
          "form": "많이는 못마시고 한잔씩?? 가볍게 마셔요 ㅠㅠ",
          "original_form": "많이는 못마시고 한잔씩?? 가볍게 마셔요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038706.1.15",
          "form": "반주 좋아하시는구나",
          "original_form": "반주 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038706.1.16",
          "form": "반주하실때 어떤음식이 좋으세요?",
          "original_form": "반주하실때 어떤음식이 좋으세요?",
          "speaker_id": "2",
          "time": "20210902 22:07"
        },
        {
          "id": "MDRW2100038706.1.17",
          "form": "저는 고기나 회요!!! 회가 최고예요!!",
          "original_form": "저는 고기나 회요!!! 회가 최고예요!!",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100038706.1.18",
          "form": "헉 회좋죠ㅠㅠㅠㅠ",
          "original_form": "헉 회좋죠ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100038706.1.19",
          "form": "회먹고싶네요ㅠㅠㅠㅠ",
          "original_form": "회먹고싶네요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100038706.1.20",
          "form": "보리숭어 먹고싶어요",
          "original_form": "보리숭어 먹고싶어요",
