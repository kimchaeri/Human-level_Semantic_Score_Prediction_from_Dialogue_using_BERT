      "utterance": [
        {
          "id": "MDRW2100003035.1.1",
          "form": "요즘엔 국내여행도 좋은데 많더라고요",
          "original_form": "요즘엔 국내여행도 좋은데 많더라고요",
          "speaker_id": "1",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100003035.1.2",
          "form": "어디 추천할만한곳 있나요?",
          "original_form": "어디 추천할만한곳 있나요?",
          "speaker_id": "2",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100003035.1.3",
          "form": "저는 진짜 싫다가도 역시",
          "original_form": "저는 진짜 싫다가도 역시",
          "speaker_id": "1",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003035.1.4",
          "form": "등산 추천ㅋㅋ",
          "original_form": "등산 추천ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:44"
        },
        {
          "id": "MDRW2100003035.1.5",
          "form": "앗 등산은 제가 제일 싫어하는데 ㅎ",
          "original_form": "앗 등산은 제가 제일 싫어하는데 ㅎ",
          "speaker_id": "2",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003035.1.6",
          "form": "어디 경치좋은 산있나요?",
          "original_form": "어디 경치좋은 산있나요?",
          "speaker_id": "2",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003035.1.7",
          "form": "요즘에 구름다리 있는 산이 많아요",
          "original_form": "요즘에 구름다리 있는 산이 많아요",
          "speaker_id": "1",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003035.1.8",
          "form": "그런데 무서워요",
          "original_form": "그런데 무서워요",
          "speaker_id": "1",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100003035.1.9",
          "form": "아 구름다리는 함 가보고싶어요ㅎ 스릴있을듯요",
          "original_form": "아 구름다리는 함 가보고싶어요ㅎ 스릴있을듯요",
          "speaker_id": "2",
          "time": "20210820 17:48"
        },
        {
          "id": "MDRW2100003035.1.10",
          "form": "그리고 작은 섬중에 예쁜 곳이 많아요",
          "original_form": "그리고 작은 섬중에 예쁜 곳이 많아요",
          "speaker_id": "1",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003035.1.11",
          "form": "국내여행은 거의 못해봤어요 차도없고ㅎ",
          "original_form": "국내여행은 거의 못해봤어요 차도없고ㅎ",
          "speaker_id": "2",
          "time": "20210820 18:02"
        },
        {
          "id": "MDRW2100003035.1.12",
          "form": "여수랑 광양 가봤는데 좋더라구요",
          "original_form": "여수랑 광양 가봤는데 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 18:02"
        },
        {
          "id": "MDRW2100003035.1.13",
          "form": "둘 다 못가봤어요.",
          "original_form": "둘 다 못가봤어요.",
          "speaker_id": "1",
          "time": "20210820 18:07"
        },
        {
          "id": "MDRW2100003035.1.14",
          "form": "여수는 신세계라던데ㅋㅋ 장범준이 살렸죠",
          "original_form": "여수는 신세계라던데ㅋㅋ 장범준이 살렸죠",
          "speaker_id": "1",
          "time": "20210820 18:07"
        },
        {
          "id": "MDRW2100003035.1.15",
          "form": "마자요 사람 엄청많더라구요",
          "original_form": "마자요 사람 엄청많더라구요",
          "speaker_id": "2",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100003035.1.16",
          "form": "게장 맛잇ㅁ다던데",
          "original_form": "게장 맛잇ㅁ다던데",
          "speaker_id": "1",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100003035.1.17",
          "form": "맛집도 많고 한번쯤 가볼만해요ㅎ",
          "original_form": "맛집도 많고 한번쯤 가볼만해요ㅎ",
          "speaker_id": "2",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100003035.1.18",
          "form": "게장도 리필되고 좋던데요",
          "original_form": "게장도 리필되고 좋던데요",
          "speaker_id": "2",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100003035.1.19",
          "form": "뭐 타고 가셨어요?",
          "original_form": "뭐 타고 가셨어요?",
          "speaker_id": "1",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100003035.1.20",
          "form": "갓김치도 맛있어요ㅎ 버스타고 갔다가 친구 광양살아서 친구차로 돌아다녔어요",
          "original_form": "갓김치도 맛있어요ㅎ 버스타고 갔다가 친구 광양살아서 친구차로 돌아다녔어요",
          "speaker_id": "2",
          "time": "20210820 18:13"
        }
      ]
