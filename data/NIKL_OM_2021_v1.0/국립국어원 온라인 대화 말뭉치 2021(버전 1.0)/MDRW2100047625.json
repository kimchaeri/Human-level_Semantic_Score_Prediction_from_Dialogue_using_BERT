      "utterance": [
        {
          "id": "MDRW2100047625.1.1",
          "form": "이게 주사를 맞아도",
          "original_form": "이게 주사를 맞아도",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047625.1.2",
          "form": "금새 또아파서",
          "original_form": "금새 또아파서",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047625.1.3",
          "form": "저는 홈트로만 쪼금씩 운동하지만",
          "original_form": "저는 홈트로만 쪼금씩 운동하지만",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047625.1.4",
          "form": "계속 해야한데요 ㅜㅜ",
          "original_form": "계속 해야한데요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047625.1.5",
          "form": "무서워요",
          "original_form": "무서워요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047625.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100047625.1.7",
          "form": "아예 재활피티나 필라테스를 하는것도 방법이라고 하더라구요",
          "original_form": "아예 재활피티나 필라테스를 하는것도 방법이라고 하더라구요",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047625.1.8",
          "form": "물리치료사 출신인 분 찾아서ㅠㅠ",
          "original_form": "물리치료사 출신인 분 찾아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047625.1.9",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047625.1.10",
          "form": "비싸죠?",
          "original_form": "비싸죠?",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047625.1.11",
          "form": "저도 금액은 정확히 모르지만 아무래도 비싸겠죠?ㅠㅠ",
          "original_form": "저도 금액은 정확히 모르지만 아무래도 비싸겠죠?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047625.1.12",
          "form": "그런데 수술까지 가지 않으려면 ㅠㅠ",
          "original_form": "그런데 수술까지 가지 않으려면 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047625.1.13",
          "form": "아 그럼 ㅜㅜ망설여 지네요",
          "original_form": "아 그럼 ㅜㅜ망설여 지네요",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047625.1.14",
          "form": "해야죠",
          "original_form": "해야죠",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047625.1.15",
          "form": "운동부터 할께요",
          "original_form": "운동부터 할께요",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047625.1.16",
          "form": "네 무리하지 마시고",
          "original_form": "네 무리하지 마시고",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047625.1.17",
          "form": "알려주셔서 감사해요",
          "original_form": "알려주셔서 감사해요",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047625.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047625.1.19",
          "form": "할수잇는한 천천히 꾸준히 해보세요",
          "original_form": "할수잇는한 천천히 꾸준히 해보세요",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047625.1.20",
          "form": "저도 처음엔 진짜 목이 젖혀지질 않앗거든요",
          "original_form": "저도 처음엔 진짜 목이 젖혀지질 않앗거든요",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100047625.1.21",
          "form": "전 참 꾸준히를 못해요",
          "original_form": "전 참 꾸준히를 못해요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100047625.1.22",
          "form": "거북목 너무 심해서 ㅠㅠ",
          "original_form": "거북목 너무 심해서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100047625.1.23",
          "form": "제가 그래요",
          "original_form": "제가 그래요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100047625.1.24",
          "form": "아 ㅜㅜ",
          "original_form": "아 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100047625.1.25",
          "form": "동지네요",
          "original_form": "동지네요",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100047625.1.26",
          "form": "지금도 사실 심해요 목뒤 뼈 튀어나와잇고 ㅠㅠ",
          "original_form": "지금도 사실 심해요 목뒤 뼈 튀어나와잇고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100047625.1.27",
          "form": "뒤통수를 타고 두통이 올 정도엿는데",
          "original_form": "뒤통수를 타고 두통이 올 정도엿는데",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100047625.1.28",
          "form": "그래도 나름 운동하면서 두통은 많이줄엇어요",
          "original_form": "그래도 나름 운동하면서 두통은 많이줄엇어요",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100047625.1.29",
          "form": "목은 아직 구부정하지만 어깨도 많이 펴졌구요",
          "original_form": "목은 아직 구부정하지만 어깨도 많이 펴졌구요",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100047625.1.30",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100047625.1.31",
          "form": "뒤통수두통",
          "original_form": "뒤통수두통",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100047625.1.32",
          "form": "저 약먹었어요 지금 ㅜㅜ",
          "original_form": "저 약먹었어요 지금 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100047625.1.33",
          "form": "견인기도 살까 싶어요",
          "original_form": "견인기도 살까 싶어요",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100047625.1.34",
          "form": "무조건 무리하지마시고 유투브에서 여러 동작중에 할만하다 싶은거 조금씩 시도해보세요",
          "original_form": "무조건 무리하지마시고 유투브에서 여러 동작중에 할만하다 싶은거 조금씩 시도해보세요",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100047625.1.35",
          "form": "견인기는 저도 안사봐서 좋은지 안좋은지 잘 몰라요 ㅎㅎ",
          "original_form": "견인기는 저도 안사봐서 좋은지 안좋은지 잘 몰라요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100047625.1.36",
          "form": "네 그럴께요",
          "original_form": "네 그럴께요",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100047625.1.37",
          "form": "안사셨군요",
          "original_form": "안사셨군요",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100047625.1.38",
          "form": "혹해서 막 사고싶어요 ㅋㅋ",
          "original_form": "혹해서 막 사고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100047625.1.39",
          "form": "저는 일단 운동부터 어떻게 해보자 싶어서 ㅋㅋㅋㅋ",
          "original_form": "저는 일단 운동부터 어떻게 해보자 싶어서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100047625.1.40",
          "form": "그게 정답이죠",
          "original_form": "그게 정답이죠",
          "speaker_id": "2",
          "time": "20210826 22:37"
        }
      ]
    }
