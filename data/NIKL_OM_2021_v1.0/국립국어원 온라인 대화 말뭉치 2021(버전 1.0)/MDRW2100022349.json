      "utterance": [
        {
          "id": "MDRW2100022349.1.1",
          "form": "아 대화평가 두번 세번씩 내는거 짜증나네",
          "original_form": "아 대화평가 두번 세번씩 내는거 짜증나네",
          "speaker_id": "2",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100022349.1.2",
          "form": "그거 혹시 다른사람한테 대화오세요?",
          "original_form": "그거 혹시 다른사람한테 대화오세요?",
          "speaker_id": "1",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100022349.1.3",
          "form": "아뇨 저 계속 여기만 보고 있어요",
          "original_form": "아뇨 저 계속 여기만 보고 있어요",
          "speaker_id": "2",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100022349.1.4",
          "form": "저 지금 참치뽈찜 냄비에 넣고 와도 돼요?",
          "original_form": "저 지금 참치뽈찜 냄비에 넣고 와도 돼요?",
          "speaker_id": "2",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022349.1.5",
          "form": "왜그러지.. 저 일부러 제출할 때 까지 대화안하거든요",
          "original_form": "왜그러지.. 저 일부러 제출할 때 까지 대화안하거든요",
          "speaker_id": "1",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022349.1.6",
          "form": "네 그러세요",
          "original_form": "네 그러세요",
          "speaker_id": "1",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022349.1.7",
          "form": "님은 점심 뭐 먹는댔죠?",
          "original_form": "님은 점심 뭐 먹는댔죠?",
          "speaker_id": "2",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022349.1.8",
          "form": "빵이요",
          "original_form": "빵이요",
          "speaker_id": "1",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022349.1.9",
          "form": "빵 먹으면 금방 배고파지진 않아요?",
          "original_form": "빵 먹으면 금방 배고파지진 않아요?",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100022349.1.10",
          "form": "요즘엔 빵이 워낙잘나와서",
          "original_form": "요즘엔 빵이 워낙잘나와서",
          "speaker_id": "1",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100022349.1.11",
          "form": "그러지는 않아요",
          "original_form": "그러지는 않아요",
          "speaker_id": "1",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100022349.1.12",
          "form": "오 그래요? 무슨빵 드시려구요",
          "original_form": "오 그래요? 무슨빵 드시려구요",
          "speaker_id": "2",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100022349.1.13",
          "form": "빵 좋아하시나요",
          "original_form": "빵 좋아하시나요",
          "speaker_id": "1",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100022349.1.14",
          "form": "저 할인해줄 때만 빵 좋아해요",
          "original_form": "저 할인해줄 때만 빵 좋아해요",
          "speaker_id": "2",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100022349.1.15",
          "form": "저 소시지빵이랑 뀐아망이라고 페스츄리요",
          "original_form": "저 소시지빵이랑 뀐아망이라고 페스츄리요",
          "speaker_id": "1",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100022349.1.16",
          "form": "요기요 파바할인 보셨나요",
          "original_form": "요기요 파바할인 보셨나요",
          "speaker_id": "1",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100022349.1.17",
          "form": "아 ㅋㅋ 맛있겠네요",
          "original_form": "아 ㅋㅋ 맛있겠네요",
          "speaker_id": "2",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100022349.1.18",
          "form": "그거해서 사온거에요",
          "original_form": "그거해서 사온거에요",
          "speaker_id": "1",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100022349.1.19",
          "form": "네 ㅋㅋ 요기요 파바할인으로 저렴하게 잘먹었어요",
          "original_form": "네 ㅋㅋ 요기요 파바할인으로 저렴하게 잘먹었어요",
          "speaker_id": "2",
          "time": "20210829 12:58"
        },
        {
          "id": "MDRW2100022349.1.20",
          "form": "두번이나 사먹었어요",
          "original_form": "두번이나 사먹었어요",
          "speaker_id": "2",
          "time": "20210829 12:58"
        }
      ]
