      "utterance": [
        {
          "id": "MDRW2100009160.1.1",
          "form": "출출하네요",
          "original_form": "출출하네요",
          "speaker_id": "2",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009160.1.2",
          "form": "저도 뼈해장국이 먹고싶네요",
          "original_form": "저도 뼈해장국이 먹고싶네요",
          "speaker_id": "1",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009160.1.3",
          "form": "요깃거리할무언가없을까요",
          "original_form": "요깃거리할무언가없을까요",
          "speaker_id": "2",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009160.1.4",
          "form": "저희집에는 과자가 있어요",
          "original_form": "저희집에는 과자가 있어요",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009160.1.5",
          "form": "얼크으으은한국물이먹고싶은데",
          "original_form": "얼크으으은한국물이먹고싶은데",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009160.1.6",
          "form": "저는 빨간국이 땡기는데요",
          "original_form": "저는 빨간국이 땡기는데요",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009160.1.7",
          "form": "뼈해장국드시고싶으시다면서요",
          "original_form": "뼈해장국드시고싶으시다면서요",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009160.1.8",
          "form": "과자는안돼요",
          "original_form": "과자는안돼요",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009160.1.9",
          "form": "열시까지라 음식점이 다 닫았네요",
          "original_form": "열시까지라 음식점이 다 닫았네요",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009160.1.10",
          "form": "ㅋㅋ네 그치만 경계하는 인물이 있어 지금 먹으면 혼나요",
          "original_form": "ㅋㅋ네 그치만 경계하는 인물이 있어 지금 먹으면 혼나요",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009160.1.11",
          "form": "조심스레 배달을 시켜볼까 합니다",
          "original_form": "조심스레 배달을 시켜볼까 합니다",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009160.1.12",
          "form": "저희집도 야식못먹게해요",
          "original_form": "저희집도 야식못먹게해요",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009160.1.13",
          "form": "해물찜도 먹고싶어요",
          "original_form": "해물찜도 먹고싶어요",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009160.1.14",
          "form": "그거정말좋은생각이십니다",
          "original_form": "그거정말좋은생각이십니다",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009160.1.15",
          "form": "다른 메뉴 추천받아요",
          "original_form": "다른 메뉴 추천받아요",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009160.1.16",
          "form": "아니면 운동간다하고 호록먹고오기",
          "original_form": "아니면 운동간다하고 호록먹고오기",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009160.1.17",
          "form": "뼈해장국정말침고이네요",
          "original_form": "뼈해장국정말침고이네요",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009160.1.18",
          "form": "뼈해장국맛집이잇나요?",
          "original_form": "뼈해장국맛집이잇나요?",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009160.1.19",
          "form": "집근처에 기가 막힌 뼈해장국집이 있어요",
          "original_form": "집근처에 기가 막힌 뼈해장국집이 있어요",
          "speaker_id": "1",
          "time": "20210822 21:53"
