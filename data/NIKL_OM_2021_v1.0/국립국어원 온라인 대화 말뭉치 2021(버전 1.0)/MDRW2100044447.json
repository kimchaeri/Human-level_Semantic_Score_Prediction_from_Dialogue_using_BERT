        },
        {
          "id": "MDRW2100044447.1.2",
          "form": "해물이요?",
          "original_form": "해물이요?",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044447.1.3",
          "form": "네 근데 올만에",
          "original_form": "네 근데 올만에",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044447.1.4",
          "form": "칼칼한 김치든것도 끌리내요",
          "original_form": "칼칼한 김치든것도 끌리내요",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044447.1.5",
          "form": "크 수제빈 어때요",
          "original_form": "크 수제빈 어때요",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044447.1.6",
          "form": "아 대박이죠ㅠㅠㅠㅠㅠ",
          "original_form": "아 대박이죠ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044447.1.7",
          "form": "얇은 수제비",
          "original_form": "얇은 수제비",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044447.1.8",
          "form": "아 저 수제비 진짜 조아해요",
          "original_form": "아 저 수제비 진짜 조아해요",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044447.1.9",
          "form": "저두요ㅠ 수제비맛집 아시나요",
          "original_form": "저두요ㅠ 수제비맛집 아시나요",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044447.1.10",
          "form": "대전 오한손 수제비?",
          "original_form": "대전 오한손 수제비?",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044447.1.11",
          "form": "비오는날 딱먹기조아요",
          "original_form": "비오는날 딱먹기조아요",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044447.1.12",
          "form": "우와 ㅜㅜㅜㅜ가구싶어요ㅠㅠㅠㅠ",
          "original_form": "우와 ㅜㅜㅜㅜ가구싶어요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044447.1.13",
          "form": "거기 그 국물 좀시원하고 거기죠",
          "original_form": "거기 그 국물 좀시원하고 거기죠",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044447.1.14",
          "form": "마자여",
          "original_form": "마자여",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044447.1.15",
          "form": "냄비에 끓여서",
          "original_form": "냄비에 끓여서",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044447.1.16",
          "form": "가고싶어요",
          "original_form": "가고싶어요",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044447.1.17",
          "form": "다움에 대전가면 같이가시죠",
          "original_form": "다움에 대전가면 같이가시죠",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044447.1.18",
          "form": "오시죠 환영잉6",
          "original_form": "오시죠 환영잉6",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044447.1.19",
          "form": "이요",
          "original_form": "이요",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044447.1.20",
          "form": "회 조아하세요?",
          "original_form": "회 조아하세요?",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044447.1.21",
          "form": "완전요",
          "original_form": "완전요",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044447.1.22",
          "form": "어떤회요",
          "original_form": "어떤회요",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044447.1.23",
          "form": "저는 그냥 광어연어 좋아해요",
          "original_form": "저는 그냥 광어연어 좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044447.1.24",
          "form": "숭어도좋구요",
          "original_form": "숭어도좋구요",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044447.1.25",
          "form": "와 방어는요",
          "original_form": "와 방어는요",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044447.1.26",
          "form": "방어도 좋죠 근데 마니먹으면 느끼하더라구요",
          "original_form": "방어도 좋죠 근데 마니먹으면 느끼하더라구요",
          "speaker_id": "1",
          "time": "20210907 23:08"
        }
