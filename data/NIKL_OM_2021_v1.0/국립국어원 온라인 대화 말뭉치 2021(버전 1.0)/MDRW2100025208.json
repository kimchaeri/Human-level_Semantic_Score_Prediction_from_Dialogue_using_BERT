        },
        {
          "id": "MDRW2100025208.1.2",
          "form": "쇼핑 최근에 가장 보람찼던 건 딱히 없긴 한데",
          "original_form": "쇼핑 최근에 가장 보람찼던 건 딱히 없긴 한데",
          "speaker_id": "2",
          "time": "20210901 00:55"
        },
        {
          "id": "MDRW2100025208.1.3",
          "form": "그래도 핫딜 뜨면",
          "original_form": "그래도 핫딜 뜨면",
          "speaker_id": "2",
          "time": "20210901 00:55"
        },
        {
          "id": "MDRW2100025208.1.4",
          "form": "반가워집니다 소비는 즐거우니까요",
          "original_form": "반가워집니다 소비는 즐거우니까요",
          "speaker_id": "2",
          "time": "20210901 00:55"
        },
        {
          "id": "MDRW2100025208.1.5",
          "form": "ㅎㅎ 뭔가 돈버는 기분이죠",
          "original_form": "ㅎㅎ 뭔가 돈버는 기분이죠",
          "speaker_id": "1",
          "time": "20210901 00:55"
        },
        {
          "id": "MDRW2100025208.1.6",
          "form": "사실 안 사야 지출 0원이데",
          "original_form": "사실 안 사야 지출 0원이데",
          "speaker_id": "1",
          "time": "20210901 00:56"
        },
        {
          "id": "MDRW2100025208.1.7",
          "form": "세럼도 핫딜이었나요?",
          "original_form": "세럼도 핫딜이었나요?",
          "speaker_id": "1",
          "time": "20210901 00:56"
        },
        {
          "id": "MDRW2100025208.1.8",
          "form": "세러은 핫딜은 아니었어요 ㅋㅋㅋㅋㅋ",
          "original_form": "세러은 핫딜은 아니었어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100025208.1.9",
          "form": "안타깝게도..ㅎㅎ",
          "original_form": "안타깝게도..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 00:57"
        },
        {
          "id": "MDRW2100025208.1.10",
          "form": "안사야 0원 공감합니다",
          "original_form": "안사야 0원 공감합니다",
          "speaker_id": "2",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100025208.1.11",
          "form": "최근 핫딜로 뭐 사셨어요?",
          "original_form": "최근 핫딜로 뭐 사셨어요?",
          "speaker_id": "1",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100025208.1.12",
          "form": "핫딜로 불탄게 아마 7월이었던 거 같은데",
          "original_form": "핫딜로 불탄게 아마 7월이었던 거 같은데",
          "speaker_id": "2",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100025208.1.13",
          "form": "그 때 먹을 것도 많이 사고 그랬어요",
          "original_form": "그 때 먹을 것도 많이 사고 그랬어요",
          "speaker_id": "2",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100025208.1.14",
          "form": "기억에 남는 건 떡볶이??",
          "original_form": "기억에 남는 건 떡볶이??",
          "speaker_id": "2",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100025208.1.15",
          "form": "오오 떡볶이가 대박딜이었나봐요",
          "original_form": "오오 떡볶이가 대박딜이었나봐요",
          "speaker_id": "1",
          "time": "20210901 01:01"
        },
        {
          "id": "MDRW2100025208.1.16",
          "form": "일반? 국물? 로제?",
          "original_form": "일반? 국물? 로제?",
          "speaker_id": "1",
          "time": "20210901 01:01"
        },
        {
          "id": "MDRW2100025208.1.17",
          "form": "네 로제 떡볶이였어요",
          "original_form": "네 로제 떡볶이였어요",
          "speaker_id": "2",
          "time": "20210901 01:01"
        },
        {
          "id": "MDRW2100025208.1.18",
          "form": "긴 떡이였고",
          "original_form": "긴 떡이였고",
          "speaker_id": "2",
          "time": "20210901 01:01"
        },
        {
          "id": "MDRW2100025208.1.19",
          "form": "19cm?",
          "original_form": "19cm?",
          "speaker_id": "2",
          "time": "20210901 01:01"
        },
        {
          "id": "MDRW2100025208.1.20",
          "form": "그래서 아주 씹는 맛이 좋답니다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래서 아주 씹는 맛이 좋답니다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100025208.1.21",
          "form": "추천드려요",
          "original_form": "추천드려요",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100025208.1.22",
          "form": "가래떡볶이인가요?",
          "original_form": "가래떡볶이인가요?",
          "speaker_id": "1",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100025208.1.23",
          "form": "아님 길이만?",
          "original_form": "아님 길이만?",
          "speaker_id": "1",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100025208.1.24",
          "form": "궁금하네요ㅎ 약간 면느낌 날것도 같고",
          "original_form": "궁금하네요ㅎ 약간 면느낌 날것도 같고",
          "speaker_id": "1",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100025208.1.25",
          "form": "두꺼운 떡은 아니고 밀떡 두께인데 길이만 길어요!",
          "original_form": "두꺼운 떡은 아니고 밀떡 두께인데 길이만 길어요!",
          "speaker_id": "2",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100025208.1.26",
          "form": "와 진짜 면같을 것 같아요",
          "original_form": "와 진짜 면같을 것 같아요",
          "speaker_id": "1",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100025208.1.27",
          "form": "이런 긴 떡볶이는 쌀 떡보다 밀떡인 것 같아요",
          "original_form": "이런 긴 떡볶이는 쌀 떡보다 밀떡인 것 같아요",
          "speaker_id": "2",
          "time": "20210901 01:04"
        },
        {
          "id": "MDRW2100025208.1.28",
          "form": "쫄깃해진 우동사리 느킴이려나요",
          "original_form": "쫄깃해진 우동사리 느킴이려나요",
          "speaker_id": "1",
          "time": "20210901 01:04"
        },
        {
          "id": "MDRW2100025208.1.29",
          "form": "ㅎㅎ그래야 잘 스미죠ㅎ",
          "original_form": "ㅎㅎ그래야 잘 스미죠ㅎ",
          "speaker_id": "1",
          "time": "20210901 01:04"
        },
        {
          "id": "MDRW2100025208.1.30",
          "form": "오오 네 좀 많이 쫄깃해진 우동사리요",
          "original_form": "오오 네 좀 많이 쫄깃해진 우동사리요",
          "speaker_id": "2",
          "time": "20210901 01:04"
        },
        {
          "id": "MDRW2100025208.1.31",
          "form": "한입에 한 떡 다 넣고 씹으면 만족감 오져요",
          "original_form": "한입에 한 떡 다 넣고 씹으면 만족감 오져요",
          "speaker_id": "2",
          "time": "20210901 01:04"
        }
      ]
