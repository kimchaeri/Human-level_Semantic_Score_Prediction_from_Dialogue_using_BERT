      "utterance": [
        {
          "id": "MDRW2100006796.1.1",
          "form": "즐겨입는 옷은 뭔가요",
          "original_form": "즐겨입는 옷은 뭔가요",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100006796.1.2",
          "form": "네 1차 접종 했어요",
          "original_form": "네 1차 접종 했어요",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100006796.1.3",
          "form": "백신 맞으셨나요",
          "original_form": "백신 맞으셨나요",
          "speaker_id": "2",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100006796.1.4",
          "form": "네 저는 얀센이요",
          "original_form": "네 저는 얀센이요",
          "speaker_id": "1",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100006796.1.5",
          "form": "아프진 않으셨나요",
          "original_form": "아프진 않으셨나요",
          "speaker_id": "2",
          "time": "20210908 09:42"
        },
        {
          "id": "MDRW2100006796.1.6",
          "form": "하루정돈 힘들더라구요",
          "original_form": "하루정돈 힘들더라구요",
          "speaker_id": "1",
          "time": "20210908 09:46"
        },
        {
          "id": "MDRW2100006796.1.7",
          "form": "다른곳은 아픈데는 없나요",
          "original_form": "다른곳은 아픈데는 없나요",
          "speaker_id": "2",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100006796.1.8",
          "form": "네 name2님은 어떠셨어요?",
          "original_form": "네 &name2&님은 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100006796.1.9",
          "form": "저도 2틀정도 아프고 그후 괜찮았죠",
          "original_form": "저도 2틀정도 아프고 그후 괜찮았죠",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100006796.1.10",
          "form": "약 드셨나요?",
          "original_form": "약 드셨나요?",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100006796.1.11",
          "form": "네 타이레놀 먹었답니다",
          "original_form": "네 타이레놀 먹었답니다",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100006796.1.12",
          "form": "건강을 위해서 꼭 접종 해야죠",
          "original_form": "건강을 위해서 꼭 접종 해야죠",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100006796.1.13",
          "form": "열도 났나요?",
          "original_form": "열도 났나요?",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100006796.1.14",
          "form": "아니요 팔 아프고 두통이 있어죠",
          "original_form": "아니요 팔 아프고 두통이 있어죠",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100006796.1.15",
          "form": "화이자였나요?",
          "original_form": "화이자였나요?",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100006796.1.16",
          "form": "네 화이자 였어요\r\n모더나에서 부족해 변경 되었죠",
          "original_form": "네 화이자 였어요\r\n모더나에서 부족해 변경 되었죠",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100006796.1.17",
          "form": "2차는 언제인가요",
          "original_form": "2차는 언제인가요",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
