      "utterance": [
        {
          "id": "MDRW2100047346.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100047346.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100047346.1.3",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100047346.1.4",
          "form": "name2 좋아해요",
          "original_form": "&name2& 좋아해요",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100047346.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100047346.1.6",
          "form": "아 그래서",
          "original_form": "아 그래서",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100047346.1.7",
          "form": "이름이 name2이군요ㅋㅋㅋ",
          "original_form": "이름이 &name2&이군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100047346.1.8",
          "form": "네ㅋㅋ name1님은요?",
          "original_form": "네ㅋㅋ &name1&님은요?",
          "speaker_id": "1",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100047346.1.9",
          "form": "전 고기 좋아해요",
          "original_form": "전 고기 좋아해요",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100047346.1.10",
          "form": "고기 싫어하는 사람은 적겠지만",
          "original_form": "고기 싫어하는 사람은 적겠지만",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100047346.1.11",
          "form": "과일도 좋아하고",
          "original_form": "과일도 좋아하고",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100047346.1.12",
          "form": "소vs닭vs돼지",
          "original_form": "소vs닭vs돼지",
          "speaker_id": "1",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100047346.1.13",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100047346.1.14",
          "form": "소요",
          "original_form": "소요",
          "speaker_id": "2",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100047346.1.15",
          "form": "어렵다",
          "original_form": "어렵다",
          "speaker_id": "2",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100047346.1.16",
          "form": "저는 소고기 맛있지 잘모르겠어요",
          "original_form": "저는 소고기 맛있지 잘모르겠어요",
          "speaker_id": "1",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100047346.1.17",
          "form": "맛있는지",
          "original_form": "맛있는지",
          "speaker_id": "1",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100047346.1.18",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100047346.1.19",
          "form": "취향마다 갈리긴 하지만",
          "original_form": "취향마다 갈리긴 하지만",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100047346.1.20",
          "form": "부드럽고",
          "original_form": "부드럽고",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100047346.1.21",
          "form": "잘 안익혀도 돼서 육즙이 비교적 살아있는 느낌?",
          "original_form": "잘 안익혀도 돼서 육즙이 비교적 살아있는 느낌?",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100047346.1.22",
          "form": "돼지기름보다 소기름이 덜 니글거리기도하고",
          "original_form": "돼지기름보다 소기름이 덜 니글거리기도하고",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100047346.1.23",
          "form": "와인하고 잘어울리더라구요",
          "original_form": "와인하고 잘어울리더라구요",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100047346.1.24",
          "form": "오저는 반대로 돼지기름보다 소기름이 느글느글하더라고용",
          "original_form": "오저는 반대로 돼지기름보다 소기름이 느글느글하더라고용",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100047346.1.25",
          "form": "스테이크 좋아하시나요?",
          "original_form": "스테이크 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100047346.1.26",
          "form": "스테이크 좋아하죠!",
          "original_form": "스테이크 좋아하죠!",
          "speaker_id": "2",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100047346.1.27",
          "form": "입맛은 다 다르군요",
          "original_form": "입맛은 다 다르군요",
          "speaker_id": "2",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100047346.1.28",
          "form": "개인적으로 돼지고기는 비계가 좀 부담스럽더라구요",
          "original_form": "개인적으로 돼지고기는 비계가 좀 부담스럽더라구요",
          "speaker_id": "2",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100047346.1.29",
          "form": "그래서 목살 부위 좋아해요",
          "original_form": "그래서 목살 부위 좋아해요",
          "speaker_id": "2",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100047346.1.30",
          "form": "저도 기름진 부위를 안좋아해서",
          "original_form": "저도 기름진 부위를 안좋아해서",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100047346.1.31",
          "form": "목살이랑 사태 자주 먹어요!",
          "original_form": "목살이랑 사태 자주 먹어요!",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100047346.1.32",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100047346.1.33",
          "form": "목살 그냥 구워먹어도 맛있고",
          "original_form": "목살 그냥 구워먹어도 맛있고",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100047346.1.34",
          "form": "김치찌개에 넣어먹어도 맛있죠",
          "original_form": "김치찌개에 넣어먹어도 맛있죠",
          "speaker_id": "2",
          "time": "20210826 13:24"
        }
      ]
    }
