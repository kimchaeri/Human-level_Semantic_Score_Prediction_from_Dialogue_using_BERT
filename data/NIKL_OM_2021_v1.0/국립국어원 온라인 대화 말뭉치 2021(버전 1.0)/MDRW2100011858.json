      "utterance": [
        {
          "id": "MDRW2100011858.1.1",
          "form": "종교가 있으세요?",
          "original_form": "종교가 있으세요?",
          "speaker_id": "1",
          "time": "20210824 16:47"
        },
        {
          "id": "MDRW2100011858.1.2",
          "form": "아니요. ㅋㅋ 근데 교회간적은 있어요.",
          "original_form": "아니요. ㅋㅋ 근데 교회간적은 있어요.",
          "speaker_id": "2",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011858.1.3",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "2",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011858.1.4",
          "form": "아뇨 저도 무교요",
          "original_form": "아뇨 저도 무교요",
          "speaker_id": "1",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011858.1.5",
          "form": "근데 똑같이 교회 간 적 있어요 ㅋㅋ",
          "original_form": "근데 똑같이 교회 간 적 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011858.1.6",
          "form": "ㅋㅋㅋ 네네 나중에 믿고싶은 종교있으신가요?",
          "original_form": "ㅋㅋㅋ 네네 나중에 믿고싶은 종교있으신가요?",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100011858.1.7",
          "form": "음 전 귀찮음이 심해",
          "original_form": "음 전 귀찮음이 심해",
          "speaker_id": "1",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011858.1.8",
          "form": "종교를 갖게될지 모르겠어요",
          "original_form": "종교를 갖게될지 모르겠어요",
          "speaker_id": "1",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011858.1.9",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210824 17:12"
        },
        {
          "id": "MDRW2100011858.1.10",
          "form": "ㅋㅋ저는 나이들어서는 교회를 나가볼까생각만하고있어요.",
          "original_form": "ㅋㅋ저는 나이들어서는 교회를 나가볼까생각만하고있어요.",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011858.1.11",
          "form": "근데 요즘같은 코로나시대면 생각도못하죠뭐ㅜ",
          "original_form": "근데 요즘같은 코로나시대면 생각도못하죠뭐ㅜ",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011858.1.12",
          "form": "요즘은 교회 못가죠",
          "original_form": "요즘은 교회 못가죠",
          "speaker_id": "1",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100011858.1.13",
          "form": "교회를 선택한 이유가 있을까요?",
          "original_form": "교회를 선택한 이유가 있을까요?",
          "speaker_id": "1",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100011858.1.14",
          "form": "그냥 친숙한느낌? 그런게있어요.ㅋ",
          "original_form": "그냥 친숙한느낌? 그런게있어요.ㅋ",
          "speaker_id": "2",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011858.1.15",
          "form": "아하 뭔가 알 거 같기도 해요",
          "original_form": "아하 뭔가 알 거 같기도 해요",
          "speaker_id": "1",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011858.1.16",
          "form": "절은요??",
          "original_form": "절은요??",
          "speaker_id": "1",
          "time": "20210824 17:50"
        },
        {
          "id": "MDRW2100011858.1.17",
          "form": "절도 좋아요ㅋㅋ",
          "original_form": "절도 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011858.1.18",
          "form": "절 좋아하셔요?",
          "original_form": "절 좋아하셔요?",
          "speaker_id": "2",
          "time": "20210824 17:59"
        },
        {
          "id": "MDRW2100011858.1.19",
          "form": "네ㅋㅋㅋㅋ 엄청 좋이하는 건 아니지만",
          "original_form": "네ㅋㅋㅋㅋ 엄청 좋이하는 건 아니지만",
          "speaker_id": "1",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100011858.1.20",
          "form": "궁금한단계에요",
          "original_form": "궁금한단계에요",
          "speaker_id": "1",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100011858.1.21",
          "form": "ㅋㅋㅋ 절도 조용하니 좋긴하더라구요ㅋ",
          "original_form": "ㅋㅋㅋ 절도 조용하니 좋긴하더라구요ㅋ",
          "speaker_id": "2",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100011858.1.22",
          "form": "특히 전 절밥이 궁금해요ㅋㅋㅋㅋㅋ",
          "original_form": "특히 전 절밥이 궁금해요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100011858.1.23",
          "form": "앗 저도요ㅋㅋㅋ궁금하네요ㅋ",
          "original_form": "앗 저도요ㅋㅋㅋ궁금하네요ㅋ",
