      "utterance": [
        {
          "id": "MDRW2100040565.1.1",
          "form": "두끼 가보셨나요?",
          "original_form": "두끼 가보셨나요?",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040565.1.2",
          "form": "네네 !",
          "original_form": "네네 !",
          "speaker_id": "1",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040565.1.3",
          "form": "연애하시나요?",
          "original_form": "연애하시나요?",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040565.1.4",
          "form": "네 지금만나는사람있어요~",
          "original_form": "네 지금만나는사람있어요~",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100040565.1.5",
          "form": "연애중이신가요?",
          "original_form": "연애중이신가요?",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100040565.1.6",
          "form": "아뇨ㅎㅎ",
          "original_form": "아뇨ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100040565.1.7",
          "form": "언제 만나셨나요",
          "original_form": "언제 만나셨나요",
          "speaker_id": "2",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100040565.1.8",
          "form": "2년좀 안되었어요 ㅎ",
          "original_form": "2년좀 안되었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100040565.1.9",
          "form": "한참 좋으실때네요!",
          "original_form": "한참 좋으실때네요!",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040565.1.10",
          "form": "그런가요 ㅎ ?",
          "original_form": "그런가요 ㅎ ?",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040565.1.11",
          "form": "어린나이는아니다보니 ㅎ 뭔가 잔잔한거같아요 ㅎ",
          "original_form": "어린나이는아니다보니 ㅎ 뭔가 잔잔한거같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040565.1.12",
          "form": "ㅋㅋ소개로 만나셨나요?",
          "original_form": "ㅋㅋ소개로 만나셨나요?",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040565.1.13",
          "form": "네 ㅎ",
          "original_form": "네 ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040565.1.14",
          "form": "결혼생각있으세요 ?",
          "original_form": "결혼생각있으세요 ?",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100040565.1.15",
          "form": "전 아직ㅋㅋ",
          "original_form": "전 아직ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100040565.1.16",
          "form": "결혼준비중이세요?",
          "original_form": "결혼준비중이세요?",
          "speaker_id": "2",
          "time": "20210904 23:38"
        },
        {
          "id": "MDRW2100040565.1.17",
          "form": "그건아닌데 전 꼭해야지 이런생각은없어서",
          "original_form": "그건아닌데 전 꼭해야지 이런생각은없어서",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100040565.1.18",
          "form": "아이에 대한 생각도없어서 한다해도",
          "original_form": "아이에 대한 생각도없어서 한다해도",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100040565.1.19",
          "form": "아이는 안낳을꺼같아요",
          "original_form": "아이는 안낳을꺼같아요",
          "speaker_id": "1",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100040565.1.20",
          "form": "아 저도  좋은 사람 생기면 연애하더라도 출산은.. ㅋㅋ",
          "original_form": "아 저도  좋은 사람 생기면 연애하더라도 출산은.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:40"
        },
        {
          "id": "MDRW2100040565.1.21",
          "form": "2세에 대한생각이없다보니 결혼해서 뭔가 가정을 꾸린다보다",
          "original_form": "2세에 대한생각이없다보니 결혼해서 뭔가 가정을 꾸린다보다",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100040565.1.22",
          "form": "그냥 편하게 만나고 연애하고 잘지내는게 뭐가 다른가싶기두해요 ㅎㅎ",
          "original_form": "그냥 편하게 만나고 연애하고 잘지내는게 뭐가 다른가싶기두해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:41"
        },
        {
          "id": "MDRW2100040565.1.23",
          "form": "전 연애도 오랫동안 안해서.. ㅋㅋ",
          "original_form": "전 연애도 오랫동안 안해서.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040565.1.24",
          "form": "따로 이유가있으신가요~?",
          "original_form": "따로 이유가있으신가요~?",
          "speaker_id": "1",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040565.1.25",
          "form": "좋은 사람 찾으신거 축하드립니다",
          "original_form": "좋은 사람 찾으신거 축하드립니다",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040565.1.26",
          "form": "마땅한 분이ㅎㅎ 없더라고요ㅎㅎ",
          "original_form": "마땅한 분이ㅎㅎ 없더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040565.1.27",
          "form": "또모르죠 하하 앞날은 모르는거니ㅜ",
          "original_form": "또모르죠 하하 앞날은 모르는거니ㅜ",
          "speaker_id": "1",
          "time": "20210904 23:43"
        },
        {
          "id": "MDRW2100040565.1.28",
          "form": "좋은분만나실꺼에요!",
          "original_form": "좋은분만나실꺼에요!",
          "speaker_id": "1",
          "time": "20210904 23:44"
        }
      ]
    }
  ]
