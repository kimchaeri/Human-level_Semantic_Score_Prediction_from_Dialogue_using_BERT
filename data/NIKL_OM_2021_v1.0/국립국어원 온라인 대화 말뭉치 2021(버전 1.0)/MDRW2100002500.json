      "utterance": [
        {
          "id": "MDRW2100002500.1.1",
          "form": "여행 좋아하시나봐요",
          "original_form": "여행 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002500.1.2",
          "form": "네 여행 좋아하죠~",
          "original_form": "네 여행 좋아하죠~",
          "speaker_id": "2",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002500.1.3",
          "form": "가장 최근에 간곳은 어딘가요??",
          "original_form": "가장 최근에 간곳은 어딘가요??",
          "speaker_id": "1",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002500.1.4",
          "form": "최근에 간곳은 거제 바닷가? 가본거 같아요!",
          "original_form": "최근에 간곳은 거제 바닷가? 가본거 같아요!",
          "speaker_id": "2",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002500.1.5",
          "form": "거기랑 이제 대구쪽도 갔고..",
          "original_form": "거기랑 이제 대구쪽도 갔고..",
          "speaker_id": "2",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002500.1.6",
          "form": "오 여름에 가신건가요",
          "original_form": "오 여름에 가신건가요",
          "speaker_id": "1",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002500.1.7",
          "form": "코로나 잠잠해졌을때 갔어요",
          "original_form": "코로나 잠잠해졌을때 갔어요",
          "speaker_id": "2",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002500.1.8",
          "form": "한 봄여름 중간쯤이었던거같아요",
          "original_form": "한 봄여름 중간쯤이었던거같아요",
          "speaker_id": "2",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002500.1.9",
          "form": "저는 코로나 전에 간 여행이",
          "original_form": "저는 코로나 전에 간 여행이",
          "speaker_id": "1",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002500.1.10",
          "form": "마지막이예요..",
          "original_form": "마지막이예요..",
          "speaker_id": "1",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002500.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002500.1.12",
          "form": "이게 여행을 코로나로 안다닐려고 해도",
          "original_form": "이게 여행을 코로나로 안다닐려고 해도",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002500.1.13",
          "form": "몸이 근질근질해서",
          "original_form": "몸이 근질근질해서",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002500.1.14",
          "form": "어쩔수없이 방역수칙 지키고 가끔씩 가게되더라고요",
          "original_form": "어쩔수없이 방역수칙 지키고 가끔씩 가게되더라고요",
          "speaker_id": "2",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002500.1.15",
          "form": "ㅋㅋㅋㅋ그래도",
          "original_form": "ㅋㅋㅋㅋ그래도",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002500.1.16",
          "form": "바람쐬야",
          "original_form": "바람쐬야",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002500.1.17",
          "form": "우울증안걸리고 그러죠...",
          "original_form": "우울증안걸리고 그러죠...",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002500.1.18",
          "form": "하도 집에만 있으니까 하고 싶은게 많네요",
          "original_form": "하도 집에만 있으니까 하고 싶은게 많네요",
          "speaker_id": "1",
          "time": "20210820 12:55"
        },
        {
          "id": "MDRW2100002500.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002500.1.20",
          "form": "집에만 있으면 코로나블루가 심해져서",
          "original_form": "집에만 있으면 코로나블루가 심해져서",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002500.1.21",
          "form": "집근처 공원이라도 가야 좀 풀리더라고요",
          "original_form": "집근처 공원이라도 가야 좀 풀리더라고요",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002500.1.22",
          "form": "저는 강아지 산책하면서 풀고있어요 ㅋㅋ",
          "original_form": "저는 강아지 산책하면서 풀고있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002500.1.23",
          "form": "그렇군요!",
          "original_form": "그렇군요!",
          "speaker_id": "2",
          "time": "20210820 12:56"
        },
        {
          "id": "MDRW2100002500.1.24",
          "form": "그렇게라도 하면 좀 풀리니 좋을겁니다",
          "original_form": "그렇게라도 하면 좀 풀리니 좋을겁니다",
          "speaker_id": "2",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002500.1.25",
          "form": "ㅋㅋㅋ그래서 한번 나가면 한시간 이상 걷다와서",
          "original_form": "ㅋㅋㅋ그래서 한번 나가면 한시간 이상 걷다와서",
          "speaker_id": "1",
          "time": "20210820 12:57"
        },
        {
          "id": "MDRW2100002500.1.26",
          "form": "좀 힘들어요",
          "original_form": "좀 힘들어요",
          "speaker_id": "1",
          "time": "20210820 12:57"
        }
      ]
    }
  ]
}
