        },
        {
          "id": "MDRW2100033949.1.2",
          "form": "안녕하세요 여행 좋아하시나용",
          "original_form": "안녕하세요 여행 좋아하시나용",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100033949.1.3",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100033949.1.4",
          "form": "님은요?!",
          "original_form": "님은요?!",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100033949.1.5",
          "form": "네 저도 좋아해요 근데 코로나이후론 ㅠㅠ",
          "original_form": "네 저도 좋아해요 근데 코로나이후론 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100033949.1.6",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033949.1.7",
          "form": "제주도가고싶네요",
          "original_form": "제주도가고싶네요",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033949.1.8",
          "form": "제주도 저도 어릴때 가보고 커서는 못가서 가보고 싶은데..",
          "original_form": "제주도 저도 어릴때 가보고 커서는 못가서 가보고 싶은데..",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100033949.1.9",
          "form": "가보셨나요?",
          "original_form": "가보셨나요?",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100033949.1.10",
          "form": "네네 저는",
          "original_form": "네네 저는",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100033949.1.11",
          "form": "3번인가4번?가본거같네요ㅠ",
          "original_form": "3번인가4번?가본거같네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100033949.1.12",
          "form": "제주도넘좋죠",
          "original_form": "제주도넘좋죠",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100033949.1.13",
          "form": "와 많이 가셨네요",
          "original_form": "와 많이 가셨네요",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100033949.1.14",
          "form": "맛집도 많이 가셨나요",
          "original_form": "맛집도 많이 가셨나요",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100033949.1.15",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100033949.1.16",
          "form": "당연하죠~~~~",
          "original_form": "당연하죠~~~~",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100033949.1.17",
          "form": "맛있는게 너무너무",
          "original_form": "맛있는게 너무너무",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100033949.1.18",
          "form": "많더라고요",
          "original_form": "많더라고요",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100033949.1.19",
          "form": "그러니까요 뭐가 제일 기억에 남으세요",
          "original_form": "그러니까요 뭐가 제일 기억에 남으세요",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100033949.1.20",
          "form": "너무많아서 잘 손꼽질못하겠네요",
          "original_form": "너무많아서 잘 손꼽질못하겠네요",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100033949.1.21",
          "form": "회도맛있고",
          "original_form": "회도맛있고",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100033949.1.22",
          "form": "흑돼지도요",
          "original_form": "흑돼지도요",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100033949.1.23",
          "form": "해산물도 갈치조림도요",
          "original_form": "해산물도 갈치조림도요",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100033949.1.24",
          "form": "다 맛있는거네요 ㅋㅋㅋ",
          "original_form": "다 맛있는거네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100033949.1.25",
          "form": "흑돼지 진짜 궁금해요",
          "original_form": "흑돼지 진짜 궁금해요",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100033949.1.26",
          "form": "맛있어요ㅋㅋㅋ",
          "original_form": "맛있어요ㅋㅋㅋ",
