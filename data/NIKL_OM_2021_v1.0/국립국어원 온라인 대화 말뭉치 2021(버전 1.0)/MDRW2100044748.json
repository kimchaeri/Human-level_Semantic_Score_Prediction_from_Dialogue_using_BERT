      "utterance": [
        {
          "id": "MDRW2100044748.1.1",
          "form": "그거는 정말 좋은것 같아여",
          "original_form": "그거는 정말 좋은것 같아여",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044748.1.2",
          "form": "데릴러 왔다갔다하는게 엄청 ..귀찮더라구요 크니까 학원도 혼자가고",
          "original_form": "데릴러 왔다갔다하는게 엄청 ..귀찮더라구요 크니까 학원도 혼자가고",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044748.1.3",
          "form": "우와ㅜㅜ\n3학년은 그게 다 가능하군용",
          "original_form": "우와ㅜㅜ\n3학년은 그게 다 가능하군용",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044748.1.4",
          "form": "네 !! 혼자 시간도 체크해서 왔다갔다해요 ㅎ",
          "original_form": "네 !! 혼자 시간도 체크해서 왔다갔다해요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044748.1.5",
          "form": "핸드폰도 있어요?",
          "original_form": "핸드폰도 있어요?",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044748.1.6",
          "form": "네 1학년 되면 다 사줘요",
          "original_form": "네 1학년 되면 다 사줘요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044748.1.7",
          "form": "조카는 2학년인데 아직 없거든요ㅠ",
          "original_form": "조카는 2학년인데 아직 없거든요ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044748.1.8",
          "form": "아! 그럼 엄마가 조금 불편하실텐데..ㅎㅎ",
          "original_form": "아! 그럼 엄마가 조금 불편하실텐데..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044748.1.9",
          "form": "새언니가 등하교 시키더라구요ㅠ",
          "original_form": "새언니가 등하교 시키더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044748.1.10",
          "form": "근데 친구들이랑 노느라",
          "original_form": "근데 친구들이랑 노느라",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044748.1.11",
          "form": "ㅋㅋㅋ 그때되면 그럴나이죠",
          "original_form": "ㅋㅋㅋ 그때되면 그럴나이죠",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044748.1.12",
          "form": "엇갈릴때도 종종 있어요",
          "original_form": "엇갈릴때도 종종 있어요",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044748.1.13",
          "form": "전 제가 걱정되서ㅠㅠ",
          "original_form": "전 제가 걱정되서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044748.1.14",
          "form": "맞아요ㅠ\n세상이 하도 흉흉해서요",
          "original_form": "맞아요ㅠ\n세상이 하도 흉흉해서요",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044748.1.15",
          "form": "저희어릴땐 안그랬는데 조금 컷어도 사실 전화했는데 안받으면 불안하긴해요ㅠ",
          "original_form": "저희어릴땐 안그랬는데 조금 컷어도 사실 전화했는데 안받으면 불안하긴해요ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:03"
        }
