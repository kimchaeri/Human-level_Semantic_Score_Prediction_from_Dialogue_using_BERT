      "utterance": [
        {
          "id": "MDRW2100039524.1.1",
          "form": "오늘이 마지막 병원 퇴근!",
          "original_form": "오늘이 마지막 병원 퇴근!",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100039524.1.2",
          "form": "여얼~",
          "original_form": "여얼~",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039524.1.3",
          "form": "이제 사대보험과 함께 하는구나",
          "original_form": "이제 사대보험과 함께 하는구나",
          "speaker_id": "2",
          "time": "20210903 18:42"
        },
        {
          "id": "MDRW2100039524.1.4",
          "form": "그건 다다음주 부터지만 암튼 그렇다!",
          "original_form": "그건 다다음주 부터지만 암튼 그렇다!",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039524.1.5",
          "form": "ㅋㅋㅋㅋ중요하지 보험...",
          "original_form": "ㅋㅋㅋㅋ중요하지 보험...",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100039524.1.6",
          "form": "이번 보험 받으면",
          "original_form": "이번 보험 받으면",
          "speaker_id": "1",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100039524.1.7",
          "form": "실업 급여 받는다...",
          "original_form": "실업 급여 받는다...",
          "speaker_id": "1",
          "time": "20210903 18:44"
        },
        {
          "id": "MDRW2100039524.1.8",
          "form": "나라가 주는 불로소득",
          "original_form": "나라가 주는 불로소득",
          "speaker_id": "2",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039524.1.9",
          "form": "좋아용",
          "original_form": "좋아용",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039524.1.10",
          "form": "이번에 머 3회 수급 이후로 주네 안주네",
          "original_form": "이번에 머 3회 수급 이후로 주네 안주네",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039524.1.11",
          "form": "어쩌고 하던데....",
          "original_form": "어쩌고 하던데....",
          "speaker_id": "1",
          "time": "20210903 18:45"
        },
        {
          "id": "MDRW2100039524.1.12",
          "form": "제대루 찾아바바",
          "original_form": "제대루 찾아바바",
          "speaker_id": "2",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100039524.1.13",
          "form": "다 잘 받아먹든데",
          "original_form": "다 잘 받아먹든데",
          "speaker_id": "2",
          "time": "20210903 18:47"
        },
        {
          "id": "MDRW2100039524.1.14",
          "form": "아니 바뀌네 마네 하는 것 같아서",
          "original_form": "아니 바뀌네 마네 하는 것 같아서",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100039524.1.15",
          "form": "난 해당 아니곸ㅋㅋㅋㅋㅋ",
          "original_form": "난 해당 아니곸ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100039524.1.16",
          "form": "조으네 잘 받아먹어",
          "original_form": "조으네 잘 받아먹어",
          "speaker_id": "2",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039524.1.17",
          "form": "조아용~",
          "original_form": "조아용~",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100039524.1.18",
          "form": "충대 일 없었으면 좋겠다",
          "original_form": "충대 일 없었으면 좋겠다",
          "speaker_id": "1",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100039524.1.19",
          "form": "알바한테 머 시킬게 있겠어",
          "original_form": "알바한테 머 시킬게 있겠어",
          "speaker_id": "2",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100039524.1.20",
          "form": "아님 처음에 사고 와당탕치면 안시켜",
          "original_form": "아님 처음에 사고 와당탕치면 안시켜",
          "speaker_id": "2",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100039524.1.21",
          "form": "세상에....",
          "original_form": "세상에....",
          "speaker_id": "1",
          "time": "20210903 18:55"
        },
        {
          "id": "MDRW2100039524.1.22",
          "form": "정말 계약직 때에만 쓸 수 있는 방법",
          "original_form": "정말 계약직 때에만 쓸 수 있는 방법",
          "speaker_id": "1",
          "time": "20210903 18:55"
        }
