      "utterance": [
        {
          "id": "MDRW2100018418.1.1",
          "form": "무슨음식을먹으면 가렵나요",
          "original_form": "무슨음식을먹으면 가렵나요",
          "speaker_id": "2",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018418.1.2",
          "form": "맛없는 음식이요",
          "original_form": "맛없는 음식이요",
          "speaker_id": "1",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018418.1.3",
          "form": "제 아는사람은 얼굴에 뭐가 빨갛게나더라고요",
          "original_form": "제 아는사람은 얼굴에 뭐가 빨갛게나더라고요",
          "speaker_id": "2",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018418.1.4",
          "form": "사실 음식 알러지는 잘 모르겠어요",
          "original_form": "사실 음식 알러지는 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018418.1.5",
          "form": "얼굴이 가렵기도하다고하고요",
          "original_form": "얼굴이 가렵기도하다고하고요",
          "speaker_id": "2",
          "time": "20210827 01:33"
        },
        {
          "id": "MDRW2100018418.1.6",
          "form": "전 스트레스 받으면 그렇게 되더라고요",
          "original_form": "전 스트레스 받으면 그렇게 되더라고요",
          "speaker_id": "1",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100018418.1.7",
          "form": "스파게티 좋아하세여?",
          "original_form": "스파게티 좋아하세여?",
          "speaker_id": "1",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100018418.1.8",
          "form": "저는 좋아해요",
          "original_form": "저는 좋아해요",
          "speaker_id": "2",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100018418.1.9",
          "form": "저는 내일 파스타를 먹고싶내요",
          "original_form": "저는 내일 파스타를 먹고싶내요",
          "speaker_id": "1",
          "time": "20210827 01:34"
        },
        {
          "id": "MDRW2100018418.1.10",
          "form": "오 저는 비빔국수나 먹을까 고민중이네요",
          "original_form": "오 저는 비빔국수나 먹을까 고민중이네요",
          "speaker_id": "2",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018418.1.11",
          "form": "하지만 국수를 먹어야 할지도 모르겠네요",
          "original_form": "하지만 국수를 먹어야 할지도 모르겠네요",
          "speaker_id": "1",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018418.1.12",
          "form": "무슨국수를 드시려고요",
          "original_form": "무슨국수를 드시려고요",
          "speaker_id": "2",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018418.1.13",
          "form": "김치국수요",
          "original_form": "김치국수요",
          "speaker_id": "1",
          "time": "20210827 01:35"
        },
        {
          "id": "MDRW2100018418.1.14",
          "form": "오.. 그렇군요 !! 너무 맛잇겟네요",
          "original_form": "오.. 그렇군요 !! 너무 맛잇겟네요",
          "speaker_id": "2",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018418.1.15",
          "form": "누가 만들어줬으면 좋겠내요",
          "original_form": "누가 만들어줬으면 좋겠내요",
          "speaker_id": "1",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018418.1.16",
          "form": "저도 오늘 그거 해먹었는데 맛잇더라고요",
          "original_form": "저도 오늘 그거 해먹었는데 맛잇더라고요",
          "speaker_id": "2",
          "time": "20210827 01:36"
        },
        {
          "id": "MDRW2100018418.1.17",
          "form": "김치말이국수요",
          "original_form": "김치말이국수요",
