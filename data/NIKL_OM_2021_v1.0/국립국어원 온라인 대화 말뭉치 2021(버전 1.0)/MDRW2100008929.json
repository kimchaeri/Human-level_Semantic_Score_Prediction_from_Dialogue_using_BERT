      "utterance": [
        {
          "id": "MDRW2100008929.1.1",
          "form": "요즘 제일 관심 분야가 어디세요",
          "original_form": "요즘 제일 관심 분야가 어디세요",
          "speaker_id": "2",
          "time": "20210822 19:44"
        },
        {
          "id": "MDRW2100008929.1.2",
          "form": "다이어트인거같아요",
          "original_form": "다이어트인거같아요",
          "speaker_id": "1",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100008929.1.3",
          "form": "오 저도 다이어트 자주하죠ㅋㅋ",
          "original_form": "오 저도 다이어트 자주하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100008929.1.4",
          "form": "요즘 성공적이신가요?",
          "original_form": "요즘 성공적이신가요?",
          "speaker_id": "2",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100008929.1.5",
          "form": "아뇨 이제 시작인거같아요ㅠㅠㅋㅋㅋ",
          "original_form": "아뇨 이제 시작인거같아요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100008929.1.6",
          "form": "ㅋㅋ 정해두신 목표가 있으세요?",
          "original_form": "ㅋㅋ 정해두신 목표가 있으세요?",
          "speaker_id": "2",
          "time": "20210822 19:46"
        },
        {
          "id": "MDRW2100008929.1.7",
          "form": "3키로 빼기가 목표인데 너무 먹고있어서 큰일이에요",
          "original_form": "3키로 빼기가 목표인데 너무 먹고있어서 큰일이에요",
          "speaker_id": "1",
          "time": "20210822 19:46"
        },
        {
          "id": "MDRW2100008929.1.8",
          "form": "ㅋㅋ입이 한번 터지면 막기힘들죠ㅜ",
          "original_form": "ㅋㅋ입이 한번 터지면 막기힘들죠ㅜ",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008929.1.9",
          "form": "저도 요즘 단게 많이 땡겨서요ㅜ",
          "original_form": "저도 요즘 단게 많이 땡겨서요ㅜ",
          "speaker_id": "2",
          "time": "20210822 19:47"
        },
        {
          "id": "MDRW2100008929.1.10",
          "form": "맞아요 다른것보다 단거 끊는게 힘들어요ㅠㅠ",
          "original_form": "맞아요 다른것보다 단거 끊는게 힘들어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 19:48"
        },
        {
          "id": "MDRW2100008929.1.11",
          "form": "저도 1년전에 4키로정도 빼고 지금 유지하다가 운동중이었는데",
          "original_form": "저도 1년전에 4키로정도 빼고 지금 유지하다가 운동중이었는데",
          "speaker_id": "2",
          "time": "20210822 19:48"
        },
        {
          "id": "MDRW2100008929.1.12",
          "form": "코로나로 헬스장을 못가서ㅠ",
          "original_form": "코로나로 헬스장을 못가서ㅠ",
          "speaker_id": "2",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008929.1.13",
          "form": "헬스장이 자꾸 열었다 닫았다 하니 몸도 엉망ㅜㅜㅜ",
          "original_form": "헬스장이 자꾸 열었다 닫았다 하니 몸도 엉망ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008929.1.14",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "2",
          "time": "20210822 19:49"
        },
        {
          "id": "MDRW2100008929.1.15",
          "form": "근육만들기 힘드네요ㅎㅎ",
          "original_form": "근육만들기 힘드네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008929.1.16",
          "form": "홈트는 너무 의지가 안생겨요ㅎㅎㅎ",
          "original_form": "홈트는 너무 의지가 안생겨요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008929.1.17",
          "form": "맞아요 홈트는진짜 아무나하는거 아니더라고요",
          "original_form": "맞아요 홈트는진짜 아무나하는거 아니더라고요",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008929.1.18",
          "form": "정말요...",
          "original_form": "정말요...",
