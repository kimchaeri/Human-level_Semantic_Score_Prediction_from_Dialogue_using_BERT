      "utterance": [
        {
          "id": "MDRW2100024520.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100024520.1.2",
          "form": "벌써 월말이네요 ㅋㅋㅋ",
          "original_form": "벌써 월말이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100024520.1.3",
          "form": "전 근무일지를 후딱 작성하고 있어요 2일치를 아직 안썼거든요",
          "original_form": "전 근무일지를 후딱 작성하고 있어요 2일치를 아직 안썼거든요",
          "speaker_id": "1",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100024520.1.4",
          "form": "8월을 잘 마무리하셨나요?",
          "original_form": "8월을 잘 마무리하셨나요?",
          "speaker_id": "1",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100024520.1.5",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100024520.1.6",
          "form": "소설 보다가 채팅을 늦게 확인 했어요ㅋㅋㅋ",
          "original_form": "소설 보다가 채팅을 늦게 확인 했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100024520.1.7",
          "form": "시간 참 빠르네요 벌써 8월 마지막 날...",
          "original_form": "시간 참 빠르네요 벌써 8월 마지막 날...",
          "speaker_id": "2",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100024520.1.8",
          "form": "근무일지는 다 쓰셨나요?",
          "original_form": "근무일지는 다 쓰셨나요?",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100024520.1.9",
          "form": "네 다 스고 마무리 했어요 ㅋㅋ",
          "original_form": "네 다 스고 마무리 했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100024520.1.10",
          "form": "고생하셨습니다~~",
          "original_form": "고생하셨습니다~~",
          "speaker_id": "2",
          "time": "20210831 20:55"
        },
        {
          "id": "MDRW2100024520.1.11",
          "form": "name2님도 고생하셨습니다 ㅋㅋ",
          "original_form": "&name2&님도 고생하셨습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100024520.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 어휴 일이 힘들거나 많은 건 아닌데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 어휴 일이 힘들거나 많은 건 아닌데",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100024520.1.13",
          "form": "일이라는 것만으로도 하기가 시르네여...",
          "original_form": "일이라는 것만으로도 하기가 시르네여...",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100024520.1.14",
          "form": "놀고 돈을 받으려면 옛날엔 건물주하랬는데",
          "original_form": "놀고 돈을 받으려면 옛날엔 건물주하랬는데",
          "speaker_id": "1",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100024520.1.15",
          "form": "지금은 주식과 코인도 ㅋㅋ많이 나오죠",
          "original_form": "지금은 주식과 코인도 ㅋㅋ많이 나오죠",
          "speaker_id": "1",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100024520.1.16",
          "form": "주식이나 코인은 팔기 전까진 내 돈이 아니고",
          "original_form": "주식이나 코인은 팔기 전까진 내 돈이 아니고",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100024520.1.17",
          "form": "변동성이 심해서...",
          "original_form": "변동성이 심해서...",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100024520.1.18",
          "form": "역시 건물주가 최고라고 생각함다",
          "original_form": "역시 건물주가 최고라고 생각함다",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100024520.1.19",
          "form": "고를 필요가 없이 다 가지는 목표를 잡자고요",
          "original_form": "고를 필요가 없이 다 가지는 목표를 잡자고요",
          "speaker_id": "1",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100024520.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그건... 그렇죠.",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그건... 그렇죠.",
          "speaker_id": "2",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100024520.1.21",
          "form": "님이 아프간 국민이어봐요 부동산? 코인이어야합니다",
          "original_form": "님이 아프간 국민이어봐요 부동산? 코인이어야합니다",
          "speaker_id": "1",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100024520.1.22",
          "form": "한국이면 부동산도 좋고요 ㅋㅋ",
          "original_form": "한국이면 부동산도 좋고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100024520.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 비교대상이 왜 아프간인가요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 비교대상이 왜 아프간인가요",
          "speaker_id": "2",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100024520.1.24",
          "form": "아프간도 인터넷은 될 테니 코인 거래는 가능하겠지만...",
          "original_form": "아프간도 인터넷은 될 테니 코인 거래는 가능하겠지만...",
          "speaker_id": "2",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024520.1.25",
          "form": "환전이 될까요?",
          "original_form": "환전이 될까요?",
          "speaker_id": "2",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024520.1.26",
          "form": "돈이 있으면 아프간은 뜨는 게 답인 거 같아요.",
          "original_form": "돈이 있으면 아프간은 뜨는 게 답인 거 같아요.",
          "speaker_id": "2",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024520.1.27",
          "form": "코인 지갑은 비번만 알면 되잖아요",
          "original_form": "코인 지갑은 비번만 알면 되잖아요",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024520.1.28",
          "form": "달러들고 도망다니는 대신 비번 외우고 도망다님 될것 같아요",
          "original_form": "달러들고 도망다니는 대신 비번 외우고 도망다님 될것 같아요",
          "speaker_id": "1",
          "time": "20210831 20:59"
        },
        {
          "id": "MDRW2100024520.1.29",
          "form": "ㅋㅋㅋㅋㅋ 코인지갑에 몇 억이 있는들 꺼내야 돈이잖아요",
          "original_form": "ㅋㅋㅋㅋㅋ 코인지갑에 몇 억이 있는들 꺼내야 돈이잖아요",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100024520.1.30",
          "form": "신카나 전자결제가 가능할 것 같지도 않고...",
