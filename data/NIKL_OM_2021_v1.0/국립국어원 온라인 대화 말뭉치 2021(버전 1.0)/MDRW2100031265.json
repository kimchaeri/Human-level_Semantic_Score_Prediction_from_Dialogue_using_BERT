      "utterance": [
        {
          "id": "MDRW2100031265.1.1",
          "form": "안녕하세요 요새 날씨가 좋죠?:)",
          "original_form": "안녕하세요 요새 날씨가 좋죠?:)",
          "speaker_id": "1",
          "time": "20210904 19:32"
        },
        {
          "id": "MDRW2100031265.1.2",
          "form": "가을장마라서ㅜㅜ",
          "original_form": "가을장마라서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 19:56"
        },
        {
          "id": "MDRW2100031265.1.3",
          "form": "오늘은 날씨 좋네요",
          "original_form": "오늘은 날씨 좋네요",
          "speaker_id": "2",
          "time": "20210904 19:56"
        },
        {
          "id": "MDRW2100031265.1.4",
          "form": "아 그렇네요 제가 오늘 날씨가 좋았다고 잠시 착각했네요 ㅎㅎㅎ..",
          "original_form": "아 그렇네요 제가 오늘 날씨가 좋았다고 잠시 착각했네요 ㅎㅎㅎ..",
          "speaker_id": "1",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100031265.1.5",
          "form": "이제 가을인가봐요",
          "original_form": "이제 가을인가봐요",
          "speaker_id": "2",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100031265.1.6",
          "form": "저녁에 선선하네요",
          "original_form": "저녁에 선선하네요",
          "speaker_id": "2",
          "time": "20210904 19:57"
        },
        {
          "id": "MDRW2100031265.1.7",
          "form": "네 그러게요! 올해는 작년이나 제작년에 비해서 미세먼지도 덜한것같아요",
          "original_form": "네 그러게요! 올해는 작년이나 제작년에 비해서 미세먼지도 덜한것같아요",
          "speaker_id": "1",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100031265.1.8",
          "form": "그건 좋은거같아요",
          "original_form": "그건 좋은거같아요",
          "speaker_id": "2",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100031265.1.9",
          "form": "맑은느낌!",
          "original_form": "맑은느낌!",
          "speaker_id": "2",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100031265.1.10",
          "form": "그쵸 ㅎㅎ! 오늘보니까 하늘도 높더라구요",
          "original_form": "그쵸 ㅎㅎ! 오늘보니까 하늘도 높더라구요",
          "speaker_id": "1",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031265.1.11",
          "form": "이제 슬슬 긴팔 입어야할듯요",
          "original_form": "이제 슬슬 긴팔 입어야할듯요",
          "speaker_id": "2",
          "time": "20210904 19:59"
        },
        {
          "id": "MDRW2100031265.1.12",
          "form": "네네 ㅎㅎ 그러고 이제 곧 패딩 꺼내야할것같아요",
          "original_form": "네네 ㅎㅎ 그러고 이제 곧 패딩 꺼내야할것같아요",
          "speaker_id": "1",
          "time": "20210904 20:00"
        },
        {
          "id": "MDRW2100031265.1.13",
          "form": "가을은 항상 짧아서 아쉬운 것 같습니당",
          "original_form": "가을은 항상 짧아서 아쉬운 것 같습니당",
          "speaker_id": "1",
          "time": "20210904 20:01"
        },
        {
          "id": "MDRW2100031265.1.14",
          "form": "맞아요 봄가을이 좀 길면 좋을텐데ㅠ",
          "original_form": "맞아요 봄가을이 좀 길면 좋을텐데ㅠ",
          "speaker_id": "2",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100031265.1.15",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100031265.1.16",
          "form": "그래도 다음주는 날씨가 맑네요!",
          "original_form": "그래도 다음주는 날씨가 맑네요!",
          "speaker_id": "1",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100031265.1.17",
          "form": "계속 좋았음 좋겠어요",
          "original_form": "계속 좋았음 좋겠어요",
          "speaker_id": "2",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100031265.1.18",
          "form": "외국보니 홍수났더라구요",
          "original_form": "외국보니 홍수났더라구요",
          "speaker_id": "2",
          "time": "20210904 21:23"
        },
        {
          "id": "MDRW2100031265.1.19",
          "form": "그러게요 이제 곧 추석인데 비가 좀 덜왔으면 좋겠네요.",
          "original_form": "그러게요 이제 곧 추석인데 비가 좀 덜왔으면 좋겠네요.",
          "speaker_id": "1",
          "time": "20210904 21:54"
        }
      ]
