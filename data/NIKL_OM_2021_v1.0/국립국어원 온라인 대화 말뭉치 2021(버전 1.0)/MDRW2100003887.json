      "utterance": [
        {
          "id": "MDRW2100003887.1.1",
          "form": "요즘 무슨 프로그램 보나요?",
          "original_form": "요즘 무슨 프로그램 보나요?",
          "speaker_id": "1",
          "time": "20210821 13:20"
        },
        {
          "id": "MDRW2100003887.1.2",
          "form": "드라마 자주보는거같아요",
          "original_form": "드라마 자주보는거같아요",
          "speaker_id": "2",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100003887.1.3",
          "form": "드라마 보시나요?",
          "original_form": "드라마 보시나요?",
          "speaker_id": "2",
          "time": "20210821 13:21"
        },
        {
          "id": "MDRW2100003887.1.4",
          "form": "드라마는 엄마가 보실때 같이 보는편이에요",
          "original_form": "드라마는 엄마가 보실때 같이 보는편이에요",
          "speaker_id": "1",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003887.1.5",
          "form": "요즘 딱히 볼만한게 없어요",
          "original_form": "요즘 딱히 볼만한게 없어요",
          "speaker_id": "1",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003887.1.6",
          "form": "자극적인게 너무 많아서",
          "original_form": "자극적인게 너무 많아서",
          "speaker_id": "2",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003887.1.7",
          "form": "피로도가 쌓이더라구요",
          "original_form": "피로도가 쌓이더라구요",
          "speaker_id": "2",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003887.1.8",
          "form": "그래서 힐링물들 찾아보는데 라켓소년단 재밌더라구요",
          "original_form": "그래서 힐링물들 찾아보는데 라켓소년단 재밌더라구요",
          "speaker_id": "2",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100003887.1.9",
          "form": "name1님은 어떤 프로 보시나요?",
          "original_form": "&name1&님은 어떤 프로 보시나요?",
          "speaker_id": "2",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003887.1.10",
          "form": "맞아요 요즘 자극적이고 막장이 많아요",
          "original_form": "맞아요 요즘 자극적이고 막장이 많아요",
          "speaker_id": "1",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003887.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003887.1.12",
          "form": "근데 엄마가 욕하면서 봐요 전 그냥 귀로 들으며 딴거 해요",
          "original_form": "근데 엄마가 욕하면서 봐요 전 그냥 귀로 들으며 딴거 해요",
          "speaker_id": "1",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003887.1.13",
          "form": "막장이라는게 그런거 같아요..욕하면서 찾아보게되는ㅎ",
          "original_form": "막장이라는게 그런거 같아요..욕하면서 찾아보게되는ㅎ",
          "speaker_id": "2",
          "time": "20210821 13:27"
        },
        {
          "id": "MDRW2100003887.1.14",
          "form": "그래도 요즘 도가 좀 지나친거 같아요",
          "original_form": "그래도 요즘 도가 좀 지나친거 같아요",
          "speaker_id": "2",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100003887.1.15",
          "form": "맞아요 아무리 19세 달아도 심하더라구요",
          "original_form": "맞아요 아무리 19세 달아도 심하더라구요",
          "speaker_id": "1",
          "time": "20210821 13:29"
        },
        {
          "id": "MDRW2100003887.1.16",
          "form": "이야기도 점점 산으로 가서 볼 맛이 없어요",
          "original_form": "이야기도 점점 산으로 가서 볼 맛이 없어요",
          "speaker_id": "1",
          "time": "20210821 13:30"
        },
        {
          "id": "MDRW2100003887.1.17",
          "form": "악마 판사도 초중반은 봤는데 이야기가 점점 어렵더라구요",
          "original_form": "악마 판사도 초중반은 봤는데 이야기가 점점 어렵더라구요",
          "speaker_id": "1",
          "time": "20210821 13:30"
        },
        {
          "id": "MDRW2100003887.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 13:31"
        },
        {
          "id": "MDRW2100003887.1.19",
          "form": "용두용미인 드라마가 잘 없는거같아요",
          "original_form": "용두용미인 드라마가 잘 없는거같아요",
          "speaker_id": "2",
          "time": "20210821 13:31"
        },
        {
          "id": "MDRW2100003887.1.20",
          "form": "약간 잘 가다가 옆으로 새버리는?",
          "original_form": "약간 잘 가다가 옆으로 새버리는?",
          "speaker_id": "2",
          "time": "20210821 13:31"
        },
        {
          "id": "MDRW2100003887.1.21",
          "form": "요즘 마무리가 이게 뭐지? 싶어서 맥이 없어요",
          "original_form": "요즘 마무리가 이게 뭐지? 싶어서 맥이 없어요",
          "speaker_id": "1",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100003887.1.22",
          "form": "맞아요 그래서 뭔가 사람들 흥미도 떨어지고 잘 안보게 되나봐요ㅠ",
          "original_form": "맞아요 그래서 뭔가 사람들 흥미도 떨어지고 잘 안보게 되나봐요ㅠ",
          "speaker_id": "2",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100003887.1.23",
          "form": "한 번 만들 때 잘 만들어주면 좋은데..",
          "original_form": "한 번 만들 때 잘 만들어주면 좋은데..",
          "speaker_id": "2",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100003887.1.24",
          "form": "그래서 요즘 뭔가 새로운 걸 보는게 부담스러운 것도 있는 것도 있어요",
          "original_form": "그래서 요즘 뭔가 새로운 걸 보는게 부담스러운 것도 있는 것도 있어요",
