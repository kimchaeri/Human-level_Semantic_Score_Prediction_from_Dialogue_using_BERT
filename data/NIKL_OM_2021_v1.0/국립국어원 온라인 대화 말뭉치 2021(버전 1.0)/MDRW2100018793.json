      "utterance": [
        {
          "id": "MDRW2100018793.1.1",
          "form": "아니 인디자인이라고 책 표지 디자인 하는거래",
          "original_form": "아니 인디자인이라고 책 표지 디자인 하는거래",
          "speaker_id": "2",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100018793.1.2",
          "form": "그걸 왜 우리가 해는지 모르겠는데",
          "original_form": "그걸 왜 우리가 해는지 모르겠는데",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018793.1.3",
          "form": "해야",
          "original_form": "해야",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018793.1.4",
          "form": "책표지까지디자인해?",
          "original_form": "책표지까지디자인해?",
          "speaker_id": "1",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018793.1.5",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018793.1.6",
          "form": "하래",
          "original_form": "하래",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018793.1.7",
          "form": "책표지디자인은",
          "original_form": "책표지디자인은",
          "speaker_id": "1",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018793.1.8",
          "form": "따로있지않아?",
          "original_form": "따로있지않아?",
          "speaker_id": "1",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018793.1.9",
          "form": "관련학과랑직업이?",
          "original_form": "관련학과랑직업이?",
          "speaker_id": "1",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018793.1.10",
          "form": "그니까 디자인과가 있는데 왜",
          "original_form": "그니까 디자인과가 있는데 왜",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018793.1.11",
          "form": "극작과가 우리가 오ㅑ?",
          "original_form": "극작과가 우리가 오ㅑ?",
          "speaker_id": "2",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100018793.1.12",
          "form": "아니 진심 왜?",
          "original_form": "아니 진심 왜?",
          "speaker_id": "2",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100018793.1.13",
          "form": "그니까 시나리오만 잘 쓰면 되지",
          "original_form": "그니까 시나리오만 잘 쓰면 되지",
          "speaker_id": "1",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100018793.1.14",
          "form": "에바라니까",
          "original_form": "에바라니까",
          "speaker_id": "2",
          "time": "20210827 12:14"
        },
        {
          "id": "MDRW2100018793.1.15",
          "form": "그러면전공중에디자인관련이있겠네?",
          "original_form": "그러면전공중에디자인관련이있겠네?",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018793.1.16",
          "form": "글쓰기 과목 커리큘럼에 인디자인이 포함되어 있대",
          "original_form": "글쓰기 과목 커리큘럼에 인디자인이 포함되어 있대",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100018793.1.17",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018793.1.18",
          "form": "진짜이것저것많이배우는듯",
          "original_form": "진짜이것저것많이배우는듯",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018793.1.19",
          "form": "정말부럽다",
          "original_form": "정말부럽다",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018793.1.20",
          "form": "디자인까지배우고",
          "original_form": "디자인까지배우고",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018793.1.21",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018793.1.22",
          "form": "너무 좋지...? 우리 과에 올래?",
          "original_form": "너무 좋지...? 우리 과에 올래?",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018793.1.23",
          "form": "이런 일이 있다니",
          "original_form": "이런 일이 있다니",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018793.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ내가듣기에는너무좋은과인듯",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ내가듣기에는너무좋은과인듯",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018793.1.25",
          "form": "너많이들어",
          "original_form": "너많이들어",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018793.1.26",
          "form": "정말부럽다",
          "original_form": "정말부럽다",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018793.1.27",
          "form": "이런 정말...",
          "original_form": "이런 정말...",
          "speaker_id": "2",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018793.1.28",
          "form": "근데잡긴잡음?",
          "original_form": "근데잡긴잡음?",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018793.1.29",
          "form": "아직도존버중?",
          "original_form": "아직도존버중?",
          "speaker_id": "1",
          "time": "20210827 12:21"
        }
      ]
    }
  ]
