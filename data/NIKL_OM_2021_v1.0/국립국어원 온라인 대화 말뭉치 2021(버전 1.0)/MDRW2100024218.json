      "utterance": [
        {
          "id": "MDRW2100024218.1.1",
          "form": "저녁 어떤거 드셨어요?",
          "original_form": "저녁 어떤거 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024218.1.2",
          "form": "저는 삼겹살 먹었습니당ㅎㅎ",
          "original_form": "저는 삼겹살 먹었습니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024218.1.3",
          "form": "크으 고기는 맛없는 날이 없을거에요",
          "original_form": "크으 고기는 맛없는 날이 없을거에요",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024218.1.4",
          "form": "틀림없어요",
          "original_form": "틀림없어요",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024218.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ인정입니다~",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ인정입니다~",
          "speaker_id": "2",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100024218.1.6",
          "form": "먹는 얘기하니깐",
          "original_form": "먹는 얘기하니깐",
          "speaker_id": "2",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100024218.1.7",
          "form": "간식땡기네요ㅋㅋㅋㅋ",
          "original_form": "간식땡기네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100024218.1.8",
          "form": "저두요ㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024218.1.9",
          "form": "지금 먹음 후회하겠죠ㅋㅋㄱ",
          "original_form": "지금 먹음 후회하겠죠ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024218.1.10",
          "form": "넼ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024218.1.11",
          "form": "저도 지금 참는 중이에요ㅎㅎㅎ",
          "original_form": "저도 지금 참는 중이에요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024218.1.12",
          "form": "ㅋㅋㅋㅋㅋ아까 먹을걸 그랬나봐요",
          "original_form": "ㅋㅋㅋㅋㅋ아까 먹을걸 그랬나봐요",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024218.1.13",
          "form": "흡",
          "original_form": "흡",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100024218.1.14",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024218.1.15",
          "form": "저는 지금 너무 레몬 케이크가 땡겨욬ㅋㅋㅋ",
          "original_form": "저는 지금 너무 레몬 케이크가 땡겨욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100024218.1.16",
          "form": "아..  밤에 레몬케이크라뇨..",
          "original_form": "아..  밤에 레몬케이크라뇨..",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024218.1.17",
          "form": "홍차랑 먹고싶네요ㅋㄲㅋ",
          "original_form": "홍차랑 먹고싶네요ㅋㄲㅋ",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024218.1.18",
          "form": "어우...",
          "original_form": "어우...",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024218.1.19",
          "form": "둘의 조합은 두말하면 잔소리죠ㅠㅠ",
          "original_form": "둘의 조합은 두말하면 잔소리죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024218.1.20",
          "form": "저 지금 눈감고 한입 먹었어요",
          "original_form": "저 지금 눈감고 한입 먹었어요",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024218.1.21",
          "form": "핳.. 낼 각이네요ㅋㅋㅋ",
          "original_form": "핳.. 낼 각이네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024218.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ저도요ㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024218.1.23",
          "form": "낼 밖에 나가면 사 와야겠어요ㅎㅎㅎ",
          "original_form": "낼 밖에 나가면 사 와야겠어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100024218.1.24",
          "form": "레몬케이크가 어디가 맛있는지 검색하고 자야겠어용ㅋㅋㄱ",
