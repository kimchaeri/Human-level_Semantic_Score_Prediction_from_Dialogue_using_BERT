      "utterance": [
        {
          "id": "MDRW2100017354.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 18:51"
        },
        {
          "id": "MDRW2100017354.1.2",
          "form": "네 안녕하세요!",
          "original_form": "네 안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 18:52"
        },
        {
          "id": "MDRW2100017354.1.3",
          "form": "공연 보시는 거 좋아하세요?",
          "original_form": "공연 보시는 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 18:52"
        },
        {
          "id": "MDRW2100017354.1.4",
          "form": "네 님은요?",
          "original_form": "네 님은요?",
          "speaker_id": "1",
          "time": "20210826 18:52"
        },
        {
          "id": "MDRW2100017354.1.5",
          "form": "저는 사실 공연파는 아니라",
          "original_form": "저는 사실 공연파는 아니라",
          "speaker_id": "2",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100017354.1.6",
          "form": "연말에나 연말기분 내러 다녔어요",
          "original_form": "연말에나 연말기분 내러 다녔어요",
          "speaker_id": "2",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017354.1.7",
          "form": "그렇군요 어떤 공연인가요?",
          "original_form": "그렇군요 어떤 공연인가요?",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017354.1.8",
          "form": "크리스마스때쯤에 하는 공연들",
          "original_form": "크리스마스때쯤에 하는 공연들",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017354.1.9",
          "form": "넬이나 윤하 이런 가수들 다녔어요",
          "original_form": "넬이나 윤하 이런 가수들 다녔어요",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017354.1.10",
          "form": "오 추억의 가수네요",
          "original_form": "오 추억의 가수네요",
          "speaker_id": "1",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017354.1.11",
          "form": "네ㅋㅋㅋ사실 안보러간지 꽤 되어서..",
          "original_form": "네ㅋㅋㅋ사실 안보러간지 꽤 되어서..",
          "speaker_id": "2",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017354.1.12",
          "form": "무슨 공연 보러다니셔요",
          "original_form": "무슨 공연 보러다니셔요",
          "speaker_id": "2",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017354.1.13",
          "form": "아이유랑 내한 보러 다녔어요",
          "original_form": "아이유랑 내한 보러 다녔어요",
          "speaker_id": "1",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017354.1.14",
          "form": "아 아이유 진짜 제가 제일 가고싶은 공연 중에 하난데",
          "original_form": "아 아이유 진짜 제가 제일 가고싶은 공연 중에 하난데",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017354.1.15",
          "form": "부럽다......내한은 뭐 보셨어요",
          "original_form": "부럽다......내한은 뭐 보셨어요",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017354.1.16",
          "form": "디엔씨이, 케이티페리 등등이요",
          "original_form": "디엔씨이, 케이티페리 등등이요",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017354.1.17",
          "form": "부지런히 다니셨네요! 전 수도권에 살진 않아서 잘 못 가겠어요",
          "original_form": "부지런히 다니셨네요! 전 수도권에 살진 않아서 잘 못 가겠어요",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017354.1.18",
          "form": "저도 지방살아요ㅎㅎ",
          "original_form": "저도 지방살아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:04"
