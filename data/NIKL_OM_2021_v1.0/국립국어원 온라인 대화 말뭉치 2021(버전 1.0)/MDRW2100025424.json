      "utterance": [
        {
          "id": "MDRW2100025424.1.1",
          "form": "연애하시나요~!?",
          "original_form": "연애하시나요~!?",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100025424.1.2",
          "form": "ㅋㅋ아뇨ㅋㅋ 연애중이신가요?",
          "original_form": "ㅋㅋ아뇨ㅋㅋ 연애중이신가요?",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100025424.1.3",
          "form": "아뇨! ㅋㅋㅋ솔로입니당",
          "original_form": "아뇨! ㅋㅋㅋ솔로입니당",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100025424.1.4",
          "form": "ㅋㅋ오래되셨나요??",
          "original_form": "ㅋㅋ오래되셨나요??",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100025424.1.5",
          "form": "좀 됐어요ㅋㅋㅋ",
          "original_form": "좀 됐어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100025424.1.6",
          "form": "ㅋㅋ저는 꽤 오래ㅋㅋ",
          "original_form": "ㅋㅋ저는 꽤 오래ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100025424.1.7",
          "form": "혼자 사는것도 힘든데 ㅋㅋ 남까지 신경쓰는건 못하겠어요",
          "original_form": "혼자 사는것도 힘든데 ㅋㅋ 남까지 신경쓰는건 못하겠어요",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025424.1.8",
          "form": "외로우신가요?",
          "original_form": "외로우신가요?",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025424.1.9",
          "form": "아뇨ㅋㅋ",
          "original_form": "아뇨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025424.1.10",
          "form": "아! 저 사실 비혼..\nㅋㅋ",
          "original_form": "아! 저 사실 비혼..\nㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025424.1.11",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025424.1.12",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025424.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025424.1.14",
          "form": "결혼...별로에요",
          "original_form": "결혼...별로에요",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025424.1.15",
          "form": "ㅋㅋ 저도 그냥 혼자 ㅋㅋ",
          "original_form": "ㅋㅋ 저도 그냥 혼자 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100025424.1.16",
          "form": "결혼안해서 인생망하는 경우는 없잖아요ㅋㅋ",
          "original_form": "결혼안해서 인생망하는 경우는 없잖아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100025424.1.17",
          "form": "그니깐요ㅋㅋ",
          "original_form": "그니깐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100025424.1.18",
          "form": "요즘 그래서",
          "original_form": "요즘 그래서",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100025424.1.19",
          "form": "반대는 있지만요ㅋㅋ",
          "original_form": "반대는 있지만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100025424.1.20",
          "form": "내집마련 땜에 고민이 많아요 ㅋ",
          "original_form": "내집마련 땜에 고민이 많아요 ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100025424.1.21",
          "form": "아... 그래서 저는 이 나이에도 부모님과....ㅜ",
          "original_form": "아... 그래서 저는 이 나이에도 부모님과....ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100025424.1.22",
          "form": "마흔되면 쫒아낼거라고ㅜㅠ",
          "original_form": "마흔되면 쫒아낼거라고ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 21:16"
        }
      ]
