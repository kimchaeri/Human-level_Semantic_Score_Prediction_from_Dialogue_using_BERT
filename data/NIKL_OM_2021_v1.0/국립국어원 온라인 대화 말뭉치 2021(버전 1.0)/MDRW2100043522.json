        },
        {
          "id": "MDRW2100043522.1.2",
          "form": "청국장 유목민이야",
          "original_form": "청국장 유목민이야",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043522.1.3",
          "form": "깊은맛 좋아해",
          "original_form": "깊은맛 좋아해",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043522.1.4",
          "form": "아짇 몰라 와사 먹어보고 알려줌 ㅋㅋㅋ",
          "original_form": "아짇 몰라 와사 먹어보고 알려줌 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043522.1.5",
          "form": "홈쇼핑에서 24팩삼",
          "original_form": "홈쇼핑에서 24팩삼",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043522.1.6",
          "form": "매운맛 12개 그냥 12개",
          "original_form": "매운맛 12개 그냥 12개",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043522.1.7",
          "form": "6만 9배건 인데 쿠폰이랑 청구할인해서 5.4에삼",
          "original_form": "6만 9배건 인데 쿠폰이랑 청구할인해서 5.4에삼",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043522.1.8",
          "form": "아 청국장에 밥비벼먹고싶더",
          "original_form": "아 청국장에 밥비벼먹고싶더",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043522.1.9",
          "form": "한덩이가 2,3인분인가",
          "original_form": "한덩이가 2,3인분인가",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043522.1.10",
          "form": "청국장에 두부 졸라마니 넣고싶어",
          "original_form": "청국장에 두부 졸라마니 넣고싶어",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043522.1.11",
          "form": "ㅇㅇ 그런듯",
          "original_form": "ㅇㅇ 그런듯",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043522.1.12",
          "form": "홀려서샀어 엄마랑",
          "original_form": "홀려서샀어 엄마랑",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100043522.1.13",
          "form": "그 두부작게넣어서 비벼먹으면 최고",
          "original_form": "그 두부작게넣어서 비벼먹으면 최고",
          "speaker_id": "2",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100043522.1.14",
          "form": "알쥐알쥐 ㅠㅠ 김에 싸먹으면..?",
          "original_form": "알쥐알쥐 ㅠㅠ 김에 싸먹으면..?",
