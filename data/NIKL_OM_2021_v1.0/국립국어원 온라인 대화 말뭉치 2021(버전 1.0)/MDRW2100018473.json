      "utterance": [
        {
          "id": "MDRW2100018473.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018473.1.2",
          "form": "반가워요 전 고양이 키우는 집사입니다 ㅎ",
          "original_form": "반가워요 전 고양이 키우는 집사입니다 ㅎ",
          "speaker_id": "1",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018473.1.3",
          "form": "우와 오양이 키우세요??",
          "original_form": "우와 오양이 키우세요??",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018473.1.4",
          "form": "고양이 너무 귀엽겠어요ㅠㅠㅠ 몇마리 키우시나영",
          "original_form": "고양이 너무 귀엽겠어요ㅠㅠㅠ 몇마리 키우시나영",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018473.1.5",
          "form": "한마리에요",
          "original_form": "한마리에요",
          "speaker_id": "1",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018473.1.6",
          "form": "코리안숏헤어 줄여서 코숏이라는 종이에요",
          "original_form": "코리안숏헤어 줄여서 코숏이라는 종이에요",
          "speaker_id": "1",
          "time": "20210827 02:29"
        },
        {
          "id": "MDRW2100018473.1.7",
          "form": "그렇구나~ 고양이 키우면 어때요? 후기 좀 들려주세요ㅋㅋ",
          "original_form": "그렇구나~ 고양이 키우면 어때요? 후기 좀 들려주세요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:29"
        },
        {
          "id": "MDRW2100018473.1.8",
          "form": "고양이 귀엽고 키우는 재미가 있어요",
          "original_form": "고양이 귀엽고 키우는 재미가 있어요",
          "speaker_id": "1",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018473.1.9",
          "form": "그런데 관리하기가 까다롭기는 해요",
          "original_form": "그런데 관리하기가 까다롭기는 해요",
          "speaker_id": "1",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018473.1.10",
          "form": "엇 진짜요...? 어떤 부분이 까다롭나요?",
          "original_form": "엇 진짜요...? 어떤 부분이 까다롭나요?",
          "speaker_id": "2",
          "time": "20210827 02:30"
        },
        {
          "id": "MDRW2100018473.1.11",
          "form": "고양이가 깔끔한 동물이기는 한데",
          "original_form": "고양이가 깔끔한 동물이기는 한데",
          "speaker_id": "1",
          "time": "20210827 02:31"
        },
        {
          "id": "MDRW2100018473.1.12",
          "form": "물을 싫어해서 씻기기 너무 힘들어요",
          "original_form": "물을 싫어해서 씻기기 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210827 02:31"
        },
        {
          "id": "MDRW2100018473.1.13",
          "form": "매번이 전쟁이에요ㅠ",
          "original_form": "매번이 전쟁이에요ㅠ",
          "speaker_id": "1",
          "time": "20210827 02:31"
        },
        {
          "id": "MDRW2100018473.1.14",
          "form": "아하... 고양이 물 닿는거 싫어한다는 이야기는 많이 들어봤어요",
          "original_form": "아하... 고양이 물 닿는거 싫어한다는 이야기는 많이 들어봤어요",
          "speaker_id": "2",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018473.1.15",
          "form": "전 예전에 강아지 키운적이 있는데",
          "original_form": "전 예전에 강아지 키운적이 있는데",
          "speaker_id": "2",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018473.1.16",
          "form": "저희집 강아지는 물을 너무 좋아해서 탈이었어요ㅋㅋㅋ",
          "original_form": "저희집 강아지는 물을 너무 좋아해서 탈이었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018473.1.17",
          "form": "아 그랬구나ㅎㅎ 그래도 강아지는 살가워서",
          "original_form": "아 그랬구나ㅎㅎ 그래도 강아지는 살가워서",
          "speaker_id": "1",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018473.1.18",
          "form": "궁금하네요 고양이랑 강아지 많이 키우는게 다르거든요",
          "original_form": "궁금하네요 고양이랑 강아지 많이 키우는게 다르거든요",
          "speaker_id": "1",
          "time": "20210827 02:33"
        },
        {
          "id": "MDRW2100018473.1.19",
          "form": "맞아요 그렇다더라구요 지금은 안키우지만",
          "original_form": "맞아요 그렇다더라구요 지금은 안키우지만",
          "speaker_id": "2",
          "time": "20210827 02:33"
        },
        {
          "id": "MDRW2100018473.1.20",
          "form": "반려동물과 함께한다는건 참 좋은 일같아요",
          "original_form": "반려동물과 함께한다는건 참 좋은 일같아요",
          "speaker_id": "2",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018473.1.21",
          "form": "맞아요 가슴이 따뜻해져요",
          "original_form": "맞아요 가슴이 따뜻해져요",
          "speaker_id": "1",
          "time": "20210827 02:34"
        }
      ]
