      "utterance": [
        {
          "id": "MDRW2100010500.1.1",
          "form": "홈앤쇼핑에서 낙지 방송하네",
          "original_form": "홈앤쇼핑에서 낙지 방송하네",
          "speaker_id": "2",
          "time": "20210823 19:30"
        },
        {
          "id": "MDRW2100010500.1.2",
          "form": "ㅎㅎ 맛있어 보여?",
          "original_form": "ㅎㅎ 맛있어 보여?",
          "speaker_id": "1",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010500.1.3",
          "form": "누적판매 1000만팩이래",
          "original_form": "누적판매 1000만팩이래",
          "speaker_id": "2",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010500.1.4",
          "form": "광고할 때는 다 그렇게 말하지",
          "original_form": "광고할 때는 다 그렇게 말하지",
          "speaker_id": "1",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010500.1.5",
          "form": "딩동 팬인데 홈쇼핑도 하나봐",
          "original_form": "딩동 팬인데 홈쇼핑도 하나봐",
          "speaker_id": "2",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010500.1.6",
          "form": "ㅎㅎ 맛있게 보이면 하나 사던지",
          "original_form": "ㅎㅎ 맛있게 보이면 하나 사던지",
          "speaker_id": "1",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010500.1.7",
          "form": "10팩에 54900원이면 싼건가? ㅎㅎ",
          "original_form": "10팩에 54900원이면 싼건가? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010500.1.8",
          "form": "잘 모르겠네 시세를 몰라서",
          "original_form": "잘 모르겠네 시세를 몰라서",
          "speaker_id": "1",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010500.1.9",
          "form": "앱결제하면 완전 싸게 살 수 있는데 고민중 ㅎㅎ",
          "original_form": "앱결제하면 완전 싸게 살 수 있는데 고민중 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:32"
        },
        {
          "id": "MDRW2100010500.1.10",
          "form": "얼마나 싸게 살 수 있는데?",
          "original_form": "얼마나 싸게 살 수 있는데?",
          "speaker_id": "1",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010500.1.11",
          "form": "5천원 넘게 저렴 ㅎㅎ",
          "original_form": "5천원 넘게 저렴 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010500.1.12",
          "form": "ㅎㅎ 고민되겠네",
          "original_form": "ㅎㅎ 고민되겠네",
          "speaker_id": "1",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010500.1.13",
          "form": "요즘 반찬할 거 없어서 하나 있으면 좋을듯",
          "original_form": "요즘 반찬할 거 없어서 하나 있으면 좋을듯",
