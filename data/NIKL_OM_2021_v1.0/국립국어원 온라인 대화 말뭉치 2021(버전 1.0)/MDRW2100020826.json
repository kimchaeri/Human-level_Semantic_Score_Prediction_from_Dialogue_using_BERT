      "utterance": [
        {
          "id": "MDRW2100020826.1.1",
          "form": "날이 좋으니 발라드가 생각나네요",
          "original_form": "날이 좋으니 발라드가 생각나네요",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020826.1.2",
          "form": "어떤 노래 생각나세요?",
          "original_form": "어떤 노래 생각나세요?",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020826.1.3",
          "form": "sg워너비 노래요!",
          "original_form": "sg워너비 노래요!",
          "speaker_id": "2",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020826.1.4",
          "form": "놀면뭐하니 영향인가봐요ㅋㅋ",
          "original_form": "놀면뭐하니 영향인가봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020826.1.5",
          "form": "아 ㅋㅋ 저 어렸을 때 엄마가 좋아했어요",
          "original_form": "아 ㅋㅋ 저 어렸을 때 엄마가 좋아했어요",
          "speaker_id": "1",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020826.1.6",
          "form": "라라라 명곡이죠",
          "original_form": "라라라 명곡이죠",
          "speaker_id": "1",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020826.1.7",
          "form": "맞아용ㅋㅋ",
          "original_form": "맞아용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100020826.1.8",
          "form": "라라라~  이게 맴돌아요ㅋㅋ",
          "original_form": "라라라~  이게 맴돌아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100020826.1.9",
          "form": "ㅋㅋㅋ근데 어떤 곡 생각나신거에요?",
          "original_form": "ㅋㅋㅋ근데 어떤 곡 생각나신거에요?",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020826.1.10",
          "form": "그대를 사랑합니다요!",
          "original_form": "그대를 사랑합니다요!",
          "speaker_id": "2",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100020826.1.11",
          "form": "아 그 곡도 좋죠 ㅎㅎ",
          "original_form": "아 그 곡도 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020826.1.12",
          "form": "요즘 워너비 활동 해요?",
          "original_form": "요즘 워너비 활동 해요?",
          "speaker_id": "1",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100020826.1.13",
          "form": "그건 잘 모르겠어요..!",
          "original_form": "그건 잘 모르겠어요..!",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100020826.1.14",
          "form": "음원사이트에 있길래 듣고있었거든요",
          "original_form": "음원사이트에 있길래 듣고있었거든요",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100020826.1.15",
          "form": "ㅠㅠ 옛날 가수들 한때 예능에 하나둘씩 나오던데",
          "original_form": "ㅠㅠ 옛날 가수들 한때 예능에 하나둘씩 나오던데",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020826.1.16",
          "form": "그렇게 반갑더라구요",
          "original_form": "그렇게 반갑더라구요",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020826.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100020826.1.18",
          "form": "아! 이 노래! 이러면서요ㅋㅋ",
          "original_form": "아! 이 노래! 이러면서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020826.1.19",
          "form": "그 추억의 노래 소환하는 티비프로그램 뭐였죠?",
          "original_form": "그 추억의 노래 소환하는 티비프로그램 뭐였죠?",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020826.1.20",
          "form": "10대 20대 30대 방청객 데려다놓고 하는거.",
          "original_form": "10대 20대 30대 방청객 데려다놓고 하는거.",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020826.1.21",
          "form": "예전에 무도에서 했던거일까요?!",
          "original_form": "예전에 무도에서 했던거일까요?!",
          "speaker_id": "2",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100020826.1.22",
          "form": "어.. 무도는 아니었구",
          "original_form": "어.. 무도는 아니었구",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020826.1.23",
          "form": "제목이 슈가? 뭐 그랬던 것 같ㅇ느데",
          "original_form": "제목이 슈가? 뭐 그랬던 것 같ㅇ느데",
          "speaker_id": "1",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100020826.1.24",
          "form": "음.. 뭘까요 궁금하네욥",
          "original_form": "음.. 뭘까요 궁금하네욥",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020826.1.25",
          "form": "아! 슈가맨 일까요?!",
          "original_form": "아! 슈가맨 일까요?!",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020826.1.26",
          "form": "알면 전구 키는?",
          "original_form": "알면 전구 키는?",
          "speaker_id": "2",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020826.1.27",
          "form": "엌ㅋㅋㅋㅋㅋ그거 맞아요!",
          "original_form": "엌ㅋㅋㅋㅋㅋ그거 맞아요!",
          "speaker_id": "1",
          "time": "20210828 14:52"
        },
        {
          "id": "MDRW2100020826.1.28",
          "form": "거기서도 반가운 노래들이 많이 나오던데용",
          "original_form": "거기서도 반가운 노래들이 많이 나오던데용",
          "speaker_id": "1",
          "time": "20210828 14:52"
        }
      ]
