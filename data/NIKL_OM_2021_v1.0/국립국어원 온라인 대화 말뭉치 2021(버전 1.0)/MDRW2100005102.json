      "utterance": [
        {
          "id": "MDRW2100005102.1.1",
          "form": "특히 고양이는 털이 공중에 날아다녀서",
          "original_form": "특히 고양이는 털이 공중에 날아다녀서",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005102.1.2",
          "form": "기관지가 안좋아지는거같더라구요",
          "original_form": "기관지가 안좋아지는거같더라구요",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005102.1.3",
          "form": "네 털도 많이 날리고",
          "original_form": "네 털도 많이 날리고",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005102.1.4",
          "form": "괜찮으세요?",
          "original_form": "괜찮으세요?",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005102.1.5",
          "form": "털관리 힘드시죠?",
          "original_form": "털관리 힘드시죠?",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005102.1.6",
          "form": "그러다 보니 도마뱀 키워요",
          "original_form": "그러다 보니 도마뱀 키워요",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005102.1.7",
          "form": "털이 없어서요",
          "original_form": "털이 없어서요",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005102.1.8",
          "form": "그냥 털과 함께 살아가요..",
          "original_form": "그냥 털과 함께 살아가요..",
          "speaker_id": "2",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005102.1.9",
          "form": "털밥먹구 털옷입구..",
          "original_form": "털밥먹구 털옷입구..",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005102.1.10",
          "form": "ㅎㅎ... 도마뱀 그런면에서 너무 부러워요..",
          "original_form": "ㅎㅎ... 도마뱀 그런면에서 너무 부러워요..",
          "speaker_id": "2",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005102.1.11",
          "form": "네 근데 비늘이 ㅋㅋ벗겨져요",
          "original_form": "네 근데 비늘이 ㅋㅋ벗겨져요",
          "speaker_id": "1",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100005102.1.12",
          "form": "벗겨지면 관리해줘야하나요?",
          "original_form": "벗겨지면 관리해줘야하나요?",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005102.1.13",
          "form": "목욕 온욕해줘야 해요",
          "original_form": "목욕 온욕해줘야 해요",
          "speaker_id": "1",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005102.1.14",
          "form": "안그럼 괴사해요 ㅜㅜ",
          "original_form": "안그럼 괴사해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005102.1.15",
          "form": "그부분이 굳으면 안돼요",
          "original_form": "그부분이 굳으면 안돼요",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005102.1.16",
          "form": "고양이는 빗질해주시죠",
          "original_form": "고양이는 빗질해주시죠",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005102.1.17",
          "form": "청소도 자주 하실것 같아요",
          "original_form": "청소도 자주 하실것 같아요",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005102.1.18",
          "form": "저희애처럼 도도하고 까칠한가요?",
          "original_form": "저희애처럼 도도하고 까칠한가요?",
          "speaker_id": "1",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005102.1.19",
          "form": "괴사라니!!! 진짜 신경써줘야겠네요 ㅠㅠ",
          "original_form": "괴사라니!!! 진짜 신경써줘야겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005102.1.20",
          "form": "저희집 고양이는 개냥이에요 ㅎㅎ",
          "original_form": "저희집 고양이는 개냥이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005102.1.21",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005102.1.22",
          "form": "애교도많고 사람품을 좋아해서 항상 같이자요",
          "original_form": "애교도많고 사람품을 좋아해서 항상 같이자요",
          "speaker_id": "2",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005102.1.23",
          "form": "좋으시겠어요",
          "original_form": "좋으시겠어요",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005102.1.24",
          "form": "아 부러워요",
          "original_form": "아 부러워요",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100005102.1.25",
          "form": "그래서 침대청소를.. 자주해야해요 ㅎㅎ",
          "original_form": "그래서 침대청소를.. 자주해야해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005102.1.26",
          "form": "어쩌다 저희집은 도마뱀이 상전 입니다",
          "original_form": "어쩌다 저희집은 도마뱀이 상전 입니다",
          "speaker_id": "1",
          "time": "20210821 20:37"
        },
        {
          "id": "MDRW2100005102.1.27",
          "form": "저희집도 고양이가 상전인걸요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저희집도 고양이가 상전인걸요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005102.1.28",
          "form": "돌봐줘야하니 어쩔수없나봐요 다들 ㅋㅋㅋ",
          "original_form": "돌봐줘야하니 어쩔수없나봐요 다들 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005102.1.29",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005102.1.30",
          "form": "전 앞으로 오래 살텐데 내가 잘 키울수 있을까 항상 걱정돼요",
          "original_form": "전 앞으로 오래 살텐데 내가 잘 키울수 있을까 항상 걱정돼요",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005102.1.31",
          "form": "아플때 말도 못하는데 대처못할까도 겁나고",
          "original_form": "아플때 말도 못하는데 대처못할까도 겁나고",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005102.1.32",
          "form": "도마뱀은 병원고 마땅치 않아요",
          "original_form": "도마뱀은 병원고 마땅치 않아요",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005102.1.33",
          "form": "아 아무래도 병원이 진짜 그렇겠어요",
          "original_form": "아 아무래도 병원이 진짜 그렇겠어요",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005102.1.34",
          "form": "파충류 전문 병원이 흔치않죠??",
          "original_form": "파충류 전문 병원이 흔치않죠??",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005102.1.35",
          "form": "네 지방이라 더 없어요",
          "original_form": "네 지방이라 더 없어요",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005102.1.36",
          "form": "저번에도 다쳤는데 자가치료해서 겨우 나았어요",
          "original_form": "저번에도 다쳤는데 자가치료해서 겨우 나았어요",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005102.1.37",
          "form": "가면 다리를 무조건 절단한대요",
          "original_form": "가면 다리를 무조건 절단한대요",
          "speaker_id": "1",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005102.1.38",
          "form": "헉.. 너무 무서운 소리에요 다리절단이라니 ㅜㅜㅜㅜ",
          "original_form": "헉.. 너무 무서운 소리에요 다리절단이라니 ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005102.1.39",
          "form": "자가치료라니 그것도 너무 대단해요",
          "original_form": "자가치료라니 그것도 너무 대단해요",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005102.1.40",
          "form": "네 저 막 울었어요",
          "original_form": "네 저 막 울었어요",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005102.1.41",
          "form": "고양이는 안아프죠?",
          "original_form": "고양이는 안아프죠?",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005102.1.42",
          "form": "다찾아보고 소독 수시로하고 약바르고했는데 재생됐어요",
          "original_form": "다찾아보고 소독 수시로하고 약바르고했는데 재생됐어요",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005102.1.43",
          "form": "살이 신기해요",
          "original_form": "살이 신기해요",
          "speaker_id": "1",
          "time": "20210821 20:43"
        }
      ]
