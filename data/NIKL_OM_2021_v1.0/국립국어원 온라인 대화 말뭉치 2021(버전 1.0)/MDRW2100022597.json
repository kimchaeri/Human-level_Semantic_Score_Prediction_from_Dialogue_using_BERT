      "utterance": [
        {
          "id": "MDRW2100022597.1.1",
          "form": "조거팬츠",
          "original_form": "조거팬츠",
          "speaker_id": "2",
          "time": "20210830 14:03"
        },
        {
          "id": "MDRW2100022597.1.2",
          "form": "존예",
          "original_form": "존예",
          "speaker_id": "2",
          "time": "20210830 14:03"
        },
        {
          "id": "MDRW2100022597.1.3",
          "form": "나도 하나 사고싶어",
          "original_form": "나도 하나 사고싶어",
          "speaker_id": "2",
          "time": "20210830 14:03"
        },
        {
          "id": "MDRW2100022597.1.4",
          "form": "안어울리는사람이",
          "original_form": "안어울리는사람이",
          "speaker_id": "2",
          "time": "20210830 14:04"
        },
        {
          "id": "MDRW2100022597.1.5",
          "form": "있을까?",
          "original_form": "있을까?",
          "speaker_id": "2",
          "time": "20210830 14:04"
        },
        {
          "id": "MDRW2100022597.1.6",
          "form": "오 너도 생각중이야?",
          "original_form": "오 너도 생각중이야?",
          "speaker_id": "1",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100022597.1.7",
          "form": "편하게 입어도",
          "original_form": "편하게 입어도",
          "speaker_id": "1",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100022597.1.8",
          "form": "패션패션해 보이는게",
          "original_form": "패션패션해 보이는게",
          "speaker_id": "1",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100022597.1.9",
          "form": "너무 좋을듯",
          "original_form": "너무 좋을듯",
          "speaker_id": "1",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100022597.1.10",
          "form": "카고조거 어떨지",
          "original_form": "카고조거 어떨지",
          "speaker_id": "1",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100022597.1.11",
          "form": "진지하게 고민해봤었어",
          "original_form": "진지하게 고민해봤었어",
          "speaker_id": "1",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100022597.1.12",
          "form": "운동할때 좋잖아",
          "original_form": "운동할때 좋잖아",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100022597.1.13",
          "form": "근데 뭔가 항상 그렇지만",
          "original_form": "근데 뭔가 항상 그렇지만",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100022597.1.14",
          "form": "나는 입어보고싶다는 생각만하고",
          "original_form": "나는 입어보고싶다는 생각만하고",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100022597.1.15",
          "form": "실제로 사지는않지",
          "original_form": "실제로 사지는않지",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100022597.1.16",
          "form": "너는 사봐",
          "original_form": "너는 사봐",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100022597.1.17",
          "form": "후깆8ㅁ",
          "original_form": "후깆8ㅁ",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100022597.1.18",
          "form": "후기좀",
          "original_form": "후기좀",
          "speaker_id": "2",
          "time": "20210831 10:26"
        },
        {
          "id": "MDRW2100022597.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100022597.1.20",
          "form": "베타테스터야?",
          "original_form": "베타테스터야?",
          "speaker_id": "1",
          "time": "20210831 19:00"
        },
        {
          "id": "MDRW2100022597.1.21",
          "form": "후기 좋으면",
          "original_form": "후기 좋으면",
          "speaker_id": "1",
          "time": "20210831 19:01"
        },
        {
          "id": "MDRW2100022597.1.22",
          "form": "살거야?",
          "original_form": "살거야?",
          "speaker_id": "1",
          "time": "20210831 19:01"
        },
        {
          "id": "MDRW2100022597.1.23",
          "form": "그럴 의향이 있지",
          "original_form": "그럴 의향이 있지",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100022597.1.24",
          "form": "의향은 있는데... 안살지도 모르고..ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "의향은 있는데... 안살지도 모르고..ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100022597.1.25",
          "form": "사는경우 : 조거팬츠가 무진장 세일해!",
          "original_form": "사는경우 : 조거팬츠가 무진장 세일해!",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100022597.1.26",
          "form": "아 근데 나 생각해보니까 우리집에 바지 너무 많아",
          "original_form": "아 근데 나 생각해보니까 우리집에 바지 너무 많아",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100022597.1.27",
          "form": "청바지가 8개였나",
          "original_form": "청바지가 8개였나",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100022597.1.28",
          "form": "그래서 좀 옷사는건 자제하고있어",
          "original_form": "그래서 좀 옷사는건 자제하고있어",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100022597.1.29",
          "form": "청바지 원툴러야?",
          "original_form": "청바지 원툴러야?",
          "speaker_id": "1",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100022597.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100022597.1.31",
          "form": "색깔별로 핏별로",
          "original_form": "색깔별로 핏별로",
          "speaker_id": "1",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100022597.1.32",
          "form": "따로있는건가",
          "original_form": "따로있는건가",
          "speaker_id": "1",
          "time": "20210901 10:37"
        },
        {
          "id": "MDRW2100022597.1.33",
          "form": "ㅋㅋㅋㅋㅋ아니 주변에서 안입는거 받다보니..",
          "original_form": "ㅋㅋㅋㅋㅋ아니 주변에서 안입는거 받다보니..",
          "speaker_id": "2",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100022597.1.34",
          "form": "그렇다고 전에입던걸 버리자니 아까워서",
          "original_form": "그렇다고 전에입던걸 버리자니 아까워서",
          "speaker_id": "2",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100022597.1.35",
          "form": "나름 핏별로 있지",
          "original_form": "나름 핏별로 있지",
          "speaker_id": "2",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100022597.1.36",
          "form": "중청 흑청연청",
          "original_form": "중청 흑청연청",
          "speaker_id": "2",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100022597.1.37",
          "form": "와이드 스키니 슬림핏",
          "original_form": "와이드 스키니 슬림핏",
          "speaker_id": "2",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100022597.1.38",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100022597.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:11"
        },
        {
          "id": "MDRW2100022597.1.40",
          "form": "아침마다",
          "original_form": "아침마다",
          "speaker_id": "1",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100022597.1.41",
          "form": "골라입기 하겠네",
          "original_form": "골라입기 하겠네",
          "speaker_id": "1",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100022597.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ사실 그렇지는 않은게",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ사실 그렇지는 않은게",
          "speaker_id": "2",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100022597.1.43",
          "form": "요즘은 와이드만입어",
          "original_form": "요즘은 와이드만입어",
          "speaker_id": "2",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100022597.1.44",
          "form": "입는것만 주구장창..",
          "original_form": "입는것만 주구장창..",
          "speaker_id": "2",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100022597.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100022597.1.46",
          "form": "왜냐면 제일 예뻐보이는걸입는게",
          "original_form": "왜냐면 제일 예뻐보이는걸입는게",
          "speaker_id": "2",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100022597.1.47",
          "form": "트렌디하시네요",
          "original_form": "트렌디하시네요",
          "speaker_id": "1",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100022597.1.48",
          "form": "와이드",
          "original_form": "와이드",
          "speaker_id": "1",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100022597.1.49",
          "form": "이득이라는걸 알아버렸거든",
          "original_form": "이득이라는걸 알아버렸거든",
          "speaker_id": "2",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100022597.1.50",
          "form": "와이드 입으면 편해?",
          "original_form": "와이드 입으면 편해?",
          "speaker_id": "1",
          "time": "20210901 12:12"
        },
        {
          "id": "MDRW2100022597.1.51",
          "form": "예쁘다고 아끼면 똥돼",
          "original_form": "예쁘다고 아끼면 똥돼",
          "speaker_id": "2",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100022597.1.52",
          "form": "편하지",
          "original_form": "편하지",
          "speaker_id": "2",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100022597.1.53",
          "form": "스키니는 무릎아파",
          "original_form": "스키니는 무릎아파",
          "speaker_id": "2",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100022597.1.54",
          "form": "와이드도 도전해볼라하는데",
          "original_form": "와이드도 도전해볼라하는데",
          "speaker_id": "1",
          "time": "20210901 12:13"
        }
      ]
