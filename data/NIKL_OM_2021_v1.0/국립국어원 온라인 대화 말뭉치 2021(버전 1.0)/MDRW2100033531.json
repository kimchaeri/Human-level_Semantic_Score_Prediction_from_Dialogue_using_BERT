      "utterance": [
        {
          "id": "MDRW2100033531.1.1",
          "form": "잠은 잘자는가",
          "original_form": "잠은 잘자는가",
          "speaker_id": "2",
          "time": "20210824 11:41"
        },
        {
          "id": "MDRW2100033531.1.2",
          "form": "어제는 좀 설침",
          "original_form": "어제는 좀 설침",
          "speaker_id": "1",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100033531.1.3",
          "form": "내성발톱에대해 뭐 아는거 있으심?",
          "original_form": "내성발톱에대해 뭐 아는거 있으심?",
          "speaker_id": "1",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100033531.1.4",
          "form": "오 실톱인가 그게 특효라던데",
          "original_form": "오 실톱인가 그게 특효라던데",
          "speaker_id": "2",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100033531.1.5",
          "form": "실톱으로 뭘해야하나여",
          "original_form": "실톱으로 뭘해야하나여",
          "speaker_id": "1",
          "time": "20210824 11:44"
        },
        {
          "id": "MDRW2100033531.1.6",
          "form": "갈아주면좋은 포인트가 있대",
          "original_form": "갈아주면좋은 포인트가 있대",
          "speaker_id": "2",
          "time": "20210824 11:44"
        },
        {
          "id": "MDRW2100033531.1.7",
          "form": "나는 어제 손톱깍기로",
          "original_form": "나는 어제 손톱깍기로",
          "speaker_id": "1",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100033531.1.8",
          "form": "잘랐음 저 안까시",
          "original_form": "잘랐음 저 안까시",
          "speaker_id": "1",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100033531.1.9",
          "form": "까지",
          "original_form": "까지",
          "speaker_id": "1",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100033531.1.10",
          "form": "안대ㅜㅜㅜㅜ",
          "original_form": "안대ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100033531.1.11",
          "form": "내성발톱이었구나",
          "original_form": "내성발톱이었구나",
          "speaker_id": "2",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100033531.1.12",
          "form": "그죠 잘못한거겠죠ㅜㅜ",
          "original_form": "그죠 잘못한거겠죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 11:45"
        },
        {
          "id": "MDRW2100033531.1.13",
          "form": "머가 잘못한거여 ㅜㅜ  병원가봤음?",
          "original_form": "머가 잘못한거여 ㅜㅜ  병원가봤음?",
          "speaker_id": "2",
          "time": "20210824 11:47"
        },
        {
          "id": "MDRW2100033531.1.14",
          "form": "병원가긴 병원비가ㅜ",
          "original_form": "병원가긴 병원비가ㅜ",
          "speaker_id": "1",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100033531.1.15",
          "form": "ㅜ 유툽이나 블로그에도 정보많더랑",
          "original_form": "ㅜ 유툽이나 블로그에도 정보많더랑",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100033531.1.16",
          "form": "막 죽을병동아니고 자력구제 해봐야겠네요",
          "original_form": "막 죽을병동아니고 자력구제 해봐야겠네요",
          "speaker_id": "1",
          "time": "20210824 11:57"
        },
        {
          "id": "MDRW2100033531.1.17",
          "form": "나는 발에 티눈생겨서 블로그보고 치료함",
          "original_form": "나는 발에 티눈생겨서 블로그보고 치료함",
          "speaker_id": "2",
          "time": "20210824 11:59"
        },
        {
          "id": "MDRW2100033531.1.18",
          "form": "오 글쿠나 난 일단 자라날때 어떻게 해야할지 한번 알아봐야겠음",
          "original_form": "오 글쿠나 난 일단 자라날때 어떻게 해야할지 한번 알아봐야겠음",
          "speaker_id": "1",
          "time": "20210824 13:37"
        }
      ]
    }
