          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028157.1.9",
          "form": "너라도 사지",
          "original_form": "너라도 사지",
          "speaker_id": "1",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028157.1.10",
          "form": "아 물어보고 살라그랬지 근데 다 잤잖아ㅡㅡ",
          "original_form": "아 물어보고 살라그랬지 근데 다 잤잖아ㅡㅡ",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028157.1.11",
          "form": "핫도그만 남아서 그거 샀다",
          "original_form": "핫도그만 남아서 그거 샀다",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028157.1.12",
          "form": "얼마였는데",
          "original_form": "얼마였는데",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028157.1.13",
          "form": "쿠폰할인받아서 삼천원",
          "original_form": "쿠폰할인받아서 삼천원",
          "speaker_id": "2",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028157.1.14",
          "form": "아 그럼 사야지 몇개사지",
          "original_form": "아 그럼 사야지 몇개사지",
          "speaker_id": "1",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028157.1.15",
          "form": "아이디당 하나여따",
          "original_form": "아이디당 하나여따",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028157.1.16",
          "form": "자기들이 자놓고ㅡㅡ",
          "original_form": "자기들이 자놓고ㅡㅡ",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028157.1.17",
          "form": "아깝네 어쩔수없지",
          "original_form": "아깝네 어쩔수없지",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100028157.1.18",
          "form": "냉장고에 넣을 자리는 있나?",
          "original_form": "냉장고에 넣을 자리는 있나?",
          "speaker_id": "2",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028157.1.19",
          "form": "만두 많이 먹어서 자리 좀 났어",
          "original_form": "만두 많이 먹어서 자리 좀 났어",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028157.1.20",
          "form": "그래 계속 만두 돌려먹고 만두국 끓이더니ㅋㅋㅋ",
          "original_form": "그래 계속 만두 돌려먹고 만두국 끓이더니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028157.1.21",
          "form": "다시마도 사야 되는데",
          "original_form": "다시마도 사야 되는데",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
