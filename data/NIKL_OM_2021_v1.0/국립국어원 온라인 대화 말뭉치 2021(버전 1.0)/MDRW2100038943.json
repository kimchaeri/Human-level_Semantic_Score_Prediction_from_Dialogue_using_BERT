      "utterance": [
        {
          "id": "MDRW2100038943.1.1",
          "form": "챙겨보는 프로그램 있으시나요?",
          "original_form": "챙겨보는 프로그램 있으시나요?",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100038943.1.2",
          "form": "바라는바다요",
          "original_form": "바라는바다요",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038943.1.3",
          "form": "처음 들어요~ 뭐하는 프로인가요?",
          "original_form": "처음 들어요~ 뭐하는 프로인가요?",
          "speaker_id": "1",
          "time": "20210903 07:13"
        },
        {
          "id": "MDRW2100038943.1.4",
          "form": "음악으로 힐링 하는 프로그램이요",
          "original_form": "음악으로 힐링 하는 프로그램이요",
          "speaker_id": "2",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100038943.1.5",
          "form": "진행자가 있나요?",
          "original_form": "진행자가 있나요?",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100038943.1.6",
          "form": "배우들이  카페하소 가수들이 노래 들려줘요 ㅎㅎ",
          "original_form": "배우들이  카페하소 가수들이 노래 들려줘요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100038943.1.7",
          "form": "배우는  저승사자씨. 도깨비신부님  등등",
          "original_form": "배우는  저승사자씨. 도깨비신부님  등등",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100038943.1.8",
          "form": "가수 는 윤종신  악뮤 수현   등등",
          "original_form": "가수 는 윤종신  악뮤 수현   등등",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100038943.1.9",
          "form": "언뜻 본것 같기도 한데요~",
          "original_form": "언뜻 본것 같기도 한데요~",
          "speaker_id": "1",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100038943.1.10",
          "form": "듣고 있음 힐링되요 ㅎㅎ",
          "original_form": "듣고 있음 힐링되요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100038943.1.11",
          "form": "다른 나라 가서 하는건가요?",
          "original_form": "다른 나라 가서 하는건가요?",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100038943.1.12",
          "form": "코로나때문에 ㅎㅎ",
          "original_form": "코로나때문에 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100038943.1.13",
          "form": "비긴어게인이 외국에서 햇엇죠 ㅎㅎ",
          "original_form": "비긴어게인이 외국에서 햇엇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100038943.1.14",
          "form": "제가 본건 비긴어게인이군요~ 박정현도 나오던것 같던데요",
          "original_form": "제가 본건 비긴어게인이군요~ 박정현도 나오던것 같던데요",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100038943.1.15",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100038943.1.16",
          "form": "바라는바다는 국내편이라고보면 될거 같아요",
          "original_form": "바라는바다는 국내편이라고보면 될거 같아요",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100038943.1.17",
          "form": "아~ 그럼 가수는 박정현은 안나오고요??",
          "original_form": "아~ 그럼 가수는 박정현은 안나오고요??",
          "speaker_id": "1",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100038943.1.18",
          "form": "네네   안나와서 아쉬움여",
          "original_form": "네네   안나와서 아쉬움여",
          "speaker_id": "2",
          "time": "20210903 22:44"
