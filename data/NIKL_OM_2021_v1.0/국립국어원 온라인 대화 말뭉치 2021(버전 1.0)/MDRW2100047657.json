      "utterance": [
        {
          "id": "MDRW2100047657.1.1",
          "form": "저희집에도 샘플이 가득 이써여ㅠㅠ",
          "original_form": "저희집에도 샘플이 가득 이써여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100047657.1.2",
          "form": "글쵸",
          "original_form": "글쵸",
          "speaker_id": "2",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100047657.1.3",
          "form": "그래도 샘플준다는 정보뜨면 신청하고있어요",
          "original_form": "그래도 샘플준다는 정보뜨면 신청하고있어요",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047657.1.4",
          "form": "근데 막상 저는 귀찮아서 그런지 샘플들을 잘 안바르다보니 ㅠㅠ 특히 비닐에 든것들요ㅠㅠ 그득그득해여",
          "original_form": "근데 막상 저는 귀찮아서 그런지 샘플들을 잘 안바르다보니 ㅠㅠ 특히 비닐에 든것들요ㅠㅠ 그득그득해여",
          "speaker_id": "1",
          "time": "20210827 00:11"
        },
        {
          "id": "MDRW2100047657.1.5",
          "form": "맞아요 비닐에든거",
          "original_form": "맞아요 비닐에든거",
          "speaker_id": "2",
          "time": "20210827 00:14"
        },
        {
          "id": "MDRW2100047657.1.6",
          "form": "저도 그득근득해요",
          "original_form": "저도 그득근득해요",
          "speaker_id": "2",
          "time": "20210827 00:15"
        },
        {
          "id": "MDRW2100047657.1.7",
          "form": "한번에 다바를수도  업고..ㅋㅋㅋ",
          "original_form": "한번에 다바를수도  업고..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:15"
        },
        {
          "id": "MDRW2100047657.1.8",
          "form": "한번씩 정리하면",
          "original_form": "한번씩 정리하면",
          "speaker_id": "2",
          "time": "20210827 00:15"
        },
        {
          "id": "MDRW2100047657.1.9",
          "form": "반버리게되요",
          "original_form": "반버리게되요",
          "speaker_id": "2",
          "time": "20210827 00:15"
        },
        {
          "id": "MDRW2100047657.1.10",
          "form": "저도 정리하면 버릴것 많을거 같네요ㅠㅠ",
          "original_form": "저도 정리하면 버릴것 많을거 같네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100047657.1.11",
          "form": "유통기한 얼마안남은건 몸에 막 발라서",
          "original_form": "유통기한 얼마안남은건 몸에 막 발라서",
          "speaker_id": "2",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100047657.1.12",
          "form": "소진해요",
          "original_form": "소진해요",
          "speaker_id": "2",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100047657.1.13",
          "form": "맞아요 듬뿍 발라서",
          "original_form": "맞아요 듬뿍 발라서",
          "speaker_id": "1",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100047657.1.14",
          "form": "듬뿍  맞아요",
          "original_form": "듬뿍  맞아요",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100047657.1.15",
          "form": "그렇게라도 써야할것같은 느낌 ㅋ",
          "original_form": "그렇게라도 써야할것같은 느낌 ㅋ",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100047657.1.16",
          "form": "좀전에 냉동실에 날짜지난떡 버렸어요ㅡㅜ",
          "original_form": "좀전에 냉동실에 날짜지난떡 버렸어요ㅡㅜ",
          "speaker_id": "1",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100047657.1.17",
          "form": "아...아까비라",
          "original_form": "아...아까비라",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100047657.1.18",
          "form": "냉동실은 진짜 놓치기쉬운거같아요",
          "original_form": "냉동실은 진짜 놓치기쉬운거같아요",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100047657.1.19",
          "form": "ㅜㅠ떡도 좋아하는데 냉동실들어가면 ㅠㅠ",
          "original_form": "ㅜㅠ떡도 좋아하는데 냉동실들어가면 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100047657.1.20",
          "form": "저도 떡 마니좋아해요 ㅎ",
          "original_form": "저도 떡 마니좋아해요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100047657.1.21",
          "form": "근데 기지떡 지난번에샀는데",
          "original_form": "근데 기지떡 지난번에샀는데",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100047657.1.22",
          "form": "후기들은 좋던데 ㅜㅠ",
          "original_form": "후기들은 좋던데 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100047657.1.23",
          "form": "맛이 없더라구요",
          "original_form": "맛이 없더라구요",
