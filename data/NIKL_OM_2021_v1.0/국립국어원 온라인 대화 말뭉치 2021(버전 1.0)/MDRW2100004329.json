      "utterance": [
        {
          "id": "MDRW2100004329.1.1",
          "form": "키우는 반려동물있으세요??",
          "original_form": "키우는 반려동물있으세요??",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004329.1.2",
          "form": "여기 있는 주제 하나씩 다해봐도 재밌겠다",
          "original_form": "여기 있는 주제 하나씩 다해봐도 재밌겠다",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004329.1.3",
          "form": "저는 물고기 키워요!",
          "original_form": "저는 물고기 키워요!",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004329.1.4",
          "form": "오!?? 신기하다",
          "original_form": "오!?? 신기하다",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004329.1.5",
          "form": "뭐 키우세요??",
          "original_form": "뭐 키우세요??",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004329.1.6",
          "form": "베타요!",
          "original_form": "베타요!",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004329.1.7",
          "form": "오 그 예쁘다는!",
          "original_form": "오 그 예쁘다는!",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.8",
          "form": "ㅎㅎㅎ 맞아요",
          "original_form": "ㅎㅎㅎ 맞아요",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.9",
          "form": "하지만 투어라서",
          "original_form": "하지만 투어라서",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.10",
          "form": "혼자둬야하는..",
          "original_form": "혼자둬야하는..",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.11",
          "form": "아아아아",
          "original_form": "아아아아",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.12",
          "form": "반려동물 키우시나용?",
          "original_form": "반려동물 키우시나용?",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.13",
          "form": "아뇨 저는 안키워요",
          "original_form": "아뇨 저는 안키워요",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.14",
          "form": "사실 살면서 한번도",
          "original_form": "사실 살면서 한번도",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.15",
          "form": "안키워봤는데",
          "original_form": "안키워봤는데",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.16",
          "form": "동물 좋아하시나요?",
          "original_form": "동물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.17",
          "form": "앗 그렇구나",
          "original_form": "앗 그렇구나",
          "speaker_id": "1",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.18",
          "form": "보는 것만 좋아해요..ㅋㅋㅋ",
          "original_form": "보는 것만 좋아해요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:04"
        },
        {
          "id": "MDRW2100004329.1.19",
          "form": "어렸을때 친구집 강아지가",
          "original_form": "어렸을때 친구집 강아지가",
          "speaker_id": "2",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004329.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 뭔지 알거같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아 뭔지 알거같아요",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004329.1.21",
          "form": "물어가지고..약간 무서워서 가까이는 못가고",
          "original_form": "물어가지고..약간 무서워서 가까이는 못가고",
