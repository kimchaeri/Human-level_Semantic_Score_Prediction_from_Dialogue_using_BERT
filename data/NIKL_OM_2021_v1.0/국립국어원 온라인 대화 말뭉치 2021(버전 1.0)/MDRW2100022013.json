      "utterance": [
        {
          "id": "MDRW2100022013.1.1",
          "form": "요즘 먹고 싶은 음료 있어?",
          "original_form": "요즘 먹고 싶은 음료 있어?",
          "speaker_id": "2",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100022013.1.2",
          "form": "오늘 카페 라떼 마시려고 했는데 깜박했어",
          "original_form": "오늘 카페 라떼 마시려고 했는데 깜박했어",
          "speaker_id": "1",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100022013.1.3",
          "form": "카페 라테 맛있지",
          "original_form": "카페 라테 맛있지",
          "speaker_id": "2",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100022013.1.4",
          "form": "깜빡했어",
          "original_form": "깜빡했어",
          "speaker_id": "1",
          "time": "20210829 00:54"
        },
        {
          "id": "MDRW2100022013.1.5",
          "form": "ㅋㅋ 그럴수 있지",
          "original_form": "ㅋㅋ 그럴수 있지",
          "speaker_id": "2",
          "time": "20210829 00:55"
        },
        {
          "id": "MDRW2100022013.1.6",
          "form": "내일은 잊어버리지 말고 먹어야지",
          "original_form": "내일은 잊어버리지 말고 먹어야지",
          "speaker_id": "1",
          "time": "20210829 00:55"
        },
        {
          "id": "MDRW2100022013.1.7",
          "form": "기억해둬",
          "original_form": "기억해둬",
          "speaker_id": "2",
          "time": "20210829 00:55"
        },
        {
          "id": "MDRW2100022013.1.8",
          "form": "근데 이게 까먹는 것도 까먹는 건데 타이밍을 자꾸 놓쳐",
          "original_form": "근데 이게 까먹는 것도 까먹는 건데 타이밍을 자꾸 놓쳐",
          "speaker_id": "1",
          "time": "20210829 00:55"
        },
        {
          "id": "MDRW2100022013.1.9",
          "form": "메모 같은거 해놔",
          "original_form": "메모 같은거 해놔",
          "speaker_id": "2",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100022013.1.10",
          "form": "근데 뭐 그걸 알람 맞춰 놀 수도 없고",
          "original_form": "근데 뭐 그걸 알람 맞춰 놀 수도 없고",
          "speaker_id": "1",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100022013.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:57"
        },
        {
          "id": "MDRW2100022013.1.12",
          "form": "그리고 나는 우유 들어간 음료 잘 못 마시면 탈라니까",
          "original_form": "그리고 나는 우유 들어간 음료 잘 못 마시면 탈라니까",
          "speaker_id": "1",
          "time": "20210829 00:58"
        },
        {
          "id": "MDRW2100022013.1.13",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "2",
          "time": "20210829 00:58"
        },
        {
          "id": "MDRW2100022013.1.14",
          "form": "녹차 별로 안 좋아하지?",
          "original_form": "녹차 별로 안 좋아하지?",
