      "utterance": [
        {
          "id": "MDRW2100003549.1.1",
          "form": "딸~ 어제 바이러스 영화 제목이 뭔가?",
          "original_form": "딸~ 어제 바이러스 영화 제목이 뭔가?",
          "speaker_id": "2",
          "time": "20210821 00:58"
        },
        {
          "id": "MDRW2100003549.1.2",
          "form": "컨테이져 이요~",
          "original_form": "컨테이져 이요~",
          "speaker_id": "1",
          "time": "20210821 00:58"
        },
        {
          "id": "MDRW2100003549.1.3",
          "form": "그거 넷플릭스에 있어?",
          "original_form": "그거 넷플릭스에 있어?",
          "speaker_id": "2",
          "time": "20210821 00:58"
        },
        {
          "id": "MDRW2100003549.1.4",
          "form": "어.. 있을수도 있고 없을수도 잇고~",
          "original_form": "어.. 있을수도 있고 없을수도 잇고~",
          "speaker_id": "1",
          "time": "20210821 00:58"
        },
        {
          "id": "MDRW2100003549.1.5",
          "form": "어?! 넷플릭스에 없네요..",
          "original_form": "어?! 넷플릭스에 없네요..",
          "speaker_id": "1",
          "time": "20210821 00:58"
        },
        {
          "id": "MDRW2100003549.1.6",
          "form": "그래? 아쉽네",
          "original_form": "그래? 아쉽네",
          "speaker_id": "2",
          "time": "20210821 00:59"
        },
        {
          "id": "MDRW2100003549.1.7",
          "form": "그거랑 비슷한 '인베이젼'은 있는데",
          "original_form": "그거랑 비슷한 '인베이젼'은 있는데",
          "speaker_id": "1",
          "time": "20210821 00:59"
        },
        {
          "id": "MDRW2100003549.1.8",
          "form": "이건 외계인이 나오는 거라서... 그닥",
          "original_form": "이건 외계인이 나오는 거라서... 그닥",
          "speaker_id": "1",
          "time": "20210821 00:59"
        },
        {
          "id": "MDRW2100003549.1.9",
          "form": "외계인은 별로..",
          "original_form": "외계인은 별로..",
          "speaker_id": "2",
          "time": "20210821 00:59"
        },
        {
          "id": "MDRW2100003549.1.10",
          "form": "스토리도 좀 별로에요..",
          "original_form": "스토리도 좀 별로에요..",
          "speaker_id": "1",
          "time": "20210821 00:59"
        },
        {
          "id": "MDRW2100003549.1.11",
          "form": "컨테이젼은 괜찮은 영화인데",
          "original_form": "컨테이젼은 괜찮은 영화인데",
          "speaker_id": "1",
          "time": "20210821 00:59"
        },
        {
          "id": "MDRW2100003549.1.12",
          "form": "나중에 또 나오겠지",
          "original_form": "나중에 또 나오겠지",
          "speaker_id": "2",
          "time": "20210821 00:59"
        },
        {
          "id": "MDRW2100003549.1.13",
          "form": "그때 보면 되~",
          "original_form": "그때 보면 되~",
          "speaker_id": "2",
          "time": "20210821 00:59"
        },
        {
          "id": "MDRW2100003549.1.14",
          "form": "그거 나온지 꽤 되서 넷플릭스에 있을 줄 알았는데",
          "original_form": "그거 나온지 꽤 되서 넷플릭스에 있을 줄 알았는데",
          "speaker_id": "1",
          "time": "20210821 01:00"
        },
        {
          "id": "MDRW2100003549.1.15",
          "form": "없네요..",
          "original_form": "없네요..",
          "speaker_id": "1",
          "time": "20210821 01:00"
        },
        {
          "id": "MDRW2100003549.1.16",
          "form": "다른 거 보면 되",
          "original_form": "다른 거 보면 되",
          "speaker_id": "2",
          "time": "20210821 01:00"
        },
        {
          "id": "MDRW2100003549.1.17",
          "form": "찾아보면 다른 거 많을 거에요~",
          "original_form": "찾아보면 다른 거 많을 거에요~",
          "speaker_id": "1",
          "time": "20210821 01:00"
        }
      ]
