        {
          "id": "MDRW2100019023.1.1",
          "form": "저 곱창이요",
          "original_form": "저 곱창이요",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019023.1.2",
          "form": "소? 돼지?",
          "original_form": "소? 돼지?",
          "speaker_id": "1",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019023.1.3",
          "form": "아 당연히 소죠",
          "original_form": "아 당연히 소죠",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019023.1.4",
          "form": "전 장기류는 별로...",
          "original_form": "전 장기류는 별로...",
          "speaker_id": "1",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019023.1.5",
          "form": "뭐 좋아하세요",
          "original_form": "뭐 좋아하세요",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019023.1.6",
          "form": "닭발은 먹을 수있어요",
          "original_form": "닭발은 먹을 수있어요",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019023.1.7",
          "form": "크 닭발",
          "original_form": "크 닭발",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019023.1.8",
          "form": "비오니까 치즈쭈꾸미 먹고싶네요",
          "original_form": "비오니까 치즈쭈꾸미 먹고싶네요",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019023.1.9",
          "form": "ㅠㅠ소주도요",
          "original_form": "ㅠㅠ소주도요",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019023.1.10",
          "form": "라면 세개끓여서 다 먹고싶네요",
          "original_form": "라면 세개끓여서 다 먹고싶네요",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019023.1.11",
          "form": "푸드파이터시네요ㅋㅋ",
          "original_form": "푸드파이터시네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019023.1.12",
          "form": "그정도는 먹어줘얒ㅛ",
          "original_form": "그정도는 먹어줘얒ㅛ",
          "speaker_id": "1",
          "time": "20210827 14:44"
