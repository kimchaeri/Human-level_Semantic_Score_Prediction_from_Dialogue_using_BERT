      "utterance": [
        {
          "id": "MDRW2100014376.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014376.1.2",
          "form": "좋아하는 음식이나 맛집 있으신가요?",
          "original_form": "좋아하는 음식이나 맛집 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014376.1.3",
          "form": "전 초밥이요ㅋㅋㅋ",
          "original_form": "전 초밥이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:28"
        },
        {
          "id": "MDRW2100014376.1.4",
          "form": "초밥 저도 좋아해요!",
          "original_form": "초밥 저도 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014376.1.5",
          "form": "무슨 초밥 좋아하세요?",
          "original_form": "무슨 초밥 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014376.1.6",
          "form": "전 다좋해요!",
          "original_form": "전 다좋해요!",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100014376.1.7",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100014376.1.8",
          "form": "저도 안가리고 다좋아해요! 사시미도 좋아하고요",
          "original_form": "저도 안가리고 다좋아해요! 사시미도 좋아하고요",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014376.1.9",
          "form": "계란 초밥도 좋아해요",
          "original_form": "계란 초밥도 좋아해요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014376.1.10",
          "form": "ㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋ저도요",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100014376.1.11",
          "form": "전 초밥킬러예요",
          "original_form": "전 초밥킬러예요",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100014376.1.12",
          "form": "좋아하는 초밥집이 따로 있으신가요?",
          "original_form": "좋아하는 초밥집이 따로 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100014376.1.13",
          "form": "두루두루 다좋아해요ㅋㅋㅋㅋ",
          "original_form": "두루두루 다좋아해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100014376.1.14",
          "form": "연어vs광어?",
          "original_form": "연어vs광어?",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100014376.1.15",
          "form": "아 이렇게 어려운 질문을",
          "original_form": "아 이렇게 어려운 질문을",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100014376.1.16",
          "form": "예전엔 연어를 더좋아했는데 요즘은 광어가 땡기네요",
          "original_form": "예전엔 연어를 더좋아했는데 요즘은 광어가 땡기네요",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100014376.1.17",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100014376.1.18",
          "form": "전",
          "original_form": "전",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100014376.1.19",
          "form": "닥광어파입니다",
          "original_form": "닥광어파입니다",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100014376.1.20",
          "form": "뭔가 식감때문에 광어가 맛있는거같아요",
          "original_form": "뭔가 식감때문에 광어가 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100014376.1.21",
          "form": "연어는 회보다는 연어장이 더맛있더라구요",
          "original_form": "연어는 회보다는 연어장이 더맛있더라구요",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100014376.1.22",
          "form": "ㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100014376.1.23",
          "form": "연어장 맛있죠",
          "original_form": "연어장 맛있죠",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100014376.1.24",
          "form": "연어는 살짝 느끼해요",
          "original_form": "연어는 살짝 느끼해요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100014376.1.25",
          "form": "맞아요 그래서 그냥보다 훈제연어나 연어장으로 먹으면 좋더라구요",
