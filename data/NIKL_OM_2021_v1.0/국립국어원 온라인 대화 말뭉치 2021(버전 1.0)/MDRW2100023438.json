      "utterance": [
        {
          "id": "MDRW2100023438.1.1",
          "form": "도쿄요!",
          "original_form": "도쿄요!",
          "speaker_id": "2",
          "time": "20210830 02:30"
        },
        {
          "id": "MDRW2100023438.1.2",
          "form": "헉 거기 물가 비싸다고 들어서",
          "original_form": "헉 거기 물가 비싸다고 들어서",
          "speaker_id": "1",
          "time": "20210830 02:30"
        },
        {
          "id": "MDRW2100023438.1.3",
          "form": "이번 올림픽때 직접 가보고 싶었는데 코로나땜에ㅠㅠ",
          "original_form": "이번 올림픽때 직접 가보고 싶었는데 코로나땜에ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 02:30"
        },
        {
          "id": "MDRW2100023438.1.4",
          "form": "전 시도 못해봤는대",
          "original_form": "전 시도 못해봤는대",
          "speaker_id": "1",
          "time": "20210830 02:30"
        },
        {
          "id": "MDRW2100023438.1.5",
          "form": "어떠셨어요??",
          "original_form": "어떠셨어요??",
          "speaker_id": "1",
          "time": "20210830 02:30"
        },
        {
          "id": "MDRW2100023438.1.6",
          "form": "아아ㅠㅠㅠ",
          "original_form": "아아ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 02:30"
        },
        {
          "id": "MDRW2100023438.1.7",
          "form": "그래도 일본이 여행가기에 좋지않나여",
          "original_form": "그래도 일본이 여행가기에 좋지않나여",
          "speaker_id": "2",
          "time": "20210830 02:31"
        },
        {
          "id": "MDRW2100023438.1.8",
          "form": "물가는... 국내여행지도,,ㅠㅠㅠ",
          "original_form": "물가는... 국내여행지도,,ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 02:31"
        },
        {
          "id": "MDRW2100023438.1.9",
          "form": "네네ㅎㅎ 가깝고 생활권도 비슷하고",
          "original_form": "네네ㅎㅎ 가깝고 생활권도 비슷하고",
          "speaker_id": "1",
          "time": "20210830 02:31"
        },
        {
          "id": "MDRW2100023438.1.10",
          "form": "아 그렇긴하죠ㅎㅎ",
          "original_form": "아 그렇긴하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:31"
        },
        {
          "id": "MDRW2100023438.1.11",
          "form": "도쿄면 자유여행이셨겠네용",
          "original_form": "도쿄면 자유여행이셨겠네용",
          "speaker_id": "1",
          "time": "20210830 02:31"
        },
        {
          "id": "MDRW2100023438.1.12",
          "form": "네 친구랑 같이 일주일정도 갔다왓어요",
          "original_form": "네 친구랑 같이 일주일정도 갔다왓어요",
          "speaker_id": "2",
          "time": "20210830 02:32"
        },
        {
          "id": "MDRW2100023438.1.13",
          "form": "도쿄에 제일 유명한 관광지는 어디가 있을까요?",
          "original_form": "도쿄에 제일 유명한 관광지는 어디가 있을까요?",
          "speaker_id": "1",
          "time": "20210830 02:32"
        },
        {
          "id": "MDRW2100023438.1.14",
          "form": "그 도쿄타워?",
          "original_form": "그 도쿄타워?",
          "speaker_id": "2",
          "time": "20210830 02:32"
        },
        {
          "id": "MDRW2100023438.1.15",
          "form": "랑 스카이뭐시기 아닐까요",
          "original_form": "랑 스카이뭐시기 아닐까요",
          "speaker_id": "2",
          "time": "20210830 02:32"
        },
        {
          "id": "MDRW2100023438.1.16",
          "form": "아아!",
          "original_form": "아아!",
          "speaker_id": "1",
          "time": "20210830 02:32"
        },
        {
          "id": "MDRW2100023438.1.17",
          "form": "거기 저녁에 가보셨어용?",
          "original_form": "거기 저녁에 가보셨어용?",
          "speaker_id": "1",
          "time": "20210830 02:32"
        },
        {
          "id": "MDRW2100023438.1.18",
          "form": "네네 야경이 예쁘더라구요",
          "original_form": "네네 야경이 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210830 02:33"
        },
        {
          "id": "MDRW2100023438.1.19",
          "form": "우와앙ㅎㅎ 나중에 도쿄에도 꼭 놀러가봐야겠어용",
          "original_form": "우와앙ㅎㅎ 나중에 도쿄에도 꼭 놀러가봐야겠어용",
          "speaker_id": "1",
          "time": "20210830 02:33"
        },
        {
          "id": "MDRW2100023438.1.20",
          "form": "name1은 어디가보셨어요??",
          "original_form": "&name1&은 어디가보셨어요??",
