      "utterance": [
        {
          "id": "MDRW2100046049.1.1",
          "form": "뭐가요?",
          "original_form": "뭐가요?",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046049.1.2",
          "form": "점심시간 넉넉한거는",
          "original_form": "점심시간 넉넉한거는",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046049.1.3",
          "form": "좋은데",
          "original_form": "좋은데",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046049.1.4",
          "form": "세시에끝나면 뭐하기",
          "original_form": "세시에끝나면 뭐하기",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046049.1.5",
          "form": "애매하지않아요?",
          "original_form": "애매하지않아요?",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046049.1.6",
          "form": "전 학교다닐때 그런게젤싫던데",
          "original_form": "전 학교다닐때 그런게젤싫던데",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046049.1.7",
          "form": "음..어차피 온라인수업이라",
          "original_form": "음..어차피 온라인수업이라",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046049.1.8",
          "form": "잘모르겠어요",
          "original_form": "잘모르겠어요",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046049.1.9",
          "form": "아맞다",
          "original_form": "아맞다",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046049.1.10",
          "form": "화상수업이랬짘ㅋㅋㅋㅋ",
          "original_form": "화상수업이랬짘ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046049.1.11",
          "form": "그리고 공강인날도 있어서",
          "original_form": "그리고 공강인날도 있어서",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046049.1.12",
          "form": "괜차나요",
          "original_form": "괜차나요",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046049.1.13",
          "form": "저는진짜 1학년때",
          "original_form": "저는진짜 1학년때",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046049.1.14",
          "form": "공강꿈도못꿨어요",
          "original_form": "공강꿈도못꿨어요",
          "speaker_id": "1",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046049.1.15",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046049.1.16",
          "form": "시간표정해줬어요?",
          "original_form": "시간표정해줬어요?",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046049.1.17",
          "form": "그건 아닌데",
          "original_form": "그건 아닌데",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046049.1.18",
          "form": "뭔가 제가듣고싶은수업을 들으면",
          "original_form": "뭔가 제가듣고싶은수업을 들으면",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046049.1.19",
          "form": "공강이 안되더라고요ㅠ",
          "original_form": "공강이 안되더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046049.1.20",
          "form": "아.....저는 다행히",
          "original_form": "아.....저는 다행히",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046049.1.21",
          "form": "되더라고요",
          "original_form": "되더라고요",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046049.1.22",
          "form": "금요일 공강이",
          "original_form": "금요일 공강이",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046049.1.23",
          "form": "와!!!!미쳐따",
          "original_form": "와!!!!미쳐따",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046049.1.24",
          "form": "금공강 꿈의 금공강",
          "original_form": "금공강 꿈의 금공강",
          "speaker_id": "1",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046049.1.25",
          "form": "ㅋㅋㅋ친구들도 그러던데",
          "original_form": "ㅋㅋㅋ친구들도 그러던데",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046049.1.26",
          "form": "부럽다고",
          "original_form": "부럽다고",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046049.1.27",
          "form": "당연하죠!",
          "original_form": "당연하죠!",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046049.1.28",
          "form": "금토일 쉴수있는데!!!",
          "original_form": "금토일 쉴수있는데!!!",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046049.1.29",
          "form": "근데 금요일 공강이어도",
          "original_form": "근데 금요일 공강이어도",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046049.1.30",
          "form": "쉬진못해요ㅜ",
          "original_form": "쉬진못해요ㅜ",
          "speaker_id": "2",
          "time": "20210908 09:56"
        }
      ]
    }
  ]
