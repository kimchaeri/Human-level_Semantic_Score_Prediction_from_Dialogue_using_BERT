      "utterance": [
        {
          "id": "MDRW2100029368.1.1",
          "form": "저 이번올림픽 당연히",
          "original_form": "저 이번올림픽 당연히",
          "speaker_id": "1",
          "time": "20210903 03:14"
        },
        {
          "id": "MDRW2100029368.1.2",
          "form": "눈물을 흘리며 즐겼답니다",
          "original_form": "눈물을 흘리며 즐겼답니다",
          "speaker_id": "1",
          "time": "20210903 03:14"
        },
        {
          "id": "MDRW2100029368.1.3",
          "form": "진짜요ㅜㅜㅜㅠㅜㅜ양궁 여배 체조 전부 챙겨봤어요",
          "original_form": "진짜요ㅜㅜㅜㅠㅜㅜ양궁 여배 체조 전부 챙겨봤어요",
          "speaker_id": "2",
          "time": "20210903 03:15"
        },
        {
          "id": "MDRW2100029368.1.4",
          "form": "배구가 그렇게 재밌는줄",
          "original_form": "배구가 그렇게 재밌는줄",
          "speaker_id": "1",
          "time": "20210903 03:15"
        },
        {
          "id": "MDRW2100029368.1.5",
          "form": "왜몰랐을까요ㅠ",
          "original_form": "왜몰랐을까요ㅠ",
          "speaker_id": "1",
          "time": "20210903 03:15"
        },
        {
          "id": "MDRW2100029368.1.6",
          "form": "그러니까요ㅜㅜㅠ 하이큐라고 배구 만화 볼때도",
          "original_form": "그러니까요ㅜㅜㅠ 하이큐라고 배구 만화 볼때도",
          "speaker_id": "2",
          "time": "20210903 03:17"
        },
        {
          "id": "MDRW2100029368.1.7",
          "form": "이정도로 벅차지는 않았는데",
          "original_form": "이정도로 벅차지는 않았는데",
          "speaker_id": "2",
          "time": "20210903 03:17"
        },
        {
          "id": "MDRW2100029368.1.8",
          "form": "만화보다 더 만화같았죠 이번경기들",
          "original_form": "만화보다 더 만화같았죠 이번경기들",
          "speaker_id": "1",
          "time": "20210903 03:17"
        },
        {
          "id": "MDRW2100029368.1.9",
          "form": "아정말 눈물콧물",
          "original_form": "아정말 눈물콧물",
          "speaker_id": "1",
          "time": "20210903 03:17"
        },
        {
          "id": "MDRW2100029368.1.10",
          "form": "다뺐죠,,,",
          "original_form": "다뺐죠,,,",
          "speaker_id": "1",
          "time": "20210903 03:17"
        },
        {
          "id": "MDRW2100029368.1.11",
          "form": "내내 마음 졸이면서",
          "original_form": "내내 마음 졸이면서",
          "speaker_id": "2",
          "time": "20210903 03:18"
        },
        {
          "id": "MDRW2100029368.1.12",
          "form": "양궁도,,,그렇게 재밌는 경기였다니,,,!",
          "original_form": "양궁도,,,그렇게 재밌는 경기였다니,,,!",
          "speaker_id": "1",
          "time": "20210903 03:18"
        },
        {
          "id": "MDRW2100029368.1.13",
          "form": "아 양궁 진짜ㅜㅜㅜㅜㅜㅠ",
          "original_form": "아 양궁 진짜ㅜㅜㅜㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 03:18"
        },
        {
          "id": "MDRW2100029368.1.14",
          "form": "그 로빈후드 화살보고",
          "original_form": "그 로빈후드 화살보고",
          "speaker_id": "2",
          "time": "20210903 03:18"
        },
        {
          "id": "MDRW2100029368.1.15",
          "form": "김안산은 화살을 찢는,,,!",
          "original_form": "김안산은 화살을 찢는,,,!",
          "speaker_id": "1",
          "time": "20210903 03:19"
        },
        {
          "id": "MDRW2100029368.1.16",
          "form": "맞아요 안산선수님 인스타ㅋㅋㅋㅋㅋㅋㅋㅋ그 화살 게다가 박물관에도 기증된다던데",
          "original_form": "맞아요 안산선수님 인스타ㅋㅋㅋㅋㅋㅋㅋㅋ그 화살 게다가 박물관에도 기증된다던데",
          "speaker_id": "2",
          "time": "20210903 03:20"
        },
        {
          "id": "MDRW2100029368.1.17",
          "form": "대존멋,,,",
          "original_form": "대존멋,,,",
          "speaker_id": "1",
          "time": "20210903 03:20"
        },
        {
          "id": "MDRW2100029368.1.18",
          "form": "진짜... 뒤집어놓으셨다...",
          "original_form": "진짜... 뒤집어놓으셨다...",
          "speaker_id": "2",
          "time": "20210903 03:20"
        },
        {
          "id": "MDRW2100029368.1.19",
