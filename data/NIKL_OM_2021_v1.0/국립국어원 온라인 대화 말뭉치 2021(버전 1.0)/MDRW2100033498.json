      "utterance": [
        {
          "id": "MDRW2100033498.1.1",
          "form": "저도요ㅠㅠ",
          "original_form": "저도요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 02:04"
        },
        {
          "id": "MDRW2100033498.1.2",
          "form": "담다가 4분지나서주문이ㅠㅠ",
          "original_form": "담다가 4분지나서주문이ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 02:04"
        },
        {
          "id": "MDRW2100033498.1.3",
          "form": "낼은 저녁에 쿠폰받아서 사려구요",
          "original_form": "낼은 저녁에 쿠폰받아서 사려구요",
          "speaker_id": "2",
          "time": "20210824 02:04"
        },
        {
          "id": "MDRW2100033498.1.4",
          "form": "꼭 성공해보아요ㅋㅋ",
          "original_form": "꼭 성공해보아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 02:05"
        },
        {
          "id": "MDRW2100033498.1.5",
          "form": "27일까진가 하니까 슈퍼클럽 열심히 쓰고 알뜰쇼핑해야겠어요ㅎ",
          "original_form": "27일까진가 하니까 슈퍼클럽 열심히 쓰고 알뜰쇼핑해야겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 02:05"
        },
        {
          "id": "MDRW2100033498.1.6",
          "form": "진짜슈퍼클럽 혜자인것 같아요",
          "original_form": "진짜슈퍼클럽 혜자인것 같아요",
          "speaker_id": "1",
          "time": "20210824 02:05"
        },
        {
          "id": "MDRW2100033498.1.7",
          "form": "배민도 복권인가 랜덤쿠폰주던데요",
          "original_form": "배민도 복권인가 랜덤쿠폰주던데요",
          "speaker_id": "2",
          "time": "20210824 02:05"
        },
        {
          "id": "MDRW2100033498.1.8",
          "form": "저는지방이라ㅠㅠ",
          "original_form": "저는지방이라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 02:06"
        },
        {
          "id": "MDRW2100033498.1.9",
          "form": "그거는 수도권만 되더라구요ㅋㅋ",
          "original_form": "그거는 수도권만 되더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 02:06"
        },
        {
          "id": "MDRW2100033498.1.10",
          "form": "아  배민 1만 되는듯요",
          "original_form": "아  배민 1만 되는듯요",
          "speaker_id": "2",
          "time": "20210824 02:06"
        },
        {
          "id": "MDRW2100033498.1.11",
          "form": "혹시 네이버카페 하시나요?",
          "original_form": "혹시 네이버카페 하시나요?",
          "speaker_id": "1",
          "time": "20210824 02:06"
        },
        {
          "id": "MDRW2100033498.1.12",
          "form": "할인정보 다 알고 계시네요ㅎ",
          "original_form": "할인정보 다 알고 계시네요ㅎ",
          "speaker_id": "2",
          "time": "20210824 02:06"
        },
        {
          "id": "MDRW2100033498.1.13",
          "form": "전 주로 카페서 정보얻어서",
          "original_form": "전 주로 카페서 정보얻어서",
          "speaker_id": "2",
          "time": "20210824 02:07"
        },
        {
          "id": "MDRW2100033498.1.14",
          "form": "저도요ㅋㅋ같은카페가 아닐까",
          "original_form": "저도요ㅋㅋ같은카페가 아닐까",
          "speaker_id": "1",
          "time": "20210824 02:07"
        },
        {
          "id": "MDRW2100033498.1.15",
          "form": "싶네여ㅋㅋ",
          "original_form": "싶네여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 02:07"
        },
        {
          "id": "MDRW2100033498.1.16",
          "form": "어디신가요?ㅎ",
          "original_form": "어디신가요?ㅎ",
          "speaker_id": "2",
          "time": "20210824 02:08"
        },
        {
          "id": "MDRW2100033498.1.17",
          "form": "현명한소비?? 아시나요?ㅋㅋ",
          "original_form": "현명한소비?? 아시나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 02:08"
        },
        {
          "id": "MDRW2100033498.1.18",
          "form": "엇ㅋ 저도 거기예요",
          "original_form": "엇ㅋ 저도 거기예요",
          "speaker_id": "2",
          "time": "20210824 02:09"
        },
        {
          "id": "MDRW2100033498.1.19",
          "form": "짠카페에서 옮겼어요",
          "original_form": "짠카페에서 옮겼어요",
          "speaker_id": "2",
          "time": "20210824 02:09"
        },
        {
          "id": "MDRW2100033498.1.20",
          "form": "왠지 느낌이 그랬어요~ 반가워요 ㅋㅋ여기서뵙다니",
          "original_form": "왠지 느낌이 그랬어요~ 반가워요 ㅋㅋ여기서뵙다니",
