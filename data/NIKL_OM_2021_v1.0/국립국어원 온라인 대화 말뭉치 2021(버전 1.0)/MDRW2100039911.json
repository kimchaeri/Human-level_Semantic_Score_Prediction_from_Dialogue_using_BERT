      "utterance": [
        {
          "id": "MDRW2100039911.1.1",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039911.1.2",
          "form": "네 지난주에요",
          "original_form": "네 지난주에요",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039911.1.3",
          "form": "맞으셨어요?",
          "original_form": "맞으셨어요?",
          "speaker_id": "2",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039911.1.4",
          "form": "2차까지 다 맞으셨나요?",
          "original_form": "2차까지 다 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039911.1.5",
          "form": "아뇨 1차요",
          "original_form": "아뇨 1차요",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039911.1.6",
          "form": "다맞으셨어요?",
          "original_form": "다맞으셨어요?",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039911.1.7",
          "form": "전 수요일에 2차까지 완료요^-^",
          "original_form": "전 수요일에 2차까지 완료요^-^",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039911.1.8",
          "form": "홀가분합니다^-^",
          "original_form": "홀가분합니다^-^",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039911.1.9",
          "form": "뭐로 맞으셨나요?",
          "original_form": "뭐로 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039911.1.10",
          "form": "아 너무 부럽습니다!!!",
          "original_form": "아 너무 부럽습니다!!!",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039911.1.11",
          "form": "화이자요",
          "original_form": "화이자요",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039911.1.12",
          "form": "괜찮으셨나요? 전 1차 맞고 엄청 잤어요",
          "original_form": "괜찮으셨나요? 전 1차 맞고 엄청 잤어요",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039911.1.13",
          "form": "아 전 팔이 좀아프더라고요\r\n이틀간",
          "original_form": "아 전 팔이 좀아프더라고요\r\n이틀간",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039911.1.14",
          "form": "그 이후엔 좋아요",
          "original_form": "그 이후엔 좋아요",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039911.1.15",
          "form": "2차 아프셨어요??",
          "original_form": "2차 아프셨어요??",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039911.1.16",
          "form": "아뇨~",
          "original_form": "아뇨~",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039911.1.17",
          "form": "맞을때만 칼로 후벼파는것 같았고 1차보다 나았어요 ㅎ",
          "original_form": "맞을때만 칼로 후벼파는것 같았고 1차보다 나았어요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039911.1.18",
          "form": "진짜요??? 칼 무섭네요!!!ㅜ",
          "original_form": "진짜요??? 칼 무섭네요!!!ㅜ",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039911.1.19",
          "form": "진짜 아팠어요 맞을때",
          "original_form": "진짜 아팠어요 맞을때",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100039911.1.20",
          "form": "11월에 2차 맞아요ㅜ 빨리 맞고싶어요!",
          "original_form": "11월에 2차 맞아요ㅜ 빨리 맞고싶어요!",
          "speaker_id": "2",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100039911.1.21",
          "form": "2차는 각오 하라던대",
          "original_form": "2차는 각오 하라던대",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100039911.1.22",
          "form": "이갓도 케바케인가봐요",
          "original_form": "이갓도 케바케인가봐요",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100039911.1.23",
          "form": "주사가 진짜 아프더라고요 저 주사 꽤나 맞아본 사람\r\n인데!!!",
          "original_form": "주사가 진짜 아프더라고요 저 주사 꽤나 맞아본 사람\r\n인데!!!",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100039911.1.24",
          "form": "전 완전 멀쩡.. ㅋ",
          "original_form": "전 완전 멀쩡.. ㅋ",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100039911.1.25",
          "form": "2차는 진짜 넘 아파요",
          "original_form": "2차는 진짜 넘 아파요",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100039911.1.26",
          "form": "뛰쳐 나갈뻔 했네요 ㅠ",
          "original_form": "뛰쳐 나갈뻔 했네요 ㅠ",
