      "utterance": [
        {
          "id": "MDRW2100021059.1.1",
          "form": "안먹어봤져",
          "original_form": "안먹어봤져",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021059.1.2",
          "form": "저기 예시에적힌 진로두꺼비는뭔지알아요?",
          "original_form": "저기 예시에적힌 진로두꺼비는뭔지알아요?",
          "speaker_id": "1",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021059.1.3",
          "form": "네네 진로 두꺼비 카톡으로 보여드릴가여?",
          "original_form": "네네 진로 두꺼비 카톡으로 보여드릴가여?",
          "speaker_id": "2",
          "time": "20210828 18:40"
        },
        {
          "id": "MDRW2100021059.1.4",
          "form": "네!!!",
          "original_form": "네!!!",
          "speaker_id": "1",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100021059.1.5",
          "form": "근데name2님은안쉬시나요",
          "original_form": "근데&name2&님은안쉬시나요",
          "speaker_id": "1",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100021059.1.6",
          "form": "ㅋㅋㅋㄱㄲ",
          "original_form": "ㅋㅋㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100021059.1.7",
          "form": "ㅋㅋㅋㅋㅋ쉬다 온거에요",
          "original_form": "ㅋㅋㅋㅋㅋ쉬다 온거에요",
          "speaker_id": "2",
          "time": "20210828 18:42"
        },
        {
          "id": "MDRW2100021059.1.8",
          "form": "대단합니다ㅋㅋㄱㅈㅋㅋㅋ",
          "original_form": "대단합니다ㅋㅋㄱㅈㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100021059.1.9",
          "form": "곰표 팝콘도있나요?",
          "original_form": "곰표 팝콘도있나요?",
          "speaker_id": "1",
          "time": "20210828 18:50"
        },
        {
          "id": "MDRW2100021059.1.10",
          "form": "혹시아세용?",
          "original_form": "혹시아세용?",
          "speaker_id": "1",
          "time": "20210828 18:50"
        },
        {
          "id": "MDRW2100021059.1.11",
          "form": "ㅋㅋㅋㅋㅋ네네 먹엇어요",
          "original_form": "ㅋㅋㅋㅋㅋ네네 먹엇어요",
          "speaker_id": "2",
          "time": "20210828 18:50"
        },
        {
          "id": "MDRW2100021059.1.12",
          "form": "편의점 잘 안가시는군여?",
          "original_form": "편의점 잘 안가시는군여?",
          "speaker_id": "2",
          "time": "20210828 18:50"
        },
        {
          "id": "MDRW2100021059.1.13",
          "form": "도배가엄청나신데요..?",
          "original_form": "도배가엄청나신데요..?",
          "speaker_id": "1",
          "time": "20210828 18:50"
        },
        {
          "id": "MDRW2100021059.1.14",
          "form": "무슨 도배요?",
          "original_form": "무슨 도배요?",
          "speaker_id": "2",
          "time": "20210828 18:52"
        },
        {
          "id": "MDRW2100021059.1.15",
          "form": "카톡보세요 무서워요ㄲㅋㅋㅋ",
          "original_form": "카톡보세요 무서워요ㄲㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:53"
        },
        {
          "id": "MDRW2100021059.1.16",
          "form": "진로감자칩 맛있겠어용...",
          "original_form": "진로감자칩 맛있겠어용...",
          "speaker_id": "1",
          "time": "20210828 18:53"
        },
        {
          "id": "MDRW2100021059.1.17",
          "form": "도배는제탓아니에요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "도배는제탓아니에요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:54"
        },
        {
          "id": "MDRW2100021059.1.18",
          "form": "진로는 하나도 안먹어봤오요",
          "original_form": "진로는 하나도 안먹어봤오요",
          "speaker_id": "2",
          "time": "20210828 18:55"
        },
        {
          "id": "MDRW2100021059.1.19",
          "form": "저는저렇게생겼다는것도첨알앗네용!",
          "original_form": "저는저렇게생겼다는것도첨알앗네용!",
          "speaker_id": "1",
          "time": "20210828 18:57"
        },
        {
          "id": "MDRW2100021059.1.20",
          "form": "근데맛있어보여요ㅋㅋㄱㅋ두꺼비진짜귀엽다",
          "original_form": "근데맛있어보여요ㅋㅋㄱㅋ두꺼비진짜귀엽다",
          "speaker_id": "1",
          "time": "20210828 18:57"
        },
        {
          "id": "MDRW2100021059.1.21",
          "form": "흐",
          "original_form": "흐",
          "speaker_id": "1",
          "time": "20210828 18:57"
        },
        {
          "id": "MDRW2100021059.1.22",
          "form": "그때무슨",
          "original_form": "그때무슨",
          "speaker_id": "1",
          "time": "20210828 18:57"
        },
        {
          "id": "MDRW2100021059.1.23",
          "form": "유성매직이랑 구두약그브랜드도먹을거나온다고안했었나용?",
          "original_form": "유성매직이랑 구두약그브랜드도먹을거나온다고안했었나용?",
          "speaker_id": "1",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100021059.1.24",
          "form": "마자여 나오다가 반대먹고 금방 없어진걸로 들엇어요",
          "original_form": "마자여 나오다가 반대먹고 금방 없어진걸로 들엇어요",
          "speaker_id": "2",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100021059.1.25",
          "form": "아!!!!",
          "original_form": "아!!!!",
          "speaker_id": "1",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100021059.1.26",
          "form": "결국반대먹고접은거구나.ㅈ",
          "original_form": "결국반대먹고접은거구나.ㅈ",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021059.1.27",
          "form": "좀심하다싶었는데",
          "original_form": "좀심하다싶었는데",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100021059.1.28",
          "form": "잘접었네요",
