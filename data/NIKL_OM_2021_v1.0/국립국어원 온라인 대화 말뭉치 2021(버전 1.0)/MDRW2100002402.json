      "utterance": [
        {
          "id": "MDRW2100002402.1.1",
          "form": "여름이라 모공이 넓어졌어요",
          "original_form": "여름이라 모공이 넓어졌어요",
          "speaker_id": "1",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002402.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002402.1.3",
          "form": "요새 이것저것 바르는데 효과없네요",
          "original_form": "요새 이것저것 바르는데 효과없네요",
          "speaker_id": "2",
          "time": "20210820 11:14"
        },
        {
          "id": "MDRW2100002402.1.4",
          "form": "정답은 피부과인데 그돈도 무시못하고",
          "original_form": "정답은 피부과인데 그돈도 무시못하고",
          "speaker_id": "1",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002402.1.5",
          "form": "나중되면 원상복귀 되더라고요",
          "original_form": "나중되면 원상복귀 되더라고요",
          "speaker_id": "1",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002402.1.6",
          "form": "혹 거긴 넘비싸요",
          "original_form": "혹 거긴 넘비싸요",
          "speaker_id": "2",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002402.1.7",
          "form": "저 전에 자가혈레이저했는데 그거하고 피부 망가졌어요",
          "original_form": "저 전에 자가혈레이저했는데 그거하고 피부 망가졌어요",
          "speaker_id": "1",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002402.1.8",
          "form": "잘못하면 망가져요 피부는 진짜 조심히 다뤄야해요",
          "original_form": "잘못하면 망가져요 피부는 진짜 조심히 다뤄야해요",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002402.1.9",
          "form": "맞아요 잘못하면 돈버리고 몸버려요",
          "original_form": "맞아요 잘못하면 돈버리고 몸버려요",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002402.1.10",
          "form": "저는 그때 망친이후로 피부과를 안가요",
          "original_form": "저는 그때 망친이후로 피부과를 안가요",
          "speaker_id": "1",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002402.1.11",
          "form": "돈 몸 정말 다버리고 돌아오지 않고",
          "original_form": "돈 몸 정말 다버리고 돌아오지 않고",
          "speaker_id": "2",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002402.1.12",
          "form": "네 피부과 돌팔이 많아요",
          "original_form": "네 피부과 돌팔이 많아요",
          "speaker_id": "2",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100002402.1.13",
          "form": "맞아요 그병원 결국 폐업했어요",
          "original_form": "맞아요 그병원 결국 폐업했어요",
          "speaker_id": "1",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002402.1.14",
          "form": "세상에 당했네요",
          "original_form": "세상에 당했네요",
          "speaker_id": "2",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002402.1.15",
          "form": "1회권 남아서 열받았는데 생각해보면 다행인가싶어요",
          "original_form": "1회권 남아서 열받았는데 생각해보면 다행인가싶어요",
          "speaker_id": "1",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002402.1.16",
          "form": "저도 예전에 돈만 버린적 있어요",
          "original_form": "저도 예전에 돈만 버린적 있어요",
          "speaker_id": "2",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002402.1.17",
          "form": "그거 받았음 피부 더 망가졌을듯해요",
          "original_form": "그거 받았음 피부 더 망가졌을듯해요",
          "speaker_id": "1",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002402.1.18",
          "form": "다행이예요 ㅜㅜ",
          "original_form": "다행이예요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002402.1.19",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "1",
          "time": "20210820 11:21"
        },
        {
          "id": "MDRW2100002402.1.20",
          "form": "먹튀들 진짜 짜증나요 ㅎㅎ",
          "original_form": "먹튀들 진짜 짜증나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002402.1.21",
          "form": "맞아 요 피부과 치과들",
          "original_form": "맞아 요 피부과 치과들",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100002402.1.22",
          "form": "최근에 가는 피부과는 잘맞아서 다행이예요",
          "original_form": "최근에 가는 피부과는 잘맞아서 다행이예요",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002402.1.23",
          "form": "자기가 맞는데 찾아야해요",
          "original_form": "자기가 맞는데 찾아야해요",
          "speaker_id": "2",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100002402.1.24",
          "form": "네 이번선생님은 미용보다는 치료목적이라 다행이예요",
          "original_form": "네 이번선생님은 미용보다는 치료목적이라 다행이예요",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100002402.1.25",
          "form": "피지가 안나게 막아주는 레이저도 했는데 무소용 ㅋㅋ",
          "original_form": "피지가 안나게 막아주는 레이저도 했는데 무소용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100002402.1.26",
          "form": "소용없죠 좋은 분이네요 치료가 시급하죠",
          "original_form": "소용없죠 좋은 분이네요 치료가 시급하죠",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100002402.1.27",
          "form": "네 미용 목적은 결국 다 돌아오는 것 같아요",
          "original_form": "네 미용 목적은 결국 다 돌아오는 것 같아요",
          "speaker_id": "1",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100002402.1.28",
          "form": "네 돌아오죠 피부도 계속 변하고",
          "original_form": "네 돌아오죠 피부도 계속 변하고",
          "speaker_id": "2",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100002402.1.29",
          "form": "전 지성 이였다 건성으로 변했어요",
          "original_form": "전 지성 이였다 건성으로 변했어요",
          "speaker_id": "2",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100002402.1.30",
          "form": "건성 힘들어요",
          "original_form": "건성 힘들어요",
          "speaker_id": "1",
          "time": "20210820 11:27"
        },
        {
          "id": "MDRW2100002402.1.31",
          "form": "전 그래서 매일 팩하려고 노력중이예요",
          "original_form": "전 그래서 매일 팩하려고 노력중이예요",
          "speaker_id": "1",
          "time": "20210820 11:27"
        },
        {
          "id": "MDRW2100002402.1.32",
          "form": "네 근데 전 팩은 못하겠어요",
          "original_form": "네 근데 전 팩은 못하겠어요",
          "speaker_id": "2",
          "time": "20210820 11:27"
        },
        {
          "id": "MDRW2100002402.1.33",
          "form": "뭔가 막고있음 불편해서 석고는 아예 못해요",
          "original_form": "뭔가 막고있음 불편해서 석고는 아예 못해요",
          "speaker_id": "2",
          "time": "20210820 11:28"
        }
      ]
