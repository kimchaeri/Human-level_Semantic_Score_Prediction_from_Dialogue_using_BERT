      "utterance": [
        {
          "id": "MDRW2100024600.1.1",
          "form": "점심은 드셨낭6?",
          "original_form": "점심은 드셨낭6?",
          "speaker_id": "1",
          "time": "20210831 15:16"
        },
        {
          "id": "MDRW2100024600.1.2",
          "form": "나요?",
          "original_form": "나요?",
          "speaker_id": "1",
          "time": "20210831 15:16"
        },
        {
          "id": "MDRW2100024600.1.3",
          "form": "네! 김치나베먹었어요",
          "original_form": "네! 김치나베먹었어요",
          "speaker_id": "2",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100024600.1.4",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100024600.1.5",
          "form": "저는 컵라면 먹었어요ㅋㅋ",
          "original_form": "저는 컵라면 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100024600.1.6",
          "form": "어떤 라면을 드셨나요??",
          "original_form": "어떤 라면을 드셨나요??",
          "speaker_id": "2",
          "time": "20210831 15:38"
        },
        {
          "id": "MDRW2100024600.1.7",
          "form": "그... 포치즈 불닭? 발견해서 먹어봤네요",
          "original_form": "그... 포치즈 불닭? 발견해서 먹어봤네요",
          "speaker_id": "1",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100024600.1.8",
          "form": "오잉 처음들어봐요 ㅋㅋㅋ",
          "original_form": "오잉 처음들어봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:01"
        },
        {
          "id": "MDRW2100024600.1.9",
          "form": "맛있었나요??",
          "original_form": "맛있었나요??",
          "speaker_id": "2",
          "time": "20210831 16:01"
        },
        {
          "id": "MDRW2100024600.1.10",
          "form": "불닭만큼 맵지는 않았어요 치즈들어갔을뿐인데 달아진게 좀 요상했고요ㅋㅋ 단거 좋아하는 편이 아니라서ㅎ",
          "original_form": "불닭만큼 맵지는 않았어요 치즈들어갔을뿐인데 달아진게 좀 요상했고요ㅋㅋ 단거 좋아하는 편이 아니라서ㅎ",
          "speaker_id": "1",
          "time": "20210831 16:02"
        },
        {
          "id": "MDRW2100024600.1.11",
          "form": "ㅋㅋㅋㅋ헉..전 단거 좋아해서 마싯을것 같아요 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ헉..전 단거 좋아해서 마싯을것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024600.1.12",
          "form": "전 단거 있음 잘 먹긴 하지만 짠게 좋아요ㅋㅋ 디저트류 좋아하시나요??",
          "original_form": "전 단거 있음 잘 먹긴 하지만 짠게 좋아요ㅋㅋ 디저트류 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100024600.1.13",
          "form": "마카롱 좋아해요!!",
          "original_form": "마카롱 좋아해요!!",
          "speaker_id": "2",
          "time": "20210831 17:48"
        },
        {
          "id": "MDRW2100024600.1.14",
          "form": "근데 가격이 좀 사악하죠...",
          "original_form": "근데 가격이 좀 사악하죠...",
          "speaker_id": "2",
          "time": "20210831 17:49"
        },
        {
          "id": "MDRW2100024600.1.15",
          "form": "저도 마카롱은 환장해요ㅋㅋㅋ 솔티캬라멜이랑 요거트맛이면 무조건 사요ㅋㅋ",
          "original_form": "저도 마카롱은 환장해요ㅋㅋㅋ 솔티캬라멜이랑 요거트맛이면 무조건 사요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:50"
        },
        {
          "id": "MDRW2100024600.1.16",
          "form": "저도 요거트맛 좋아해요",
          "original_form": "저도 요거트맛 좋아해요",
          "speaker_id": "2",
          "time": "20210906 10:35"
        },
        {
          "id": "MDRW2100024600.1.17",
          "form": "레몬맛도 있으면 삽니당 ㅎㅎ",
          "original_form": "레몬맛도 있으면 삽니당 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 10:35"
        },
        {
