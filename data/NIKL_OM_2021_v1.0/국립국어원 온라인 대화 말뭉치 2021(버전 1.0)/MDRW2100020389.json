      "utterance": [
        {
          "id": "MDRW2100020389.1.1",
          "form": "잘생겼죠 ㅎㅎ",
          "original_form": "잘생겼죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020389.1.2",
          "form": "저 원래 이민기 별로 안 좋아했는데",
          "original_form": "저 원래 이민기 별로 안 좋아했는데",
          "speaker_id": "1",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020389.1.3",
          "form": "나이먹을수록 잘 생겨지는거 같아요",
          "original_form": "나이먹을수록 잘 생겨지는거 같아요",
          "speaker_id": "1",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020389.1.4",
          "form": "맞아요 깊이가 생겼어요",
          "original_form": "맞아요 깊이가 생겼어요",
          "speaker_id": "2",
          "time": "20210828 02:37"
        },
        {
          "id": "MDRW2100020389.1.5",
          "form": "네 ㅋㅋ 그 조선명탐정에서도",
          "original_form": "네 ㅋㅋ 그 조선명탐정에서도",
          "speaker_id": "1",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020389.1.6",
          "form": "넘 잘생겨져서",
          "original_form": "넘 잘생겨져서",
          "speaker_id": "1",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020389.1.7",
          "form": "못알아봤어요",
          "original_form": "못알아봤어요",
          "speaker_id": "1",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020389.1.8",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020389.1.9",
          "form": "약간 퇴폐적인??",
          "original_form": "약간 퇴폐적인??",
          "speaker_id": "2",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020389.1.10",
          "form": "네 이번생은 처음이라에서도",
          "original_form": "네 이번생은 처음이라에서도",
          "speaker_id": "1",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020389.1.11",
          "form": "넘 좋았어요",
          "original_form": "넘 좋았어요",
          "speaker_id": "1",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020389.1.12",
          "form": "그건 못봤어요 ㅎ",
          "original_form": "그건 못봤어요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020389.1.13",
          "form": "전 거의 다 봐서ㅠㅋㅋㅋ",
          "original_form": "전 거의 다 봐서ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020389.1.14",
          "form": "그러신 것 같아요 ㅎㅎ",
          "original_form": "그러신 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 02:38"
        },
        {
          "id": "MDRW2100020389.1.15",
          "form": "근데 모두의 거짓말에선 연기 이상하네요",
          "original_form": "근데 모두의 거짓말에선 연기 이상하네요",
          "speaker_id": "1",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100020389.1.16",
          "form": "아 그래요?? ㅎㅎ",
          "original_form": "아 그래요?? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100020389.1.17",
          "form": "네 너무 어색해요..",
          "original_form": "네 너무 어색해요..",
          "speaker_id": "1",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100020389.1.18",
          "form": "근데 작가탓인것 같아요",
          "original_form": "근데 작가탓인것 같아요",
          "speaker_id": "1",
          "time": "20210828 02:39"
        },
        {
          "id": "MDRW2100020389.1.19",
          "form": "대사가 좀 이상한가보네요",
          "original_form": "대사가 좀 이상한가보네요",
