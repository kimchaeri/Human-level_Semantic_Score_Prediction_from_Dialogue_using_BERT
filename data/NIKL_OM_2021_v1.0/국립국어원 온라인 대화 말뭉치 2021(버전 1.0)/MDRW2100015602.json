      "utterance": [
        {
          "id": "MDRW2100015602.1.1",
          "form": "오늘 뭐드셨나요?ㅎㅎ",
          "original_form": "오늘 뭐드셨나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015602.1.2",
          "form": "캡슐은 어디꺼 사드시나요?",
          "original_form": "캡슐은 어디꺼 사드시나요?",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015602.1.3",
          "form": "오늘은 소고기국 먹었어요 ㅎ",
          "original_form": "오늘은 소고기국 먹었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015602.1.4",
          "form": "맛있겠네용 ㅎㅎ",
          "original_form": "맛있겠네용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015602.1.5",
          "form": "저는 오늘 피자먹었어용",
          "original_form": "저는 오늘 피자먹었어용",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015602.1.6",
          "form": "피자나라치킨공주 아세요?",
          "original_form": "피자나라치킨공주 아세요?",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015602.1.7",
          "form": "아뇨..맛있나요??",
          "original_form": "아뇨..맛있나요??",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015602.1.8",
          "form": "저도 시켜먹어봐야겠어요",
          "original_form": "저도 시켜먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015602.1.9",
          "form": "오늘 처음 먹어봤는데",
          "original_form": "오늘 처음 먹어봤는데",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015602.1.10",
          "form": "진짜 맛있어요ㅋㅋㅋ",
          "original_form": "진짜 맛있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015602.1.11",
          "form": "포테이토 피자 좋아하시면 꼭 드셔보세요",
          "original_form": "포테이토 피자 좋아하시면 꼭 드셔보세요",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015602.1.12",
          "form": "오 네넵!",
          "original_form": "오 네넵!",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015602.1.13",
          "form": "이름도 잼있어서 금방외웠어요",
          "original_form": "이름도 잼있어서 금방외웠어요",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015602.1.14",
          "form": "꼭 시켜볼께요",
          "original_form": "꼭 시켜볼께요",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015602.1.15",
          "form": "더블포테이토랑 반반치킨 세트로 먹으면 돼요ㅎㅎ",
          "original_form": "더블포테이토랑 반반치킨 세트로 먹으면 돼요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015602.1.16",
          "form": "가격이 엄청싸서 신기해요",
          "original_form": "가격이 엄청싸서 신기해요",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015602.1.17",
          "form": "얼마인가요?",
          "original_form": "얼마인가요?",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015602.1.18",
          "form": "금요일에 시켜야겠어요^^",
          "original_form": "금요일에 시켜야겠어요^^",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015602.1.19",
          "form": "세트로 19800원이에요ㅎㅎ",
          "original_form": "세트로 19800원이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015602.1.20",
          "form": "엄청싸죠?",
          "original_form": "엄청싸죠?",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015602.1.21",
          "form": "와 정말 착한 가격 이네요",
          "original_form": "와 정말 착한 가격 이네요",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015602.1.22",
          "form": "요즘 피자한판이 2000원이넘는데",
          "original_form": "요즘 피자한판이 2000원이넘는데",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015602.1.23",
          "form": "진짜 신기한 가격이에요ㅋㅋㅋ",
          "original_form": "진짜 신기한 가격이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015602.1.24",
          "form": "그렇네요 ㅎ 배고프네요",
          "original_form": "그렇네요 ㅎ 배고프네요",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015602.1.25",
          "form": "피자 얘기하니",
          "original_form": "피자 얘기하니",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100015602.1.26",
          "form": "딱 야식 땡길 시간이긴 하네요 ㅋㅋㅋ",
          "original_form": "딱 야식 땡길 시간이긴 하네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015602.1.27",
          "form": "야식 자주 드세요?",
          "original_form": "야식 자주 드세요?",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100015602.1.28",
          "form": "야식은 잘안먹어요 \r\n어느순간 속이 너무 안좋더라구요",
          "original_form": "야식은 잘안먹어요 \r\n어느순간 속이 너무 안좋더라구요",
          "speaker_id": "1",
          "time": "20210825 23:53"
        }
      ]
    }
