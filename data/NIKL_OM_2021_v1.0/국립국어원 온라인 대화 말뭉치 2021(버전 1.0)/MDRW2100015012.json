      "utterance": [
        {
          "id": "MDRW2100015012.1.1",
          "form": "엄마 고양이 키우고 싶다",
          "original_form": "엄마 고양이 키우고 싶다",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015012.1.2",
          "form": "고양이 같은 소리한다",
          "original_form": "고양이 같은 소리한다",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015012.1.3",
          "form": "왜 얼마나 귀여운데",
          "original_form": "왜 얼마나 귀여운데",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015012.1.4",
          "form": "털 날려서 안돼",
          "original_form": "털 날려서 안돼",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015012.1.5",
          "form": "귀여움으로 커버된다고ㅠㅠ",
          "original_form": "귀여움으로 커버된다고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015012.1.6",
          "form": "누가 돌보냐 고양이를",
          "original_form": "누가 돌보냐 고양이를",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015012.1.7",
          "form": "아 내가 할거라고ㅠㅜ",
          "original_form": "아 내가 할거라고ㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015012.1.8",
          "form": "귀엽긴 하더라",
          "original_form": "귀엽긴 하더라",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015012.1.9",
          "form": "그지? 한마리만 키우게 해줘 내가 다 알아서 하께",
          "original_form": "그지? 한마리만 키우게 해줘 내가 다 알아서 하께",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015012.1.10",
          "form": "똥싼거 니가 치울수있다고?",
          "original_form": "똥싼거 니가 치울수있다고?",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015012.1.11",
          "form": "치울수있다고 내가 한다고ㅠ",
          "original_form": "치울수있다고 내가 한다고ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015012.1.12",
          "form": "생각만해라 생각만",
          "original_form": "생각만해라 생각만",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015012.1.13",
          "form": "엄마 너무해ㅠㅠ",
          "original_form": "엄마 너무해ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:50"
