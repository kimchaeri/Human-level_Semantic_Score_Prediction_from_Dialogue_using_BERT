      "utterance": [
        {
          "id": "MDRW2100007328.1.1",
          "form": "가고 싶은 장소는?",
          "original_form": "가고 싶은 장소는?",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007328.1.2",
          "form": "미국",
          "original_form": "미국",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007328.1.3",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007328.1.4",
          "form": "미국? 나는 아이슬란드ㅋㅋㅋ",
          "original_form": "미국? 나는 아이슬란드ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007328.1.5",
          "form": "아 북한도 가보고 싶고",
          "original_form": "아 북한도 가보고 싶고",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007328.1.6",
          "form": "미국도 뭐 디즈니랜드 가보고 싶다",
          "original_form": "미국도 뭐 디즈니랜드 가보고 싶다",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007328.1.7",
          "form": "북한? 가서 뭐하려고??ㅋㅋㅋ",
          "original_form": "북한? 가서 뭐하려고??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007328.1.8",
          "form": "가서 보고싶어",
          "original_form": "가서 보고싶어",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007328.1.9",
          "form": "그냥 보고싶어",
          "original_form": "그냥 보고싶어",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007328.1.10",
          "form": "그냥 동네 구경?",
          "original_form": "그냥 동네 구경?",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007328.1.11",
          "form": "그 샘해밍턴의 페이스북 이란 다큐?프로? 있는데",
          "original_form": "그 샘해밍턴의 페이스북 이란 다큐?프로? 있는데",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007328.1.12",
          "form": "그런거?ㅇ",
          "original_form": "그런거?ㅇ",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007328.1.13",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007328.1.14",
          "form": "평양도 궁금하고",
          "original_form": "평양도 궁금하고",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007328.1.15",
          "form": "북한사람은 한국어쓰니까 그것도 궁금함",
          "original_form": "북한사람은 한국어쓰니까 그것도 궁금함",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007328.1.16",
          "form": "음식도 입에 맞을듯?",
          "original_form": "음식도 입에 맞을듯?",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007328.1.17",
          "form": "음식은 우리랑 같지 않으려나??ㅋㅋㅋㅋ",
          "original_form": "음식은 우리랑 같지 않으려나??ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007328.1.18",
          "form": "좀 다르지않을까? 지역마다 한국도 좀 다르자너",
          "original_form": "좀 다르지않을까? 지역마다 한국도 좀 다르자너",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007328.1.19",
          "form": "그건 그렇긴하지",
          "original_form": "그건 그렇긴하지",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007328.1.20",
          "form": "평양냉면 한국 온다며 나중에 가서 먹어봐",
          "original_form": "평양냉면 한국 온다며 나중에 가서 먹어봐",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007328.1.21",
          "form": "??원래있자나",
          "original_form": "??원래있자나",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007328.1.22",
          "form": "식당에 평양냉면이라고 팔지않나?",
          "original_form": "식당에 평양냉면이라고 팔지않나?",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007328.1.23",
          "form": "마자 누가 추천해줬어 을밀대",
          "original_form": "마자 누가 추천해줬어 을밀대",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007328.1.24",
          "form": "평양에서 들어온대 나도 정확히는 모르겠어",
          "original_form": "평양에서 들어온대 나도 정확히는 모르겠어",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007328.1.25",
          "form": "옥류관인가",
          "original_form": "옥류관인가",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007328.1.26",
          "form": "음!!? 근데 평양냉면은 맛이좀 다른가?",
          "original_form": "음!!? 근데 평양냉면은 맛이좀 다른가?",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007328.1.27",
          "form": "조금 더 밍밍한 맛이지",
          "original_form": "조금 더 밍밍한 맛이지",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007328.1.28",
          "form": "나는 어쨌든 북한..",
          "original_form": "나는 어쨌든 북한..",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007328.1.29",
          "form": "그 다른나라는 사실",
          "original_form": "그 다른나라는 사실",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007328.1.30",
          "form": "유튜브에 티비에 자주나오기도 하니까?",
          "original_form": "유튜브에 티비에 자주나오기도 하니까?",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007328.1.31",
          "form": "재밌겠네 나중에 가봐",
          "original_form": "재밌겠네 나중에 가봐",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007328.1.32",
          "form": "갈 수 있으려나 모르겠지만",
          "original_form": "갈 수 있으려나 모르겠지만",
          "speaker_id": "1",
          "time": "20210822 01:45"
        }
      ]
