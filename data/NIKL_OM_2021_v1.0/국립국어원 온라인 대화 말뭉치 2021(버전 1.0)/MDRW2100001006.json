      "utterance": [
        {
          "id": "MDRW2100001006.1.1",
          "form": "진로 두꺼비 콜라보레이션 마케팅하는거 본 적 있어?",
          "original_form": "진로 두꺼비 콜라보레이션 마케팅하는거 본 적 있어?",
          "speaker_id": "1",
          "time": "20210820 11:45"
        },
        {
          "id": "MDRW2100001006.1.2",
          "form": "그게 뭐야??",
          "original_form": "그게 뭐야??",
          "speaker_id": "2",
          "time": "20210820 11:45"
        },
        {
          "id": "MDRW2100001006.1.3",
          "form": "나 저번에 밥먹으러 갔는데",
          "original_form": "나 저번에 밥먹으러 갔는데",
          "speaker_id": "1",
          "time": "20210820 11:45"
        },
        {
          "id": "MDRW2100001006.1.4",
          "form": "진로 두꺼비 그려진 컵 주더라",
          "original_form": "진로 두꺼비 그려진 컵 주더라",
          "speaker_id": "1",
          "time": "20210820 11:45"
        },
        {
          "id": "MDRW2100001006.1.5",
          "form": "그런것도 있었구나",
          "original_form": "그런것도 있었구나",
          "speaker_id": "2",
          "time": "20210820 11:46"
        },
        {
          "id": "MDRW2100001006.1.6",
          "form": "곰표는 곰표 상표 단 패딩 팔던데",
          "original_form": "곰표는 곰표 상표 단 패딩 팔던데",
          "speaker_id": "1",
          "time": "20210820 11:46"
        },
        {
          "id": "MDRW2100001006.1.7",
          "form": "아 그것도 봤다",
          "original_form": "아 그것도 봤다",
          "speaker_id": "1",
          "time": "20210820 11:46"
        },
        {
          "id": "MDRW2100001006.1.8",
          "form": "서울우유 콜라보한 서울우유처럼 생긴 바디워시",
          "original_form": "서울우유 콜라보한 서울우유처럼 생긴 바디워시",
          "speaker_id": "1",
          "time": "20210820 11:46"
        },
        {
          "id": "MDRW2100001006.1.9",
          "form": "그거 잘못하다가 마시는거 아니야?",
          "original_form": "그거 잘못하다가 마시는거 아니야?",
          "speaker_id": "2",
          "time": "20210820 11:47"
        },
        {
          "id": "MDRW2100001006.1.10",
          "form": "응 실제로 우리집 근처에 있는 홈플러스에서",
          "original_form": "응 실제로 우리집 근처에 있는 홈플러스에서",
          "speaker_id": "1",
          "time": "20210820 11:47"
        },
        {
          "id": "MDRW2100001006.1.11",
          "form": "서울우유 밑에 서울우유 바디워시 갖다놔서",
          "original_form": "서울우유 밑에 서울우유 바디워시 갖다놔서",
          "speaker_id": "1",
          "time": "20210820 11:47"
        },
        {
          "id": "MDRW2100001006.1.12",
          "form": "인터넷에서 논란이더라",
          "original_form": "인터넷에서 논란이더라",
          "speaker_id": "1",
          "time": "20210820 11:47"
        },
        {
          "id": "MDRW2100001006.1.13",
          "form": "위험한 콜라보인데 왜그렇게 한데",
          "original_form": "위험한 콜라보인데 왜그렇게 한데",
          "speaker_id": "2",
          "time": "20210820 11:48"
        },
        {
          "id": "MDRW2100001006.1.14",
          "form": "그래서 이제 정부에서 규제한대",
          "original_form": "그래서 이제 정부에서 규제한대",
          "speaker_id": "1",
          "time": "20210820 11:48"
        },
        {
          "id": "MDRW2100001006.1.15",
          "form": "먹을거랑 오인되게 만들면 안된다고",
          "original_form": "먹을거랑 오인되게 만들면 안된다고",
          "speaker_id": "1",
          "time": "20210820 11:48"
        },
        {
          "id": "MDRW2100001006.1.16",
          "form": "다행이다 사고날뻔했는데",
          "original_form": "다행이다 사고날뻔했는데",
          "speaker_id": "2",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100001006.1.17",
          "form": "응응 사고 아직 난적은 없는것 같은데 다행이야",
          "original_form": "응응 사고 아직 난적은 없는것 같은데 다행이야",
          "speaker_id": "1",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100001006.1.18",
          "form": "유성매직처럼 생긴 음료수도 팔더라",
          "original_form": "유성매직처럼 생긴 음료수도 팔더라",
          "speaker_id": "1",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100001006.1.19",
          "form": "다음에 실제로 유성매직보고 먹는걸로 착각하면... ㄷㄷ",
          "original_form": "다음에 실제로 유성매직보고 먹는걸로 착각하면... ㄷㄷ",
          "speaker_id": "1",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100001006.1.20",
          "form": "콜라보하는건 좋은데 먹을걸로는 장난 안쳤으면 좋겠어",
          "original_form": "콜라보하는건 좋은데 먹을걸로는 장난 안쳤으면 좋겠어",
          "speaker_id": "2",
          "time": "20210820 11:50"
        },
        {
          "id": "MDRW2100001006.1.21",
          "form": "식품은 정말 조심해야지",
          "original_form": "식품은 정말 조심해야지",
          "speaker_id": "1",
          "time": "20210820 11:50"
        }
