        },
        {
          "id": "MDRW2100019338.1.2",
          "form": "어떤소설에서 작가가",
          "original_form": "어떤소설에서 작가가",
          "speaker_id": "1",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019338.1.3",
          "form": "허허실실 웃는모습이 어쩌구저쩌구",
          "original_form": "허허실실 웃는모습이 어쩌구저쩌구",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019338.1.4",
          "form": "이렇게 써놓은거임",
          "original_form": "이렇게 써놓은거임",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019338.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019338.1.6",
          "form": "근데 허허실실 사자성어잖아",
          "original_form": "근데 허허실실 사자성어잖아",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019338.1.7",
          "form": "나도 어휘력딸려서 먼뜻인지는 기억안났는데",
          "original_form": "나도 어휘력딸려서 먼뜻인지는 기억안났는데",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019338.1.8",
          "form": "응 그런데?",
          "original_form": "응 그런데?",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019338.1.9",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019338.1.10",
          "form": "허허 웃는모습",
          "original_form": "허허 웃는모습",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019338.1.11",
          "form": "뭐였더라 이러고 있네",
          "original_form": "뭐였더라 이러고 있네",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019338.1.12",
          "form": "이거로 표현해놓은거임ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이거로 표현해놓은거임ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019338.1.13",
          "form": "누가?",
          "original_form": "누가?",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019338.1.14",
          "form": "작가가?",
          "original_form": "작가가?",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019338.1.15",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019338.1.16",
          "form": "걍 단어 어감만보고",
          "original_form": "걍 단어 어감만보고",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019338.1.17",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019338.1.18",
          "form": "그렇게썼나봐",
          "original_form": "그렇게썼나봐",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019338.1.19",
          "form": "허허 들어가면 다 웃는건줄 아나보네",
          "original_form": "허허 들어가면 다 웃는건줄 아나보네",
