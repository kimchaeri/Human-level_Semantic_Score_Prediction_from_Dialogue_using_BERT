        {
          "id": "MDRW2100016037.1.1",
          "form": "이거 재밌네요",
          "original_form": "이거 재밌네요",
          "speaker_id": "2",
          "time": "20210826 08:30"
        },
        {
          "id": "MDRW2100016037.1.2",
          "form": "ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 08:30"
        },
        {
          "id": "MDRW2100016037.1.3",
          "form": "마트가면 주로 뭐 사시나요",
          "original_form": "마트가면 주로 뭐 사시나요",
          "speaker_id": "1",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.4",
          "form": "저는 식품요",
          "original_form": "저는 식품요",
          "speaker_id": "2",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.5",
          "form": "돼지네요",
          "original_form": "돼지네요",
          "speaker_id": "2",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.6",
          "form": "허허",
          "original_form": "허허",
          "speaker_id": "2",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.7",
          "form": "마트 어디로 다니세요",
          "original_form": "마트 어디로 다니세요",
          "speaker_id": "1",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.8",
          "form": "달걀",
          "original_form": "달걀",
          "speaker_id": "2",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.9",
          "form": "이마트!",
          "original_form": "이마트!",
          "speaker_id": "2",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.10",
          "form": "동네 마트갓나",
          "original_form": "동네 마트갓나",
          "speaker_id": "1",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.11",
          "form": "가시나",
          "original_form": "가시나",
          "speaker_id": "1",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.12",
          "form": "ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.13",
          "form": "역시 마트는 이마트죠",
          "original_form": "역시 마트는 이마트죠",
          "speaker_id": "1",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.14",
          "form": "맞죠",
          "original_form": "맞죠",
          "speaker_id": "2",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.15",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210826 08:31"
        },
        {
          "id": "MDRW2100016037.1.16",
          "form": "이마트가면 꼭 사는거 있나요",
          "original_form": "이마트가면 꼭 사는거 있나요",
          "speaker_id": "1",
          "time": "20210826 08:32"
        },
        {
          "id": "MDRW2100016037.1.17",
          "form": "저는 노브랜드 또띠아칩..",
          "original_form": "저는 노브랜드 또띠아칩..",
          "speaker_id": "1",
          "time": "20210826 08:32"
        },
        {
          "id": "MDRW2100016037.1.18",
          "form": "계란요",
          "original_form": "계란요",
          "speaker_id": "2",
          "time": "20210826 08:32"
        },
        {
          "id": "MDRW2100016037.1.19",
          "form": "20퍼 할인",
          "original_form": "20퍼 할인",
          "speaker_id": "2",
          "time": "20210826 08:32"
        },
        {
          "id": "MDRW2100016037.1.20",
          "form": "그거 너무맛있어요",
          "original_form": "그거 너무맛있어요",
          "speaker_id": "1",
          "time": "20210826 08:32"
        },
        {
          "id": "MDRW2100016037.1.21",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210826 08:32"
        },
        {
          "id": "MDRW2100016037.1.22",
          "form": "아 계란",
          "original_form": "아 계란",
          "speaker_id": "1",
          "time": "20210826 08:32"
        },
        {
          "id": "MDRW2100016037.1.23",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210826 08:32"
        },
        {
          "id": "MDRW2100016037.1.24",
          "form": "요즘 계란 비싸니까",
          "original_form": "요즘 계란 비싸니까",
          "speaker_id": "1",
          "time": "20210826 08:32"
