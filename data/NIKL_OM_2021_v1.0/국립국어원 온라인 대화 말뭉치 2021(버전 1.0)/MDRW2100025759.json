      "utterance": [
        {
          "id": "MDRW2100025759.1.1",
          "form": "저는 해외에 나가본건 오키나와 딱 한번이라 오키나와요",
          "original_form": "저는 해외에 나가본건 오키나와 딱 한번이라 오키나와요",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100025759.1.2",
          "form": "오 오키나와",
          "original_form": "오 오키나와",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025759.1.3",
          "form": "가보고 싶었는데요",
          "original_form": "가보고 싶었는데요",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025759.1.4",
          "form": "오키나와 좋아요",
          "original_form": "오키나와 좋아요",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100025759.1.5",
          "form": "일본의 제주도 같은 느낌ㅋㅋ",
          "original_form": "일본의 제주도 같은 느낌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100025759.1.6",
          "form": "ㅋㅋ 가보고 싶네요 이쁘다던데",
          "original_form": "ㅋㅋ 가보고 싶네요 이쁘다던데",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025759.1.7",
          "form": "엄청 이쁘고 아기자기하게 돌아다닐 만한 데가 많아요",
          "original_form": "엄청 이쁘고 아기자기하게 돌아다닐 만한 데가 많아요",
          "speaker_id": "2",
          "time": "20210901 18:00"
        },
        {
          "id": "MDRW2100025759.1.8",
          "form": "ㅋㅋ 날씨 따뜻한가요",
          "original_form": "ㅋㅋ 날씨 따뜻한가요",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025759.1.9",
          "form": "더워요... 우리나라 날씨로 겨울때 갔는데 옷 얇게 입고 돌아다녔어요",
          "original_form": "더워요... 우리나라 날씨로 겨울때 갔는데 옷 얇게 입고 돌아다녔어요",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025759.1.10",
          "form": "오호 겨울에 비싸겠군요!",
          "original_form": "오호 겨울에 비싸겠군요!",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100025759.1.11",
          "form": "가격은 정확하게 기억이 안나지만 그럴거 같아요ㅋㅋ",
          "original_form": "가격은 정확하게 기억이 안나지만 그럴거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100025759.1.12",
          "form": "ㅋㅋ아쉬운대로",
          "original_form": "ㅋㅋ아쉬운대로",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100025759.1.13",
          "form": "국내라도. . .",
          "original_form": "국내라도. . .",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100025759.1.14",
          "form": "국내 여행도 안간 지 오래됐죠",
          "original_form": "국내 여행도 안간 지 오래됐죠",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100025759.1.15",
          "form": "망할놈의 코로나..",
          "original_form": "망할놈의 코로나..",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100025759.1.16",
          "form": "ㅋㅋ국내는 좀 갔어요",
          "original_form": "ㅋㅋ국내는 좀 갔어요",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100025759.1.17",
          "form": "슈슝",
          "original_form": "슈슝",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100025759.1.18",
          "form": "어디 다녀오셨어요?",
          "original_form": "어디 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100025759.1.19",
          "form": "저 울릉도요!!!!",
          "original_form": "저 울릉도요!!!!",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100025759.1.20",
          "form": "울릉도 가보고 싶은데",
          "original_form": "울릉도 가보고 싶은데",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100025759.1.21",
          "form": "뱃멀미 있으면 못가겠죠...?",
          "original_form": "뱃멀미 있으면 못가겠죠...?",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100025759.1.22",
          "form": "그거 운이 좋아야 ㅎㅎ",
          "original_form": "그거 운이 좋아야 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100025759.1.23",
          "form": "파도 심한 날은 진짜 심하대요",
          "original_form": "파도 심한 날은 진짜 심하대요",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100025759.1.24",
          "form": "아하... 못갈수도 있군요",
          "original_form": "아하... 못갈수도 있군요",
          "speaker_id": "2",
          "time": "20210902 07:37"
        },
        {
          "id": "MDRW2100025759.1.25",
          "form": "네 독도도 다녀왔습죠",
          "original_form": "네 독도도 다녀왔습죠",
          "speaker_id": "1",
          "time": "20210902 08:58"
        },
        {
          "id": "MDRW2100025759.1.26",
          "form": "독도는 가기 더 힘들지 않나요",
          "original_form": "독도는 가기 더 힘들지 않나요",
          "speaker_id": "2",
          "time": "20210902 08:59"
        },
        {
          "id": "MDRW2100025759.1.27",
          "form": "네 운이 좋았어요",
          "original_form": "네 운이 좋았어요",
          "speaker_id": "1",
          "time": "20210902 09:14"
        },
        {
          "id": "MDRW2100025759.1.28",
          "form": "딱 파도 없어서 ㅎㅎ",
          "original_form": "딱 파도 없어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 09:14"
        }
      ]
    }
