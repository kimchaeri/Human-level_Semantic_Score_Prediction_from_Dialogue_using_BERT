      "utterance": [
        {
          "id": "MDRW2100043775.1.1",
          "form": "에그타르트 존맛",
          "original_form": "에그타르트 존맛",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043775.1.2",
          "form": "나도 에그타르트 너무 먹고싶은데",
          "original_form": "나도 에그타르트 너무 먹고싶은데",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043775.1.3",
          "form": "빵집이 걸어서 30분 거리에 있어",
          "original_form": "빵집이 걸어서 30분 거리에 있어",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043775.1.4",
          "form": "헐 빵세권이",
          "original_form": "헐 빵세권이",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043775.1.5",
          "form": "안타깝잖아",
          "original_form": "안타깝잖아",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043775.1.6",
          "form": "빵을 사야되는데!!!!!!!!!!!!!!!",
          "original_form": "빵을 사야되는데!!!!!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043775.1.7",
          "form": "ㅜㅜㅜㅠㅠㅜㅠㅠㅜㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅠㅠㅜㅠㅠㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043775.1.8",
          "form": "근데 웃긴게",
          "original_form": "근데 웃긴게",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043775.1.9",
          "form": "메뉴 세권에 들어가있으면 또 안 사먹음",
          "original_form": "메뉴 세권에 들어가있으면 또 안 사먹음",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043775.1.10",
          "form": "서브웨이도 30분거리쯤에 있는데",
          "original_form": "서브웨이도 30분거리쯤에 있는데",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043775.1.11",
          "form": "가는 날마다 맨날 안 사먹거",
          "original_form": "가는 날마다 맨날 안 사먹거",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043775.1.12",
          "form": "넌 서브웨이 먹어봤니?",
          "original_form": "넌 서브웨이 먹어봤니?",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043775.1.13",
          "form": "나 먹어봤는데",
          "original_form": "나 먹어봤는데",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043775.1.14",
          "form": "솔직히 무맛임",
          "original_form": "솔직히 무맛임",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043775.1.15",
          "form": "비싸긴 드럽게 비싸고",
          "original_form": "비싸긴 드럽게 비싸고",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043775.1.16",
          "form": "아 진짜? 그냥 일반 샌드위치랑 비슷해?",
          "original_form": "아 진짜? 그냥 일반 샌드위치랑 비슷해?",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043775.1.17",
          "form": "응 비슷해",
          "original_form": "응 비슷해",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043775.1.18",
          "form": "맛은 사실 더없지",
          "original_form": "맛은 사실 더없지",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043775.1.19",
          "form": "건강한 버전이라",
          "original_form": "건강한 버전이라",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043775.1.20",
          "form": "아 우리 폭립이",
          "original_form": "아 우리 폭립이",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043775.1.21",
          "form": "건강한 맛을 싫어하지 참",
          "original_form": "건강한 맛을 싫어하지 참",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043775.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ마!좀 건강식 먹어라!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ마!좀 건강식 먹어라!",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043775.1.23",
          "form": "ㅋㅋㄱㅋㄱㅋ ㄱ네....맞습니다",
          "original_form": "ㅋㅋㄱㅋㄱㅋ ㄱ네....맞습니다",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043775.1.24",
          "form": "건강한맛을 왜 찾아다니는지",
          "original_form": "건강한맛을 왜 찾아다니는지",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043775.1.25",
          "form": "너무해 이 야채배척자야",
          "original_form": "너무해 이 야채배척자야",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043775.1.26",
          "form": "세상에 이렇게 맛있는 음식이 많은디",
          "original_form": "세상에 이렇게 맛있는 음식이 많은디",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043775.1.27",
          "form": "위장 플랙스 해버렸다",
          "original_form": "위장 플랙스 해버렸다",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043775.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043775.1.29",
          "form": "난 지금 복숭아 먹지롱",
          "original_form": "난 지금 복숭아 먹지롱",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043775.1.30",
          "form": "복숭아 맛있지",
          "original_form": "복숭아 맛있지",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
