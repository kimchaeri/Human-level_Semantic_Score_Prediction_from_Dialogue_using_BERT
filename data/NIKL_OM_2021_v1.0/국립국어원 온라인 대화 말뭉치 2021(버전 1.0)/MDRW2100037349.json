      "utterance": [
        {
          "id": "MDRW2100037349.1.1",
          "form": "아니 넷플릭스와 공구박스가 무슨 관곈ㄷㅔ?",
          "original_form": "아니 넷플릭스와 공구박스가 무슨 관곈ㄷㅔ?",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037349.1.2",
          "form": "하하 그 공구박스가아니라",
          "original_form": "하하 그 공구박스가아니라",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037349.1.3",
          "form": "넷플릭스측도 수리기사가 필요한거야?",
          "original_form": "넷플릭스측도 수리기사가 필요한거야?",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037349.1.4",
          "form": "아 이런. 내가 무지했구나",
          "original_form": "아 이런. 내가 무지했구나",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037349.1.5",
          "form": "다같이 돈을내고 나눠보는 공동구매의 준말이야",
          "original_form": "다같이 돈을내고 나눠보는 공동구매의 준말이야",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037349.1.6",
          "form": "아 참. 요즘은 줄임말이 참 많네",
          "original_form": "아 참. 요즘은 줄임말이 참 많네",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037349.1.7",
          "form": "요즘 넷플릭스의 DP라는 드라마가 핫하던데",
          "original_form": "요즘 넷플릭스의 DP라는 드라마가 핫하던데",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037349.1.8",
          "form": "그래서 넷플릭스 공구가 왜?",
          "original_form": "그래서 넷플릭스 공구가 왜?",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037349.1.9",
          "form": "혹시 들어봤니??",
          "original_form": "혹시 들어봤니??",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037349.1.10",
          "form": "디피는 또 뭐야ㅡ 아니 처음 들어",
          "original_form": "디피는 또 뭐야ㅡ 아니 처음 들어",
          "speaker_id": "2",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037349.1.11",
          "form": "그거때문에 공구를 구하는 사람들이 많아졌어",
          "original_form": "그거때문에 공구를 구하는 사람들이 많아졌어",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037349.1.12",
          "form": "넷플릭스 공구를 하고싶은거야?",
          "original_form": "넷플릭스 공구를 하고싶은거야?",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100037349.1.13",
          "form": "어떤건지 궁금하다",
          "original_form": "어떤건지 궁금하다",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100037349.1.14",
          "form": "무슨 탈영병이야기래",
          "original_form": "무슨 탈영병이야기래",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100037349.1.15",
          "form": "외국 드라마겠지?",
          "original_form": "외국 드라마겠지?",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100037349.1.16",
          "form": "아니 우리나라꺼야",
          "original_form": "아니 우리나라꺼야",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100037349.1.17",
          "form": "아 한국건가봐. 그 군인 얼굴은 본 더 같아",
          "original_form": "아 한국건가봐. 그 군인 얼굴은 본 더 같아",
          "speaker_id": "2",
          "time": "20210831 22:45"
        }
      ]
