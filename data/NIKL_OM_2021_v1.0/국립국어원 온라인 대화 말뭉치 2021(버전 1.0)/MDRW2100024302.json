      "utterance": [
        {
          "id": "MDRW2100024302.1.1",
          "form": "연애때 기억에 남는 장소는 어디인가요?",
          "original_form": "연애때 기억에 남는 장소는 어디인가요?",
          "speaker_id": "2",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024302.1.2",
          "form": "음...저는에버랜드요",
          "original_form": "음...저는에버랜드요",
          "speaker_id": "1",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024302.1.3",
          "form": "거기랑올전ㅋㅇ",
          "original_form": "거기랑올전ㅋㅇ",
          "speaker_id": "1",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024302.1.4",
          "form": "온천",
          "original_form": "온천",
          "speaker_id": "1",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024302.1.5",
          "form": "저는이제 연애한시절보다 같이산세월이몇배는 길어져서",
          "original_form": "저는이제 연애한시절보다 같이산세월이몇배는 길어져서",
          "speaker_id": "2",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024302.1.6",
          "form": "온천좋아해요ㅋㅋ",
          "original_form": "온천좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024302.1.7",
          "form": "데이트로딱인데ㅠ",
          "original_form": "데이트로딱인데ㅠ",
          "speaker_id": "1",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024302.1.8",
          "form": "연애때기억이안나요",
          "original_form": "연애때기억이안나요",
          "speaker_id": "2",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024302.1.9",
          "form": "온천좋죠",
          "original_form": "온천좋죠",
          "speaker_id": "2",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024302.1.10",
          "form": "데이트장소로 딱인듯",
          "original_form": "데이트장소로 딱인듯",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024302.1.11",
          "form": "저는이제 두 기간이 비슷한듯ㅋㅋㅋㅋ",
          "original_form": "저는이제 두 기간이 비슷한듯ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024302.1.12",
          "form": "에버랜드도 굿굿이고",
          "original_form": "에버랜드도 굿굿이고",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024302.1.13",
          "form": "연애를 오래하셨네요",
          "original_form": "연애를 오래하셨네요",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024302.1.14",
          "form": "너무 오래 만났네옇ㅎㅎ",
          "original_form": "너무 오래 만났네옇ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024302.1.15",
          "form": "좋은걸까나쁜걸까",
          "original_form": "좋은걸까나쁜걸까",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024302.1.16",
          "form": "아이없이 단둘이는 싫어요",
          "original_form": "아이없이 단둘이는 싫어요",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024302.1.17",
          "form": "어색해요",
          "original_form": "어색해요",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024302.1.18",
          "form": "전아이없는둘이좋아여",
          "original_form": "전아이없는둘이좋아여",
          "speaker_id": "1",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024302.1.19",
          "form": "아직 신혼이네요",
          "original_form": "아직 신혼이네요",
          "speaker_id": "2",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024302.1.20",
          "form": "데이트하고싶네요",
          "original_form": "데이트하고싶네요",
          "speaker_id": "1",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024302.1.21",
          "form": "님이랑ㅋㅋ",
          "original_form": "님이랑ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024302.1.22",
          "form": "금요일에 뭐하시나요",
          "original_form": "금요일에 뭐하시나요",
          "speaker_id": "2",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024302.1.23",
          "form": "만날까요",
          "original_form": "만날까요",
          "speaker_id": "2",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024302.1.24",
          "form": "데이트하면 좋지요ㅋㅋㅋ",
          "original_form": "데이트하면 좋지요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024302.1.25",
          "form": "검단에",
          "original_form": "검단에",
          "speaker_id": "2",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100024302.1.26",
          "form": "할이야기가 많아요ㅋㅋ",
          "original_form": "할이야기가 많아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:28"
        }
      ]
    }
