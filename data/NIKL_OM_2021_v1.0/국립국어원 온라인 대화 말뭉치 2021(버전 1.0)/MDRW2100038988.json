      "utterance": [
        {
          "id": "MDRW2100038988.1.1",
          "form": "오늘 몰랐었던 사실 많이 알게된다",
          "original_form": "오늘 몰랐었던 사실 많이 알게된다",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100038988.1.2",
          "form": "엉 그래서 옥탑방의 문제아들이 시청률이 좋은듯",
          "original_form": "엉 그래서 옥탑방의 문제아들이 시청률이 좋은듯",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100038988.1.3",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100038988.1.4",
          "form": "절차도 중요한가봐",
          "original_form": "절차도 중요한가봐",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100038988.1.5",
          "form": "악용의 우려때문이라고 하시네",
          "original_form": "악용의 우려때문이라고 하시네",
          "speaker_id": "2",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100038988.1.6",
          "form": "참 이게 얘매모호하다 정말로",
          "original_form": "참 이게 얘매모호하다 정말로",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100038988.1.7",
          "form": "그렇다 또 광고",
          "original_form": "그렇다 또 광고",
          "speaker_id": "2",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100038988.1.8",
          "form": "정두영사건 희미하다",
          "original_form": "정두영사건 희미하다",
          "speaker_id": "1",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100038988.1.9",
          "form": "10억을 모으겠다고 9명을 살인",
          "original_form": "10억을 모으겠다고 9명을 살인",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100038988.1.10",
          "form": "직업 살인마라는데",
          "original_form": "직업 살인마라는데",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100038988.1.11",
          "form": "무섭다",
          "original_form": "무섭다",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100038988.1.12",
          "form": "정형돈 말 무시한다",
          "original_form": "정형돈 말 무시한다",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100038988.1.13",
          "form": "ㅋㅋ웃긴다",
          "original_form": "ㅋㅋ웃긴다",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100038988.1.14",
          "form": "헐 내 속의 악마가 있고 그 악마가 저지른 짓이다 라고 정두영이 말햇다는데",
          "original_form": "헐 내 속의 악마가 있고 그 악마가 저지른 짓이다 라고 정두영이 말햇다는데",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100038988.1.15",
          "form": "그게 말이가",
          "original_form": "그게 말이가",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100038988.1.16",
          "form": "사랑하는 여자가 생겨서 아파트 사고 사업할려고 돈을 모을려고 했데",
          "original_form": "사랑하는 여자가 생겨서 아파트 사고 사업할려고 돈을 모을려고 했데",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100038988.1.17",
          "form": "그럼 노동으로 벌어야지",
          "original_form": "그럼 노동으로 벌어야지",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100038988.1.18",
          "form": "그러게 생각부터가 잘못된거지",
          "original_form": "그러게 생각부터가 잘못된거지",
          "speaker_id": "1",
          "time": "20210903 00:24"
        }
      ]
