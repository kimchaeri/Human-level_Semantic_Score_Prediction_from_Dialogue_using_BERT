      "utterance": [
        {
          "id": "MDRW2100041961.1.1",
          "form": "name1님은 저녁 드셨나요?",
          "original_form": "&name1&님은 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210906 17:57"
        },
        {
          "id": "MDRW2100041961.1.2",
          "form": "네 저는 불닭에 짜파게티섞어서 먹었습니다",
          "original_form": "네 저는 불닭에 짜파게티섞어서 먹었습니다",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041961.1.3",
          "form": "2개끓였더니 배부르네요 ㅎㅎ",
          "original_form": "2개끓였더니 배부르네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041961.1.4",
          "form": "메뉴는 뭘로 드실건가요??",
          "original_form": "메뉴는 뭘로 드실건가요??",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041961.1.5",
          "form": "저는 아직 못 정했어요",
          "original_form": "저는 아직 못 정했어요",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041961.1.6",
          "form": "근데 식사를 일찍 하셨네요",
          "original_form": "근데 식사를 일찍 하셨네요",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041961.1.7",
          "form": "네 ㅎㅎ 많이먹을때 좀 일찍먹게 되더라구요",
          "original_form": "네 ㅎㅎ 많이먹을때 좀 일찍먹게 되더라구요",
          "speaker_id": "1",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100041961.1.8",
          "form": "저는 닭구이가 먹고싶네요 ㅎㅎ",
          "original_form": "저는 닭구이가 먹고싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100041961.1.9",
          "form": "닭구이는 통닭이나 치킨이랑 다른건가요?",
          "original_form": "닭구이는 통닭이나 치킨이랑 다른건가요?",
          "speaker_id": "2",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100041961.1.10",
          "form": "숯불에 구워먹는 닭다리살이나 주로 부위를 구워먹는건데 맛있어요!!",
          "original_form": "숯불에 구워먹는 닭다리살이나 주로 부위를 구워먹는건데 맛있어요!!",
          "speaker_id": "1",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100041961.1.11",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210906 18:48"
        },
        {
          "id": "MDRW2100041961.1.12",
          "form": "name1님은 좋아하시는 음식 있으신가요?",
          "original_form": "&name1&님은 좋아하시는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210906 18:48"
        },
        {
          "id": "MDRW2100041961.1.13",
          "form": "저는 해산물 좋아해요 진짜 맛있잖아요 ㅠ 담백하고",
          "original_form": "저는 해산물 좋아해요 진짜 맛있잖아요 ㅠ 담백하고",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041961.1.14",
          "form": "양고기도 좋아하구요",
          "original_form": "양고기도 좋아하구요",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041961.1.15",
          "form": "님은요??",
          "original_form": "님은요??",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100041961.1.16",
          "form": "저는 떡볶이요",
          "original_form": "저는 떡볶이요",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100041961.1.17",
          "form": "근데 양고기 파는 곳은 적지 않나요?",
          "original_form": "근데 양고기 파는 곳은 적지 않나요?",
          "speaker_id": "2",
          "time": "20210906 18:54"
        },
        {
          "id": "MDRW2100041961.1.18",
          "form": "떡볶이 너무 맛있죠 저는 두꺼운떡좋아해요!!할매떡볶이 같은떡이요",
          "original_form": "떡볶이 너무 맛있죠 저는 두꺼운떡좋아해요!!할매떡볶이 같은떡이요",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100041961.1.19",
          "form": "양고기는 마트에서 구워먹거나 식당들도 꽤 있어서 괜찮더라구요",
          "original_form": "양고기는 마트에서 구워먹거나 식당들도 꽤 있어서 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100041961.1.20",
          "form": "요즘 인터넷으로 주문도 되구요!",
          "original_form": "요즘 인터넷으로 주문도 되구요!",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100041961.1.21",
          "form": "양고기를 식당에서 많이 파나요???\r\n주로 어떤 식당에서 파나요??",
          "original_form": "양고기를 식당에서 많이 파나요???\r\n주로 어떤 식당에서 파나요??",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100041961.1.22",
          "form": "네 양고기집이 따로 있어요 ㅎㅎ",
          "original_form": "네 양고기집이 따로 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100041961.1.23",
          "form": "아니면 양고기 집에서 구워먹기 쉽거든요",
          "original_form": "아니면 양고기 집에서 구워먹기 쉽거든요",
          "speaker_id": "1",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100041961.1.24",
          "form": "마트에서 사도 됩니다!",
          "original_form": "마트에서 사도 됩니다!",
          "speaker_id": "1",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100041961.1.25",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 11:13"
        }
      ]
    }
