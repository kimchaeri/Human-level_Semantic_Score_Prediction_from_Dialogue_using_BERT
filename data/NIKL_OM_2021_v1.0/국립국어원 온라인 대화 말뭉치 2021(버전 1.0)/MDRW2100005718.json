      "utterance": [
        {
          "id": "MDRW2100005718.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210907 09:02"
        },
        {
          "id": "MDRW2100005718.1.2",
          "form": "안녕하세요 텀블러 쓰시나요?",
          "original_form": "안녕하세요 텀블러 쓰시나요?",
          "speaker_id": "2",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100005718.1.3",
          "form": "넵ㅋㅋ회사에서 종이컵 잘안써요",
          "original_form": "넵ㅋㅋ회사에서 종이컵 잘안써요",
          "speaker_id": "1",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100005718.1.4",
          "form": "네네 저도 회사 컵이 있어서 이거 써용",
          "original_form": "네네 저도 회사 컵이 있어서 이거 써용",
          "speaker_id": "2",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100005718.1.5",
          "form": "카페갈때도 늘 지참합니다",
          "original_form": "카페갈때도 늘 지참합니다",
          "speaker_id": "1",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100005718.1.6",
          "form": "어디꺼 쓰세요?",
          "original_form": "어디꺼 쓰세요?",
          "speaker_id": "1",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100005718.1.7",
          "form": "저희는 논슬립 컵이에요 브랜드는 없고",
          "original_form": "저희는 논슬립 컵이에요 브랜드는 없고",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100005718.1.8",
          "form": "안넘어지는 컵",
          "original_form": "안넘어지는 컵",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100005718.1.9",
          "form": "아!!! 맞추신거예요?",
          "original_form": "아!!! 맞추신거예요?",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100005718.1.10",
          "form": "아 네내 일괄로 주문한거같아요",
          "original_form": "아 네내 일괄로 주문한거같아요",
          "speaker_id": "2",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100005718.1.11",
          "form": "좋네요! 저도 여기저기 기념품으로 많이 받아서ㅋㅋ",
          "original_form": "좋네요! 저도 여기저기 기념품으로 많이 받아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100005718.1.12",
          "form": "평생 다 못쓸ㅋㅋ",
          "original_form": "평생 다 못쓸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100005718.1.13",
          "form": "아 그쵸 ㅋㅋㅋㅋ",
          "original_form": "아 그쵸 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:53"
        },
        {
          "id": "MDRW2100005718.1.14",
          "form": "잘 사용해야하는데 무겁고 그래서 잘 안써져요",
          "original_form": "잘 사용해야하는데 무겁고 그래서 잘 안써져요",
          "speaker_id": "2",
          "time": "20210907 09:53"
        },
        {
          "id": "MDRW2100005718.1.15",
          "form": "맞아요 전 따뜻한거 위주로 먹어서 스댕만 가능하거든요ㅜ",
          "original_form": "맞아요 전 따뜻한거 위주로 먹어서 스댕만 가능하거든요ㅜ",
          "speaker_id": "1",
          "time": "20210907 09:56"
        },
        {
          "id": "MDRW2100005718.1.16",
          "form": "우와 그렇구나 저도 따뜻한거 마셔야하는데",
          "original_form": "우와 그렇구나 저도 따뜻한거 마셔야하는데",
