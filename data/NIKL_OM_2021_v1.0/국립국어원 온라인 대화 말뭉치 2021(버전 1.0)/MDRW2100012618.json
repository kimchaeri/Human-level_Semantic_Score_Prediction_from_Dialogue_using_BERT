        {
          "id": "MDRW2100012618.1.1",
          "form": "치킨좋아하세요? 내일 답주세요ㅋㅋㅋ",
          "original_form": "치킨좋아하세요? 내일 답주세요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100012618.1.2",
          "form": "네 치킨 좋아해요ㅡ \r\n네네 양념 맛나던데",
          "original_form": "네 치킨 좋아해요ㅡ \r\n네네 양념 맛나던데",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100012618.1.3",
          "form": "치킨 좋아하세요?",
          "original_form": "치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100012618.1.4",
          "form": "네네 좋아하죠ㅋㅋㅋ",
          "original_form": "네네 좋아하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100012618.1.5",
          "form": "파닭좋아하세요?",
          "original_form": "파닭좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100012618.1.6",
          "form": "파닭 맛있죠. 파채가 느끼함을 잡아줘서요",
          "original_form": "파닭 맛있죠. 파채가 느끼함을 잡아줘서요",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100012618.1.7",
          "form": "노랑통닭 파닭드셔보셨어요??",
          "original_form": "노랑통닭 파닭드셔보셨어요??",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100012618.1.8",
          "form": "파채추가나",
          "original_form": "파채추가나",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100012618.1.9",
          "form": "아니요ㅡ 노랑통닭 먹어보고 싶은데 맛있어요?",
          "original_form": "아니요ㅡ 노랑통닭 먹어보고 싶은데 맛있어요?",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100012618.1.10",
          "form": "ㅋㅋㅋㅋ 한번꼭드셔보세요",
          "original_form": "ㅋㅋㅋㅋ 한번꼭드셔보세요",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100012618.1.11",
          "form": "저놀랐잖아요",
          "original_form": "저놀랐잖아요",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100012618.1.12",
          "form": "네네파닭보다맛있더라고요",
          "original_form": "네네파닭보다맛있더라고요",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100012618.1.13",
          "form": "노랑통닭 왠지 튀긴옷이 얇고 바삭할거 같은데 어때요? 네네는 튀김옷이 좀 두꺼운 거 같거든요",
          "original_form": "노랑통닭 왠지 튀긴옷이 얇고 바삭할거 같은데 어때요? 네네는 튀김옷이 좀 두꺼운 거 같거든요",
          "speaker_id": "2",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100012618.1.14",
          "form": "완전 바삭바삭해요ㅋㅋㅋㅋ",
          "original_form": "완전 바삭바삭해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100012618.1.15",
          "form": "전 파채추가로도 먹었었는데 소스랑 완전꿀맛",
          "original_form": "전 파채추가로도 먹었었는데 소스랑 완전꿀맛",
          "speaker_id": "1",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100012618.1.16",
          "form": "네네는 생각보다 쌉쌀?하더라고요",
          "original_form": "네네는 생각보다 쌉쌀?하더라고요",
          "speaker_id": "1",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100012618.1.17",
          "form": "아.. 네네는 파채 아린맛이 강한가 보네요.",
