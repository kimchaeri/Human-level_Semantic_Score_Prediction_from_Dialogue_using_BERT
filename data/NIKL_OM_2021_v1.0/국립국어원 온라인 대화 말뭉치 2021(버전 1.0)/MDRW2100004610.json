      "utterance": [
        {
          "id": "MDRW2100004610.1.1",
          "form": "비타민정도만 먹는거같아요!",
          "original_form": "비타민정도만 먹는거같아요!",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004610.1.2",
          "form": "요즘 비타민 유산균 오메가 엄청많던데",
          "original_form": "요즘 비타민 유산균 오메가 엄청많던데",
          "speaker_id": "1",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004610.1.3",
          "form": "특히 비타민D 영양제를 좀 주로 먹는거같네요",
          "original_form": "특히 비타민D 영양제를 좀 주로 먹는거같네요",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004610.1.4",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004610.1.5",
          "form": "아! 유산균도 먹어요!",
          "original_form": "아! 유산균도 먹어요!",
          "speaker_id": "2",
          "time": "20210821 17:28"
        },
        {
          "id": "MDRW2100004610.1.6",
          "form": "부지런해야 챙겨먹겠던데",
          "original_form": "부지런해야 챙겨먹겠던데",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004610.1.7",
          "form": "전 비타민사면 며칠먹다 꼭날짜지나서 다버리는 ㅠ",
          "original_form": "전 비타민사면 며칠먹다 꼭날짜지나서 다버리는 ㅠ",
          "speaker_id": "1",
          "time": "20210821 17:29"
        },
        {
          "id": "MDRW2100004610.1.8",
          "form": "아하 ㅠㅠ",
          "original_form": "아하 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004610.1.9",
          "form": "그게 일일이 생각하고 먹기가 은근 힘들더라고요",
          "original_form": "그게 일일이 생각하고 먹기가 은근 힘들더라고요",
          "speaker_id": "2",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004610.1.10",
          "form": "까먹고 그러다보니",
          "original_form": "까먹고 그러다보니",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004610.1.11",
          "form": "그니까요 비타민d는 먹는게 좋다구 하던데",
          "original_form": "그니까요 비타민d는 먹는게 좋다구 하던데",
          "speaker_id": "1",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004610.1.12",
          "form": "유산균은 어디꺼드세요?",
          "original_form": "유산균은 어디꺼드세요?",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004610.1.13",
          "form": "락토핏이요!",
          "original_form": "락토핏이요!",
          "speaker_id": "2",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004610.1.14",
          "form": "랏토핏 색깔이 다양하던데",
          "original_form": "랏토핏 색깔이 다양하던데",
          "speaker_id": "1",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004610.1.15",
          "form": "전 주황 먹어봤네요",
          "original_form": "전 주황 먹어봤네요",
          "speaker_id": "1",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004610.1.16",
          "form": "노란색 락토핏먹죠~!",
          "original_form": "노란색 락토핏먹죠~!",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004610.1.17",
          "form": "저는 노랑만 먹네요 ㅎㅎ",
          "original_form": "저는 노랑만 먹네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004610.1.18",
          "form": "무슨 차이인가요ㅎ",
          "original_form": "무슨 차이인가요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004610.1.19",
          "form": "딱히 큰 차이는 모르겠어요 ㅋㅋ",
          "original_form": "딱히 큰 차이는 모르겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004610.1.20",
          "form": "그냥 노란색이 무난할거같아서 먹고있어요",
          "original_form": "그냥 노란색이 무난할거같아서 먹고있어요",
          "speaker_id": "2",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004610.1.21",
          "form": "장에 효과가 있나요? 먹어도 전 잘몰겠어요",
          "original_form": "장에 효과가 있나요? 먹어도 전 잘몰겠어요",
          "speaker_id": "1",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004610.1.22",
          "form": "효과는 좀 있는거같아요!",
          "original_form": "효과는 좀 있는거같아요!",
          "speaker_id": "2",
          "time": "20210821 17:39"
        },
        {
          "id": "MDRW2100004610.1.23",
          "form": "변비해결이 되려나요?ㅎㅎ",
          "original_form": "변비해결이 되려나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004610.1.24",
          "form": "저도 변비였었는데 해결하긴했어요!",
          "original_form": "저도 변비였었는데 해결하긴했어요!",
          "speaker_id": "2",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004610.1.25",
          "form": "물론 락토핏으로만 효과를 봤다기보단",
          "original_form": "물론 락토핏으로만 효과를 봤다기보단",
          "speaker_id": "2",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004610.1.26",
          "form": "패턴도 좀 수정했고 물도 더 많이 먹어서",
          "original_form": "패턴도 좀 수정했고 물도 더 많이 먹어서",
          "speaker_id": "2",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004610.1.27",
          "form": "효과를 더욱 크게 본거같네요@",
          "original_form": "효과를 더욱 크게 본거같네요@",
          "speaker_id": "2",
          "time": "20210821 17:40"
        },
        {
          "id": "MDRW2100004610.1.28",
          "form": "오ㅎ 글쿤요ㅎ",
          "original_form": "오ㅎ 글쿤요ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:40"
        }
      ]
