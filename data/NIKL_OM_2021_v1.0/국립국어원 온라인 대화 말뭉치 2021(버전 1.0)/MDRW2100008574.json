      "utterance": [
        {
          "id": "MDRW2100008574.1.1",
          "form": "아 어제선풍기 샀음",
          "original_form": "아 어제선풍기 샀음",
          "speaker_id": "2",
          "time": "20210822 16:46"
        },
        {
          "id": "MDRW2100008574.1.2",
          "form": "올",
          "original_form": "올",
          "speaker_id": "1",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008574.1.3",
          "form": "선풍기 고장나서?",
          "original_form": "선풍기 고장나서?",
          "speaker_id": "1",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008574.1.4",
          "form": "알고보니 선풍기가 무려 05년에 산거였음 ㄷㄷ",
          "original_form": "알고보니 선풍기가 무려 05년에 산거였음 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008574.1.5",
          "form": "보내줄때가 되았음 ㅋㅋ",
          "original_form": "보내줄때가 되았음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008574.1.6",
          "form": "장수했네",
          "original_form": "장수했네",
          "speaker_id": "1",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008574.1.7",
          "form": "나는 최근에 산것이 먹는거 외엔 없네",
          "original_form": "나는 최근에 산것이 먹는거 외엔 없네",
          "speaker_id": "1",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008574.1.8",
          "form": "왁스실을 샀는데",
          "original_form": "왁스실을 샀는데",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008574.1.9",
          "form": "아직도 미산가팔찌를 못만들고있음",
          "original_form": "아직도 미산가팔찌를 못만들고있음",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008574.1.10",
          "form": "오 손재주가 좋은분!",
          "original_form": "오 손재주가 좋은분!",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008574.1.11",
          "form": "소리가 너무시끄러위서",
          "original_form": "소리가 너무시끄러위서",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008574.1.12",
          "form": "사실 아직도 고장은 안남",
          "original_form": "사실 아직도 고장은 안남",
          "speaker_id": "2",
          "time": "20210822 16:53"
        },
        {
          "id": "MDRW2100008574.1.13",
          "form": "아주튼튼한 신일 선풍기",
          "original_form": "아주튼튼한 신일 선풍기",
          "speaker_id": "2",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008574.1.14",
          "form": "아.. 그거 아쉽네",
          "original_form": "아.. 그거 아쉽네",
          "speaker_id": "1",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008574.1.15",
          "form": "당근마켓함?",
          "original_form": "당근마켓함?",
          "speaker_id": "1",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008574.1.16",
          "form": "놉놉 위메프함",
          "original_form": "놉놉 위메프함",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008574.1.17",
          "form": "잘했네",
          "original_form": "잘했네",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008574.1.18",
          "form": "새로나온 무소음선풍기",
          "original_form": "새로나온 무소음선풍기",
          "speaker_id": "2",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008574.1.19",
          "form": "무려 15데시벨",
          "original_form": "무려 15데시벨",
          "speaker_id": "2",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008574.1.20",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008574.1.21",
          "form": "과학이 참 좋다잉",
          "original_form": "과학이 참 좋다잉",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008574.1.22",
          "form": "에어컨은 왜 안씀?",
          "original_form": "에어컨은 왜 안씀?",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008574.1.23",
          "form": "에어컨 계속 썼는데",
          "original_form": "에어컨 계속 썼는데",
          "speaker_id": "2",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008574.1.24",
          "form": "최근엔 더위도 많이 가셨고",
          "original_form": "최근엔 더위도 많이 가셨고",
          "speaker_id": "2",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008574.1.25",
          "form": "좀 에어컨켜긴 아까운 날씨랄까",
          "original_form": "좀 에어컨켜긴 아까운 날씨랄까",
          "speaker_id": "2",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008574.1.26",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "1",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008574.1.27",
          "form": "제습기 켜야함",
          "original_form": "제습기 켜야함",
          "speaker_id": "1",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008574.1.28",
          "form": "내가 작년에 안경을샀는데",
          "original_form": "내가 작년에 안경을샀는데",
          "speaker_id": "1",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100008574.1.29",
          "form": "재난지원금으로",
          "original_form": "재난지원금으로",
          "speaker_id": "1",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100008574.1.30",
          "form": "그때 패션안경점에서 샀는데 지금 얼굴닿는부분이 녹슬었어ㅜ",
          "original_form": "그때 패션안경점에서 샀는데 지금 얼굴닿는부분이 녹슬었어ㅜ",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100008574.1.31",
          "form": "그전 안경이 좀더 튼튼한거같아",
          "original_form": "그전 안경이 좀더 튼튼한거같아",
          "speaker_id": "1",
          "time": "20210823 00:08"
        }
      ]
    }
