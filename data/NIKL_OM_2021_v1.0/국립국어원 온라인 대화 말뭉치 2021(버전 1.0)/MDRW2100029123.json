      "utterance": [
        {
          "id": "MDRW2100029123.1.1",
          "form": "저컵사고싶다",
          "original_form": "저컵사고싶다",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029123.1.2",
          "form": "큰컵",
          "original_form": "큰컵",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029123.1.3",
          "form": "텀블러도 사고싶다",
          "original_form": "텀블러도 사고싶다",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029123.1.4",
          "form": "저 컵 제주도에서 산 텀블러",
          "original_form": "저 컵 제주도에서 산 텀블러",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029123.1.5",
          "form": "아직 팔고 있음",
          "original_form": "아직 팔고 있음",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029123.1.6",
          "form": "그거 예뻤어요",
          "original_form": "그거 예뻤어요",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029123.1.7",
          "form": "컵하니까 생각나는데",
          "original_form": "컵하니까 생각나는데",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029123.1.8",
          "form": "최근에 써봄",
          "original_form": "최근에 써봄",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029123.1.9",
          "form": "생리컵 하나 더 사볼까 고민중",
          "original_form": "생리컵 하나 더 사볼까 고민중",
          "speaker_id": "2",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029123.1.10",
          "form": "아 그 커",
          "original_form": "아 그 커",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100029123.1.11",
          "form": "괜찮았나 봐요",
          "original_form": "괜찮았나 봐요",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029123.1.12",
          "form": "편했어",
          "original_form": "편했어",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029123.1.13",
          "form": "들어가있을때 신세계",
          "original_form": "들어가있을때 신세계",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029123.1.14",
          "form": "나한테 맞는 게 있다라는 게 있다라는 게 좋은 듯",
          "original_form": "나한테 맞는 게 있다라는 게 있다라는 게 좋은 듯",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029123.1.15",
          "form": "그런데 넣고 뺄때 지옥이었음",
          "original_form": "그런데 넣고 뺄때 지옥이었음",
          "speaker_id": "2",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029123.1.16",
          "form": "외국인처럼 말이 나온다",
          "original_form": "외국인처럼 말이 나온다",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029123.1.17",
          "form": "너 코 뺄 때 많이 아파요",
          "original_form": "너 코 뺄 때 많이 아파요",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029123.1.18",
          "form": "하나밖에없으니",
          "original_form": "하나밖에없으니",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029123.1.19",
          "form": "내가 산게 맞는건지",
          "original_form": "내가 산게 맞는건지",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029123.1.20",
          "form": "궁금함",
          "original_form": "궁금함",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029123.1.21",
          "form": "근데 가격대가 좀비싸",
          "original_form": "근데 가격대가 좀비싸",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029123.1.22",
          "form": "고민됨",
          "original_form": "고민됨",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029123.1.23",
          "form": "반영구 석이니까",
          "original_form": "반영구 석이니까",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029123.1.24",
          "form": "그래도 일년은 쓸 수 있지 않을까요",
          "original_form": "그래도 일년은 쓸 수 있지 않을까요",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100029123.1.25",
          "form": "반영구여도 몇년쓰고는 바까줘먀하는듯",
          "original_form": "반영구여도 몇년쓰고는 바까줘먀하는듯",
