        {
          "id": "MDRW2100007861.1.1",
          "form": "오늘 점심 피자먹을까?",
          "original_form": "오늘 점심 피자먹을까?",
          "speaker_id": "1",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007861.1.2",
          "form": "응 그러쟈 어디께좋아",
          "original_form": "응 그러쟈 어디께좋아",
          "speaker_id": "2",
          "time": "20210822 11:27"
        },
        {
          "id": "MDRW2100007861.1.3",
          "form": "음 감자도 좋고 치즈도 잔뜩올라간게 좋은데",
          "original_form": "음 감자도 좋고 치즈도 잔뜩올라간게 좋은데",
          "speaker_id": "1",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007861.1.4",
          "form": "그럼 도피?",
          "original_form": "그럼 도피?",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007861.1.5",
          "form": "도미노피자보다 다른 치즈 더 많은거",
          "original_form": "도미노피자보다 다른 치즈 더 많은거",
          "speaker_id": "1",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007861.1.6",
          "form": "그럼 어디게 좋지",
          "original_form": "그럼 어디게 좋지",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100007861.1.7",
          "form": "골라보자 배민보곹ㅋㅋㅋ",
          "original_form": "골라보자 배민보곹ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100007861.1.8",
          "form": "그래 ㅋ 감자랑 치즈면 반올림은?",
          "original_form": "그래 ㅋ 감자랑 치즈면 반올림은?",
          "speaker_id": "2",
          "time": "20210822 11:30"
        },
        {
          "id": "MDRW2100007861.1.9",
          "form": "반올림 좋당",
          "original_form": "반올림 좋당",
          "speaker_id": "1",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007861.1.10",
          "form": "그럼 그걸로하쟈",
          "original_form": "그럼 그걸로하쟈",
          "speaker_id": "2",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100007861.1.11",
          "form": "주문해야겠다ㅋㅋㅋㅋ",
          "original_form": "주문해야겠다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:32"
        },
        {
          "id": "MDRW2100007861.1.12",
          "form": "언제문열어?",
          "original_form": "언제문열어?",
          "speaker_id": "2",
          "time": "20210822 11:32"
        },
        {
          "id": "MDRW2100007861.1.13",
          "form": "30분 쯤 확인해볼께",
          "original_form": "30분 쯤 확인해볼께",
          "speaker_id": "1",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007861.1.14",
          "form": "그럼 보고 시키자",
          "original_form": "그럼 보고 시키자",
          "speaker_id": "2",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100007861.1.15",
          "form": "배달음식 줄여야하는데ㅋ",
