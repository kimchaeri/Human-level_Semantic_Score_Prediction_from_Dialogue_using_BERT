      "utterance": [
        {
          "id": "MDRW2100034170.1.1",
          "form": "여행가면 기념품",
          "original_form": "여행가면 기념품",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034170.1.2",
          "form": "뭐 이런 거 구매하세요?",
          "original_form": "뭐 이런 거 구매하세요?",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100034170.1.3",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100034170.1.4",
          "form": "선물로 줄 수 있는거 사려고 해요ㅎㅎ먹을거라도",
          "original_form": "선물로 줄 수 있는거 사려고 해요ㅎㅎ먹을거라도",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100034170.1.5",
          "form": "오 좋죠 ㅎ",
          "original_form": "오 좋죠 ㅎ",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034170.1.6",
          "form": "사신 것 중에",
          "original_form": "사신 것 중에",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034170.1.7",
          "form": "반응 좋았던 것도 있나요?",
          "original_form": "반응 좋았던 것도 있나요?",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100034170.1.8",
          "form": "먹을거는 진짜 별로 호불호 안갈리는거 같구",
          "original_form": "먹을거는 진짜 별로 호불호 안갈리는거 같구",
          "speaker_id": "1",
          "time": "20210828 12:04"
        },
        {
          "id": "MDRW2100034170.1.9",
          "form": "특별한건 막 기억 안나긴 하는데ㅎㅎ엽서같은것도 써서 같이 주고 그랬어용ㅎ",
          "original_form": "특별한건 막 기억 안나긴 하는데ㅎㅎ엽서같은것도 써서 같이 주고 그랬어용ㅎ",
          "speaker_id": "1",
          "time": "20210828 12:05"
        },
        {
          "id": "MDRW2100034170.1.10",
          "form": "아 엽서",
          "original_form": "아 엽서",
          "speaker_id": "2",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100034170.1.11",
          "form": "정말 좋을 거 같아요ㅋㅋㅋㅋ",
          "original_form": "정말 좋을 거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100034170.1.12",
          "form": "ㅋㅋㅋㅋㅋ완전 아날로그 느낌이죠..ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ완전 아날로그 느낌이죠..ㅎ",
          "speaker_id": "1",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100034170.1.13",
          "form": "네 그런 거",
          "original_form": "네 그런 거",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100034170.1.14",
          "form": "아즈 좋아요",
          "original_form": "아즈 좋아요",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100034170.1.15",
          "form": "괜히 선물줄때 센치해져요ㅋㅋㅋ",
          "original_form": "괜히 선물줄때 센치해져요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100034170.1.16",
          "form": "아 그리고 여행지에서 예쁜 냉장고 자석 있음 사와요",
          "original_form": "아 그리고 여행지에서 예쁜 냉장고 자석 있음 사와요",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100034170.1.17",
          "form": "ㅋㅋㅋㅋㅋ어 마그넷",
          "original_form": "ㅋㅋㅋㅋㅋ어 마그넷",
          "speaker_id": "2",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100034170.1.18",
          "form": "그건 정말 좋은 거 같아요",
          "original_form": "그건 정말 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100034170.1.19",
          "form": "모아서 붙여놓으면 예쁘더라고요",
          "original_form": "모아서 붙여놓으면 예쁘더라고요",
          "speaker_id": "1",
          "time": "20210829 11:57"
        },
        {
          "id": "MDRW2100034170.1.20",
          "form": "냉장고 열면서 괜히 여행 생각도 나구ㅎㅎ",
          "original_form": "냉장고 열면서 괜히 여행 생각도 나구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:57"
        },
        {
          "id": "MDRW2100034170.1.21",
          "form": "맞아요 ㅎ 그거 사는 시람",
          "original_form": "맞아요 ㅎ 그거 사는 시람",
          "speaker_id": "2",
          "time": "20210829 12:03"
        },
        {
          "id": "MDRW2100034170.1.22",
          "form": "되게.많더라구요",
          "original_form": "되게.많더라구요",
          "speaker_id": "2",
          "time": "20210829 12:03"
        },
        {
          "id": "MDRW2100034170.1.23",
          "form": "저도 여행지에서 사온진 얼마 안됐는데",
          "original_form": "저도 여행지에서 사온진 얼마 안됐는데",
          "speaker_id": "1",
          "time": "20210829 12:04"
        },
        {
          "id": "MDRW2100034170.1.24",
          "form": "전부터 사올걸 싶더라구요ㅠㅠ",
          "original_form": "전부터 사올걸 싶더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 12:04"
        },
        {
          "id": "MDRW2100034170.1.25",
          "form": "아 저드요..",
          "original_form": "아 저드요..",
          "speaker_id": "2",
          "time": "20210829 12:05"
        },
        {
          "id": "MDRW2100034170.1.26",
          "form": "모으고싶어요",
          "original_form": "모으고싶어요",
          "speaker_id": "2",
          "time": "20210829 12:05"
        }
      ]
    }
