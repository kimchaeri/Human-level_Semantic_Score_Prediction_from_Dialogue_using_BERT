      "utterance": [
        {
          "id": "MDRW2100009014.1.1",
          "form": "name1님도 도서관에 희망도서 신청해서 보고싶은책 대여해봐",
          "original_form": "&name1&님도 도서관에 희망도서 신청해서 보고싶은책 대여해봐",
          "speaker_id": "2",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009014.1.2",
          "form": "나는 이사다녀야해서ㅜㅜㅜ 책이 큰 짐이닷",
          "original_form": "나는 이사다녀야해서ㅜㅜㅜ 책이 큰 짐이닷",
          "speaker_id": "2",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009014.1.3",
          "form": "난 요새 이북으로 봄 그래서",
          "original_form": "난 요새 이북으로 봄 그래서",
          "speaker_id": "1",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009014.1.4",
          "form": "영화는 그렇다쳐도",
          "original_form": "영화는 그렇다쳐도",
          "speaker_id": "1",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009014.1.5",
          "form": "난 보고싶은만화책이 있는데 종이책 살지 이북살지",
          "original_form": "난 보고싶은만화책이 있는데 종이책 살지 이북살지",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009014.1.6",
          "form": "넘고민돼",
          "original_form": "넘고민돼",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009014.1.7",
          "form": "콘서트는 이제어캐되려나",
          "original_form": "콘서트는 이제어캐되려나",
          "speaker_id": "1",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009014.1.8",
          "form": "bts는 어케하더라",
          "original_form": "bts는 어케하더라",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009014.1.9",
          "form": "돈많으니 알아서잘하지않을까",
          "original_form": "돈많으니 알아서잘하지않을까",
          "speaker_id": "2",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009014.1.10",
          "form": "나는 뒹굴거리면서 만화책 읽는거 좋아하는데",
          "original_form": "나는 뒹굴거리면서 만화책 읽는거 좋아하는데",
          "speaker_id": "2",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009014.1.11",
          "form": "휴ㅜㅜ 짐되서 슬퍼ㅜ",
          "original_form": "휴ㅜㅜ 짐되서 슬퍼ㅜ",
          "speaker_id": "2",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009014.1.12",
          "form": "음 이북을 사야할듯",
          "original_form": "음 이북을 사야할듯",
          "speaker_id": "1",
          "time": "20210823 00:47"
        },
        {
          "id": "MDRW2100009014.1.13",
          "form": "좋은 리더기와",
          "original_form": "좋은 리더기와",
          "speaker_id": "1",
          "time": "20210823 00:47"
        },
        {
          "id": "MDRW2100009014.1.14",
          "form": "이북 리더기는 당근마켓에 많이 올라온대",
          "original_form": "이북 리더기는 당근마켓에 많이 올라온대",
          "speaker_id": "2",
          "time": "20210823 00:47"
        },
        {
          "id": "MDRW2100009014.1.15",
          "form": "이북리더기로 만화책읽기 괜찮으려나",
          "original_form": "이북리더기로 만화책읽기 괜찮으려나",
          "speaker_id": "2",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009014.1.16",
          "form": "생소하네",
          "original_form": "생소하네",
          "speaker_id": "2",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009014.1.17",
          "form": "아님 태블릿이나",
          "original_form": "아님 태블릿이나",
          "speaker_id": "1",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009014.1.18",
          "form": "마이홈 없으면",
          "original_form": "마이홈 없으면",
          "speaker_id": "1",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009014.1.19",
          "form": "진짜 만화책은..",
          "original_form": "진짜 만화책은..",
          "speaker_id": "1",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009014.1.20",
          "form": "따흑흐구ㅜㅜㅜㅜ",
          "original_form": "따흑흐구ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009014.1.21",
          "form": "좋아하는작가님 신간나왔는데에에ㅜㅜㅜㅜ",
          "original_form": "좋아하는작가님 신간나왔는데에에ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 00:49"
        },
        {
          "id": "MDRW2100009014.1.22",
          "form": "고민해야하다니",
          "original_form": "고민해야하다니",
          "speaker_id": "2",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009014.1.23",
          "form": "슬프군",
          "original_form": "슬프군",
          "speaker_id": "2",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009014.1.24",
          "form": "또 구겨지거나 손상되면 맘 아프잖아여ㅜ",
          "original_form": "또 구겨지거나 손상되면 맘 아프잖아여ㅜ",
          "speaker_id": "1",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009014.1.25",
          "form": "만화책때문에 집이 갖고파 ㅋ",
          "original_form": "만화책때문에 집이 갖고파 ㅋ",
          "speaker_id": "2",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009014.1.26",
          "form": "으어 너무나 큰꿈이다 그거ㅜ",
          "original_form": "으어 너무나 큰꿈이다 그거ㅜ",
          "speaker_id": "1",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009014.1.27",
          "form": "ㅋㅋㅋ 넘 야무져버렸어!!",
          "original_form": "ㅋㅋㅋ 넘 야무져버렸어!!",
          "speaker_id": "2",
          "time": "20210823 00:54"
        },
        {
          "id": "MDRW2100009014.1.28",
          "form": "못사고왔거든ㅋㅋㅋ 젠장!",
          "original_form": "못사고왔거든ㅋㅋㅋ 젠장!",
