      "utterance": [
        {
          "id": "MDRW2100040948.1.1",
          "form": "산은 다녀오면 엄청 뿌듯해요!",
          "original_form": "산은 다녀오면 엄청 뿌듯해요!",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100040948.1.2",
          "form": "올라가는게힘들지민",
          "original_form": "올라가는게힘들지민",
          "speaker_id": "2",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100040948.1.3",
          "form": "올라가면좋을거같긴해요",
          "original_form": "올라가면좋을거같긴해요",
          "speaker_id": "2",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100040948.1.4",
          "form": "맞아요 성취감이!!",
          "original_form": "맞아요 성취감이!!",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100040948.1.5",
          "form": "하지만 너무 힘들어요 ㅋㅋㅋㅋ",
          "original_form": "하지만 너무 힘들어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100040948.1.6",
          "form": "맞아요 그래서 저도 바다선호하는편",
          "original_form": "맞아요 그래서 저도 바다선호하는편",
          "speaker_id": "2",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100040948.1.7",
          "form": "수영 할 줄 아세요?",
          "original_form": "수영 할 줄 아세요?",
          "speaker_id": "1",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100040948.1.8",
          "form": "넹넹!!",
          "original_form": "넹넹!!",
          "speaker_id": "2",
          "time": "20210905 18:17"
        },
        {
          "id": "MDRW2100040948.1.9",
          "form": "우와 멋지네요 전 못하거든요",
          "original_form": "우와 멋지네요 전 못하거든요",
          "speaker_id": "1",
          "time": "20210906 09:00"
        },
        {
          "id": "MDRW2100040948.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저도 어릴때 빠진기억있어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저도 어릴때 빠진기억있어서",
          "speaker_id": "2",
          "time": "20210906 09:21"
        },
        {
          "id": "MDRW2100040948.1.11",
          "form": "열씨미배웠어요",
          "original_form": "열씨미배웠어요",
          "speaker_id": "2",
          "time": "20210906 09:21"
        },
        {
          "id": "MDRW2100040948.1.12",
          "form": "빠지면 본능적으로 수영하게 되더라구요",
          "original_form": "빠지면 본능적으로 수영하게 되더라구요",
          "speaker_id": "2",
          "time": "20210906 09:21"
        },
        {
          "id": "MDRW2100040948.1.13",
          "form": "우와 진짜 멋져요 생존수영",
          "original_form": "우와 진짜 멋져요 생존수영",
          "speaker_id": "1",
          "time": "20210906 13:39"
        },
        {
          "id": "MDRW2100040948.1.14",
          "form": "바다에서도 물에 뜨신다니!",
          "original_form": "바다에서도 물에 뜨신다니!",
          "speaker_id": "1",
          "time": "20210906 13:39"
        },
        {
          "id": "MDRW2100040948.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ생존 수영이 정말위대합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ생존 수영이 정말위대합니다",
          "speaker_id": "2",
          "time": "20210906 14:00"
        },
        {
          "id": "MDRW2100040948.1.16",
          "form": "저는 수영복입기 부담스러워서 수영 안배우고잇어요",
          "original_form": "저는 수영복입기 부담스러워서 수영 안배우고잇어요",
          "speaker_id": "1",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100040948.1.17",
          "form": "맞어요 그거 조꼼부끄럽더라구요",
          "original_form": "맞어요 그거 조꼼부끄럽더라구요",
          "speaker_id": "2",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100040948.1.18",
          "form": "레쉬가든입고 하세오!!",
          "original_form": "레쉬가든입고 하세오!!",
          "speaker_id": "2",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100040948.1.19",
          "form": "젤편안..",
          "original_form": "젤편안..",
          "speaker_id": "2",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100040948.1.20",
          "form": "래쉬가드! 저 루즈핏으로 삿는데 일반 수영장에서 받아줄지모르겟네요",
          "original_form": "래쉬가드! 저 루즈핏으로 삿는데 일반 수영장에서 받아줄지모르겟네요",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100040948.1.21",
          "form": "안해줘도 친구랑 가서",
          "original_form": "안해줘도 친구랑 가서",
          "speaker_id": "2",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100040948.1.22",
          "form": "오리발쓰고 열씨미 연습하시먄",
          "original_form": "오리발쓰고 열씨미 연습하시먄",
          "speaker_id": "2",
          "time": "20210906 15:06"
        },
        {
          "id": "MDRW2100040948.1.23",
