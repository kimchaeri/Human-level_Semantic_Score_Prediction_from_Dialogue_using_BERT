      "utterance": [
        {
          "id": "MDRW2100001492.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20211014 23:50"
        },
        {
          "id": "MDRW2100001492.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211015 08:24"
        },
        {
          "id": "MDRW2100001492.1.3",
          "form": "오늘 또 비가 내릴 거 같네요 ㅠㅠ",
          "original_form": "오늘 또 비가 내릴 거 같네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211015 08:25"
        },
        {
          "id": "MDRW2100001492.1.4",
          "form": "이번 가을은 뭔가 엄청 빨리 지나간 느낌이에요..",
          "original_form": "이번 가을은 뭔가 엄청 빨리 지나간 느낌이에요..",
          "speaker_id": "2",
          "time": "20211015 08:25"
        },
        {
          "id": "MDRW2100001492.1.5",
          "form": "그러게요 시간이 너무 빨라요ㅠㅠ",
          "original_form": "그러게요 시간이 너무 빨라요{emoji:…}ㅠㅠ",
          "speaker_id": "1",
          "time": "20211015 10:16"
        },
        {
          "id": "MDRW2100001492.1.6",
          "form": "일요일에는 영상 2도까지 떨어진다고 하더라구요....",
          "original_form": "일요일에는 영상 2도까지 떨어진다고 하더라구요....",
          "speaker_id": "2",
          "time": "20211015 10:16"
        },
        {
          "id": "MDRW2100001492.1.7",
          "form": "주말에 나가실 일 있으면",
          "original_form": "주말에 나가실 일 있으면",
          "speaker_id": "2",
          "time": "20211015 10:16"
        },
        {
          "id": "MDRW2100001492.1.8",
          "form": "꼭 외투 따뜻하게 입고 나가세요!",
          "original_form": "꼭 외투 따뜻하게 입고 나가세요!",
          "speaker_id": "2",
          "time": "20211015 10:17"
        },
        {
          "id": "MDRW2100001492.1.9",
          "form": "넵 감사합니다ㅎㅎ",
          "original_form": "넵 감사합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 10:32"
        },
        {
          "id": "MDRW2100001492.1.10",
          "form": "보내주시는 분도 감기조심하세요!!",
          "original_form": "보내주시는 분도 감기조심하세요!!",
          "speaker_id": "1",
          "time": "20211015 10:32"
        },
        {
          "id": "MDRW2100001492.1.11",
          "form": "ㅎㅎ 네네 감사합니다",
          "original_form": "ㅎㅎ 네네 감사합니다",
          "speaker_id": "2",
          "time": "20211015 10:32"
        },
        {
          "id": "MDRW2100001492.1.12",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "2",
          "time": "20211015 10:32"
        },
        {
          "id": "MDRW2100001492.1.13",
          "form": "추위 많이 타시나요?",
          "original_form": "추위 많이 타시나요?",
          "speaker_id": "2",
          "time": "20211015 10:32"
        },
        {
          "id": "MDRW2100001492.1.14",
          "form": "네.. 많이 타는 편인거 같긴 해요..ㅋㅋㅋㅋ",
          "original_form": "네.. 많이 타는 편인거 같긴 해요..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211015 16:16"
        },
        {
          "id": "MDRW2100001492.1.15",
          "form": "근데 더위를 더 많이 타는거 같아요ㅋㅋㅋㅋ",
          "original_form": "근데 더위를 더 많이 타는거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211015 16:16"
        },
        {
          "id": "MDRW2100001492.1.16",
          "form": "오 저랑 똑같으시네요 ㅋㅋㅋㅋㅋ",
          "original_form": "오 저랑 똑같으시네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 16:16"
        },
        {
          "id": "MDRW2100001492.1.17",
          "form": "추운 것도 힘들긴한데..",
          "original_form": "추운 것도 힘들긴한데..",
          "speaker_id": "2",
          "time": "20211015 16:16"
        },
        {
          "id": "MDRW2100001492.1.18",
          "form": "더운건 정말 너무 싫어요 ㅠ",
          "original_form": "더운건 정말 너무 싫어요 ㅠ",
          "speaker_id": "2",
          "time": "20211015 16:17"
        },
        {
          "id": "MDRW2100001492.1.19",
          "form": "약속도 잘안잡아요 ㅋㅋㅋㅋㅋ",
          "original_form": "약속도 잘안잡아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 16:17"
        },
        {
          "id": "MDRW2100001492.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요...",
          "speaker_id": "1",
          "time": "20211015 16:39"
        },
        {
          "id": "MDRW2100001492.1.21",
          "form": "봄가을이 딱 좋은데...",
          "original_form": "봄가을이 딱 좋은데...",
          "speaker_id": "1",
          "time": "20211015 16:39"
        },
        {
          "id": "MDRW2100001492.1.22",
          "form": "봄가을중 어떤게 더 좋으세요?",
          "original_form": "봄가을중 어떤게 더 좋으세요?",
