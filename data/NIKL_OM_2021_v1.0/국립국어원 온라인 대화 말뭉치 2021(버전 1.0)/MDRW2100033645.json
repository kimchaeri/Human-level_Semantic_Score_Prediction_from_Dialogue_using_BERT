      "utterance": [
        {
          "id": "MDRW2100033645.1.1",
          "form": "밥먹으면서 뭐보시나요",
          "original_form": "밥먹으면서 뭐보시나요",
          "speaker_id": "1",
          "time": "20210824 19:53"
        },
        {
          "id": "MDRW2100033645.1.2",
          "form": "아 저 대탈출 보려구요ㅎㅎ 뭐 보시나요?",
          "original_form": "아 저 대탈출 보려구요ㅎㅎ 뭐 보시나요?",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100033645.1.3",
          "form": "저는 좋좋소기업인가",
          "original_form": "저는 좋좋소기업인가",
          "speaker_id": "1",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100033645.1.4",
          "form": "왓챠에 있는데 에피소드 짧아서 좋아요",
          "original_form": "왓챠에 있는데 에피소드 짧아서 좋아요",
          "speaker_id": "1",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100033645.1.5",
          "form": "아ㅠㅠㅠ그거 너무 현실이라 못보겠던데ㅋㅋㅋ",
          "original_form": "아ㅠㅠㅠ그거 너무 현실이라 못보겠던데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100033645.1.6",
          "form": "ㅋㅋㅋ넘 현실이라 더 흥미진진하더라구요",
          "original_form": "ㅋㅋㅋ넘 현실이라 더 흥미진진하더라구요",
          "speaker_id": "1",
          "time": "20210824 19:57"
        },
        {
          "id": "MDRW2100033645.1.7",
          "form": "너무 재밌다는데ㅠㅠㅠ그냥 썸네일만 봐도 갑갑해요ㅋㅋ",
          "original_form": "너무 재밌다는데ㅠㅠㅠ그냥 썸네일만 봐도 갑갑해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:58"
        },
        {
          "id": "MDRW2100033645.1.8",
          "form": "ㅋㅋ 리얼리즘은 싫으시군요 ㅋㅋ",
          "original_form": "ㅋㅋ 리얼리즘은 싫으시군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100033645.1.9",
          "form": "미생도 못봤어요ㅋㅋ그건 판타지같다고 하던데 구래도ㅋㅋ",
          "original_form": "미생도 못봤어요ㅋㅋ그건 판타지같다고 하던데 구래도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100033645.1.10",
          "form": "미생이 판타지라구요? 또잉 ㅋㅋ",
          "original_form": "미생이 판타지라구요? 또잉 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100033645.1.11",
          "form": "하긴 그럴수도있겠다",
          "original_form": "하긴 그럴수도있겠다",
          "speaker_id": "1",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100033645.1.12",
          "form": "안봐서 다큐인지 판타지인지 잘모르지만요ㅋㅋ",
          "original_form": "안봐서 다큐인지 판타지인지 잘모르지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:20"
        },
        {
          "id": "MDRW2100033645.1.13",
          "form": "그럼 name2님은 어떤장르가취향이신가요",
          "original_form": "그럼 &name2&님은 어떤장르가취향이신가요",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100033645.1.14",
          "form": "저는 그냥 가볍게 보는게 좋아요ㅎㅎ",
          "original_form": "저는 그냥 가볍게 보는게 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100033645.1.15",
          "form": "짱구같은거요?",
          "original_form": "짱구같은거요?",
          "speaker_id": "1",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100033645.1.16",
          "form": "애니는 코난 좀 보구...잘안봐서 모르겠네요ㅎㅎ",
          "original_form": "애니는 코난 좀 보구...잘안봐서 모르겠네요ㅎㅎ",
