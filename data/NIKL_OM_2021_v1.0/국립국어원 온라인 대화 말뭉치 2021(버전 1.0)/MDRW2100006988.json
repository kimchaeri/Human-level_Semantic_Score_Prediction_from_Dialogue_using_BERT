      "utterance": [
        {
          "id": "MDRW2100006988.1.1",
          "form": "안녕하세요^^",
          "original_form": "안녕하세요^^",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006988.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006988.1.3",
          "form": "날씨 얘기해야하는건가요?",
          "original_form": "날씨 얘기해야하는건가요?",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006988.1.4",
          "form": "지금 비는 그친것 같던데",
          "original_form": "지금 비는 그친것 같던데",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006988.1.5",
          "form": "그런가봐요  남부지방 비많이오던데",
          "original_form": "그런가봐요  남부지방 비많이오던데",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006988.1.6",
          "form": "남쪽이세요?",
          "original_form": "남쪽이세요?",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006988.1.7",
          "form": "지금도 비가 오려나요?",
          "original_form": "지금도 비가 오려나요?",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006988.1.8",
          "form": "전 서울이요",
          "original_form": "전 서울이요",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006988.1.9",
          "form": "여긴 많이오는거같진않아요",
          "original_form": "여긴 많이오는거같진않아요",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006988.1.10",
          "form": "경북입니당.",
          "original_form": "경북입니당.",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006988.1.11",
          "form": "태풍온다던데 ㅠ",
          "original_form": "태풍온다던데 ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006988.1.12",
          "form": "서울은 영향권은 아닌듯해요",
          "original_form": "서울은 영향권은 아닌듯해요",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006988.1.13",
          "form": "내일은 맑았음 좋겠는데",
          "original_form": "내일은 맑았음 좋겠는데",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006988.1.14",
          "form": "부산도 난리났던데 괜찮으신가요?",
          "original_form": "부산도 난리났던데 괜찮으신가요?",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006988.1.15",
          "form": "아 부산은 아니여서;;;",
          "original_form": "아 부산은 아니여서;;;",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006988.1.16",
          "form": "부산 비 많이왔나여?",
          "original_form": "부산 비 많이왔나여?",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006988.1.17",
          "form": "뉴스를 안봐서 몰랐네요",
          "original_form": "뉴스를 안봐서 몰랐네요",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006988.1.18",
          "form": "담주까지 비온대요",
          "original_form": "담주까지 비온대요",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006988.1.19",
          "form": "와 오랫만에 채팅하니까 어색^^;;;",
          "original_form": "와 오랫만에 채팅하니까 어색^^;;;",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006988.1.20",
          "form": "헐 장마도 없었는데..",
          "original_form": "헐 장마도 없었는데..",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006988.1.21",
          "form": "요새 날씨 왜이러죠?",
          "original_form": "요새 날씨 왜이러죠?",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006988.1.22",
          "form": "ㅎㅎ 가을장마온다던데요",
          "original_form": "ㅎㅎ 가을장마온다던데요",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006988.1.23",
          "form": "지구가 아픈가봐요",
          "original_form": "지구가 아픈가봐요",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006988.1.24",
          "form": "급 춥더니 비오고 종잡을수가 없어요",
          "original_form": "급 춥더니 비오고 종잡을수가 없어요",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006988.1.25",
          "form": "비오는거 좀..별로라 ㅠ",
          "original_form": "비오는거 좀..별로라 ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006988.1.26",
          "form": "진짜 기후변화가 체감되는기분이예요",
          "original_form": "진짜 기후변화가 체감되는기분이예요",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006988.1.27",
          "form": "해수면도 상승한다고하고..",
          "original_form": "해수면도 상승한다고하고..",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006988.1.28",
          "form": "비오고나면 가을오지 싶어요",
          "original_form": "비오고나면 가을오지 싶어요",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006988.1.29",
          "form": "장마는 짧고 여름은 내리쬐고",
          "original_form": "장마는 짧고 여름은 내리쬐고",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006988.1.30",
          "form": "그니까요 봄가을도 짧아지고",
          "original_form": "그니까요 봄가을도 짧아지고",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006988.1.31",
          "form": "가을  짧게 지나갈까 걱정입니당ㅠ",
          "original_form": "가을  짧게 지나갈까 걱정입니당ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006988.1.32",
          "form": "겨울 추위가 벌써부터 걱정이라",
          "original_form": "겨울 추위가 벌써부터 걱정이라",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006988.1.33",
          "form": "올해도 짧을거같아요",
          "original_form": "올해도 짧을거같아요",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006988.1.34",
          "form": "그러게요 짧아지는게 확실한듯",
          "original_form": "그러게요 짧아지는게 확실한듯",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006988.1.35",
          "form": "요샌 비도 열대지방처럼 스콜이고 무섭네여",
          "original_form": "요샌 비도 열대지방처럼 스콜이고 무섭네여",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006988.1.36",
          "form": "단풍구경이나 언능하고싶어요",
          "original_form": "단풍구경이나 언능하고싶어요",
          "speaker_id": "2",
          "time": "20210822 00:22"
        }
      ]
    }
