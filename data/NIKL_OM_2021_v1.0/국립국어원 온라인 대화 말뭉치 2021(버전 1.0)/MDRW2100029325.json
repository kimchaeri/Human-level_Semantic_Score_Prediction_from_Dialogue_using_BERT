      "utterance": [
        {
          "id": "MDRW2100029325.1.1",
          "form": "김세정 옹성우 김종국 유재석",
          "original_form": "김세정 옹성우 김종국 유재석",
          "speaker_id": "2",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029325.1.2",
          "form": "김세정 귀여움",
          "original_form": "김세정 귀여움",
          "speaker_id": "2",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029325.1.3",
          "form": "여자 맞죠?",
          "original_form": "여자 맞죠?",
          "speaker_id": "2",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029325.1.4",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029325.1.5",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210903 01:34"
        },
        {
          "id": "MDRW2100029325.1.6",
          "form": "진짜 졸귀",
          "original_form": "진짜 졸귀",
          "speaker_id": "2",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029325.1.7",
          "form": "앞의 두명은 프듀 12보고 팬 됐죠ㅎㅎ",
          "original_form": "앞의 두명은 프듀 12보고 팬 됐죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029325.1.8",
          "form": "아 그걸 봤어야되는데",
          "original_form": "아 그걸 봤어야되는데",
          "speaker_id": "2",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029325.1.9",
          "form": "저는 티비를 잘 안봐서",
          "original_form": "저는 티비를 잘 안봐서",
          "speaker_id": "2",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029325.1.10",
          "form": "드라마로 접하셨나요",
          "original_form": "드라마로 접하셨나요",
          "speaker_id": "1",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029325.1.11",
          "form": "아녀 가끔 돌리다가",
          "original_form": "아녀 가끔 돌리다가",
          "speaker_id": "2",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029325.1.12",
          "form": "드라마도 안봄니다 ㅋㅋ",
          "original_form": "드라마도 안봄니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029325.1.13",
          "form": "슬의만 봐요",
          "original_form": "슬의만 봐요",
          "speaker_id": "2",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029325.1.14",
          "form": "실은 저도...티비는 필요에 의해 보는 뉴스 외엔 잘 안봐요",
          "original_form": "실은 저도...티비는 필요에 의해 보는 뉴스 외엔 잘 안봐요",
          "speaker_id": "1",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029325.1.15",
          "form": "무슨뉴스보시나요?",
          "original_form": "무슨뉴스보시나요?",
          "speaker_id": "2",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029325.1.16",
          "form": "YTN?",
          "original_form": "YTN?",
          "speaker_id": "2",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029325.1.17",
          "form": "그래서 차선호 나온것을 본게 없죠ㅎ",
          "original_form": "그래서 차선호 나온것을 본게 없죠ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029325.1.18",
          "form": "회사에선 그거 틀어놓고 있죠",
          "original_form": "회사에선 그거 틀어놓고 있죠",
          "speaker_id": "1",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029325.1.19",
          "form": "집에선.. 아버지께서 하루종일 채널에이를 틀고 계셔서;",
          "original_form": "집에선.. 아버지께서 하루종일 채널에이를 틀고 계셔서;",
          "speaker_id": "1",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029325.1.20",
          "form": "차선호가 사람이네요",
          "original_form": "차선호가 사람이네요",
          "speaker_id": "2",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029325.1.21",
          "form": "아버님도 언론인?",
          "original_form": "아버님도 언론인?",
          "speaker_id": "2",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029325.1.22",
          "form": "그냥 성향 상관없이 집선 안 봅니다ㅎ",
          "original_form": "그냥 성향 상관없이 집선 안 봅니다ㅎ",
          "speaker_id": "1",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029325.1.23",
          "form": "아뇨! 그냥 은퇴한 직장인이요",
          "original_form": "아뇨! 그냥 은퇴한 직장인이요",
          "speaker_id": "1",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029325.1.24",
          "form": "아 어른들은 보시는것만 보시더라구요",
          "original_form": "아 어른들은 보시는것만 보시더라구요",
          "speaker_id": "2",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029325.1.25",
          "form": "네 이게 균형감을 유지해야 해서ㅜ",
          "original_form": "네 이게 균형감을 유지해야 해서ㅜ",
          "speaker_id": "1",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029325.1.26",
          "form": "경상도는 티비조선 많이 보시더라구요",
          "original_form": "경상도는 티비조선 많이 보시더라구요",
          "speaker_id": "2",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029325.1.27",
          "form": "신문도 골고루 보거든요 저는",
          "original_form": "신문도 골고루 보거든요 저는",
          "speaker_id": "1",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029325.1.28",
          "form": "건강하십니다!",
          "original_form": "건강하십니다!",
          "speaker_id": "2",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029325.1.29",
          "form": "아 연애인데",
          "original_form": "아 연애인데",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029325.1.30",
          "form": "저 잠이 쏟아져서요 ㅠ",
          "original_form": "저 잠이 쏟아져서요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029325.1.31",
          "form": "이상형이 김세정 옹성우 김종국 유재석 하휘동입니다!!",
          "original_form": "이상형이 김세정 옹성우 김종국 유재석 하휘동입니다!!",
          "speaker_id": "1",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029325.1.32",
          "form": "주무세요ㅎㅎ",
          "original_form": "주무세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029325.1.33",
          "form": "기억하겠습니다",
          "original_form": "기억하겠습니다",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029325.1.34",
          "form": "하휘동",
          "original_form": "하휘동",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029325.1.35",
          "form": "ㅎㅎ 굿나잇!!!",
          "original_form": "ㅎㅎ 굿나잇!!!",
          "speaker_id": "1",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029325.1.36",
          "form": "검색해보고 자야종",
          "original_form": "검색해보고 자야종",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029325.1.37",
          "form": "낼봐용 ㅎㅎ",
          "original_form": "낼봐용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029325.1.38",
          "form": "그분은 댄서ㅎㅎ",
          "original_form": "그분은 댄서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029325.1.39",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210903 01:40"
        }
      ]
