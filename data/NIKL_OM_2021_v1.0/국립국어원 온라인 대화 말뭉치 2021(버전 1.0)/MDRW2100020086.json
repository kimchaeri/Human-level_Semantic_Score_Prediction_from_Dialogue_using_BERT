      "utterance": [
        {
          "id": "MDRW2100020086.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020086.1.2",
          "form": "아니요 졸업한지 오래되었어요",
          "original_form": "아니요 졸업한지 오래되었어요",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020086.1.3",
          "form": "그러시구나 저도 오래되었네요",
          "original_form": "그러시구나 저도 오래되었네요",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020086.1.4",
          "form": "동아리활동 학교다닐때 하셨나요?",
          "original_form": "동아리활동 학교다닐때 하셨나요?",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020086.1.5",
          "form": "학교에 정이 안들어서 끝나고 늘 집에 갔어요...하하",
          "original_form": "학교에 정이 안들어서 끝나고 늘 집에 갔어요...하하",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020086.1.6",
          "form": "그래도 고등학교2학년때 선생님 정말 좋은 분이셨어요",
          "original_form": "그래도 고등학교2학년때 선생님 정말 좋은 분이셨어요",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020086.1.7",
          "form": "어떤분이셨어요?",
          "original_form": "어떤분이셨어요?",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020086.1.8",
          "form": "성적으로 아이들을 압박하지않았어요",
          "original_form": "성적으로 아이들을 압박하지않았어요",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020086.1.9",
          "form": "친절하고 다정했지요",
          "original_form": "친절하고 다정했지요",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020086.1.10",
          "form": "참스승이시네요",
          "original_form": "참스승이시네요",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020086.1.11",
          "form": "어떤과목 당담이셨나요?",
          "original_form": "어떤과목 당담이셨나요?",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020086.1.12",
          "form": "국어였어요",
          "original_form": "국어였어요",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020086.1.13",
          "form": "그러셨구나 1,3학년땐 성적으로 아이들을 압박하는 분이 계셨나요?",
          "original_form": "그러셨구나 1,3학년땐 성적으로 아이들을 압박하는 분이 계셨나요?",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020086.1.14",
          "form": "1학년때 선생님이 젊어서 그런지 좀 심했어요",
          "original_form": "1학년때 선생님이 젊어서 그런지 좀 심했어요",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020086.1.15",
          "form": "그분도 국어였네요",
          "original_form": "그분도 국어였네요",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020086.1.16",
          "form": "같은 국어담당이신데...에궁",
          "original_form": "같은 국어담당이신데...에궁",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020086.1.17",
          "form": "어떻게 압박하셨나요?",
          "original_form": "어떻게 압박하셨나요?",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020086.1.18",
          "form": "성적으로 차별했나요?",
          "original_form": "성적으로 차별했나요?",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020086.1.19",
          "form": "그러게요 요즘 아이들 수도 줄어드니까 학업과잉경쟁도 줄어들길 바랄뿐입니다",
          "original_form": "그러게요 요즘 아이들 수도 줄어드니까 학업과잉경쟁도 줄어들길 바랄뿐입니다",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020086.1.20",
          "form": "아니요 손바닥 때렸어요 그때는 체벌이 가능했으니까",
          "original_form": "아니요 손바닥 때렸어요 그때는 체벌이 가능했으니까",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020086.1.21",
          "form": "아...너무 하셨다 정말... 그런식으로는 학업에 도움도 안되는데...",
          "original_form": "아...너무 하셨다 정말... 그런식으로는 학업에 도움도 안되는데...",
          "speaker_id": "1",
          "time": "20210827 23:49"
        }
      ]
