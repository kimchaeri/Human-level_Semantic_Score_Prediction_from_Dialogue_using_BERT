      "utterance": [
        {
          "id": "MDRW2100022940.1.1",
          "form": "결혼안했지",
          "original_form": "결혼안했지",
          "speaker_id": "2",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022940.1.2",
          "form": "몇살인데",
          "original_form": "몇살인데",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022940.1.3",
          "form": "나 22살이야",
          "original_form": "나 22살이야",
          "speaker_id": "2",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022940.1.4",
          "form": "우아 늙었다 근데 안했어?",
          "original_form": "우아 늙었다 근데 안했어?",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022940.1.5",
          "form": "22살이 왜 결혼해",
          "original_form": "22살이 왜 결혼해",
          "speaker_id": "2",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022940.1.6",
          "form": "빨리했을수도 있지",
          "original_form": "빨리했을수도 있지",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022940.1.7",
          "form": "너는 어떤 사람이랑 결혼 할거야?",
          "original_form": "너는 어떤 사람이랑 결혼 할거야?",
          "speaker_id": "2",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100022940.1.8",
          "form": "이쁘고 섹시하고 귀엽고 재롱잘피우고 나만보는 착한아이",
          "original_form": "이쁘고 섹시하고 귀엽고 재롱잘피우고 나만보는 착한아이",
          "speaker_id": "1",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100022940.1.9",
          "form": "바라는게 많네",
          "original_form": "바라는게 많네",
          "speaker_id": "2",
          "time": "20210829 19:28"
        },
        {
          "id": "MDRW2100022940.1.10",
          "form": "넌 아니야?",
          "original_form": "넌 아니야?",
          "speaker_id": "1",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022940.1.11",
          "form": "나는 경제관념있고 세상살아가는데 필요한 지식있는 사람이랑 결혼하고싶어",
          "original_form": "나는 경제관념있고 세상살아가는데 필요한 지식있는 사람이랑 결혼하고싶어",
          "speaker_id": "2",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022940.1.12",
          "form": "어 그럼 난 아니다",
          "original_form": "어 그럼 난 아니다",
          "speaker_id": "1",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022940.1.13",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210829 19:30"
        },
        {
          "id": "MDRW2100022940.1.14",
          "form": "난 멍청하고 경제관념없어서",
          "original_form": "난 멍청하고 경제관념없어서",
          "speaker_id": "1",
          "time": "20210829 19:30"
        },
        {
          "id": "MDRW2100022940.1.15",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210829 19:30"
        },
        {
          "id": "MDRW2100022940.1.16",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100022940.1.17",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100022940.1.18",
          "form": "나는 경제관념은 잇러",
          "original_form": "나는 경제관념은 잇러",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100022940.1.19",
          "form": "있어",
          "original_form": "있어",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100022940.1.20",
          "form": "너는 몇살에 결혼할거야?",
          "original_form": "너는 몇살에 결혼할거야?",
          "speaker_id": "2",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100022940.1.21",
          "form": "나는 빨리하고싶어 사랑하는 사람 있어!",
          "original_form": "나는 빨리하고싶어 사랑하는 사람 있어!",
          "speaker_id": "1",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100022940.1.22",
          "form": "오 정확히 몇살때하고싶은데?",
          "original_form": "오 정확히 몇살때하고싶은데?",
          "speaker_id": "2",
          "time": "20210831 19:19"
        },
        {
          "id": "MDRW2100022940.1.23",
          "form": "나 27아님 28! 여기 부소대장님도 27인데 전에 우0딩사진두 찍구 다하샸어 식은 안올리샸을걸",
          "original_form": "나 27아님 28! 여기 부소대장님도 27인데 전에 우0딩사진두 찍구 다하샸어 식은 안올리샸을걸",
          "speaker_id": "1",
          "time": "20210831 19:19"
        },
        {
          "id": "MDRW2100022940.1.24",
          "form": "헐 엄청 빨리하는구나",
          "original_form": "헐 엄청 빨리하는구나",
          "speaker_id": "2",
          "time": "20210831 19:19"
        }
      ]
