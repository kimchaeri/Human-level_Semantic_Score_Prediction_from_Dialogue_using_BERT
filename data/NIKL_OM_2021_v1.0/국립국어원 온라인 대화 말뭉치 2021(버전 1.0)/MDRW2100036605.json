      "utterance": [
        {
          "id": "MDRW2100036605.1.1",
          "form": "안녕하세요 운동하시나요?",
          "original_form": "안녕하세요 운동하시나요?",
          "speaker_id": "1",
          "time": "20210831 01:44"
        },
        {
          "id": "MDRW2100036605.1.2",
          "form": "넹 ㅋㅋ그냥 이것저것.. 가끔씩..",
          "original_form": "넹 ㅋㅋ그냥 이것저것.. 가끔씩..",
          "speaker_id": "2",
          "time": "20210831 01:45"
        },
        {
          "id": "MDRW2100036605.1.3",
          "form": "운동잘하시나봐요 여러개 하시다니...",
          "original_form": "운동잘하시나봐요 여러개 하시다니...",
          "speaker_id": "1",
          "time": "20210831 01:45"
        },
        {
          "id": "MDRW2100036605.1.4",
          "form": "잘은 못하고 끄적끄적해요 ㅠ",
          "original_form": "잘은 못하고 끄적끄적해요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 01:47"
        },
        {
          "id": "MDRW2100036605.1.5",
          "form": "걍 피티받구요 ㅋㅋㅋ",
          "original_form": "걍 피티받구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:47"
        },
        {
          "id": "MDRW2100036605.1.6",
          "form": "name1님은용",
          "original_form": "&name1&님은용",
          "speaker_id": "2",
          "time": "20210831 01:47"
        },
        {
          "id": "MDRW2100036605.1.7",
          "form": "저는 잘못해요",
          "original_form": "저는 잘못해요",
          "speaker_id": "1",
          "time": "20210831 01:48"
        },
        {
          "id": "MDRW2100036605.1.8",
          "form": "수영 하나만 할줄알아요",
          "original_form": "수영 하나만 할줄알아요",
          "speaker_id": "1",
          "time": "20210831 01:48"
        },
        {
          "id": "MDRW2100036605.1.9",
          "form": "오! 수영 저도 수영좋아해용",
          "original_form": "오! 수영 저도 수영좋아해용",
          "speaker_id": "2",
          "time": "20210831 01:48"
        },
        {
          "id": "MDRW2100036605.1.10",
          "form": "얼마나하셨나용",
          "original_form": "얼마나하셨나용",
          "speaker_id": "2",
          "time": "20210831 01:49"
        },
        {
          "id": "MDRW2100036605.1.11",
          "form": "2-3년정도했어요",
          "original_form": "2-3년정도했어요",
          "speaker_id": "1",
          "time": "20210831 01:50"
        },
        {
          "id": "MDRW2100036605.1.12",
          "form": "와ㅋㅋㅋ 수영 넘 어려워요",
          "original_form": "와ㅋㅋㅋ 수영 넘 어려워요",
          "speaker_id": "2",
          "time": "20210831 01:50"
        },
        {
          "id": "MDRW2100036605.1.13",
          "form": "얼마나자주하세용",
          "original_form": "얼마나자주하세용",
          "speaker_id": "2",
          "time": "20210831 01:50"
        },
        {
          "id": "MDRW2100036605.1.14",
          "form": "안한지 1년넘었어요",
          "original_form": "안한지 1년넘었어요",
          "speaker_id": "1",
          "time": "20210831 01:50"
        },
        {
          "id": "MDRW2100036605.1.15",
          "form": "아 ㅋㅋㅋ 전 접영잘하고싶어요",
          "original_form": "아 ㅋㅋㅋ 전 접영잘하고싶어요",
          "speaker_id": "2",
          "time": "20210831 01:51"
        },
        {
          "id": "MDRW2100036605.1.16",
          "form": "네  저도 접영을 젤못해요  ㅜ",
          "original_form": "네  저도 접영을 젤못해요  ㅜ",
          "speaker_id": "1",
          "time": "20210831 01:57"
        },
        {
          "id": "MDRW2100036605.1.17",
          "form": "힘을 잘 못쓰는것 같아요 흐름타는것도 힘들고",
          "original_form": "힘을 잘 못쓰는것 같아요 흐름타는것도 힘들고",
          "speaker_id": "2",
          "time": "20210831 01:59"
        },
        {
          "id": "MDRW2100036605.1.18",
          "form": "네 뭔가 트여야할거같은데",
          "original_form": "네 뭔가 트여야할거같은데",
          "speaker_id": "1",
          "time": "20210831 02:00"
        },
        {
          "id": "MDRW2100036605.1.19",
          "form": "아직 요령을 잘 모르겠어요",
          "original_form": "아직 요령을 잘 모르겠어요",
