      "utterance": [
        {
          "id": "MDRW2100032414.1.1",
          "form": "겨울은 어떻게 생각하세요",
          "original_form": "겨울은 어떻게 생각하세요",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032414.1.2",
          "form": "겨울은 시원해서 좋아요",
          "original_form": "겨울은 시원해서 좋아요",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032414.1.3",
          "form": "추우니까 땀을 흘리지 않겠네요",
          "original_form": "추우니까 땀을 흘리지 않겠네요",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032414.1.4",
          "form": "하지만 겨드랑이는 겨터파크 개장합니다",
          "original_form": "하지만 겨드랑이는 겨터파크 개장합니다",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032414.1.5",
          "form": "맞아요 스웨터를 입으면 겨드랑이에 땀이납니다",
          "original_form": "맞아요 스웨터를 입으면 겨드랑이에 땀이납니다",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032414.1.6",
          "form": "그거빼고 공기자 차가운건 좋아요",
          "original_form": "그거빼고 공기자 차가운건 좋아요",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032414.1.7",
          "form": "너무 추우니까 건조 하더라고요",
          "original_form": "너무 추우니까 건조 하더라고요",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032414.1.8",
          "form": "바디로션을 꼭 챙겨서 발라야 합니다",
          "original_form": "바디로션을 꼭 챙겨서 발라야 합니다",
          "speaker_id": "1",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032414.1.9",
          "form": "겨울엔 크리스마스 분위기가 좋아요",
          "original_form": "겨울엔 크리스마스 분위기가 좋아요",
          "speaker_id": "2",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032414.1.10",
          "form": "크리스마스 연말이라 분위기 있지요",
          "original_form": "크리스마스 연말이라 분위기 있지요",
          "speaker_id": "1",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032414.1.11",
          "form": "이번 겨울은 좀 나가서 사람도 만나고 하면 좋겠어요",
          "original_form": "이번 겨울은 좀 나가서 사람도 만나고 하면 좋겠어요",
          "speaker_id": "2",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032414.1.12",
          "form": "저도 그러기를 바랍니다 덜 춥기를",
          "original_form": "저도 그러기를 바랍니다 덜 춥기를",
          "speaker_id": "1",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032414.1.13",
          "form": "겨울에는 꼭 밥을 먹어야죠",
          "original_form": "겨울에는 꼭 밥을 먹어야죠",
          "speaker_id": "2",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032414.1.14",
          "form": "배둘레 춥지 않도록 비축해야지요",
          "original_form": "배둘레 춥지 않도록 비축해야지요",
          "speaker_id": "1",
