      "utterance": [
        {
          "id": "MDRW2100010688.1.1",
          "form": "저는 무교예요",
          "original_form": "저는 무교예요",
          "speaker_id": "2",
          "time": "20210824 11:51"
        },
        {
          "id": "MDRW2100010688.1.2",
          "form": "저도 무교입니다 근데 전에 교회를 다녔었어요",
          "original_form": "저도 무교입니다 근데 전에 교회를 다녔었어요",
          "speaker_id": "1",
          "time": "20210824 11:52"
        },
        {
          "id": "MDRW2100010688.1.3",
          "form": "오 모태신앙이신가요",
          "original_form": "오 모태신앙이신가요",
          "speaker_id": "2",
          "time": "20210824 11:52"
        },
        {
          "id": "MDRW2100010688.1.4",
          "form": "아뇨 초등학교부터 고등학생 때까지 다니다가 그만뒀죠",
          "original_form": "아뇨 초등학교부터 고등학생 때까지 다니다가 그만뒀죠",
          "speaker_id": "1",
          "time": "20210824 11:54"
        },
        {
          "id": "MDRW2100010688.1.5",
          "form": "왜 그만두셨나요",
          "original_form": "왜 그만두셨나요",
          "speaker_id": "2",
          "time": "20210824 11:54"
        },
        {
          "id": "MDRW2100010688.1.6",
          "form": "다른 할일이 더 중요해져서요",
          "original_form": "다른 할일이 더 중요해져서요",
          "speaker_id": "1",
          "time": "20210824 11:55"
        },
        {
          "id": "MDRW2100010688.1.7",
          "form": "name2은 절이나 교회 가신 적 한 번도 없으신가요?",
          "original_form": "&name2&은 절이나 교회 가신 적 한 번도 없으신가요?",
          "speaker_id": "1",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100010688.1.8",
          "form": "교회는 초등학생때 문화상품권 준다고해서 딱 한번 가봤습니다",
          "original_form": "교회는 초등학생때 문화상품권 준다고해서 딱 한번 가봤습니다",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100010688.1.9",
          "form": "ㅋㅋㅋ초등학생 때는 다들 그렇죠... 그럼 그 이후에는 한번도 가보신 적 없으신가요?",
          "original_form": "ㅋㅋㅋ초등학생 때는 다들 그렇죠... 그럼 그 이후에는 한번도 가보신 적 없으신가요?",
          "speaker_id": "1",
          "time": "20210824 11:57"
        },
        {
          "id": "MDRW2100010688.1.10",
          "form": "절은 봉은사 다녀온적있어요",
          "original_form": "절은 봉은사 다녀온적있어요",
          "speaker_id": "2",
          "time": "20210824 11:57"
        },
        {
          "id": "MDRW2100010688.1.11",
          "form": "거기서 뭘 하셨나요?",
          "original_form": "거기서 뭘 하셨나요?",
          "speaker_id": "1",
          "time": "20210824 12:03"
        },
        {
          "id": "MDRW2100010688.1.12",
          "form": "절 구경하고 스님이 종치는 걸 구경했어요",
          "original_form": "절 구경하고 스님이 종치는 걸 구경했어요",
          "speaker_id": "2",
          "time": "20210824 12:06"
        },
        {
          "id": "MDRW2100010688.1.13",
          "form": "저는 절을 한 번도 안가봐서 가보고 싶네요",
          "original_form": "저는 절을 한 번도 안가봐서 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210824 12:07"
        },
        {
          "id": "MDRW2100010688.1.14",
          "form": "오랜만에 절밥 먹고싶네요",
          "original_form": "오랜만에 절밥 먹고싶네요",
