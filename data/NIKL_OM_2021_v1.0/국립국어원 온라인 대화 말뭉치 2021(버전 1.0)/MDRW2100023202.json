        {
          "id": "MDRW2100023202.1.1",
          "form": "리얼월드에 크라임씬 게임 해보셨어요?",
          "original_form": "리얼월드에 크라임씬 게임 해보셨어요?",
          "speaker_id": "1",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023202.1.2",
          "form": "아니용ㅎㅎ",
          "original_form": "아니용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100023202.1.3",
          "form": "제가 게임은 잘 안해서",
          "original_form": "제가 게임은 잘 안해서",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100023202.1.4",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100023202.1.5",
          "form": "재밌나용??",
          "original_form": "재밌나용??",
          "speaker_id": "2",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100023202.1.6",
          "form": "저거 게임이라기 보다",
          "original_form": "저거 게임이라기 보다",
          "speaker_id": "1",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100023202.1.7",
          "form": "방탈출?같은건데요",
          "original_form": "방탈출?같은건데요",
          "speaker_id": "1",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100023202.1.8",
          "form": "어플로 친구들 하고 하는거예요",
          "original_form": "어플로 친구들 하고 하는거예요",
          "speaker_id": "1",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100023202.1.9",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210830 00:06"
        },
        {
          "id": "MDRW2100023202.1.10",
          "form": "온라인 방 탈출인 건가요??",
          "original_form": "온라인 방 탈출인 건가요??",
          "speaker_id": "2",
          "time": "20210830 00:07"
        },
        {
          "id": "MDRW2100023202.1.11",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210830 00:08"
        },
        {
          "id": "MDRW2100023202.1.12",
          "form": "9월 1일까지인가? 무료로 풀려있는데",
          "original_form": "9월 1일까지인가? 무료로 풀려있는데",
          "speaker_id": "1",
          "time": "20210830 00:08"
        },
        {
          "id": "MDRW2100023202.1.13",
          "form": "친구들하고 줌 켜두고 하기좋아요",
          "original_form": "친구들하고 줌 켜두고 하기좋아요",
          "speaker_id": "1",
          "time": "20210830 00:09"
        },
        {
          "id": "MDRW2100023202.1.14",
          "form": "재밌을 것 같네요!!",
          "original_form": "재밌을 것 같네요!!",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023202.1.15",
          "form": "친구들한테 같이 하자고 물어봐야겠어요ㅎㅎ",
          "original_form": "친구들한테 같이 하자고 물어봐야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023202.1.16",
          "form": "크라임씬 프로그램. 보셨으면 금방 규칙 잘아실거예요",
          "original_form": "크라임씬 프로그램. 보셨으면 금방 규칙 잘아실거예요",
          "speaker_id": "1",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023202.1.17",
          "form": "크라임씬 안봤어요ㅠㅠㅠ",
          "original_form": "크라임씬 안봤어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 08:34"
        },
        {
          "id": "MDRW2100023202.1.18",
          "form": "저두 안봤는데 친구한명이 봐서",
          "original_form": "저두 안봤는데 친구한명이 봐서",
          "speaker_id": "1",
          "time": "20210830 08:38"
        },
        {
          "id": "MDRW2100023202.1.19",
          "form": "대충진행이 되었어요",
          "original_form": "대충진행이 되었어요",
          "speaker_id": "1",
          "time": "20210830 08:38"
        },
        {
          "id": "MDRW2100023202.1.20",
          "form": "오 그렇군용",
          "original_form": "오 그렇군용",
          "speaker_id": "2",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100023202.1.21",
          "form": "크라임씬 말고 다른 게임 하는 거 있으세요??",
          "original_form": "크라임씬 말고 다른 게임 하는 거 있으세요??",
          "speaker_id": "2",
          "time": "20210830 10:34"
        },
        {
          "id": "MDRW2100023202.1.22",
          "form": "저는 pc게임도 하고 닌텐도게임도 해요",
          "original_form": "저는 pc게임도 하고 닌텐도게임도 해요",
          "speaker_id": "1",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100023202.1.23",
          "form": "게임좋아하는편이라서요",
          "original_form": "게임좋아하는편이라서요",
          "speaker_id": "1",
          "time": "20210830 10:44"
        },
        {
          "id": "MDRW2100023202.1.24",
          "form": "많이 하시네요!!",
          "original_form": "많이 하시네요!!",
          "speaker_id": "2",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100023202.1.25",
          "form": "저도 예전에 롤 조금 했었는데",
          "original_form": "저도 예전에 롤 조금 했었는데",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023202.1.26",
          "form": "멀미 나서 못하겠더라구요ㅠㅠ",
          "original_form": "멀미 나서 못하겠더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023202.1.27",
          "form": "전 롤 너무 캐릭터많아서 못하겠어요",
