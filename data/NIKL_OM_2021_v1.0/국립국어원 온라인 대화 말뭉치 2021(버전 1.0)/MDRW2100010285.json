      "utterance": [
        {
          "id": "MDRW2100010285.1.1",
          "form": "다이어트하시나요?",
          "original_form": "다이어트하시나요?",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010285.1.2",
          "form": "아니요~ ㅎㅎ",
          "original_form": "아니요~ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010285.1.3",
          "form": "다이어트 하는 중이세여?",
          "original_form": "다이어트 하는 중이세여?",
          "speaker_id": "2",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010285.1.4",
          "form": "아니용 ㅎ ㅎ",
          "original_form": "아니용 ㅎ ㅎ",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010285.1.5",
          "form": "점심으로 김치찌개먹었습니다",
          "original_form": "점심으로 김치찌개먹었습니다",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010285.1.6",
          "form": "다이어트잠깐 쉬고있어요ㅠㅠ",
          "original_form": "다이어트잠깐 쉬고있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 16:27"
        },
        {
          "id": "MDRW2100010285.1.7",
          "form": "다이어트는 왜 하시는거예요? 체중조절?",
          "original_form": "다이어트는 왜 하시는거예요? 체중조절?",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010285.1.8",
          "form": "다이어트 넘나 어렵 ㅜㅜ",
          "original_form": "다이어트 넘나 어렵 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010285.1.9",
          "form": "건강위해서 챙겨먹는것도 너무 어려운데 식단조절하는건 너무 힘든일인거 같아요ㅠㅠ",
          "original_form": "건강위해서 챙겨먹는것도 너무 어려운데 식단조절하는건 너무 힘든일인거 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010285.1.10",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010285.1.11",
          "form": "넵 체중조절이요..패션에 관심이 있다보니ㅠㅠㅠ",
          "original_form": "넵 체중조절이요..패션에 관심이 있다보니ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010285.1.12",
          "form": "그렇군요.. 건강을 위해서는 몰라도 너무 무리하지는 마세요",
          "original_form": "그렇군요.. 건강을 위해서는 몰라도 너무 무리하지는 마세요",
          "speaker_id": "2",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100010285.1.13",
          "form": "저는 환절기라서 비염이 다시 시작했어요ㅠㅠㅜㅜ",
          "original_form": "저는 환절기라서 비염이 다시 시작했어요ㅠㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010285.1.14",
          "form": "혹시 비염 있으세요?",
          "original_form": "혹시 비염 있으세요?",
          "speaker_id": "2",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010285.1.15",
          "form": "헉 맞아요...",
          "original_form": "헉 맞아요...",
          "speaker_id": "1",
          "time": "20210823 21:13"
        },
        {
          "id": "MDRW2100010285.1.16",
          "form": "저도있답니다",
          "original_form": "저도있답니다",
          "speaker_id": "1",
          "time": "20210823 21:13"
        },
        {
          "id": "MDRW2100010285.1.17",
          "form": "편도비대증이있어서수술했어요",
          "original_form": "편도비대증이있어서수술했어요",
          "speaker_id": "1",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010285.1.18",
          "form": "아아 그게 혹시 아데노이드? 인가요??",
          "original_form": "아아 그게 혹시 아데노이드? 인가요??",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010285.1.19",
          "form": "제 동생도 어렸을때 비염도 심하고 축농증이 엄청심해서 그 수술했었는데!!",
          "original_form": "제 동생도 어렸을때 비염도 심하고 축농증이 엄청심해서 그 수술했었는데!!",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010285.1.20",
          "form": "ㅜㅜㅜ 진짜 지난 주말부터 비염때문에 난리예요ㅠㅠ",
          "original_form": "ㅜㅜㅜ 진짜 지난 주말부터 비염때문에 난리예요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010285.1.21",
          "form": "요즘 건강관리는 어떻게 하세요?",
          "original_form": "요즘 건강관리는 어떻게 하세요?",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100010285.1.22",
          "form": "네 아데노이드요ㅠㅠㅠ",
          "original_form": "네 아데노이드요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100010285.1.23",
          "form": "지금봤네요ㅠㅠ죄송해요ㅜㅠㅠ",
          "original_form": "지금봤네요ㅠㅠ죄송해요ㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100010285.1.24",
          "form": "오늘 백신맞고왔어요!",
          "original_form": "오늘 백신맞고왔어요!",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100010285.1.25",
          "form": "코로나어떡하세요ㅠㅠ",
          "original_form": "코로나어떡하세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:05"
        },
        {
          "id": "MDRW2100010285.1.26",
          "form": "오 화이자 맞으셨어요??",
          "original_form": "오 화이자 맞으셨어요??",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100010285.1.27",
          "form": "어떠신가요!!!!!",
          "original_form": "어떠신가요!!!!!",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100010285.1.28",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100010285.1.29",
          "form": "너무 졸려요 ㅎㅎ그것말고는 없네요 ㅎㅎ",
          "original_form": "너무 졸려요 ㅎㅎ그것말고는 없네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100010285.1.30",
          "form": "운동못하는게 좀 마음이 아파요",
          "original_form": "운동못하는게 좀 마음이 아파요",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100010285.1.31",
          "form": "평소에 운동하시너요?",
          "original_form": "평소에 운동하시너요?",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100010285.1.32",
          "form": "아니...여 ㅋㅋㅋㅋ",
          "original_form": "아니...여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100010285.1.33",
          "form": "어떤운동하세요? 저는 집에 소도구는 많은데 귀차니즘이 절 이겨요..",
          "original_form": "어떤운동하세요? 저는 집에 소도구는 많은데 귀차니즘이 절 이겨요..",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100010285.1.34",
          "form": "ㅎㅎ 저는 헬스장다녀요!",
          "original_form": "ㅎㅎ 저는 헬스장다녀요!",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100010285.1.35",
          "form": "집에 케틀벨, 매트, 밴드 있어서 집에서도 해요",
          "original_form": "집에 케틀벨, 매트, 밴드 있어서 집에서도 해요",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100010285.1.36",
          "form": "근데 발에 골절이 생겨서ㅠㅠ",
          "original_form": "근데 발에 골절이 생겨서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100010285.1.37",
          "form": "어 어쩌다가요ㅜㅜㅜㅜ",
          "original_form": "어 어쩌다가요ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100010285.1.38",
          "form": "운동하시다 그러셨어요??",
          "original_form": "운동하시다 그러셨어요??",
          "speaker_id": "2",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100010285.1.39",
          "form": "뛰다가요ㅠㅠㅠ",
          "original_form": "뛰다가요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100010285.1.40",
          "form": "운동좋아하세요",
          "original_form": "운동좋아하세요",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100010285.1.41",
          "form": "저는 운동의 필요성은 아는데 요즘은 잘 안하고 있어요ㅠㅠ",
          "original_form": "저는 운동의 필요성은 아는데 요즘은 잘 안하고 있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 23:25"
        }
      ]
