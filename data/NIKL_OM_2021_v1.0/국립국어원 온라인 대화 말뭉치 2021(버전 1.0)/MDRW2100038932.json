      "utterance": [
        {
          "id": "MDRW2100038932.1.1",
          "form": "저 거기들어봤어요",
          "original_form": "저 거기들어봤어요",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038932.1.2",
          "form": "유명하던데",
          "original_form": "유명하던데",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038932.1.3",
          "form": "제가 안조아해서근가 별뤀ㅋㅋㅋㅋ",
          "original_form": "제가 안조아해서근가 별뤀ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038932.1.4",
          "form": "거기 생길때부터 엄청 유명하고 웨이팅 쩔었대요 ㅋㅋㅋㅋ",
          "original_form": "거기 생길때부터 엄청 유명하고 웨이팅 쩔었대요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038932.1.5",
          "form": "아마 그냥 혜자로워서",
          "original_form": "아마 그냥 혜자로워서",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038932.1.6",
          "form": "9천원 익스펜시브",
          "original_form": "9천원 익스펜시브",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038932.1.7",
          "form": "더 인기많는것같앗어요 ㅋㅋㅋㅋㅋ",
          "original_form": "더 인기많는것같앗어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.8",
          "form": "김찌가 나오긴 하는데..",
          "original_form": "김찌가 나오긴 하는데..",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.9",
          "form": "흠..",
          "original_form": "흠..",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.10",
          "form": "막 계란이랑그런거 같이나온댔는데 ㅋㅋㅋ",
          "original_form": "막 계란이랑그런거 같이나온댔는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.11",
          "form": "따로시키던데용..계란말이7처넌",
          "original_form": "따로시키던데용..계란말이7처넌",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.12",
          "form": "그치만 전 줄서는게 귀찮아서 가보진않았어요",
          "original_form": "그치만 전 줄서는게 귀찮아서 가보진않았어요",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.13",
          "form": "거기가 관장님 회원의 어머니가게",
          "original_form": "거기가 관장님 회원의 어머니가게",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.14",
          "form": "근방 가게 3개운영하심",
          "original_form": "근방 가게 3개운영하심",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.15",
          "form": "헐ㅋㅋㅋㅋㅋㅋ 부자되셨겠다",
          "original_form": "헐ㅋㅋㅋㅋㅋㅋ 부자되셨겠다",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.16",
          "form": "대단... 그뵤",
          "original_form": "대단... 그뵤",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.17",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.18",
          "form": "젊으시던디 ㅎ",
          "original_form": "젊으시던디 ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.19",
          "form": "애가7살",
          "original_form": "애가7살",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038932.1.20",
          "form": "ㅋㅋㅋ대전은 지금 6시이후에 몇명 같이 밥먹으러갈수있어요?",
          "original_form": "ㅋㅋㅋ대전은 지금 6시이후에 몇명 같이 밥먹으러갈수있어요?",
