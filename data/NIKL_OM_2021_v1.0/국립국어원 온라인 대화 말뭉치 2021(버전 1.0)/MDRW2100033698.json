      "utterance": [
        {
          "id": "MDRW2100033698.1.1",
          "form": "학업 스트레스 받고 있습니다ㅜㅜㅜ",
          "original_form": "학업 스트레스 받고 있습니다ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033698.1.2",
          "form": "어떤스트레스입니까ㅋㅋ",
          "original_form": "어떤스트레스입니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033698.1.3",
          "form": "컴활 학원 공부가 힘듭니다",
          "original_form": "컴활 학원 공부가 힘듭니다",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100033698.1.4",
          "form": "토익 시험도 힘듭니다ㅜㅜ",
          "original_form": "토익 시험도 힘듭니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100033698.1.5",
          "form": "컴활도 다니시군요",
          "original_form": "컴활도 다니시군요",
          "speaker_id": "2",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100033698.1.6",
          "form": "두마리토끼 잡아야하는 name1 부지런해야겠어요",
          "original_form": "두마리토끼 잡아야하는 &name1& 부지런해야겠어요",
          "speaker_id": "2",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100033698.1.7",
          "form": "국비지원 받고 있습니다",
          "original_form": "국비지원 받고 있습니다",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033698.1.8",
          "form": "아ㅜㅜ 세금을 받고계시군요",
          "original_form": "아ㅜㅜ 세금을 받고계시군요",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033698.1.9",
          "form": "컴활공부는 좀 어때용",
          "original_form": "컴활공부는 좀 어때용",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100033698.1.10",
          "form": "엑셀에는 우리가 몰랐던 많은 기능이 있더라고요",
          "original_form": "엑셀에는 우리가 몰랐던 많은 기능이 있더라고요",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100033698.1.11",
          "form": "엑세스가 뭔지 아시나요",
          "original_form": "엑세스가 뭔지 아시나요",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100033698.1.12",
          "form": "아 아니요ㅜㅜㅜㅜㅜ",
          "original_form": "아 아니요ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100033698.1.13",
          "form": "저도 머리 털 나고 처음 들었습니다",
          "original_form": "저도 머리 털 나고 처음 들었습니다",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100033698.1.14",
          "form": "세상에 듣도 못한걸 배워야해요",
          "original_form": "세상에 듣도 못한걸 배워야해요",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100033698.1.15",
          "form": "name1님이 나중에 알려주세용ㄱㅋㅋ든든",
          "original_form": "&name1&님이 나중에 알려주세용ㄱㅋㅋ든든",
          "speaker_id": "2",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100033698.1.16",
          "form": "그리고 모든게 9/24일에 끝나요",
          "original_form": "그리고 모든게 9/24일에 끝나요",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033698.1.17",
          "form": "속성도 이런 속성이 없네요 ㅋㅋ",
          "original_form": "속성도 이런 속성이 없네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033698.1.18",
          "form": "뭐야..넘 빠르잖아",
          "original_form": "뭐야..넘 빠르잖아",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033698.1.19",
          "form": "다른사람들은 잘 따라옴?",
          "original_form": "다른사람들은 잘 따라옴?",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033698.1.20",
          "form": "다들 20대라서 잘하는듯 해보임",
          "original_form": "다들 20대라서 잘하는듯 해보임",
          "speaker_id": "1",
