      "utterance": [
        {
          "id": "MDRW2100027264.1.1",
          "form": "신의조화",
          "original_form": "신의조화",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.2",
          "form": "카페 얼음은 오래갑니다",
          "original_form": "카페 얼음은 오래갑니다",
          "speaker_id": "1",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.3",
          "form": "아이스크림과커피",
          "original_form": "아이스크림과커피",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.4",
          "form": "집에선 금방 녹지요",
          "original_form": "집에선 금방 녹지요",
          "speaker_id": "1",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.5",
          "form": "얼음깨서 먹어줘야해요",
          "original_form": "얼음깨서 먹어줘야해요",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.6",
          "form": "바닐라아이스크림",
          "original_form": "바닐라아이스크림",
          "speaker_id": "1",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.7",
          "form": "얼리는방법이다를까요",
          "original_form": "얼리는방법이다를까요",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.8",
          "form": "아이스크림은 하겐다즈죠",
          "original_form": "아이스크림은 하겐다즈죠",
          "speaker_id": "1",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.9",
          "form": "녹차이이스크림좋아해요",
          "original_form": "녹차이이스크림좋아해요",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.10",
          "form": "존맛이죠",
          "original_form": "존맛이죠",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.11",
          "form": "이것도 비싸서 1+1할때 먹습니다",
          "original_form": "이것도 비싸서 1+1할때 먹습니다",
          "speaker_id": "1",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.12",
          "form": "뻥튀기에 투게더 넣어서",
          "original_form": "뻥튀기에 투게더 넣어서",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100027264.1.13",
          "form": "샌드위치처럼만들어드서보세요",
          "original_form": "샌드위치처럼만들어드서보세요",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027264.1.14",
          "form": "이마트 하겐다즈 40%할인중입니다",
          "original_form": "이마트 하겐다즈 40%할인중입니다",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027264.1.15",
          "form": "홈플은 우유아이스크림 1+1입니다",
          "original_form": "홈플은 우유아이스크림 1+1입니다",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027264.1.16",
          "form": "나뚜루는 녹차맛이죠",
          "original_form": "나뚜루는 녹차맛이죠",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027264.1.17",
          "form": "샌드위치좋네요",
          "original_form": "샌드위치좋네요",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027264.1.18",
          "form": "베스킨도 맛있죠",
          "original_form": "베스킨도 맛있죠",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027264.1.19",
          "form": "베스킨",
          "original_form": "베스킨",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027264.1.20",
          "form": "아이스크림 종류가 다양합니다",
          "original_form": "아이스크림 종류가 다양합니다",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027264.1.21",
          "form": "한번씩 할인해서 좋아요",
          "original_form": "한번씩 할인해서 좋아요",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027264.1.22",
          "form": "쿠앤크맛",
          "original_form": "쿠앤크맛",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
