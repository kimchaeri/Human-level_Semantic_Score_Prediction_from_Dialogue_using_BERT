      "utterance": [
        {
          "id": "MDRW2100031891.1.1",
          "form": "엄청 고민하고 골랏서요",
          "original_form": "엄청 고민하고 골랏서요",
          "speaker_id": "2",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100031891.1.2",
          "form": "네 그런게 필요한 거 같아요",
          "original_form": "네 그런게 필요한 거 같아요",
          "speaker_id": "1",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100031891.1.3",
          "form": "그런데 주말인데 엄청 일찍 일어나셨네요",
          "original_form": "그런데 주말인데 엄청 일찍 일어나셨네요",
          "speaker_id": "2",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100031891.1.4",
          "form": "네 잠을 빨리 깼어요",
          "original_form": "네 잠을 빨리 깼어요",
          "speaker_id": "1",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100031891.1.5",
          "form": "혼자 사시나요?",
          "original_form": "혼자 사시나요?",
          "speaker_id": "2",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100031891.1.6",
          "form": "아니요 부모님이랑 같이 살아요",
          "original_form": "아니요 부모님이랑 같이 살아요",
          "speaker_id": "1",
          "time": "20210905 07:48"
        },
        {
          "id": "MDRW2100031891.1.7",
          "form": "좋겠습니다 ㅎㅎ",
          "original_form": "좋겠습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100031891.1.8",
          "form": "밥도 다해주시고",
          "original_form": "밥도 다해주시고",
          "speaker_id": "2",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100031891.1.9",
          "form": "집밥먹고싶어용",
          "original_form": "집밥먹고싶어용",
          "speaker_id": "2",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100031891.1.10",
          "form": "엄마가 해주는 밥!",
          "original_form": "엄마가 해주는 밥!",
          "speaker_id": "2",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100031891.1.11",
          "form": "집밥 안 드세요?",
          "original_form": "집밥 안 드세요?",
          "speaker_id": "1",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100031891.1.12",
          "form": "애기밥위주라",
          "original_form": "애기밥위주라",
          "speaker_id": "2",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100031891.1.13",
          "form": "아 ㅜ",
          "original_form": "아 ㅜ",
          "speaker_id": "1",
          "time": "20210905 07:50"
        },
        {
          "id": "MDRW2100031891.1.14",
          "form": "거의 시켜먹엇씁니다 ㅋㅋ",
          "original_form": "거의 시켜먹엇씁니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100031891.1.15",
          "form": "헉 ㅋㅋㅋ",
          "original_form": "헉 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100031891.1.16",
          "form": "요즘 애기가 밥먹아사",
          "original_form": "요즘 애기가 밥먹아사",
          "speaker_id": "2",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100031891.1.17",
          "form": "그러고 보니 밥해주는 남자 있음 당장 결혼할 것 같아요",
          "original_form": "그러고 보니 밥해주는 남자 있음 당장 결혼할 것 같아요",
          "speaker_id": "1",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100031891.1.18",
          "form": "밥을 좀 하긴하더라구요",
          "original_form": "밥을 좀 하긴하더라구요",
          "speaker_id": "2",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100031891.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100031891.1.20",
          "form": "밥은하는데",
          "original_form": "밥은하는데",
          "speaker_id": "2",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100031891.1.21",
          "form": "반찬이 문제가",
          "original_form": "반찬이 문제가",
          "speaker_id": "2",
          "time": "20210905 07:51"
        },
        {
          "id": "MDRW2100031891.1.22",
          "form": "요리는 안하세요?",
          "original_form": "요리는 안하세요?",
          "speaker_id": "1",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100031891.1.23",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100031891.1.24",
          "form": "최근에",
          "original_form": "최근에",
          "speaker_id": "2",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100031891.1.25",
          "form": "삼겹살 고추장 볶음",
          "original_form": "삼겹살 고추장 볶음",
          "speaker_id": "2",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100031891.1.26",
          "form": "했는데",
          "original_form": "했는데",
          "speaker_id": "2",
          "time": "20210905 07:52"
        }
      ]
