      "utterance": [
        {
          "id": "MDRW2100013118.1.1",
          "form": "아 저희는 따로 여름 휴가는 없어요",
          "original_form": "아 저희는 따로 여름 휴가는 없어요",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013118.1.2",
          "form": "그냥 주야비휴주야비휴에",
          "original_form": "그냥 주야비휴주야비휴에",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013118.1.3",
          "form": "알아서 휴가 넣어요",
          "original_form": "알아서 휴가 넣어요",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013118.1.4",
          "form": "보통 안겹치면 써도 지장 없어요 ㅎㅎ",
          "original_form": "보통 안겹치면 써도 지장 없어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013118.1.5",
          "form": "아 상관없군요ㅎ",
          "original_form": "아 상관없군요ㅎ",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013118.1.6",
          "form": "역에서  근무함 점심은 나가서드세요?",
          "original_form": "역에서  근무함 점심은 나가서드세요?",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013118.1.7",
          "form": "각자 돌아가면서 먹나요?",
          "original_form": "각자 돌아가면서 먹나요?",
          "speaker_id": "1",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100013118.1.8",
          "form": "돌아가면서 먹긴하는데",
          "original_form": "돌아가면서 먹긴하는데",
          "speaker_id": "2",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013118.1.9",
          "form": "요새는 코로나때문에 거의 포장해서 먹어요ㅎㅎ",
          "original_form": "요새는 코로나때문에 거의 포장해서 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013118.1.10",
          "form": "불안불안해서",
          "original_form": "불안불안해서",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013118.1.11",
          "form": "아 혼밥 전 못해가지고ㅎ",
          "original_form": "아 혼밥 전 못해가지고ㅎ",
          "speaker_id": "1",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013118.1.12",
          "form": "업무는 그럼 앉아서 보고서쓰고 그런건 안하시겠어요",
          "original_form": "업무는 그럼 앉아서 보고서쓰고 그런건 안하시겠어요",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013118.1.13",
          "form": "음 쓰는 경우도 있어서",
          "original_form": "음 쓰는 경우도 있어서",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013118.1.14",
          "form": "이례상황 생기면 보고 올려요 ㅎㅎ",
          "original_form": "이례상황 생기면 보고 올려요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013118.1.15",
          "form": "아무래도 빈도가 적긴하죠",
          "original_form": "아무래도 빈도가 적긴하죠",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013118.1.16",
          "form": "사고터지거나 그럼 보고용으로 필요하긴 하겠어요",
          "original_form": "사고터지거나 그럼 보고용으로 필요하긴 하겠어요",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013118.1.17",
          "form": "네 그런거죠ㅋㅋ 업종은 어떤 쪽이세요?",
          "original_form": "네 그런거죠ㅋㅋ 업종은 어떤 쪽이세요?",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013118.1.18",
          "form": "전 원자력관련일~ 컨설팅이라고해야되나",
          "original_form": "전 원자력관련일~ 컨설팅이라고해야되나",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013118.1.19",
          "form": "분석하고 보고서쓰고",
          "original_form": "분석하고 보고서쓰고",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013118.1.20",
          "form": "그런일하는 회사요",
          "original_form": "그런일하는 회사요",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013118.1.21",
          "form": "오 원자력 멋있어요",
          "original_form": "오 원자력 멋있어요",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013118.1.22",
          "form": "말만 그럴싸해요",
          "original_form": "말만 그럴싸해요",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013118.1.23",
          "form": "별거없음요",
          "original_form": "별거없음요",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013118.1.24",
          "form": "ㅋㅋ전문적이자나요",
          "original_form": "ㅋㅋ전문적이자나요",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013118.1.25",
          "form": "분야가 살짝 그렇긴하죠ㅎㅎ",
          "original_form": "분야가 살짝 그렇긴하죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100013118.1.26",
          "form": "ㅋㅋ 멋있음요",
          "original_form": "ㅋㅋ 멋있음요",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013118.1.27",
          "form": "노노 전혀요ㅎ",
          "original_form": "노노 전혀요ㅎ",
          "speaker_id": "1",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013118.1.28",
          "form": "푹쉬셔서  덜피곤하실듯",
          "original_form": "푹쉬셔서  덜피곤하실듯",
          "speaker_id": "1",
          "time": "20210826 10:41"
        }
      ]
    }
