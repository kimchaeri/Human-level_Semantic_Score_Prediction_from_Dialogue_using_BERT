      "utterance": [
        {
          "id": "MDRW2100009404.1.1",
          "form": "미용 관심많아요?",
          "original_form": "미용 관심많아요?",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009404.1.2",
          "form": "그럭저럭요",
          "original_form": "그럭저럭요",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009404.1.3",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009404.1.4",
          "form": "어릴땐 많았는데 요즘은 ..",
          "original_form": "어릴땐 많았는데 요즘은 ..",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009404.1.5",
          "form": "그래도 없진않아요 ㅎㅎ",
          "original_form": "그래도 없진않아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009404.1.6",
          "form": "어떤거 주로 관리하세요?",
          "original_form": "어떤거 주로 관리하세요?",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009404.1.7",
          "form": "나이들수록 피부랑 몸매 신경쓰는거 같아요 아직 뚠뚠이지만 ..ㅋㅋ",
          "original_form": "나이들수록 피부랑 몸매 신경쓰는거 같아요 아직 뚠뚠이지만 ..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009404.1.8",
          "form": "그쵸 피부랑 몸매가 중요하죠",
          "original_form": "그쵸 피부랑 몸매가 중요하죠",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009404.1.9",
          "form": "저도 일일1팩 붙이려고 다짐만하고 있어요",
          "original_form": "저도 일일1팩 붙이려고 다짐만하고 있어요",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009404.1.10",
          "form": "화장품도 좋은거사게되고 하더라구요 ㅠ",
          "original_form": "화장품도 좋은거사게되고 하더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009404.1.11",
          "form": "아이크림도 바르세요?",
          "original_form": "아이크림도 바르세요?",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009404.1.12",
          "form": "아직 아이크림은 안발라용 ㅠㅠ 아이크림이 주름개선이죠??",
          "original_form": "아직 아이크림은 안발라용 ㅠㅠ 아이크림이 주름개선이죠??",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009404.1.13",
          "form": "넹 눈밑 주름개선이요\r\n그건 20대부터 바르면 좋다던데요",
          "original_form": "넹 눈밑 주름개선이요\r\n그건 20대부터 바르면 좋다던데요",
          "speaker_id": "2",
          "time": "20210822 23:18"
        },
        {
          "id": "MDRW2100009404.1.14",
          "form": "주름은 아직없눈데",
          "original_form": "주름은 아직없눈데",
          "speaker_id": "1",
          "time": "20210822 23:19"
        },
        {
          "id": "MDRW2100009404.1.15",
          "form": "젤 고민은 모공이예요 ㅠㅠ",
          "original_form": "젤 고민은 모공이예요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009404.1.16",
          "form": "아 모공 그건 어떻게 관리해야되죠",
          "original_form": "아 모공 그건 어떻게 관리해야되죠",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009404.1.17",
          "form": "그냥 모공에좋다는 폼클 오일 다사용하긴해요 ..",
          "original_form": "그냥 모공에좋다는 폼클 오일 다사용하긴해요 ..",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009404.1.18",
          "form": "아 저도 클렌징오일로 노폐물 녹이는게 젤 좋은거 같더라구요",
          "original_form": "아 저도 클렌징오일로 노폐물 녹이는게 젤 좋은거 같더라구요",
          "speaker_id": "2",
          "time": "20210822 23:22"
        }
      ]
