      "utterance": [
        {
          "id": "MDRW2100032465.1.1",
          "form": "저녁은 먹엇어?",
          "original_form": "저녁은 먹엇어?",
          "speaker_id": "1",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100032465.1.2",
          "form": "난 저녁 늦게 먹는 편이야",
          "original_form": "난 저녁 늦게 먹는 편이야",
          "speaker_id": "2",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100032465.1.3",
          "form": "20시쯤?",
          "original_form": "20시쯤?",
          "speaker_id": "2",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100032465.1.4",
          "form": "ㅇㅎ 지금 먹겠네 나는 두부 먹었어",
          "original_form": "ㅇㅎ 지금 먹겠네 나는 두부 먹었어",
          "speaker_id": "1",
          "time": "20210905 20:09"
        },
        {
          "id": "MDRW2100032465.1.5",
          "form": "밥이랑",
          "original_form": "밥이랑",
          "speaker_id": "1",
          "time": "20210905 20:09"
        },
        {
          "id": "MDRW2100032465.1.6",
          "form": "맞아 나도 먹었어",
          "original_form": "맞아 나도 먹었어",
          "speaker_id": "2",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100032465.1.7",
          "form": "간단하게 먹네?",
          "original_form": "간단하게 먹네?",
          "speaker_id": "2",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100032465.1.8",
          "form": "간단하진 않은데? 두부 어제 시장에서 사온거",
          "original_form": "간단하진 않은데? 두부 어제 시장에서 사온거",
          "speaker_id": "1",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100032465.1.9",
          "form": "맛있어",
          "original_form": "맛있어",
          "speaker_id": "1",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100032465.1.10",
          "form": "뭐먹었는데?",
          "original_form": "뭐먹었는데?",
          "speaker_id": "1",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100032465.1.11",
          "form": "난 미역국 밥 뭐 반찬들?",
          "original_form": "난 미역국 밥 뭐 반찬들?",
          "speaker_id": "2",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100032465.1.12",
          "form": "맛있지 두부",
          "original_form": "맛있지 두부",
          "speaker_id": "2",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100032465.1.13",
          "form": "미역국? 맛있는데",
          "original_form": "미역국? 맛있는데",
          "speaker_id": "1",
          "time": "20210905 20:15"
        },
        {
          "id": "MDRW2100032465.1.14",
          "form": "소고기??",
          "original_form": "소고기??",
          "speaker_id": "1",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100032465.1.15",
          "form": "응응 맛있지",
          "original_form": "응응 맛있지",
          "speaker_id": "2",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100032465.1.16",
          "form": "맞아 소고기",
          "original_form": "맞아 소고기",
          "speaker_id": "2",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100032465.1.17",
          "form": "미역국은 소고기가 제일 맛있더라",
          "original_form": "미역국은 소고기가 제일 맛있더라",
          "speaker_id": "1",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100032465.1.18",
          "form": "맞아 너 먹을 줄 아는구나",
          "original_form": "맞아 너 먹을 줄 아는구나",
          "speaker_id": "2",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100032465.1.19",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "2",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100032465.1.20",
          "form": "근데 또 미역국 엄마꺼만 맛잇어",
          "original_form": "근데 또 미역국 엄마꺼만 맛잇어",
          "speaker_id": "1",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032465.1.21",
          "form": "다른사람껀 별로더라",
          "original_form": "다른사람껀 별로더라",
          "speaker_id": "1",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032465.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ익숙한게 최고지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ익숙한게 최고지",
          "speaker_id": "2",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100032465.1.23",
          "form": "청주 맛집 추천해줘",
          "original_form": "청주 맛집 추천해줘",
          "speaker_id": "2",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100032465.1.24",
          "form": "잘 모르나?",
          "original_form": "잘 모르나?",
          "speaker_id": "2",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100032465.1.25",
          "form": "본토막창 맛있어!",
          "original_form": "본토막창 맛있어!",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032465.1.26",
          "form": "청주 맛집은 삼미집 같아",
          "original_form": "청주 맛집은 삼미집 같아",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032465.1.27",
          "form": "또 부추칼국수도 맛있고",
          "original_form": "또 부추칼국수도 맛있고",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032465.1.28",
          "form": "본토막창도 좋고",
          "original_form": "본토막창도 좋고",
          "speaker_id": "1",
          "time": "20210905 20:24"
        }
      ]
    }
  ]
