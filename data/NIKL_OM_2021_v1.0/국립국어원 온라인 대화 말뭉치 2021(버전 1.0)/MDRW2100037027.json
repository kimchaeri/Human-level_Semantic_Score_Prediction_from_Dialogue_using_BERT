      "utterance": [
        {
          "id": "MDRW2100037027.1.1",
          "form": "요즘 밀키트같은 거 많이 사드세요?",
          "original_form": "요즘 밀키트같은 거 많이 사드세요?",
          "speaker_id": "1",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100037027.1.2",
          "form": "먹고있진 않지만",
          "original_form": "먹고있진 않지만",
          "speaker_id": "2",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100037027.1.3",
          "form": "먹을볼까 고민이에요 ㅠ",
          "original_form": "먹을볼까 고민이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100037027.1.4",
          "form": "자주 드세요?",
          "original_form": "자주 드세요?",
          "speaker_id": "2",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100037027.1.5",
          "form": "저는 가끔요!",
          "original_form": "저는 가끔요!",
          "speaker_id": "1",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100037027.1.6",
          "form": "냉면이나 똠얌꿍같은거...",
          "original_form": "냉면이나 똠얌꿍같은거...",
          "speaker_id": "1",
          "time": "20210831 18:00"
        },
        {
          "id": "MDRW2100037027.1.7",
          "form": "냉면 키트는",
          "original_form": "냉면 키트는",
          "speaker_id": "2",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100037027.1.8",
          "form": "진짜 좋은 거 같아요ㅋㅋ",
          "original_form": "진짜 좋은 거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100037027.1.9",
          "form": "맞아요ㅋㅋ요즘은 배달이 너무 잘되어있어서 참 좋아요",
          "original_form": "맞아요ㅋㅋ요즘은 배달이 너무 잘되어있어서 참 좋아요",
          "speaker_id": "1",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100037027.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100037027.1.11",
          "form": "뭐든 배달로",
          "original_form": "뭐든 배달로",
          "speaker_id": "2",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100037027.1.12",
          "form": "뚝딱이에여",
          "original_form": "뚝딱이에여",
          "speaker_id": "2",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100037027.1.13",
          "form": "새벽배송이라 항상 그다음날 도착하고",
          "original_form": "새벽배송이라 항상 그다음날 도착하고",
          "speaker_id": "1",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100037027.1.14",
          "form": "그게 정말 신세계죠ㅋㅋ",
          "original_form": "그게 정말 신세계죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100037027.1.15",
          "form": "마켓컬리에서 보니까 음식만파는게 아닌더라구요?",
          "original_form": "마켓컬리에서 보니까 음식만파는게 아닌더라구요?",
          "speaker_id": "1",
          "time": "20210831 19:55"
        },
        {
          "id": "MDRW2100037027.1.16",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210831 19:55"
        },
        {
          "id": "MDRW2100037027.1.17",
          "form": "저도 음식만 파는 줄... 알아썽요",
          "original_form": "저도 음식만 파는 줄... 알아썽요",
          "speaker_id": "2",
          "time": "20210831 19:55"
        },
        {
          "id": "MDRW2100037027.1.18",
          "form": "화장품도 팔구 심지어 갤워치도 팔아요ㅋㅋㅋㅋ",
          "original_form": "화장품도 팔구 심지어 갤워치도 팔아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:55"
        },
        {
          "id": "MDRW2100037027.1.19",
          "form": "저도 그런줄알았는데 오늘 자세히 보니까 별걸 다 팔더라구요",
          "original_form": "저도 그런줄알았는데 오늘 자세히 보니까 별걸 다 팔더라구요",
          "speaker_id": "1",
          "time": "20210831 19:55"
        },
        {
          "id": "MDRW2100037027.1.20",
          "form": "ㅋㅋㅋ 헐 진짜 다양하네여",
          "original_form": "ㅋㅋㅋ 헐 진짜 다양하네여",
          "speaker_id": "2",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100037027.1.21",
          "form": "그러니까요ㅋㅋ신기신기",
          "original_form": "그러니까요ㅋㅋ신기신기",
          "speaker_id": "1",
          "time": "20210831 20:11"
        }
      ]
