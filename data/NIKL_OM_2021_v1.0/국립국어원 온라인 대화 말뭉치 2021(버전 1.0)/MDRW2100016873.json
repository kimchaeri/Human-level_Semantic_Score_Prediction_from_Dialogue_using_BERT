      "utterance": [
        {
          "id": "MDRW2100016873.1.1",
          "form": "글고보니 제2의나라는 소식이 없네",
          "original_form": "글고보니 제2의나라는 소식이 없네",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016873.1.2",
          "form": "내가 안찾는건가",
          "original_form": "내가 안찾는건가",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016873.1.3",
          "form": "나도 못들어봄",
          "original_form": "나도 못들어봄",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016873.1.4",
          "form": "스토어 매출 3위네",
          "original_form": "스토어 매출 3위네",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016873.1.5",
          "form": "뭐지 포레스트..뫄뫄... 힐링겜도 사전예약 했었는데",
          "original_form": "뭐지 포레스트..뫄뫄... 힐링겜도 사전예약 했었는데",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016873.1.6",
          "form": "리니지 레볼루션2인데",
          "original_form": "리니지 레볼루션2인데",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016873.1.7",
          "form": "나중에 리뷰보니깐 힐링겜 아닌거 같드라",
          "original_form": "나중에 리뷰보니깐 힐링겜 아닌거 같드라",
          "speaker_id": "1",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016873.1.8",
          "form": "린레볼도 잘 나갔어서 잘 팔리나봄",
          "original_form": "린레볼도 잘 나갔어서 잘 팔리나봄",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016873.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016873.1.10",
          "form": "ㅇㅇ 린레볼2임 스킨만 씌운거",
          "original_form": "ㅇㅇ 린레볼2임 스킨만 씌운거",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016873.1.11",
          "form": "아 포레스트 그거 중국겜",
          "original_form": "아 포레스트 그거 중국겜",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016873.1.12",
          "form": "구래? 몰랐다",
          "original_form": "구래? 몰랐다",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016873.1.13",
          "form": "중국겜 짱많네",
          "original_form": "중국겜 짱많네",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016873.1.14",
          "form": "그래픽을 못만들었던데",
          "original_form": "그래픽을 못만들었던데",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016873.1.15",
          "form": "그거 그림체 지브리 풍 따라한느낌 나더라",
          "original_form": "그거 그림체 지브리 풍 따라한느낌 나더라",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016873.1.16",
          "form": "맞어 그래보였음",
          "original_form": "맞어 그래보였음",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016873.1.17",
          "form": "베낀건가?",
          "original_form": "베낀건가?",
          "speaker_id": "1",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016873.1.18",
