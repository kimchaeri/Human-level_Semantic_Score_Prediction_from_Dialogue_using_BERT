          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100025724.1.4",
          "form": "얼탱",
          "original_form": "얼탱",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100025724.1.5",
          "form": "배틀한애가 지젼이라며",
          "original_form": "배틀한애가 지젼이라며",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100025724.1.6",
          "form": "그 허니제이쪽보다",
          "original_form": "그 허니제이쪽보다",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100025724.1.7",
          "form": "지전이 뭐져",
          "original_form": "지전이 뭐져",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100025724.1.8",
          "form": "훨씬사이별로라고",
          "original_form": "훨씬사이별로라고",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100025724.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100025724.1.10",
          "form": "마잨ㅋㅋㅋㅋ",
          "original_form": "마잨ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100025724.1.11",
          "form": "허니제이낸",
          "original_form": "허니제이낸",
          "speaker_id": "2",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100025724.1.12",
          "form": "아름다운 결말",
          "original_form": "아름다운 결말",
          "speaker_id": "2",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100025724.1.13",
          "form": "우리 언니가 지다니 흑흑",
          "original_form": "우리 언니가 지다니 흑흑",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100025724.1.14",
          "form": "거기 맞팔햇댘",
          "original_form": "거기 맞팔햇댘",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100025724.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100025724.1.16",
          "form": "아 진짜?ㅋㅋㅋㅋ",
          "original_form": "아 진짜?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100025724.1.17",
          "form": "그런건 어케 아는거야 ㅋㅋㅋ",
          "original_form": "그런건 어케 아는거야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100025724.1.18",
          "form": "커뮤에서들엇슴ㅋㅋㅋ",
          "original_form": "커뮤에서들엇슴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100025724.1.19",
          "form": "다 우는거보고 나도울뻔ㅜㅜ",
          "original_form": "다 우는거보고 나도울뻔ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100025724.1.20",
          "form": "ㅋㅋㅋㅋㅋ 마자 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 마자 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100025724.1.21",
          "form": "진짜 재밌오",
          "original_form": "진짜 재밌오",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100025724.1.22",
          "form": "그리고 리더들 ㅋㅋ춤 선택 이유",
          "original_form": "그리고 리더들 ㅋㅋ춤 선택 이유",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100025724.1.23",
          "form": "소름돋 ㅋㅋㅋㅋ",
          "original_form": "소름돋 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100025724.1.24",
          "form": "라시카에 시미즈?? 그아이 좀 볼수록 이쁜거 가터",
          "original_form": "라시카에 시미즈?? 그아이 좀 볼수록 이쁜거 가터",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100025724.1.25",
          "form": "이유뭔데??? 못봄",
          "original_form": "이유뭔데??? 못봄",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100025724.1.26",
          "form": "스포환영",
          "original_form": "스포환영",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100025724.1.27",
          "form": "가비랑 미즈",
          "original_form": "가비랑 미즈",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100025724.1.28",
          "form": "우리가수되고싶나?이거짱웃ㅋㅋㅋ",
          "original_form": "우리가수되고싶나?이거짱웃ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100025724.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 가수가 되고싶니 뭐여 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 가수가 되고싶니 뭐여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100025724.1.30",
          "form": "이유 그거 노제꺼 선택햇는데 춤",
          "original_form": "이유 그거 노제꺼 선택햇는데 춤",
          "speaker_id": "2",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100025724.1.31",
          "form": "뺏어오기 쉬울거 같아서 ㅋㅋㅋㅋ",
          "original_form": "뺏어오기 쉬울거 같아서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100025724.1.32",
          "form": "견제ㅎㄷㄷ",
          "original_form": "견제ㅎㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100025724.1.33",
          "form": "난 카이댄서로 언급터진것도 몰랐음ㅋㅋ",
          "original_form": "난 카이댄서로 언급터진것도 몰랐음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100025724.1.34",
          "form": "다시봐도 그닥 춤모르겠던데 노제",
          "original_form": "다시봐도 그닥 춤모르겠던데 노제",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100025724.1.35",
          "form": "얼굴은 존존예",
          "original_form": "얼굴은 존존예",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100025724.1.36",
          "form": "얼굴이 너무 존예니까 ㅋㅋㅋㅋㅋ",
          "original_form": "얼굴이 너무 존예니까 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100025724.1.37",
          "form": "춤은 ㅋㅋㅋㅋㅋㅋ 내가 잘 몰라 말 못하겠다만",
          "original_form": "춤은 ㅋㅋㅋㅋㅋㅋ 내가 잘 몰라 말 못하겠다만",
          "speaker_id": "2",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100025724.1.38",
          "form": "진짜 우와눈 아냐",
          "original_form": "진짜 우와눈 아냐",
          "speaker_id": "2",
          "time": "20210902 19:23"
        },
