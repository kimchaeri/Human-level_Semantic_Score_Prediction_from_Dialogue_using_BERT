      "utterance": [
        {
          "id": "MDRW2100042247.1.1",
          "form": "요리는 주로 name2님이 하시는거죠?",
          "original_form": "요리는 주로 &name2&님이 하시는거죠?",
          "speaker_id": "1",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100042247.1.2",
          "form": "네넹 ㅎㅎ 그쵸",
          "original_form": "네넹 ㅎㅎ 그쵸",
          "speaker_id": "2",
          "time": "20210906 21:00"
        },
        {
          "id": "MDRW2100042247.1.3",
          "form": "아이가 많이 어린가봐용",
          "original_form": "아이가 많이 어린가봐용",
          "speaker_id": "2",
          "time": "20210906 21:01"
        },
        {
          "id": "MDRW2100042247.1.4",
          "form": "두다 유치원생예요~ name2님은요?",
          "original_form": "두다 유치원생예요~ &name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 21:02"
        },
        {
          "id": "MDRW2100042247.1.5",
          "form": "저희아이는 5살요 가정보육중이에요 코로나 ㅜ",
          "original_form": "저희아이는 5살요 가정보육중이에요 코로나 ㅜ",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100042247.1.6",
          "form": "가정 보육 하다보면 마음이 긴급해져서 긴급보육 보내게.되지.않나요??",
          "original_form": "가정 보육 하다보면 마음이 긴급해져서 긴급보육 보내게.되지.않나요??",
          "speaker_id": "1",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100042247.1.7",
          "form": "어머님이 봐주셔요",
          "original_form": "어머님이 봐주셔요",
          "speaker_id": "2",
          "time": "20210906 21:04"
        },
        {
          "id": "MDRW2100042247.1.8",
          "form": "아~ name2님은 일하시고요?",
          "original_form": "아~ &name2&님은 일하시고요?",
          "speaker_id": "1",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042247.1.9",
          "form": "네넴",
          "original_form": "네넴",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042247.1.10",
          "form": "곧 다시  어린이집 갈거 같긴해요",
          "original_form": "곧 다시  어린이집 갈거 같긴해요",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042247.1.11",
          "form": "이제 친구들이 좋을 나이라",
          "original_form": "이제 친구들이 좋을 나이라",
          "speaker_id": "2",
          "time": "20210906 21:05"
        },
        {
          "id": "MDRW2100042247.1.12",
          "form": "저희 둘째가 5살인데 너무 귀엽거든요",
          "original_form": "저희 둘째가 5살인데 너무 귀엽거든요",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042247.1.13",
          "form": "아이가 1명신거죠?",
          "original_form": "아이가 1명신거죠?",
          "speaker_id": "1",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042247.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 21:06"
        },
        {
          "id": "MDRW2100042247.1.15",
          "form": "5살 귀여울 나이죠 ㅎ",
          "original_form": "5살 귀여울 나이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042247.1.16",
          "form": "name2님 아이도 너무너무 귀여우신가요? ㅋ",
          "original_form": "&name2&님 아이도 너무너무 귀여우신가요? ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042247.1.17",
          "form": "네네 ㅋ 근데  가끔  참을인을 엄청 쓰죠 ㅎㅎㅎ",
          "original_form": "네네 ㅋ 근데  가끔  참을인을 엄청 쓰죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100042247.1.18",
          "form": "첫째가 5살땐 이렇게 안귀여웠거든요",
          "original_form": "첫째가 5살땐 이렇게 안귀여웠거든요",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100042247.1.19",
