      "utterance": [
        {
          "id": "MDRW2100024994.1.1",
          "form": "오늘은 청소기를 밀었네요 ㅎㅎ",
          "original_form": "오늘은 청소기를 밀었네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024994.1.2",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100024994.1.3",
          "form": "무슨 청소요?",
          "original_form": "무슨 청소요?",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100024994.1.4",
          "form": "화장실이대박이죠",
          "original_form": "화장실이대박이죠",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100024994.1.5",
          "form": "아 맞아요 ㅋㅋ 화장실이 젤 할 일 많아요 싱크대랑",
          "original_form": "아 맞아요 ㅋㅋ 화장실이 젤 할 일 많아요 싱크대랑",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100024994.1.6",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100024994.1.7",
          "form": "적당히 희석해야죠 ㅋㅋ 전 비누칠해서 닦는게더힘들던데요ㅠ",
          "original_form": "적당히 희석해야죠 ㅋㅋ 전 비누칠해서 닦는게더힘들던데요ㅠ",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100024994.1.8",
          "form": "곰팡이안 생기게 자주햐죠",
          "original_form": "곰팡이안 생기게 자주햐죠",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100024994.1.9",
          "form": "ㅋㅋ그게중요해요 화장실곰팡이피면제일불쾌하니까요",
          "original_form": "ㅋㅋ그게중요해요 화장실곰팡이피면제일불쾌하니까요",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100024994.1.10",
          "form": "냄새도",
          "original_form": "냄새도",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100024994.1.11",
