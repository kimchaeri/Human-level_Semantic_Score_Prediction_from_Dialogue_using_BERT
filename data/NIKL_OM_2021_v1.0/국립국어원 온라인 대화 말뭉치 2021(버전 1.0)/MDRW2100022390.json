      "utterance": [
        {
          "id": "MDRW2100022390.1.1",
          "form": "점심은 먹었나요?",
          "original_form": "점심은 먹었나요?",
          "speaker_id": "1",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022390.1.2",
          "form": "아직 식전이에요",
          "original_form": "아직 식전이에요",
          "speaker_id": "2",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022390.1.3",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "2",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022390.1.4",
          "form": "가자미구이에 밥 먹었답니다",
          "original_form": "가자미구이에 밥 먹었답니다",
          "speaker_id": "1",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022390.1.5",
          "form": "식전이면 배고프겠네요",
          "original_form": "식전이면 배고프겠네요",
          "speaker_id": "1",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022390.1.6",
          "form": "가자미구이  너무 맛있었겠어요",
          "original_form": "가자미구이  너무 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022390.1.7",
          "form": "뭐 먹으로계획인가요?",
          "original_form": "뭐 먹으로계획인가요?",
          "speaker_id": "1",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022390.1.8",
          "form": "아 빵 먹을 예정입니다",
          "original_form": "아 빵 먹을 예정입니다",
          "speaker_id": "2",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100022390.1.9",
          "form": "빵 좋아하시나요",
          "original_form": "빵 좋아하시나요",
          "speaker_id": "2",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100022390.1.10",
          "form": "팥들어가있는 걸 좋아해요\r\n단팥이요",
          "original_form": "팥들어가있는 걸 좋아해요\r\n단팥이요",
          "speaker_id": "1",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100022390.1.11",
          "form": "단팥빵 너무맛있어요",
          "original_form": "단팥빵 너무맛있어요",
          "speaker_id": "2",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100022390.1.12",
          "form": "팥의 달콤함이 너무좋더라구요",
          "original_form": "팥의 달콤함이 너무좋더라구요",
          "speaker_id": "2",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100022390.1.13",
          "form": "혹시 완두콩빵 이런건 안좋아하시나요",
          "original_form": "혹시 완두콩빵 이런건 안좋아하시나요",
          "speaker_id": "2",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100022390.1.14",
          "form": "초록앙금들은거요",
          "original_form": "초록앙금들은거요",
          "speaker_id": "2",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100022390.1.15",
          "form": "그것도 좋죠 근데 콩알러지가 있어서 콩보다는 팥을 더 좋아한답니다",
          "original_form": "그것도 좋죠 근데 콩알러지가 있어서 콩보다는 팥을 더 좋아한답니다",
          "speaker_id": "1",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100022390.1.16",
          "form": "콩 알러지가 있으시구나",
          "original_form": "콩 알러지가 있으시구나",
          "speaker_id": "2",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100022390.1.17",
          "form": "저는 새우알러지가 있어서 이해합니다",
          "original_form": "저는 새우알러지가 있어서 이해합니다",
          "speaker_id": "2",
          "time": "20210829 13:26"
        },
        {
          "id": "MDRW2100022390.1.18",
          "form": "가자미 구이는 어떠셨나요",
          "original_form": "가자미 구이는 어떠셨나요",
          "speaker_id": "2",
          "time": "20210829 13:26"
        },
        {
          "id": "MDRW2100022390.1.19",
          "form": "새우 맛있는데 알러지 있다니 안타깝네요",
          "original_form": "새우 맛있는데 알러지 있다니 안타깝네요",
          "speaker_id": "1",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022390.1.20",
          "form": "가자미구이 맛있었죠",
          "original_form": "가자미구이 맛있었죠",
          "speaker_id": "1",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022390.1.21",
          "form": "생선요리를 좋아하거든요",
          "original_form": "생선요리를 좋아하거든요",
          "speaker_id": "1",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022390.1.22",
          "form": "생선 너무 맛있죠",
          "original_form": "생선 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022390.1.23",
          "form": "또 어떤 생선을 좋아하시나요",
          "original_form": "또 어떤 생선을 좋아하시나요",
          "speaker_id": "2",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022390.1.24",
          "form": "예전엔 고등어 좋아했는데 이젠 삼치나 갈치쪽이 더 좋더군요",
          "original_form": "예전엔 고등어 좋아했는데 이젠 삼치나 갈치쪽이 더 좋더군요",
          "speaker_id": "1",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022390.1.25",
          "form": "삼치 슴슴해서 좋아해요",
          "original_form": "삼치 슴슴해서 좋아해요",
          "speaker_id": "2",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022390.1.26",
          "form": "보통 생선은 구이로 해서 드시나요",
          "original_form": "보통 생선은 구이로 해서 드시나요",
          "speaker_id": "2",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100022390.1.27",
          "form": "조림을 좋아하는데 짜게 먹는 편이라 싱겁게 먹으려고 구이로 요리하는 편입니다",
          "original_form": "조림을 좋아하는데 짜게 먹는 편이라 싱겁게 먹으려고 구이로 요리하는 편입니다",
          "speaker_id": "1",
          "time": "20210829 13:31"
        }
      ]
    }
  ]
