      "utterance": [
        {
          "id": "MDRW2100037537.1.1",
          "form": "오늘 점심",
          "original_form": "오늘 점심",
          "speaker_id": "2",
          "time": "20210901 09:14"
        },
        {
          "id": "MDRW2100037537.1.2",
          "form": "뭐 드실더에요",
          "original_form": "뭐 드실더에요",
          "speaker_id": "2",
          "time": "20210901 09:14"
        },
        {
          "id": "MDRW2100037537.1.3",
          "form": "헐 지금봤네요  점심에는 볶음밥 먹었어요",
          "original_form": "헐 지금봤네요  점심에는 볶음밥 먹었어요",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100037537.1.4",
          "form": "무슨 볶음밥이유?",
          "original_form": "무슨 볶음밥이유?",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100037537.1.5",
          "form": "김치볶음밥이요!",
          "original_form": "김치볶음밥이요!",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100037537.1.6",
          "form": "맛있겠어요.. 김볶...",
          "original_form": "맛있겠어요.. 김볶...",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100037537.1.7",
          "form": "점심에 뭐드셨나요?",
          "original_form": "점심에 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100037537.1.8",
          "form": "저 김치찌개요 ㅎㅎ",
          "original_form": "저 김치찌개요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037537.1.9",
          "form": "오 맛있었겠네요",
          "original_form": "오 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100037537.1.10",
          "form": "아쉽ㄱ게도",
          "original_form": "아쉽ㄱ게도",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100037537.1.11",
          "form": "그냥 그랬어요...",
          "original_form": "그냥 그랬어요...",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100037537.1.12",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100037537.1.13",
          "form": "ㅋㅋㅋㅋ네 ㅠ",
          "original_form": "ㅋㅋㅋㅋ네 ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100037537.1.14",
          "form": "아쉬웠답니다..",
          "original_form": "아쉬웠답니다..",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100037537.1.15",
          "form": "오늘 점심에는 뭐 드실 예정이에요?",
          "original_form": "오늘 점심에는 뭐 드실 예정이에요?",
          "speaker_id": "1",
          "time": "20210902 10:30"
        },
