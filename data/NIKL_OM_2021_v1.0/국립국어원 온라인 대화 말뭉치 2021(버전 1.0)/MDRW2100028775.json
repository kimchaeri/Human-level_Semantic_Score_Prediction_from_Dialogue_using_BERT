      "utterance": [
        {
          "id": "MDRW2100028775.1.1",
          "form": "네 전 너무단것먹으면 속도안좋고 입도 찝찝해서요 ㅎ",
          "original_form": "네 전 너무단것먹으면 속도안좋고 입도 찝찝해서요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028775.1.2",
          "form": "맞아요 그렇긴해요",
          "original_form": "맞아요 그렇긴해요",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028775.1.3",
          "form": "그래도 가뜸 땡기드라구요 단게 ㅠㅠ",
          "original_form": "그래도 가뜸 땡기드라구요 단게 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028775.1.4",
          "form": "네 가끔먹으면 엄청맛나요 ㅎ",
          "original_form": "네 가끔먹으면 엄청맛나요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028775.1.5",
          "form": "저녁은 뭐드셨나요",
          "original_form": "저녁은 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028775.1.6",
          "form": "저 돈가스 먹었어요 근데",
          "original_form": "저 돈가스 먹었어요 근데",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028775.1.7",
          "form": "배가 금방 꺼지네요 ㅠㅠ",
          "original_form": "배가 금방 꺼지네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028775.1.8",
          "form": "일찍드셨나봐요",
          "original_form": "일찍드셨나봐요",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028775.1.9",
          "form": "전 삼계탕조금먹고요",
          "original_form": "전 삼계탕조금먹고요",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028775.1.10",
          "form": "빵이랑 과자도 덤으로요 ㅜ",
          "original_form": "빵이랑 과자도 덤으로요 ㅜ",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028775.1.11",
          "form": "우아 삼계탕 ㅠㅜ 몸보신 하셨네용 ㅠㅜㅜㅜ",
          "original_form": "우아 삼계탕 ㅠㅜ 몸보신 하셨네용 ㅠㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028775.1.12",
          "form": "과자 저 너무 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "과자 저 너무 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028775.1.13",
          "form": "저두요 ㅋㅋ새우깡 자갈치 이런 옛날과자도 넘맛나요",
          "original_form": "저두요 ㅋㅋ새우깡 자갈치 이런 옛날과자도 넘맛나요",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028775.1.14",
          "form": "와 갑자기 자갈치 먹고싶어요 ㅋㅋㅋ",
          "original_form": "와 갑자기 자갈치 먹고싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028775.1.15",
          "form": "저는 치토스 먹을려구요 ㅎㅎ",
          "original_form": "저는 치토스 먹을려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028775.1.16",
          "form": "아 치토스도 맛나죵 ㅎ",
          "original_form": "아 치토스도 맛나죵 ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028775.1.17",
          "form": "우유 좋아하세요.?",
          "original_form": "우유 좋아하세요.?",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028775.1.18",
          "form": "네 가끔마시는데 좋아해요",
          "original_form": "네 가끔마시는데 좋아해요",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028775.1.19",
          "form": "우유 마실려구용",
          "original_form": "우유 마실려구용",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028775.1.20",
          "form": "바나나우유 !!",
          "original_form": "바나나우유 !!",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028775.1.21",
          "form": "아 맛있겠어요 바나나우유쭈쭈바도 엄청맛나드라구요",
