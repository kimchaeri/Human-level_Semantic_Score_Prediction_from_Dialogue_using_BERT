      "utterance": [
        {
          "id": "MDRW2100022102.1.1",
          "form": "큼큼,, 어떤 날씨 좋아하시는지?",
          "original_form": "큼큼,, 어떤 날씨 좋아하시는지?",
          "speaker_id": "1",
          "time": "20210829 06:14"
        },
        {
          "id": "MDRW2100022102.1.2",
          "form": "비오는날 집에있는걸 좋아합니다",
          "original_form": "비오는날 집에있는걸 좋아합니다",
          "speaker_id": "2",
          "time": "20210829 09:52"
        },
        {
          "id": "MDRW2100022102.1.3",
          "form": "여름이 좋으세요? 겨울이 좋으세요?",
          "original_form": "여름이 좋으세요? 겨울이 좋으세요?",
          "speaker_id": "2",
          "time": "20210829 09:52"
        },
        {
          "id": "MDRW2100022102.1.4",
          "form": "저는 그나마 겨울이 좋습니다,, name2는요 ?",
          "original_form": "저는 그나마 겨울이 좋습니다,, &name2&는요 ?",
          "speaker_id": "1",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100022102.1.5",
          "form": "name2은 어느계절에 마셔도 맛이 좋읍니다",
          "original_form": "&name2&은 어느계절에 마셔도 맛이 좋읍니다",
          "speaker_id": "2",
          "time": "20210829 14:38"
        },
        {
          "id": "MDRW2100022102.1.6",
          "form": "올여름 겪어보니 겨울이 난거같아요,,",
          "original_form": "올여름 겪어보니 겨울이 난거같아요,,",
          "speaker_id": "2",
          "time": "20210829 14:39"
        },
        {
          "id": "MDRW2100022102.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022102.1.8",
          "form": "겨울에는 샤워하고 몸데우는게 넘 오래 걸려서 개롭더라구요,,,",
          "original_form": "겨울에는 샤워하고 몸데우는게 넘 오래 걸려서 개롭더라구요,,,",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022102.1.9",
          "form": "역시 집에 있어야",
          "original_form": "역시 집에 있어야",
          "speaker_id": "1",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022102.1.10",
          "form": "혹시 쪄죽따세요?",
          "original_form": "혹시 쪄죽따세요?",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100022102.1.11",
          "form": "아뇨,, 쪄죽미지근 정도입니더,,,",
          "original_form": "아뇨,, 쪄죽미지근 정도입니더,,,",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022102.1.12",
          "form": "님은요??",
          "original_form": "님은요??",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022102.1.13",
          "form": "이제 쪄죽하면 어지러워서 못해요,,",
          "original_form": "이제 쪄죽하면 어지러워서 못해요,,",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022102.1.14",
          "form": "얼죽아는 어떠신지",
          "original_form": "얼죽아는 어떠신지",
          "speaker_id": "1",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022102.1.15",
          "form": "저는 강경 얼죽아입니다",
          "original_form": "저는 강경 얼죽아입니다",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022102.1.16",
          "form": "저도요 큼큼,,",
          "original_form": "저도요 큼큼,,",
          "speaker_id": "1",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022102.1.17",
          "form": "그쪽 날씨는 어떠신지?",
          "original_form": "그쪽 날씨는 어떠신지?",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022102.1.18",
          "form": "여기 눈와요~~",
          "original_form": "여기 눈와요~~",
          "speaker_id": "2",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022102.1.19",
