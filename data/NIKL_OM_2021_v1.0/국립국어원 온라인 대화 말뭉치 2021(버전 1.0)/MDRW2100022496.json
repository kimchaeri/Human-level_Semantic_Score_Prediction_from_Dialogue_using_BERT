      "utterance": [
        {
          "id": "MDRW2100022496.1.1",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "2",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100022496.1.2",
          "form": "얼음 보석? 설명 보면",
          "original_form": "얼음 보석? 설명 보면",
          "speaker_id": "1",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100022496.1.3",
          "form": "스네즈나야 신이 주체인데",
          "original_form": "스네즈나야 신이 주체인데",
          "speaker_id": "1",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100022496.1.4",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100022496.1.5",
          "form": "신이 우인단한테 하는 이야기고",
          "original_form": "신이 우인단한테 하는 이야기고",
          "speaker_id": "1",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100022496.1.6",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 15:05"
        },
        {
          "id": "MDRW2100022496.1.7",
          "form": "초창기 이벤트에서 (스카라무슈 나온",
          "original_form": "초창기 이벤트에서 (스카라무슈 나온",
          "speaker_id": "1",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100022496.1.8",
          "form": "우인단 상위 랭커?들은",
          "original_form": "우인단 상위 랭커?들은",
          "speaker_id": "1",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100022496.1.9",
          "form": "세상이 가짜라는걸 알고있다고 나옴",
          "original_form": "세상이 가짜라는걸 알고있다고 나옴",
          "speaker_id": "1",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100022496.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100022496.1.11",
          "form": "얼음 신이랑 우인단 목표는",
          "original_form": "얼음 신이랑 우인단 목표는",
          "speaker_id": "1",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100022496.1.12",
          "form": "집정관들 심장을 모아서",
          "original_form": "집정관들 심장을 모아서",
          "speaker_id": "1",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100022496.1.13",
          "form": "천리의 체계를 무너트리는거임",
          "original_form": "천리의 체계를 무너트리는거임",
          "speaker_id": "1",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100022496.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100022496.1.15",
          "form": "이쪽이",
          "original_form": "이쪽이",
          "speaker_id": "2",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100022496.1.16",
          "form": "저항군이엇네",
          "original_form": "저항군이엇네",
          "speaker_id": "2",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100022496.1.17",
          "form": "나 그럼",
          "original_form": "나 그럼",
          "speaker_id": "2",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100022496.1.18",
          "form": "ㅇㅇㅋㅋㅋㅋㅋ 눈치없이 정부군에 끼어있던 주인공...",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋ 눈치없이 정부군에 끼어있던 주인공...",
          "speaker_id": "1",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100022496.1.19",
          "form": "겁나 트롤링하는중인거 아님....?",
          "original_form": "겁나 트롤링하는중인거 아님....?",
          "speaker_id": "2",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100022496.1.20",
          "form": "그렇지 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇지 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100022496.1.21",
          "form": "이럴수가....",
          "original_form": "이럴수가....",
          "speaker_id": "2",
          "time": "20210829 15:07"
        },
        {
          "id": "MDRW2100022496.1.22",
          "form": "최초의 우인단이 어릿광대 페드롤리노고",
          "original_form": "최초의 우인단이 어릿광대 페드롤리노고",
          "speaker_id": "1",
          "time": "20210829 15:08"
        },
        {
          "id": "MDRW2100022496.1.23",
          "form": "우인단 문양 보면 11시 방향에 잇는 캐임",
          "original_form": "우인단 문양 보면 11시 방향에 잇는 캐임",
          "speaker_id": "1",
