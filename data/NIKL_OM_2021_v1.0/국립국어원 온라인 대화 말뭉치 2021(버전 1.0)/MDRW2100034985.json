      "utterance": [
        {
          "id": "MDRW2100034985.1.1",
          "form": "콜라가 깡패네요 ㅋㅋ  카페가면 머드시나요",
          "original_form": "콜라가 깡패네요 ㅋㅋ  카페가면 머드시나요",
          "speaker_id": "2",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100034985.1.2",
          "form": "저도 사이다좋아해요",
          "original_form": "저도 사이다좋아해요",
          "speaker_id": "2",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100034985.1.3",
          "form": "저 카페가면 스무디 주로 마셔용",
          "original_form": "저 카페가면 스무디 주로 마셔용",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100034985.1.4",
          "form": "사이다 칠성 좋아하세요",
          "original_form": "사이다 칠성 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100034985.1.5",
          "form": "스프라이트 좋아하세요?",
          "original_form": "스프라이트 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100034985.1.6",
          "form": "스프라이트 좋죠",
          "original_form": "스프라이트 좋죠",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100034985.1.7",
          "form": "더위많이 타시나봐요",
          "original_form": "더위많이 타시나봐요",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100034985.1.8",
          "form": "칠성이 탄산이 더 많이 들었대요",
          "original_form": "칠성이 탄산이 더 많이 들었대요",
          "speaker_id": "1",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100034985.1.9",
          "form": "저는 둘다 많이 타는것같아용ㅎㅎ",
          "original_form": "저는 둘다 많이 타는것같아용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034985.1.10",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034985.1.11",
          "form": "저는 탄산보다는 녹차말차흑임자좋아하는데",
          "original_form": "저는 탄산보다는 녹차말차흑임자좋아하는데",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034985.1.12",
          "form": "네네 사이다보면 그래서 칠성사이다 좋아하시는분들이 더 많더라구요",
          "original_form": "네네 사이다보면 그래서 칠성사이다 좋아하시는분들이 더 많더라구요",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034985.1.13",
          "form": "헉 흑임자!",
          "original_form": "헉 흑임자!",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034985.1.14",
          "form": "버거랑은 탄산이죠",
          "original_form": "버거랑은 탄산이죠",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034985.1.15",
          "form": "최근에 스타벅스 흑임자라떼 드셔보셨어요?",
          "original_form": "최근에 스타벅스 흑임자라떼 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034985.1.16",
          "form": "흑임자 좋아하시나요?",
          "original_form": "흑임자 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034985.1.17",
          "form": "호공 아니요",
          "original_form": "호공 아니요",
          "speaker_id": "2",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034985.1.18",
          "form": "좋아해요ㅎㅎ",
          "original_form": "좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100034985.1.19",
          "form": "이거 맛있는데",
          "original_form": "이거 맛있는데",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100034985.1.20",
          "form": "먹어봐야겠네요 흑임자라니!!!",
          "original_form": "먹어봐야겠네요 흑임자라니!!!",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100034985.1.21",
          "form": "시즌이 끝났네요ㅠㅠ",
          "original_form": "시즌이 끝났네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
