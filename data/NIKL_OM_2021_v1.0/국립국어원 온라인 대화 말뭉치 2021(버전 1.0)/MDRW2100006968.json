      "utterance": [
        {
          "id": "MDRW2100006968.1.1",
          "form": "오늘도 새벽에 지퍼백 산게 왔는데 오류여서 취소가 댔는데 몇개만 왔네",
          "original_form": "오늘도 새벽에 지퍼백 산게 왔는데 오류여서 취소가 댔는데 몇개만 왔네",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006968.1.2",
          "form": "아 카페에서 봤어",
          "original_form": "아 카페에서 봤어",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006968.1.3",
          "form": "와 그것도 놓치지않았구나",
          "original_form": "와 그것도 놓치지않았구나",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006968.1.4",
          "form": "멋지다ㅋㅋ",
          "original_form": "멋지다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006968.1.5",
          "form": "아 근데 그거 나는 사고도 취소될거 같아서 안보냈거든",
          "original_form": "아 근데 그거 나는 사고도 취소될거 같아서 안보냈거든",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006968.1.6",
          "form": "새벽에 사기도 했고",
          "original_form": "새벽에 사기도 했고",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006968.1.7",
          "form": "응응 ㅋㅋㅋ잘했네ㅋ",
          "original_form": "응응 ㅋㅋㅋ잘했네ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006968.1.8",
          "form": "근데 웃긴게 그게 낱개 가격이었는데 5개로 되어있었어",
          "original_form": "근데 웃긴게 그게 낱개 가격이었는데 5개로 되어있었어",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006968.1.9",
          "form": "최저가 보니 그렇더라고",
          "original_form": "최저가 보니 그렇더라고",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006968.1.10",
          "form": "그게 100장에 1900원 인데",
          "original_form": "그게 100장에 1900원 인데",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006968.1.11",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006968.1.12",
          "form": "그래서 난리였구나",
          "original_form": "그래서 난리였구나",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006968.1.13",
          "form": "응 300장을 샀지",
          "original_form": "응 300장을 샀지",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006968.1.14",
          "form": "20개 5통 1세트 총 3세트??",
          "original_form": "20개 5통 1세트 총 3세트??",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006968.1.15",
          "form": "근데 취소가 되찌",
          "original_form": "근데 취소가 되찌",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006968.1.16",
          "form": "응응 ㅋㅋ",
          "original_form": "응응 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006968.1.17",
          "form": "그래두왔구나 몇개",
          "original_form": "그래두왔구나 몇개",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006968.1.18",
          "form": "안오겠구나 했는데 다른것도 시켰자나 그 변기잴",
          "original_form": "안오겠구나 했는데 다른것도 시켰자나 그 변기잴",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006968.1.19",
          "form": "젤",
          "original_form": "젤",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006968.1.20",
          "form": "응응ㅋㅋ 변기젤ㅋㅋ아 웃겨",
          "original_form": "응응ㅋㅋ 변기젤ㅋㅋ아 웃겨",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006968.1.21",
          "form": "그것도 오류야 분명 근데 모르는것 같애",
          "original_form": "그것도 오류야 분명 근데 모르는것 같애",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006968.1.22",
          "form": "취소 안되고 왓어 ㅋ 지퍼백은 3세트인데 3개만 옴 ㅋㅋ",
          "original_form": "취소 안되고 왓어 ㅋ 지퍼백은 3세트인데 3개만 옴 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006968.1.23",
          "form": "20장 3개",
          "original_form": "20장 3개",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006968.1.24",
          "form": "아아 그렇구나. 득템했다ㅋㅋ좋다",
          "original_form": "아아 그렇구나. 득템했다ㅋㅋ좋다",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006968.1.25",
          "form": "부럽노ㅋㅋ",
          "original_form": "부럽노ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006968.1.26",
          "form": "나는 그런게 잘안보인다ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나는 그런게 잘안보인다ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006968.1.27",
          "form": "난 그거 다른데서 먼저 봤는데 사면서도 음 취소 되겠지 했는데 이미 다른데도 다 올라와서 아 끝났구나 싶더라",
          "original_form": "난 그거 다른데서 먼저 봤는데 사면서도 음 취소 되겠지 했는데 이미 다른데도 다 올라와서 아 끝났구나 싶더라",
          "speaker_id": "1",
          "time": "20210822 00:13"
        }
      ]
