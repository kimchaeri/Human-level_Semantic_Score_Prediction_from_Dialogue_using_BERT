      "utterance": [
        {
          "id": "MDRW2100044284.1.1",
          "form": "야식 먹고싶지않으세요?",
          "original_form": "야식 먹고싶지않으세요?",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044284.1.2",
          "form": "야식 이야기 들으니 뭔가 먹고싶네요",
          "original_form": "야식 이야기 들으니 뭔가 먹고싶네요",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044284.1.3",
          "form": "저는 라면이 먹고싶네요 ㅋㅋ",
          "original_form": "저는 라면이 먹고싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044284.1.4",
          "form": "와 라면 좋죠ㅠㅜ",
          "original_form": "와 라면 좋죠ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044284.1.5",
          "form": "낮보다 밤에먹는게 넘 맛있어요",
          "original_form": "낮보다 밤에먹는게 넘 맛있어요",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044284.1.6",
          "form": "맞아요 ㅠㅠ 한강에서 먹어야 진짜 맛있는데 말이죠 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ 한강에서 먹어야 진짜 맛있는데 말이죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044284.1.7",
          "form": "와 한강에서 먹으면 진짜 맛있겠네요ㅠ",
          "original_form": "와 한강에서 먹으면 진짜 맛있겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044284.1.8",
          "form": "이상하게 밖에서 먹는 라면은 더 맛있잖아요",
          "original_form": "이상하게 밖에서 먹는 라면은 더 맛있잖아요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044284.1.9",
          "form": "그쵸그쵸 추울때 먹어야 제맛이에요 ㅋㅋ",
          "original_form": "그쵸그쵸 추울때 먹어야 제맛이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044284.1.10",
          "form": "코로나때문에 못나가지만 괜찮아지면 나가서 라면하나 먹고싶네요",
          "original_form": "코로나때문에 못나가지만 괜찮아지면 나가서 라면하나 먹고싶네요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044284.1.11",
          "form": "소시지도 하나사서 먹으면 딱인데..",
          "original_form": "소시지도 하나사서 먹으면 딱인데..",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044284.1.12",
          "form": "맞아요 ㅠㅠ 어묵도 먹고싶네요 ㅠㅠ 국물이랑",
          "original_form": "맞아요 ㅠㅠ 어묵도 먹고싶네요 ㅠㅠ 국물이랑",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044284.1.13",
          "form": "슬슬 추워지니까 딱이죠ㅠㅜ",
          "original_form": "슬슬 추워지니까 딱이죠ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044284.1.14",
          "form": "붕어빵도..",
          "original_form": "붕어빵도..",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044284.1.15",
          "form": "붕어빵 슈랑 팥중에 뭐 좋아하세요??",
          "original_form": "붕어빵 슈랑 팥중에 뭐 좋아하세요??",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044284.1.16",
          "form": "저는 슈크림이 좋더라구요ㅎㅎ",
          "original_form": "저는 슈크림이 좋더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044284.1.17",
          "form": "오 저도요!!",
