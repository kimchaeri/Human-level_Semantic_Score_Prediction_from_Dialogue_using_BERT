      "utterance": [
        {
          "id": "MDRW2100002997.1.1",
          "form": "오늘 점심은 뭘 드셨나요",
          "original_form": "오늘 점심은 뭘 드셨나요",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002997.1.2",
          "form": "오늘 점심은 카레 먹었어요",
          "original_form": "오늘 점심은 카레 먹었어요",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002997.1.3",
          "form": "직접 만드셨나요",
          "original_form": "직접 만드셨나요",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002997.1.4",
          "form": "name2님은 뭐 드셨나요?",
          "original_form": "&name2&님은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002997.1.5",
          "form": "하숙집주인이 만들어줬어요",
          "original_form": "하숙집주인이 만들어줬어요",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002997.1.6",
          "form": "저는 오늘 장어구이를 먹었습니다",
          "original_form": "저는 오늘 장어구이를 먹었습니다",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002997.1.7",
          "form": "부장님이 사주셨습니다",
          "original_form": "부장님이 사주셨습니다",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002997.1.8",
          "form": "우왕 최고에요",
          "original_form": "우왕 최고에요",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100002997.1.9",
          "form": "장어먹고 보양해야죠",
          "original_form": "장어먹고 보양해야죠",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100002997.1.10",
          "form": "사람들이 많이먹어서 저는 조금밖에 못 먹었습니다",
          "original_form": "사람들이 많이먹어서 저는 조금밖에 못 먹었습니다",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100002997.1.11",
          "form": "배가 고픕니다",
          "original_form": "배가 고픕니다",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100002997.1.12",
          "form": "아이코",
          "original_form": "아이코",
          "speaker_id": "1",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100002997.1.13",
          "form": "장어도 경쟁이네요",
          "original_form": "장어도 경쟁이네요",
          "speaker_id": "1",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100002997.1.14",
          "form": "예 전어회도 나왔습니다",
          "original_form": "예 전어회도 나왔습니다",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100002997.1.15",
          "form": "그거는 인기가 없어서 많이 남았습니다",
          "original_form": "그거는 인기가 없어서 많이 남았습니다",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100002997.1.16",
          "form": "전어회도 맛있겠는데요",
          "original_form": "전어회도 맛있겠는데요",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100002997.1.17",
          "form": "회 먹으러 가고싶네요",
          "original_form": "회 먹으러 가고싶네요",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100002997.1.18",
          "form": "저는 초밥을 좋아합니다",
          "original_form": "저는 초밥을 좋아합니다",
          "speaker_id": "2",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002997.1.19",
          "form": "초밥좋아하시나요",
          "original_form": "초밥좋아하시나요",
          "speaker_id": "2",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002997.1.20",
          "form": "초밥 좋죠",
          "original_form": "초밥 좋죠",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002997.1.21",
          "form": "어떤 초밥 좋아해요?",
          "original_form": "어떤 초밥 좋아해요?",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002997.1.22",
          "form": "저는 흰살생선을 좋아합니다",
          "original_form": "저는 흰살생선을 좋아합니다",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100002997.1.23",
          "form": "계란초밥도 좋아합니다",
          "original_form": "계란초밥도 좋아합니다",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100002997.1.24",
          "form": "어떤 초밥을 좋아하시나요",
          "original_form": "어떤 초밥을 좋아하시나요",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100002997.1.25",
          "form": "전 연어초밥이 좋아요",
          "original_form": "전 연어초밥이 좋아요",
