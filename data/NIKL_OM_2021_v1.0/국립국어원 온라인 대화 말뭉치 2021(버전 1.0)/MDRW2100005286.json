      "utterance": [
        {
          "id": "MDRW2100005286.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005286.1.2",
          "form": "그냥 보통이요",
          "original_form": "그냥 보통이요",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005286.1.3",
          "form": "저는 좋아요",
          "original_form": "저는 좋아요",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005286.1.4",
          "form": "어디어디가 좋으세요 ?",
          "original_form": "어디어디가 좋으세요 ?",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005286.1.5",
          "form": "요샌 자주 못가죠?",
          "original_form": "요샌 자주 못가죠?",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005286.1.6",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005286.1.7",
          "form": "어디가 좋으셨어요?",
          "original_form": "어디가 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005286.1.8",
          "form": "호주요",
          "original_form": "호주요",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005286.1.9",
          "form": "아 호주 좋죠",
          "original_form": "아 호주 좋죠",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005286.1.10",
          "form": "전 장거리 비행은 못해요 ㅜㅜ",
          "original_form": "전 장거리 비행은 못해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005286.1.11",
          "form": "네 멀더라구요",
          "original_form": "네 멀더라구요",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005286.1.12",
          "form": "그렇죠 저희 엄마도 다녀오시고 힘드시다 하더라고요",
          "original_form": "그렇죠 저희 엄마도 다녀오시고 힘드시다 하더라고요",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005286.1.13",
          "form": "근데 넘좋았다 하셨어요",
          "original_form": "근데 넘좋았다 하셨어요",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005286.1.14",
          "form": "네 강추뇨",
          "original_form": "네 강추뇨",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005286.1.15",
          "form": "ㅎㅎ 정말 좋으셨나봐요",
          "original_form": "ㅎㅎ 정말 좋으셨나봐요",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005286.1.16",
          "form": "아 언제 해외여행 다시갈지 모르겠어요",
          "original_form": "아 언제 해외여행 다시갈지 모르겠어요",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005286.1.17",
          "form": "여행은 기념품과 음식인데",
          "original_form": "여행은 기념품과 음식인데",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005286.1.18",
          "form": "뭐드시고 사오셨어요",
          "original_form": "뭐드시고 사오셨어요",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005286.1.19",
          "form": "오래되서 기억이",
          "original_form": "오래되서 기억이",
          "speaker_id": "1",
