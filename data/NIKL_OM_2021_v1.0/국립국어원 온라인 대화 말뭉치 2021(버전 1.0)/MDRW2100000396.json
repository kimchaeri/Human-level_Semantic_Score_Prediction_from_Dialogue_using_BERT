      "utterance": [
        {
          "id": "MDRW2100000396.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000396.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000396.1.3",
          "form": "요즘 날씨가 많이 선선해졌죠!!!",
          "original_form": "요즘 날씨가 많이 선선해졌죠!!!",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000396.1.4",
          "form": "네~오늘은 가을 날씨더라구요",
          "original_form": "네~오늘은 가을 날씨더라구요",
          "speaker_id": "1",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000396.1.5",
          "form": "올 여름 너무 더웠는데",
          "original_form": "올 여름 너무 더웠는데",
          "speaker_id": "2",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000396.1.6",
          "form": "이제 조금 살만하네요ㅎ",
          "original_form": "이제 조금 살만하네요ㅎ",
          "speaker_id": "2",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000396.1.7",
          "form": "네 진짜 더웠어요 ㅠㅠ",
          "original_form": "네 진짜 더웠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000396.1.8",
          "form": "곧 또 추워지겠죠ㅠㅠ",
          "original_form": "곧 또 추워지겠죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000396.1.9",
          "form": "여름이 좋으세요 겨울이 좋으세요?",
          "original_form": "여름이 좋으세요 겨울이 좋으세요?",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000396.1.10",
          "form": "저는 여름이요 겨울은 너무 추워서요",
          "original_form": "저는 여름이요 겨울은 너무 추워서요",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000396.1.11",
          "form": "니니님은요!",
          "original_form": "니니님은요!",
          "speaker_id": "1",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000396.1.12",
          "form": "저도 여름을 더 좋아했는데",
          "original_form": "저도 여름을 더 좋아했는데",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000396.1.13",
          "form": "이번여름은ㅜㅜ",
          "original_form": "이번여름은ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000396.1.14",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000396.1.15",
          "form": "또 겨울되면 여름이 그립겠져",
          "original_form": "또 겨울되면 여름이 그립겠져",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000396.1.16",
          "form": "그럴수도 있겠어요 ㅋㅋㅋㅋ",
          "original_form": "그럴수도 있겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000396.1.17",
          "form": "다음주는 계속 비온다던데 저희지역은",
          "original_form": "다음주는 계속 비온다던데 저희지역은",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000396.1.18",
          "form": "아 정말요? 저희는 어제 많이오고 오늘은 맑아요",
          "original_form": "아 정말요? 저희는 어제 많이오고 오늘은 맑아요",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000396.1.19",
          "form": "비 오고나면 진짜 가을이 올것같아요",
          "original_form": "비 오고나면 진짜 가을이 올것같아요",
          "speaker_id": "2",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000396.1.20",
          "form": "네~오늘도 바람이 선선해요",
          "original_form": "네~오늘도 바람이 선선해요",
          "speaker_id": "1",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000396.1.21",
          "form": "코로나때문에 여행을 못가서 아쉽네요",
          "original_form": "코로나때문에 여행을 못가서 아쉽네요",
          "speaker_id": "2",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000396.1.22",
          "form": "네 맞아요~ㅠㅠ 날씨도 좋은데 아쉬워요",
          "original_form": "네 맞아요~ㅠㅠ 날씨도 좋은데 아쉬워요",
          "speaker_id": "1",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000396.1.23",
          "form": "쇼핑몰도 다 이제 가을옷팔더라고요",
          "original_form": "쇼핑몰도 다 이제 가을옷팔더라고요",
          "speaker_id": "2",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000396.1.24",
          "form": "아 이제 가을이 오나보네요",
          "original_form": "아 이제 가을이 오나보네요",
          "speaker_id": "1",
          "time": "20210819 17:22"
        }
      ]
    }
