      "utterance": [
        {
          "id": "MDRW2100024430.1.1",
          "form": "우정키링하려고 구매했는데 제걸 안샀어욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우정키링하려고 구매했는데 제걸 안샀어욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:11"
        },
        {
          "id": "MDRW2100024430.1.2",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:11"
        },
        {
          "id": "MDRW2100024430.1.3",
          "form": "우정 완성ㅇㅣ..ㅋㅋㅋㅋㅋ",
          "original_form": "우정 완성ㅇㅣ..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:11"
        },
        {
          "id": "MDRW2100024430.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ친구분은 만드신건가요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ친구분은 만드신건가요?",
          "speaker_id": "1",
          "time": "20210831 09:11"
        },
        {
          "id": "MDRW2100024430.1.5",
          "form": "넹ㅋㅋ 저까지 6명인데 친구들거 다 만들고 보니 읎더라구요",
          "original_form": "넹ㅋㅋ 저까지 6명인데 친구들거 다 만들고 보니 읎더라구요",
          "speaker_id": "2",
          "time": "20210831 09:12"
        },
        {
          "id": "MDRW2100024430.1.6",
          "form": "색별로 준비했는데..흑",
          "original_form": "색별로 준비했는데..흑",
          "speaker_id": "2",
          "time": "20210831 09:12"
        },
        {
          "id": "MDRW2100024430.1.7",
          "form": "그래서 다시 가고싶어요ㅋㅋㅋ",
          "original_form": "그래서 다시 가고싶어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:12"
        },
        {
          "id": "MDRW2100024430.1.8",
          "form": "아 ㅡㅋㅋㅋㅜㅠㅠㅠ",
          "original_form": "아 ㅡㅋㅋㅋㅜㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 09:13"
        },
        {
          "id": "MDRW2100024430.1.9",
          "form": "다시 가야겠네요!!!",
          "original_form": "다시 가야겠네요!!!",
          "speaker_id": "1",
          "time": "20210831 09:13"
        },
        {
          "id": "MDRW2100024430.1.10",
          "form": "아예 많이 만들어서 팔아보시는건...",
          "original_form": "아예 많이 만들어서 팔아보시는건...",
          "speaker_id": "1",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100024430.1.11",
          "form": "어떠세요?ㅎㅎㅎ",
          "original_form": "어떠세요?ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100024430.1.12",
          "form": "전 손재주가 없어서 그렇게 잘 만드시는 분들보면",
          "original_form": "전 손재주가 없어서 그렇게 잘 만드시는 분들보면",
          "speaker_id": "1",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100024430.1.13",
          "form": "오..?! 그것도 괜찮겠네요..",
          "original_form": "오..?! 그것도 괜찮겠네요..",
          "speaker_id": "2",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100024430.1.14",
          "form": "엇 왜 나쁜말이라고 하죠",
          "original_form": "엇 왜 나쁜말이라고 하죠",
          "speaker_id": "1",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100024430.1.15",
          "form": "앗 저도 샘플따라..",
          "original_form": "앗 저도 샘플따라..",
          "speaker_id": "2",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100024430.1.16",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "2",
          "time": "20210831 09:14"
        },
        {
          "id": "MDRW2100024430.1.17",
          "form": "많이 만들어주셨으면 좋겠어요!",
          "original_form": "많이 만들어주셨으면 좋겠어요!",
          "speaker_id": "1",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100024430.1.18",
          "form": "오 이건 되네요ㅎㄹ",
          "original_form": "오 이건 되네요ㅎㄹ",
          "speaker_id": "1",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100024430.1.19",
          "form": "이거 좀 이상한게 있더라구요",
          "original_form": "이거 좀 이상한게 있더라구요",
          "speaker_id": "2",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100024430.1.20",
          "form": "옷ㅋㅋㅋ",
          "original_form": "옷ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100024430.1.21",
          "form": "근데 어디 팔아야할까요?ㅠ",
          "original_form": "근데 어디 팔아야할까요?ㅠ",
          "speaker_id": "2",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100024430.1.22",
          "form": "음 사진을 예쁘게 찍어서",
          "original_form": "음 사진을 예쁘게 찍어서",
          "speaker_id": "1",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100024430.1.23",
          "form": "인스타에 올리거나 블로그 마켓같은건 어떠신가요?",
          "original_form": "인스타에 올리거나 블로그 마켓같은건 어떠신가요?",
          "speaker_id": "1",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100024430.1.24",
          "form": "대량으로 만드시면 아이디어스같은",
          "original_form": "대량으로 만드시면 아이디어스같은",
          "speaker_id": "1",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100024430.1.25",
          "form": "사이트도 있어요!! 친구가 하더라구요",
          "original_form": "사이트도 있어요!! 친구가 하더라구요",
          "speaker_id": "1",
          "time": "20210831 09:16"
        },
        {
          "id": "MDRW2100024430.1.26",
          "form": "오오.. 인스타나 블로그에 올려서 팔수도 있군요",
          "original_form": "오오.. 인스타나 블로그에 올려서 팔수도 있군요",
          "speaker_id": "2",
          "time": "20210831 09:17"
        },
        {
          "id": "MDRW2100024430.1.27",
          "form": "몰랐네용!",
          "original_form": "몰랐네용!",
          "speaker_id": "2",
          "time": "20210831 09:17"
        },
        {
          "id": "MDRW2100024430.1.28",
          "form": "알아봐야겠어요ㅣㄱㅋ",
          "original_form": "알아봐야겠어요ㅣㄱㅋ",
          "speaker_id": "2",
          "time": "20210831 09:17"
        },
        {
          "id": "MDRW2100024430.1.29",
          "form": "ㅎㅎ 손재주있으시면 꼭 해보세요!",
          "original_form": "ㅎㅎ 손재주있으시면 꼭 해보세요!",
          "speaker_id": "1",
          "time": "20210831 09:17"
        },
        {
          "id": "MDRW2100024430.1.30",
          "form": "요즘 소소히 부업하는것도 인기라더니 여러가지가 있네요",
          "original_form": "요즘 소소히 부업하는것도 인기라더니 여러가지가 있네요",
          "speaker_id": "2",
          "time": "20210831 09:17"
        },
        {
          "id": "MDRW2100024430.1.31",
          "form": "앗! 일단 슬쩍 담가보겠습니닷!",
          "original_form": "앗! 일단 슬쩍 담가보겠습니닷!",
          "speaker_id": "2",
          "time": "20210831 09:18"
        }
      ]
