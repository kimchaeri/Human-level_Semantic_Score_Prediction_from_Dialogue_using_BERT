        },
        {
          "id": "MDRW2100026664.1.2",
          "form": "회사 다니세요?",
          "original_form": "회사 다니세요?",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026664.1.3",
          "form": "지금 쉬고 있는 중이에요",
          "original_form": "지금 쉬고 있는 중이에요",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026664.1.4",
          "form": "휴식 중요하죠ㅜㅜ",
          "original_form": "휴식 중요하죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026664.1.5",
          "form": "무슨쪽 일 하셨어요?",
          "original_form": "무슨쪽 일 하셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026664.1.6",
          "form": "ㅋㅋ 그건 아니고 회사상황이 그래서 나왔어요",
          "original_form": "ㅋㅋ 그건 아니고 회사상황이 그래서 나왔어요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026664.1.7",
          "form": "디자인 업계였어요",
          "original_form": "디자인 업계였어요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026664.1.8",
          "form": "아 ᆢ 코로나땜시 ㅜㅜ",
          "original_form": "아 ᆢ 코로나땜시 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026664.1.9",
          "form": "오 저도 영상디자인해요",
          "original_form": "오 저도 영상디자인해요",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026664.1.10",
          "form": "워낙에 안좋았는데",
          "original_form": "워낙에 안좋았는데",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026664.1.11",
          "form": "오 진짜여!!",
          "original_form": "오 진짜여!!",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026664.1.12",
          "form": "근데 저는 시각ㅋㅋ",
          "original_form": "근데 저는 시각ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026664.1.13",
          "form": "비슷한 계통이었네요",
          "original_form": "비슷한 계통이었네요",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026664.1.14",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026664.1.15",
          "form": "네 반가워욬ㅋㅋㅋ",
          "original_form": "네 반가워욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026664.1.16",
          "form": "저 이제 뭐해야될지 고민중이었어요",
          "original_form": "저 이제 뭐해야될지 고민중이었어요",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026664.1.17",
          "form": "회사가 문을 닫은거예요?",
          "original_form": "회사가 문을 닫은거예요?",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026664.1.18",
          "form": "ㅜㅜㅢ",
          "original_form": "ㅜㅜㅢ",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026664.1.19",
          "form": "영상은 그래도 시각보다 할게 많으니까 좋겠어요",
          "original_form": "영상은 그래도 시각보다 할게 많으니까 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026664.1.20",
          "form": "네 ㅠㅠ",
          "original_form": "네 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026664.1.21",
          "form": "할게 많긴한데 3d직종이예요ㅜ",
          "original_form": "할게 많긴한데 3d직종이예요ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026664.1.22",
          "form": "아 야근 더 많으시겠구나",
          "original_form": "아 야근 더 많으시겠구나",
          "speaker_id": "2",
          "time": "20210901 23:31"
