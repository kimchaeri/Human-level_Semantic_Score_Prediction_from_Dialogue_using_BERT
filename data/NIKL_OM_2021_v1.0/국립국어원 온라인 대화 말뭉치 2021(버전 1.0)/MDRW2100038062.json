        },
        {
          "id": "MDRW2100038062.1.2",
          "form": "나 오늘 첫 출근했어",
          "original_form": "나 오늘 첫 출근했어",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038062.1.3",
          "form": "아 맞다 어땠어",
          "original_form": "아 맞다 어땠어",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100038062.1.4",
          "form": "내가 간대가 정부대학원이잖아",
          "original_form": "내가 간대가 정부대학원이잖아",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038062.1.5",
          "form": "응 연구소 같은 곳이지",
          "original_form": "응 연구소 같은 곳이지",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038062.1.6",
          "form": "그래서 보안이 까다로워서 불편했어",
          "original_form": "그래서 보안이 까다로워서 불편했어",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038062.1.7",
          "form": "신분 검사를 철저히 하는구나",
          "original_form": "신분 검사를 철저히 하는구나",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038062.1.8",
          "form": "출입증을 1시간 동안 서서 받았어",
          "original_form": "출입증을 1시간 동안 서서 받았어",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038062.1.9",
          "form": "통근 시간은 얼마나 걸려",
          "original_form": "통근 시간은 얼마나 걸려",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038062.1.10",
          "form": "넉넉하게 잡아서 1시간 정도",
          "original_form": "넉넉하게 잡아서 1시간 정도",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038062.1.11",
          "form": "생각보다 오래 걸리네",
          "original_form": "생각보다 오래 걸리네",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038062.1.12",
          "form": "아는 사람들이 있었어",
          "original_form": "아는 사람들이 있었어",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038062.1.13",
          "form": "아침에 일찍 나가야겠네",
          "original_form": "아침에 일찍 나가야겠네",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038062.1.14",
          "form": "맞아 그래도 지하철은 한 번만 갈았다",
          "original_form": "맞아 그래도 지하철은 한 번만 갈았다",
