      "utterance": [
        {
          "id": "MDRW2100013167.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100013167.1.2",
          "form": "안녕하세요 ㅋㅋㅋ",
          "original_form": "안녕하세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013167.1.3",
          "form": "좋아하는 음식있으세요?",
          "original_form": "좋아하는 음식있으세요?",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013167.1.4",
          "form": "저 초밥이요ㅋㅋㅋ",
          "original_form": "저 초밥이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013167.1.5",
          "form": "회랑요",
          "original_form": "회랑요",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013167.1.6",
          "form": "오 바다음식~",
          "original_form": "오 바다음식~",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013167.1.7",
          "form": "추억탕같은 것도 즐기시나요?",
          "original_form": "추억탕같은 것도 즐기시나요?",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013167.1.8",
          "form": "오우노우",
          "original_form": "오우노우",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013167.1.9",
          "form": "추어탕은 못먹죠",
          "original_form": "추어탕은 못먹죠",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013167.1.10",
          "form": "광어 방어 우니 육회 참치",
          "original_form": "광어 방어 우니 육회 참치",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013167.1.11",
          "form": "이런종류요",
          "original_form": "이런종류요",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013167.1.12",
          "form": "오우 노우 입이 고-급이시네요",
          "original_form": "오우 노우 입이 고-급이시네요",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013167.1.13",
          "form": "저는 고기파에요",
          "original_form": "저는 고기파에요",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013167.1.14",
          "form": "소고기 돼지고기 닭고기",
          "original_form": "소고기 돼지고기 닭고기",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013167.1.15",
          "form": "냠냠",
          "original_form": "냠냠",
          "speaker_id": "2",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013167.1.16",
          "form": "ㅋㅋㅋ저도고기도 다좋죠ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ저도고기도 다좋죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013167.1.17",
          "form": "흑돼지먹고싶네요",
          "original_form": "흑돼지먹고싶네요",
          "speaker_id": "1",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013167.1.18",
          "form": "오~  그냥 돼지랑 다른가요?",
          "original_form": "오~  그냥 돼지랑 다른가요?",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100013167.1.19",
          "form": "네네 맛있어요ㅋㅋㅋ",
          "original_form": "네네 맛있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100013167.1.20",
          "form": "제주도가고싶네요",
          "original_form": "제주도가고싶네요",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100013167.1.21",
          "form": "오 궁금하다",
          "original_form": "오 궁금하다",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100013167.1.22",
          "form": "전 고등학교때 먹어본거 같기도 한데 기억이 안나네요",
          "original_form": "전 고등학교때 먹어본거 같기도 한데 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100013167.1.23",
          "form": "맥주vs소주는 어떠세요?",
          "original_form": "맥주vs소주는 어떠세요?",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100013167.1.24",
          "form": "저는 소주파요",
          "original_form": "저는 소주파요",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100013167.1.25",
          "form": "맥주는 먹으면 배에서 보글거려서 ㅠ",
          "original_form": "맥주는 먹으면 배에서 보글거려서 ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100013167.1.26",
          "form": "헉ㄷㄷ",
          "original_form": "헉ㄷㄷ",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100013167.1.27",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100013167.1.28",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100013167.1.29",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 13:31"
        }
      ]
    }
  ]
