      "utterance": [
        {
          "id": "MDRW2100027925.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027925.1.2",
          "form": "끝내주게 잠자기 도  노는거니까",
          "original_form": "끝내주게 잠자기 도  노는거니까",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027925.1.3",
          "form": "부지런하지않을래여 ㅋㅋㅋ",
          "original_form": "부지런하지않을래여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027925.1.4",
          "form": "잠자기 좋죠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "잠자기 좋죠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100027925.1.5",
          "form": "푹잘자면 칼로리도 소모된대요",
          "original_form": "푹잘자면 칼로리도 소모된대요",
          "speaker_id": "1",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100027925.1.6",
          "form": "근데 밤에 일찍자야하는데",
          "original_form": "근데 밤에 일찍자야하는데",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100027925.1.7",
          "form": "전 새벽에 자요ㅠ",
          "original_form": "전 새벽에 자요ㅠ",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100027925.1.8",
          "form": "아침엔 늦게일어나고 항상..",
          "original_form": "아침엔 늦게일어나고 항상..",
          "speaker_id": "2",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100027925.1.9",
          "form": "저도 비슷해요ㅋㅋ",
          "original_form": "저도 비슷해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100027925.1.10",
          "form": "5시에 자다가 개강해서",
          "original_form": "5시에 자다가 개강해서",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100027925.1.11",
          "form": "일찍자는게 3시에요",
          "original_form": "일찍자는게 3시에요",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100027925.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100027925.1.13",
          "form": "진짜ㅡ.....",
          "original_form": "진짜ㅡ.....",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100027925.1.14",
          "form": "일찍자기가 세상에서 가장 힘들어요 ㅋㅋ",
          "original_form": "일찍자기가 세상에서 가장 힘들어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100027925.1.15",
          "form": "일찍 자야숙면에 좋다는데..",
          "original_form": "일찍 자야숙면에 좋다는데..",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100027925.1.16",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100027925.1.17",
          "form": "패션에 관심 많으세요?",
          "original_form": "패션에 관심 많으세요?",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100027925.1.18",
          "form": "관심있어요!",
          "original_form": "관심있어요!",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100027925.1.19",
          "form": "많이 사진못하지만요ㅋㅋ",
          "original_form": "많이 사진못하지만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100027925.1.20",
          "form": "저는 옷을 잘 몰라서 브랜드도 잘 모르고.. 어떻게 입어야 잘어울리고 좋은지를 몰라서",
          "original_form": "저는 옷을 잘 몰라서 브랜드도 잘 모르고.. 어떻게 입어야 잘어울리고 좋은지를 몰라서",
          "speaker_id": "2",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100027925.1.21",
          "form": "그냥 싼 옷 사서 대충 입어요ㅠㅠ",
          "original_form": "그냥 싼 옷 사서 대충 입어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100027925.1.22",
          "form": "스스로 좀 잘 차려입고싶은데 잘 볼줄도 모르고 가격이 부담되니까 바뀌질 않네옹",
          "original_form": "스스로 좀 잘 차려입고싶은데 잘 볼줄도 모르고 가격이 부담되니까 바뀌질 않네옹",
          "speaker_id": "2",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100027925.1.23",
          "form": "어느정도선에서 타협하게 되는것같아요ㅠ",
          "original_form": "어느정도선에서 타협하게 되는것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100027925.1.24",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100027925.1.25",
          "form": "그래두 세트로 맞춰사면 거의 성공해용",
          "original_form": "그래두 세트로 맞춰사면 거의 성공해용",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100027925.1.26",
          "form": "수트를 사고싶은데 수트는 인쇼에서 봐도 최소 이십은 써야되더라고요..",
          "original_form": "수트를 사고싶은데 수트는 인쇼에서 봐도 최소 이십은 써야되더라고요..",
          "speaker_id": "2",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100027925.1.27",
          "form": "수트가 진짜 애매해요",
          "original_form": "수트가 진짜 애매해요",
          "speaker_id": "1",
          "time": "20210902 18:45"
        }
      ]
    }
  ]
