      "utterance": [
        {
          "id": "MDRW2100012272.1.1",
          "form": "스타벅스는 친구랑 수다 떨러만 갔는데",
          "original_form": "스타벅스는 친구랑 수다 떨러만 갔는데",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012272.1.2",
          "form": "원두 분쇄 서비스도 있었다니!!",
          "original_form": "원두 분쇄 서비스도 있었다니!!",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012272.1.3",
          "form": "원두분쇄는 집에서 머신을 사도 스벅보단 싼거라서.",
          "original_form": "원두분쇄는 집에서 머신을 사도 스벅보단 싼거라서.",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100012272.1.4",
          "form": "매장서 갈면 좋아요.",
          "original_form": "매장서 갈면 좋아요.",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100012272.1.5",
          "form": "원두 사서 갈아서 핸드드립까지 하기엔...",
          "original_form": "원두 사서 갈아서 핸드드립까지 하기엔...",
          "speaker_id": "1",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100012272.1.6",
          "form": "제가 너무 게으르네요...",
          "original_form": "제가 너무 게으르네요...",
          "speaker_id": "1",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012272.1.7",
          "form": "캡슐이나 드립이나 비슷해요.",
          "original_form": "캡슐이나 드립이나 비슷해요.",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012272.1.8",
          "form": "혹시 환경보호를 신경쓴다면 드립도 좋아요",
          "original_form": "혹시 환경보호를 신경쓴다면 드립도 좋아요",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012272.1.9",
          "form": "저는 환경을 보호하지 않고",
          "original_form": "저는 환경을 보호하지 않고",
          "speaker_id": "1",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012272.1.10",
          "form": "저의 편리를 추구할 거예요",
          "original_form": "저의 편리를 추구할 거예요",
          "speaker_id": "1",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100012272.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100012272.1.12",
          "form": "그것도 좋죠! 어떤 캡슐머신을 쓰세요?",
          "original_form": "그것도 좋죠! 어떤 캡슐머신을 쓰세요?",
          "speaker_id": "2",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100012272.1.13",
          "form": "저는 일리 머신-일리 캡슐 써요",
          "original_form": "저는 일리 머신-일리 캡슐 써요",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100012272.1.14",
          "form": "네스프레소랑 큐리그랑 일리 세 가질 먹어봤는데",
          "original_form": "네스프레소랑 큐리그랑 일리 세 가질 먹어봤는데",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100012272.1.15",
          "form": "일리가 제일 맛있어요!",
          "original_form": "일리가 제일 맛있어요!",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100012272.1.16",
          "form": "일리가 맛있군요 이태리회사죠?",
          "original_form": "일리가 맛있군요 이태리회사죠?",
          "speaker_id": "2",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100012272.1.17",
          "form": "그럴 거예요!",
          "original_form": "그럴 거예요!",
          "speaker_id": "1",
          "time": "20210824 20:55"
        },
        {
          "id": "MDRW2100012272.1.18",
          "form": "작동법 설명서만 봐서 잘은 모르지만~",
          "original_form": "작동법 설명서만 봐서 잘은 모르지만~",
          "speaker_id": "1",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100012272.1.19",
          "form": "앗 너무 대충 사는 거 같네요 하하하하핳",
          "original_form": "앗 너무 대충 사는 거 같네요 하하하하핳",
          "speaker_id": "1",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100012272.1.20",
          "form": "그냥 말해봤어요 ㅋㅋ 어디인게 뭐가 중요하겠어요.",
          "original_form": "그냥 말해봤어요 ㅋㅋ 어디인게 뭐가 중요하겠어요.",
          "speaker_id": "2",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100012272.1.21",
          "form": "마저요 맛있고 AS 잘 되는 회사가 제일 좋아요ㅋㅋ",
          "original_form": "마저요 맛있고 AS 잘 되는 회사가 제일 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:57"
        },
        {
          "id": "MDRW2100012272.1.22",
          "form": "일리 커피는 가격이 얼만가요ㅡ",
          "original_form": "일리 커피는 가격이 얼만가요ㅡ",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100012272.1.23",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100012272.1.24",
          "form": "10만원 대 초반이었던 거 같아요.",
          "original_form": "10만원 대 초반이었던 거 같아요.",
          "speaker_id": "1",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100012272.1.25",
          "form": "커피값이 10만원이요? 캡슐개당 가격이 궁금해요",
          "original_form": "커피값이 10만원이요? 캡슐개당 가격이 궁금해요",
          "speaker_id": "2",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100012272.1.26",
          "form": "아 머신 가격이요ㅋㅋㅋ",
          "original_form": "아 머신 가격이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100012272.1.27",
          "form": "일리 캡슐은...",
          "original_form": "일리 캡슐은...",
          "speaker_id": "1",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100012272.1.28",
          "form": "개당 5-600원 정도 준 거 같아요.",
          "original_form": "개당 5-600원 정도 준 거 같아요.",
          "speaker_id": "1",
          "time": "20210824 21:00"
        }
      ]
    }
