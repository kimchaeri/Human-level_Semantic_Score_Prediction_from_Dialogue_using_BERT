      "utterance": [
        {
          "id": "MDRW2100001399.1.1",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "1",
          "time": "20211014 10:50"
        },
        {
          "id": "MDRW2100001399.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211014 10:50"
        },
        {
          "id": "MDRW2100001399.1.3",
          "form": "혹시 요즘 즐겨하는 게임이 있으신가요?",
          "original_form": "혹시 요즘 즐겨하는 게임이 있으신가요?",
          "speaker_id": "2",
          "time": "20211014 10:50"
        },
        {
          "id": "MDRW2100001399.1.4",
          "form": "안녕하세요 ㅎㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20211014 10:50"
        },
        {
          "id": "MDRW2100001399.1.5",
          "form": "앗 음",
          "original_form": "앗 음",
          "speaker_id": "1",
          "time": "20211014 10:50"
        },
        {
          "id": "MDRW2100001399.1.6",
          "form": "혹시 스팀게임아세요? ㅎㅎ",
          "original_form": "혹시 스팀게임아세요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20211014 10:51"
        },
        {
          "id": "MDRW2100001399.1.7",
          "form": "폴가이즈나 어몽어스도 스팀게임인가요?",
          "original_form": "폴가이즈나 어몽어스도 스팀게임인가요?",
          "speaker_id": "2",
          "time": "20211014 10:51"
        },
        {
          "id": "MDRW2100001399.1.8",
          "form": "아아 맞아요 폴가이즈는 스팀이랑 플레이스테이션 모두에서 될 거예요 ㅎㅎ",
          "original_form": "아아 맞아요 폴가이즈는 스팀이랑 플레이스테이션 모두에서 될 거예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211014 10:52"
        },
        {
          "id": "MDRW2100001399.1.9",
          "form": "주로 하시는 스팀게임있으신가요?",
          "original_form": "주로 하시는 스팀게임있으신가요?",
          "speaker_id": "2",
          "time": "20211014 10:52"
        },
        {
          "id": "MDRW2100001399.1.10",
          "form": "저는 스팀게임에 문명이라는 게임을 좋아해요!",
          "original_form": "저는 스팀게임에 문명이라는 게임을 좋아해요!",
          "speaker_id": "1",
          "time": "20211014 10:52"
        },
        {
          "id": "MDRW2100001399.1.11",
          "form": "문명 아시나요? ㅎㅎㅎㅎ",
          "original_form": "문명 아시나요? ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20211014 10:52"
        },
        {
          "id": "MDRW2100001399.1.12",
          "form": "엇 알지만 직접 플레이해본적은 없어요 ㅎㅎ",
          "original_form": "엇 알지만 직접 플레이해본적은 없어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211014 10:53"
        },
        {
          "id": "MDRW2100001399.1.13",
          "form": "어떤 형식의 게임인가요?",
          "original_form": "어떤 형식의 게임인가요?",
          "speaker_id": "2",
          "time": "20211014 10:53"
        },
        {
          "id": "MDRW2100001399.1.14",
          "form": "앗 name2님 문명 아시는군요 ㅎㅎㅎ 문명은",
          "original_form": "앗 &name2&님 문명 아시는군요 ㅎㅎㅎ 문명은",
          "speaker_id": "1",
          "time": "20211014 10:54"
        },
        {
          "id": "MDRW2100001399.1.15",
          "form": "하나의 문명을 선택해서, 나라를 세우고 그 나라의 문명을 키우는 전략게임 같은 거예요!",
          "original_form": "하나의 문명을 선택해서, 나라를 세우고 그 나라의 문명을 키우는 전략게임 같은 거예요!",
          "speaker_id": "1",
          "time": "20211014 10:54"
        },
        {
          "id": "MDRW2100001399.1.16",
          "form": "아~ 약간 심즈랑 비슷한 개념인가요?",
          "original_form": "아~ 약간 심즈랑 비슷한 개념인가요?",
          "speaker_id": "2",
          "time": "20211014 10:55"
        },
        {
          "id": "MDRW2100001399.1.17",
          "form": "심즈해보신적 있으신가요?",
          "original_form": "심즈해보신적 있으신가요?",
          "speaker_id": "2",
          "time": "20211014 10:55"
        },
        {
          "id": "MDRW2100001399.1.18",
          "form": "앗 맞아요 심즈랑 비슷한 면도 있어요! ㅎㅎㅎ 심즈 해보고 싶은데",
          "original_form": "앗 맞아요 심즈랑 비슷한 면도 있어요! ㅎㅎㅎ 심즈 해보고 싶은데",
          "speaker_id": "1",
          "time": "20211014 10:56"
        },
        {
          "id": "MDRW2100001399.1.19",
          "form": "아직 못해봤네요 ㅠㅠㅠ name2님은 심즈 좋아하시나요??",
          "original_form": "아직 못해봤네요 ㅠㅠㅠ &name2&님은 심즈 좋아하시나요??",
          "speaker_id": "1",
          "time": "20211014 10:56"
        },
        {
          "id": "MDRW2100001399.1.20",
          "form": "저는 옛날에 잠깐했었는데 심즈는 용량을 너무 많이 차지하더라구요..",
