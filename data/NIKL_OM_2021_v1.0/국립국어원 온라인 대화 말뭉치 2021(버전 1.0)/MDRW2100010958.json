      "utterance": [
        {
          "id": "MDRW2100010958.1.1",
          "form": "최근에 산 비싼물건은?",
          "original_form": "최근에 산 비싼물건은?",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010958.1.2",
          "form": "닌텐도요ㅋㅋ",
          "original_form": "닌텐도요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010958.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010958.1.4",
          "form": "저는 한동안 쇼핑을 안했어요",
          "original_form": "저는 한동안 쇼핑을 안했어요",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010958.1.5",
          "form": "게임 좋아하시면 닌텐도 추천드려요",
          "original_form": "게임 좋아하시면 닌텐도 추천드려요",
          "speaker_id": "1",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010958.1.6",
          "form": "닌텐도게임도 따로 사야하는 거 아닌가요?",
          "original_form": "닌텐도게임도 따로 사야하는 거 아닌가요?",
          "speaker_id": "2",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010958.1.7",
          "form": "맞아요 돈이 많이드는 게임기지만 만족해요",
          "original_form": "맞아요 돈이 많이드는 게임기지만 만족해요",
          "speaker_id": "1",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010958.1.8",
          "form": "저는 요즘 청소기 갖고싶어요",
          "original_form": "저는 요즘 청소기 갖고싶어요",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010958.1.9",
          "form": "다이슨이요?",
          "original_form": "다이슨이요?",
          "speaker_id": "1",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010958.1.10",
          "form": "다이슨은 너무 무겁대서 제로코드가 좋대요",
          "original_form": "다이슨은 너무 무겁대서 제로코드가 좋대요",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010958.1.11",
          "form": "무선청소기도 진짜 비싸죠ㅜㅜ",
          "original_form": "무선청소기도 진짜 비싸죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010958.1.12",
          "form": "그쵸 최저가가 75만원정도라 모아야할 거같아요",
          "original_form": "그쵸 최저가가 75만원정도라 모아야할 거같아요",
          "speaker_id": "2",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010958.1.13",
          "form": "추석 때 할인하지 않을까요?",
          "original_form": "추석 때 할인하지 않을까요?",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010958.1.14",
          "form": "세일을 노리는 것두 좋네요",
          "original_form": "세일을 노리는 것두 좋네요",
          "speaker_id": "2",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010958.1.15",
          "form": "중고거래도 좋은데 청소기는 좀 그렇네요...",
          "original_form": "중고거래도 좋은데 청소기는 좀 그렇네요...",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010958.1.16",
          "form": "아무래도 그렇죠ㅎㅎ 또 위시리스트 있나요?",
          "original_form": "아무래도 그렇죠ㅎㅎ 또 위시리스트 있나요?",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010958.1.17",
          "form": "지플립 새로 나온 거 탐나요",
          "original_form": "지플립 새로 나온 거 탐나요",
          "speaker_id": "1",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010958.1.18",
          "form": "저는 1이 젤 예쁜 거같던데....요즘 핸드폰 진짜 비싸요",
          "original_form": "저는 1이 젤 예쁜 거같던데....요즘 핸드폰 진짜 비싸요",
          "speaker_id": "2",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100010958.1.19",
          "form": "그러게요 그래도 한방에 자급제로 사고 알뜰요금제 쓰면 좀 나아요",
          "original_form": "그러게요 그래도 한방에 자급제로 사고 알뜰요금제 쓰면 좀 나아요",
          "speaker_id": "1",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100010958.1.20",
          "form": "글쿤요",
          "original_form": "글쿤요",
          "speaker_id": "2",
