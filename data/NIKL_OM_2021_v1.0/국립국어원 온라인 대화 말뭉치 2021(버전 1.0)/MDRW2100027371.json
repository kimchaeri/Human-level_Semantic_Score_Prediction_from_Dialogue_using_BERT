      "utterance": [
        {
          "id": "MDRW2100027371.1.1",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027371.1.2",
          "form": "점심메뉴 뭐인가요!",
          "original_form": "점심메뉴 뭐인가요!",
          "speaker_id": "2",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027371.1.3",
          "form": "육개장먹는 중이에요!",
          "original_form": "육개장먹는 중이에요!",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100027371.1.4",
          "form": "오 저는 방금 된장찌개 먹었어요!",
          "original_form": "오 저는 방금 된장찌개 먹었어요!",
          "speaker_id": "2",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100027371.1.5",
          "form": "와된장찌개도 좋죠 ㅎㅎ",
          "original_form": "와된장찌개도 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027371.1.6",
          "form": "빨리드셨네요 ㅎㅎ",
          "original_form": "빨리드셨네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027371.1.7",
          "form": "날씨가 쌀쌀해서 국물있는게 좋네여",
          "original_form": "날씨가 쌀쌀해서 국물있는게 좋네여",
          "speaker_id": "2",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027371.1.8",
          "form": "네 저희회사 점심시간이 11시30분부터라 ㅠㅠ",
          "original_form": "네 저희회사 점심시간이 11시30분부터라 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027371.1.9",
          "form": "아 빠르네요 점심시간이 ㅎㅎ",
          "original_form": "아 빠르네요 점심시간이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027371.1.10",
          "form": "비가안와서좋긴한데",
          "original_form": "비가안와서좋긴한데",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027371.1.11",
          "form": "살짝 쌀쌀해진거같긴해요 ㅎㅎ",
          "original_form": "살짝 쌀쌀해진거같긴해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027371.1.12",
          "form": "맞아요. 저는 지난주부터 아아 포기하고 뜨아 마셔요 ㅋㅋㅋ",
          "original_form": "맞아요. 저는 지난주부터 아아 포기하고 뜨아 마셔요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027371.1.13",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027371.1.14",
          "form": "그정도는 아니지않아요?",
          "original_form": "그정도는 아니지않아요?",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027371.1.15",
          "form": "습하고",
          "original_form": "습하고",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027371.1.16",
          "form": "뭔가 더운건지뭔지하는 느낌?",
          "original_form": "뭔가 더운건지뭔지하는 느낌?",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027371.1.17",
          "form": "아 제가 추위를 엄청 타요 ㅠㅠㅋㅋㅋㅋ",
          "original_form": "아 제가 추위를 엄청 타요 ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:10"
        },
        {
          "id": "MDRW2100027371.1.18",
          "form": "오늘도 긴팔자켓 입고 나옴...",
          "original_form": "오늘도 긴팔자켓 입고 나옴...",
          "speaker_id": "2",
          "time": "20210902 12:10"
        },
        {
          "id": "MDRW2100027371.1.19",
          "form": "와 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:11"
        },
        {
          "id": "MDRW2100027371.1.20",
          "form": "진짜 추위엄청타시나보ㅏ요",
          "original_form": "진짜 추위엄청타시나보ㅏ요",
          "speaker_id": "1",
          "time": "20210902 12:11"
        },
        {
          "id": "MDRW2100027371.1.21",
          "form": "긴팔은 생각두못했어요",
          "original_form": "긴팔은 생각두못했어요",
          "speaker_id": "1",
          "time": "20210902 12:11"
        },
        {
          "id": "MDRW2100027371.1.22",
          "form": "ㅋㅋ오늘 저녁도 뜨거운 쌀국수 먹으려고요 ㅠㅠㅋㅋ",
          "original_form": "ㅋㅋ오늘 저녁도 뜨거운 쌀국수 먹으려고요 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027371.1.23",
          "form": "저 8월중순부터 겨울이불 덮었읍니다..",
          "original_form": "저 8월중순부터 겨울이불 덮었읍니다..",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027371.1.24",
          "form": "저녁메뉴 뭘로하실거에요?!",
          "original_form": "저녁메뉴 뭘로하실거에요?!",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027371.1.25",
          "form": "생각안해봤어요 ㅋㅋㅋㅋㅋㅋㅋ 아직 점심도 먹는중이랔ㅋㅋㅋㅋㅋ",
          "original_form": "생각안해봤어요 ㅋㅋㅋㅋㅋㅋㅋ 아직 점심도 먹는중이랔ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027371.1.26",
          "form": "쌀국수맛있겠네요 ㅎㅎ",
          "original_form": "쌀국수맛있겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027371.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ 아침먹으면서 점심생각하고 점심먹으면서 저녁생각을..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아침먹으면서 점심생각하고 점심먹으면서 저녁생각을..",
          "speaker_id": "2",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027371.1.28",
          "form": "맛집다니는거 좋아하세요?",
          "original_form": "맛집다니는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027371.1.29",
          "form": "전 사실 기다리는 거 안 좋아해서 별로 안 좋아해여 ㅠㅋㅋ",
          "original_form": "전 사실 기다리는 거 안 좋아해서 별로 안 좋아해여 ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027371.1.30",
          "form": "좋아하지만 동지가 잘없어요 ㅠ",
          "original_form": "좋아하지만 동지가 잘없어요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027371.1.31",
          "form": "아 기다리는거보다",
          "original_form": "아 기다리는거보다",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027371.1.32",
          "form": "노포집?",
          "original_form": "노포집?",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027371.1.33",
          "form": "좋아하요",
          "original_form": "좋아하요",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027371.1.34",
          "form": "안기다리고싶어요",
          "original_form": "안기다리고싶어요",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027371.1.35",
          "form": "기다려도 맛 보장은 안되더라구요",
          "original_form": "기다려도 맛 보장은 안되더라구요",
          "speaker_id": "1",
          "time": "20210902 12:15"
        }
      ]
    }
  ]
