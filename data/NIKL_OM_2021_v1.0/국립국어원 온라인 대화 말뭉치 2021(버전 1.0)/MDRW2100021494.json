      "utterance": [
        {
          "id": "MDRW2100021494.1.1",
          "form": "컴퓨터 사고싶다",
          "original_form": "컴퓨터 사고싶다",
          "speaker_id": "1",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100021494.1.2",
          "form": "나 이번에 샀는데",
          "original_form": "나 이번에 샀는데",
          "speaker_id": "2",
          "time": "20210829 11:57"
        },
        {
          "id": "MDRW2100021494.1.3",
          "form": "아 진짜? 본체만 샀어?",
          "original_form": "아 진짜? 본체만 샀어?",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100021494.1.4",
          "form": "아니 모니터 스피커 본체 키보드 마우스 싹 샀어ㅋㅋ",
          "original_form": "아니 모니터 스피커 본체 키보드 마우스 싹 샀어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100021494.1.5",
          "form": "헐 얼마 들었어?",
          "original_form": "헐 얼마 들었어?",
          "speaker_id": "1",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100021494.1.6",
          "form": "한 150정도!",
          "original_form": "한 150정도!",
          "speaker_id": "2",
          "time": "20210829 12:01"
        },
        {
          "id": "MDRW2100021494.1.7",
          "form": "개비싸네.. 모니터 몇인치야",
          "original_form": "개비싸네.. 모니터 몇인치야",
          "speaker_id": "1",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100021494.1.8",
          "form": "27인치인가 그래!",
          "original_form": "27인치인가 그래!",
          "speaker_id": "2",
          "time": "20210829 12:03"
        },
        {
          "id": "MDRW2100021494.1.9",
          "form": "와 엄청 크다!",
          "original_form": "와 엄청 크다!",
          "speaker_id": "1",
          "time": "20210829 12:03"
        },
        {
          "id": "MDRW2100021494.1.10",
          "form": "근데 그렇게 크지 않은데.. 27인치가 아닌가...",
          "original_form": "근데 그렇게 크지 않은데.. 27인치가 아닌가...",
          "speaker_id": "2",
          "time": "20210829 12:04"
        },
        {
          "id": "MDRW2100021494.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:04"
        },
        {
          "id": "MDRW2100021494.1.12",
          "form": "아 그래? 가로로 긴가? 내것도 그래",
          "original_form": "아 그래? 가로로 긴가? 내것도 그래",
          "speaker_id": "1",
          "time": "20210829 12:04"
        },
        {
          "id": "MDRW2100021494.1.13",
          "form": "응응ㅋㅋㅋㅋ 약간 그런느낌",
          "original_form": "응응ㅋㅋㅋㅋ 약간 그런느낌",
          "speaker_id": "2",
          "time": "20210829 12:05"
        },
        {
          "id": "MDRW2100021494.1.14",
          "form": "내 모니터는 29인치인가 그런데 가로가 80센치야 ㅋㅋ세로는 작아",
          "original_form": "내 모니터는 29인치인가 그런데 가로가 80센치야 ㅋㅋ세로는 작아",
          "speaker_id": "1",
          "time": "20210829 12:05"
        },
        {
          "id": "MDRW2100021494.1.15",
