        },
        {
          "id": "MDRW2100011381.1.2",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "2",
          "time": "20210824 12:14"
        },
        {
          "id": "MDRW2100011381.1.3",
          "form": "무교입니다 누누님은요",
          "original_form": "무교입니다 누누님은요",
          "speaker_id": "1",
          "time": "20210824 12:15"
        },
        {
          "id": "MDRW2100011381.1.4",
          "form": "저도 무교입니다",
          "original_form": "저도 무교입니다",
          "speaker_id": "2",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011381.1.5",
          "form": "왜 종교를 안믿으시나요?",
          "original_form": "왜 종교를 안믿으시나요?",
          "speaker_id": "2",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011381.1.6",
          "form": "귀찮아서요",
          "original_form": "귀찮아서요",
          "speaker_id": "1",
          "time": "20210824 12:23"
        },
        {
          "id": "MDRW2100011381.1.7",
          "form": "특이한이유군요",
          "original_form": "특이한이유군요",
          "speaker_id": "2",
          "time": "20210824 12:29"
        },
        {
          "id": "MDRW2100011381.1.8",
          "form": "누누님은 왜 무교이신가요",
          "original_form": "누누님은 왜 무교이신가요",
          "speaker_id": "1",
          "time": "20210824 12:29"
        },
        {
          "id": "MDRW2100011381.1.9",
          "form": "저는 딱히",
          "original_form": "저는 딱히",
          "speaker_id": "2",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100011381.1.10",
          "form": "신앙심이 생기지가않아서요",
          "original_form": "신앙심이 생기지가않아서요",
          "speaker_id": "2",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100011381.1.11",
          "form": "절이나 교회 가보신적있으세요",
          "original_form": "절이나 교회 가보신적있으세요",
          "speaker_id": "1",
          "time": "20210824 12:35"
        },
        {
          "id": "MDRW2100011381.1.12",
          "form": "교회가본적있어요 어릴때친구따라서",
          "original_form": "교회가본적있어요 어릴때친구따라서",
          "speaker_id": "2",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100011381.1.13",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "2",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100011381.1.14",
          "form": "저도 초등학생때 문화상품권 준다고해서 가봤습니다",
          "original_form": "저도 초등학생때 문화상품권 준다고해서 가봤습니다",
          "speaker_id": "1",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011381.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011381.1.16",
          "form": "가서 받았나요진짜?",
          "original_form": "가서 받았나요진짜?",
          "speaker_id": "2",
          "time": "20210824 12:49"
        },
