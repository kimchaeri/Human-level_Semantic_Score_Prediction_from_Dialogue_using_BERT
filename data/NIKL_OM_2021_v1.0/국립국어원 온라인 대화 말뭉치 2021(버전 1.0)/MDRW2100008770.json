      "utterance": [
        {
          "id": "MDRW2100008770.1.1",
          "form": "공연 다니는 거 좋아하시나요",
          "original_form": "공연 다니는 거 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008770.1.2",
          "form": "네 원래 좋아하는데 요즘엔 많이 못보러 다녀요ㅠㅠ",
          "original_form": "네 원래 좋아하는데 요즘엔 많이 못보러 다녀요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008770.1.3",
          "form": "저도 콘서트 다니는 거 정말 좋은데",
          "original_form": "저도 콘서트 다니는 거 정말 좋은데",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008770.1.4",
          "form": "못 가서 너무 슬퍼요",
          "original_form": "못 가서 너무 슬퍼요",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008770.1.5",
          "form": "언제쯤 지나갈지..공연보는게 스트레스 풀리고 딱인데ㅠㅜ",
          "original_form": "언제쯤 지나갈지..공연보는게 스트레스 풀리고 딱인데ㅠㅜ",
          "speaker_id": "1",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008770.1.6",
          "form": "맞죠 소리지르고 응원하는 게 진짜 스트레스도 풀고 좋은데 말이에요ㅠㅠ",
          "original_form": "맞죠 소리지르고 응원하는 게 진짜 스트레스도 풀고 좋은데 말이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008770.1.7",
          "form": "그니까요ㅎㅎ 진짜 힘들어도 에너지 얻고 오는 느낌ㅜㅜ",
          "original_form": "그니까요ㅎㅎ 진짜 힘들어도 에너지 얻고 오는 느낌ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008770.1.8",
          "form": "여름엔 덥고 습하지만 소리지르면서 스트레스 빠아아악 풀고 넘 좋은데ㅠㅠ",
          "original_form": "여름엔 덥고 습하지만 소리지르면서 스트레스 빠아아악 풀고 넘 좋은데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008770.1.9",
          "form": "맞아요 오션월드에서 하는 행사같은것도 이젠 꿈도 못꾸는ㅜㅜ",
          "original_form": "맞아요 오션월드에서 하는 행사같은것도 이젠 꿈도 못꾸는ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008770.1.10",
          "form": "거긴 마스크도 더 못 쓰니깐 ㅠㅠ",
          "original_form": "거긴 마스크도 더 못 쓰니깐 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008770.1.11",
          "form": "콘서트가서 떼창하고 싶어요",
          "original_form": "콘서트가서 떼창하고 싶어요",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008770.1.12",
          "form": "저두요...ㅠㅠ진짜 재밌는데",
          "original_form": "저두요...ㅠㅠ진짜 재밌는데",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008770.1.13",
          "form": "코로나 끝나고 하는 공연은 티켓팅 더 힘들어지겠죠??",
          "original_form": "코로나 끝나고 하는 공연은 티켓팅 더 힘들어지겠죠??",
          "speaker_id": "2",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008770.1.14",
          "form": "벼르고 있은 분들 많아서 그럴거같아요ㅠㅠ",
          "original_form": "벼르고 있은 분들 많아서 그럴거같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008770.1.15",
          "form": "그 전에도 힘들었는데",
          "original_form": "그 전에도 힘들었는데",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008770.1.16",
          "form": "더 표 구하기 힘들 것 같아요",
          "original_form": "더 표 구하기 힘들 것 같아요",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008770.1.17",
          "form": "암표도 많고",
          "original_form": "암표도 많고",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008770.1.18",
          "form": "그러게 말이에요",
          "original_form": "그러게 말이에요",
          "speaker_id": "1",
          "time": "20210822 18:28"
        }
      ]
