      "utterance": [
        {
          "id": "MDRW2100000378.1.1",
          "form": "요즘엔 어느 커뮤니티가 핫한가요?",
          "original_form": "요즘엔 어느 커뮤니티가 핫한가요?",
          "speaker_id": "1",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000378.1.2",
          "form": "저는 아기엄마라 맘베??",
          "original_form": "저는 아기엄마라 맘베??",
          "speaker_id": "2",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000378.1.3",
          "form": "아기 용품 핫딜때문에 자주가게 되더라고요..",
          "original_form": "아기 용품 핫딜때문에 자주가게 되더라고요..",
          "speaker_id": "2",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000378.1.4",
          "form": "핫딜 정보가 최고이지요.",
          "original_form": "핫딜 정보가 최고이지요.",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000378.1.5",
          "form": "스사사라고 각종 경제정보 모이는 곳도 핫해요",
          "original_form": "스사사라고 각종 경제정보 모이는 곳도 핫해요",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000378.1.6",
          "form": "어디 이용하는 커뮤니티 있으세요??",
          "original_form": "어디 이용하는 커뮤니티 있으세요??",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000378.1.7",
          "form": "오 스사사도 알지요!",
          "original_form": "오 스사사도 알지요!",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000378.1.8",
          "form": "알면 알수록 이젠 모르면 안되겠더라고요",
          "original_form": "알면 알수록 이젠 모르면 안되겠더라고요",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000378.1.9",
          "form": "예전에는 카드 적립때문에 많이 들락날락 했었는데",
          "original_form": "예전에는 카드 적립때문에 많이 들락날락 했었는데",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000378.1.10",
          "form": "항공사 포인트 모으는 재미가 쏠쏠했는데..",
          "original_form": "항공사 포인트 모으는 재미가 쏠쏠했는데..",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000378.1.11",
          "form": "이젠 나갈일이 없으니 뜸해졌네요",
          "original_form": "이젠 나갈일이 없으니 뜸해졌네요",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000378.1.12",
          "form": "비대면 시기라 커뮤니티는 어쩌면 더 활발해진듯도 해요",
          "original_form": "비대면 시기라 커뮤니티는 어쩌면 더 활발해진듯도 해요",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000378.1.13",
          "form": "그래요???",
          "original_form": "그래요???",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000378.1.14",
          "form": "안들어간지 꽤 된거 같아요 ㅠㅠ",
          "original_form": "안들어간지 꽤 된거 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000378.1.15",
          "form": "하긴 정모 같은 것은 싹 사라졌네요.",
          "original_form": "하긴 정모 같은 것은 싹 사라졌네요.",
          "speaker_id": "1",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000378.1.16",
          "form": "스사사 안에서 정모도 했었군요!!",
          "original_form": "스사사 안에서 정모도 했었군요!!",
          "speaker_id": "2",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000378.1.17",
          "form": "필요한 정보만 쏙쏙 했나봐요",
          "original_form": "필요한 정보만 쏙쏙 했나봐요",
          "speaker_id": "2",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000378.1.18",
          "form": "다른 커뮤에서는 줌으로 정모하고 그러기도 하던데",
          "original_form": "다른 커뮤에서는 줌으로 정모하고 그러기도 하던데",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000378.1.19",
          "form": "스사사도 그런거 하지 않을까요??",
          "original_form": "스사사도 그런거 하지 않을까요??",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000378.1.20",
          "form": "아 하긴 줌 모임 활성화 되어 있죠",
          "original_form": "아 하긴 줌 모임 활성화 되어 있죠",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000378.1.21",
          "form": "근데 정모는 꼭 뒤탈이 많이 생기더라고요.",
          "original_form": "근데 정모는 꼭 뒤탈이 많이 생기더라고요.",
          "speaker_id": "1",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000378.1.22",
          "form": "그래요??",
          "original_form": "그래요??",
          "speaker_id": "2",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000378.1.23",
          "form": "정모는 한번도 가본적이 없어서.",
          "original_form": "정모는 한번도 가본적이 없어서.",
