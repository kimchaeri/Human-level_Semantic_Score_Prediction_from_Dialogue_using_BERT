        },
        {
          "id": "MDRW2100038263.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038263.1.3",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100038263.1.4",
          "form": "네ㅎㅎ 먹고 설거지까지 끝냈어요",
          "original_form": "네ㅎㅎ 먹고 설거지까지 끝냈어요",
          "speaker_id": "2",
          "time": "20210902 14:48"
        },
        {
          "id": "MDRW2100038263.1.5",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100038263.1.6",
          "form": "어제 점심과 비슷하게 먹었어요\r\n반찬있던거+계란후라이요",
          "original_form": "어제 점심과 비슷하게 먹었어요\r\n반찬있던거+계란후라이요",
          "speaker_id": "2",
          "time": "20210902 14:57"
        },
        {
          "id": "MDRW2100038263.1.7",
          "form": "오 저도 오늘 김치볶음밥에 계란후라이 먹엇죠ㅎㅎ",
          "original_form": "오 저도 오늘 김치볶음밥에 계란후라이 먹엇죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100038263.1.8",
          "form": "오 직접 요리하셨나요?",
          "original_form": "오 직접 요리하셨나요?",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100038263.1.9",
          "form": "아니요 만들어준거 먹었어요",
          "original_form": "아니요 만들어준거 먹었어요",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100038263.1.10",
          "form": "저 당분간 요리 안하려고요ㅠ",
          "original_form": "저 당분간 요리 안하려고요ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100038263.1.11",
          "form": "왜 안하시려구요?",
          "original_form": "왜 안하시려구요?",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038263.1.12",
          "form": "제가 안남미를 구입해서 냄비밥을 첨 해봤죠ㅜ 망해서 타격이 커요ㅠㅜ",
          "original_form": "제가 안남미를 구입해서 냄비밥을 첨 해봤죠ㅜ 망해서 타격이 커요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100038263.1.13",
          "form": "아 냄비밥을 도전하셨었군요!\r\n냄비밥 은근 까다롭죠ㅜㅜ",
          "original_form": "아 냄비밥을 도전하셨었군요!\r\n냄비밥 은근 까다롭죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100038263.1.14",
          "form": "안남미가 특이해선지 어렵더라고요ㅠ",
          "original_form": "안남미가 특이해선지 어렵더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:37"
        }
