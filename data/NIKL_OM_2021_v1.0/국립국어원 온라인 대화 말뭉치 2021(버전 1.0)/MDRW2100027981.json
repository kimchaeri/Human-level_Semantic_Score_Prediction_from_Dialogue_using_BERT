      "utterance": [
        {
          "id": "MDRW2100027981.1.1",
          "form": "대단은요",
          "original_form": "대단은요",
          "speaker_id": "1",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100027981.1.2",
          "form": "늘상 집에서 하는 일",
          "original_form": "늘상 집에서 하는 일",
          "speaker_id": "1",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100027981.1.3",
          "form": "장소만 바뀌는 거죠",
          "original_form": "장소만 바뀌는 거죠",
          "speaker_id": "1",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100027981.1.4",
          "form": "그래도요",
          "original_form": "그래도요",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027981.1.5",
          "form": "그렇게 말해 주니 참 고맙네요",
          "original_form": "그렇게 말해 주니 참 고맙네요",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027981.1.6",
          "form": "세상의 모든 어머님들은 참 대단하신 분 같아요",
          "original_form": "세상의 모든 어머님들은 참 대단하신 분 같아요",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027981.1.7",
          "form": "집안일이라는게",
          "original_form": "집안일이라는게",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027981.1.8",
          "form": "이렇게 누가 인정해주면 신명나서 할 수 있는데",
          "original_form": "이렇게 누가 인정해주면 신명나서 할 수 있는데",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027981.1.9",
          "form": "하찮게 보는 사람들도 있어서",
          "original_form": "하찮게 보는 사람들도 있어서",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100027981.1.10",
          "form": "아니 그런 막되먹은 사람들이 있다니 누가 그러나요",
          "original_form": "아니 그런 막되먹은 사람들이 있다니 누가 그러나요",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027981.1.11",
          "form": "가사의 가치를 모르다니",
          "original_form": "가사의 가치를 모르다니",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027981.1.12",
          "form": "집안일과 육아 힘들어서 비혼 독신 고집하는 사람도 많잖아요",
          "original_form": "집안일과 육아 힘들어서 비혼 독신 고집하는 사람도 많잖아요",
          "speaker_id": "2",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027981.1.13",
          "form": "요즘에 시대가 조금 바뀐 거 같기는 해요",
          "original_form": "요즘에 시대가 조금 바뀐 거 같기는 해요",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027981.1.14",
          "form": "옛날에는 집에서 밥 하는 가정주부",
          "original_form": "옛날에는 집에서 밥 하는 가정주부",
          "speaker_id": "1",
          "time": "20210902 16:38"
        },
        {
          "id": "MDRW2100027981.1.15",
          "form": "설 자리가 정말 없었어요",
          "original_form": "설 자리가 정말 없었어요",
          "speaker_id": "1",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027981.1.16",
          "form": "맞아요 저희 할머니 부모님 세대에",
          "original_form": "맞아요 저희 할머니 부모님 세대에",
          "speaker_id": "2",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027981.1.17",
          "form": "그런 일이 많았던 것 같아요",
          "original_form": "그런 일이 많았던 것 같아요",
          "speaker_id": "2",
          "time": "20210902 16:39"
        },
        {
          "id": "MDRW2100027981.1.18",
          "form": "어찌 보면 가정생활을 책임지는 아주 아주 중요한 일인데",
          "original_form": "어찌 보면 가정생활을 책임지는 아주 아주 중요한 일인데",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027981.1.19",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027981.1.20",
          "form": "다들 홀대 했죠",
          "original_form": "다들 홀대 했죠",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100027981.1.21",
          "form": "전업주부 가사노동의 가치가 그래도 요즘은 조금 제대로 알려지는 거 같아 다행이에요",
          "original_form": "전업주부 가사노동의 가치가 그래도 요즘은 조금 제대로 알려지는 거 같아 다행이에요",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100027981.1.22",
          "form": "사회 분위기도 분위기지만 시댁이나 남편을 누구를 만나는지도 참 중요해요",
          "original_form": "사회 분위기도 분위기지만 시댁이나 남편을 누구를 만나는지도 참 중요해요",
          "speaker_id": "1",
          "time": "20210902 16:49"
        }
      ]
