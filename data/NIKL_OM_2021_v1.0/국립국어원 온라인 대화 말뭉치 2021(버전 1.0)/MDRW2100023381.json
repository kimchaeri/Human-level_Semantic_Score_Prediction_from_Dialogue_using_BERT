      "utterance": [
        {
          "id": "MDRW2100023381.1.1",
          "form": "아뇨 안먹어봤는데 어떻게 나와요??",
          "original_form": "아뇨 안먹어봤는데 어떻게 나와요??",
          "speaker_id": "2",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023381.1.2",
          "form": "새우도 튀겨주나요?",
          "original_form": "새우도 튀겨주나요?",
          "speaker_id": "2",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023381.1.3",
          "form": "네네 새우 튀긴거랑 같이 버무려서 나와용",
          "original_form": "네네 새우 튀긴거랑 같이 버무려서 나와용",
          "speaker_id": "1",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023381.1.4",
          "form": "약간 깐풍기 스타일인데",
          "original_form": "약간 깐풍기 스타일인데",
          "speaker_id": "1",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023381.1.5",
          "form": "맛있어요!",
          "original_form": "맛있어요!",
          "speaker_id": "1",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023381.1.6",
          "form": "추천드려요ㅎㅎ",
          "original_form": "추천드려요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023381.1.7",
          "form": "오~~ 양념스타일이군요 궁금하당",
          "original_form": "오~~ 양념스타일이군요 궁금하당",
          "speaker_id": "2",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023381.1.8",
          "form": "넹넹ㅎㅎ",
          "original_form": "넹넹ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100023381.1.9",
          "form": "새우 껍질은 까져있나요",
          "original_form": "새우 껍질은 까져있나요",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023381.1.10",
          "form": "네네 칵테일새우인것 같아여",
          "original_form": "네네 칵테일새우인것 같아여",
          "speaker_id": "1",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023381.1.11",
          "form": "항상 먹을때 그거 까는게 참 귀찮아서 ㅠㅠ",
          "original_form": "항상 먹을때 그거 까는게 참 귀찮아서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023381.1.12",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023381.1.13",
          "form": "아 맞아요ㅋㅋㅋㅋ 새우까는게 일이죠",
          "original_form": "아 맞아요ㅋㅋㅋㅋ 새우까는게 일이죠",
          "speaker_id": "1",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023381.1.14",
          "form": "오오 감사합니당 한번 먹어볼게요 브랜드가",
          "original_form": "오오 감사합니당 한번 먹어볼게요 브랜드가",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023381.1.15",
          "form": "어디라고 하셨죠?",
          "original_form": "어디라고 하셨죠?",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023381.1.16",
          "form": "치킨매니아에용!",
          "original_form": "치킨매니아에용!",
          "speaker_id": "1",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023381.1.17",
          "form": "오옷 참고할게요ㅎㅎㅎ",
          "original_form": "오옷 참고할게요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100023381.1.18",
          "form": "카페는 자주 가세요?",
          "original_form": "카페는 자주 가세요?",
          "speaker_id": "2",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100023381.1.19",
          "form": "저",
          "original_form": "저",
          "speaker_id": "1",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100023381.1.20",
          "form": "최근에 스타벅스",
          "original_form": "최근에 스타벅스",
          "speaker_id": "1",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100023381.1.21",
          "form": "미니 시리즈?",
          "original_form": "미니 시리즈?",
          "speaker_id": "1",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100023381.1.22",
          "form": "그거 먹으러 갔었어요!",
          "original_form": "그거 먹으러 갔었어요!",
          "speaker_id": "1",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100023381.1.23",
          "form": "미니 시리즈? 어떻게 나오는 거예요?",
          "original_form": "미니 시리즈? 어떻게 나오는 거예요?",
          "speaker_id": "2",
          "time": "20210830 01:17"
        },
        {
          "id": "MDRW2100023381.1.24",
          "form": "자동차 미니쿠퍼 아세용?",
          "original_form": "자동차 미니쿠퍼 아세용?",
          "speaker_id": "1",
          "time": "20210830 01:18"
        }
      ]
