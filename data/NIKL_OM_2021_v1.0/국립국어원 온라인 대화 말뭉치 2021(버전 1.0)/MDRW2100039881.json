      "utterance": [
        {
          "id": "MDRW2100039881.1.1",
          "form": "하긴 그림그릴때도 코디를 더 신경쓰는 사람이니까",
          "original_form": "하긴 그림그릴때도 코디를 더 신경쓰는 사람이니까",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039881.1.2",
          "form": "생각해보면 너 어릴 적부터",
          "original_form": "생각해보면 너 어릴 적부터",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039881.1.3",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039881.1.4",
          "form": "내가 듬성듬성 그려댈때",
          "original_form": "내가 듬성듬성 그려댈때",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039881.1.5",
          "form": "넌 디테일 하나까지 다 디자인하곤 했잖니",
          "original_form": "넌 디테일 하나까지 다 디자인하곤 했잖니",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039881.1.6",
          "form": "너만의 색깔이 있었어",
          "original_form": "너만의 색깔이 있었어",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039881.1.7",
          "form": "난 내 옷 대충 입는다고",
          "original_form": "난 내 옷 대충 입는다고",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039881.1.8",
          "form": "코르셋은 아니지만",
          "original_form": "코르셋은 아니지만",
          "speaker_id": "2",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039881.1.9",
          "form": "그림까지 대충 입힘",
          "original_form": "그림까지 대충 입힘",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039881.1.10",
          "form": "날 표현하는 방식중에 하나라고 생각하니까",
          "original_form": "날 표현하는 방식중에 하나라고 생각하니까",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039881.1.11",
          "form": "옷을 잘입는게 좋아졌어",
          "original_form": "옷을 잘입는게 좋아졌어",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039881.1.12",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039881.1.13",
          "form": "왠지 찡함 자아 성장기 보는 것 같아",
          "original_form": "왠지 찡함 자아 성장기 보는 것 같아",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039881.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039881.1.15",
          "form": "나도 옷 한번 물갈이 해야 하는데",
          "original_form": "나도 옷 한번 물갈이 해야 하는데",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039881.1.16",
          "form": "교복 입을때 입던 옷이 아직도 안 헤져ㅓ",
          "original_form": "교복 입을때 입던 옷이 아직도 안 헤져ㅓ",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039881.1.17",
          "form": "안 헤져서",
          "original_form": "안 헤져서",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039881.1.18",
          "form": "고딩때 스타일 그대로 박제돼버렸어",
          "original_form": "고딩때 스타일 그대로 박제돼버렸어",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039881.1.19",
          "form": "ㅋㅋㅋㄱㅋㄱㅋㄱㅋ근데 나 유튜버중에",
          "original_form": "ㅋㅋㅋㄱㅋㄱㅋㄱㅋ근데 나 유튜버중에",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039881.1.20",
          "form": "밀라노나 아니",
          "original_form": "밀라노나 아니",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039881.1.21",
          "form": "그분이 20년된? 옷들 계속 입는거보고",
          "original_form": "그분이 20년된? 옷들 계속 입는거보고",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039881.1.22",
          "form": "응 완전 멋진 분",
          "original_form": "응 완전 멋진 분",
          "speaker_id": "1",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039881.1.23",
          "form": "오래된 옷은 버리라던가",
          "original_form": "오래된 옷은 버리라던가",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039881.1.24",
          "form": "패스트패션이라던가",
          "original_form": "패스트패션이라던가",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039881.1.25",
          "form": "그런걸 지양하려고",
          "original_form": "그런걸 지양하려고",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039881.1.26",
          "form": "헐 진짜? 그분도 물건에 애정이 깊은가봐",
          "original_form": "헐 진짜? 그분도 물건에 애정이 깊은가봐",
          "speaker_id": "1",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039881.1.27",
          "form": "최대한 꾸역꾸역 내가 오래잘 관리해서",
          "original_form": "최대한 꾸역꾸역 내가 오래잘 관리해서",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039881.1.28",
          "form": "입으려고",
          "original_form": "입으려고",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039881.1.29",
          "form": "나도 내 물건 오래 쓰는 거 좋아하는디",
          "original_form": "나도 내 물건 오래 쓰는 거 좋아하는디",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039881.1.30",
          "form": "가끔 좀 너무 캐주얼한가 싶은 생각이 들 때도 있어",
          "original_form": "가끔 좀 너무 캐주얼한가 싶은 생각이 들 때도 있어",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039881.1.31",
          "form": "옷 사려면 또 골라야하고 돈들고 멀쩡한 옷도 버려지잖아",
          "original_form": "옷 사려면 또 골라야하고 돈들고 멀쩡한 옷도 버려지잖아",
          "speaker_id": "1",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039881.1.32",
          "form": "맞어맞어",
          "original_form": "맞어맞어",
          "speaker_id": "2",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039881.1.33",
          "form": "그건 싫어",
          "original_form": "그건 싫어",
          "speaker_id": "2",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039881.1.34",
          "form": "아오 자꾸 나쁜말이래",
          "original_form": "아오 자꾸 나쁜말이래",
          "speaker_id": "1",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039881.1.35",
          "form": "캐주얼하지만 그냥 입을까",
          "original_form": "캐주얼하지만 그냥 입을까",
          "speaker_id": "1",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039881.1.36",
          "form": "까짓꺼 내 색깔이라고 주장하면",
          "original_form": "까짓꺼 내 색깔이라고 주장하면",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039881.1.37",
          "form": "되겠거니 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "되겠거니 ㅋㅋㅋㅋㅋㅋ",
