      "utterance": [
        {
          "id": "MDRW2100009080.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100009080.1.2",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "1",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100009080.1.3",
          "form": "네 직장인이에요^^",
          "original_form": "네 직장인이에요^^",
          "speaker_id": "2",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100009080.1.4",
          "form": "내일은 출근하시나요?ㅠㅠ",
          "original_form": "내일은 출근하시나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100009080.1.5",
          "form": "벌써 출근이라니......ㅎ.ㅏ.......",
          "original_form": "벌써 출근이라니......ㅎ.ㅏ.......",
          "speaker_id": "1",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100009080.1.6",
          "form": "저는...",
          "original_form": "저는...",
          "speaker_id": "2",
          "time": "20210822 21:09"
        },
        {
          "id": "MDRW2100009080.1.7",
          "form": "출근이 나오요",
          "original_form": "출근이 나오요",
          "speaker_id": "2",
          "time": "20210822 21:09"
        },
        {
          "id": "MDRW2100009080.1.8",
          "form": "나요",
          "original_form": "나요",
          "speaker_id": "2",
          "time": "20210822 21:09"
        },
        {
          "id": "MDRW2100009080.1.9",
          "form": "나아요",
          "original_form": "나아요",
          "speaker_id": "2",
          "time": "20210822 21:09"
        },
        {
          "id": "MDRW2100009080.1.10",
          "form": "헐 진짜요??",
          "original_form": "헐 진짜요??",
          "speaker_id": "1",
          "time": "20210822 21:09"
        },
        {
          "id": "MDRW2100009080.1.11",
          "form": "네....",
          "original_form": "네....",
          "speaker_id": "2",
          "time": "20210822 21:09"
        },
        {
          "id": "MDRW2100009080.1.12",
          "form": "주말에는..육아가...",
          "original_form": "주말에는..육아가...",
          "speaker_id": "2",
          "time": "20210822 21:09"
        },
        {
          "id": "MDRW2100009080.1.13",
          "form": "앗.... 그건 생각도 못했네요 제가 미혼이라ㅠㅠ",
          "original_form": "앗.... 그건 생각도 못했네요 제가 미혼이라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009080.1.14",
          "form": "아 갑자기 이해됩니다ㅠㅠ",
          "original_form": "아 갑자기 이해됩니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:10"
        },
        {
          "id": "MDRW2100009080.1.15",
          "form": "너무 예쁜데....",
          "original_form": "너무 예쁜데....",
          "speaker_id": "2",
          "time": "20210822 21:12"
        },
        {
          "id": "MDRW2100009080.1.16",
          "form": "그래도 제 시간도 필요하네요 흑흑",
          "original_form": "그래도 제 시간도 필요하네요 흑흑",
          "speaker_id": "2",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009080.1.17",
          "form": "출근은 몇시쯤 하세요?",
          "original_form": "출근은 몇시쯤 하세요?",
          "speaker_id": "1",
          "time": "20210822 21:13"
        },
        {
          "id": "MDRW2100009080.1.18",
          "form": "8시40분까지라서 8시 전에 나와요",
          "original_form": "8시40분까지라서 8시 전에 나와요",
          "speaker_id": "2",
          "time": "20210822 21:14"
        },
        {
          "id": "MDRW2100009080.1.19",
          "form": "얄루님은요???",
          "original_form": "얄루님은요???",
          "speaker_id": "2",
          "time": "20210822 21:14"
        },
        {
          "id": "MDRW2100009080.1.20",
          "form": "저는 9시까지라서 8시반에 나갑니다! 걸어서 갈 수 있는 거리거든요",
          "original_form": "저는 9시까지라서 8시반에 나갑니다! 걸어서 갈 수 있는 거리거든요",
          "speaker_id": "1",
          "time": "20210822 21:14"
        },
        {
          "id": "MDRW2100009080.1.21",
          "form": "우와 진짜 좋네요",
          "original_form": "우와 진짜 좋네요",
          "speaker_id": "2",
          "time": "20210822 21:15"
        },
        {
          "id": "MDRW2100009080.1.22",
          "form": "가까운게 제일 좋은 것 같아요",
          "original_form": "가까운게 제일 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210822 21:15"
        },
        {
          "id": "MDRW2100009080.1.23",
          "form": "맞아요! 교통비만 안들어도 넘 좋더라구요",
          "original_form": "맞아요! 교통비만 안들어도 넘 좋더라구요",
