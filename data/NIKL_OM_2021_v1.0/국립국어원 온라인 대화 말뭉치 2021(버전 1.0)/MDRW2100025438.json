      "utterance": [
        {
          "id": "MDRW2100025438.1.1",
          "form": "맛난거먹고싶어요",
          "original_form": "맛난거먹고싶어요",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100025438.1.2",
          "form": "가방샀어요",
          "original_form": "가방샀어요",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100025438.1.3",
          "form": "샴푸도샀어요",
          "original_form": "샴푸도샀어요",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100025438.1.4",
          "form": "큰가요?",
          "original_form": "큰가요?",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100025438.1.5",
          "form": "많이요",
          "original_form": "많이요",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100025438.1.6",
          "form": "겁나커요",
          "original_form": "겁나커요",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100025438.1.7",
          "form": "가방",
          "original_form": "가방",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100025438.1.8",
          "form": "사람도 들어가요?",
          "original_form": "사람도 들어가요?",
          "speaker_id": "2",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100025438.1.9",
          "form": "고앙이는들어갈 크기에요",
          "original_form": "고앙이는들어갈 크기에요",
          "speaker_id": "1",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100025438.1.10",
          "form": "삼푸는탈모샴푸에요",
          "original_form": "삼푸는탈모샴푸에요",
          "speaker_id": "1",
          "time": "20210902 10:59"
        },
        {
          "id": "MDRW2100025438.1.11",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100025438.1.12",
          "form": "사고 싶었는데",
          "original_form": "사고 싶었는데",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100025438.1.13",
          "form": "좋던가요?",
          "original_form": "좋던가요?",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100025438.1.14",
          "form": "배송료만내서",
          "original_form": "배송료만내서",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100025438.1.15",
          "form": "쓸만해요",
          "original_form": "쓸만해요",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100025438.1.16",
          "form": "향은모르겠어요",
          "original_form": "향은모르겠어요",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100025438.1.17",
          "form": "할인이짱이죠",
          "original_form": "할인이짱이죠",
          "speaker_id": "1",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100025438.1.18",
          "form": "대박 좋네요",
          "original_form": "대박 좋네요",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100025438.1.19",
          "form": "순발력이 필요하군요",
          "original_form": "순발력이 필요하군요",
          "speaker_id": "2",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100025438.1.20",
          "form": "만원할인쿠폰",
          "original_form": "만원할인쿠폰",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100025438.1.21",
          "form": "받으셨나요",
          "original_form": "받으셨나요",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100025438.1.22",
          "form": "빨리마감이되어서",
          "original_form": "빨리마감이되어서",
          "speaker_id": "1",
          "time": "20210902 11:02"
        },
        {
          "id": "MDRW2100025438.1.23",
          "form": "ㅠ전 핫도그 11개",
          "original_form": "ㅠ전 핫도그 11개",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100025438.1.24",
          "form": "1000원주고 샀어요",
          "original_form": "1000원주고 샀어요",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100025438.1.25",
          "form": "살게 안보여서ㅜㅜ",
          "original_form": "살게 안보여서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100025438.1.26",
          "form": "다시 찾아볼게요!",
          "original_form": "다시 찾아볼게요!",
          "speaker_id": "2",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100025438.1.27",
          "form": "흔한핫도그?",
          "original_form": "흔한핫도그?",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100025438.1.28",
          "form": "흔한이라는 이름이 들어갔는데",
          "original_form": "흔한이라는 이름이 들어갔는데",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100025438.1.29",
          "form": "2만원부터무배라",
          "original_form": "2만원부터무배라",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100025438.1.30",
          "form": "11개시켜야 무배적용돼서",
          "original_form": "11개시켜야 무배적용돼서",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100025438.1.31",
          "form": "1000원결제됩니다",
          "original_form": "1000원결제됩니다",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100025438.1.32",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100025438.1.33",
          "form": "쇼핑불발",
          "original_form": "쇼핑불발",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100025438.1.34",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100025438.1.35",
