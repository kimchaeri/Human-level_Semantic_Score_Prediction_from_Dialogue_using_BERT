      "utterance": [
        {
          "id": "MDRW2100038106.1.1",
          "form": "음 좀옛날인데",
          "original_form": "음 좀옛날인데",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038106.1.2",
          "form": "아 참고로 난 마켓컬리 직원은 절대아냐",
          "original_form": "아 참고로 난 마켓컬리 직원은 절대아냐",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038106.1.3",
          "form": "일단 들어볼께 어떤거야??",
          "original_form": "일단 들어볼께 어떤거야??",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038106.1.4",
          "form": "아 이미 이용한 적 있구나",
          "original_form": "아 이미 이용한 적 있구나",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038106.1.5",
          "form": "그럼 그냥 내가 제품 하나 추천만 할게",
          "original_form": "그럼 그냥 내가 제품 하나 추천만 할게",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038106.1.6",
          "form": "푸드렐라 제퓸이야",
          "original_form": "푸드렐라 제퓸이야",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038106.1.7",
          "form": "예전에 연어를 시켜먹어본적있을껄?",
          "original_form": "예전에 연어를 시켜먹어본적있을껄?",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038106.1.8",
          "form": "잡표같은데 맛있더라",
          "original_form": "잡표같은데 맛있더라",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038106.1.9",
          "form": "이름이 신데렐라를 따라한걸까",
          "original_form": "이름이 신데렐라를 따라한걸까",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100038106.1.10",
          "form": "푸드렐라의 나쵸치킨이 맛있어",
          "original_form": "푸드렐라의 나쵸치킨이 맛있어",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.11",
          "form": "그럼 치킨에서 나쵸맛이나?",
          "original_form": "그럼 치킨에서 나쵸맛이나?",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.12",
          "form": "구워먹으면 치킨너겟과",
          "original_form": "구워먹으면 치킨너겟과",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.13",
          "form": "치즈 조금",
          "original_form": "치즈 조금",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.14",
          "form": "그리고 할라피뇨향이 조금 올라와",
          "original_form": "그리고 할라피뇨향이 조금 올라와",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.15",
          "form": "와 치킨너겟 맛있겠다",
          "original_form": "와 치킨너겟 맛있겠다",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.16",
          "form": "참 맛있어",
          "original_form": "참 맛있어",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.17",
          "form": "침이 줄줄 고인다",
          "original_form": "침이 줄줄 고인다",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.18",
          "form": "생각만해도 군침돈다",
          "original_form": "생각만해도 군침돈다",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.19",
          "form": "꼭 추천할게",
          "original_form": "꼭 추천할게",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.20",
          "form": "후추를 뿌려먹으면 더 맛있을까?",
          "original_form": "후추를 뿌려먹으면 더 맛있을까?",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.21",
          "form": "근데 양이 적어 조심해.",
          "original_form": "근데 양이 적어 조심해.",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.22",
          "form": "후추는 항상 맛있지",
          "original_form": "후추는 항상 맛있지",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.23",
          "form": "근데 소금이 든 후추라면 모르겠어.",
          "original_form": "근데 소금이 든 후추라면 모르겠어.",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.24",
          "form": "좋아 집에서 가져와야겠어",
          "original_form": "좋아 집에서 가져와야겠어",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.25",
          "form": "좀 짜니??",
          "original_form": "좀 짜니??",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038106.1.26",
          "form": "우리집은 소금없는 갈아먹는 후추도 있거든.",
          "original_form": "우리집은 소금없는 갈아먹는 후추도 있거든.",
          "speaker_id": "1",
          "time": "20210901 23:17"
        }
      ]
    }
  ]
