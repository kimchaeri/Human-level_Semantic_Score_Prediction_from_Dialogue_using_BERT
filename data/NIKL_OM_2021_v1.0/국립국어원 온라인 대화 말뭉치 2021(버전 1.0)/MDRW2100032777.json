      "utterance": [
        {
          "id": "MDRW2100032777.1.1",
          "form": "name2님 좋아하는 음식 있으신가요?",
          "original_form": "&name2&님 좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100032777.1.2",
          "form": "전 가리지않고 다 좋아해요ㅋㅋ 특히 밀가루",
          "original_form": "전 가리지않고 다 좋아해요ㅋㅋ 특히 밀가루",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100032777.1.3",
          "form": "밀가루 정말 맛있죠 저는 특히 빵을 좋아한답니다",
          "original_form": "밀가루 정말 맛있죠 저는 특히 빵을 좋아한답니다",
          "speaker_id": "1",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100032777.1.4",
          "form": "빵 진짜 안물리죠!",
          "original_form": "빵 진짜 안물리죠!",
          "speaker_id": "2",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032777.1.5",
          "form": "요즘 식사빵 좋아해요",
          "original_form": "요즘 식사빵 좋아해요",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032777.1.6",
          "form": "식사빵 중에 제일 좋아하는 거 있나요?",
          "original_form": "식사빵 중에 제일 좋아하는 거 있나요?",
          "speaker_id": "1",
          "time": "20210905 23:31"
        },
        {
          "id": "MDRW2100032777.1.7",
          "form": "치아바타 좋아해요!",
          "original_form": "치아바타 좋아해요!",
          "speaker_id": "2",
          "time": "20210906 07:55"
        },
        {
          "id": "MDRW2100032777.1.8",
          "form": "저도 치아바타 좋아해요",
          "original_form": "저도 치아바타 좋아해요",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100032777.1.9",
          "form": "올리브나 치즈 들어가면 짱맛",
          "original_form": "올리브나 치즈 들어가면 짱맛",
          "speaker_id": "2",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100032777.1.10",
          "form": "맞아요 콘치즈 들어간 치아바타를 먹었는데 정말 맛있더라고요",
          "original_form": "맞아요 콘치즈 들어간 치아바타를 먹었는데 정말 맛있더라고요",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100032777.1.11",
          "form": "저는 통밀빵과 호밀빵도 좋아해요",
          "original_form": "저는 통밀빵과 호밀빵도 좋아해요",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100032777.1.12",
          "form": "치아바타 만들어보셨어요?",
          "original_form": "치아바타 만들어보셨어요?",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100032777.1.13",
          "form": "아니요 다른 빵은 만들어봤는데 치아바타는 아직이요 name2님은요?",
          "original_form": "아니요 다른 빵은 만들어봤는데 치아바타는 아직이요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100032777.1.14",
          "form": "저 만들어봤는데 무반죽치아바타 쉬운데 4시간정도 기다려야해서 ㅋㅋ",
          "original_form": "저 만들어봤는데 무반죽치아바타 쉬운데 4시간정도 기다려야해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100032777.1.15",
          "form": "맛은 얼추 비슷해요!",
          "original_form": "맛은 얼추 비슷해요!",
          "speaker_id": "2",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100032777.1.16",
          "form": "와 무반죽 대단하세요 그 파는 빵과 비슷한 맛이 나나요?",
          "original_form": "와 무반죽 대단하세요 그 파는 빵과 비슷한 맛이 나나요?",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100032777.1.17",
          "form": "네네 비슷하더라고요!",
          "original_form": "네네 비슷하더라고요!",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100032777.1.18",
          "form": "제가 만들고 제가 먹어서 객관성은 좀 떨어지지만요",
          "original_form": "제가 만들고 제가 먹어서 객관성은 좀 떨어지지만요",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100032777.1.19",
          "form": "신기하네요 홈베이킹 자주하시나요?",
          "original_form": "신기하네요 홈베이킹 자주하시나요?",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100032777.1.20",
          "form": "근데 여름되곤 안하고있어요ㅋㅋ 더워서ㅋㅋ",
          "original_form": "근데 여름되곤 안하고있어요ㅋㅋ 더워서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100032777.1.21",
          "form": "여름에는 홈베이킹 하기 힘들죠",
          "original_form": "여름에는 홈베이킹 하기 힘들죠",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100032777.1.22",
          "form": "저는 여름에 스콘 만드는데 여름이라 버터가 녹아서 실패했던 경험이있네요",
          "original_form": "저는 여름에 스콘 만드는데 여름이라 버터가 녹아서 실패했던 경험이있네요",
          "speaker_id": "1",
          "time": "20210906 22:15"
        }
      ]
    }
