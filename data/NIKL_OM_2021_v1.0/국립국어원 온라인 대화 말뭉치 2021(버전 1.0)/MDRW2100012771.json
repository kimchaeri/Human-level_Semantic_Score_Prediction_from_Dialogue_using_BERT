      "utterance": [
        {
          "id": "MDRW2100012771.1.1",
          "form": "안녕하세요 여행하기 좋아하세요?",
          "original_form": "안녕하세요 여행하기 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 00:58"
        },
        {
          "id": "MDRW2100012771.1.2",
          "form": "전에는 꽤 다녔는데 최근에는 잘 못 갔어요",
          "original_form": "전에는 꽤 다녔는데 최근에는 잘 못 갔어요",
          "speaker_id": "2",
          "time": "20210825 01:00"
        },
        {
          "id": "MDRW2100012771.1.3",
          "form": "기억에 남는 여행지 있나요?",
          "original_form": "기억에 남는 여행지 있나요?",
          "speaker_id": "2",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100012771.1.4",
          "form": "국내는 전주가 괜찮았고 외국은 대만이 괜찮았어요",
          "original_form": "국내는 전주가 괜찮았고 외국은 대만이 괜찮았어요",
          "speaker_id": "1",
          "time": "20210825 01:10"
        },
        {
          "id": "MDRW2100012771.1.5",
          "form": "전주 좋죠! 한옥마을에서 어디 가셨어요?",
          "original_form": "전주 좋죠! 한옥마을에서 어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100012771.1.6",
          "form": "그 주변 길이랑 상가 구경했어요",
          "original_form": "그 주변 길이랑 상가 구경했어요",
          "speaker_id": "1",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100012771.1.7",
          "form": "근처에 카페 좋더라구요 성당 근처에요",
          "original_form": "근처에 카페 좋더라구요 성당 근처에요",
          "speaker_id": "1",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100012771.1.8",
          "form": "풍경 자체가 멋지죠",
          "original_form": "풍경 자체가 멋지죠",
          "speaker_id": "2",
          "time": "20210825 01:21"
        },
        {
          "id": "MDRW2100012771.1.9",
          "form": "맛집도 많고요",
          "original_form": "맛집도 많고요",
          "speaker_id": "2",
          "time": "20210825 01:21"
        },
        {
          "id": "MDRW2100012771.1.10",
          "form": "전주도 그렇고 지방도시 특유의 조용하고 정돈된 분위기가 좋아서요",
          "original_form": "전주도 그렇고 지방도시 특유의 조용하고 정돈된 분위기가 좋아서요",
          "speaker_id": "1",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100012771.1.11",
          "form": "네 청주가보셨어요?",
          "original_form": "네 청주가보셨어요?",
          "speaker_id": "1",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100012771.1.12",
          "form": "청주는 안 가 본 것 같아요. 가봤어도 어릴 때라 기억이 안 날지도",
          "original_form": "청주는 안 가 본 것 같아요. 가봤어도 어릴 때라 기억이 안 날지도",
          "speaker_id": "2",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100012771.1.13",
          "form": "화려하지는 않은데 조용조용해요",
          "original_form": "화려하지는 않은데 조용조용해요",
          "speaker_id": "1",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100012771.1.14",
          "form": "전주도 비슷하지만 관광지 분위기가 더 나는거같아요",
          "original_form": "전주도 비슷하지만 관광지 분위기가 더 나는거같아요",
          "speaker_id": "1",
          "time": "20210825 01:29"
        },
        {
          "id": "MDRW2100012771.1.15",
          "form": "청주는 뭐가 유명한가요?",
          "original_form": "청주는 뭐가 유명한가요?",
          "speaker_id": "2",
          "time": "20210825 01:30"
        },
        {
          "id": "MDRW2100012771.1.16",
          "form": "관광으로 유명한건 모르겠어요",
          "original_form": "관광으로 유명한건 모르겠어요",
          "speaker_id": "1",
          "time": "20210825 01:31"
        },
        {
          "id": "MDRW2100012771.1.17",
          "form": "청주에 공장꽤있다던데요",
          "original_form": "청주에 공장꽤있다던데요",
          "speaker_id": "1",
          "time": "20210825 01:32"
        },
        {
          "id": "MDRW2100012771.1.18",
          "form": "유명하지만 경주도 좋더라구요. 작지만 알찬 느낌",
          "original_form": "유명하지만 경주도 좋더라구요. 작지만 알찬 느낌",
          "speaker_id": "2",
          "time": "20210825 01:36"
        },
        {
          "id": "MDRW2100012771.1.19",
          "form": "네 경주도 좋아요",
          "original_form": "네 경주도 좋아요",
          "speaker_id": "1",
          "time": "20210825 01:39"
        }
      ]
