      "utterance": [
        {
          "id": "MDRW2100008044.1.1",
          "form": "여행가고싶어",
          "original_form": "여행가고싶어",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008044.1.2",
          "form": "그 좀 활발하게 움직이는 여행 말고",
          "original_form": "그 좀 활발하게 움직이는 여행 말고",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008044.1.3",
          "form": "온천이나 호텔에서 휴양하는 여행말이야",
          "original_form": "온천이나 호텔에서 휴양하는 여행말이야",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008044.1.4",
          "form": "아 온천 그런거",
          "original_form": "아 온천 그런거",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008044.1.5",
          "form": "저도 짱좋아해요",
          "original_form": "저도 짱좋아해요",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008044.1.6",
          "form": "담에 저랑 온천 가실???",
          "original_form": "담에 저랑 온천 가실???",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008044.1.7",
          "form": "코로나 잠잠해지면 바로 고다",
          "original_form": "코로나 잠잠해지면 바로 고다",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100008044.1.8",
          "form": "해외까지 나가는건 넘 부담스럽고",
          "original_form": "해외까지 나가는건 넘 부담스럽고",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008044.1.9",
          "form": "국내에 온천 유명한곳 어디 있나? 부산에 하나 있는건 아는데",
          "original_form": "국내에 온천 유명한곳 어디 있나? 부산에 하나 있는건 아는데",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100008044.1.10",
          "form": "온천하면",
          "original_form": "온천하면",
          "speaker_id": "2",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100008044.1.11",
          "form": "아산쪽에 온양온천이 유명하지않아요???",
          "original_form": "아산쪽에 온양온천이 유명하지않아요???",
          "speaker_id": "2",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100008044.1.12",
          "form": "는 제가 아는 온천이 거기뿐 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "는 제가 아는 온천이 거기뿐 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100008044.1.13",
          "form": "오 국내 여행지 잘 몰랐는데",
          "original_form": "오 국내 여행지 잘 몰랐는데",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100008044.1.14",
          "form": "아산 킵해둘게 ^^!",
          "original_form": "아산 킵해둘게 ^^!",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100008044.1.15",
          "form": "가서 온천>식사>온천>식사 무한반복해서 보들보들한 돼지가 되어야지",
          "original_form": "가서 온천>식사>온천>식사 무한반복해서 보들보들한 돼지가 되어야지",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008044.1.16",
          "form": "도야지좋져",
          "original_form": "도야지좋져",
          "speaker_id": "2",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008044.1.17",
          "form": "그쪽에 그 가는 길에",
          "original_form": "그쪽에 그 가는 길에",
          "speaker_id": "2",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008044.1.18",
          "form": "호두과자도 유명한 거기 지나치니까... 거기 어디더라",
          "original_form": "호두과자도 유명한 거기 지나치니까... 거기 어디더라",
          "speaker_id": "2",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008044.1.19",
          "form": "암튼 호두과자도 냠냠합시다",
          "original_form": "암튼 호두과자도 냠냠합시다",
          "speaker_id": "2",
          "time": "20210822 13:05"
        },
        {
          "id": "MDRW2100008044.1.20",
          "form": "이왕 가는거 일정 잘 잡아서",
          "original_form": "이왕 가는거 일정 잘 잡아서",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008044.1.21",
          "form": "좀 길게 다녀오고 싶다",
          "original_form": "좀 길게 다녀오고 싶다",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008044.1.22",
          "form": "전국 온천순회 요런걸루 ㅋㅋㅋ",
          "original_form": "전국 온천순회 요런걸루 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008044.1.23",
          "form": "좋죠좋죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "좋죠좋죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008044.1.24",
          "form": "해외여행 풀리면 사실",
          "original_form": "해외여행 풀리면 사실",
          "speaker_id": "2",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008044.1.25",
          "form": "대만쪽이나 일본쪽 온천도 가보고 싶어요",
          "original_form": "대만쪽이나 일본쪽 온천도 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100008044.1.26",
          "form": "맞아맞아 국내랑 해외랑 얼마나 다른지 비교해보고 싶어",
          "original_form": "맞아맞아 국내랑 해외랑 얼마나 다른지 비교해보고 싶어",
          "speaker_id": "1",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008044.1.27",
          "form": "예전에는 배낭여행을 동경했었는데",
          "original_form": "예전에는 배낭여행을 동경했었는데",
          "speaker_id": "1",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008044.1.28",
          "form": "이제는 휴양지가 최고다 싶어 ㅋㅋㅋㅋㅋ",
          "original_form": "이제는 휴양지가 최고다 싶어 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:09"
        },
        {
          "id": "MDRW2100008044.1.29",
          "form": "배...낭...여..행...?",
          "original_form": "배...낭...여..행...?",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008044.1.30",
          "form": "전 그런 것 동경해본적 없어요",
          "original_form": "전 그런 것 동경해본적 없어요",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008044.1.31",
          "form": "돈내고 개고생하는 코스같아서... ㅎㅎ..ㅠ",
          "original_form": "돈내고 개고생하는 코스같아서... ㅎㅎ..ㅠ",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100008044.1.32",
          "form": "ㅋㅋㅋㅋ 사람 만나는거 좋아하고 활발한 타입이면",
          "original_form": "ㅋㅋㅋㅋ 사람 만나는거 좋아하고 활발한 타입이면",
          "speaker_id": "1",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008044.1.33",
          "form": "휴양지에서 굴러다니는걸 못견뎌 하더라",
          "original_form": "휴양지에서 굴러다니는걸 못견뎌 하더라",
          "speaker_id": "1",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008044.1.34",
          "form": "그치만 난 굴러다닐것이다...",
          "original_form": "그치만 난 굴러다닐것이다...",
          "speaker_id": "1",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008044.1.35",
          "form": "저도 굴러다닐 것이다...",
          "original_form": "저도 굴러다닐 것이다...",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008044.1.36",
          "form": "전 근데 한 번 여행하게되면 빡 움직이는 스타일이라",
          "original_form": "전 근데 한 번 여행하게되면 빡 움직이는 스타일이라",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008044.1.37",
          "form": "어디 유적지나 문화재탐방 이런건 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "어디 유적지나 문화재탐방 이런건 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100008044.1.38",
          "form": "앗 나두나두",
          "original_form": "앗 나두나두",
          "speaker_id": "1",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008044.1.39",
          "form": "그럼 온천 식사 관광 온천 식사 관광으로",
          "original_form": "그럼 온천 식사 관광 온천 식사 관광으로",
          "speaker_id": "1",
          "time": "20210822 13:15"
        },
        {
          "id": "MDRW2100008044.1.40",
          "form": "건강하고 반질반질한 돼지가 되어보자",
          "original_form": "건강하고 반질반질한 돼지가 되어보자",
