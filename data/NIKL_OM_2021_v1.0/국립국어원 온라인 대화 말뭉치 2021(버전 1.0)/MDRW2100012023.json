      "utterance": [
        {
          "id": "MDRW2100012023.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100012023.1.2",
          "form": "아뇨 키우시나요,",
          "original_form": "아뇨 키우시나요,",
          "speaker_id": "2",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100012023.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100012023.1.4",
          "form": "어렸을때",
          "original_form": "어렸을때",
          "speaker_id": "1",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100012023.1.5",
          "form": "닭키운적있어요",
          "original_form": "닭키운적있어요",
          "speaker_id": "1",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012023.1.6",
          "form": "와 집에서요? 주택이셔ㅆ나요?",
          "original_form": "와 집에서요? 주택이셔ㅆ나요?",
          "speaker_id": "2",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012023.1.7",
          "form": "빌라예요",
          "original_form": "빌라예요",
          "speaker_id": "1",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012023.1.8",
          "form": "가정에서 키우긴 힘들긴해요",
          "original_form": "가정에서 키우긴 힘들긴해요",
          "speaker_id": "1",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012023.1.9",
          "form": "병아리때부터 키우신건가요?",
          "original_form": "병아리때부터 키우신건가요?",
          "speaker_id": "2",
          "time": "20210824 18:44"
        },
        {
          "id": "MDRW2100012023.1.10",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "1",
          "time": "20210824 18:44"
        },
        {
          "id": "MDRW2100012023.1.11",
          "form": "초딩때 오백원주고 데려왔어요",
          "original_form": "초딩때 오백원주고 데려왔어요",
          "speaker_id": "1",
          "time": "20210824 18:45"
        },
        {
          "id": "MDRW2100012023.1.12",
          "form": "와 그런데 닭까지 키우신거군요!!",
          "original_form": "와 그런데 닭까지 키우신거군요!!",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012023.1.13",
          "form": "애지중지키웠어요",
          "original_form": "애지중지키웠어요",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012023.1.14",
          "form": "썸써미님은 동물키운적 있나요?",
          "original_form": "썸써미님은 동물키운적 있나요?",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012023.1.15",
          "form": "닭은 어디로 갔나요?",
          "original_form": "닭은 어디로 갔나요?",
          "speaker_id": "2",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012023.1.16",
          "form": "수명다해서 죽었어요ㅜ",
          "original_form": "수명다해서 죽었어요ㅜ",
          "speaker_id": "1",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012023.1.17",
          "form": "속상하셨겠어요 애정이 컸을텐데",
          "original_form": "속상하셨겠어요 애정이 컸을텐데",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012023.1.18",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210824 18:49"
        }
      ]
