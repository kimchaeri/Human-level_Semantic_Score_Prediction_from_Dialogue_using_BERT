      "utterance": [
        {
          "id": "MDRW2100035326.1.1",
          "form": "이제 순위한다 12위에 연령대가 센 언니들 이란다 ㅋㅋ",
          "original_form": "이제 순위한다 12위에 연령대가 센 언니들 이란다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100035326.1.2",
          "form": "연령이 많은 언니?  누구지?",
          "original_form": "연령이 많은 언니?  누구지?",
          "speaker_id": "1",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100035326.1.3",
          "form": "엄정화 이효리 제시 화사 ㅋㅋ 놀면뭐하니",
          "original_form": "엄정화 이효리 제시 화사 ㅋㅋ 놀면뭐하니",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100035326.1.4",
          "form": "아 그럼 아이키네",
          "original_form": "아 그럼 아이키네",
          "speaker_id": "1",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100035326.1.5",
          "form": "아이키 누군지 모르겟지만 지금 게스트로 나옴",
          "original_form": "아이키 누군지 모르겟지만 지금 게스트로 나옴",
          "speaker_id": "2",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100035326.1.6",
          "form": "안무가고 센언니 노래 안무 짰다",
          "original_form": "안무가고 센언니 노래 안무 짰다",
          "speaker_id": "1",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100035326.1.7",
          "form": "지금 공동 12위인가보다 훅팀 나옴",
          "original_form": "지금 공동 12위인가보다 훅팀 나옴",
          "speaker_id": "2",
          "time": "20210829 14:57"
        },
        {
          "id": "MDRW2100035326.1.8",
          "form": "미국에 아메리카 무슨 댄스 대회에서 4위",
          "original_form": "미국에 아메리카 무슨 댄스 대회에서 4위",
          "speaker_id": "1",
          "time": "20210829 14:57"
        },
        {
          "id": "MDRW2100035326.1.9",
          "form": "환불원정대도 계속 나오고 ㅋㅋ",
          "original_form": "환불원정대도 계속 나오고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100035326.1.10",
          "form": "근데 그 안무 좀 별로긴 하던데",
          "original_form": "근데 그 안무 좀 별로긴 하던데",
          "speaker_id": "1",
          "time": "20210829 14:59"
        },
        {
          "id": "MDRW2100035326.1.11",
          "form": "음 원 수 익 전 부 기 부",
          "original_form": "음 원 수 익 전 부 기 부",
          "speaker_id": "2",
          "time": "20210829 15:00"
        },
        {
          "id": "MDRW2100035326.1.12",
          "form": "코로나만 아님 행사 엄청 뛰었을건데",
          "original_form": "코로나만 아님 행사 엄청 뛰었을건데",
          "speaker_id": "1",
          "time": "20210829 15:01"
        },
        {
          "id": "MDRW2100035326.1.13",
          "form": "음원으로 엄청 났으니 뭐 괜찮지 싶다",
          "original_form": "음원으로 엄청 났으니 뭐 괜찮지 싶다",
