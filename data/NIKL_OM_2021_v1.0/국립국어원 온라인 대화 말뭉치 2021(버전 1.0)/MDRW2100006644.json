      "utterance": [
        {
          "id": "MDRW2100006644.1.1",
          "form": "name2 지금 배안고프세요??",
          "original_form": "&name2& 지금 배안고프세요??",
          "speaker_id": "1",
          "time": "20210908 04:53"
        },
        {
          "id": "MDRW2100006644.1.2",
          "form": "완전 배고파요ㅠㅠ",
          "original_form": "완전 배고파요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:53"
        },
        {
          "id": "MDRW2100006644.1.3",
          "form": "스벅 민초 먹고싶네요",
          "original_form": "스벅 민초 먹고싶네요",
          "speaker_id": "2",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100006644.1.4",
          "form": "민초 좋아하세요?",
          "original_form": "민초 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100006644.1.5",
          "form": "저두요..ㅠㅠ저두 뱌고파요 그건 밥 아니고 간식 아닌가요",
          "original_form": "저두요..ㅠㅠ저두 뱌고파요 그건 밥 아니고 간식 아닌가요",
          "speaker_id": "1",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100006644.1.6",
          "form": "육회도 먹고싶구...",
          "original_form": "육회도 먹고싶구...",
          "speaker_id": "2",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100006644.1.7",
          "form": "전 주면 다 잘먹어요 ㅋㅋㅋㅋ",
          "original_form": "전 주면 다 잘먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100006644.1.8",
          "form": "저 초밥 먹고싶어요",
          "original_form": "저 초밥 먹고싶어요",
          "speaker_id": "1",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100006644.1.9",
          "form": "저도요ㅋㅋㅋ완전 먹보",
          "original_form": "저도요ㅋㅋㅋ완전 먹보",
          "speaker_id": "2",
          "time": "20210908 04:54"
        },
        {
          "id": "MDRW2100006644.1.10",
          "form": "허어얼 진짜 땡긴다 갑자기ㅠㅠ",
          "original_form": "허어얼 진짜 땡긴다 갑자기ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100006644.1.11",
          "form": "연어 먹고싶어지네요ㅠㅠ 참치랑...",
          "original_form": "연어 먹고싶어지네요ㅠㅠ 참치랑...",
          "speaker_id": "2",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100006644.1.12",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100006644.1.13",
          "form": "이시간에 깨어있는게 죄죠",
          "original_form": "이시간에 깨어있는게 죄죠",
          "speaker_id": "1",
          "time": "20210908 04:55"
        },
        {
          "id": "MDRW2100006644.1.14",
          "form": "아침에 뭐실거에요?",
          "original_form": "아침에 뭐실거에요?",
          "speaker_id": "1",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100006644.1.15",
          "form": "그러게요...ㅋㅋㅠㅠㅠ",
          "original_form": "그러게요...ㅋㅋㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100006644.1.16",
          "form": "쌀국수도 목고싶어요",
          "original_form": "쌀국수도 목고싶어요",
          "speaker_id": "1",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100006644.1.17",
          "form": "음 시리얼 먹고...뒤늦게 잘 것 같아요ㅋㅋㅋ",
          "original_form": "음 시리얼 먹고...뒤늦게 잘 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100006644.1.18",
          "form": "잔치국수도요ㅠㅠㅠ",
          "original_form": "잔치국수도요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100006644.1.19",
          "form": "피자도 갑자기 먹고싶다",
          "original_form": "피자도 갑자기 먹고싶다",
          "speaker_id": "2",
          "time": "20210908 04:56"
        },
        {
          "id": "MDRW2100006644.1.20",
          "form": "파파존스 치즈스틱 드셔보셨어요? 존맛이에요",
          "original_form": "파파존스 치즈스틱 드셔보셨어요? 존맛이에요",
          "speaker_id": "2",
          "time": "20210908 04:57"
        },
        {
          "id": "MDRW2100006644.1.21",
          "form": "파파존스 치즈스틱 안먹어보ㅓㅆ엉ㅅ",
          "original_form": "파파존스 치즈스틱 안먹어보ㅓㅆ엉ㅅ",
          "speaker_id": "1",
          "time": "20210908 04:57"
        },
        {
          "id": "MDRW2100006644.1.22",
          "form": "안먹어봤어요",
          "original_form": "안먹어봤어요",
          "speaker_id": "1",
          "time": "20210908 04:57"
        },
        {
          "id": "MDRW2100006644.1.23",
          "form": "피자만 먹어봤는데",
          "original_form": "피자만 먹어봤는데",
          "speaker_id": "1",
          "time": "20210908 04:58"
        },
        {
          "id": "MDRW2100006644.1.24",
          "form": "뭐 다른데랑 다른가용?",
          "original_form": "뭐 다른데랑 다른가용?",
          "speaker_id": "1",
          "time": "20210908 04:58"
        },
        {
          "id": "MDRW2100006644.1.25",
          "form": "피자같은 도우에 치즈가 두껍게 올라가 있어요!",
          "original_form": "피자같은 도우에 치즈가 두껍게 올라가 있어요!",
          "speaker_id": "2",
          "time": "20210908 04:58"
        },
        {
          "id": "MDRW2100006644.1.26",
          "form": "치즈피자같은 느낌인데 왕맛있어요",
          "original_form": "치즈피자같은 느낌인데 왕맛있어요",
          "speaker_id": "2",
          "time": "20210908 04:58"
        },
        {
          "id": "MDRW2100006644.1.27",
          "form": "아악 배에서 소리나요 ㅋㅋㅋ",
          "original_form": "아악 배에서 소리나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:59"
        },
        {
          "id": "MDRW2100006644.1.28",
          "form": "떡볶이 좋아하세요?",
          "original_form": "떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 04:59"
        }
      ]
    }
  ]
