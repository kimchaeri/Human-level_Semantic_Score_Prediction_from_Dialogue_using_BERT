      "utterance": [
        {
          "id": "MDRW2100034683.1.1",
          "form": "오 드디어!!!",
          "original_form": "오 드디어!!!",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034683.1.2",
          "form": "어떤사람?",
          "original_form": "어떤사람?",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034683.1.3",
          "form": "하나씩 물어봐여 ㅋㅋ",
          "original_form": "하나씩 물어봐여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034683.1.4",
          "form": "나이",
          "original_form": "나이",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100034683.1.5",
          "form": "동갑이지만 내동생이 빠른91이라서 90년생 아직 존댓말써준대요 동생이 ㅋㅋ",
          "original_form": "동갑이지만 내동생이 빠른91이라서 90년생 아직 존댓말써준대요 동생이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100034683.1.6",
          "form": "아유 동갑이네 머하는사람?",
          "original_form": "아유 동갑이네 머하는사람?",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100034683.1.7",
          "form": "한전다닌대여",
          "original_form": "한전다닌대여",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100034683.1.8",
          "form": "공기업인가?",
          "original_form": "공기업인가?",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100034683.1.9",
          "form": "부산에 한전이있는가 흠",
          "original_form": "부산에 한전이있는가 흠",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100034683.1.10",
          "form": "오 좋네",
          "original_form": "오 좋네",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100034683.1.11",
          "form": "성격은어떰",
          "original_form": "성격은어떰",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100034683.1.12",
          "form": "지말 잘듣는다나",
          "original_form": "지말 잘듣는다나",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100034683.1.13",
          "form": "착하네",
          "original_form": "착하네",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034683.1.14",
          "form": "전남친이랑 정반대",
          "original_form": "전남친이랑 정반대",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034683.1.15",
          "form": "어케만남",
          "original_form": "어케만남",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034683.1.16",
          "form": "ㅇㅇ착하다고일단",
          "original_form": "ㅇㅇ착하다고일단",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034683.1.17",
          "form": "전남친은 나이가많았제ㅜ",
          "original_form": "전남친은 나이가많았제ㅜ",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034683.1.18",
          "form": "소개팅앱 ㅋㅋ",
          "original_form": "소개팅앱 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100034683.1.19",
          "form": "요즘 스똬일 ㅋㅋ",
          "original_form": "요즘 스똬일 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
