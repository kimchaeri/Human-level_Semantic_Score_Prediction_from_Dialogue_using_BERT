      "utterance": [
        {
          "id": "MDRW2100043224.1.1",
          "form": "와플은 자주 해드셨었어요??",
          "original_form": "와플은 자주 해드셨었어요??",
          "speaker_id": "2",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043224.1.2",
          "form": "족발얘기했었죠??",
          "original_form": "족발얘기했었죠??",
          "speaker_id": "1",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043224.1.3",
          "form": "네 족발 ㅋㅋㅋ 붕어빵이요",
          "original_form": "네 족발 ㅋㅋㅋ 붕어빵이요",
          "speaker_id": "2",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100043224.1.4",
          "form": "혹시 모르니까 보내놔야겠네요",
          "original_form": "혹시 모르니까 보내놔야겠네요",
          "speaker_id": "2",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100043224.1.5",
          "form": "와플 얘기도 했었어요!",
          "original_form": "와플 얘기도 했었어요!",
          "speaker_id": "2",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100043224.1.6",
          "form": "아 붕어빵..ㅋㅋㅋ",
          "original_form": "아 붕어빵..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100043224.1.7",
          "form": "와플팬 있으시다는 얘기를 했거덩옄ㅋㅋㅋ",
          "original_form": "와플팬 있으시다는 얘기를 했거덩옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100043224.1.8",
          "form": "또 붕어빵의계절이 왔으니 ㅠ 돈털릴준비를",
          "original_form": "또 붕어빵의계절이 왔으니 ㅠ 돈털릴준비를",
          "speaker_id": "1",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100043224.1.9",
          "form": "저는 준비 됐어요",
          "original_form": "저는 준비 됐어요",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043224.1.10",
          "form": "초딩애가 맨날 집앞에 붕어빵 사먹게 천원만 천원만 ㅎㅎ",
          "original_form": "초딩애가 맨날 집앞에 붕어빵 사먹게 천원만 천원만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043224.1.11",
          "form": "마음속에 삼천원 품고 갑니다",
          "original_form": "마음속에 삼천원 품고 갑니다",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043224.1.12",
          "form": "근데 말이 천원이지 최소3개는 먹어줘야되는데",
          "original_form": "근데 말이 천원이지 최소3개는 먹어줘야되는데",
          "speaker_id": "1",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043224.1.13",
          "form": "ㅋㅋㅋ 천원이면 두개밖에 못먹을수 있다니 슬픕니다",
          "original_form": "ㅋㅋㅋ 천원이면 두개밖에 못먹을수 있다니 슬픕니다",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043224.1.14",
          "form": "그것도 동네마다 다르던데요",
          "original_form": "그것도 동네마다 다르던데요",
          "speaker_id": "1",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043224.1.15",
          "form": "그러니까요 삼천원은 가지고 다녀야해요",
          "original_form": "그러니까요 삼천원은 가지고 다녀야해요",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100043224.1.16",
          "form": "비싼동네는 팥도  두개",
          "original_form": "비싼동네는 팥도  두개",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043224.1.17",
          "form": "여기는 그나마 3개였는데 올해는 어쩔라나",
          "original_form": "여기는 그나마 3개였는데 올해는 어쩔라나",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043224.1.18",
          "form": "저희동네도 작년에 세마리됐어요 올해 시세는",
          "original_form": "저희동네도 작년에 세마리됐어요 올해 시세는",
          "speaker_id": "2",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043224.1.19",
          "form": "어떻게 되려는지 모르겠네요",
          "original_form": "어떻게 되려는지 모르겠네요",
          "speaker_id": "2",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043224.1.20",
          "form": "아니. 회도아니고 ㅎㅎ 시세",
          "original_form": "아니. 회도아니고 ㅎㅎ 시세",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043224.1.21",
          "form": "웃기당 ㅋㅋㅋ",
          "original_form": "웃기당 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043224.1.22",
          "form": "횟집은 싯가..ㅋㅋㅋ",
          "original_form": "횟집은 싯가..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043224.1.23",
          "form": "ㅋㅋㅋ 붕어빵 타이쿤",
          "original_form": "ㅋㅋㅋ 붕어빵 타이쿤",
          "speaker_id": "2",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043224.1.24",
          "form": "같은 느낌이에요",
          "original_form": "같은 느낌이에요",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043224.1.25",
          "form": "와 옛날에",
          "original_form": "와 옛날에",
          "speaker_id": "1",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043224.1.26",
          "form": "진짜 마니구었는데",
          "original_form": "진짜 마니구었는데",
          "speaker_id": "1",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043224.1.27",
          "form": "해보셨나요",
          "original_form": "해보셨나요",
          "speaker_id": "1",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043224.1.28",
          "form": "와 핸드폰을 진짜 마니 했는데",
          "original_form": "와 핸드폰을 진짜 마니 했는데",
          "speaker_id": "1",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043224.1.29",
          "form": "그시절 그게임 진짜 많이 했졐ㅋㅋㅋ",
