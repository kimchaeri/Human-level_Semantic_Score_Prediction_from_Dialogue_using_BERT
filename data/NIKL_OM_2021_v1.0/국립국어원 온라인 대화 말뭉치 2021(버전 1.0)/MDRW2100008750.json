      "utterance": [
        {
          "id": "MDRW2100008750.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008750.1.2",
          "form": "좋아하지는 않아도 하려고는해요ㅜ",
          "original_form": "좋아하지는 않아도 하려고는해요ㅜ",
          "speaker_id": "2",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100008750.1.3",
          "form": "운동 좋아하는 사람이되고파요",
          "original_form": "운동 좋아하는 사람이되고파요",
          "speaker_id": "2",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008750.1.4",
          "form": "다이어트 해야하는데 맨날 내일부터 이러고있어요",
          "original_form": "다이어트 해야하는데 맨날 내일부터 이러고있어요",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008750.1.5",
          "form": "따로 운동 하시나여",
          "original_form": "따로 운동 하시나여",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008750.1.6",
          "form": "걷기만 합니다ㅎㅎ",
          "original_form": "걷기만 합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008750.1.7",
          "form": "오 저도 요새 꾸준히 걸어요",
          "original_form": "오 저도 요새 꾸준히 걸어요",
          "speaker_id": "2",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008750.1.8",
          "form": "1만보 이상 2만보도 많이걷고요",
          "original_form": "1만보 이상 2만보도 많이걷고요",
          "speaker_id": "2",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008750.1.9",
          "form": "1만보만 꾸준히 해도 ㅎㅎ",
          "original_form": "1만보만 꾸준히 해도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100008750.1.10",
          "form": "근데 그만큼 입맛돌아서 잘먹어요ㅜㅜㅜ",
          "original_form": "근데 그만큼 입맛돌아서 잘먹어요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 21:24"
        },
        {
          "id": "MDRW2100008750.1.11",
          "form": "건강하면 된거죠",
          "original_form": "건강하면 된거죠",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100008750.1.12",
          "form": "어휴 그래도 너무 확찐자가 되서 슬퍼요ㅜ",
          "original_form": "어휴 그래도 너무 확찐자가 되서 슬퍼요ㅜ",
          "speaker_id": "2",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100008750.1.13",
          "form": "ㅋㅋ 나중에 빼면 되죠!",
          "original_form": "ㅋㅋ 나중에 빼면 되죠!",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100008750.1.14",
          "form": "나이들수록 빼기 힘든거 같아요 ㅋㅋ",
          "original_form": "나이들수록 빼기 힘든거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100008750.1.15",
          "form": "ㅋㅋ 때되면 될겁니다",
          "original_form": "ㅋㅋ 때되면 될겁니다",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100008750.1.16",
          "form": "필라테스같은거 해보고싶긴 하더라고요",
          "original_form": "필라테스같은거 해보고싶긴 하더라고요",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100008750.1.17",
          "form": "운동 잘된다고 하더라고요",
          "original_form": "운동 잘된다고 하더라고요",
