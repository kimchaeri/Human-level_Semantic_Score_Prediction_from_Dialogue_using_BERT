      "utterance": [
        {
          "id": "MDRW2100022024.1.1",
          "form": "귀신만 안나오면 되기 때문에 그런건 잘 보는 편이에요!",
          "original_form": "귀신만 안나오면 되기 때문에 그런건 잘 보는 편이에요!",
          "speaker_id": "2",
          "time": "20210829 01:01"
        },
        {
          "id": "MDRW2100022024.1.2",
          "form": "아 킹덤까지요!",
          "original_form": "아 킹덤까지요!",
          "speaker_id": "1",
          "time": "20210829 01:01"
        },
        {
          "id": "MDRW2100022024.1.3",
          "form": "무섭지만 다 보고 블레도 샀어욬ㅋㅋㅋㅋㅋ",
          "original_form": "무섭지만 다 보고 블레도 샀어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:01"
        },
        {
          "id": "MDRW2100022024.1.4",
          "form": "우왕ㅋㅋㅋㅋ",
          "original_form": "우왕ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:01"
        },
        {
          "id": "MDRW2100022024.1.5",
          "form": "저는 저중에 카이로스 진짜 재밌게봤어요",
          "original_form": "저는 저중에 카이로스 진짜 재밌게봤어요",
          "speaker_id": "1",
          "time": "20210829 01:01"
        },
        {
          "id": "MDRW2100022024.1.6",
          "form": "신성록 배우의 재발견",
          "original_form": "신성록 배우의 재발견",
          "speaker_id": "1",
          "time": "20210829 01:01"
        },
        {
          "id": "MDRW2100022024.1.7",
          "form": "그건 어떤 내용이에요??",
          "original_form": "그건 어떤 내용이에요??",
          "speaker_id": "2",
          "time": "20210829 01:01"
        },
        {
          "id": "MDRW2100022024.1.8",
          "form": "앗 조금 스포될것같은데",
          "original_form": "앗 조금 스포될것같은데",
          "speaker_id": "1",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100022024.1.9",
          "form": "간략히 말씀드릴까용?ㅋㅋㅋ",
          "original_form": "간략히 말씀드릴까용?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100022024.1.10",
          "form": "스포따위 신경쓰지 않으니 어떻게 말쑴해주셔듀 괜찮아요!",
          "original_form": "스포따위 신경쓰지 않으니 어떻게 말쑴해주셔듀 괜찮아요!",
          "speaker_id": "2",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100022024.1.11",
          "form": "신성록이랑 이세영 배우가 나오는데",
          "original_form": "신성록이랑 이세영 배우가 나오는데",
          "speaker_id": "1",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100022024.1.12",
          "form": "이세영 배우가 핸드폰을 잃어버려요",
          "original_form": "이세영 배우가 핸드폰을 잃어버려요",
          "speaker_id": "1",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100022024.1.13",
          "form": "그래서 자기 핸드폰으로 전화를 거는데",
          "original_form": "그래서 자기 핸드폰으로 전화를 거는데",
          "speaker_id": "1",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100022024.1.14",
          "form": "신성록이 받는거에용",
          "original_form": "신성록이 받는거에용",
          "speaker_id": "1",
          "time": "20210829 01:02"
        },
        {
          "id": "MDRW2100022024.1.15",
          "form": "그래서 뭐야 내 핸드폰 주웠으면 돌려줘요 했는데",
          "original_form": "그래서 뭐야 내 핸드폰 주웠으면 돌려줘요 했는데",
          "speaker_id": "1",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100022024.1.16",
          "form": "알고보니까 신성록이 이세영 배우보다",
          "original_form": "알고보니까 신성록이 이세영 배우보다",
          "speaker_id": "1",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100022024.1.17",
          "form": "한달 후의 시간을 살아가는 사람이고",
          "original_form": "한달 후의 시간을 살아가는 사람이고",
          "speaker_id": "1",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100022024.1.18",
          "form": "한달 후의 시간에서는 이세영 배우가 죽어서 핸드폰 번호가 신성록 배우한테 넘어간거였어요",
          "original_form": "한달 후의 시간에서는 이세영 배우가 죽어서 핸드폰 번호가 신성록 배우한테 넘어간거였어요",
          "speaker_id": "1",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100022024.1.19",
          "form": "헐....",
          "original_form": "헐....",
          "speaker_id": "2",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100022024.1.20",
          "form": "아 죽었나? 교도소 갔나",
          "original_form": "아 죽었나? 교도소 갔나",
          "speaker_id": "1",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100022024.1.21",
          "form": "그럼 이세영배우님의 캐릭터가 죽어서 신성록배우님의 캐릭터가 전화번호를 이어받았다는 건가요??",
          "original_form": "그럼 이세영배우님의 캐릭터가 죽어서 신성록배우님의 캐릭터가 전화번호를 이어받았다는 건가요??",
          "speaker_id": "2",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100022024.1.22",
          "form": "아 아니다 교도소 갔어요!",
          "original_form": "아 아니다 교도소 갔어요!",
          "speaker_id": "1",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100022024.1.23",
          "form": "그리고 계~~속 죽을뻔해요",
          "original_form": "그리고 계~~속 죽을뻔해요",
          "speaker_id": "1",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100022024.1.24",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100022024.1.25",
          "form": "흐아 심장이 쫄깃해지는 기분이에요..",
          "original_form": "흐아 심장이 쫄깃해지는 기분이에요..",
          "speaker_id": "2",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100022024.1.26",
          "form": "엄청 재밌어요",
          "original_form": "엄청 재밌어요",
          "speaker_id": "1",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100022024.1.27",
          "form": "계속 죽을뻔하다가 교도소라니... 이럴수가....",
          "original_form": "계속 죽을뻔하다가 교도소라니... 이럴수가....",
          "speaker_id": "2",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100022024.1.28",
          "form": "꼭 추천드려용ㅋㅋㅋ",
          "original_form": "꼭 추천드려용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:05"
        }
      ]
