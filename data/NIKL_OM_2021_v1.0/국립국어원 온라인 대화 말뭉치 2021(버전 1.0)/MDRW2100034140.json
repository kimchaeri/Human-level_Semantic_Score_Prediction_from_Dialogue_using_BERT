      "utterance": [
        {
          "id": "MDRW2100034140.1.1",
          "form": "저녁 뭐드실 예정인가요?",
          "original_form": "저녁 뭐드실 예정인가요?",
          "speaker_id": "1",
          "time": "20210827 17:47"
        },
        {
          "id": "MDRW2100034140.1.2",
          "form": "보쌈이랑 쟁반국수 먹으려고요!",
          "original_form": "보쌈이랑 쟁반국수 먹으려고요!",
          "speaker_id": "2",
          "time": "20210827 18:06"
        },
        {
          "id": "MDRW2100034140.1.3",
          "form": "오아 맛있겠다",
          "original_form": "오아 맛있겠다",
          "speaker_id": "1",
          "time": "20210827 18:06"
        },
        {
          "id": "MDRW2100034140.1.4",
          "form": "쟁반막국수ㅠㅠ",
          "original_form": "쟁반막국수ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100034140.1.5",
          "form": "맛집인가요!!",
          "original_form": "맛집인가요!!",
          "speaker_id": "1",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100034140.1.6",
          "form": "네네! 집 가는 길에 픽업해서 가려구요",
          "original_form": "네네! 집 가는 길에 픽업해서 가려구요",
          "speaker_id": "2",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100034140.1.7",
          "form": "짱이다 맛있게드세용",
          "original_form": "짱이다 맛있게드세용",
          "speaker_id": "1",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100034140.1.8",
          "form": "족발보다 보쌈 더 좋아하세요?",
          "original_form": "족발보다 보쌈 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100034140.1.9",
          "form": "매번 다르긴 하지만 보쌈을 더 좋아해요",
          "original_form": "매번 다르긴 하지만 보쌈을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100034140.1.10",
          "form": "하긴 둘중 하나만 고르긴 어렵긴 하죠!",
          "original_form": "하긴 둘중 하나만 고르긴 어렵긴 하죠!",
          "speaker_id": "1",
          "time": "20210827 18:23"
        },
        {
          "id": "MDRW2100034140.1.11",
          "form": "둘 다 맛있으니까요ㅎㅎ name1님은요?",
          "original_form": "둘 다 맛있으니까요ㅎㅎ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100034140.1.12",
          "form": "저는....오늘은 족발이 땡기네용ㅋㅋ",
          "original_form": "저는....오늘은 족발이 땡기네용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100034140.1.13",
          "form": "족발도 맛있죠...조만간 먹어봐야겠어요ㅋㅋ",
          "original_form": "족발도 맛있죠...조만간 먹어봐야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100034140.1.14",
          "form": "ㅋㅋㅋㅋ동네에 족발 맛집 있는데 가끔 생각나요",
          "original_form": "ㅋㅋㅋㅋ동네에 족발 맛집 있는데 가끔 생각나요",
          "speaker_id": "1",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100034140.1.15",
          "form": "화곡영양족발이라고 찐맛집...",
          "original_form": "화곡영양족발이라고 찐맛집...",
          "speaker_id": "1",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100034140.1.16",
          "form": "기회되면 드셔보세요",
          "original_form": "기회되면 드셔보세요",
          "speaker_id": "1",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100034140.1.17",
          "form": "캡쳐해뒀어요! 기회 되면 먹어볼게요ㅎㅎ",
          "original_form": "캡쳐해뒀어요! 기회 되면 먹어볼게요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100034140.1.18",
          "form": "ㅋㅋㅋ넹 회사에서 족발얘기했더니 족발 먹고싶어졌어요",
          "original_form": "ㅋㅋㅋ넹 회사에서 족발얘기했더니 족발 먹고싶어졌어요",
          "speaker_id": "1",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100034140.1.19",
          "form": "회사에서는 더 먹고싶어지죠...ㅠㅠ",
          "original_form": "회사에서는 더 먹고싶어지죠...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:30"
        }
