      "utterance": [
        {
          "id": "MDRW2100027523.1.1",
          "form": "구두 ...ㅋㅋㅋ",
          "original_form": "구두 ...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027523.1.2",
          "form": "파란색 구두인데 별로 못신었어요 ㅋㅋㅋㅋ",
          "original_form": "파란색 구두인데 별로 못신었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027523.1.3",
          "form": "와 파랑구두 특이해욤",
          "original_form": "와 파랑구두 특이해욤",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027523.1.4",
          "form": "어떻게 코디 하시고싶나용",
          "original_form": "어떻게 코디 하시고싶나용",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027523.1.5",
          "form": "시원한색 원피스에 같이 입으려구요!!",
          "original_form": "시원한색 원피스에 같이 입으려구요!!",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027523.1.6",
          "form": "내년이 되겠지만 ㅎㅎㅎ...",
          "original_form": "내년이 되겠지만 ㅎㅎㅎ...",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027523.1.7",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027523.1.8",
          "form": "여름끝자락에 한번 개봉하셔요 ㅋㅋ",
          "original_form": "여름끝자락에 한번 개봉하셔요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027523.1.9",
          "form": "흠 그럴까요 ㅎㅎ 한번 외출할때가 된거같네요",
          "original_form": "흠 그럴까요 ㅎㅎ 한번 외출할때가 된거같네요",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027523.1.10",
          "form": "이 소낙비가 좀 그쳐야 할텐데!!",
          "original_form": "이 소낙비가 좀 그쳐야 할텐데!!",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027523.1.11",
          "form": "다시비오네요 지금 여기는",
          "original_form": "다시비오네요 지금 여기는",
          "speaker_id": "1",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027523.1.12",
          "form": "여기도요 ㅠㅠ 비가 너무 불규칙적으로 와서 옷 맞추기도 어려워요",
          "original_form": "여기도요 ㅠㅠ 비가 너무 불규칙적으로 와서 옷 맞추기도 어려워요",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100027523.1.13",
          "form": "그쵸 우산 항상 들구댕겨요",
          "original_form": "그쵸 우산 항상 들구댕겨요",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027523.1.14",
          "form": "날씨 오락가락이고 더웠다 추웠다 하니까 얇은 긴팔 반팔부터 고민되는 ㅋㅋㅋ",
          "original_form": "날씨 오락가락이고 더웠다 추웠다 하니까 얇은 긴팔 반팔부터 고민되는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100027523.1.15",
          "form": "맞습니다 감기안걸리게 조심해야해요",
          "original_form": "맞습니다 감기안걸리게 조심해야해요",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100027523.1.16",
          "form": "밤에 문 열어놓고 자면 감기 걸릴듯해요 ㅎㅎ 집에서도 따숩게 입어야 할것같더라구요",
          "original_form": "밤에 문 열어놓고 자면 감기 걸릴듯해요 ㅎㅎ 집에서도 따숩게 입어야 할것같더라구요",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027523.1.17",
          "form": "한낮에는 더운데 말이죠",
          "original_form": "한낮에는 더운데 말이죠",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027523.1.18",
          "form": "맞아요 지금은 또 후텁디근 ㅋㅋ그래도이정도면 살만해요",
          "original_form": "맞아요 지금은 또 후텁디근 ㅋㅋ그래도이정도면 살만해요",
          "speaker_id": "1",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100027523.1.19",
          "form": "맞아요 ㅋㅋㅋㅋ 비올때는 좀 찝찝하긴 해도 ㅠㅠㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ 비올때는 좀 찝찝하긴 해도 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100027523.1.20",
          "form": "금방 또 가을옷 입겠죠!! 하늘하늘한 옷 입을거 생각하니 기대되네요",
          "original_form": "금방 또 가을옷 입겠죠!! 하늘하늘한 옷 입을거 생각하니 기대되네요",
          "speaker_id": "2",
          "time": "20210902 17:43"
        }
      ]
