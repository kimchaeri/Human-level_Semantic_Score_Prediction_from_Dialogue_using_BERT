      "utterance": [
        {
          "id": "MDRW2100025410.1.1",
          "form": "반려동물 좋아하시나요",
          "original_form": "반려동물 좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 11:00"
        },
        {
          "id": "MDRW2100025410.1.2",
          "form": "그럼요!",
          "original_form": "그럼요!",
          "speaker_id": "2",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025410.1.3",
          "form": "비록 저는 없지만... 남의 반려동물은 잘 귀여워한답니다 ㅎㅎ",
          "original_form": "비록 저는 없지만... 남의 반려동물은 잘 귀여워한답니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025410.1.4",
          "form": "저도 없어요.. ㅋㅋㅋㅋ",
          "original_form": "저도 없어요.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025410.1.5",
          "form": "남의 아이들이 기엽더라구요 ㅠㅠㅠ",
          "original_form": "남의 아이들이 기엽더라구요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 12:00"
        },
        {
          "id": "MDRW2100025410.1.6",
          "form": "제 친구들은 다 고양이 아니면 강아지랑 살더라구요,,",
          "original_form": "제 친구들은 다 고양이 아니면 강아지랑 살더라구요,,",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025410.1.7",
          "form": "맞아요... 저는 꼭 키우라면 강아지??",
          "original_form": "맞아요... 저는 꼭 키우라면 강아지??",
          "speaker_id": "1",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025410.1.8",
          "form": "헉 저두요!",
          "original_form": "헉 저두요!",
          "speaker_id": "2",
          "time": "20210901 12:02"
        },
        {
          "id": "MDRW2100025410.1.9",
          "form": "사실... 예쁜건 고양이긴 한데",
          "original_form": "사실... 예쁜건 고양이긴 한데",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025410.1.10",
          "form": "저는 교감 많이 하고싶어서....!!!",
          "original_form": "저는 교감 많이 하고싶어서....!!!",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025410.1.11",
          "form": "강아지같은 고양이가",
          "original_form": "강아지같은 고양이가",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025410.1.12",
          "form": "좋은고같어요",
          "original_form": "좋은고같어요",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025410.1.13",
          "form": "기엽더라구요 ㅠ",
          "original_form": "기엽더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025410.1.14",
          "form": "헉 맞아요 개냥이~!",
          "original_form": "헉 맞아요 개냥이~!",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025410.1.15",
          "form": "최고의 조합이죠....",
          "original_form": "최고의 조합이죠....",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025410.1.16",
          "form": "저 방금 물범 관련한 슬픈 소식을 봤어요ㅠ",
          "original_form": "저 방금 물범 관련한 슬픈 소식을 봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025410.1.17",
          "form": "사람을 좋아하는 물범이 죽은 채 발견됐대요",
          "original_form": "사람을 좋아하는 물범이 죽은 채 발견됐대요",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025410.1.18",
          "form": "헐랭 ㅠㅜㅜ물에 사는애들은대체적으로 사람을마니좋아하는거같아요 ㅠㅠ",
          "original_form": "헐랭 ㅠㅜㅜ물에 사는애들은대체적으로 사람을마니좋아하는거같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 12:49"
        },
        {
          "id": "MDRW2100025410.1.19",
          "form": "헉 그러게요... 왜 그런걸까요?",
          "original_form": "헉 그러게요... 왜 그런걸까요?",
          "speaker_id": "2",
          "time": "20210901 12:51"
        },
        {
          "id": "MDRW2100025410.1.20",
          "form": "해달도 그렇고...",
          "original_form": "해달도 그렇고...",
          "speaker_id": "2",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100025410.1.21",
          "form": "마음이 너무 아파요ㅠ",
          "original_form": "마음이 너무 아파요ㅠ",
          "speaker_id": "2",
          "time": "20210901 12:52"
        },
        {
          "id": "MDRW2100025410.1.22",
          "form": "저도 돌고래 이런거 키우고싶아오 기엽겠져 ㅋㅋㅋ",
          "original_form": "저도 돌고래 이런거 키우고싶아오 기엽겠져 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025410.1.23",
          "form": "네,,, 진짜 평화롭게 살 수 있을 거 같아요",
          "original_form": "네,,, 진짜 평화롭게 살 수 있을 거 같아요",
          "speaker_id": "2",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025410.1.24",
          "form": "돌고래랑 껴안지는 못하겠지만 ㅠ",
          "original_form": "돌고래랑 껴안지는 못하겠지만 ㅠ",
          "speaker_id": "2",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025410.1.25",
          "form": "물무서워하세요?",
          "original_form": "물무서워하세요?",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025410.1.26",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025410.1.27",
          "form": "수영도 못해요!",
          "original_form": "수영도 못해요!",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025410.1.28",
          "form": "아진짜요??",
          "original_form": "아진짜요??",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025410.1.29",
          "form": "전물좋아해서",
          "original_form": "전물좋아해서",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025410.1.30",
          "form": "돌고래 타보고 싶네요 ㅋㅋㅋ",
          "original_form": "돌고래 타보고 싶네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025410.1.31",
          "form": "우와 돌고래랑 같이 신나게 지내실 수 있겠어요 ㅋㅋㅋㅋ",
          "original_form": "우와 돌고래랑 같이 신나게 지내실 수 있겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:02"
        }
      ]
    }
