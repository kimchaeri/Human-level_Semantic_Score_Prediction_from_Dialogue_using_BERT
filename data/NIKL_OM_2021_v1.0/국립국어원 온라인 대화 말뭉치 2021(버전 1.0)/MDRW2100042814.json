      "utterance": [
        {
          "id": "MDRW2100042814.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 09:28"
        },
        {
          "id": "MDRW2100042814.1.2",
          "form": "아침 식사하셨어요??",
          "original_form": "아침 식사하셨어요??",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042814.1.3",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042814.1.4",
          "form": "생각이 없어서 안먹었어요",
          "original_form": "생각이 없어서 안먹었어요",
          "speaker_id": "2",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042814.1.5",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "2",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042814.1.6",
          "form": "전 사과 견과류 먹었어요",
          "original_form": "전 사과 견과류 먹었어요",
          "speaker_id": "1",
          "time": "20210907 09:30"
        },
        {
          "id": "MDRW2100042814.1.7",
          "form": "보통 아침은 잘 안 드시나요??",
          "original_form": "보통 아침은 잘 안 드시나요??",
          "speaker_id": "1",
          "time": "20210907 09:30"
        },
        {
          "id": "MDRW2100042814.1.8",
          "form": "전 원래 안 챙겨먹고 다니는데",
          "original_form": "전 원래 안 챙겨먹고 다니는데",
          "speaker_id": "1",
          "time": "20210907 09:30"
        },
        {
          "id": "MDRW2100042814.1.9",
          "form": "요즘에는 건강을 생각해서",
          "original_form": "요즘에는 건강을 생각해서",
          "speaker_id": "1",
          "time": "20210907 09:30"
        },
        {
          "id": "MDRW2100042814.1.10",
          "form": "조금이라도 먹으려고 하고 있어요",
          "original_form": "조금이라도 먹으려고 하고 있어요",
          "speaker_id": "1",
          "time": "20210907 09:30"
        },
        {
          "id": "MDRW2100042814.1.11",
          "form": "저도 아침밥보단 아침잠이 더 좋은데\r\n아이들이 있다보니 아이들 먹이면서\r\n같이 먹는 편이에요:)",
          "original_form": "저도 아침밥보단 아침잠이 더 좋은데\r\n아이들이 있다보니 아이들 먹이면서\r\n같이 먹는 편이에요:)",
          "speaker_id": "2",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042814.1.12",
          "form": "오 좋은 습관 형성중이시네요 ",
          "original_form": "오 좋은 습관 형성중이시네요 ",
          "speaker_id": "2",
          "time": "20210907 09:31"
        },
        {
          "id": "MDRW2100042814.1.13",
          "form": "아이들은 몇 살인가요??",
          "original_form": "아이들은 몇 살인가요??",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042814.1.14",
          "form": "저는 늘 아이들만 챙겨주고",
          "original_form": "저는 늘 아이들만 챙겨주고",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042814.1.15",
          "form": "저는 굶고 나오는데",
          "original_form": "저는 굶고 나오는데",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042814.1.16",
          "form": "5살이랑 2살이에요:)",
          "original_form": "5살이랑 2살이에요:)",
          "speaker_id": "2",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042814.1.17",
          "form": "내일 빈속에 커피를 마셔서",
          "original_form": "내일 빈속에 커피를 마셔서",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042814.1.18",
          "form": "이제 좀 챙겨 먹으라구요",
          "original_form": "이제 좀 챙겨 먹으라구요",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042814.1.19",
          "form": "아ㅠㅠ 아이들 챙길때\r\n조금이라도 같이 드세요",
          "original_form": "아ㅠㅠ 아이들 챙길때\r\n조금이라도 같이 드세요",
          "speaker_id": "2",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042814.1.20",
          "form": "2살 너무 귀엽겠어요 ^^",
          "original_form": "2살 너무 귀엽겠어요 ^^",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042814.1.21",
          "form": "전 8살 5살이예요 ㅎㅎ",
          "original_form": "전 8살 5살이예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042814.1.22",
          "form": "제일 손 많이 간다는 8살이 있네요:)",
          "original_form": "제일 손 많이 간다는 8살이 있네요:)",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042814.1.23",
          "form": "줌수업하면\r\n식사랑 간식 챙겨주는것도\r\n일일거 같아요ㅠ",
          "original_form": "줌수업하면\r\n식사랑 간식 챙겨주는것도\r\n일일거 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042814.1.24",
          "form": "네 다행이 줌수업은 안하고",
          "original_form": "네 다행이 줌수업은 안하고",
          "speaker_id": "1",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042814.1.25",
          "form": "올해는 일이 학년은",
          "original_form": "올해는 일이 학년은",
          "speaker_id": "1",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042814.1.26",
          "form": "다 등교할 수 있게 돼서",
          "original_form": "다 등교할 수 있게 돼서",
          "speaker_id": "1",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042814.1.27",
          "form": "잘 가고 있어요",
          "original_form": "잘 가고 있어요",
          "speaker_id": "1",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042814.1.28",
          "form": "방학 때 밥 챙겨주고 간식 챙겨주라니까 힘들더라구요",
          "original_form": "방학 때 밥 챙겨주고 간식 챙겨주라니까 힘들더라구요",
          "speaker_id": "1",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042814.1.29",
          "form": "그쵸:) 많이 힘들거 같아요",
          "original_form": "그쵸:) 많이 힘들거 같아요",
          "speaker_id": "2",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042814.1.30",
          "form": "그래도 등교할 수 있어서\r\n감사하네요",
          "original_form": "그래도 등교할 수 있어서\r\n감사하네요",
          "speaker_id": "2",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042814.1.31",
          "form": "학교에서 점심은 먹는거죠?",
          "original_form": "학교에서 점심은 먹는거죠?",
          "speaker_id": "2",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042814.1.32",
          "form": "네 학교에서점심먹고 와요",
          "original_form": "네 학교에서점심먹고 와요",
          "speaker_id": "1",
          "time": "20210907 09:45"
        },
        {
          "id": "MDRW2100042814.1.33",
          "form": "한끼먹고 오는게 큰거같아요",
          "original_form": "한끼먹고 오는게 큰거같아요",
          "speaker_id": "1",
          "time": "20210907 09:46"
        }
      ]
    }
