      "utterance": [
        {
          "id": "MDRW2100019722.1.1",
          "form": "반려동물을 키우십니까?",
          "original_form": "반려동물을 키우십니까?",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019722.1.2",
          "form": "아뇨! 지금은 키우지않아요",
          "original_form": "아뇨! 지금은 키우지않아요",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019722.1.3",
          "form": "저도 키우지않습니다",
          "original_form": "저도 키우지않습니다",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019722.1.4",
          "form": "그러시군요 키우게된다면 어떤 동물을 키우고싶으세요?",
          "original_form": "그러시군요 키우게된다면 어떤 동물을 키우고싶으세요?",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019722.1.5",
          "form": "강아지를 키우고싶습니다 귀여워서요",
          "original_form": "강아지를 키우고싶습니다 귀여워서요",
          "speaker_id": "1",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019722.1.6",
          "form": "저도요! 강아지랑 토끼 엄청 좋아해요",
          "original_form": "저도요! 강아지랑 토끼 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019722.1.7",
          "form": "토끼도 귀엽지요",
          "original_form": "토끼도 귀엽지요",
          "speaker_id": "1",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019722.1.8",
          "form": "초등학교때 학교에서 토끼를 키웠습니다",
          "original_form": "초등학교때 학교에서 토끼를 키웠습니다",
          "speaker_id": "1",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019722.1.9",
          "form": "name2님도 학교에서 동물을 키웠나요?",
          "original_form": "&name2&님도 학교에서 동물을 키웠나요?",
          "speaker_id": "1",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019722.1.10",
          "form": "어렴풋이 토끼를 학교에서 키웠던것도같아요ㅎㅎ",
          "original_form": "어렴풋이 토끼를 학교에서 키웠던것도같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019722.1.11",
          "form": "저희는 닭도 키웠는데 민원들어와서 철수시켰습니다",
          "original_form": "저희는 닭도 키웠는데 민원들어와서 철수시켰습니다",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019722.1.12",
          "form": "ㅋㅋㅋ귀엽네용",
          "original_form": "ㅋㅋㅋ귀엽네용",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019722.1.13",
          "form": "닭이 계속 울어서 그랬나봐요",
          "original_form": "닭이 계속 울어서 그랬나봐요",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019722.1.14",
          "form": "네 근처에 주거구역이다보니 새벽마다 울었나봅니다",
          "original_form": "네 근처에 주거구역이다보니 새벽마다 울었나봅니다",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100019722.1.15",
          "form": "그랬겠네요 요즘 유튜브보면 동물관련 영상도 많아서 너무 좋아요",
          "original_form": "그랬겠네요 요즘 유튜브보면 동물관련 영상도 많아서 너무 좋아요",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019722.1.16",
          "form": "보기만해도 힐링입니다",
          "original_form": "보기만해도 힐링입니다",
          "speaker_id": "2",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100019722.1.17",
          "form": "맞습니다 고양이도 귀엽더라고요",
          "original_form": "맞습니다 고양이도 귀엽더라고요",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019722.1.18",
          "form": "내가 키우지 않아서 고생하지않다보니 더 그런가봅니다",
          "original_form": "내가 키우지 않아서 고생하지않다보니 더 그런가봅니다",
          "speaker_id": "1",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019722.1.19",
          "form": "진짜 공감되네요ㅎㅎ 실제로는 엄청 케어하기도 힘들고 치울것도 많겠죠",
          "original_form": "진짜 공감되네요ㅎㅎ 실제로는 엄청 케어하기도 힘들고 치울것도 많겠죠",
          "speaker_id": "2",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019722.1.20",
          "form": "영상으로는 귀여운 모습만 주로 나오니까요",
          "original_form": "영상으로는 귀여운 모습만 주로 나오니까요",
          "speaker_id": "2",
          "time": "20210827 21:54"
        }
      ]
