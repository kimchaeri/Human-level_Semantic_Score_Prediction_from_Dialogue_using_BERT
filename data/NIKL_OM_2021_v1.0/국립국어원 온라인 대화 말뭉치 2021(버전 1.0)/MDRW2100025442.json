      "utterance": [
        {
          "id": "MDRW2100025442.1.1",
          "form": "메타버스란게 뭘까요",
          "original_form": "메타버스란게 뭘까요",
          "speaker_id": "1",
          "time": "20210901 11:26"
        },
        {
          "id": "MDRW2100025442.1.2",
          "form": "브이알 겜해보셨나요",
          "original_form": "브이알 겜해보셨나요",
          "speaker_id": "2",
          "time": "20210901 11:26"
        },
        {
          "id": "MDRW2100025442.1.3",
          "form": "브이알게임인가요?!",
          "original_form": "브이알게임인가요?!",
          "speaker_id": "1",
          "time": "20210901 11:26"
        },
        {
          "id": "MDRW2100025442.1.4",
          "form": "포켓몬고 같은?",
          "original_form": "포켓몬고 같은?",
          "speaker_id": "1",
          "time": "20210901 11:26"
        },
        {
          "id": "MDRW2100025442.1.5",
          "form": "VR겜에서 많이 사용하는거 같아요",
          "original_form": "VR겜에서 많이 사용하는거 같아요",
          "speaker_id": "2",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100025442.1.6",
          "form": "최근에 도깨비라는 게임 광고 영상봤는데",
          "original_form": "최근에 도깨비라는 게임 광고 영상봤는데",
          "speaker_id": "2",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100025442.1.7",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100025442.1.8",
          "form": "도깨비!",
          "original_form": "도깨비!",
          "speaker_id": "1",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100025442.1.9",
          "form": "거기도 메타버스 추가될 예정이라고 하더라구요",
          "original_form": "거기도 메타버스 추가될 예정이라고 하더라구요",
          "speaker_id": "2",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100025442.1.10",
          "form": "영상대박이죠?!",
          "original_form": "영상대박이죠?!",
          "speaker_id": "1",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100025442.1.11",
          "form": "네 완전요",
          "original_form": "네 완전요",
          "speaker_id": "2",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100025442.1.12",
          "form": "콘솔게임인가요?",
          "original_form": "콘솔게임인가요?",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025442.1.13",
          "form": "요즘 메타버스가 인기여서",
          "original_form": "요즘 메타버스가 인기여서",
          "speaker_id": "2",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025442.1.14",
          "form": "모바일인가요?",
          "original_form": "모바일인가요?",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025442.1.15",
          "form": "주식 관련으로도 많이 오르더라구요",
          "original_form": "주식 관련으로도 많이 오르더라구요",
          "speaker_id": "2",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025442.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025442.1.17",
          "form": "아마 컴터로 하는게임이거나",
          "original_form": "아마 컴터로 하는게임이거나",
          "speaker_id": "2",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025442.1.18",
          "form": "급상승ㅋ",
          "original_form": "급상승ㅋ",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025442.1.19",
          "form": "그럴것같아요",
          "original_form": "그럴것같아요",
          "speaker_id": "2",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025442.1.20",
          "form": "컴터가 없는데ㅜㅜ",
          "original_form": "컴터가 없는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025442.1.21",
          "form": "모바일은 아마 차후에요?",
          "original_form": "모바일은 아마 차후에요?",
          "speaker_id": "2",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025442.1.22",
          "form": "콘솔로 나올지도 모르죠",
          "original_form": "콘솔로 나올지도 모르죠",
          "speaker_id": "2",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100025442.1.23",
          "form": "노트북을 장만해야하나..'",
          "original_form": "노트북을 장만해야하나..'",
          "speaker_id": "1",
          "time": "20210901 11:29"
        },
        {
          "id": "MDRW2100025442.1.24",
          "form": "메타버스 가미되면",
          "original_form": "메타버스 가미되면",
          "speaker_id": "2",
          "time": "20210901 11:29"
        },
        {
          "id": "MDRW2100025442.1.25",
          "form": "게임에서",
          "original_form": "게임에서",
          "speaker_id": "2",
          "time": "20210901 11:29"
        }
      ]
