      "utterance": [
        {
          "id": "MDRW2100030321.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030321.1.2",
          "form": "네 자주는아니고 전 팔기만해요",
          "original_form": "네 자주는아니고 전 팔기만해요",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030321.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030321.1.4",
          "form": "중고 사는거 안좋아해서요",
          "original_form": "중고 사는거 안좋아해서요",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030321.1.5",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030321.1.6",
          "form": "그렇조",
          "original_form": "그렇조",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030321.1.7",
          "form": "진짜 구하기 힘든거아니면",
          "original_form": "진짜 구하기 힘든거아니면",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030321.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 08:35"
        },
        {
          "id": "MDRW2100030321.1.9",
          "form": "최근에 뭐 파셨어요?",
          "original_form": "최근에 뭐 파셨어요?",
          "speaker_id": "2",
          "time": "20210904 10:41"
        },
        {
          "id": "MDRW2100030321.1.10",
          "form": "공기청정기요",
          "original_form": "공기청정기요",
          "speaker_id": "1",
          "time": "20210904 10:43"
        },
        {
          "id": "MDRW2100030321.1.11",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "2",
          "time": "20210904 10:43"
        },
        {
          "id": "MDRW2100030321.1.12",
          "form": "큰거파셨네요?",
          "original_form": "큰거파셨네요?",
          "speaker_id": "2",
          "time": "20210904 10:43"
        },
        {
          "id": "MDRW2100030321.1.13",
          "form": "차량용이라소형이에요",
          "original_form": "차량용이라소형이에요",
          "speaker_id": "1",
          "time": "20210904 10:44"
        },
        {
          "id": "MDRW2100030321.1.14",
          "form": "아 차량용",
          "original_form": "아 차량용",
          "speaker_id": "2",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100030321.1.15",
          "form": "거실용인줄 ㅋㅋ",
          "original_form": "거실용인줄 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030321.1.16",
          "form": "저는 보통",
          "original_form": "저는 보통",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030321.1.17",
          "form": "휴대폰 이어폰 그런거 팔아요",
          "original_form": "휴대폰 이어폰 그런거 팔아요",
          "speaker_id": "2",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030321.1.18",
          "form": "폰 얼마나 쓰시는데요?",
          "original_form": "폰 얼마나 쓰시는데요?",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100030321.1.19",
          "form": "자주하시넹?",
          "original_form": "자주하시넹?",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030321.1.20",
          "form": "폰은 새거사면 팔구",
          "original_form": "폰은 새거사면 팔구",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100030321.1.21",
          "form": "이어폰은 쓰다가 아니다싶음 팔구 그래요",
          "original_form": "이어폰은 쓰다가 아니다싶음 팔구 그래요",
