      "utterance": [
        {
          "id": "MDRW2100013716.1.1",
          "form": "백신은 맞으셨어요??",
          "original_form": "백신은 맞으셨어요??",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013716.1.2",
          "form": "네 여행간다면 백신은맞아야할것같아서",
          "original_form": "네 여행간다면 백신은맞아야할것같아서",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013716.1.3",
          "form": "사전예약으로 1차맞고 좀있으면 2차맞아요",
          "original_form": "사전예약으로 1차맞고 좀있으면 2차맞아요",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013716.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013716.1.5",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013716.1.6",
          "form": "저 내일 1차 맞아욤",
          "original_form": "저 내일 1차 맞아욤",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013716.1.7",
          "form": "어떤걸로 맞으시나요",
          "original_form": "어떤걸로 맞으시나요",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013716.1.8",
          "form": "화이자요!",
          "original_form": "화이자요!",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013716.1.9",
          "form": "지금은 거의 화이자인가보네요",
          "original_form": "지금은 거의 화이자인가보네요",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013716.1.10",
          "form": "저도 얼른 2차맞고 마무리하고싶어요ㅜ",
          "original_form": "저도 얼른 2차맞고 마무리하고싶어요ㅜ",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013716.1.11",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013716.1.12",
          "form": "2차는 6주 뒤더라고요ㅠㅠㅠ",
          "original_form": "2차는 6주 뒤더라고요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013716.1.13",
          "form": "저도 마무리하고 싶은데",
          "original_form": "저도 마무리하고 싶은데",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013716.1.14",
          "form": "너무 늦어서",
          "original_form": "너무 늦어서",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013716.1.15",
          "form": "그거 1주는 땡겨주긴할텐데요ㅜ",
          "original_form": "그거 1주는 땡겨주긴할텐데요ㅜ",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013716.1.16",
          "form": "전화해보셨어요?",
          "original_form": "전화해보셨어요?",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100013716.1.17",
          "form": "헉 그런가요???",
          "original_form": "헉 그런가요???",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100013716.1.18",
          "form": "네 그날일있어서 못간다고하니 일주일 땡겨주드라구여",
          "original_form": "네 그날일있어서 못간다고하니 일주일 땡겨주드라구여",
          "speaker_id": "2",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100013716.1.19",
          "form": "1차 맞고 13시간 잤아요...",
          "original_form": "1차 맞고 13시간 잤아요...",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100013716.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ잠이 많아졌네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ잠이 많아졌네요",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100013716.1.21",
          "form": "2차는 한번 1주 땡기도록 전화해봐야겠네용",
          "original_form": "2차는 한번 1주 땡기도록 전화해봐야겠네용",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100013716.1.22",
          "form": "ㅋㅋㅋㅋㅋ저도 한 3일은 잠이어찌나 오던지",
          "original_form": "ㅋㅋㅋㅋㅋ저도 한 3일은 잠이어찌나 오던지",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100013716.1.23",
          "form": "백신맞고 증상은어떠셨어요",
          "original_form": "백신맞고 증상은어떠셨어요",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100013716.1.24",
          "form": "언능 2차까지다맞아서 여행가고싶어요",
          "original_form": "언능 2차까지다맞아서 여행가고싶어요",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100013716.1.25",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100013716.1.26",
          "form": "음 처음 맞았을 땐",
          "original_form": "음 처음 맞았을 땐",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100013716.1.27",
          "form": "약간 뒷목 뻐근했는데",
          "original_form": "약간 뒷목 뻐근했는데",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100013716.1.28",
          "form": "그것도 심하지 않고 바로 사라지고",
          "original_form": "그것도 심하지 않고 바로 사라지고",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100013716.1.29",
          "form": "한 5시간 지나고부터?",
          "original_form": "한 5시간 지나고부터?",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100013716.1.30",
          "form": "지금까지 맞은 부위만 약간 근육통 읶어요",
          "original_form": "지금까지 맞은 부위만 약간 근육통 읶어요",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100013716.1.31",
          "form": "그외에는 잠만 오고",
          "original_form": "그외에는 잠만 오고",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100013716.1.32",
          "form": "별로 아프지 않네요",
          "original_form": "별로 아프지 않네요",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100013716.1.33",
          "form": "2차 맞으면 방학 때 못 간 여행도 가고 소소한 것부터 일상 되찾을 거예요ㅠㅠㅠㅠ",
          "original_form": "2차 맞으면 방학 때 못 간 여행도 가고 소소한 것부터 일상 되찾을 거예요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100013716.1.34",
          "form": "다들 잘도 돌아다니던데 여행 한번도 못 갔거든요...",
          "original_form": "다들 잘도 돌아다니던데 여행 한번도 못 갔거든요...",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100013716.1.35",
          "form": "저도 비슷했어요",
          "original_form": "저도 비슷했어요",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100013716.1.36",
          "form": "맞아요 여행한번도못갔어요 드라이브로만 달래고...바다에 발담그고싶어요",
          "original_form": "맞아요 여행한번도못갔어요 드라이브로만 달래고...바다에 발담그고싶어요",
          "speaker_id": "2",
          "time": "20210827 22:29"
        }
      ]
    }
