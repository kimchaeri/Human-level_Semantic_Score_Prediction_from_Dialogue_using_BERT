      "utterance": [
        {
          "id": "MDRW2100019627.1.1",
          "form": "소개팅 해보셨어요? ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "소개팅 해보셨어요? ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019627.1.2",
          "form": "음..과팅은 해봤어요!",
          "original_form": "음..과팅은 해봤어요!",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100019627.1.3",
          "form": "오 어때요 과팅은??",
          "original_form": "오 어때요 과팅은??",
          "speaker_id": "1",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019627.1.4",
          "form": "다른과 친구",
          "original_form": "다른과 친구",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019627.1.5",
          "form": "만나는느낌으로나간거라,,,",
          "original_form": "만나는느낌으로나간거라,,,",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019627.1.6",
          "form": "지금은 화석니지만",
          "original_form": "지금은 화석니지만",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019627.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019627.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래도 조은 경험이었을거같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래도 조은 경험이었을거같아요",
          "speaker_id": "1",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019627.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋname1님은요??!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ&name1&님은요??!!",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019627.1.10",
          "form": "맞아야! 그때 친구들이 아직도 수업이나 수강신청때 서로 도와주고있으니까여!",
          "original_form": "맞아야! 그때 친구들이 아직도 수업이나 수강신청때 서로 도와주고있으니까여!",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019627.1.11",
          "form": "저는 안해봤어요 ㅠㅜ",
          "original_form": "저는 안해봤어요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019627.1.12",
          "form": "전 소개팅 부담스러워 하는 스타일이라 ㅋㅋㅋㅋ",
          "original_form": "전 소개팅 부담스러워 하는 스타일이라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019627.1.13",
          "form": "아하아 ㅠㅠ",
          "original_form": "아하아 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019627.1.14",
          "form": "자만추..?!?",
