      "utterance": [
        {
          "id": "MDRW2100011081.1.1",
          "form": "너 책읽는 거 좋아하니?",
          "original_form": "너 책읽는 거 좋아하니?",
          "speaker_id": "2",
          "time": "20210824 00:40"
        },
        {
          "id": "MDRW2100011081.1.2",
          "form": "독서 좋아해?",
          "original_form": "독서 좋아해?",
          "speaker_id": "2",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100011081.1.3",
          "form": "응 시간은 별로 없지만",
          "original_form": "응 시간은 별로 없지만",
          "speaker_id": "1",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100011081.1.4",
          "form": "어떤 장르의 책을 조항하니?",
          "original_form": "어떤 장르의 책을 조항하니?",
          "speaker_id": "2",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100011081.1.5",
          "form": "방학때 좀 읽으려고 노력했어",
          "original_form": "방학때 좀 읽으려고 노력했어",
          "speaker_id": "1",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100011081.1.6",
          "form": "나는 추리 스릴러를 좋아해ㄴ",
          "original_form": "나는 추리 스릴러를 좋아해ㄴ",
          "speaker_id": "1",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100011081.1.7",
          "form": "오. 이번 방학?",
          "original_form": "오. 이번 방학?",
          "speaker_id": "2",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100011081.1.8",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "1",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100011081.1.9",
          "form": "나는 로맨스판타지를 좋아해.",
          "original_form": "나는 로맨스판타지를 좋아해.",
          "speaker_id": "2",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100011081.1.10",
          "form": "신분이 높았으면 좋겠어.",
          "original_form": "신분이 높았으면 좋겠어.",
          "speaker_id": "2",
          "time": "20210824 00:41"
        },
        {
          "id": "MDRW2100011081.1.11",
          "form": "너도 좋아하지?",
          "original_form": "너도 좋아하지?",
          "speaker_id": "2",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.12",
          "form": "응응 이번방학에 틈나는대로 읽었지",
          "original_form": "응응 이번방학에 틈나는대로 읽었지",
          "speaker_id": "1",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.13",
          "form": "당연하지.",
          "original_form": "당연하지.",
          "speaker_id": "1",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.14",
          "form": "그런건 좀 대리만족이 되는 것 같아",
          "original_form": "그런건 좀 대리만족이 되는 것 같아",
          "speaker_id": "1",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.15",
          "form": "아 그ㅜ장르를 틈나는대로 읽은거야?",
          "original_form": "아 그ㅜ장르를 틈나는대로 읽은거야?",
          "speaker_id": "2",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.16",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.17",
          "form": "몰입감이 장난아냐",
          "original_form": "몰입감이 장난아냐",
          "speaker_id": "2",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.18",
          "form": "맞아 그리고 종류도 엄청 많더라",
          "original_form": "맞아 그리고 종류도 엄청 많더라",
          "speaker_id": "1",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.19",
          "form": "요즘 유행인가봐",
          "original_form": "요즘 유행인가봐",
          "speaker_id": "1",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.20",
          "form": "너도 한번 써보는건 어때",
          "original_form": "너도 한번 써보는건 어때",
          "speaker_id": "1",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.21",
          "form": "맞아 다양한 플랫폼에서 그 장르를 밀어주더라",
          "original_form": "맞아 다양한 플랫폼에서 그 장르를 밀어주더라",
          "speaker_id": "2",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.22",
          "form": "가끔은 유칳9",
          "original_form": "가끔은 유칳9",
          "speaker_id": "2",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.23",
          "form": "아 그럴까?",
          "original_form": "아 그럴까?",
          "speaker_id": "2",
          "time": "20210824 00:42"
        },
        {
          "id": "MDRW2100011081.1.24",
          "form": "돈을 많이 번대",
          "original_form": "돈을 많이 번대",
          "speaker_id": "1",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100011081.1.25",
          "form": "그럼 내 작품의 서브쥬인공이 되어줄래?",
          "original_form": "그럼 내 작품의 서브쥬인공이 되어줄래?",
          "speaker_id": "2",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100011081.1.26",
          "form": "그래 좋아!",
          "original_form": "그래 좋아!",
          "speaker_id": "1",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100011081.1.27",
          "form": "그것참 솔깃하구나.",
          "original_form": "그것참 솔깃하구나.",
          "speaker_id": "2",
          "time": "20210824 00:43"
        }
      ]
    }
  ]
