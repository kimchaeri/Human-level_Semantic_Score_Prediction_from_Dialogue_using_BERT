      "utterance": [
        {
          "id": "MDRW2100019960.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100019960.1.2",
          "form": "네 자취하고 있어서 다해요",
          "original_form": "네 자취하고 있어서 다해요",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019960.1.3",
          "form": "전  청2",
          "original_form": "전  청2",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019960.1.4",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019960.1.5",
          "form": "역할분담이 되있으시구나",
          "original_form": "역할분담이 되있으시구나",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019960.1.6",
          "form": "주기적으로 하세요? 아님 그냥 더러워보일때 치우는 편이세요?",
          "original_form": "주기적으로 하세요? 아님 그냥 더러워보일때 치우는 편이세요?",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019960.1.7",
          "form": "주기적으로일주일에한번씩",
          "original_form": "주기적으로일주일에한번씩",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019960.1.8",
          "form": "부지런하시네요 저는 후자거든요 ㅋㅋ",
          "original_form": "부지런하시네요 저는 후자거든요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019960.1.9",
          "form": "이제 옷정리도 해야하는데",
          "original_form": "이제 옷정리도 해야하는데",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019960.1.10",
          "form": "차일피일 미루고 있어요",
          "original_form": "차일피일 미루고 있어요",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019960.1.11",
          "form": "간절기엇 필요하져",
          "original_form": "간절기엇 필요하져",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019960.1.12",
          "form": "맞아요 점점 기온이 내려갈 것 같은데 또 비 안오는 날은 더울 것 같아서 반팔을 다 넣기도 뭐하고",
          "original_form": "맞아요 점점 기온이 내려갈 것 같은데 또 비 안오는 날은 더울 것 같아서 반팔을 다 넣기도 뭐하고",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019960.1.13",
          "form": "애매하네요",
          "original_form": "애매하네요",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019960.1.14",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019960.1.15",
          "form": "청소하실때 뭘 제일 먼저 하나요?",
          "original_form": "청소하실때 뭘 제일 먼저 하나요?",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019960.1.16",
          "form": "환기죠",
          "original_form": "환기죠",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019960.1.17",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019960.1.18",
          "form": "환기하신 후엔 어딜 먼저 청소하시나요?",
          "original_form": "환기하신 후엔 어딜 먼저 청소하시나요?",
