      "utterance": [
        {
          "id": "MDRW2100041775.1.1",
          "form": "해마다 단풍구경 다녔었는데 작년부터 못가고 있네요",
          "original_form": "해마다 단풍구경 다녔었는데 작년부터 못가고 있네요",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100041775.1.2",
          "form": "단풍 좋아하나요?^^",
          "original_form": "단풍 좋아하나요?^^",
          "speaker_id": "2",
          "time": "20210906 15:55"
        },
        {
          "id": "MDRW2100041775.1.3",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100041775.1.4",
          "form": "넘 예쁘지요?",
          "original_form": "넘 예쁘지요?",
          "speaker_id": "1",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100041775.1.5",
          "form": "단풍을 정말 좋아해서 가을을 가장 좋아한답니다",
          "original_form": "단풍을 정말 좋아해서 가을을 가장 좋아한답니다",
          "speaker_id": "2",
          "time": "20210906 19:44"
        },
        {
          "id": "MDRW2100041775.1.6",
          "form": "곧 예쁜 단풍들을 또 볼  수 있겠죠? 기대가 되네요",
          "original_form": "곧 예쁜 단풍들을 또 볼  수 있겠죠? 기대가 되네요",
          "speaker_id": "2",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100041775.1.7",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100041775.1.8",
          "form": "저도 가을이 젤 좋아요",
          "original_form": "저도 가을이 젤 좋아요",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100041775.1.9",
          "form": "좋아하는 계절이 돌아와서 참 좋아요",
          "original_form": "좋아하는 계절이 돌아와서 참 좋아요",
          "speaker_id": "2",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100041775.1.10",
          "form": "그래서 보통 가을에 여행 많이 다녔는데",
          "original_form": "그래서 보통 가을에 여행 많이 다녔는데",
          "speaker_id": "2",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100041775.1.11",
          "form": "저도요ㅎ",
          "original_form": "저도요ㅎ",
          "speaker_id": "1",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100041775.1.12",
          "form": "또자엄마님도 단풍구경 여기저기 다니셨나요?",
          "original_form": "또자엄마님도 단풍구경 여기저기 다니셨나요?",
          "speaker_id": "2",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100041775.1.13",
          "form": "어디가 제일 기억나나요?",
          "original_form": "어디가 제일 기억나나요?",
          "speaker_id": "2",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100041775.1.14",
          "form": "전 설악산이여",
          "original_form": "전 설악산이여",
          "speaker_id": "1",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100041775.1.15",
          "form": "정말 아름다웠어요",
          "original_form": "정말 아름다웠어요",
          "speaker_id": "1",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100041775.1.16",
          "form": "그렇구나 설악산 좋았어요 저도",
          "original_form": "그렇구나 설악산 좋았어요 저도",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100041775.1.17",
          "form": "저는 내장산이 기억에 남아요",
          "original_form": "저는 내장산이 기억에 남아요",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100041775.1.18",
          "form": "내장산 단풍도 꽤 예쁘더라구요",
          "original_form": "내장산 단풍도 꽤 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100041775.1.19",
          "form": "오 내장산",
          "original_form": "오 내장산",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100041775.1.20",
          "form": "한번 가보고싶네요",
          "original_form": "한번 가보고싶네요",
          "speaker_id": "1",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100041775.1.21",
          "form": "안가보셨다면 추천합니다",
          "original_form": "안가보셨다면 추천합니다",
          "speaker_id": "2",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100041775.1.22",
          "form": "마음에 들거에요",
          "original_form": "마음에 들거에요",
          "speaker_id": "2",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100041775.1.23",
          "form": "게다가 근처 음식점도 맛있어요",
          "original_form": "게다가 근처 음식점도 맛있어요",
          "speaker_id": "2",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100041775.1.24",
          "form": "전라도니까요",
          "original_form": "전라도니까요",
          "speaker_id": "2",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100041775.1.25",
          "form": "꼭 가볼께요ㅎ",
          "original_form": "꼭 가볼께요ㅎ",
          "speaker_id": "1",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100041775.1.26",
          "form": "네 강추입니다",
          "original_form": "네 강추입니다",
          "speaker_id": "2",
          "time": "20210906 20:14"
        }
      ]
    }
