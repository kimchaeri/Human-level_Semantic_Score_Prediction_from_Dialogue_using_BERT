      "utterance": [
        {
          "id": "MDRW2100026246.1.1",
          "form": "직장 생활 만족하시나요??",
          "original_form": "직장 생활 만족하시나요??",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026246.1.2",
          "form": "아니요... ㅋㅋㅋㅋ",
          "original_form": "아니요... ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026246.1.3",
          "form": "하는일은 맘에들어요 제가 좋아하는것이기도하구요",
          "original_form": "하는일은 맘에들어요 제가 좋아하는것이기도하구요",
          "speaker_id": "2",
          "time": "20210902 06:42"
        },
        {
          "id": "MDRW2100026246.1.4",
          "form": "직장 사람때문에 스트레스 많이 받으세요??",
          "original_form": "직장 사람때문에 스트레스 많이 받으세요??",
          "speaker_id": "1",
          "time": "20210902 09:16"
        },
        {
          "id": "MDRW2100026246.1.5",
          "form": "맞아요 정말 안맞아요 ㅠㅠ",
          "original_form": "맞아요 정말 안맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 09:19"
        },
        {
          "id": "MDRW2100026246.1.6",
          "form": "일도 일이지만 같이 일하는 사람이 진짜 중요한거같아요ㅜㅜ",
          "original_form": "일도 일이지만 같이 일하는 사람이 진짜 중요한거같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100026246.1.7",
          "form": "맞아요 진짜 속터져서 하루하루 스트레스 축척이랍니당...ㅎ",
          "original_form": "맞아요 진짜 속터져서 하루하루 스트레스 축척이랍니당...ㅎ",
          "speaker_id": "2",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100026246.1.8",
          "form": "저는 아직 취업 전이라 취업 후 걱정이네요ㅜㅜㅜ",
          "original_form": "저는 아직 취업 전이라 취업 후 걱정이네요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100026246.1.9",
          "form": "진짜 잘 골라야하는거같아 ㅛ",
          "original_form": "진짜 잘 골라야하는거같아 ㅛ",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100026246.1.10",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100026246.1.11",
          "form": "저처럼 이러면",
          "original_form": "저처럼 이러면",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100026246.1.12",
          "form": "퇴근후 맥주 마렵습니다..ㅎ",
          "original_form": "퇴근후 맥주 마렵습니다..ㅎ",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100026246.1.13",
          "form": "하루의 낙이네요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "하루의 낙이네요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100026246.1.14",
          "form": "이직 생각은 없으세요??ㅌ",
          "original_form": "이직 생각은 없으세요??ㅌ",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100026246.1.15",
          "form": "오늘은 좀 갠춘하네요",
          "original_form": "오늘은 좀 갠춘하네요",
          "speaker_id": "2",
          "time": "20210902 11:11"
        },
        {
          "id": "MDRW2100026246.1.16",
          "form": "내일은 마시고..ㅎ",
          "original_form": "내일은 마시고..ㅎ",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100026246.1.17",
          "form": "ㅋㅋㅋㅋ내일 금요일이니까요!",
          "original_form": "ㅋㅋㅋㅋ내일 금요일이니까요!",
