      "utterance": [
        {
          "id": "MDRW2100018607.1.1",
          "form": "오늘 그쪽 지역 날씨 어떠신가요",
          "original_form": "오늘 그쪽 지역 날씨 어떠신가요",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018607.1.2",
          "form": "저희 지역은 조금 습하네요",
          "original_form": "저희 지역은 조금 습하네요",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018607.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018607.1.4",
          "form": "이쪽은 비가 오다가 안오다가 하네요",
          "original_form": "이쪽은 비가 오다가 안오다가 하네요",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018607.1.5",
          "form": "가을장마라는데 장마가 너무 늦게 오는거같아요",
          "original_form": "가을장마라는데 장마가 너무 늦게 오는거같아요",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018607.1.6",
          "form": "그러게요. 요즘 우산 필수죠 일기예보도 예측 못하게 비가 내리니까",
          "original_form": "그러게요. 요즘 우산 필수죠 일기예보도 예측 못하게 비가 내리니까",
          "speaker_id": "1",
          "time": "20210827 09:09"
        },
        {
          "id": "MDRW2100018607.1.7",
          "form": "어제 밤에 비 안온다고 했는데 비가 조금 와서 더 습해졌더라고요",
          "original_form": "어제 밤에 비 안온다고 했는데 비가 조금 와서 더 습해졌더라고요",
          "speaker_id": "1",
          "time": "20210827 09:09"
        },
        {
          "id": "MDRW2100018607.1.8",
          "form": "휴가기간인데 날씨가 별로라 외출할 생각도 안드는거같아요 ㅠ",
          "original_form": "휴가기간인데 날씨가 별로라 외출할 생각도 안드는거같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 09:52"
        },
        {
          "id": "MDRW2100018607.1.9",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "2",
          "time": "20210827 09:52"
        },
        {
          "id": "MDRW2100018607.1.10",
          "form": "그쵸 코로나도 기승이고 날씨도 별로고 그러네요..\n저는 가을이 좋아요!",
          "original_form": "그쵸 코로나도 기승이고 날씨도 별로고 그러네요..\n저는 가을이 좋아요!",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100018607.1.11",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100018607.1.12",
          "form": "저는 겨울 좋아해요!",
          "original_form": "저는 겨울 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100018607.1.13",
          "form": "습한 것보다는 건조한게 나은거같아요",
          "original_form": "습한 것보다는 건조한게 나은거같아요",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100018607.1.14",
          "form": "와 저도 완전 공감이요",
          "original_form": "와 저도 완전 공감이요",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100018607.1.15",
          "form": "저는 올해 여름이 덥긴한데 괜찮았던거는",
          "original_form": "저는 올해 여름이 덥긴한데 괜찮았던거는",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100018607.1.16",
          "form": "많이 습하지 않아서 좋았어요",
          "original_form": "많이 습하지 않아서 좋았어요",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100018607.1.17",
          "form": "그쵸 너무 더워서",
          "original_form": "그쵸 너무 더워서",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100018607.1.18",
          "form": "습기까지 바짝 마른 느낌이었어요 ㅋㅋㅋㅋ",
          "original_form": "습기까지 바짝 마른 느낌이었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100018607.1.19",
          "form": "맞아요 그 비유가 딱이네요",
          "original_form": "맞아요 그 비유가 딱이네요",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100018607.1.20",
          "form": "가을장마가 지나면 이제 추워지겠죠",
          "original_form": "가을장마가 지나면 이제 추워지겠죠",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100018607.1.21",
          "form": "오늘 비오고 그쳤는데 너무 시원해서 걸어다셨어요",
          "original_form": "오늘 비오고 그쳤는데 너무 시원해서 걸어다셨어요",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100018607.1.22",
          "form": "맞아요 오늘 벌써 서늘하더라구요",
          "original_form": "맞아요 오늘 벌써 서늘하더라구요",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100018607.1.23",
          "form": "그쵸 슬금슬금 긴팔을 꺼낼 시기가 온거죠",
          "original_form": "그쵸 슬금슬금 긴팔을 꺼낼 시기가 온거죠",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100018607.1.24",
          "form": "얼른 가을 왔으면 좋겠네요",
          "original_form": "얼른 가을 왔으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100018607.1.25",
          "form": "가을은 뭔가 산책하기 좋은 날씨여서 기다려지는것같아요",
          "original_form": "가을은 뭔가 산책하기 좋은 날씨여서 기다려지는것같아요",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100018607.1.26",
          "form": "땀이 날 것 같으면서 시원하게 말려주니까 그게 너무 좋더라고요",
          "original_form": "땀이 날 것 같으면서 시원하게 말려주니까 그게 너무 좋더라고요",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100018607.1.27",
          "form": "맞아요 봄이랑 가을이 적당히 좋아요",
          "original_form": "맞아요 봄이랑 가을이 적당히 좋아요",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100018607.1.28",
          "form": "요즘은 갈수록 짧아지는거같아서 아쉽지만요",
          "original_form": "요즘은 갈수록 짧아지는거같아서 아쉽지만요",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100018607.1.29",
          "form": "그러게요..",
          "original_form": "그러게요..",
          "speaker_id": "1",
          "time": "20210827 23:22"
        }
      ]
    }
  ]
