        },
        {
          "id": "MDRW2100009270.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009270.1.30",
