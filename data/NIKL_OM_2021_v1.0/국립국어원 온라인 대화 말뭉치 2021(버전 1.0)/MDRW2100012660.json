      "utterance": [
        {
          "id": "MDRW2100012660.1.1",
          "form": "중고거래 해보신적 있으세요?",
          "original_form": "중고거래 해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012660.1.2",
          "form": "중고거래 어렸을 때 많이 해봤어요",
          "original_form": "중고거래 어렸을 때 많이 해봤어요",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012660.1.3",
          "form": "소설책을 중고고 많이 샀었거든요",
          "original_form": "소설책을 중고고 많이 샀었거든요",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012660.1.4",
          "form": "사기당한적은 없으세요 ㅠ?",
          "original_form": "사기당한적은 없으세요 ㅠ?",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012660.1.5",
          "form": "다행이도 아직 사기 당한적은 없어요",
          "original_form": "다행이도 아직 사기 당한적은 없어요",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012660.1.6",
          "form": "ㅠ ㅠ 저도 중고거래 즐겨 했었는데 얼마전에 사기당해서 무서워졌어요",
          "original_form": "ㅠ ㅠ 저도 중고거래 즐겨 했었는데 얼마전에 사기당해서 무서워졌어요",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012660.1.7",
          "form": "아 ㅜ ㅜ 많이 당혹스러웠겠어요",
          "original_form": "아 ㅜ ㅜ 많이 당혹스러웠겠어요",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012660.1.8",
          "form": "해결이 됐나요??",
          "original_form": "해결이 됐나요??",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012660.1.9",
          "form": "아뇨 금액이 큰편이아니라서 딱히 경찰에 신고하기도 그렇고 그냥 똥 밟았다 생각하고 말았습니다 ㅠ",
          "original_form": "아뇨 금액이 큰편이아니라서 딱히 경찰에 신고하기도 그렇고 그냥 똥 밟았다 생각하고 말았습니다 ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100012660.1.10",
          "form": "ㅜ ㅜ 사기꾼때문에 중고거래가 무서워지다니 정말 안타까워요",
          "original_form": "ㅜ ㅜ 사기꾼때문에 중고거래가 무서워지다니 정말 안타까워요",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012660.1.11",
          "form": "사는건 안하고 파는것만 하려구요 ㅠ ㅠ",
          "original_form": "사는건 안하고 파는것만 하려구요 ㅠ ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012660.1.12",
          "form": "그래요 그래요 ㅜ",
          "original_form": "그래요 그래요 ㅜ",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012660.1.13",
          "form": "원래 중고거래는 어딜 통해서 주로했나요?",
          "original_form": "원래 중고거래는 어딜 통해서 주로했나요?",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012660.1.14",
          "form": "저는 네이버 중고나라에서 많이 했는데",
          "original_form": "저는 네이버 중고나라에서 많이 했는데",
          "speaker_id": "2",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100012660.1.15",
          "form": "당근마켓이나 번개장터요 ㅋㅋ",
          "original_form": "당근마켓이나 번개장터요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012660.1.16",
          "form": "오 저 아직 한번도 당근 안써봐서 궁금해요",
          "original_form": "오 저 아직 한번도 당근 안써봐서 궁금해요",
          "speaker_id": "2",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012660.1.17",
          "form": "중고거래는 하도 사기가 판치는거 같아서 웬만하면 직거래로 하려고 하는 편인데 이번엔 기프티콘 사려고 돈 입금했다가 막상 쓰려고 보니까 이미 사용한거래서 멘붕왔어요 ㅋㅋㄱ",
          "original_form": "중고거래는 하도 사기가 판치는거 같아서 웬만하면 직거래로 하려고 하는 편인데 이번엔 기프티콘 사려고 돈 입금했다가 막상 쓰려고 보니까 이미 사용한거래서 멘붕왔어요 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012660.1.18",
          "form": "아 뭔가 착오가 있었던 건 아닐까요?",
          "original_form": "아 뭔가 착오가 있었던 건 아닐까요?",
          "speaker_id": "2",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012660.1.19",
          "form": "판매자와 연락이 끊겼나요?",
          "original_form": "판매자와 연락이 끊겼나요?",
          "speaker_id": "2",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012660.1.20",
          "form": "네 보니까 지금까지도 안읽네요 ㅠ ㅋㅋㅋㄱ",
          "original_form": "네 보니까 지금까지도 안읽네요 ㅠ ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100012660.1.21",
          "form": "아휴 왜그럴까요 ㅜ",
          "original_form": "아휴 왜그럴까요 ㅜ",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012660.1.22",
          "form": "그런식으로 남의 돈 야금야금 뜯어먹고 사나봐요 ㅠ",
          "original_form": "그런식으로 남의 돈 야금야금 뜯어먹고 사나봐요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100012660.1.23",
          "form": "ㅜ 얼른 잊어버리시는 게 상책이겠네요",
          "original_form": "ㅜ 얼른 잊어버리시는 게 상책이겠네요",
