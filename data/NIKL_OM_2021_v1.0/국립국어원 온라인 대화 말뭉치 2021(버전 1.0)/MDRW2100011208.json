      "utterance": [
        {
          "id": "MDRW2100011208.1.1",
          "form": "저희 선풍기 춥단얘기했어요 히히",
          "original_form": "저희 선풍기 춥단얘기했어요 히히",
          "speaker_id": "1",
          "time": "20210824 07:38"
        },
        {
          "id": "MDRW2100011208.1.2",
          "form": "대면 한다고 하나유?!",
          "original_form": "대면 한다고 하나유?!",
          "speaker_id": "1",
          "time": "20210824 07:38"
        },
        {
          "id": "MDRW2100011208.1.3",
          "form": "아 선풍기ㅋㅋㅋㅋ",
          "original_form": "아 선풍기ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:45"
        },
        {
          "id": "MDRW2100011208.1.4",
          "form": "4단계 내려가면 대면할 것 같아여",
          "original_form": "4단계 내려가면 대면할 것 같아여",
          "speaker_id": "2",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100011208.1.5",
          "form": "아이고아이고....",
          "original_form": "아이고아이고....",
          "speaker_id": "1",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100011208.1.6",
          "form": "언제까지 비대면을 해야하는지ㅜㅜ",
          "original_form": "언제까지 비대면을 해야하는지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100011208.1.7",
          "form": "완전 사이버대학이죠",
          "original_form": "완전 사이버대학이죠",
          "speaker_id": "1",
          "time": "20210824 16:57"
        },
        {
          "id": "MDRW2100011208.1.8",
          "form": "맞아요ㅋㅋㅋㅋ ㅠ",
          "original_form": "맞아요ㅋㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100011208.1.9",
          "form": "저희도 보니까 비대면 하다가 대면 돌릴수도 있는것처럼 말하던데",
          "original_form": "저희도 보니까 비대면 하다가 대면 돌릴수도 있는것처럼 말하던데",
          "speaker_id": "1",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100011208.1.10",
          "form": "대면 생각 안하고 시간표 짜신 분들은 난리났더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "대면 생각 안하고 시간표 짜신 분들은 난리났더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100011208.1.11",
          "form": "아이고 백신 맞고 잠이 많아져서 답을 못했네요...",
          "original_form": "아이고 백신 맞고 잠이 많아져서 답을 못했네요...",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100011208.1.12",
          "form": "맞아요... 저는 대면할까봐 알바도 아직 못 잡았어요",
          "original_form": "맞아요... 저는 대면할까봐 알바도 아직 못 잡았어요",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100011208.1.13",
          "form": "아이고아이고 그럴 수 있겠어요",
          "original_form": "아이고아이고 그럴 수 있겠어요",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100011208.1.14",
          "form": "백신 어떤거 맞으셨나유",
          "original_form": "백신 어떤거 맞으셨나유",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100011208.1.15",
          "form": "화이자요!",
          "original_form": "화이자요!",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100011208.1.16",
          "form": "오오 2차 조심하세요진짜 부작용 많이온대요",
          "original_form": "오오 2차 조심하세요진짜 부작용 많이온대요",
          "speaker_id": "1",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100011208.1.17",
          "form": "지금 웬만한 학교는 다 비대면이네욬ㅋ큐ㅠ",
          "original_form": "지금 웬만한 학교는 다 비대면이네욬ㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210828 09:45"
        },
        {
          "id": "MDRW2100011208.1.18",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100011208.1.19",
          "form": "비대면 하면 등록금 너무 아까워요",
          "original_form": "비대면 하면 등록금 너무 아까워요",
          "speaker_id": "2",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100011208.1.20",
          "form": "맞아요 진짜 등록금 너무아깝더라구요",
          "original_form": "맞아요 진짜 등록금 너무아깝더라구요",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100011208.1.21",
          "form": "이게 사이버대학이지...",
          "original_form": "이게 사이버대학이지...",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100011208.1.22",
          "form": "별 생각 없었는데",
          "original_form": "별 생각 없었는데",
          "speaker_id": "2",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100011208.1.23",
          "form": "원래 축제 같은 걸로 등록금 뽕 뽑는 건데",
          "original_form": "원래 축제 같은 걸로 등록금 뽕 뽑는 건데",
          "speaker_id": "2",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100011208.1.24",
          "form": "그것도 안 하니ㅜ 강탈당하는 기분이에요",
          "original_form": "그것도 안 하니ㅜ 강탈당하는 기분이에요",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100011208.1.25",
          "form": "와 그러네요 축제도 안하는구나",
          "original_form": "와 그러네요 축제도 안하는구나",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100011208.1.26",
          "form": "이게 대학교냐...!!!!!",
          "original_form": "이게 대학교냐...!!!!!",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100011208.1.27",
          "form": "ㅋㅋㅋㅋㅋ진짜요ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ진짜요ㅜ",
          "speaker_id": "2",
          "time": "20210828 21:53"
        }
      ]
