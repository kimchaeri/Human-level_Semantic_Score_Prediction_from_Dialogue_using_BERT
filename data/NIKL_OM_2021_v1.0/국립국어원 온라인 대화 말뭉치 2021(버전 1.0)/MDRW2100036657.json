        },
        {
          "id": "MDRW2100036657.1.2",
          "form": "최근 구입한거 있나요?",
          "original_form": "최근 구입한거 있나요?",
          "speaker_id": "1",
          "time": "20210831 04:32"
        },
        {
          "id": "MDRW2100036657.1.3",
          "form": "양말사써욧",
          "original_form": "양말사써욧",
          "speaker_id": "2",
          "time": "20210831 04:32"
        },
        {
          "id": "MDRW2100036657.1.4",
          "form": "어떤 양말이요?",
          "original_form": "어떤 양말이요?",
          "speaker_id": "1",
          "time": "20210831 04:32"
        },
        {
          "id": "MDRW2100036657.1.5",
          "form": "검은양말이욧",
          "original_form": "검은양말이욧",
          "speaker_id": "2",
          "time": "20210831 04:33"
        },
        {
          "id": "MDRW2100036657.1.6",
          "form": "질은 좋나요?",
          "original_form": "질은 좋나요?",
          "speaker_id": "1",
          "time": "20210831 04:33"
        },
        {
          "id": "MDRW2100036657.1.7",
          "form": "좋대욥",
          "original_form": "좋대욥",
          "speaker_id": "2",
          "time": "20210831 04:34"
        },
        {
          "id": "MDRW2100036657.1.8",
          "form": "언제 올까요?",
          "original_form": "언제 올까요?",
          "speaker_id": "1",
          "time": "20210831 04:34"
        },
        {
          "id": "MDRW2100036657.1.9",
          "form": "곧 와욥",
          "original_form": "곧 와욥",
          "speaker_id": "2",
          "time": "20210831 04:34"
        },
        {
          "id": "MDRW2100036657.1.10",
          "form": "어떤 무늬죠?",
          "original_form": "어떤 무늬죠?",
          "speaker_id": "1",
          "time": "20210831 04:34"
        },
        {
          "id": "MDRW2100036657.1.11",
          "form": "검은바탕에 공룡이 형광으로 그려져잇어욥",
          "original_form": "검은바탕에 공룡이 형광으로 그려져잇어욥",
          "speaker_id": "2",
          "time": "20210831 04:35"
        },
        {
          "id": "MDRW2100036657.1.12",
          "form": "공룡 좋아하시나보다",
