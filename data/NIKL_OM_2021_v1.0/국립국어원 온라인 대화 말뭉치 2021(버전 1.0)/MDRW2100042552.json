      "utterance": [
        {
          "id": "MDRW2100042552.1.1",
          "form": "혹시 학생때 좋아하던 과목이 있으신가요?",
          "original_form": "혹시 학생때 좋아하던 과목이 있으신가요?",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042552.1.2",
          "form": "전 공부랑 크게 관련이없었지만 그나마 국어랑 지구과학좋아했어요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 공부랑 크게 관련이없었지만 그나마 국어랑 지구과학좋아했어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042552.1.3",
          "form": "아 ㅎㅎ혹시 이과셨나요?",
          "original_form": "아 ㅎㅎ혹시 이과셨나요?",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042552.1.4",
          "form": "국어는 문학소설읽는거재밌었고 지구과학은 천체를 좋아했었어서",
          "original_form": "국어는 문학소설읽는거재밌었고 지구과학은 천체를 좋아했었어서",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042552.1.5",
          "form": "네..수포자이지만 이과였습니당",
          "original_form": "네..수포자이지만 이과였습니당",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042552.1.6",
          "form": "name2님은여?",
          "original_form": "&name2&님은여?",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042552.1.7",
          "form": "아 ㅎㅎㅎ그러시군요!",
          "original_form": "아 ㅎㅎㅎ그러시군요!",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042552.1.8",
          "form": "항상 이과친구들은 본인들을 수포자라고 하더라구요 ㅎㅎㅎㅎㅎ",
          "original_form": "항상 이과친구들은 본인들을 수포자라고 하더라구요 ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042552.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ사실이니까여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ사실이니까여",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042552.1.10",
          "form": "저는 수학이랑 영어 좋아했어요!",
          "original_form": "저는 수학이랑 영어 좋아했어요!",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042552.1.11",
          "form": "저 수학 공부 더럽게안했어요",
          "original_form": "저 수학 공부 더럽게안했어요",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042552.1.12",
          "form": "그치만 문과를 가서 수포자가 될수없었습니다 ㅋㅋㅋ",
          "original_form": "그치만 문과를 가서 수포자가 될수없었습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042552.1.13",
          "form": "오 수학좋아하셨구나 이과친구들이랑거보니 문과셨거",
          "original_form": "오 수학좋아하셨구나 이과친구들이랑거보니 문과셨거",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042552.1.14",
          "form": "ㅋㅋㅋㅋ수학영어좋아하면 공부좀 하신거같은데여",
          "original_form": "ㅋㅋㅋㅋ수학영어좋아하면 공부좀 하신거같은데여",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042552.1.15",
          "form": "앗ㅎㅎㅎ 그렇네요 예리하시군요!",
          "original_form": "앗ㅎㅎㅎ 그렇네요 예리하시군요!",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042552.1.16",
          "form": "ㅎㅎ앗 아니에욥 저는 근데",
          "original_form": "ㅎㅎ앗 아니에욥 저는 근데",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042552.1.17",
          "form": "국어를 지지리도 못해서 ㅋㅋㅋㅋ담임쌤이",
          "original_form": "국어를 지지리도 못해서 ㅋㅋㅋㅋ담임쌤이",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042552.1.18",
          "form": "딱 공부잘하는애들이 수학영어 좋아하드라구여",
          "original_form": "딱 공부잘하는애들이 수학영어 좋아하드라구여",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042552.1.19",
          "form": "왜 문과왔냐고 하실정도였어요...ㅎ..",
          "original_form": "왜 문과왔냐고 하실정도였어요...ㅎ..",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042552.1.20",
          "form": "ㅎ...전 이과왜왔냐들었었는데",
          "original_form": "ㅎ...전 이과왜왔냐들었었는데",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042552.1.21",
          "form": "이상하게 통했네여 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이상하게 통했네여 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:53"
        }
      ]
    }
