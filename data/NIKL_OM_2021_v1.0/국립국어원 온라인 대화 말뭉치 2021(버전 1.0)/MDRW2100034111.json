      "utterance": [
        {
          "id": "MDRW2100034111.1.1",
          "form": "그래도 젤 무거운거 들어주네요",
          "original_form": "그래도 젤 무거운거 들어주네요",
          "speaker_id": "2",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100034111.1.2",
          "form": "웬수에요ㅋㅋ저는저런 아들을 둔적이 없어요",
          "original_form": "웬수에요ㅋㅋ저는저런 아들을 둔적이 없어요",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100034111.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100034111.1.4",
          "form": "내아들이었으면 콱",
          "original_form": "내아들이었으면 콱",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100034111.1.5",
          "form": "ㅋㅋ이럴수ㅋㅋㅋㅋ고생이 많으시네요",
          "original_form": "ㅋㅋ이럴수ㅋㅋㅋㅋ고생이 많으시네요",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100034111.1.6",
          "form": "의리로 삽니다",
          "original_form": "의리로 삽니다",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100034111.1.7",
          "form": "ㅋㅋ애들이 있어서",
          "original_form": "ㅋㅋ애들이 있어서",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100034111.1.8",
          "form": "도원결의까진아니지만 인내를 쌓고계시군요",
          "original_form": "도원결의까진아니지만 인내를 쌓고계시군요",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100034111.1.9",
          "form": "아 맞아요ㅠㅠ 하 인내 인내",
          "original_form": "아 맞아요ㅠㅠ 하 인내 인내",
          "speaker_id": "1",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100034111.1.10",
          "form": "ㅋㅋㅋ빨리 애들이 커서 엄마 도와주면 좋겠네요",
          "original_form": "ㅋㅋㅋ빨리 애들이 커서 엄마 도와주면 좋겠네요",
          "speaker_id": "2",
          "time": "20210827 19:04"
        },
        {
          "id": "MDRW2100034111.1.11",
          "form": "도움까지는 안바라고ㅋㅋ 치대는것만 덜해줘도ㅎㅎ 감사하죠",
          "original_form": "도움까지는 안바라고ㅋㅋ 치대는것만 덜해줘도ㅎㅎ 감사하죠",
          "speaker_id": "1",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100034111.1.12",
          "form": "ㅋㅋㅋㅋ 딱 교육시겨야됴",
          "original_form": "ㅋㅋㅋㅋ 딱 교육시겨야됴",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100034111.1.13",
          "form": "설거지 방청소",
          "original_form": "설거지 방청소",
          "speaker_id": "2",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100034111.1.14",
          "form": "엄마가 다해주면 안돼요 ㅋㅋ",
          "original_form": "엄마가 다해주면 안돼요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100034111.1.15",
          "form": "아직 둘째가3살이라",
          "original_form": "아직 둘째가3살이라",
          "speaker_id": "1",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100034111.1.16",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100034111.1.17",
          "form": "손이많이가요ㅠㅠ",
          "original_form": "손이많이가요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100034111.1.18",
          "form": "큰애부터 딱 잡아야죠",
          "original_form": "큰애부터 딱 잡아야죠",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100034111.1.19",
          "form": "큰애는초3학년인데 책상정리하라하라하면 억지로 대충 넣고..엉망ㅠ",
          "original_form": "큰애는초3학년인데 책상정리하라하라하면 억지로 대충 넣고..엉망ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100034111.1.20",
          "form": "ㅋㅋㅋ아유 쉽지가않네요",
          "original_form": "ㅋㅋㅋ아유 쉽지가않네요",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100034111.1.21",
          "form": "원래 어릴때는 다그런거지요",
          "original_form": "원래 어릴때는 다그런거지요",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100034111.1.22",
          "form": "근데 고등학교 가서도 엄마가 똥닦아주는 아들두있대요",
          "original_form": "근데 고등학교 가서도 엄마가 똥닦아주는 아들두있대요",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100034111.1.23",
          "form": "스스로하는 버릇만들어야한대요",
          "original_form": "스스로하는 버릇만들어야한대요",
          "speaker_id": "2",
          "time": "20210827 19:54"
        },
        {
          "id": "MDRW2100034111.1.24",
