      "utterance": [
        {
          "id": "MDRW2100026779.1.1",
          "form": "아악 기기분석 어렵다하더라고요",
          "original_form": "아악 기기분석 어렵다하더라고요",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026779.1.2",
          "form": "철회도 거의 20프로하고",
          "original_form": "철회도 거의 20프로하고",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026779.1.3",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026779.1.4",
          "form": "그래서 시험 한 번 보고나면",
          "original_form": "그래서 시험 한 번 보고나면",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026779.1.5",
          "form": "점수가 극단적이에요!!!!",
          "original_form": "점수가 극단적이에요!!!!",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026779.1.6",
          "form": "아악",
          "original_form": "아악",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026779.1.7",
          "form": "에이마도 받기힘들까요",
          "original_form": "에이마도 받기힘들까요",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026779.1.8",
          "form": "저는 그 학기에 중도휴학을 했지만",
          "original_form": "저는 그 학기에 중도휴학을 했지만",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026779.1.9",
          "form": "전공 2개들으니까 몰빵공부 가능하지않을까요",
          "original_form": "전공 2개들으니까 몰빵공부 가능하지않을까요",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026779.1.10",
          "form": "중간고사에서 딱 평균이었어요 ㅎ",
          "original_form": "중간고사에서 딱 평균이었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026779.1.11",
          "form": "흑...",
          "original_form": "흑...",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026779.1.12",
          "form": "이번학기 두렵네여 정말",
          "original_form": "이번학기 두렵네여 정말",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100026779.1.13",
          "form": "음음",
          "original_form": "음음",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026779.1.14",
          "form": "아 전공 2개면 가능할지도 모르겠네요!!!",
          "original_form": "아 전공 2개면 가능할지도 모르겠네요!!!",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026779.1.15",
          "form": "그렇다면.... 기기분석을 듣는다?!",
          "original_form": "그렇다면.... 기기분석을 듣는다?!",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026779.1.16",
          "form": "화실기 안듣는다면요ㅋㅋ",
          "original_form": "화실기 안듣는다면요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026779.1.17",
          "form": "기기분석 아빠의 추천이었는데....",
          "original_form": "기기분석 아빠의 추천이었는데....",
