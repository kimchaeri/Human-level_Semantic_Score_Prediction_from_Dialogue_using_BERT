      "utterance": [
        {
          "id": "MDRW2100034883.1.1",
          "form": "개훌륭이요 님은요?",
          "original_form": "개훌륭이요 님은요?",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034883.1.2",
          "form": "그거 인기 많나봐요 많이들 보시네요",
          "original_form": "그거 인기 많나봐요 많이들 보시네요",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100034883.1.3",
          "form": "개 좋아하면 다들 보죠 ㅋㅋ",
          "original_form": "개 좋아하면 다들 보죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100034883.1.4",
          "form": "그 분이 그렇게 대단하다면서요~",
          "original_form": "그 분이 그렇게 대단하다면서요~",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034883.1.5",
          "form": "실력있어 보여요 잘가르치던",
          "original_form": "실력있어 보여요 잘가르치던",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034883.1.6",
          "form": "하지만 전 개를 안키우니깐 ㅋㅋ보듬교육에 돈내고 가진 않아서 잘은 몰라요",
          "original_form": "하지만 전 개를 안키우니깐 ㅋㅋ보듬교육에 돈내고 가진 않아서 잘은 몰라요",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034883.1.7",
          "form": "아~ 개 키우시는 줄 알았는데 아니시군요",
          "original_form": "아~ 개 키우시는 줄 알았는데 아니시군요",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100034883.1.8",
          "form": "네 아니에요 ㅋㅋ키우시나요?",
          "original_form": "네 아니에요 ㅋㅋ키우시나요?",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034883.1.9",
          "form": "아뇨 저도 안키워요 개 키우려면 부지런 해야하잖아요",
          "original_form": "아뇨 저도 안키워요 개 키우려면 부지런 해야하잖아요",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100034883.1.10",
          "form": "ㅠ그리고 돈도 드니깐",
          "original_form": "ㅠ그리고 돈도 드니깐",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100034883.1.11",
          "form": "예능에서 열심히 봐요 ㅋㅋㅋ",
          "original_form": "예능에서 열심히 봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100034883.1.12",
          "form": "님은 뭐 보세요?",
          "original_form": "님은 뭐 보세요?",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100034883.1.13",
          "form": "저도 예능 좋아해요 오늘은 놀뭐 봤어요",
          "original_form": "저도 예능 좋아해요 오늘은 놀뭐 봤어요",
          "speaker_id": "1",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100034883.1.14",
          "form": "아 유재석..",
          "original_form": "아 유재석..",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100034883.1.15",
          "form": "전 놀뭐는 안봐요 안맞더라구요",
          "original_form": "전 놀뭐는 안봐요 안맞더라구요",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100034883.1.16",
          "form": "이효리 나올때 좀 봤어요",
          "original_form": "이효리 나올때 좀 봤어요",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100034883.1.17",
          "form": "이효리 좋아하시는군요?",
          "original_form": "이효리 좋아하시는군요?",
          "speaker_id": "1",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100034883.1.18",
          "form": "이효리도 좋아하고 개를 좋아하니깐",
          "original_form": "이효리도 좋아하고 개를 좋아하니깐",
          "speaker_id": "2",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100034883.1.19",
          "form": "개때문에도 가끔 봤어요",
          "original_form": "개때문에도 가끔 봤어요",
          "speaker_id": "2",
          "time": "20210828 22:04"
        }
      ]
