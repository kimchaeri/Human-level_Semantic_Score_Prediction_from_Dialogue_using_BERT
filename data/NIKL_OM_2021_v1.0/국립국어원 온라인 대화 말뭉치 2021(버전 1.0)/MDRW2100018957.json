      "utterance": [
        {
          "id": "MDRW2100018957.1.1",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018957.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018957.1.3",
          "form": "점심 맛있게 드셧어요??",
          "original_form": "점심 맛있게 드셧어요??",
          "speaker_id": "1",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018957.1.4",
          "form": "네 전 돈까스 먹었어요",
          "original_form": "네 전 돈까스 먹었어요",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018957.1.5",
          "form": "점심 어떤거 드셨나요?",
          "original_form": "점심 어떤거 드셨나요?",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018957.1.6",
          "form": "오 맛잇었겟네요.저는 비빔국수에 오리고기 곁들여서 먹었어요",
          "original_form": "오 맛잇었겟네요.저는 비빔국수에 오리고기 곁들여서 먹었어요",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018957.1.7",
          "form": "비빔국수 좋죠",
          "original_form": "비빔국수 좋죠",
          "speaker_id": "2",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018957.1.8",
          "form": "오늘 날씨가 흐려서 매콤한 음식이 좋은거같아요",
          "original_form": "오늘 날씨가 흐려서 매콤한 음식이 좋은거같아요",
          "speaker_id": "2",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018957.1.9",
          "form": "ㅋㅋ 그런데 양념에서 겨자가 너무 과해서 속이안좋네요",
          "original_form": "ㅋㅋ 그런데 양념에서 겨자가 너무 과해서 속이안좋네요",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018957.1.10",
          "form": "배달하셨나요?",
          "original_form": "배달하셨나요?",
          "speaker_id": "2",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018957.1.11",
          "form": "아뇨 ㅋㅋ그냥 집에서 만들어먹었어요 ㅎㅎ",
          "original_form": "아뇨 ㅋㅋ그냥 집에서 만들어먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018957.1.12",
          "form": "돈까스는 배달시켜드신거엿어요??",
          "original_form": "돈까스는 배달시켜드신거엿어요??",
          "speaker_id": "1",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018957.1.13",
          "form": "전 배달시켰는데 생각보다 늦게와서 좀 급하게 먹었어요",
          "original_form": "전 배달시켰는데 생각보다 늦게와서 좀 급하게 먹었어요",
          "speaker_id": "2",
          "time": "20210827 13:59"
        },
        {
          "id": "MDRW2100018957.1.14",
          "form": "배달음식 시키는 것도 일인거같아요...",
          "original_form": "배달음식 시키는 것도 일인거같아요...",
          "speaker_id": "2",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018957.1.15",
          "form": "ㅜ그러시군요ㅜ",
          "original_form": "ㅜ그러시군요ㅜ",
          "speaker_id": "1",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018957.1.16",
          "form": "맛집인가봐요..",
          "original_form": "맛집인가봐요..",
          "speaker_id": "1",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018957.1.17",
          "form": "네 그래서 단골 됐어요",
          "original_form": "네 그래서 단골 됐어요",
          "speaker_id": "2",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100018957.1.18",
          "form": "돈까스 시키면 와사비도 같이 주는데 은근히 잘어울리더라구요",
          "original_form": "돈까스 시키면 와사비도 같이 주는데 은근히 잘어울리더라구요",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018957.1.19",
          "form": "기본 돈까스인건가요??",
          "original_form": "기본 돈까스인건가요??",
          "speaker_id": "1",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018957.1.20",
          "form": "오 일식느낌인가봐요",
          "original_form": "오 일식느낌인가봐요",
          "speaker_id": "1",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018957.1.21",
          "form": "네 등심이요",
          "original_form": "네 등심이요",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018957.1.22",
          "form": "저는 경양식돈까스 소스같은거 좋아하거든요",
          "original_form": "저는 경양식돈까스 소스같은거 좋아하거든요",
          "speaker_id": "1",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018957.1.23",
          "form": "튀김옷이 끝내줬을것같아요.",
          "original_form": "튀김옷이 끝내줬을것같아요.",
          "speaker_id": "1",
          "time": "20210827 14:01"
        }
      ]
