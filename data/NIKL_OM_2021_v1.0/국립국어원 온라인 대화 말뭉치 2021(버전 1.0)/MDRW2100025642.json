        },
        {
          "id": "MDRW2100025642.1.2",
          "form": "즐거운걷기였나?",
          "original_form": "즐거운걷기였나?",
          "speaker_id": "1",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100025642.1.3",
          "form": "아 그거",
          "original_form": "아 그거",
          "speaker_id": "2",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025642.1.4",
          "form": "슈퍼 가서 살게 있어서 나거는 김에 켜봤음",
          "original_form": "슈퍼 가서 살게 있어서 나거는 김에 켜봤음",
          "speaker_id": "2",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025642.1.5",
          "form": "나가는",
          "original_form": "나가는",
          "speaker_id": "2",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025642.1.6",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025642.1.7",
          "form": "ㅇ엉ㅇㅇ",
          "original_form": "ㅇ엉ㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025642.1.8",
          "form": "야 변함없이 시끄럽더라",
          "original_form": "야 변함없이 시끄럽더라",
          "speaker_id": "2",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025642.1.9",
          "form": "ㄹ달리기랑",
          "original_form": "ㄹ달리기랑",
          "speaker_id": "1",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025642.1.10",
          "form": "비슷해?",
          "original_form": "비슷해?",
          "speaker_id": "1",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025642.1.11",
          "form": "약간 그래도 노래를 좀 더 들을 수 있음",
          "original_form": "약간 그래도 노래를 좀 더 들을 수 있음",
          "speaker_id": "2",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025642.1.12",
          "form": "근데도 시끄러",
          "original_form": "근데도 시끄러",
          "speaker_id": "2",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025642.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ뭐라말해",
