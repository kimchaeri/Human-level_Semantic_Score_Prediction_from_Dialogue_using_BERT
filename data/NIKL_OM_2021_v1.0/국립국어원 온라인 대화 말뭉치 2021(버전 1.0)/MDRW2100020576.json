      "utterance": [
        {
          "id": "MDRW2100020576.1.1",
          "form": "안녕하세요 좋아하는 티비 프로그램 있으신가요?",
          "original_form": "안녕하세요 좋아하는 티비 프로그램 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 10:04"
        },
        {
          "id": "MDRW2100020576.1.2",
          "form": "저 런닝맨 봐요. 예능 주로 보는거 같네요",
          "original_form": "저 런닝맨 봐요. 예능 주로 보는거 같네요",
          "speaker_id": "2",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020576.1.3",
          "form": "오 런닝맨 광수 하차하고나서는 못봤는데",
          "original_form": "오 런닝맨 광수 하차하고나서는 못봤는데",
          "speaker_id": "1",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020576.1.4",
          "form": "여전히 재밌나요?",
          "original_form": "여전히 재밌나요?",
          "speaker_id": "1",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100020576.1.5",
          "form": "네 재밌더라구요",
          "original_form": "네 재밌더라구요",
          "speaker_id": "2",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020576.1.6",
          "form": "영어로 말하는 코너",
          "original_form": "영어로 말하는 코너",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020576.1.7",
          "form": "클립영상으로봤는데",
          "original_form": "클립영상으로봤는데",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020576.1.8",
          "form": "재밌었어요ㅋㅋ",
          "original_form": "재밌었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100020576.1.9",
          "form": "그거 재밌죠ㅋㅋ 의외로 힘든거 같더라구요",
          "original_form": "그거 재밌죠ㅋㅋ 의외로 힘든거 같더라구요",
          "speaker_id": "2",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020576.1.10",
          "form": "보다가 빵터졌어요ㅋㅋㅋㅋ 노 셀럽",
          "original_form": "보다가 빵터졌어요ㅋㅋㅋㅋ 노 셀럽",
          "speaker_id": "1",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020576.1.11",
          "form": "이 부분에서ㅋㅋㅋㅋ",
          "original_form": "이 부분에서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020576.1.12",
          "form": "지석진님한테 c나 d급이래요ㅋㅋㅋ",
          "original_form": "지석진님한테 c나 d급이래요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020576.1.13",
          "form": "ㅋㅋㅋㅋ요새 멤버들이 지석진 놀리는데 신난거 같더라구요",
          "original_form": "ㅋㅋㅋㅋ요새 멤버들이 지석진 놀리는데 신난거 같더라구요",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020576.1.14",
          "form": "나이가 많은데도 어린 연예인들이랑 잘 어울리는거 보면",
          "original_form": "나이가 많은데도 어린 연예인들이랑 잘 어울리는거 보면",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020576.1.15",
          "form": "나이가 중요하진 않은가봐요",
          "original_form": "나이가 중요하진 않은가봐요",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020576.1.16",
          "form": "사람이 중요하지ㅋㅋㅋ",
          "original_form": "사람이 중요하지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020576.1.17",
          "form": "맞아요ㅋㅋ 다들 편하게 대하는 것 같구요",
          "original_form": "맞아요ㅋㅋ 다들 편하게 대하는 것 같구요",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020576.1.18",
          "form": "이제 없으면 아쉬운 멤버에요ㅠㅠ",
          "original_form": "이제 없으면 아쉬운 멤버에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020576.1.19",
          "form": "인기맛에 엄청 열심히 한다니깐 빠지진 않을거 같아요ㅋㅋ",
          "original_form": "인기맛에 엄청 열심히 한다니깐 빠지진 않을거 같아요ㅋㅋ",
