      "utterance": [
        {
          "id": "MDRW2100019534.1.1",
          "form": "치즈케이크 혹시 기회되시면",
          "original_form": "치즈케이크 혹시 기회되시면",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.2",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.3",
          "form": "무너미라는 곳의 케이크를 추천하고 싶어요",
          "original_form": "무너미라는 곳의 케이크를 추천하고 싶어요",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.4",
          "form": "헉 첨들어봐요",
          "original_form": "헉 첨들어봐요",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.5",
          "form": "제가 너무 맛있게 먹어섴ㅋㅋ",
          "original_form": "제가 너무 맛있게 먹어섴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.6",
          "form": "이름이 너무 귀엽네요",
          "original_form": "이름이 너무 귀엽네요",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.7",
          "form": "그 서울 수유동에 있는덴데",
          "original_form": "그 서울 수유동에 있는덴데",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.8",
          "form": "찾아볼게요 무너미!!",
          "original_form": "찾아볼게요 무너미!!",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.9",
          "form": "한번 보세요!ㅋㅋㅋㅋ",
          "original_form": "한번 보세요!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.10",
          "form": "수유동 안가봤는데",
          "original_form": "수유동 안가봤는데",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.11",
          "form": "제가 거기 치케 먹고 치케가 좋아졌어요ㅠㅠ",
          "original_form": "제가 거기 치케 먹고 치케가 좋아졌어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.12",
          "form": "겸사겸사 가봐야겠네여 ㅋㅋㅋ",
          "original_form": "겸사겸사 가봐야겠네여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019534.1.13",
          "form": "헉 ㅠㅠ인생치케인가요?",
          "original_form": "헉 ㅠㅠ인생치케인가요?",
          "speaker_id": "1",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100019534.1.14",
          "form": "음....완전 인생은 아니고 쪼꼼 인생이요...",
          "original_form": "음....완전 인생은 아니고 쪼꼼 인생이요...",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100019534.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100019534.1.16",
          "form": "꽂혀서 삼일 내내 먹었지 뭐에요",
