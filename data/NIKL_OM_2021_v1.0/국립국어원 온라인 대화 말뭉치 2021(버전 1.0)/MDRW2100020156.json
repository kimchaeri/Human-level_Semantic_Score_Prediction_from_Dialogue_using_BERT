      "utterance": [
        {
          "id": "MDRW2100020156.1.1",
          "form": "아이돌 오디션 프로가 한때 유행이었어",
          "original_form": "아이돌 오디션 프로가 한때 유행이었어",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020156.1.2",
          "form": "프로듀스 원오원재밌게 봤어",
          "original_form": "프로듀스 원오원재밌게 봤어",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020156.1.3",
          "form": "거기서 나온 데뷔 조가 정말 인기가 많았던 건 알아",
          "original_form": "거기서 나온 데뷔 조가 정말 인기가 많았던 건 알아",
          "speaker_id": "2",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020156.1.4",
          "form": "아 아이오아이 말하는 거야?",
          "original_form": "아 아이오아이 말하는 거야?",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020156.1.5",
          "form": "맞아 맞아 너도 혹시 좋아했니",
          "original_form": "맞아 맞아 너도 혹시 좋아했니",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020156.1.6",
          "form": "완전 재밌게 봤지",
          "original_form": "완전 재밌게 봤지",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020156.1.7",
          "form": "좋아하는 연예인도 있었니",
          "original_form": "좋아하는 연예인도 있었니",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020156.1.8",
          "form": "내픽은 전소미",
          "original_form": "내픽은 전소미",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020156.1.9",
          "form": "전소미 가 일등이 잖아",
          "original_form": "전소미 가 일등이 잖아",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020156.1.10",
          "form": "너는 누구 투표 했어",
          "original_form": "너는 누구 투표 했어",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020156.1.11",
          "form": "나는 청하를 투표 했어",
          "original_form": "나는 청하를 투표 했어",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020156.1.12",
          "form": "청하 춤 진짜 잘추더라",
          "original_form": "청하 춤 진짜 잘추더라",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020156.1.13",
          "form": "전 소미는 이번 노래가 정말 좋던데",
          "original_form": "전 소미는 이번 노래가 정말 좋던데",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020156.1.14",
          "form": "이번 노래 덤덤 좋더라",
          "original_form": "이번 노래 덤덤 좋더라",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020156.1.15",
          "form": "춤이 꼭 배트맨 같더라",
          "original_form": "춤이 꼭 배트맨 같더라",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020156.1.16",
          "form": "맞아 춤이 재미있는 듯",
          "original_form": "맞아 춤이 재미있는 듯",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020156.1.17",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210828 00:03"
        }
      ]
