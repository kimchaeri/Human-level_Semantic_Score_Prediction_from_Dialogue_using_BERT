      "utterance": [
        {
          "id": "MDRW2100029252.1.1",
          "form": "너무 많이 먹ㅇㅡ면",
          "original_form": "너무 많이 먹ㅇㅡ면",
          "speaker_id": "2",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029252.1.2",
          "form": "운동이라도",
          "original_form": "운동이라도",
          "speaker_id": "1",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029252.1.3",
          "form": "간이 해독작용하듯 해야해서",
          "original_form": "간이 해독작용하듯 해야해서",
          "speaker_id": "2",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029252.1.4",
          "form": "아님 스트레칭이라도",
          "original_form": "아님 스트레칭이라도",
          "speaker_id": "1",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029252.1.5",
          "form": "간이나 신장이 안좋아져서",
          "original_form": "간이나 신장이 안좋아져서",
          "speaker_id": "2",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029252.1.6",
          "form": "내가 그쪽으로 약할까봐",
          "original_form": "내가 그쪽으로 약할까봐",
          "speaker_id": "2",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029252.1.7",
          "form": "걱정돼서 못먹겠음",
          "original_form": "걱정돼서 못먹겠음",
          "speaker_id": "2",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029252.1.8",
          "form": "즙도 그렇대요",
          "original_form": "즙도 그렇대요",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.10",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.11",
          "form": "단 명이 꿈이신가요",
          "original_form": "단 명이 꿈이신가요",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.12",
          "form": "마그네슘은",
          "original_form": "마그네슘은",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.13",
          "form": "나한테 ㅋㅋㅋㅋㄱㄱㅋ",
          "original_form": "나한테 ㅋㅋㅋㅋㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.14",
          "form": "잘맞아서",
          "original_form": "잘맞아서",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.15",
          "form": "스트레칭이라도 해주세요",
          "original_form": "스트레칭이라도 해주세요",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.16",
          "form": "변비가 있었는데",
          "original_form": "변비가 있었는데",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.17",
          "form": "진짜 잘나왔음",
          "original_form": "진짜 잘나왔음",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.18",
          "form": "변비 비켜",
          "original_form": "변비 비켜",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.19",
          "form": "훌륭한 약이네요",
          "original_form": "훌륭한 약이네요",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.20",
          "form": "루테인필요",
          "original_form": "루테인필요",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.21",
          "form": "휴대폰 하루종일해서",
          "original_form": "휴대폰 하루종일해서",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029252.1.22",
          "form": "눈건강이 걱정임",
          "original_form": "눈건강이 걱정임",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
        {
          "id": "MDRW2100029252.1.23",
          "form": "아니면 루테인이 힘들면",
          "original_form": "아니면 루테인이 힘들면",
          "speaker_id": "1",
          "time": "20210903 00:43"
        }
      ]
