      "utterance": [
        {
          "id": "MDRW2100017221.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017221.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 18:07"
        },
        {
          "id": "MDRW2100017221.1.3",
          "form": "책 자주 읽으세요?",
          "original_form": "책 자주 읽으세요?",
          "speaker_id": "1",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017221.1.4",
          "form": "저는 취향이 없어서 베스트셀러에서 고르는 편이에요",
          "original_form": "저는 취향이 없어서 베스트셀러에서 고르는 편이에요",
          "speaker_id": "1",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017221.1.5",
          "form": "아니요 자주 안 읽어요",
          "original_form": "아니요 자주 안 읽어요",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017221.1.6",
          "form": "마지막으로 책 뭐 보셨어요?",
          "original_form": "마지막으로 책 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017221.1.7",
          "form": "저는 신작 중에 이수정교수님꺼 범죄관련 서적읽었는데 재밌더라고요",
          "original_form": "저는 신작 중에 이수정교수님꺼 범죄관련 서적읽었는데 재밌더라고요",
          "speaker_id": "1",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017221.1.8",
          "form": "오 재밌겠어요",
          "original_form": "오 재밌겠어요",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017221.1.9",
          "form": "저는 인플레이션 관련 책이요",
          "original_form": "저는 인플레이션 관련 책이요",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017221.1.10",
          "form": "오.. 어려운 분야네요..",
          "original_form": "오.. 어려운 분야네요..",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017221.1.11",
          "form": "재미가 없었어요",
          "original_form": "재미가 없었어요",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017221.1.12",
          "form": "어쩌다 읽게되신 거예요?",
          "original_form": "어쩌다 읽게되신 거예요?",
          "speaker_id": "1",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017221.1.13",
          "form": "유명한 책이라서?",
          "original_form": "유명한 책이라서?",
          "speaker_id": "1",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017221.1.14",
          "form": "주식을 하고 있는데 인플레이션이 주식에 어떤 영향을 끼치는지 궁금해서요",
          "original_form": "주식을 하고 있는데 인플레이션이 주식에 어떤 영향을 끼치는지 궁금해서요",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017221.1.15",
          "form": "아아 주식관련 책도 엄청 많더라고요",
          "original_form": "아아 주식관련 책도 엄청 많더라고요",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017221.1.16",
          "form": "그럼 관심있는 분야는 없으세요? 소설이나 수필같은",
          "original_form": "그럼 관심있는 분야는 없으세요? 소설이나 수필같은",
          "speaker_id": "1",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017221.1.17",
          "form": "학창시절에는 소설 많이 읽었었어요",
          "original_form": "학창시절에는 소설 많이 읽었었어요",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017221.1.18",
          "form": "그렇구나ㅎㅎ",
          "original_form": "그렇구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017221.1.19",
          "form": "요즘엔 통 못읽었어요",
          "original_form": "요즘엔 통 못읽었어요",
          "speaker_id": "2",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017221.1.20",
          "form": "요즘도 소설 재밌어보이는거 많이 나오더라고요",
          "original_form": "요즘도 소설 재밌어보이는거 많이 나오더라고요",
          "speaker_id": "1",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017221.1.21",
          "form": "달러구트의 꿈백화점 엄청 재밌어보이는데 도서관 예약이 꽉차서 못 읽고 있어요ㅠ",
          "original_form": "달러구트의 꿈백화점 엄청 재밌어보이는데 도서관 예약이 꽉차서 못 읽고 있어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017221.1.22",
          "form": "오~어떤 내용이에요?",
          "original_form": "오~어떤 내용이에요?",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017221.1.23",
          "form": "아직 안읽어봤는데...ㅎㅎ 잠들면 꿈속에서 꿈백화점을 방문하고",
          "original_form": "아직 안읽어봤는데...ㅎㅎ 잠들면 꿈속에서 꿈백화점을 방문하고",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017221.1.24",
          "form": "거기서 꿈을 사는..? 그런내용이래요!",
          "original_form": "거기서 꿈을 사는..? 그런내용이래요!",
          "speaker_id": "1",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017221.1.25",
          "form": "오~재밌겠어요",
          "original_form": "오~재밌겠어요",
          "speaker_id": "2",
          "time": "20210826 19:04"
        }
      ]
    }
  ]
