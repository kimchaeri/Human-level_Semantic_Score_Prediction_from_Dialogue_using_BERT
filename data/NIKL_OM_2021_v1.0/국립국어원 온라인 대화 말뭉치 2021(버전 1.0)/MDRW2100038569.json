      "utterance": [
        {
          "id": "MDRW2100038569.1.1",
          "form": "중고거래 해보셨나요",
          "original_form": "중고거래 해보셨나요",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100038569.1.2",
          "form": "네:) 중고나라랑\r\n당근마켓 해봤어요",
          "original_form": "네:) 중고나라랑\r\n당근마켓 해봤어요",
          "speaker_id": "1",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100038569.1.3",
          "form": "중고거래 경험 있나요?",
          "original_form": "중고거래 경험 있나요?",
          "speaker_id": "1",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100038569.1.4",
          "form": "저는 한번도 안해봤어요",
          "original_form": "저는 한번도 안해봤어요",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100038569.1.5",
          "form": "찜만 해놓고요ㅋㅋ",
          "original_form": "찜만 해놓고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100038569.1.6",
          "form": "아 찜만 해두셨었군용ㅋㅋ",
          "original_form": "아 찜만 해두셨었군용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100038569.1.7",
          "form": "아무래도 직거래가 좋아보이지만",
          "original_form": "아무래도 직거래가 좋아보이지만",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100038569.1.8",
          "form": "나갈 용기가 없어서요",
          "original_form": "나갈 용기가 없어서요",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100038569.1.9",
          "form": "요샌 코로나 때문에\r\n비대면 거래도 많아요:)",
          "original_form": "요샌 코로나 때문에\r\n비대면 거래도 많아요:)",
          "speaker_id": "1",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100038569.1.10",
          "form": "불편하심 비대면 거래로\r\n해보세요 ^^",
          "original_form": "불편하심 비대면 거래로\r\n해보세요 ^^",
          "speaker_id": "1",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100038569.1.11",
          "form": "비대면으로 하면 택배로 하는건가요?",
          "original_form": "비대면으로 하면 택배로 하는건가요?",
          "speaker_id": "2",
          "time": "20210902 18:49"
        },
        {
          "id": "MDRW2100038569.1.12",
          "form": "아니요:)\r\n당근같은 경우는\r\n거의 동네거래라서",
          "original_form": "아니요:)\r\n당근같은 경우는\r\n거의 동네거래라서",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100038569.1.13",
          "form": "판매자분 사는 곳에서\r\n거래가 이루어지는데\r\n집앞에 두시는 거에요:)",
          "original_form": "판매자분 사는 곳에서\r\n거래가 이루어지는데\r\n집앞에 두시는 거에요:)",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100038569.1.14",
          "form": "와 그런 방법도 있군요",
          "original_form": "와 그런 방법도 있군요",
          "speaker_id": "2",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100038569.1.15",
          "form": "네:) 비대면이\r\n좀 편하긴 하죵ㅋㅋ",
          "original_form": "네:) 비대면이\r\n좀 편하긴 하죵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100038569.1.16",
          "form": "대신 집주소를 알려줘야하는군요",
          "original_form": "대신 집주소를 알려줘야하는군요",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100038569.1.17",
          "form": "요즘 그러면 무섭지 않아요?",
          "original_form": "요즘 그러면 무섭지 않아요?",
          "speaker_id": "2",
          "time": "20210902 20:12"
        },
        {
          "id": "MDRW2100038569.1.18",
          "form": "그렇게 생각하면\r\n택배도 제 주소로 받는거니까요ㅠ",
          "original_form": "그렇게 생각하면\r\n택배도 제 주소로 받는거니까요ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100038569.1.19",
          "form": "요즘 무섭긴 하죵",
          "original_form": "요즘 무섭긴 하죵",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100038569.1.20",
          "form": "안전한 거래가 없네요",
          "original_form": "안전한 거래가 없네요",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100038569.1.21",
          "form": "당근은 온도로 확인하죠?",
          "original_form": "당근은 온도로 확인하죠?",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100038569.1.22",
          "form": "넹:) 저는 온도 보고 거래해요",
          "original_form": "넹:) 저는 온도 보고 거래해요",
          "speaker_id": "1",
          "time": "20210902 20:16"
        }
      ]
