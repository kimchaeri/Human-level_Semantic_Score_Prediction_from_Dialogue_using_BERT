      "utterance": [
        {
          "id": "MDRW2100013644.1.1",
          "form": "요즘 학교 가기 쉽지 않아요",
          "original_form": "요즘 학교 가기 쉽지 않아요",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013644.1.2",
          "form": "아무래도 코로나 때문에 힘드실거같아요",
          "original_form": "아무래도 코로나 때문에 힘드실거같아요",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013644.1.3",
          "form": "비대면수업하시나요",
          "original_form": "비대면수업하시나요",
          "speaker_id": "2",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013644.1.4",
          "form": "네 아마 다음 학기도 비대면일것 같아요",
          "original_form": "네 아마 다음 학기도 비대면일것 같아요",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013644.1.5",
          "form": "너무 심심해요",
          "original_form": "너무 심심해요",
          "speaker_id": "1",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013644.1.6",
          "form": "아무래도 그렇겠어요",
          "original_form": "아무래도 그렇겠어요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013644.1.7",
          "form": "학비도 비싼데 비대면이면",
          "original_form": "학비도 비싼데 비대면이면",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013644.1.8",
          "form": "진짜..",
          "original_form": "진짜..",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013644.1.9",
          "form": "비대면으로 인해 뭐가 가장 아쉬운가요",
          "original_form": "비대면으로 인해 뭐가 가장 아쉬운가요",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013644.1.10",
          "form": "제일 아쉬운건 아무래도",
          "original_form": "제일 아쉬운건 아무래도",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013644.1.11",
          "form": "사람을 못 만나는게 제일 아쉽죠",
          "original_form": "사람을 못 만나는게 제일 아쉽죠",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013644.1.12",
          "form": "음 아무래도 그게 가장 클 것 같았어요",
          "original_form": "음 아무래도 그게 가장 클 것 같았어요",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013644.1.13",
          "form": "축제도 비대면으로 하는 학교가 있더라구요",
          "original_form": "축제도 비대면으로 하는 학교가 있더라구요",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013644.1.14",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013644.1.15",
          "form": "그게 의미가 있나..",
          "original_form": "그게 의미가 있나..",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013644.1.16",
          "form": "그게 축젠가요",
          "original_form": "그게 축젠가요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013644.1.17",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013644.1.18",
          "form": "그냥.. 티비보는거랑 다를게 뭐지.ㅈ.",
          "original_form": "그냥.. 티비보는거랑 다를게 뭐지.ㅈ.",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013644.1.19",
          "form": "그런 이상한 학교도 있다니 참 별  일이 다 있네요",
          "original_form": "그런 이상한 학교도 있다니 참 별  일이 다 있네요",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013644.1.20",
          "form": "다른 학교는 가수 초대해서 유튜브 라이브로 중계하기도 해요",
          "original_form": "다른 학교는 가수 초대해서 유튜브 라이브로 중계하기도 해요",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013644.1.21",
          "form": "으잉 그게 뭐래요",
          "original_form": "으잉 그게 뭐래요",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013644.1.22",
          "form": "누굴 위한 축제인거지",
          "original_form": "누굴 위한 축제인거지",
          "speaker_id": "2",
          "time": "20210825 15:49"
        }
      ]
