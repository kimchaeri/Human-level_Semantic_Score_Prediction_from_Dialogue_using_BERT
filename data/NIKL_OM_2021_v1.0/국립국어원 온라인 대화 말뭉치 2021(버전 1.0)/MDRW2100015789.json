        },
        {
          "id": "MDRW2100015789.1.3",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015789.1.4",
          "form": "넹넹  ㅋㅋㅋ",
          "original_form": "넹넹  ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015789.1.5",
          "form": "올림픽 재밌었죠",
          "original_form": "올림픽 재밌었죠",
          "speaker_id": "2",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015789.1.6",
          "form": "네 배구 팬되었어요",
          "original_form": "네 배구 팬되었어요",
          "speaker_id": "1",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015789.1.7",
          "form": "앜ㅋㅋ 저도배구생각했슴다",
          "original_form": "앜ㅋㅋ 저도배구생각했슴다",
          "speaker_id": "2",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015789.1.8",
          "form": "응원하는 팀 있나요",
          "original_form": "응원하는 팀 있나요",
          "speaker_id": "1",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015789.1.9",
          "form": "그건 아니용 아직 잘 몰라요그건 ㅋㅋ",
          "original_form": "그건 아니용 아직 잘 몰라요그건 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015789.1.10",
          "form": "직관 가볼까 생각은 햿었는뎈ㅋㅋㅋ",
          "original_form": "직관 가볼까 생각은 햿었는뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015789.1.11",
          "form": "저도 가보고싶어요",
          "original_form": "저도 가보고싶어요",
          "speaker_id": "1",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015789.1.12",
          "form": "인삼공사 거기가면 홍삼준다하곸ㅋㅋㅋ 쏠쏠하다곸ㅋㅋㅋ",
          "original_form": "인삼공사 거기가면 홍삼준다하곸ㅋㅋㅋ 쏠쏠하다곸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015789.1.13",
          "form": "네??? 진짜요?ㅋㅋㅋ",
          "original_form": "네??? 진짜요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015789.1.14",
          "form": "인삼공사 가까운데",
          "original_form": "인삼공사 가까운데",
          "speaker_id": "1",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015789.1.15",
          "form": "가봐야겠네요",
          "original_form": "가봐야겠네요",
          "speaker_id": "1",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015789.1.16",
          "form": "앗 저랑 같은데 사시나보다",
          "original_form": "앗 저랑 같은데 사시나보다",
          "speaker_id": "2",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015789.1.17",
