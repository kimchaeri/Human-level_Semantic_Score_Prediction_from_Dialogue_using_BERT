      "utterance": [
        {
          "id": "MDRW2100024336.1.1",
          "form": "요새 부업이",
          "original_form": "요새 부업이",
          "speaker_id": "2",
          "time": "20210831 01:08"
        },
        {
          "id": "MDRW2100024336.1.2",
          "form": "안좋았는데",
          "original_form": "안좋았는데",
          "speaker_id": "2",
          "time": "20210831 01:08"
        },
        {
          "id": "MDRW2100024336.1.3",
          "form": "간만에 좀 했어요",
          "original_form": "간만에 좀 했어요",
          "speaker_id": "2",
          "time": "20210831 01:08"
        },
        {
          "id": "MDRW2100024336.1.4",
          "form": "택배보내고왔어요",
          "original_form": "택배보내고왔어요",
          "speaker_id": "2",
          "time": "20210831 01:08"
        },
        {
          "id": "MDRW2100024336.1.5",
          "form": "ㅋㅋㅋ잘하셨어요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ잘하셨어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:08"
        },
        {
          "id": "MDRW2100024336.1.6",
          "form": "아이가 아직 뒤집기를 안하는군요",
          "original_form": "아이가 아직 뒤집기를 안하는군요",
          "speaker_id": "1",
          "time": "20210831 01:09"
        },
        {
          "id": "MDRW2100024336.1.7",
          "form": "육아한지 오래라 기억이 안나느레",
          "original_form": "육아한지 오래라 기억이 안나느레",
          "speaker_id": "1",
          "time": "20210831 01:09"
        },
        {
          "id": "MDRW2100024336.1.8",
          "form": "네 아기가 누워있는거좋아해요",
          "original_form": "네 아기가 누워있는거좋아해요",
          "speaker_id": "2",
          "time": "20210831 01:09"
        },
        {
          "id": "MDRW2100024336.1.9",
          "form": "다른아기들은 다뒤집어요",
          "original_form": "다른아기들은 다뒤집어요",
          "speaker_id": "2",
          "time": "20210831 01:09"
        },
        {
          "id": "MDRW2100024336.1.10",
          "form": "안나는데보통 며칠에하지요?",
          "original_form": "안나는데보통 며칠에하지요?",
          "speaker_id": "1",
          "time": "20210831 01:09"
        },
        {
          "id": "MDRW2100024336.1.11",
          "form": "우리아기만안해요",
          "original_form": "우리아기만안해요",
          "speaker_id": "2",
          "time": "20210831 01:09"
        },
        {
          "id": "MDRW2100024336.1.12",
          "form": "150에는보통다하는듯요",
          "original_form": "150에는보통다하는듯요",
          "speaker_id": "2",
          "time": "20210831 01:09"
        },
        {
          "id": "MDRW2100024336.1.13",
          "form": "울애기170일인듯한데ㅋㅋㅋ",
          "original_form": "울애기170일인듯한데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:09"
        },
        {
          "id": "MDRW2100024336.1.14",
          "form": "뒤집기안하다가갑자기기어다닐듯ㅋㅋ",
          "original_form": "뒤집기안하다가갑자기기어다닐듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:09"
        },
        {
          "id": "MDRW2100024336.1.15",
          "form": "육아는 정답이 없는 일이니까요",
          "original_form": "육아는 정답이 없는 일이니까요",
          "speaker_id": "1",
          "time": "20210831 01:10"
        },
        {
          "id": "MDRW2100024336.1.16",
          "form": "터미터미?그건 좋아하나요?",
          "original_form": "터미터미?그건 좋아하나요?",
          "speaker_id": "1",
          "time": "20210831 01:10"
        },
        {
          "id": "MDRW2100024336.1.17",
          "form": "다리힘은 엄청좋아요",
          "original_form": "다리힘은 엄청좋아요",
          "speaker_id": "2",
          "time": "20210831 01:10"
        },
        {
          "id": "MDRW2100024336.1.18",
          "form": "터미타임싫어해요",
          "original_form": "터미타임싫어해요",
          "speaker_id": "2",
          "time": "20210831 01:10"
        },
        {
          "id": "MDRW2100024336.1.19",
          "form": "터미터미는 목 가눌때 하는건가요?ㅋ",
          "original_form": "터미터미는 목 가눌때 하는건가요?ㅋ",
          "speaker_id": "1",
          "time": "20210831 01:10"
        },
        {
          "id": "MDRW2100024336.1.20",
          "form": "육아는힘드네요",
          "original_form": "육아는힘드네요",
          "speaker_id": "2",
          "time": "20210831 01:10"
        },
        {
          "id": "MDRW2100024336.1.21",
          "form": "기억이 잘... 안나요",
          "original_form": "기억이 잘... 안나요",
          "speaker_id": "1",
          "time": "20210831 01:10"
        },
        {
          "id": "MDRW2100024336.1.22",
          "form": "아기는 예뻐요",
          "original_form": "아기는 예뻐요",
          "speaker_id": "2",
          "time": "20210831 01:10"
        },
        {
          "id": "MDRW2100024336.1.23",
          "form": "둘째육아는솔직히",
          "original_form": "둘째육아는솔직히",
          "speaker_id": "2",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100024336.1.24",
          "form": "쉽네요",
          "original_form": "쉽네요",
          "speaker_id": "2",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100024336.1.25",
          "form": "들째는 사랑입니다ㅋㅋㅋ",
          "original_form": "들째는 사랑입니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100024336.1.26",
          "form": "자꾸회사사람이 둘째 낳으라고ㅋㅋㅋ",
          "original_form": "자꾸회사사람이 둘째 낳으라고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100024336.1.27",
          "form": "아니 왜그러죠",
          "original_form": "아니 왜그러죠",
          "speaker_id": "2",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100024336.1.28",
          "form": "사랑이라며 태어나서 가장 잘 한일이래요ㅋㅋㅋ",
          "original_form": "사랑이라며 태어나서 가장 잘 한일이래요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100024336.1.29",
          "form": "돈대주거나 키워줄꺼아니면서",
          "original_form": "돈대주거나 키워줄꺼아니면서",
          "speaker_id": "2",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100024336.1.30",
          "form": "너무이쁘다고ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무이쁘다고ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100024336.1.31",
          "form": "이쁘긴이쁜데",
          "original_form": "이쁘긴이쁜데",
          "speaker_id": "2",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100024336.1.32",
          "form": "잃는것도많ㅈㆍ",
          "original_form": "잃는것도많ㅈㆍ",
          "speaker_id": "2",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100024336.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ많이이뻐하세요^",
          "original_form": "ㅋㅋㅋㅋㅋㅋ많이이뻐하세요^",
          "speaker_id": "2",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100024336.1.34",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100024336.1.35",
          "form": "이쁘긴할듯요",
          "original_form": "이쁘긴할듯요",
          "speaker_id": "1",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100024336.1.36",
          "form": "좀더애뜻할것같아여",
          "original_form": "좀더애뜻할것같아여",
          "speaker_id": "1",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100024336.1.37",
          "form": "이쁘고 이쁘고 이쁘죠",
          "original_form": "이쁘고 이쁘고 이쁘죠",
          "speaker_id": "2",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100024336.1.38",
          "form": "덜조급하고ㅋㅋㅋ",
          "original_form": "덜조급하고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100024336.1.39",
          "form": "덜불안하고ㅋㅋㅋ",
          "original_form": "덜불안하고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100024336.1.40",
          "form": "사랑을 주는법도 더잘알고",
          "original_form": "사랑을 주는법도 더잘알고",
          "speaker_id": "2",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024336.1.41",
          "form": "빨리크는거 아니까",
          "original_form": "빨리크는거 아니까",
          "speaker_id": "2",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024336.1.42",
          "form": "그래서둘째는사랑이라고하나봐요",
          "original_form": "그래서둘째는사랑이라고하나봐요",
          "speaker_id": "1",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024336.1.43",
          "form": "잘해주게되요 작고예뻐서",
          "original_form": "잘해주게되요 작고예뻐서",
          "speaker_id": "2",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024336.1.44",
          "form": "사진도많이찍고ㅋㅋ",
          "original_form": "사진도많이찍고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024336.1.45",
          "form": "사진은안찍어요ㅋㅋㅋ",
          "original_form": "사진은안찍어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024336.1.46",
          "form": "첫째랑비교하면진짜애기자나여ㅋㅋ",
          "original_form": "첫째랑비교하면진짜애기자나여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024336.1.47",
          "form": "자꾸잊네요",
          "original_form": "자꾸잊네요",
          "speaker_id": "2",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024336.1.48",
          "form": "남는건사진뿐ㅋㅋㄱㅋ",
          "original_form": "남는건사진뿐ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210831 01:13"
        },
        {
          "id": "MDRW2100024336.1.49",
          "form": "첫찌는이제 친구 선생님만좋아하고",
          "original_form": "첫찌는이제 친구 선생님만좋아하고",
          "speaker_id": "2",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024336.1.50",
          "form": "6세들은이제막찍으연안됨ㅋㅋㅋ몬난이ㅋㅋㅋ",
          "original_form": "6세들은이제막찍으연안됨ㅋㅋㅋ몬난이ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024336.1.51",
          "form": "6갤은막찍어도이쁘죠ㅋㅋ",
          "original_form": "6갤은막찍어도이쁘죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024336.1.52",
          "form": "나밖에모르는 아가가 진짜 이때뿐이라",
          "original_form": "나밖에모르는 아가가 진짜 이때뿐이라",
          "speaker_id": "2",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024336.1.53",
          "form": "맞아요ㅋㅋ6세는한글이야호를더좋아하는듯ㅋㅋㄲㄱㄲ",
          "original_form": "맞아요ㅋㅋ6세는한글이야호를더좋아하는듯ㅋㅋㄲㄱㄲ",
          "speaker_id": "1",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024336.1.54",
          "form": "오늘 밀크티체험판왔는데",
          "original_form": "오늘 밀크티체험판왔는데",
          "speaker_id": "2",
          "time": "20210831 01:14"
        },
        {
          "id": "MDRW2100024336.1.55",
          "form": "행복해하드라구요",
          "original_form": "행복해하드라구요",
          "speaker_id": "2",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024336.1.56",
          "form": "ㅋㅋ반납할수도있는기계인데ㅋㅋㅋㄲ",
          "original_form": "ㅋㅋ반납할수도있는기계인데ㅋㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024336.1.57",
          "form": "저번주는아이스크림홈런ㅋㅋㅋ\r\n본수업하고싶어하는거같은데",
          "original_form": "저번주는아이스크림홈런ㅋㅋㅋ\r\n본수업하고싶어하는거같은데",
          "speaker_id": "2",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024336.1.58",
          "form": "안해줄꺼에요",
          "original_form": "안해줄꺼에요",
          "speaker_id": "2",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100024336.1.59",
          "form": "잠시만요^^ㅋㅋㅋ",
          "original_form": "잠시만요^^ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:15"
        },
        {
