      "utterance": [
        {
          "id": "MDRW2100014354.1.1",
          "form": "지금 하는 일엔 만족하시나요?",
          "original_form": "지금 하는 일엔 만족하시나요?",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014354.1.2",
          "form": "오늘 다 할수있겟네요 ㅋ",
          "original_form": "오늘 다 할수있겟네요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014354.1.3",
          "form": "저  코로나 백수라",
          "original_form": "저  코로나 백수라",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014354.1.4",
          "form": "실업급여 받고이써요",
          "original_form": "실업급여 받고이써요",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014354.1.5",
          "form": "그러시군요 ^^ 저는 육아휴직중이요",
          "original_form": "그러시군요 ^^ 저는 육아휴직중이요",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014354.1.6",
          "form": "육아도 일인데  힘내세요",
          "original_form": "육아도 일인데  힘내세요",
          "speaker_id": "2",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014354.1.7",
          "form": "예전 일은 적성에 맞으셨나요?",
          "original_form": "예전 일은 적성에 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014354.1.8",
          "form": "그런거같아요 10년 넘게 한직종이긴했죠",
          "original_form": "그런거같아요 10년 넘게 한직종이긴했죠",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014354.1.9",
          "form": "코로나때문에 타격이 있지만요",
          "original_form": "코로나때문에 타격이 있지만요",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014354.1.10",
          "form": "와 10년 넘게 일하셨으면 오래하셨네요!",
          "original_form": "와 10년 넘게 일하셨으면 오래하셨네요!",
          "speaker_id": "1",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014354.1.11",
          "form": "육아휴직은 얼마나 가능해요,?",
          "original_form": "육아휴직은 얼마나 가능해요,?",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014354.1.12",
          "form": "1년정도하는건가요?",
          "original_form": "1년정도하는건가요?",
          "speaker_id": "2",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014354.1.13",
          "form": "네 규정상으로는 최대 3년까지도 가능하긴 해요",
          "original_form": "네 규정상으로는 최대 3년까지도 가능하긴 해요",
          "speaker_id": "1",
          "time": "20210825 18:50"
        },
        {
          "id": "MDRW2100014354.1.14",
          "form": "저는  서비스업종분야라서 주변에 없어서든요",
          "original_form": "저는  서비스업종분야라서 주변에 없어서든요",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014354.1.15",
          "form": "좋네요 와",
          "original_form": "좋네요 와",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014354.1.16",
          "form": "그러시군요 제도가 있어도 활용하기가 쉽지않은게 현실이죠 ㅠ",
          "original_form": "그러시군요 제도가 있어도 활용하기가 쉽지않은게 현실이죠 ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014354.1.17",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014354.1.18",
          "form": "휴무도 잘보장 못받는게  서비스업이라",
          "original_form": "휴무도 잘보장 못받는게  서비스업이라",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014354.1.19",
          "form": "서비스업은 그런 고충이 있군요",
          "original_form": "서비스업은 그런 고충이 있군요",
          "speaker_id": "1",
          "time": "20210825 19:20"
        }
