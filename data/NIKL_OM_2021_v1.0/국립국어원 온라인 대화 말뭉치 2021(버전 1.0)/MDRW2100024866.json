      "utterance": [
        {
          "id": "MDRW2100024866.1.1",
          "form": "엄마~~",
          "original_form": "엄마~~",
          "speaker_id": "1",
          "time": "20210831 19:55"
        },
        {
          "id": "MDRW2100024866.1.2",
          "form": "요즘에도 손톱 갈라져요?",
          "original_form": "요즘에도 손톱 갈라져요?",
          "speaker_id": "1",
          "time": "20210831 19:55"
        },
        {
          "id": "MDRW2100024866.1.3",
          "form": "어~ 건조해져서 그런가 더 심하네",
          "original_form": "어~ 건조해져서 그런가 더 심하네",
          "speaker_id": "2",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100024866.1.4",
          "form": "아! 나 이번에 손톱영양제 샀는데",
          "original_form": "아! 나 이번에 손톱영양제 샀는데",
          "speaker_id": "1",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100024866.1.5",
          "form": "그거 발라줄까요?",
          "original_form": "그거 발라줄까요?",
          "speaker_id": "1",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100024866.1.6",
          "form": "좋지~",
          "original_form": "좋지~",
          "speaker_id": "2",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100024866.1.7",
          "form": "그거 효과가 있나???",
          "original_form": "그거 효과가 있나???",
          "speaker_id": "2",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100024866.1.8",
          "form": "나 그저께 바르고 오늘 또 발라봤는데",
          "original_form": "나 그저께 바르고 오늘 또 발라봤는데",
          "speaker_id": "1",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100024866.1.9",
          "form": "괜찮은 거 같아요~",
          "original_form": "괜찮은 거 같아요~",
          "speaker_id": "1",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100024866.1.10",
          "form": "계속발라야하는거야??",
          "original_form": "계속발라야하는거야??",
          "speaker_id": "2",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100024866.1.11",
          "form": "웅웅~ 이게 이틀에 한번씩 발라주고",
          "original_form": "웅웅~ 이게 이틀에 한번씩 발라주고",
          "speaker_id": "1",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100024866.1.12",
          "form": "일주일째 되면 아세톤으로 지웠다가 다시 발라줘야 한대요~",
          "original_form": "일주일째 되면 아세톤으로 지웠다가 다시 발라줘야 한대요~",
          "speaker_id": "1",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100024866.1.13",
          "form": "그러면 단단해진대??",
          "original_form": "그러면 단단해진대??",
          "speaker_id": "2",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100024866.1.14",
          "form": "그렇게 적혀있긴해요.. ㅋㅋㅋ",
          "original_form": "그렇게 적혀있긴해요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100024866.1.15",
          "form": "비싼거야??",
          "original_form": "비싼거야??",
          "speaker_id": "2",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100024866.1.16",
          "form": "그렇게 비싸지는 않고 적당히~~",
          "original_form": "그렇게 비싸지는 않고 적당히~~",
          "speaker_id": "1",
          "time": "20210831 19:57"
        },
        {
          "id": "MDRW2100024866.1.17",
          "form": "그래~ 그럼 엄마도 발라줘~",
          "original_form": "그래~ 그럼 엄마도 발라줘~",
          "speaker_id": "2",
          "time": "20210831 19:58"
        },
        {
          "id": "MDRW2100024866.1.18",
          "form": "알았어요",
          "original_form": "알았어요",
          "speaker_id": "1",
          "time": "20210831 19:58"
        },
        {
          "id": "MDRW2100024866.1.19",
