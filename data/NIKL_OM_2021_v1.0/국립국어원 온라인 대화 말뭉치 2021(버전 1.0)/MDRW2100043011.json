      "utterance": [
        {
          "id": "MDRW2100043011.1.1",
          "form": "점심먹음?",
          "original_form": "점심먹음?",
          "speaker_id": "2",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100043011.1.2",
          "form": "아직 너는 먹었어?",
          "original_form": "아직 너는 먹었어?",
          "speaker_id": "1",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100043011.1.3",
          "form": "나도 아직ㅋㅋㄲㄱㅋ",
          "original_form": "나도 아직ㅋㅋㄲㄱㅋ",
          "speaker_id": "2",
          "time": "20210907 13:05"
        },
        {
          "id": "MDRW2100043011.1.4",
          "form": "백수들 아니랄까봐 자유롭다...",
          "original_form": "백수들 아니랄까봐 자유롭다...",
          "speaker_id": "2",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100043011.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100043011.1.6",
          "form": "나는 대충 라면 끓여 먹으려고",
          "original_form": "나는 대충 라면 끓여 먹으려고",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100043011.1.7",
          "form": "라면이 백수들의 주식이긴하지...",
          "original_form": "라면이 백수들의 주식이긴하지...",
          "speaker_id": "2",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100043011.1.8",
          "form": "넌 뭐먹어?",
          "original_form": "넌 뭐먹어?",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100043011.1.9",
          "form": "나는 오므라이스해먹을라고",
          "original_form": "나는 오므라이스해먹을라고",
          "speaker_id": "2",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100043011.1.10",
          "form": "집에 양파랑 햄이 좀 있어서",
          "original_form": "집에 양파랑 햄이 좀 있어서",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100043011.1.11",
          "form": "오...생각보다 잘해먹네",
          "original_form": "오...생각보다 잘해먹네",
          "speaker_id": "1",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100043011.1.12",
          "form": "10분정도만 있으면 한다ㅋㅋㅋㅋ",
          "original_form": "10분정도만 있으면 한다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100043011.1.13",
          "form": "간단함!",
          "original_form": "간단함!",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100043011.1.14",
          "form": "볶음밥은 간단해서 좋아",
          "original_form": "볶음밥은 간단해서 좋아",
          "speaker_id": "1",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100043011.1.15",
          "form": "뭐가 있던간에 볶으면 일단맛있음",
          "original_form": "뭐가 있던간에 볶으면 일단맛있음",
          "speaker_id": "2",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100043011.1.16",
          "form": "진짜 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100043011.1.17",
          "form": "일단 볶아본다",
          "original_form": "일단 볶아본다",
          "speaker_id": "1",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100043011.1.18",
          "form": "조미료도 꼭넣기!",
          "original_form": "조미료도 꼭넣기!",
          "speaker_id": "2",
          "time": "20210907 13:09"
        }
      ]
