      "utterance": [
        {
          "id": "MDRW2100031620.1.1",
          "form": "그래도 비싼건데 자주 애용해주세여 ㅋㅋ",
          "original_form": "그래도 비싼건데 자주 애용해주세여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100031620.1.2",
          "form": "ㅋㅋㅋㅋ그러니까요",
          "original_form": "ㅋㅋㅋㅋ그러니까요",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031620.1.3",
          "form": "저한테는 애물단지예요 ,,",
          "original_form": "저한테는 애물단지예요 ,,",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031620.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031620.1.5",
          "form": "중고거래 해보신 적 있으세요?",
          "original_form": "중고거래 해보신 적 있으세요?",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031620.1.6",
          "form": "네네 당근마켓 해요",
          "original_form": "네네 당근마켓 해요",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031620.1.7",
          "form": "오 저두요!",
          "original_form": "오 저두요!",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031620.1.8",
          "form": "온도 많이 올리셨나요?ㅎㅎ",
          "original_form": "온도 많이 올리셨나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031620.1.9",
          "form": "네네 ㅋㅋㅋ 주로 어떤 품목 파세여",
          "original_form": "네네 ㅋㅋㅋ 주로 어떤 품목 파세여",
          "speaker_id": "1",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031620.1.10",
          "form": "ㅋㅋ저는 집에 있는데 아무도 신경안쓰는 물건 팔아요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ저는 집에 있는데 아무도 신경안쓰는 물건 팔아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031620.1.11",
          "form": "옷이나 그림 같은거,,ㅋㅅㅋ",
          "original_form": "옷이나 그림 같은거,,ㅋㅅㅋ",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031620.1.12",
          "form": "와 그림이 집에 있어뇨?",
          "original_form": "와 그림이 집에 있어뇨?",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031620.1.13",
          "form": "넼ㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031620.1.14",
          "form": "아무도 안보는 거 같길래,,",
          "original_form": "아무도 안보는 거 같길래,,",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031620.1.15",
          "form": "팔았어요 ㅋㅋㅋㅋ",
          "original_form": "팔았어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:22"
        },
        {
          "id": "MDRW2100031620.1.16",
          "form": "오오 잘했어요 ㅋㅋ",
          "original_form": "오오 잘했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031620.1.17",
          "form": "애물단지에요",
          "original_form": "애물단지에요",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100031620.1.18",
          "form": "마자욬ㅋㅋㅋㅋ",
          "original_form": "마자욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031620.1.19",
          "form": "뭐 당근하세요??",
          "original_form": "뭐 당근하세요??",
          "speaker_id": "2",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100031620.1.20",
          "form": "저는 엄마가 팔라는거요 가구랑 가방이런거 ㅋㅌ",
          "original_form": "저는 엄마가 팔라는거요 가구랑 가방이런거 ㅋㅌ",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100031620.1.21",
          "form": "오옼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오옼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031620.1.22",
          "form": "꽤 쏠쏠하지 않나요?ㅋㅋㅋㅋ",
          "original_form": "꽤 쏠쏠하지 않나요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:39"
        },
        {
          "id": "MDRW2100031620.1.23",
          "form": "요즘은 잘 안팔리네요",
          "original_form": "요즘은 잘 안팔리네요",
