      "utterance": [
        {
          "id": "MDRW2100013539.1.1",
          "form": "싸이월드 들어가봤어?",
          "original_form": "싸이월드 들어가봤어?",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013539.1.2",
          "form": "아직안들어가봄",
          "original_form": "아직안들어가봄",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013539.1.3",
          "form": "오픈함????",
          "original_form": "오픈함????",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013539.1.4",
          "form": "곧 오픈한댔는데 친구가 들어는 가진다고 했던 것 같음",
          "original_form": "곧 오픈한댔는데 친구가 들어는 가진다고 했던 것 같음",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013539.1.5",
          "form": "아헐 나도빨리들어가봐야지",
          "original_form": "아헐 나도빨리들어가봐야지",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013539.1.6",
          "form": "도토리환불받앗나",
          "original_form": "도토리환불받앗나",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013539.1.7",
          "form": "내가 도토리 있었는지 기억도 안난다",
          "original_form": "내가 도토리 있었는지 기억도 안난다",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013539.1.8",
          "form": "난 2갠가못썻는데",
          "original_form": "난 2갠가못썻는데",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013539.1.9",
          "form": "내 홈피에 들어가는게 문제가 아니라 남이 들고있는 내 사진들이 문제 ㅋ",
          "original_form": "내 홈피에 들어가는게 문제가 아니라 남이 들고있는 내 사진들이 문제 ㅋ",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013539.1.10",
          "form": "받아봤자200...",
          "original_form": "받아봤자200...",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013539.1.11",
          "form": "200원?",
          "original_form": "200원?",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013539.1.12",
          "form": "아헐맞네",
          "original_form": "아헐맞네",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013539.1.13",
          "form": "생각도못햇다",
          "original_form": "생각도못햇다",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013539.1.14",
          "form": "그럼난 친구다 삭제해야겟다",
          "original_form": "그럼난 친구다 삭제해야겟다",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013539.1.15",
          "form": "타고들어올라",
          "original_form": "타고들어올라",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013539.1.16",
          "form": "친구다끊어야지",
          "original_form": "친구다끊어야지",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013539.1.17",
          "form": "멜론에 앨범보니까 싸이월드 유행했던 노래 다시 내던데 곧 나올건가봐",
          "original_form": "멜론에 앨범보니까 싸이월드 유행했던 노래 다시 내던데 곧 나올건가봐",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013539.1.18",
          "form": "엉 일단 오픈되면 다 비공개로 바뀐다는것같더라",
          "original_form": "엉 일단 오픈되면 다 비공개로 바뀐다는것같더라",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013539.1.19",
          "form": "기사에서 본 것 같네",
          "original_form": "기사에서 본 것 같네",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013539.1.20",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013539.1.21",
          "form": "다행이군",
          "original_form": "다행이군",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013539.1.22",
          "form": "싸이월드가 다시 유행하려나",
          "original_form": "싸이월드가 다시 유행하려나",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013539.1.23",
          "form": "그땐 톡이 없어서 유행했던 것 같은데",
          "original_form": "그땐 톡이 없어서 유행했던 것 같은데",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013539.1.24",
          "form": "응...미안하지만",
          "original_form": "응...미안하지만",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013539.1.25",
          "form": "갬성만보고",
          "original_form": "갬성만보고",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013539.1.26",
          "form": "망할듯",
          "original_form": "망할듯",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013539.1.27",
          "form": "사람들이용안해섴ㅋㅋ",
          "original_form": "사람들이용안해섴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013539.1.28",
          "form": "엉 초반에만 잠깐 뜰듯",
          "original_form": "엉 초반에만 잠깐 뜰듯",
          "speaker_id": "1",
