      "utterance": [
        {
          "id": "MDRW2100043563.1.1",
          "form": "오늘 저녁 뭐 드셨나요?",
          "original_form": "오늘 저녁 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043563.1.2",
          "form": "저는 한식 먹었어요!",
          "original_form": "저는 한식 먹었어요!",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043563.1.3",
          "form": "우와 한식메뉴 뭐 드샸는지 제가 맞춰볼게요",
          "original_form": "우와 한식메뉴 뭐 드샸는지 제가 맞춰볼게요",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043563.1.4",
          "form": "매운 음식인가요?",
          "original_form": "매운 음식인가요?",
          "speaker_id": "2",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043563.1.5",
          "form": "완전 맵진 않은데 매콤해요! ㅎㅎㅎ",
          "original_form": "완전 맵진 않은데 매콤해요! ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043563.1.6",
          "form": "빨간 요리였나요?",
          "original_form": "빨간 요리였나요?",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043563.1.7",
          "form": "오 맞습니다! 빨개요!!",
          "original_form": "오 맞습니다! 빨개요!!",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043563.1.8",
          "form": "와아~!",
          "original_form": "와아~!",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043563.1.9",
          "form": "국물이 있나요?",
          "original_form": "국물이 있나요?",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043563.1.10",
          "form": "네 다만 자작하게 있어요!",
          "original_form": "네 다만 자작하게 있어요!",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043563.1.11",
          "form": "오.. 빨갛고 자작한 요리..",
          "original_form": "오.. 빨갛고 자작한 요리..",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043563.1.12",
          "form": "생선이 들어가나요?",
          "original_form": "생선이 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043563.1.13",
          "form": "아뇨 들어가지 않습니다!",
          "original_form": "아뇨 들어가지 않습니다!",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043563.1.14",
          "form": "으으음 면이 들어가나요?",
          "original_form": "으으음 면이 들어가나요?",
