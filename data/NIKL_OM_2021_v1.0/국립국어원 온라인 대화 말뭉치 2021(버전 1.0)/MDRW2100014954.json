      "utterance": [
        {
          "id": "MDRW2100014954.1.1",
          "form": "그러면 음식얘기 할까요 ㅋㅋㅋㅋㅋ",
          "original_form": "그러면 음식얘기 할까요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:19"
        },
        {
          "id": "MDRW2100014954.1.2",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014954.1.3",
          "form": "오 음식얘기 좋아요!!",
          "original_form": "오 음식얘기 좋아요!!",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014954.1.4",
          "form": "저는 들깨수제비 먹었습니다",
          "original_form": "저는 들깨수제비 먹었습니다",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014954.1.5",
          "form": "뭐드셨어요??",
          "original_form": "뭐드셨어요??",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014954.1.6",
          "form": "우와 오늘같은 날씨에 완전 딱인데요? 맛있었겠어요",
          "original_form": "우와 오늘같은 날씨에 완전 딱인데요? 맛있었겠어요",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014954.1.7",
          "form": "저는 고기김치찌개 먹었어요 ㅎㅎ",
          "original_form": "저는 고기김치찌개 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014954.1.8",
          "form": "후식도 있었나요",
          "original_form": "후식도 있었나요",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014954.1.9",
          "form": "오 김치찌개 계란말이랑 먹으면 존맛인데",
          "original_form": "오 김치찌개 계란말이랑 먹으면 존맛인데",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014954.1.10",
          "form": "후식이라긴 어렵지만 스프라이트 먹었스니다",
          "original_form": "후식이라긴 어렵지만 스프라이트 먹었스니다",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014954.1.11",
          "form": "스프라이트 좋네요 ㅎㅎ",
          "original_form": "스프라이트 좋네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014954.1.12",
          "form": "평소에 제일 좋아하는 음식은 어떤거예요?",
          "original_form": "평소에 제일 좋아하는 음식은 어떤거예요?",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014954.1.13",
          "form": "저는 찌개 분식 햄버거 피자?",
          "original_form": "저는 찌개 분식 햄버거 피자?",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014954.1.14",
          "form": "초딩입맛이에요 ㅋㅋ",
          "original_form": "초딩입맛이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014954.1.15",
          "form": "어떤 음식 좋아하세요??",
          "original_form": "어떤 음식 좋아하세요??",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014954.1.16",
          "form": "오 저도 다 좋아해요 ㅋㅋㅋㅋㅋ 분식 햄버거 피자 ㅋㅋㅋ",
          "original_form": "오 저도 다 좋아해요 ㅋㅋㅋㅋㅋ 분식 햄버거 피자 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014954.1.17",
          "form": "치킨보다 피자파예요",
          "original_form": "치킨보다 피자파예요",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014954.1.18",
          "form": "근데 대부분 치킨을 많이 좋아하시더라고요",
          "original_form": "근데 대부분 치킨을 많이 좋아하시더라고요",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014954.1.19",
          "form": "대박 저도 피자파에요",
          "original_form": "대박 저도 피자파에요",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014954.1.20",
          "form": "하와이안 어떻게 생각하시죠?",
          "original_form": "하와이안 어떻게 생각하시죠?",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014954.1.21",
          "form": "저는 잘먹어요... ㅜㅜ ㅋㅋㅋㅋㅋ",
          "original_form": "저는 잘먹어요... ㅜㅜ ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014954.1.22",
          "form": "울지 마세요",
          "original_form": "울지 마세요",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014954.1.23",
          "form": "저도 하와이안 사랑해요",
          "original_form": "저도 하와이안 사랑해요",
          "speaker_id": "2",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014954.1.24",
          "form": "오 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014954.1.25",
          "form": "약간 쫄았거든요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "약간 쫄았거든요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014954.1.26",
          "form": "제일 자주시키는건 페퍼로니이긴 한데",
          "original_form": "제일 자주시키는건 페퍼로니이긴 한데",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014954.1.27",
          "form": "사실 피자는 거의 다 좋아해요 매운것만 아니면",
          "original_form": "사실 피자는 거의 다 좋아해요 매운것만 아니면",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014954.1.28",
          "form": "어쩜 이렇게 맛잘알이시죠?",
          "original_form": "어쩜 이렇게 맛잘알이시죠?",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014954.1.29",
          "form": "페퍼로니에 파인애플추가 국룰이라구요",
          "original_form": "페퍼로니에 파인애플추가 국룰이라구요",
          "speaker_id": "2",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014954.1.30",
          "form": "캬........",
          "original_form": "캬........",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014954.1.31",
          "form": "영혼의 단짝인가요",
          "original_form": "영혼의 단짝인가요",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014954.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014954.1.33",
          "form": "솔직히 들깨수제비부터 심상치 않았어요",
          "original_form": "솔직히 들깨수제비부터 심상치 않았어요",
