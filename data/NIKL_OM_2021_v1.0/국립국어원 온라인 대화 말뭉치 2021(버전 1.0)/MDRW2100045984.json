      "utterance": [
        {
          "id": "MDRW2100045984.1.1",
          "form": "두유 안단걸로 하는게 좋아요!",
          "original_form": "두유 안단걸로 하는게 좋아요!",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045984.1.2",
          "form": "담백하게!",
          "original_form": "담백하게!",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045984.1.3",
          "form": "매일에서 나온 당 안들은거!!",
          "original_form": "매일에서 나온 당 안들은거!!",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045984.1.4",
          "form": "참고할께요!",
          "original_form": "참고할께요!",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045984.1.5",
          "form": "음식도 자주하시는군요!",
          "original_form": "음식도 자주하시는군요!",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045984.1.6",
          "form": "자취 오래하다보니ㅜㅜ 배달음식 질려서요...ㅠㅠㅠ",
          "original_form": "자취 오래하다보니ㅜㅜ 배달음식 질려서요...ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045984.1.7",
          "form": "아 ㅠ 그쵸",
          "original_form": "아 ㅠ 그쵸",
          "speaker_id": "1",
          "time": "20210908 09:24"
        },
        {
          "id": "MDRW2100045984.1.8",
          "form": "베이킹도 배워서 집에서 그냥 해먹고싶네요!",
          "original_form": "베이킹도 배워서 집에서 그냥 해먹고싶네요!",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045984.1.9",
          "form": "와~~ 베이킹",
          "original_form": "와~~ 베이킹",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045984.1.10",
          "form": "카페는 자릿값이 너무 비싸니...",
          "original_form": "카페는 자릿값이 너무 비싸니...",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045984.1.11",
          "form": "요샌 냉동 생지도 잘너와서",
          "original_form": "요샌 냉동 생지도 잘너와서",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045984.1.12",
          "form": "아 저 그거 알아요!!",
          "original_form": "아 저 그거 알아요!!",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045984.1.13",
          "form": "크로와상에프하니깐 괜찮더라구요~",
          "original_form": "크로와상에프하니깐 괜찮더라구요~",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045984.1.14",
          "form": "저도 해먹었었어요 ㅋㅋㅋㅋ",
          "original_form": "저도 해먹었었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045984.1.15",
          "form": "오!!",
          "original_form": "오!!",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045984.1.16",
          "form": "버터향나면서 ㅋㅋ",
          "original_form": "버터향나면서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045984.1.17",
          "form": "에프 성능이 너무 안좋아서 아주 오래걸렸지만...",
          "original_form": "에프 성능이 너무 안좋아서 아주 오래걸렸지만...",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045984.1.18",
          "form": "맞아요 오래두오 머을 슈 있고",
          "original_form": "맞아요 오래두오 머을 슈 있고",
          "speaker_id": "2",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100045984.1.19",
          "form": "근데 너무 많이사서 거의 다먹어가는데 좀 질리네요 ㅋㅋㅋㅋ",
          "original_form": "근데 너무 많이사서 거의 다먹어가는데 좀 질리네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045984.1.20",
          "form": "아그렇군요!",
          "original_form": "아그렇군요!",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045984.1.21",
          "form": "간단한 브라우니같은거는 해먹을 수있을거 같은데ㅜㅠ",
          "original_form": "간단한 브라우니같은거는 해먹을 수있을거 같은데ㅜㅠ",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045984.1.22",
          "form": "재료가 하나도 없으니 다 돈이에요ㅠㅠ",
          "original_form": "재료가 하나도 없으니 다 돈이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:29"
        },
        {
          "id": "MDRW2100045984.1.23",
          "form": "계랑잘하고 하면ㅅ",
          "original_form": "계랑잘하고 하면ㅅ",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045984.1.24",
          "form": "집에서 베이킹하는금손들많더라구요",
          "original_form": "집에서 베이킹하는금손들많더라구요",
          "speaker_id": "1",
          "time": "20210908 09:31"
        }
      ]
    }
