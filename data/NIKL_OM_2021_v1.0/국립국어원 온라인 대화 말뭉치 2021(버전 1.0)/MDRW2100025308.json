        {
          "id": "MDRW2100025308.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "2",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025308.1.2",
          "form": "당근 가끔이요",
          "original_form": "당근 가끔이요",
          "speaker_id": "1",
          "time": "20210901 09:08"
        },
        {
          "id": "MDRW2100025308.1.3",
          "form": "저도 당근",
          "original_form": "저도 당근",
          "speaker_id": "2",
          "time": "20210901 09:11"
        },
        {
          "id": "MDRW2100025308.1.4",
          "form": "주로 어떤거 판매하세요",
          "original_form": "주로 어떤거 판매하세요",
          "speaker_id": "1",
          "time": "20210901 09:12"
        },
        {
          "id": "MDRW2100025308.1.5",
          "form": "최근애 공기청정기요",
          "original_form": "최근애 공기청정기요",
          "speaker_id": "2",
          "time": "20210901 09:13"
        },
        {
          "id": "MDRW2100025308.1.6",
          "form": "전 예전에 샀던 cd가 많아서 처분중이요ㅋ",
          "original_form": "전 예전에 샀던 cd가 많아서 처분중이요ㅋ",
          "speaker_id": "1",
          "time": "20210901 09:13"
        },
        {
          "id": "MDRW2100025308.1.7",
          "form": "잘 팔리죠?",
          "original_form": "잘 팔리죠?",
          "speaker_id": "2",
          "time": "20210901 09:16"
        },
        {
          "id": "MDRW2100025308.1.8",
          "form": "근데 어떤건 몰리고 어떤건 문의도 없어욬ㅋ",
          "original_form": "근데 어떤건 몰리고 어떤건 문의도 없어욬ㅋ",
          "speaker_id": "1",
          "time": "20210901 09:27"
        },
        {
          "id": "MDRW2100025308.1.9",
          "form": "그런것 같아요",
          "original_form": "그런것 같아요",
          "speaker_id": "2",
          "time": "20210901 09:29"
        },
        {
          "id": "MDRW2100025308.1.10",
          "form": "카세트테이프도 많은데 옛날엔 참 많이 샀었네요ㅋㅋ",
          "original_form": "카세트테이프도 많은데 옛날엔 참 많이 샀었네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 09:30"
        },
        {
          "id": "MDRW2100025308.1.11",
          "form": "추억이죠",
          "original_form": "추억이죠",
          "speaker_id": "2",
          "time": "20210901 09:32"
        },
        {
          "id": "MDRW2100025308.1.12",
          "form": "맞아요 그땐 열심이었죠",
          "original_form": "맞아요 그땐 열심이었죠",
          "speaker_id": "1",
