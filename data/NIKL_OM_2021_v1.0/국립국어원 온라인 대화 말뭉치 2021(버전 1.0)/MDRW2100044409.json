      "utterance": [
        {
          "id": "MDRW2100044409.1.1",
          "form": "아뇨 단발이예요",
          "original_form": "아뇨 단발이예요",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044409.1.2",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044409.1.3",
          "form": "올해초까지 길게기르다",
          "original_form": "올해초까지 길게기르다",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044409.1.4",
          "form": "잘랐어요ㅎㅎ",
          "original_form": "잘랐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044409.1.5",
          "form": "관리하기도 좋고",
          "original_form": "관리하기도 좋고",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044409.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044409.1.7",
          "form": "한번 단발하기 시작하면 기르기 쉽지 않더라고요",
          "original_form": "한번 단발하기 시작하면 기르기 쉽지 않더라고요",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044409.1.8",
          "form": "그쵸 뭐 일이 바쁘던지 해서 한동안 잊고있어야",
          "original_form": "그쵸 뭐 일이 바쁘던지 해서 한동안 잊고있어야",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044409.1.9",
          "form": "겨우 어깨가 넘더라고요",
          "original_form": "겨우 어깨가 넘더라고요",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044409.1.10",
          "form": "악 저두요",
          "original_form": "악 저두요",
          "speaker_id": "2",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044409.1.11",
          "form": "ㅋㅋ 단발로 자르니까",
          "original_form": "ㅋㅋ 단발로 자르니까",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044409.1.12",
          "form": "아침에 부시시해져서요 드라이기로 머리를살려줘야하더라고요",
          "original_form": "아침에 부시시해져서요 드라이기로 머리를살려줘야하더라고요",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044409.1.13",
          "form": "머리를 저녁에 감아서요",
          "original_form": "머리를 저녁에 감아서요",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044409.1.14",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044409.1.15",
          "form": "아아 저도 저녁에 감아서",
          "original_form": "아아 저도 저녁에 감아서",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044409.1.16",
          "form": "아침에 드라이해줘야해요",
          "original_form": "아침에 드라이해줘야해요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044409.1.17",
          "form": "근데 흐린날은 드라이도 소용 없더라고요",
          "original_form": "근데 흐린날은 드라이도 소용 없더라고요",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044409.1.18",
          "form": "네ㅠ 저는 그런날에는 포기하고",
          "original_form": "네ㅠ 저는 그런날에는 포기하고",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044409.1.19",
          "form": "묶습니다",
          "original_form": "묶습니다",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044409.1.20",
          "form": "저도 반묶음이라도",
          "original_form": "저도 반묶음이라도",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044409.1.21",
          "form": "반묶음하면",
          "original_form": "반묶음하면",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044409.1.22",
          "form": "뒷통수는 어떻게하세요?",
          "original_form": "뒷통수는 어떻게하세요?",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044409.1.23",
          "form": "저는 납작해서ㅜ",
          "original_form": "저는 납작해서ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100044409.1.24",
          "form": "아 저는 뒷통수가 동그랗긴 한데",
          "original_form": "아 저는 뒷통수가 동그랗긴 한데",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044409.1.25",
          "form": "디스코 스타일로 묶기도 하고요",
          "original_form": "디스코 스타일로 묶기도 하고요",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044409.1.26",
          "form": "아 그런방법도있네요",
          "original_form": "아 그런방법도있네요",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044409.1.27",
          "form": "요즘은 화장하기 정말힘들지않나요ㅎㅎ",
          "original_form": "요즘은 화장하기 정말힘들지않나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:46"
        }
      ]
    }
