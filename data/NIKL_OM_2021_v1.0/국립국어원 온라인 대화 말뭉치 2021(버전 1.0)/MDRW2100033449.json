      "utterance": [
        {
          "id": "MDRW2100033449.1.1",
          "form": "전 애니도 좋아해서",
          "original_form": "전 애니도 좋아해서",
          "speaker_id": "2",
          "time": "20210823 22:47"
        },
        {
          "id": "MDRW2100033449.1.2",
          "form": "흑.. 그렇게 또 하나하나씩 뺏겨가는 거네요",
          "original_form": "흑.. 그렇게 또 하나하나씩 뺏겨가는 거네요",
          "speaker_id": "1",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100033449.1.3",
          "form": "넷플 왓챠 애니봐요",
          "original_form": "넷플 왓챠 애니봐요",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100033449.1.4",
          "form": "애가 둘이니",
          "original_form": "애가 둘이니",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100033449.1.5",
          "form": "아주 다른거 보며 뺏죠",
          "original_form": "아주 다른거 보며 뺏죠",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100033449.1.6",
          "form": "그나마 애니 취향이 조금 딸이 비슷해서",
          "original_form": "그나마 애니 취향이 조금 딸이 비슷해서",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100033449.1.7",
          "form": "아.. 저는 애니보단 영화, 드라마, 유튜브 영화소개, 이런거 위주로",
          "original_form": "아.. 저는 애니보단 영화, 드라마, 유튜브 영화소개, 이런거 위주로",
          "speaker_id": "1",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100033449.1.8",
          "form": "끼어서 봐요",
          "original_form": "끼어서 봐요",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100033449.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100033449.1.10",
          "form": "아하! 그런 공통관심사 있으면 좋겠어요",
          "original_form": "아하! 그런 공통관심사 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100033449.1.11",
          "form": "저 영화소개좋아행ㅅ",
          "original_form": "저 영화소개좋아행ㅅ",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100033449.1.12",
          "form": "해요",
          "original_form": "해요",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100033449.1.13",
          "form": "티비 영화소개 코너도요",
          "original_form": "티비 영화소개 코너도요",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100033449.1.14",
          "form": "그거보고 영화봐에 컨텐츠에서요",
          "original_form": "그거보고 영화봐에 컨텐츠에서요",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100033449.1.15",
          "form": "저두요!!",
          "original_form": "저두요!!",
          "speaker_id": "1",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100033449.1.16",
          "form": "저번에도 그거 보고 영화봤어요 ㅋㅋ",
          "original_form": "저번에도 그거 보고 영화봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100033449.1.17",
          "form": "앙 잘챙겨봐요 재방도보고",
          "original_form": "앙 잘챙겨봐요 재방도보고",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100033449.1.18",
          "form": "드라마소개하는 것도 보다가 재미있다싶으면 거기서 스탑 !",
          "original_form": "드라마소개하는 것도 보다가 재미있다싶으면 거기서 스탑 !",
          "speaker_id": "1",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100033449.1.19",
          "form": "바로 가서 처음부터 찾아서 봐요 ㅎㅎ",
          "original_form": "바로 가서 처음부터 찾아서 봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033449.1.20",
          "form": "보고 제가 좋아하는 스타일 찾아서 정주행",
          "original_form": "보고 제가 좋아하는 스타일 찾아서 정주행",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033449.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033449.1.22",
          "form": "저도 제스탈 찾아서 ㅋㅋ",
          "original_form": "저도 제스탈 찾아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033449.1.23",
          "form": "아 요 소개만봐도 재나요",
          "original_form": "아 요 소개만봐도 재나요",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033449.1.24",
          "form": "잼나요",
          "original_form": "잼나요",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033449.1.25",
          "form": "그쵸 ㅋㅋㅋ 그 얼마전에 아이유.. 나온 드라마",
          "original_form": "그쵸 ㅋㅋㅋ 그 얼마전에 아이유.. 나온 드라마",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033449.1.26",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033449.1.27",
          "form": "제목이.. 머였더라 ㅠㅠ",
          "original_form": "제목이.. 머였더라 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033449.1.28",
          "form": "그 우중충한..",
          "original_form": "그 우중충한..",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033449.1.29",
          "form": "귀신 ?",
          "original_form": "귀신 ?",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033449.1.30",
          "form": "아저씨",
          "original_form": "아저씨",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033449.1.31",
          "form": "나의 아저씨요",
          "original_form": "나의 아저씨요",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033449.1.32",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033449.1.33",
          "form": "그거 1화보고 너무 우울하고 재미없ㅇ서 안봤었는데",
          "original_form": "그거 1화보고 너무 우울하고 재미없ㅇ서 안봤었는데",
          "speaker_id": "1",
          "time": "20210823 22:52"
        }
      ]
