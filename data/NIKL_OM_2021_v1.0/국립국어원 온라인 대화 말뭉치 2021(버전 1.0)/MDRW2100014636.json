      "utterance": [
        {
          "id": "MDRW2100014636.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014636.1.2",
          "form": "당근하시나요?",
          "original_form": "당근하시나요?",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014636.1.3",
          "form": "안녕하세요! 저는 아직 당근은 한번도 못 해봤어요ㅎㅎ",
          "original_form": "안녕하세요! 저는 아직 당근은 한번도 못 해봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014636.1.4",
          "form": "저도 최근에 한번해봤어요",
          "original_form": "저도 최근에 한번해봤어요",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014636.1.5",
          "form": "당근 많이들 하시던데 name1님 동네는 많이 활성화 돼 있나요?",
          "original_form": "당근 많이들 하시던데 &name1&님 동네는 많이 활성화 돼 있나요?",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014636.1.6",
          "form": "만화책 ㅋㅋ",
          "original_form": "만화책 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014636.1.7",
          "form": "오 파신거에요?",
          "original_form": "오 파신거에요?",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014636.1.8",
          "form": "많이 활성화는 되어있는데 걍 눈팅만해요",
          "original_form": "많이 활성화는 되어있는데 걍 눈팅만해요",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014636.1.9",
          "form": "아뇨 구매요",
          "original_form": "아뇨 구매요",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014636.1.10",
          "form": "어떤거 사셨나요?",
          "original_form": "어떤거 사셨나요?",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014636.1.11",
          "form": "귀멸 싹다모으고싶어서...",
          "original_form": "귀멸 싹다모으고싶어서...",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014636.1.12",
          "form": "아아 저는 안 봤는데 동생이 재밌게 봤다고 하더라구요",
          "original_form": "아아 저는 안 봤는데 동생이 재밌게 봤다고 하더라구요",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014636.1.13",
          "form": "미개봉인데 저렴하게 구매했어요",
          "original_form": "미개봉인데 저렴하게 구매했어요",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014636.1.14",
          "form": "재밌는데 논란이 많아서",
          "original_form": "재밌는데 논란이 많아서",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014636.1.15",
          "form": "오 운이 좋으셨네요ㅎㅎ",
          "original_form": "오 운이 좋으셨네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014636.1.16",
          "form": "네 아무래도 그렇죠 근데 그럼에도 불구하고 재밌다고 하더라구요ㅋㅋ",
          "original_form": "네 아무래도 그렇죠 근데 그럼에도 불구하고 재밌다고 하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014636.1.17",
          "form": "첫 당근 무지 떨렸어요",
          "original_form": "첫 당근 무지 떨렸어요",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014636.1.18",
          "form": "어떻게 알아보셨어요ㅎㅎ",
          "original_form": "어떻게 알아보셨어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014636.1.19",
          "form": "책을 들고계셨나ㅋㅋ",
          "original_form": "책을 들고계셨나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014636.1.20",
          "form": "큰 쇼핑백들고 폰하시길래 ㅋㅋ",
          "original_form": "큰 쇼핑백들고 폰하시길래 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014636.1.21",
          "form": "저기요 당근?",
          "original_form": "저기요 당근?",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014636.1.22",
          "form": "했어요",
          "original_form": "했어요",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014636.1.23",
          "form": "아ㅋㅋ 저도 최근에 옷장정리를 해서 당근에 팔 물건이 많은데 막상 하려니까 귀찮아서 미루게 되네요",
          "original_form": "아ㅋㅋ 저도 최근에 옷장정리를 해서 당근에 팔 물건이 많은데 막상 하려니까 귀찮아서 미루게 되네요",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014636.1.24",
          "form": "이뿌게 찍어야한다구 하더라구요 ㅋㅋ",
          "original_form": "이뿌게 찍어야한다구 하더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014636.1.25",
          "form": "아 제가 워낙 똥손이라 쉽지않네요ㅋㅋ",
          "original_form": "아 제가 워낙 똥손이라 쉽지않네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014636.1.26",
          "form": "화이팅 처음이 어렵지 ㅋㅋ",
          "original_form": "화이팅 처음이 어렵지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014636.1.27",
          "form": "하루 날 잡고 해봐야겠어요ㅎㅎ",
          "original_form": "하루 날 잡고 해봐야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014636.1.28",
          "form": "요즘 갖고싶은 물건 있으신가요?",
          "original_form": "요즘 갖고싶은 물건 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014636.1.29",
          "form": "저는 새로 나온 갤럭시 폴드가 자꾸만 눈에 띄네요ㅋㅋ",
          "original_form": "저는 새로 나온 갤럭시 폴드가 자꾸만 눈에 띄네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014636.1.30",
          "form": "저도 요즘 투고해보고 싶어서 계속 찾아봐요",
          "original_form": "저도 요즘 투고해보고 싶어서 계속 찾아봐요",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014636.1.31",
          "form": "폰바꾼지 일년밖에...",
          "original_form": "폰바꾼지 일년밖에...",
          "speaker_id": "1",
          "time": "20210825 20:16"
        }
      ]
    }
