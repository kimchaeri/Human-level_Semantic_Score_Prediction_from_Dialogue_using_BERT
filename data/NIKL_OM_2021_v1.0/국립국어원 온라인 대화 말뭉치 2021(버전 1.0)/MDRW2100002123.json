      "utterance": [
        {
          "id": "MDRW2100002123.1.1",
          "form": "공차에 윈터멜론티 먹고싶다",
          "original_form": "공차에 윈터멜론티 먹고싶다",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002123.1.2",
          "form": "아 나는 그거 항상",
          "original_form": "아 나는 그거 항상",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002123.1.3",
          "form": "근데 사실 공차 몇번 안가봄",
          "original_form": "근데 사실 공차 몇번 안가봄",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002123.1.4",
          "form": "워터멜론으로 보여ㅠㅠ",
          "original_form": "워터멜론으로 보여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002123.1.5",
          "form": "이름 좀 잘짓지!",
          "original_form": "이름 좀 잘짓지!",
          "speaker_id": "2",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002123.1.6",
          "form": "다들 워터멜론이라고해ㅠ",
          "original_form": "다들 워터멜론이라고해ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:47"
        },
        {
          "id": "MDRW2100002123.1.7",
          "form": "공차알바생들 맨날",
          "original_form": "공차알바생들 맨날",
          "speaker_id": "2",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.8",
          "form": "윈터멜론이라는...채소가 있는골 어떡해유ㅠ",
          "original_form": "윈터멜론이라는...채소가 있는골 어떡해유ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.9",
          "form": "고객님~ 윈터멜론인데 괜찮으세용",
          "original_form": "고객님~ 윈터멜론인데 괜찮으세용",
          "speaker_id": "2",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.10",
          "form": "이라고 물어볼듯",
          "original_form": "이라고 물어볼듯",
          "speaker_id": "2",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.11",
          "form": "아진짜?",
          "original_form": "아진짜?",
          "speaker_id": "1",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.12",
          "form": "아니거든요",
          "original_form": "아니거든요",
          "speaker_id": "1",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.13",
          "form": "그냥 내 추측ㅎㅎㅎ",
          "original_form": "그냥 내 추측ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.14",
          "form": "윈터멜론 시키는 사람들",
          "original_form": "윈터멜론 시키는 사람들",
          "speaker_id": "1",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.15",
          "form": "맛잘알이라 그런거 잘 시키거든요",
          "original_form": "맛잘알이라 그런거 잘 시키거든요",
          "speaker_id": "1",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.16",
          "form": "나도 공차...태어나서 5번..? 가본듯",
          "original_form": "나도 공차...태어나서 5번..? 가본듯",
          "speaker_id": "2",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.17",
          "form": "자의식과잉이네",
          "original_form": "자의식과잉이네",
          "speaker_id": "2",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.18",
          "form": "나도 그정도 ㅋㅋㅋㅋ",
          "original_form": "나도 그정도 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.19",
          "form": "오늘 빽다방 가려고했는데",
          "original_form": "오늘 빽다방 가려고했는데",
          "speaker_id": "1",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.20",
          "form": "그냥.. 귀찮아져서 지에스 커피 마심",
          "original_form": "그냥.. 귀찮아져서 지에스 커피 마심",
          "speaker_id": "1",
          "time": "20210819 23:48"
        },
        {
          "id": "MDRW2100002123.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ가격은 비슷한가?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ가격은 비슷한가?",
          "speaker_id": "2",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002123.1.22",
          "form": "지에스 커피는 아니고 지에스에서 파는 멕스웰하우스..",
          "original_form": "지에스 커피는 아니고 지에스에서 파는 멕스웰하우스..",
          "speaker_id": "1",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002123.1.23",
          "form": "행사해서 2500원에 두개주더라",
          "original_form": "행사해서 2500원에 두개주더라",
          "speaker_id": "1",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002123.1.24",
          "form": "잠 안자려고 먹은거?",
          "original_form": "잠 안자려고 먹은거?",
          "speaker_id": "2",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002123.1.25",
          "form": "근데 오백미리짜리라서 양 많음",
          "original_form": "근데 오백미리짜리라서 양 많음",
          "speaker_id": "1",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002123.1.26",
          "form": "아니 걍 요즘 두잔은 마셔야",
          "original_form": "아니 걍 요즘 두잔은 마셔야",
          "speaker_id": "1",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002123.1.27",
          "form": "오 좋다 양많은거",
          "original_form": "오 좋다 양많은거",
          "speaker_id": "2",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002123.1.28",
          "form": "뭔가... 열심히 살아있더라",
          "original_form": "뭔가... 열심히 살아있더라",
          "speaker_id": "1",
          "time": "20210819 23:49"
        },
        {
          "id": "MDRW2100002123.1.29",
          "form": "근데 잤다 마시고",
          "original_form": "근데 잤다 마시고",
          "speaker_id": "1",
          "time": "20210819 23:49"
        }
      ]
    }
