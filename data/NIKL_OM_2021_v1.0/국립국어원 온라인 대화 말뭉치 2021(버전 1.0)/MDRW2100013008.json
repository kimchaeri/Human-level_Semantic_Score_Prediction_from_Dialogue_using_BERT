      "utterance": [
        {
          "id": "MDRW2100013008.1.1",
          "form": "유튜브 자주보시나요",
          "original_form": "유튜브 자주보시나요",
          "speaker_id": "1",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100013008.1.2",
          "form": "자주는 아니고 요리나 매탈남(고양이)채널봐요.  다주보시나요?",
          "original_form": "자주는 아니고 요리나 매탈남(고양이)채널봐요.  다주보시나요?",
          "speaker_id": "2",
          "time": "20210825 10:20"
        },
        {
          "id": "MDRW2100013008.1.3",
          "form": "자주 유튜브 보시나요?",
          "original_form": "자주 유튜브 보시나요?",
          "speaker_id": "2",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100013008.1.4",
          "form": "여행유투브봅니다",
          "original_form": "여행유투브봅니다",
          "speaker_id": "1",
          "time": "20210825 10:21"
        },
        {
          "id": "MDRW2100013008.1.5",
          "form": "오 채널 몇개 알려주실수 있으세요?",
          "original_form": "오 채널 몇개 알려주실수 있으세요?",
          "speaker_id": "2",
          "time": "20210825 10:22"
        },
        {
          "id": "MDRW2100013008.1.6",
          "form": "빠니보틀 재밋어효",
          "original_form": "빠니보틀 재밋어효",
          "speaker_id": "1",
          "time": "20210825 10:22"
        },
        {
          "id": "MDRW2100013008.1.7",
          "form": "오 감사해요 빠니보틀 찾아봐야겠어요 ㅎ",
          "original_form": "오 감사해요 빠니보틀 찾아봐야겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 10:23"
        },
        {
          "id": "MDRW2100013008.1.8",
          "form": "지금 미국가있어요",
          "original_form": "지금 미국가있어요",
          "speaker_id": "1",
          "time": "20210825 10:23"
        },
        {
          "id": "MDRW2100013008.1.9",
          "form": "음 미국에 있군요 여행 유튜버는 안봤는데 덕분에 알게 되네요 ㅎㅎ",
          "original_form": "음 미국에 있군요 여행 유튜버는 안봤는데 덕분에 알게 되네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 10:26"
        },
        {
          "id": "MDRW2100013008.1.10",
          "form": "유퀴즈 보시나요?",
          "original_form": "유퀴즈 보시나요?",
          "speaker_id": "2",
          "time": "20210825 10:29"
        },
        {
          "id": "MDRW2100013008.1.11",
          "form": "자기님이신가요",
          "original_form": "자기님이신가요",
          "speaker_id": "1",
          "time": "20210825 11:17"
        },
        {
          "id": "MDRW2100013008.1.12",
          "form": "대답센스 무엇ㅋㅋ아시는군요?",
          "original_form": "대답센스 무엇ㅋㅋ아시는군요?",
          "speaker_id": "2",
          "time": "20210825 11:18"
        },
        {
          "id": "MDRW2100013008.1.13",
          "form": "너무 재밌어요ㅎㅎ",
          "original_form": "너무 재밌어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 11:20"
        },
        {
          "id": "MDRW2100013008.1.14",
          "form": "이번에 올림픽선수들꺼도 좋더라구요ㅎㅎ",
          "original_form": "이번에 올림픽선수들꺼도 좋더라구요ㅎㅎ",
