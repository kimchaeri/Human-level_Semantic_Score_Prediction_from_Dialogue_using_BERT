        },
        {
          "id": "MDRW2100042777.1.3",
          "form": "혹시 연애중이신가요?",
          "original_form": "혹시 연애중이신가요?",
          "speaker_id": "1",
          "time": "20210907 09:00"
        },
        {
          "id": "MDRW2100042777.1.4",
          "form": "아뇨 ㅎㅎㅎ name1님은요?",
          "original_form": "아뇨 ㅎㅎㅎ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 09:03"
        },
        {
          "id": "MDRW2100042777.1.5",
          "form": "저도! ㅋㅋ",
          "original_form": "저도! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:04"
        },
        {
          "id": "MDRW2100042777.1.6",
          "form": "연애안한지 오래되셨나요? ㅋㅋ",
          "original_form": "연애안한지 오래되셨나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:04"
        },
        {
          "id": "MDRW2100042777.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ아 좀 애매해요 절 좋아하는 사람이랑 같이 다니긴 햇는데 저는 그사람을 안좋아햇ㅇ러서..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 좀 애매해요 절 좋아하는 사람이랑 같이 다니긴 햇는데 저는 그사람을 안좋아햇ㅇ러서..",
          "speaker_id": "2",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042777.1.8",
          "form": "아....ㅜㅠ",
          "original_form": "아....ㅜㅠ",
          "speaker_id": "1",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042777.1.9",
          "form": "서로 좋아서 사귄지는 3년 된거같아요",
          "original_form": "서로 좋아서 사귄지는 3년 된거같아요",
          "speaker_id": "2",
          "time": "20210907 09:05"
        },
        {
          "id": "MDRW2100042777.1.10",
          "form": "그분은 소개로 만나신거예요?",
          "original_form": "그분은 소개로 만나신거예요?",
          "speaker_id": "1",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042777.1.11",
          "form": "오 아뇨 cc였어요 ㅋㅋㅋㅋ",
          "original_form": "오 아뇨 cc였어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042777.1.12",
          "form": "아 풋풋한!!! 부러워요!",
          "original_form": "아 풋풋한!!! 부러워요!",
          "speaker_id": "1",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042777.1.13",
          "form": "전 여대...ㅜㅠ",
