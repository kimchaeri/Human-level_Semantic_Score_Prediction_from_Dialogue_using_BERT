      "utterance": [
        {
          "id": "MDRW2100040047.1.1",
          "form": "스타벅스 엔제리너스 투썸플레이스 중에서 뭐가 제일 좋으세요",
          "original_form": "스타벅스 엔제리너스 투썸플레이스 중에서 뭐가 제일 좋으세요",
          "speaker_id": "1",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100040047.1.2",
          "form": "케잌은 투썸",
          "original_form": "케잌은 투썸",
          "speaker_id": "2",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100040047.1.3",
          "form": "공간은 스벅",
          "original_form": "공간은 스벅",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100040047.1.4",
          "form": "커피는 투썸 스벅 다 좋아해요ㅎ",
          "original_form": "커피는 투썸 스벅 다 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100040047.1.5",
          "form": "단골은 스벅이네요ㅎ",
          "original_form": "단골은 스벅이네요ㅎ",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100040047.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100040047.1.7",
          "form": "오 세부적인 관점 감사합니다",
          "original_form": "오 세부적인 관점 감사합니다",
          "speaker_id": "1",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100040047.1.8",
          "form": "저도 스벅 제일 자주 가는 거 같아요",
          "original_form": "저도 스벅 제일 자주 가는 거 같아요",
          "speaker_id": "1",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100040047.1.9",
          "form": "단골메뉴가 뭔가요",
          "original_form": "단골메뉴가 뭔가요",
          "speaker_id": "2",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100040047.1.10",
          "form": "아이스 아메리카노",
          "original_form": "아이스 아메리카노",
          "speaker_id": "1",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100040047.1.11",
          "form": "저 지금 수박 기프티콘 열 개 있어요",
          "original_form": "저 지금 수박 기프티콘 열 개 있어요",
          "speaker_id": "1",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100040047.1.12",
          "form": "스벅",
          "original_form": "스벅",
          "speaker_id": "1",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100040047.1.13",
          "form": "우와..!.!",
          "original_form": "우와..!.!",
          "speaker_id": "2",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100040047.1.14",
          "form": "깊티 부자네요",
          "original_form": "깊티 부자네요",
          "speaker_id": "2",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100040047.1.15",
          "form": "어쩌다 그렇게 많이 생겼나요",
          "original_form": "어쩌다 그렇게 많이 생겼나요",
          "speaker_id": "2",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040047.1.16",
          "form": "관공서 이벤트 집중 응모로요",
          "original_form": "관공서 이벤트 집중 응모로요",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100040047.1.17",
          "form": "관공서 이벤트가 그렇게 많았다니\n!!!!.",
          "original_form": "관공서 이벤트가 그렇게 많았다니\n!!!!.",
          "speaker_id": "2",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100040047.1.18",
          "form": "인생 헛살았네요ㅜ",
          "original_form": "인생 헛살았네요ㅜ",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100040047.1.19",
          "form": "우리나라의 공공기관이 그렇게 많은 줄 처음 알았어요",
          "original_form": "우리나라의 공공기관이 그렇게 많은 줄 처음 알았어요",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100040047.1.20",
          "form": "그러고 가끔 진짜 경쟁률 낮서 일대일 미달 된 것도 있었어요",
          "original_form": "그러고 가끔 진짜 경쟁률 낮서 일대일 미달 된 것도 있었어요",
          "speaker_id": "1",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100040047.1.21",
          "form": "헉 그런정보 어디서 얻으세요?",
          "original_form": "헉 그런정보 어디서 얻으세요?",
          "speaker_id": "2",
          "time": "20210904 02:04"
        },
        {
          "id": "MDRW2100040047.1.22",
          "form": "그냥 제가 일일이 검색했어요",
          "original_form": "그냥 제가 일일이 검색했어요",
          "speaker_id": "1",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100040047.1.23",
          "form": "역시 부지런이 답이네요!",
          "original_form": "역시 부지런이 답이네요!",
          "speaker_id": "2",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100040047.1.24",
          "form": "저도 좀 찾아봐야겠네요",
          "original_form": "저도 좀 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100040047.1.25",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210904 02:22"
        },
        {
          "id": "MDRW2100040047.1.26",
          "form": "좋은건 인터넷에 공유안할거 같더라구요",
