      "utterance": [
        {
          "id": "MDRW2100006702.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100006702.1.2",
          "form": "아니요 직장인입니다. 학생이신가요?",
          "original_form": "아니요 직장인입니다. 학생이신가요?",
          "speaker_id": "2",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100006702.1.3",
          "form": "저도 직장인입니다. 학생 때 기억나세요?",
          "original_form": "저도 직장인입니다. 학생 때 기억나세요?",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100006702.1.4",
          "form": "그럼요. 아직 학생으로 살았던 때가 훨씬 길어서요 ㅎㅎ",
          "original_form": "그럼요. 아직 학생으로 살았던 때가 훨씬 길어서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100006702.1.5",
          "form": "아~ 아직 학교 사람들과 연락하시나요?",
          "original_form": "아~ 아직 학교 사람들과 연락하시나요?",
          "speaker_id": "1",
          "time": "20210908 07:14"
        },
        {
          "id": "MDRW2100006702.1.6",
          "form": "고등학교 대학교 친구들은 아직 연락합니다",
          "original_form": "고등학교 대학교 친구들은 아직 연락합니다",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100006702.1.7",
          "form": "님은어떠신가요?",
          "original_form": "님은어떠신가요?",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100006702.1.8",
          "form": "저는 사회생활을 하다보니 많이 끊겼어요",
          "original_form": "저는 사회생활을 하다보니 많이 끊겼어요",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100006702.1.9",
          "form": "동아리도 하셨어요?",
          "original_form": "동아리도 하셨어요?",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100006702.1.10",
          "form": "저도 거의 끊기고 몇명안돼요",
          "original_form": "저도 거의 끊기고 몇명안돼요",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100006702.1.11",
          "form": "대학때는 국악동아리했습니다",
          "original_form": "대학때는 국악동아리했습니다",
          "speaker_id": "2",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100006702.1.12",
          "form": "와 국악동아리 멋지네요.",
          "original_form": "와 국악동아리 멋지네요.",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100006702.1.13",
          "form": "지금도 국악하세요?",
          "original_form": "지금도 국악하세요?",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100006702.1.14",
          "form": "아뇨지금은안하고요 동아리때만 잠깐한거에요",
          "original_form": "아뇨지금은안하고요 동아리때만 잠깐한거에요",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100006702.1.15",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100006702.1.16",
          "form": "동아리 어떤거하셨나요?",
          "original_form": "동아리 어떤거하셨나요?",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100006702.1.17",
          "form": "전 순우리말 ...ㅋㅋ",
          "original_form": "전 순우리말 ...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100006702.1.18",
          "form": "와 신기하네요",
          "original_form": "와 신기하네요",
          "speaker_id": "2",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100006702.1.19",
          "form": "순우리말 많이 아시겠어요",
          "original_form": "순우리말 많이 아시겠어요",
          "speaker_id": "2",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100006702.1.20",
          "form": "욕만 배우고ㅋㅋ",
          "original_form": "욕만 배우고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100006702.1.21",
          "form": "그냥 술만ㅋㅋ",
          "original_form": "그냥 술만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100006702.1.22",
          "form": "순우리말욕을 배우셨군요",
          "original_form": "순우리말욕을 배우셨군요",
