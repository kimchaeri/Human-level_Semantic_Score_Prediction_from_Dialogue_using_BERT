      "utterance": [
        {
          "id": "MDRW2100045895.1.1",
          "form": "즐겨보는 프로 있으세여",
          "original_form": "즐겨보는 프로 있으세여",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045895.1.2",
          "form": "딱히 없는데 예능 프로 좋아해요.",
          "original_form": "딱히 없는데 예능 프로 좋아해요.",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045895.1.3",
          "form": "저도 예능프로 돌려보는거 좋아해요ㅋ",
          "original_form": "저도 예능프로 돌려보는거 좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045895.1.4",
          "form": "예능토크쇼 재밌어요",
          "original_form": "예능토크쇼 재밌어요",
          "speaker_id": "2",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100045895.1.5",
          "form": "유퀴즈 자주봐요",
          "original_form": "유퀴즈 자주봐요",
          "speaker_id": "1",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045895.1.6",
          "form": "저도 유퀴드나  아는 형님 하고 있으면 봐요",
          "original_form": "저도 유퀴드나  아는 형님 하고 있으면 봐요",
          "speaker_id": "2",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045895.1.7",
          "form": "아는형님시간이 옮겨서\r\n놀토랑 같은시간에 하더라고요",
          "original_form": "아는형님시간이 옮겨서\r\n놀토랑 같은시간에 하더라고요",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045895.1.8",
          "form": "저는 본방으로 보는건 아니라서.ㅋㅋㅋ",
          "original_form": "저는 본방으로 보는건 아니라서.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045895.1.9",
          "form": "전 놀토를 챙겨보거든요ㅋ",
          "original_form": "전 놀토를 챙겨보거든요ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045895.1.10",
          "form": "놀토 재밌나요?",
          "original_form": "놀토 재밌나요?",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045895.1.11",
          "form": "네 노래맞추는거 열심히 같이해요ㅋ",
          "original_form": "네 노래맞추는거 열심히 같이해요ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100045895.1.12",
          "form": "노래 맞추는게 프로 다인거에요?",
          "original_form": "노래 맞추는게 프로 다인거에요?",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045895.1.13",
          "form": "맞추면 음식먹고\r\n간식게임도 있어요ㅋㅋ",
          "original_form": "맞추면 음식먹고\r\n간식게임도 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100045895.1.14",
          "form": "아. 한번씩 채널돌리다 봤어요.^^",
          "original_form": "아. 한번씩 채널돌리다 봤어요.^^",
