      "utterance": [
        {
          "id": "MDRW2100030733.1.1",
          "form": "아 그런 거예요 왜 그렇죠",
          "original_form": "아 그런 거예요 왜 그렇죠",
          "speaker_id": "2",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.2",
          "form": "특가 같은 것은 잘 구할 자신 있어요",
          "original_form": "특가 같은 것은 잘 구할 자신 있어요",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.3",
          "form": "에어 프라이기는 크기가 큰게 중요하다고 하던데요",
          "original_form": "에어 프라이기는 크기가 큰게 중요하다고 하던데요",
          "speaker_id": "2",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.4",
          "form": "특가 찾아 삼만리",
          "original_form": "특가 찾아 삼만리",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.5",
          "form": "하긴 기계는 새 거가 훨씬 좋죠",
          "original_form": "하긴 기계는 새 거가 훨씬 좋죠",
          "speaker_id": "2",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.6",
          "form": "에어프라이기는 클수록 좋아요",
          "original_form": "에어프라이기는 클수록 좋아요",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.7",
          "form": "전 샌드위치 만드는 기계 사고 싶어요",
          "original_form": "전 샌드위치 만드는 기계 사고 싶어요",
          "speaker_id": "2",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.8",
          "form": "그렇게 쓰다 보면 음식이 눌러 붙거든요",
          "original_form": "그렇게 쓰다 보면 음식이 눌러 붙거든요",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.9",
          "form": "그래서 새 거 사야 해요",
          "original_form": "그래서 새 거 사야 해요",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.10",
          "form": "생지 사서 넣어서 구워서 먹으면 좀 맛",
          "original_form": "생지 사서 넣어서 구워서 먹으면 좀 맛",
          "speaker_id": "2",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.11",
          "form": "좋은 팁 감사합니다",
          "original_form": "좋은 팁 감사합니다",
          "speaker_id": "2",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.12",
          "form": "에어프라이기 생지 넣고 굽기 좋아요",
          "original_form": "에어프라이기 생지 넣고 굽기 좋아요",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.13",
          "form": "아니 뭘요",
          "original_form": "아니 뭘요",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030733.1.14",
          "form": "에어프라이기를 사야겠군요",
          "original_form": "에어프라이기를 사야겠군요",
          "speaker_id": "2",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030733.1.15",
          "form": "밥솥도 사야 해요",
          "original_form": "밥솥도 사야 해요",
          "speaker_id": "1",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030733.1.16",
          "form": "밥솥은 쿠쿠인가요",
          "original_form": "밥솥은 쿠쿠인가요",
          "speaker_id": "2",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030733.1.17",
          "form": "밥그릇도 컵도",
          "original_form": "밥그릇도 컵도",
          "speaker_id": "1",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030733.1.18",
          "form": "쿠쿠 하세요 쿠쿠",
          "original_form": "쿠쿠 하세요 쿠쿠",
          "speaker_id": "2",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030733.1.19",
          "form": "쿠쿠썼지요",
          "original_form": "쿠쿠썼지요",
          "speaker_id": "1",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030733.1.20",
          "form": "쿠쿠가 유명한 제품이죠",
          "original_form": "쿠쿠가 유명한 제품이죠",
          "speaker_id": "2",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030733.1.21",
          "form": "지인주고왔어요",
          "original_form": "지인주고왔어요",
          "speaker_id": "1",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030733.1.22",
          "form": "쿠쿠 비싸잖아요 너무 안타깝네요",
          "original_form": "쿠쿠 비싸잖아요 너무 안타깝네요",
          "speaker_id": "2",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030733.1.23",
          "form": "베트남에도 쿠쿠매장있어요",
          "original_form": "베트남에도 쿠쿠매장있어요",
          "speaker_id": "1",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030733.1.24",
          "form": "한국인들이 많다보니 쿠쿠 매장도 있나 봐요",
          "original_form": "한국인들이 많다보니 쿠쿠 매장도 있나 봐요",
          "speaker_id": "2",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030733.1.25",
          "form": "중고로 샀지비",
          "original_form": "중고로 샀지비",
          "speaker_id": "1",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030733.1.26",
          "form": "귀국하는 사람들이 싸게 파는거 샀어요",
          "original_form": "귀국하는 사람들이 싸게 파는거 샀어요",
          "speaker_id": "1",
          "time": "20210904 12:04"
        }
      ]
    }
