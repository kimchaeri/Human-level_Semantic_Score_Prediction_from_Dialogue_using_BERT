      "utterance": [
        {
          "id": "MDRW2100038589.1.1",
          "form": "누구?",
          "original_form": "누구?",
          "speaker_id": "2",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100038589.1.2",
          "form": "여행자",
          "original_form": "여행자",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100038589.1.3",
          "form": "무슨 검?",
          "original_form": "무슨 검?",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038589.1.4",
          "form": "나 못봤어",
          "original_form": "나 못봤어",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038589.1.5",
          "form": "한손검",
          "original_form": "한손검",
          "speaker_id": "1",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038589.1.6",
          "form": "나도 이름만 들음",
          "original_form": "나도 이름만 들음",
          "speaker_id": "1",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038589.1.7",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038589.1.8",
          "form": "주인공 파티창에서 분류는",
          "original_form": "주인공 파티창에서 분류는",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038589.1.9",
          "form": "5성이던데",
          "original_form": "5성이던데",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038589.1.10",
          "form": "왤케 성능이 애매한거야....",
          "original_form": "왤케 성능이 애매한거야....",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038589.1.11",
          "form": "솔직히 케이아 냅두고 주인공 빼면",
          "original_form": "솔직히 케이아 냅두고 주인공 빼면",
          "speaker_id": "2",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100038589.1.12",
          "form": "더 깔끔한 파티이긴 해",
          "original_form": "더 깔끔한 파티이긴 해",
          "speaker_id": "2",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100038589.1.13",
          "form": "강림의 검",
          "original_form": "강림의 검",
          "speaker_id": "1",
          "time": "20210902 16:28"
        },
        {
          "id": "MDRW2100038589.1.14",
          "form": "어디서 얻는건데?",
          "original_form": "어디서 얻는건데?",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100038589.1.15",
          "form": "가챠?",
          "original_form": "가챠?",
          "speaker_id": "2",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100038589.1.16",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100038589.1.17",
          "form": "플레이스테이션으로 접속해야만 줌",
          "original_form": "플레이스테이션으로 접속해야만 줌",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100038589.1.18",
          "form": "여행자 착용시 공격력 상승 버프 붙어있고",
          "original_form": "여행자 착용시 공격력 상승 버프 붙어있고",
          "speaker_id": "1",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100038589.1.19",
          "form": "플스가 아니면 못씀...",
          "original_form": "플스가 아니면 못씀...",
          "speaker_id": "1",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100038589.1.20",
          "form": "ㅇㅎ....치사하다",
          "original_form": "ㅇㅎ....치사하다",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100038589.1.21",
          "form": "플스없는데",
          "original_form": "플스없는데",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100038589.1.22",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100038589.1.23",
          "form": "이번에 에일로이도",
          "original_form": "이번에 에일로이도",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100038589.1.24",
          "form": "플스유저만 2.1에서 주고",
          "original_form": "플스유저만 2.1에서 주고",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100038589.1.25",
          "form": "전체유저 2.2에서 주고...",
          "original_form": "전체유저 2.2에서 주고...",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100038589.1.26",
          "form": "아무리 플스에 신경 많이 써도",
          "original_form": "아무리 플스에 신경 많이 써도",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100038589.1.27",
          "form": "근본은 모바일겜이면서",
          "original_form": "근본은 모바일겜이면서",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100038589.1.28",
          "form": "ㅇㅇㅋㅋㅋㅋ 모바일이라서 흥행하는건데",
          "original_form": "ㅇㅇㅋㅋㅋㅋ 모바일이라서 흥행하는건데",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100038589.1.29",
          "form": "ㅇㅇ플스유저도",
          "original_form": "ㅇㅇ플스유저도",
          "speaker_id": "2",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100038589.1.30",
          "form": "모바일로 잡고 시작했을텐데",
          "original_form": "모바일로 잡고 시작했을텐데",
          "speaker_id": "2",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100038589.1.31",
          "form": "근데 플스에 미련이 있나봐 ㅎ",
          "original_form": "근데 플스에 미련이 있나봐 ㅎ",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100038589.1.32",
          "form": "플스 하려면 오픈월드 더 탄탄히 하고 와라",
          "original_form": "플스 하려면 오픈월드 더 탄탄히 하고 와라",
          "speaker_id": "1",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100038589.1.33",
          "form": "ㅁㅈ 솔직히",
          "original_form": "ㅁㅈ 솔직히",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100038589.1.34",
          "form": "모바일겜이라 혁명이지",
          "original_form": "모바일겜이라 혁명이지",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100038589.1.35",
          "form": "플스겜이었으면",
          "original_form": "플스겜이었으면",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100038589.1.36",
          "form": "지금처럼 다른 사람 집도 안열어주고 껍데기만 구현된 월드는 플스에서 경쟁력 없음 ㅇㅇ",
          "original_form": "지금처럼 다른 사람 집도 안열어주고 껍데기만 구현된 월드는 플스에서 경쟁력 없음 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100038589.1.37",
          "form": "그냥그런 야숨짭이지",
          "original_form": "그냥그런 야숨짭이지",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100038589.1.38",
          "form": "ㅇㅇ플스의 세계가",
          "original_form": "ㅇㅇ플스의 세계가",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100038589.1.39",
          "form": "얼마나 딮한데",
          "original_form": "얼마나 딮한데",
          "speaker_id": "2",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100038589.1.40",
          "form": "컴겜도 이거보단 자유도 높음",
          "original_form": "컴겜도 이거보단 자유도 높음",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100038589.1.41",
          "form": "ㅇㅇ 하다못해 검은사막도",
          "original_form": "ㅇㅇ 하다못해 검은사막도",
          "speaker_id": "1",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100038589.1.42",
          "form": "컴버전은 얘보단 잘되어있을걸?",
          "original_form": "컴버전은 얘보단 잘되어있을걸?",
          "speaker_id": "1",
          "time": "20210902 16:46"
        }
      ]
    }
