      "utterance": [
        {
          "id": "MDRW2100024429.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210831 09:08"
        },
        {
          "id": "MDRW2100024429.1.2",
          "form": "네ㅋㅋ 아까 구운계란 먹었어요~",
          "original_form": "네ㅋㅋ 아까 구운계란 먹었어요~",
          "speaker_id": "2",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024429.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024429.1.4",
          "form": "아!  저는 주스용!",
          "original_form": "아!  저는 주스용!",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024429.1.5",
          "form": "간편식이 최고네오ㅡㅣㄱㅋㅋㅋ",
          "original_form": "간편식이 최고네오ㅡㅣㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024429.1.6",
          "form": "근데 벌써 점심다가와요ㅜㅠ",
          "original_form": "근데 벌써 점심다가와요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 10:31"
        },
        {
          "id": "MDRW2100024429.1.7",
          "form": "돌밥 돌밥 입니다..",
          "original_form": "돌밥 돌밥 입니다..",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024429.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024429.1.9",
          "form": "주스 만들어드세요?",
          "original_form": "주스 만들어드세요?",
          "speaker_id": "2",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100024429.1.10",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024429.1.11",
          "form": "넹! 엄마찬스로 간편히 먹고있슴당ㅋㅋ",
          "original_form": "넹! 엄마찬스로 간편히 먹고있슴당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:34"
        },
        {
          "id": "MDRW2100024429.1.12",
          "form": "뭐 넣어서 만들어요?",
          "original_form": "뭐 넣어서 만들어요?",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024429.1.13",
          "form": "저도 뭐 디톡스 스무디 같은거 먹는데 전 주기적으로 주문해서 먹어요ㅋㅋ",
          "original_form": "저도 뭐 디톡스 스무디 같은거 먹는데 전 주기적으로 주문해서 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024429.1.14",
          "form": "해독주스나 파인양배추 대체적으로 냉동과일들에 양배추 넣어마시고 있어요~",
          "original_form": "해독주스나 파인양배추 대체적으로 냉동과일들에 양배추 넣어마시고 있어요~",
          "speaker_id": "1",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100024429.1.15",
          "form": "오오 그게 간편하고 좋긴해요! 가격이 좀 비싸다보니 갈아마시고 있어요ㅜㅜ",
          "original_form": "오오 그게 간편하고 좋긴해요! 가격이 좀 비싸다보니 갈아마시고 있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100024429.1.16",
          "form": "저도 만들어서 먹어보고 싶은데 재료를 일일이 사기가 귀찮아서 이러고 있네요..",
          "original_form": "저도 만들어서 먹어보고 싶은데 재료를 일일이 사기가 귀찮아서 이러고 있네요..",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100024429.1.17",
          "form": "그리고 제가 꾸준히 만들어 먹을 수 있을까 하는 의심이..ㅋㅋㅋ",
          "original_form": "그리고 제가 꾸준히 만들어 먹을 수 있을까 하는 의심이..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100024429.1.18",
          "form": "역시 건강은 부지런해야 챙기네요",
          "original_form": "역시 건강은 부지런해야 챙기네요",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100024429.1.19",
          "form": "맞아요ㅋㅋ쿠ㅜㅜ",
          "original_form": "맞아요ㅋㅋ쿠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 11:25"
        },
        {
          "id": "MDRW2100024429.1.20",
          "form": "저 점심부터는 막먹어서ㅋㅋ 아침이라도 양심껏..ㅋㅋㄱ",
          "original_form": "저 점심부터는 막먹어서ㅋㅋ 아침이라도 양심껏..ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 11:26"
        },
        {
          "id": "MDRW2100024429.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:22"
        },
        {
          "id": "MDRW2100024429.1.22",
          "form": "아침엔 양심 지키는거죠",
          "original_form": "아침엔 양심 지키는거죠",
          "speaker_id": "2",
          "time": "20210831 12:22"
        },
        {
          "id": "MDRW2100024429.1.23",
          "form": "전 밀가루 고기 액상과당 끊어서 음식에 좀 흥미를 잃었네요",
          "original_form": "전 밀가루 고기 액상과당 끊어서 음식에 좀 흥미를 잃었네요",
          "speaker_id": "2",
          "time": "20210831 12:23"
        },
        {
          "id": "MDRW2100024429.1.24",
          "form": "그쵸ㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100024429.1.25",
          "form": "헉..!? 그거 끊을 수 있는건가요..",
          "original_form": "헉..!? 그거 끊을 수 있는건가요..",
          "speaker_id": "1",
          "time": "20210831 12:25"
        },
        {
          "id": "MDRW2100024429.1.26",
          "form": "저도 불가능할것 같았는데",
          "original_form": "저도 불가능할것 같았는데",
          "speaker_id": "2",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100024429.1.27",
          "form": "그냥 하니까 해지더라구요",
          "original_form": "그냥 하니까 해지더라구요",
          "speaker_id": "2",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100024429.1.28",
          "form": "사실 아토피 같은것 때문이라 반강제입니다",
          "original_form": "사실 아토피 같은것 때문이라 반강제입니다",
          "speaker_id": "2",
          "time": "20210831 12:28"
        },
        {
          "id": "MDRW2100024429.1.29",
          "form": "아.. 아토피...",
          "original_form": "아.. 아토피...",
          "speaker_id": "1",
          "time": "20210831 12:36"
        },
        {
          "id": "MDRW2100024429.1.30",
          "form": "아토피시면 먹는거 신경쓰셔야해서 힘드시겠어요ㅠ",
          "original_form": "아토피시면 먹는거 신경쓰셔야해서 힘드시겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 12:36"
        }
      ]
