      "utterance": [
        {
          "id": "MDRW2100012117.1.1",
          "form": "키우고 싶은 반려동물이 있나요?",
          "original_form": "키우고 싶은 반려동물이 있나요?",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012117.1.2",
          "form": "반려식물도 좋아요",
          "original_form": "반려식물도 좋아요",
          "speaker_id": "1",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012117.1.3",
          "form": "강아지 키우고싶어요",
          "original_form": "강아지 키우고싶어요",
          "speaker_id": "2",
          "time": "20210824 19:20"
        },
        {
          "id": "MDRW2100012117.1.4",
          "form": "어릴땐 대형견 키우고 싶었고 지금도 그렇긴 한데",
          "original_form": "어릴땐 대형견 키우고 싶었고 지금도 그렇긴 한데",
          "speaker_id": "2",
          "time": "20210824 19:21"
        },
        {
          "id": "MDRW2100012117.1.5",
          "form": "강아지 좋지",
          "original_form": "강아지 좋지",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012117.1.6",
          "form": "꼬리 살랑거리면서 가까이오면",
          "original_form": "꼬리 살랑거리면서 가까이오면",
          "speaker_id": "1",
          "time": "20210824 19:23"
        },
        {
          "id": "MDRW2100012117.1.7",
          "form": "기분 좋아서 쓰러져",
          "original_form": "기분 좋아서 쓰러져",
          "speaker_id": "1",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100012117.1.8",
          "form": "으윽 삶의 행복이야",
          "original_form": "으윽 삶의 행복이야",
          "speaker_id": "1",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100012117.1.9",
          "form": "나는 대형견한테 산책 당할 거 같아서 무섭지만",
          "original_form": "나는 대형견한테 산책 당할 거 같아서 무섭지만",
          "speaker_id": "2",
          "time": "20210824 19:24"
        },
        {
          "id": "MDRW2100012117.1.10",
          "form": "그래도 리트리버 키우고 싶음",
          "original_form": "그래도 리트리버 키우고 싶음",
          "speaker_id": "2",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012117.1.11",
          "form": "너는 강아지나 고양이 중에 뭐가 좋아",
          "original_form": "너는 강아지나 고양이 중에 뭐가 좋아",
          "speaker_id": "2",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012117.1.12",
          "form": "리트리버 좋지",
          "original_form": "리트리버 좋지",
          "speaker_id": "1",
          "time": "20210824 19:49"
        },
        {
          "id": "MDRW2100012117.1.13",
          "form": "나는 예전에는 강아지는 리트리버나 허스키",
          "original_form": "나는 예전에는 강아지는 리트리버나 허스키",
          "speaker_id": "1",
          "time": "20210824 19:49"
        },
        {
          "id": "MDRW2100012117.1.14",
          "form": "고양이는 러시안 블루였는데",
          "original_form": "고양이는 러시안 블루였는데",
          "speaker_id": "1",
          "time": "20210824 19:49"
        },
        {
          "id": "MDRW2100012117.1.15",
          "form": "친구가 러시안 블루 키우는거 보니까 애가 너무 맹해서ㅋㅋㅋㅋ",
          "original_form": "친구가 러시안 블루 키우는거 보니까 애가 너무 맹해서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:49"
        },
        {
          "id": "MDRW2100012117.1.16",
          "form": "요샌 그냥 길냥이더라도 턱시도나 까만냥이 좋아",
          "original_form": "요샌 그냥 길냥이더라도 턱시도나 까만냥이 좋아",
          "speaker_id": "1",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012117.1.17",
          "form": "맹한 고양이 귀엽잖아",
          "original_form": "맹한 고양이 귀엽잖아",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012117.1.18",
          "form": "턱시도 좋지",
          "original_form": "턱시도 좋지",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012117.1.19",
          "form": "턱시도 좋아ㅠㅠ",
          "original_form": "턱시도 좋아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100012117.1.20",
          "form": "밤에 밟을까봐 겁나긴 하지만",
          "original_form": "밤에 밟을까봐 겁나긴 하지만",
          "speaker_id": "1",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100012117.1.21",
          "form": "귀여우면 됐다",
          "original_form": "귀여우면 됐다",
          "speaker_id": "1",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100012117.1.22",
          "form": "설마 밟히겠어",
          "original_form": "설마 밟히겠어",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100012117.1.23",
          "form": "얼마나 예민한데",
          "original_form": "얼마나 예민한데",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100012117.1.24",
          "form": "고냥이가 행복하면 됐다",
          "original_form": "고냥이가 행복하면 됐다",
          "speaker_id": "1",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100012117.1.25",
          "form": "맹한 고양이라면......그럴 가능성 높지...",
          "original_form": "맹한 고양이라면......그럴 가능성 높지...",
          "speaker_id": "1",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100012117.1.26",
          "form": "무시못해 그 맹함...",
          "original_form": "무시못해 그 맹함...",
          "speaker_id": "1",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100012117.1.27",
          "form": "나는 고양이보단 개가 더 귀엽달까",
          "original_form": "나는 고양이보단 개가 더 귀엽달까",
          "speaker_id": "2",
          "time": "20210824 19:54"
        },
        {
          "id": "MDRW2100012117.1.28",
          "form": "고양이는 약간 무표정하잖아",
          "original_form": "고양이는 약간 무표정하잖아",
          "speaker_id": "2",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100012117.1.29",
          "form": "무표정 사이의 미묘한 감정이 최고라구!",
          "original_form": "무표정 사이의 미묘한 감정이 최고라구!",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012117.1.30",
          "form": "근데 개도 좋아",
          "original_form": "근데 개도 좋아",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012117.1.31",
          "form": "근데 개는 딱히 종은 상관없는거 같아",
          "original_form": "근데 개는 딱히 종은 상관없는거 같아",
          "speaker_id": "1",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012117.1.32",
          "form": "나는 오히려 개가 더 종 따지게 되는듯",
          "original_form": "나는 오히려 개가 더 종 따지게 되는듯",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012117.1.33",
          "form": "물론 요즘 세상에 종 따지면 안되겠지만",
          "original_form": "물론 요즘 세상에 종 따지면 안되겠지만",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012117.1.34",
          "form": "고양이는 종 따라 외형만 조금 달라지는데 개는 너무 많은게 다름",
          "original_form": "고양이는 종 따라 외형만 조금 달라지는데 개는 너무 많은게 다름",
          "speaker_id": "2",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100012117.1.35",
          "form": "그건 그렇네",
          "original_form": "그건 그렇네",
          "speaker_id": "1",
          "time": "20210825 09:42"
        },
        {
          "id": "MDRW2100012117.1.36",
          "form": "하지만 케어할 자신이 있으면 나는 상관없는거 같아",
          "original_form": "하지만 케어할 자신이 있으면 나는 상관없는거 같아",
          "speaker_id": "1",
          "time": "20210825 09:42"
        },
        {
          "id": "MDRW2100012117.1.37",
          "form": "내 취향은 장모종 친구들이지만 입양을 생각한다면",
          "original_form": "내 취향은 장모종 친구들이지만 입양을 생각한다면",
          "speaker_id": "1",
          "time": "20210825 09:43"
        },
        {
          "id": "MDRW2100012117.1.38",
          "form": "일단 보호소를 간 뒤에 나에게 마음 열어주는 친구를 찾지 않을까 싶음",
          "original_form": "일단 보호소를 간 뒤에 나에게 마음 열어주는 친구를 찾지 않을까 싶음",
          "speaker_id": "1",
          "time": "20210825 09:43"
        },
        {
          "id": "MDRW2100012117.1.39",
          "form": "ㅇㅇ 여전히 큰 강아지 키우고 싶은 마음은 있다",
          "original_form": "ㅇㅇ 여전히 큰 강아지 키우고 싶은 마음은 있다",
          "speaker_id": "2",
          "time": "20210825 09:43"
        },
        {
          "id": "MDRW2100012117.1.40",
          "form": "맞아ㅋㅋㅋ",
          "original_form": "맞아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:43"
        },
        {
          "id": "MDRW2100012117.1.41",
          "form": "나도 대형견이 좋아",
          "original_form": "나도 대형견이 좋아",
          "speaker_id": "1",
          "time": "20210825 09:43"
        },
        {
          "id": "MDRW2100012117.1.42",
          "form": "나는 고양이는 외형 상관없고 나한테 부대껴 주는 애면 다 좋을듯",
          "original_form": "나는 고양이는 외형 상관없고 나한테 부대껴 주는 애면 다 좋을듯",
          "speaker_id": "2",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012117.1.43",
          "form": "여전히 리트리버와 허스키 못 잃어",
          "original_form": "여전히 리트리버와 허스키 못 잃어",
          "speaker_id": "1",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012117.1.44",
          "form": "나는 보더콜리도 키우고싶어졌는데",
          "original_form": "나는 보더콜리도 키우고싶어졌는데",
          "speaker_id": "2",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012117.1.45",
          "form": "보더콜리?ㅋㅋㅋ따라갈 수 있어?",
          "original_form": "보더콜리?ㅋㅋㅋ따라갈 수 있어?",
          "speaker_id": "1",
          "time": "20210825 09:44"
        },
        {
          "id": "MDRW2100012117.1.46",
          "form": "리트리버는 사냥개고 허스키는 썰매개고 보더콜리는 목양견이라",
          "original_form": "리트리버는 사냥개고 허스키는 썰매개고 보더콜리는 목양견이라",
          "speaker_id": "2",
          "time": "20210825 09:45"
        },
        {
          "id": "MDRW2100012117.1.47",
          "form": "나는 못해...",
          "original_form": "나는 못해...",
          "speaker_id": "1",
          "time": "20210825 09:45"
        },
        {
          "id": "MDRW2100012117.1.48",
          "form": "난 못 해...",
          "original_form": "난 못 해...",
          "speaker_id": "1",
          "time": "20210825 09:45"
        },
        {
          "id": "MDRW2100012117.1.49",
          "form": "셋 다 운동랑 쩔어주는 개임",
          "original_form": "셋 다 운동랑 쩔어주는 개임",
          "speaker_id": "2",
          "time": "20210825 09:45"
        },
        {
          "id": "MDRW2100012117.1.50",
          "form": "ㅇㅇ 못할거같음ㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 못할거같음ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 09:45"
        },
        {
          "id": "MDRW2100012117.1.51",
          "form": "나는 키우게되면",
          "original_form": "나는 키우게되면",
          "speaker_id": "1",
          "time": "20210825 09:47"
        },
        {
          "id": "MDRW2100012117.1.52",
          "form": "헬스장에서 근력운동 빠르게 끝내고",
          "original_form": "헬스장에서 근력운동 빠르게 끝내고",
          "speaker_id": "1",
          "time": "20210825 09:47"
        },
        {
          "id": "MDRW2100012117.1.53",
          "form": "바로 집에 와서 개 데리고 뛰러나갈듯",
          "original_form": "바로 집에 와서 개 데리고 뛰러나갈듯",
          "speaker_id": "1",
          "time": "20210825 09:47"
        },
        {
          "id": "MDRW2100012117.1.54",
          "form": "마자 유산소 따로 할 필요 없은",
          "original_form": "마자 유산소 따로 할 필요 없은",
          "speaker_id": "2",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100012117.1.55",
          "form": "개 데리고 산책하러 나가는 것이 바로 유산소",
          "original_form": "개 데리고 산책하러 나가는 것이 바로 유산소",
          "speaker_id": "2",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100012117.1.56",
          "form": "대형견이면 산책보단 누구하나는 끌려가지 않을까ㅋㅋㅋ",
          "original_form": "대형견이면 산책보단 누구하나는 끌려가지 않을까ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100012117.1.57",
          "form": "그게 내가 될진 걔가 될진",
          "original_form": "그게 내가 될진 걔가 될진",
          "speaker_id": "1",
          "time": "20210825 09:49"
        },
        {
          "id": "MDRW2100012117.1.58",
          "form": "모르겠지만",
          "original_form": "모르겠지만",
          "speaker_id": "1",
          "time": "20210825 09:50"
        },
        {
          "id": "MDRW2100012117.1.59",
          "form": "개를 끌고 갈 자신이 있나봐?ㅋㅋㅋㅋㄱ",
