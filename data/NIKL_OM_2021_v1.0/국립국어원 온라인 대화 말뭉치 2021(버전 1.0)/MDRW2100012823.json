      "utterance": [
        {
          "id": "MDRW2100012823.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 02:00"
        },
        {
          "id": "MDRW2100012823.1.2",
          "form": "네,안녕하세오~",
          "original_form": "네,안녕하세오~",
          "speaker_id": "2",
          "time": "20210825 02:01"
        },
        {
          "id": "MDRW2100012823.1.3",
          "form": "새벽이라그런지 배가고파지네요 ㅎㅎ",
          "original_form": "새벽이라그런지 배가고파지네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 02:02"
        },
        {
          "id": "MDRW2100012823.1.4",
          "form": "아.늦게 자면 가끔 배고플때도 있죠~",
          "original_form": "아.늦게 자면 가끔 배고플때도 있죠~",
          "speaker_id": "2",
          "time": "20210825 02:02"
        },
        {
          "id": "MDRW2100012823.1.5",
          "form": "저녁 뭐드셨어요?",
          "original_form": "저녁 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210825 02:03"
        },
        {
          "id": "MDRW2100012823.1.6",
          "form": "튀김떡볶이요~",
          "original_form": "튀김떡볶이요~",
          "speaker_id": "2",
          "time": "20210825 02:04"
        },
        {
          "id": "MDRW2100012823.1.7",
          "form": "저는 삼겹살먹었어요ㅎㅎ",
          "original_form": "저는 삼겹살먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 02:04"
        },
        {
          "id": "MDRW2100012823.1.8",
          "form": "오~고기맛있죠ㅎ",
          "original_form": "오~고기맛있죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:06"
        },
        {
          "id": "MDRW2100012823.1.9",
          "form": "저녁많이먹었는데 야식안먹어서그런지 ㅋㅋ 왜 배가고픈지ㅋㅋ",
          "original_form": "저녁많이먹었는데 야식안먹어서그런지 ㅋㅋ 왜 배가고픈지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:07"
        },
        {
          "id": "MDRW2100012823.1.10",
          "form": "가끔 그럴때도 있죠 저도그래요ㅎㅎ",
          "original_form": "가끔 그럴때도 있죠 저도그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 02:08"
        },
        {
          "id": "MDRW2100012823.1.11",
          "form": "배고프지면 뭐안드세요?",
          "original_form": "배고프지면 뭐안드세요?",
          "speaker_id": "2",
          "time": "20210825 02:08"
        },
        {
          "id": "MDRW2100012823.1.12",
          "form": "요새 살이쪄서 자제중이에요ㅠㅠ 맛집도 좋아하고 카페도 좋아하다보니 ㅎㅎ",
          "original_form": "요새 살이쪄서 자제중이에요ㅠㅠ 맛집도 좋아하고 카페도 좋아하다보니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 02:11"
        },
        {
          "id": "MDRW2100012823.1.13",
          "form": "맞아요 저도 살쪄서 조절중인데 힘드네요ㅎ",
          "original_form": "맞아요 저도 살쪄서 조절중인데 힘드네요ㅎ",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100012823.1.14",
          "form": "음식조절이 제일 어려운거같아요 ㅜㅜ",
          "original_form": "음식조절이 제일 어려운거같아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100012823.1.15",
          "form": "맞아요~ㅠ 먹는것도 좋아하는편이라서요",
          "original_form": "맞아요~ㅠ 먹는것도 좋아하는편이라서요",
          "speaker_id": "2",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100012823.1.16",
          "form": "저도그래요! 왜이리 맛있는 음식이 많죠?ㅋㅋ",
          "original_form": "저도그래요! 왜이리 맛있는 음식이 많죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100012823.1.17",
          "form": "그쵸ㅎㅎ",
          "original_form": "그쵸ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:50"
        }
      ]
