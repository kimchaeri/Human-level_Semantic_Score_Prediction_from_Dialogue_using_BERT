      "utterance": [
        {
          "id": "MDRW2100045361.1.1",
          "form": "헤어숍 자주 가세요?",
          "original_form": "헤어숍 자주 가세요?",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045361.1.2",
          "form": "전 연 1회ㅋㅋ",
          "original_form": "전 연 1회ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045361.1.3",
          "form": "두달에 한번정도?",
          "original_form": "두달에 한번정도?",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045361.1.4",
          "form": "연1회면 머리가 너무 무겁지 않아요?",
          "original_form": "연1회면 머리가 너무 무겁지 않아요?",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045361.1.5",
          "form": "단발이세요?",
          "original_form": "단발이세요?",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045361.1.6",
          "form": "아니요 긴머리인데",
          "original_form": "아니요 긴머리인데",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045361.1.7",
          "form": "숱이많아서",
          "original_form": "숱이많아서",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045361.1.8",
          "form": "숱을 쳐야해요ㅠㅠ",
          "original_form": "숱을 쳐야해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045361.1.9",
          "form": "저 아예 단발ㅈ로 자르고 펌해요!",
          "original_form": "저 아예 단발ㅈ로 자르고 펌해요!",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045361.1.10",
          "form": "그리고 긴 웨이브됐다가 다시 컷",
          "original_form": "그리고 긴 웨이브됐다가 다시 컷",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045361.1.11",
          "form": "단발이시구나",
          "original_form": "단발이시구나",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045361.1.12",
          "form": "전 단발이 안어울려요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 단발이 안어울려요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045361.1.13",
          "form": "근데 막상 자르면 몰라요!",
          "original_form": "근데 막상 자르면 몰라요!",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045361.1.14",
          "form": "저도 30년간 길게 살다 훅",
          "original_form": "저도 30년간 길게 살다 훅",
          "speaker_id": "1",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100045361.1.15",
          "form": "오 ㅋㅋㅋㅋㅋ 과감한 도전을 하셧군요",
          "original_form": "오 ㅋㅋㅋㅋㅋ 과감한 도전을 하셧군요",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045361.1.16",
          "form": "네 은근 다들 단발 잘어울려요!! 용기내세요!",
          "original_form": "네 은근 다들 단발 잘어울려요!! 용기내세요!",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045361.1.17",
          "form": "용기가 생기면 시도해볼게요 ㅋㅋㅋㅋㅋ",
          "original_form": "용기가 생기면 시도해볼게요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045361.1.18",
          "form": "아직은 용기가...ㅋㅋㅋ",
          "original_form": "아직은 용기가...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045361.1.19",
          "form": "ㅋㅋ 다들 이삐요!!!",
          "original_form": "ㅋㅋ 다들 이삐요!!!",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045361.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네일은 하시나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네일은 하시나요?",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045361.1.21",
          "form": "예전에 하다 지금은 아뇨",
          "original_form": "예전에 하다 지금은 아뇨",
          "speaker_id": "1",
