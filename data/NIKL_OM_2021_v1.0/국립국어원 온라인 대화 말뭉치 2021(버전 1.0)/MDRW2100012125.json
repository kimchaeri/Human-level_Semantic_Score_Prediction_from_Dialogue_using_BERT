      "utterance": [
        {
          "id": "MDRW2100012125.1.1",
          "form": "진짜 초대 많이함 ㅋㅋㅋㅋ",
          "original_form": "진짜 초대 많이함 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012125.1.2",
          "form": "가끔 지인이 오는거지요 히히",
          "original_form": "가끔 지인이 오는거지요 히히",
          "speaker_id": "1",
          "time": "20210824 19:25"
        },
        {
          "id": "MDRW2100012125.1.3",
          "form": "오랫만에 수다떨었다이ㅋㅋ",
          "original_form": "오랫만에 수다떨었다이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012125.1.4",
          "form": "울신랑은 오자마자",
          "original_form": "울신랑은 오자마자",
          "speaker_id": "1",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012125.1.5",
          "form": "최신가요 다운받아서",
          "original_form": "최신가요 다운받아서",
          "speaker_id": "1",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012125.1.6",
          "form": "ㅋㅋㅋㅋㅋ신랑 배고프겠다 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ신랑 배고프겠다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012125.1.7",
          "form": "자동차 usb에 넣어주고ㅋㅋ",
          "original_form": "자동차 usb에 넣어주고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012125.1.8",
          "form": "지금 우퍼사운드로 음악틀어주고",
          "original_form": "지금 우퍼사운드로 음악틀어주고",
          "speaker_id": "1",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012125.1.9",
          "form": "나갔다",
          "original_form": "나갔다",
          "speaker_id": "1",
          "time": "20210824 19:26"
        },
        {
          "id": "MDRW2100012125.1.10",
          "form": "신랑도 의지가 있네~울신랑은 6시만 넘어도 배고프다고 난린데~",
          "original_form": "신랑도 의지가 있네~울신랑은 6시만 넘어도 배고프다고 난린데~",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012125.1.11",
          "form": "ㅋㅋㅋㅋㄱㄲㄱㅋㅋㅋ오디오, 음악, 우퍼를 사랑하는 남편이다",
          "original_form": "ㅋㅋㅋㅋㄱㄲㄱㅋㅋㅋ오디오, 음악, 우퍼를 사랑하는 남편이다",
          "speaker_id": "1",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012125.1.12",
          "form": "응응ㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "응응ㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012125.1.13",
          "form": "그렇네~전에 뭐 홈시어터 샀다 캤나???ㅋㅋ",
          "original_form": "그렇네~전에 뭐 홈시어터 샀다 캤나???ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012125.1.14",
          "form": "오디오에 진심이다ㅋㅋ",
          "original_form": "오디오에 진심이다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012125.1.15",
          "form": "응응 샀지",
          "original_form": "응응 샀지",
          "speaker_id": "1",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012125.1.16",
          "form": "당근으로ㅋㅋ",
          "original_form": "당근으로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012125.1.17",
          "form": "다들 자기가 꽂히는게 하나씩있다이~",
          "original_form": "다들 자기가 꽂히는게 하나씩있다이~",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012125.1.18",
          "form": "그런거 보면 신기하다~",
          "original_form": "그런거 보면 신기하다~",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012125.1.19",
          "form": "응 그러니깐ㅋㅋㅋ",
          "original_form": "응 그러니깐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012125.1.20",
          "form": "좋아하는게 다 다르니깐~",
          "original_form": "좋아하는게 다 다르니깐~",
          "speaker_id": "2",
          "time": "20210824 19:27"
        },
        {
          "id": "MDRW2100012125.1.21",
          "form": "취미가 있으니 낫다해야되는건지ㅋㅋ",
          "original_form": "취미가 있으니 낫다해야되는건지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012125.1.22",
          "form": "여튼 내가 이비인후과가서 청력검사했는데 10살귀라고 상위3프로라 카대.",
          "original_form": "여튼 내가 이비인후과가서 청력검사했는데 10살귀라고 상위3프로라 카대.",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012125.1.23",
          "form": "그만큼 귀가 잘들린다고ㅋㅋ",
          "original_form": "그만큼 귀가 잘들린다고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012125.1.24",
          "form": "이요~쥑이네~",
          "original_form": "이요~쥑이네~",
          "speaker_id": "2",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012125.1.25",
          "form": "청력검사는 갑자기왜??ㅋㅋㅋ",
          "original_form": "청력검사는 갑자기왜??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012125.1.26",
          "form": "그래서 내랑 안맞다 ㅋㅋㅋㅋ맨날 크게틀어놓고",
          "original_form": "그래서 내랑 안맞다 ㅋㅋㅋㅋ맨날 크게틀어놓고",
          "speaker_id": "1",
          "time": "20210824 19:28"
        },
        {
          "id": "MDRW2100012125.1.27",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012125.1.28",
          "form": "난 할매다..안들린다ㅠ",
          "original_form": "난 할매다..안들린다ㅠ",
          "speaker_id": "2",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012125.1.29",
          "form": "아 웃겨ㅋㅋㅋ슬프노ㅋㅋ",
          "original_form": "아 웃겨ㅋㅋㅋ슬프노ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:29"
        },
        {
          "id": "MDRW2100012125.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ원래 가족이란 그런거 아니겠나 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ원래 가족이란 그런거 아니겠나 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:29"
