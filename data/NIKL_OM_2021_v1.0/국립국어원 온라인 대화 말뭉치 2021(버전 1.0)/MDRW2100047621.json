      "utterance": [
        {
          "id": "MDRW2100047621.1.1",
          "form": "회도 좋아하세요? ㅋㅋㅋㅋㅋ",
          "original_form": "회도 좋아하세요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047621.1.2",
          "form": "회도 좋죠 저는 연어를 사랑합니다.",
          "original_form": "회도 좋죠{emoji:♡} 저는 연어를 사랑합니다.",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047621.1.3",
          "form": "생연어요 ㅎㅎ",
          "original_form": "생연어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100047621.1.4",
          "form": "저도 연어 진짜 좋아해요",
          "original_form": "저도 연어 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100047621.1.5",
          "form": "훈제연어보단 생연어죠",
          "original_form": "훈제연어보단 생연어죠",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100047621.1.6",
          "form": "그 마늘초무침? 그거랑 먹으면 제대로죠ㅠㅠ",
          "original_form": "그 마늘초무침? 그거랑 먹으면 제대로죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100047621.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047621.1.8",
          "form": "양파랑 먹어도 맛잇어요",
          "original_form": "양파랑 먹어도 맛잇어요",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047621.1.9",
          "form": "으어어어.. 엄마가 연어 다드셨어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "으어어어.. 엄마가 연어 다드셨어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100047621.1.10",
          "form": "악 좀 얻어먹을걸 ㅠㅠ",
          "original_form": "악 좀 얻어먹을걸 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100047621.1.11",
          "form": "아이고 ㅠㅠ",
          "original_form": "아이고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100047621.1.12",
          "form": "한입도 못드셧나요",
          "original_form": "한입도 못드셧나요",
          "speaker_id": "2",
          "time": "20210826 22:40"
        },
        {
          "id": "MDRW2100047621.1.13",
          "form": "네...ㅜㅜ 먹으란 말없이 ㅋㅋㅋㅋ 다 드셨네옄ㅋㅋㅋㅋ",
          "original_form": "네...ㅜㅜ 먹으란 말없이 ㅋㅋㅋㅋ 다 드셨네옄ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100047621.1.14",
          "form": "옆에 좀 앉아있을걸",
          "original_form": "옆에 좀 앉아있을걸",
          "speaker_id": "1",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100047621.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ옆에서 입을 벌리고 앉아잇으셧어야...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ옆에서 입을 벌리고 앉아잇으셧어야...",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100047621.1.16",
          "form": "제가 경솔했네요..... 남겨주실거라고 생각했는데 ㅋㅋㅋㅋ 훈제연어는 안먹은지 오래됐거든요ㅠㅋㅋㅋ",
          "original_form": "제가 경솔했네요..... 남겨주실거라고 생각했는데 ㅋㅋㅋㅋ 훈제연어는 안먹은지 오래됐거든요ㅠㅋㅋㅋ",
