      "utterance": [
        {
          "id": "MDRW2100039847.1.1",
          "form": "전 연애도 안하고 결혼도 안했답니다...",
          "original_form": "전 연애도 안하고 결혼도 안했답니다...",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039847.1.2",
          "form": "연애는 왜 안하셨어요?",
          "original_form": "연애는 왜 안하셨어요?",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039847.1.3",
          "form": "주변에 코로나라 결혼 결심하는 분들은 있으시더라고요",
          "original_form": "주변에 코로나라 결혼 결심하는 분들은 있으시더라고요",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039847.1.4",
          "form": "????코로나랑",
          "original_form": "????코로나랑",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039847.1.5",
          "form": "그냥 집에서 혼자 노는게 좋아서요 ㅋㅋㅋㅋ",
          "original_form": "그냥 집에서 혼자 노는게 좋아서요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039847.1.6",
          "form": "결혼이랑 무슨 상관일까요????",
          "original_form": "결혼이랑 무슨 상관일까요????",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039847.1.7",
          "form": "저도 집에서 혼자 노는 거 넘 좋아요!",
          "original_form": "저도 집에서 혼자 노는 거 넘 좋아요!",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039847.1.8",
          "form": "결혼할 생각없어요",
          "original_form": "결혼할 생각없어요",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039847.1.9",
          "form": "현재는 연애할 생각도없고",
          "original_form": "현재는 연애할 생각도없고",
          "speaker_id": "1",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039847.1.10",
          "form": "혼자서 노는거 안좋아하는 분들이 결혼생각하던데요 ㅋㅋㅋㅋㅋ",
          "original_form": "혼자서 노는거 안좋아하는 분들이 결혼생각하던데요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039847.1.11",
          "form": "코로나때문에 어딜 못가니까 그런거 같았어요",
          "original_form": "코로나때문에 어딜 못가니까 그런거 같았어요",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039847.1.12",
          "form": "다들 가던데...",
          "original_form": "다들 가던데...",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.13",
          "form": "어디 둘이서 아주 잘...",
          "original_form": "어디 둘이서 아주 잘...",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.15",
          "form": "그냥 결혼이 하고싶은데",
          "original_form": "그냥 결혼이 하고싶은데",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.16",
          "form": "핑계아닐까요",
          "original_form": "핑계아닐까요",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.17",
          "form": "핑곈가여",
          "original_form": "핑곈가여",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.18",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.19",
          "form": "제생각엔",
          "original_form": "제생각엔",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.20",
          "form": "ㅋㅋㅋㅋㅋ 그랬구나",
          "original_form": "ㅋㅋㅋㅋㅋ 그랬구나",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.21",
          "form": "그렇습니다",
          "original_form": "그렇습니다",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.22",
          "form": "저한텐 그러더라고요",
          "original_form": "저한텐 그러더라고요",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.23",
          "form": "혼자서 안노는 분들이 연애를 열심히 하긴하더라구요",
          "original_form": "혼자서 안노는 분들이 연애를 열심히 하긴하더라구요",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.24",
          "form": "그냥 할것이지",
          "original_form": "그냥 할것이지",
          "speaker_id": "2",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039847.1.25",
          "form": "ㅋㅋㅋㅋ약올리나...ㅂㄷㅂㄷ",
          "original_form": "ㅋㅋㅋㅋ약올리나...ㅂㄷㅂㄷ",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100039847.1.26",
          "form": "연애는 하고 싶으신가요?",
          "original_form": "연애는 하고 싶으신가요?",
          "speaker_id": "1",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100039847.1.27",
          "form": "그냥 안하고 살아도 될거 같긴 해요 저 외로움 별로 안타는거 같아서 ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 안하고 살아도 될거 같긴 해요 저 외로움 별로 안타는거 같아서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100039847.1.28",
          "form": "와 그건 좀 부러워요",
          "original_form": "와 그건 좀 부러워요",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039847.1.29",
          "form": "비결 있으세요?",
          "original_form": "비결 있으세요?",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039847.1.30",
          "form": "외로움 안타는 비결",
          "original_form": "외로움 안타는 비결",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039847.1.31",
          "form": "그냥 없이 산 세월이 긴 탓 아닐까요 ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 없이 산 세월이 긴 탓 아닐까요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039847.1.32",
          "form": "사람이 뭐 외로울수도 있죠",
          "original_form": "사람이 뭐 외로울수도 있죠",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039847.1.33",
          "form": "흠...",
          "original_form": "흠...",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039847.1.34",
          "form": "같이 있어서 외로운 기분이 더 싫더라고요",
          "original_form": "같이 있어서 외로운 기분이 더 싫더라고요",
          "speaker_id": "2",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039847.1.35",
          "form": "전 첫 연애하기도 전에 외로움을 알았는데요",
          "original_form": "전 첫 연애하기도 전에 외로움을 알았는데요",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039847.1.36",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039847.1.37",
          "form": "아 그거알죠",
          "original_form": "아 그거알죠",
          "speaker_id": "1",
          "time": "20210903 22:51"
        },
        {
          "id": "MDRW2100039847.1.38",
          "form": "그게 더 싫어요진짜",
          "original_form": "그게 더 싫어요진짜",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100039847.1.39",
          "form": "차라이 혼자 외로운게 낫지",
          "original_form": "차라이 혼자 외로운게 낫지",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100039847.1.40",
          "form": "그건 비참해요",
          "original_form": "그건 비참해요",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100039847.1.41",
          "form": "그냥 혼자도 즐겁더라고요 ㅎㅎ",
          "original_form": "그냥 혼자도 즐겁더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100039847.1.42",
          "form": "제대로 된 놈을 만나야",
          "original_form": "제대로 된 놈을 만나야",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039847.1.43",
          "form": "괜찮으려나 싶네여",
          "original_form": "괜찮으려나 싶네여",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039847.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039847.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런놈이어딧어요세상에",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런놈이어딧어요세상에",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039847.1.46",
          "form": "그런 놈이 있을까여",
          "original_form": "그런 놈이 있을까여",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039847.1.47",
          "form": "없어요없어요",
          "original_form": "없어요없어요",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039847.1.48",
          "form": "세상은 혼자 사는겁니다",
          "original_form": "세상은 혼자 사는겁니다",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039847.1.49",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039847.1.50",
          "form": "그럼 혼자 뭐하구 노세요????",
          "original_form": "그럼 혼자 뭐하구 노세요????",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039847.1.51",
          "form": "ㅋㅋㅋㅋ 네 있어도 누가 데려갔을테니",
          "original_form": "ㅋㅋㅋㅋ 네 있어도 누가 데려갔을테니",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100039847.1.52",
          "form": "요즘엔 넷플릭스 보고 있어요",
          "original_form": "요즘엔 넷플릭스 보고 있어요",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039847.1.53",
          "form": "근데 사실 보는게 아니라 표지구경만",
          "original_form": "근데 사실 보는게 아니라 표지구경만",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100039847.1.54",
          "form": "보기도 전에 지치는거 있죠?ㅋㅋㅋㅋㅋ",
          "original_form": "보기도 전에 지치는거 있죠?ㅋㅋㅋㅋㅋ",
