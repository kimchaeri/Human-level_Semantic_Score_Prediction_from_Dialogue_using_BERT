      "utterance": [
        {
          "id": "MDRW2100038145.1.1",
          "form": "제가 지금 먹어봣는데",
          "original_form": "제가 지금 먹어봣는데",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100038145.1.2",
          "form": "아래가 안익어서 다시굽고 있어요",
          "original_form": "아래가 안익어서 다시굽고 있어요",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100038145.1.3",
          "form": "20분 더 구울거에욥ㅋㅋ",
          "original_form": "20분 더 구울거에욥ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100038145.1.4",
          "form": "그러면 표면이 더 딱딱해지나요ㅋㅋ",
          "original_form": "그러면 표면이 더 딱딱해지나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038145.1.5",
          "form": "그렇긴 한데 안이 덜익어서..",
          "original_form": "그렇긴 한데 안이 덜익어서..",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038145.1.6",
          "form": "더 구워도 안되면 프라이팬위에 올려서 직화할거에요",
          "original_form": "더 구워도 안되면 프라이팬위에 올려서 직화할거에요",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038145.1.7",
          "form": "wow 궁금해요 맛",
          "original_form": "wow 궁금해요 맛",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038145.1.8",
          "form": "망해봤자~ 계란과 설탕과 버터는 어딜 안가죱ㅋㅋ",
          "original_form": "망해봤자~ 계란과 설탕과 버터는 어딜 안가죱ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100038145.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100038145.1.10",
          "form": "맛있는거+맛있는거+맛있는거+불=왕맛있다",
          "original_form": "맛있는거+맛있는거+맛있는거+불=왕맛있다",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100038145.1.11",
          "form": "ㅠㅠ 왜 아직도 안 익는거지요,, 전 내일올께요 내일뵈요 ㅠ",
          "original_form": "ㅠㅠ 왜 아직도 안 익는거지요,, 전 내일올께요 내일뵈요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100038145.1.12",
          "form": "네!! 성공기원할게요!!",
          "original_form": "네!! 성공기원할게요!!",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100038145.1.13",
          "form": "결론, 망했습니다",
          "original_form": "결론, 망했습니다",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100038145.1.14",
          "form": "에프에는 열선이 위에만 있어서",
          "original_form": "에프에는 열선이 위에만 있어서",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100038145.1.15",
          "form": "위만 익고 아래는 안익었습니다",
          "original_form": "위만 익고 아래는 안익었습니다",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100038145.1.16",
          "form": "ㅜㅠ",
          "original_form": "ㅜㅠ",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100038145.1.17",
          "form": "위쪽 드셨나요...?",
          "original_form": "위쪽 드셨나요...?",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100038145.1.18",
          "form": "그것이,,",
          "original_form": "그것이,,",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100038145.1.19",
          "form": "아래가 안익으니 아랠 익히면 될것 같아서 테프론코팅 빵틀재로 직화했더니",
          "original_form": "아래가 안익으니 아랠 익히면 될것 같아서 테프론코팅 빵틀재로 직화했더니",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100038145.1.20",
          "form": "바닥이 이상해요!",
          "original_form": "바닥이 이상해요!",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100038145.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ뭔가 귀엽고 웃퍼욬ㅋㅋ쿠ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ뭔가 귀엽고 웃퍼욬ㅋㅋ쿠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100038145.1.22",
          "form": "베이킹 얼마나 자주하세요?",
          "original_form": "베이킹 얼마나 자주하세요?",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100038145.1.23",
          "form": "일주일에 한번요? 시작한지 얼마 안됐어요",
          "original_form": "일주일에 한번요? 시작한지 얼마 안됐어요",
          "speaker_id": "2",
