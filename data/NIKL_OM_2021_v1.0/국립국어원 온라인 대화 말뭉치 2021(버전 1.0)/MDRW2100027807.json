      "utterance": [
        {
          "id": "MDRW2100027807.1.1",
          "form": "매운맛 빼고 가리지 않고 다 좋아하고 잘 먹는 편이에요",
          "original_form": "매운맛 빼고 가리지 않고 다 좋아하고 잘 먹는 편이에요",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027807.1.2",
          "form": "오 건강한 식습관이네여",
          "original_form": "오 건강한 식습관이네여",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027807.1.3",
          "form": "전 고기를 안좋아해서",
          "original_form": "전 고기를 안좋아해서",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027807.1.4",
          "form": "채식선호해여",
          "original_form": "채식선호해여",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027807.1.5",
          "form": "저도요 저도 채식에 가까워요",
          "original_form": "저도요 저도 채식에 가까워요",
          "speaker_id": "2",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027807.1.6",
          "form": "배달음식에 2색나물과 청국장 이란거 나옴 좋겠는데",
          "original_form": "배달음식에 2색나물과 청국장 이란거 나옴 좋겠는데",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027807.1.7",
          "form": "다 고기고기한 것들이네요",
          "original_form": "다 고기고기한 것들이네요",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027807.1.8",
          "form": "그리고 제주도는 또 돼지고기 이런게 유명하다보니",
          "original_form": "그리고 제주도는 또 돼지고기 이런게 유명하다보니",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027807.1.9",
          "form": "더 그럴 거 같아요",
          "original_form": "더 그럴 거 같아요",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027807.1.10",
          "form": "산채비빔밥 청국장",
          "original_form": "산채비빔밥 청국장",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027807.1.11",
          "form": "요런 채식 식당이 있으면 참 좋을 텐데",
          "original_form": "요런 채식 식당이 있으면 참 좋을 텐데",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027807.1.12",
          "form": "수도권 사시나요?",
          "original_form": "수도권 사시나요?",
          "speaker_id": "1",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027807.1.13",
          "form": "서울은 비건베이커리 비건레스토랑 좀 있는데",
          "original_form": "서울은 비건베이커리 비건레스토랑 좀 있는데",
          "speaker_id": "1",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027807.1.14",
          "form": "비건베이커리 궁금해요 맛난 곳 추천해 주실 수 있어요?",
          "original_form": "비건베이커리 궁금해요 맛난 곳 추천해 주실 수 있어요?",
          "speaker_id": "2",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100027807.1.15",
          "form": "영등포구청 브레드워커 라고 있어요 비건+글루텐프리 빵집이구 흑임자크럼블 쑥크럼블이 말나요",
          "original_form": "영등포구청 브레드워커 라고 있어요 비건+글루텐프리 빵집이구 흑임자크럼블 쑥크럼블이 말나요",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027807.1.16",
          "form": "맛나요",
          "original_form": "맛나요",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027807.1.17",
          "form": "꼭 가봐야 겠어요 듣기만 해도 너무 맛있네요",
          "original_form": "꼭 가봐야 겠어요 듣기만 해도 너무 맛있네요",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027807.1.18",
          "form": "쑥 흑임자 정말 좋아하는데",
          "original_form": "쑥 흑임자 정말 좋아하는데",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027807.1.19",
          "form": "집근처여서 황송해하면서 먹억죠",
          "original_form": "집근처여서 황송해하면서 먹억죠",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027807.1.20",
          "form": "스콘도 맛있구",
          "original_form": "스콘도 맛있구",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027807.1.21",
          "form": "비건레스토랑응 합정쪽에",
          "original_form": "비건레스토랑응 합정쪽에",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027807.1.22",
          "form": "셰발레리가 최고!",
          "original_form": "셰발레리가 최고!",
          "speaker_id": "1",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027807.1.23",
          "form": "메모 해 놔야 겠어요",
          "original_form": "메모 해 놔야 겠어요",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027807.1.24",
          "form": "꿀팁 감사합니다",
          "original_form": "꿀팁 감사합니다",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027807.1.25",
          "form": "여긴 어떤 메뉴 추천하시나요",
          "original_form": "여긴 어떤 메뉴 추천하시나요",
          "speaker_id": "2",
          "time": "20210902 15:52"
        },
        {
          "id": "MDRW2100027807.1.26",
          "form": "아 기억을 더듬어보자",
          "original_form": "아 기억을 더듬어보자",
          "speaker_id": "1",
          "time": "20210902 15:53"
        },
        {
          "id": "MDRW2100027807.1.27",
          "form": "비건통밀리가토니",
          "original_form": "비건통밀리가토니",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027807.1.28",
          "form": "주3일만 열어요",
          "original_form": "주3일만 열어요",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027807.1.29",
          "form": "그렇군용 감사합니다",
          "original_form": "그렇군용 감사합니다{emoji:♥}{emoji:♥}{emoji:♥}{emoji:♥}",
