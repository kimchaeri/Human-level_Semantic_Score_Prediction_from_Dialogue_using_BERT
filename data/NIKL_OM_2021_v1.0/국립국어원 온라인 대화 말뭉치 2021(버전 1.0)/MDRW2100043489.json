        },
        {
          "id": "MDRW2100043489.1.2",
          "form": "어엉",
          "original_form": "어엉",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.3",
          "form": "연탄불고기 먹어봣남?",
          "original_form": "연탄불고기 먹어봣남?",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.4",
          "form": "아니아니",
          "original_form": "아니아니",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.5",
          "form": "그거 우동이랑 먹으면",
          "original_form": "그거 우동이랑 먹으면",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.6",
          "form": "진짜맛잇는데",
          "original_form": "진짜맛잇는데",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.7",
          "form": "먹어봣어??",
          "original_form": "먹어봣어??",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.8",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.9",
          "form": "진짜 부드러워",
          "original_form": "진짜 부드러워",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.10",
          "form": "오그래?",
          "original_form": "오그래?",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.11",
          "form": "둘이 잘어울려",
          "original_form": "둘이 잘어울려",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.12",
          "form": "환상의 궁합이구나",
          "original_form": "환상의 궁합이구나",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.13",
          "form": "어어 진짜얍",
          "original_form": "어어 진짜얍",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043489.1.14",
          "form": "나도 먹고싳네",
          "original_form": "나도 먹고싳네",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043489.1.15",
          "form": "가족하고 같이가보자",
          "original_form": "가족하고 같이가보자",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043489.1.16",
          "form": "맛집알아??",
          "original_form": "맛집알아??",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043489.1.17",
          "form": "당연히 알지",
          "original_form": "당연히 알지",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043489.1.18",
          "form": "꼭 같이가기약속이야",
