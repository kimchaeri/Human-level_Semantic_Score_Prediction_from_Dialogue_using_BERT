      "utterance": [
        {
          "id": "MDRW2100040778.1.1",
          "form": "아 그래서 예전에",
          "original_form": "아 그래서 예전에",
          "speaker_id": "1",
          "time": "20210905 04:37"
        },
        {
          "id": "MDRW2100040778.1.2",
          "form": "대학원 진학 전에는",
          "original_form": "대학원 진학 전에는",
          "speaker_id": "1",
          "time": "20210905 04:37"
        },
        {
          "id": "MDRW2100040778.1.3",
          "form": "와근데 진짜 너무 멋진일들이 대학원에서일어나고잇군요..",
          "original_form": "와근데 진짜 너무 멋진일들이 대학원에서일어나고잇군요..",
          "speaker_id": "2",
          "time": "20210905 04:37"
        },
        {
          "id": "MDRW2100040778.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210905 04:37"
        },
        {
          "id": "MDRW2100040778.1.5",
          "form": "뉴스에서 막... 어느어느 학교 연구팀에서",
          "original_form": "뉴스에서 막... 어느어느 학교 연구팀에서",
          "speaker_id": "1",
          "time": "20210905 04:37"
        },
        {
          "id": "MDRW2100040778.1.6",
          "form": "흐악 그러니깐요",
          "original_form": "흐악 그러니깐요",
          "speaker_id": "2",
          "time": "20210905 04:37"
        },
        {
          "id": "MDRW2100040778.1.7",
          "form": "신물질을 발명했습니다!",
          "original_form": "신물질을 발명했습니다!",
          "speaker_id": "1",
          "time": "20210905 04:37"
        },
        {
          "id": "MDRW2100040778.1.8",
          "form": "이러면",
          "original_form": "이러면",
          "speaker_id": "1",
          "time": "20210905 04:37"
        },
        {
          "id": "MDRW2100040778.1.9",
          "form": "크...",
          "original_form": "크...",
          "speaker_id": "2",
          "time": "20210905 04:37"
        },
        {
          "id": "MDRW2100040778.1.10",
          "form": "와... 드디어 무병장수의 시대가 도래하겠구나",
          "original_form": "와... 드디어 무병장수의 시대가 도래하겠구나",
          "speaker_id": "1",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.11",
          "form": "이렇게 생각했는데",
          "original_form": "이렇게 생각했는데",
          "speaker_id": "1",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.12",
          "form": "왘",
          "original_form": "왘",
          "speaker_id": "2",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.13",
          "form": "막상 제가 대학원생이 되어보니까",
          "original_form": "막상 제가 대학원생이 되어보니까",
          "speaker_id": "1",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.14",
          "form": "엌",
          "original_form": "엌",
          "speaker_id": "2",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.15",
          "form": "아... 그냥 진짜 샤프심만한 발견을 했는데",
          "original_form": "아... 그냥 진짜 샤프심만한 발견을 했는데",
          "speaker_id": "1",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.16",
          "form": "교수가 인맥이 좀 있나보군",
          "original_form": "교수가 인맥이 좀 있나보군",
          "speaker_id": "1",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.17",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "2",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.18",
          "form": "오졌네요",
          "original_form": "오졌네요",
          "speaker_id": "2",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.19",
          "form": "이 생각이 들어요",
          "original_form": "이 생각이 들어요",
          "speaker_id": "1",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.20",
          "form": "인맥이었네요역시",
          "original_form": "인맥이었네요역시",
          "speaker_id": "2",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.21",
          "form": "그걸",
          "original_form": "그걸",
          "speaker_id": "1",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.22",
          "form": "근데교수님이인맥이좋으면역시",
          "original_form": "근데교수님이인맥이좋으면역시",
          "speaker_id": "2",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.23",
          "form": "언론에",
          "original_form": "언론에",
          "speaker_id": "1",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.24",
          "form": "대학원생들에게도좋은거겠죠",
          "original_form": "대학원생들에게도좋은거겠죠",
          "speaker_id": "2",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.25",
          "form": "제보하는거 자체가",
          "original_form": "제보하는거 자체가",
          "speaker_id": "1",
          "time": "20210905 04:38"
        },
        {
          "id": "MDRW2100040778.1.26",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210905 04:39"
        },
        {
          "id": "MDRW2100040778.1.27",
          "form": "어느정도 인지도가 있다는 거니까...",
          "original_form": "어느정도 인지도가 있다는 거니까...",
          "speaker_id": "1",
          "time": "20210905 04:39"
        },
        {
          "id": "MDRW2100040778.1.28",
          "form": "아마 그런 뉴스 듣고",
          "original_form": "아마 그런 뉴스 듣고",
          "speaker_id": "1",
          "time": "20210905 04:39"
        },
        {
          "id": "MDRW2100040778.1.29",
          "form": "그춐",
          "original_form": "그춐",
          "speaker_id": "2",
          "time": "20210905 04:39"
        },
        {
          "id": "MDRW2100040778.1.30",
          "form": "컨택하는 학부생들도 많겠죠",
          "original_form": "컨택하는 학부생들도 많겠죠",
          "speaker_id": "1",
          "time": "20210905 04:39"
        },
        {
          "id": "MDRW2100040778.1.31",
          "form": "그러면 이제",
          "original_form": "그러면 이제",
          "speaker_id": "1",
          "time": "20210905 04:39"
        },
        {
          "id": "MDRW2100040778.1.32",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210905 04:39"
        },
        {
          "id": "MDRW2100040778.1.33",
          "form": "거기서 연구하는 도비들은",
          "original_form": "거기서 연구하는 도비들은",
          "speaker_id": "1",
          "time": "20210905 04:39"
        },
        {
          "id": "MDRW2100040778.1.34",
          "form": "후배가 들어와서 좋은거고...",
          "original_form": "후배가 들어와서 좋은거고...",
          "speaker_id": "1",
          "time": "20210905 04:39"
        }
      ]
    }
