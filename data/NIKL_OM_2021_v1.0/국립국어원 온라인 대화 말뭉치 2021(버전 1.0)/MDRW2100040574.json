      "utterance": [
        {
          "id": "MDRW2100040574.1.1",
          "form": "고모된 입장에서 고모랑 친하게 지내는 조카 좋네ㅋㅋㅋ",
          "original_form": "고모된 입장에서 고모랑 친하게 지내는 조카 좋네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040574.1.2",
          "form": "ㅋㅋㅋㅋ아니고모가 마트에서 포도를 사셨데",
          "original_form": "ㅋㅋㅋㅋ아니고모가 마트에서 포도를 사셨데",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040574.1.3",
          "form": "드럽게 맛없어",
          "original_form": "드럽게 맛없어",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040574.1.4",
          "form": "심지어 벌레도있ㄴㅁ",
          "original_form": "심지어 벌레도있ㄴㅁ",
          "speaker_id": "1",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040574.1.5",
          "form": "세계로마트?",
          "original_form": "세계로마트?",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040574.1.6",
          "form": "나 웍샀어!",
          "original_form": "나 웍샀어!",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040574.1.7",
          "form": "29560",
          "original_form": "29560",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040574.1.8",
          "form": "어디서?지마텟?",
          "original_form": "어디서?지마텟?",
          "speaker_id": "1",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040574.1.9",
          "form": "웍하나만살거임??",
          "original_form": "웍하나만살거임??",
          "speaker_id": "1",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040574.1.10",
          "form": "쿠팡이쌀걸?",
          "original_form": "쿠팡이쌀걸?",
          "speaker_id": "1",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040574.1.11",
          "form": "하나만살거면?",
          "original_form": "하나만살거면?",
          "speaker_id": "1",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040574.1.12",
          "form": "아느 세트!",
          "original_form": "아느 세트!",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040574.1.13",
          "form": "오오무슨세트야",
          "original_form": "오오무슨세트야",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040574.1.14",
          "form": "너가 보내준거?",
          "original_form": "너가 보내준거?",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040574.1.15",
          "form": "그거 그렇게 큰거??",
          "original_form": "그거 그렇게 큰거??",
          "speaker_id": "1",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100040574.1.16",
          "form": "아니 대따 싸게샀다??",
          "original_form": "아니 대따 싸게샀다??",
          "speaker_id": "1",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100040574.1.17",
          "form": "우린 한번에많이하니까 일부러큰거산건데",
          "original_form": "우린 한번에많이하니까 일부러큰거산건데",
          "speaker_id": "1",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100040574.1.18",
          "form": "몰라 엄마가 큰사이즈가 좋다던데?ㅋㅋㅋ",
          "original_form": "몰라 엄마가 큰사이즈가 좋다던데?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100040574.1.19",
          "form": "아....나 쿠폰하나 적용안함....",
          "original_form": "아....나 쿠폰하나 적용안함....",
          "speaker_id": "1",
          "time": "20210904 23:02"
        },
        {
          "id": "MDRW2100040574.1.20",
          "form": "앗 어쩐지 다시해다시",
          "original_form": "앗 어쩐지 다시해다시",
          "speaker_id": "2",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100040574.1.21",
          "form": "배송출발했어...",
          "original_form": "배송출발했어...",
          "speaker_id": "1",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040574.1.22",
          "form": "엉엉엉",
          "original_form": "엉엉엉",
          "speaker_id": "1",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040574.1.23",
          "form": "힐 벌써?",
          "original_form": "힐 벌써?",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040574.1.24",
          "form": "그니까ㅜㅜ",
          "original_form": "그니까ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040574.1.25",
          "form": "토요일주말인데ㄷㄷ",
          "original_form": "토요일주말인데ㄷㄷ",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100040574.1.26",
          "form": "반품하기는너무귀찮다",
          "original_form": "반품하기는너무귀찮다",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100040574.1.27",
          "form": "뭐 이미 싸게샀으니까.... 엉엉엉",
          "original_form": "뭐 이미 싸게샀으니까.... 엉엉엉",
          "speaker_id": "1",
          "time": "20210904 23:06"
        },
        {
          "id": "MDRW2100040574.1.28",
          "form": "원래 하나에 세일해도 이만오천넘음 ...",
          "original_form": "원래 하나에 세일해도 이만오천넘음 ...",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100040574.1.29",
          "form": "근데 왜 고모집이야?",
          "original_form": "근데 왜 고모집이야?",
          "speaker_id": "2",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100040574.1.30",
          "form": "주말에자주옴 ㅎㅎ 요구르트산거들고옴ㅋㅋ",
          "original_form": "주말에자주옴 ㅎㅎ 요구르트산거들고옴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100040574.1.31",
          "form": "엄마한테는 쿠폰말하지말아야지 ㅎㅎ",
          "original_form": "엄마한테는 쿠폰말하지말아야지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:07"
        }
      ]
    }
