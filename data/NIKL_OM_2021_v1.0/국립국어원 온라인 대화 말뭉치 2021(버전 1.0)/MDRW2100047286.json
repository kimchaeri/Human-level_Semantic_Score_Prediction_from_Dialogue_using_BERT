      "utterance": [
        {
          "id": "MDRW2100047286.1.1",
          "form": "아 백순대 맛나겠네요",
          "original_form": "아 백순대 맛나겠네요",
          "speaker_id": "2",
          "time": "20210826 03:36"
        },
        {
          "id": "MDRW2100047286.1.2",
          "form": "밀폐유 나베 사보고싶던데요",
          "original_form": "밀폐유 나베 사보고싶던데요",
          "speaker_id": "1",
          "time": "20210826 03:36"
        },
        {
          "id": "MDRW2100047286.1.3",
          "form": "고기랑 야채도 다있나봐요",
          "original_form": "고기랑 야채도 다있나봐요",
          "speaker_id": "1",
          "time": "20210826 03:36"
        },
        {
          "id": "MDRW2100047286.1.4",
          "form": "백순대 맛나요 ㅎ",
          "original_form": "백순대 맛나요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 03:37"
        },
        {
          "id": "MDRW2100047286.1.5",
          "form": "파티할때 사봤는데",
          "original_form": "파티할때 사봤는데",
          "speaker_id": "2",
          "time": "20210826 03:37"
        },
        {
          "id": "MDRW2100047286.1.6",
          "form": "맛났어요",
          "original_form": "맛났어요",
          "speaker_id": "2",
          "time": "20210826 03:37"
        },
        {
          "id": "MDRW2100047286.1.7",
          "form": "양념보다 전 백순대좋아해요",
          "original_form": "양념보다 전 백순대좋아해요",
          "speaker_id": "1",
          "time": "20210826 03:37"
        },
        {
          "id": "MDRW2100047286.1.8",
          "form": "어디꺼드세요?",
          "original_form": "어디꺼드세요?",
          "speaker_id": "2",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100047286.1.9",
          "form": "양념장 찍어서 깻잎에",
          "original_form": "양념장 찍어서 깻잎에",
          "speaker_id": "1",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100047286.1.10",
          "form": "윙잇에서 첨 사서먹었는데 괜찮길라",
          "original_form": "윙잇에서 첨 사서먹었는데 괜찮길라",
          "speaker_id": "1",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100047286.1.11",
          "form": "오늘 살예정",
          "original_form": "오늘 살예정",
          "speaker_id": "1",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100047286.1.12",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100047286.1.13",
          "form": "신림 가서먹는거랑 거의 똑같더라구요",
          "original_form": "신림 가서먹는거랑 거의 똑같더라구요",
          "speaker_id": "1",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100047286.1.14",
          "form": "지금 먹고싶은 야식있나요",
          "original_form": "지금 먹고싶은 야식있나요",
          "speaker_id": "2",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100047286.1.15",
          "form": "아까 빵을  먹었어요ㅎ",
          "original_form": "아까 빵을  먹었어요ㅎ",
          "speaker_id": "1",
          "time": "20210826 03:40"
        },
        {
          "id": "MDRW2100047286.1.16",
          "form": "저는 옥수수먹었어요ㅋㅋ",
          "original_form": "저는 옥수수먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:40"
        },
        {
          "id": "MDRW2100047286.1.17",
          "form": "초당옥수수 드셔보셨어요,",
          "original_form": "초당옥수수 드셔보셨어요,",
          "speaker_id": "1",
          "time": "20210826 03:41"
        },
        {
          "id": "MDRW2100047286.1.18",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 03:41"
        },
        {
          "id": "MDRW2100047286.1.19",
          "form": "네 먹어봤어요",
          "original_form": "네 먹어봤어요",
