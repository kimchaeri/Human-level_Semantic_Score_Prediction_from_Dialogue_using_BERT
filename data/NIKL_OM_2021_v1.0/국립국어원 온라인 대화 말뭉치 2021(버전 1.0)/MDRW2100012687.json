      "utterance": [
        {
          "id": "MDRW2100012687.1.1",
          "form": "너 아는 종교인 있니?",
          "original_form": "너 아는 종교인 있니?",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012687.1.2",
          "form": "음 종교인이면",
          "original_form": "음 종교인이면",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012687.1.3",
          "form": "아. 내가 말하는 종교인은",
          "original_form": "아. 내가 말하는 종교인은",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012687.1.4",
          "form": "교주를 말하는거야?",
          "original_form": "교주를 말하는거야?",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012687.1.5",
          "form": "단순 종교을 가진 사람이ㅜ아니야",
          "original_form": "단순 종교을 가진 사람이ㅜ아니야",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012687.1.6",
          "form": "교주.ㅠㅋㅋㅋㅋ교주..라기보단",
          "original_form": "교주.ㅠㅋㅋㅋㅋ교주..라기보단",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012687.1.7",
          "form": "그 비슷한 거.",
          "original_form": "그 비슷한 거.",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012687.1.8",
          "form": "음 광신도?",
          "original_form": "음 광신도?",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012687.1.9",
          "form": "스님이나 신부님이나 몯사님 있니?",
          "original_form": "스님이나 신부님이나 몯사님 있니?",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012687.1.10",
          "form": "아니ㅠ 직업 종교인",
          "original_form": "아니ㅠ 직업 종교인",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012687.1.11",
          "form": "아아 그런사람들을 말하는거구나",
          "original_form": "아아 그런사람들을 말하는거구나",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012687.1.12",
          "form": "교주는 아무래도 좀 사이비 같으니까.",
          "original_form": "교주는 아무래도 좀 사이비 같으니까.",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012687.1.13",
          "form": "응. 있어?",
          "original_form": "응. 있어?",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012687.1.14",
          "form": "음 예전에 친구 아버지가 목사라고 들었어",
          "original_form": "음 예전에 친구 아버지가 목사라고 들었어",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012687.1.15",
          "form": "너는 있니??",
          "original_form": "너는 있니??",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012687.1.16",
          "form": "오 내친구도",
          "original_form": "오 내친구도",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012687.1.17",
          "form": "내친구 아버지도 목사이신데",
          "original_form": "내친구 아버지도 목사이신데",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012687.1.18",
          "form": "역시 주변에 목사는 많구나.",
          "original_form": "역시 주변에 목사는 많구나.",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012687.1.19",
          "form": "그럼 친구는 술을 안마시니?",
          "original_form": "그럼 친구는 술을 안마시니?",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012687.1.20",
          "form": "마셔.",
          "original_form": "마셔.",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012687.1.21",
          "form": "잘마시더라.",
          "original_form": "잘마시더라.",
          "speaker_id": "1",
          "time": "20210825 00:08"
        }
      ]
