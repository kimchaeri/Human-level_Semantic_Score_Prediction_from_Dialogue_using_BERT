      "utterance": [
        {
          "id": "MDRW2100010926.1.1",
          "form": "음 동생은 얼른 결혼하고 싶어하던데",
          "original_form": "음 동생은 얼른 결혼하고 싶어하던데",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010926.1.2",
          "form": "저는 작년 12월에 결혼했어요",
          "original_form": "저는 작년 12월에 결혼했어요",
          "speaker_id": "1",
          "time": "20210824 07:43"
        },
        {
          "id": "MDRW2100010926.1.3",
          "form": "코로나 확진자수가 갑자기 늘어나던 때라",
          "original_form": "코로나 확진자수가 갑자기 늘어나던 때라",
          "speaker_id": "1",
          "time": "20210824 07:43"
        },
        {
          "id": "MDRW2100010926.1.4",
          "form": "걱정이 많았었는데",
          "original_form": "걱정이 많았었는데",
          "speaker_id": "1",
          "time": "20210824 07:43"
        },
        {
          "id": "MDRW2100010926.1.5",
          "form": "요즘 보면",
          "original_form": "요즘 보면",
          "speaker_id": "1",
          "time": "20210824 07:43"
        },
        {
          "id": "MDRW2100010926.1.6",
          "form": "그때 미루지않고 한 게 잘한것 같아요",
          "original_form": "그때 미루지않고 한 게 잘한것 같아요",
          "speaker_id": "1",
          "time": "20210824 07:43"
        },
        {
          "id": "MDRW2100010926.1.7",
          "form": "그때 한번 6개월정도 미룬 사람의 청첩장을 받았었는데",
          "original_form": "그때 한번 6개월정도 미룬 사람의 청첩장을 받았었는데",
          "speaker_id": "2",
          "time": "20210824 07:47"
        },
        {
          "id": "MDRW2100010926.1.8",
          "form": "그 이후 다시 청첩장이 안와서 놀랐던 기억이 나네요 ㅋㅋ",
          "original_form": "그 이후 다시 청첩장이 안와서 놀랐던 기억이 나네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 07:47"
        },
        {
          "id": "MDRW2100010926.1.9",
          "form": "잊어버렸나보다",
          "original_form": "잊어버렸나보다",
          "speaker_id": "1",
          "time": "20210824 07:49"
        },
        {
          "id": "MDRW2100010926.1.10",
          "form": "주변에 결혼한 지인 많나요?",
          "original_form": "주변에 결혼한 지인 많나요?",
          "speaker_id": "1",
          "time": "20210824 07:49"
        },
        {
          "id": "MDRW2100010926.1.11",
          "form": "반반 정도에요",
          "original_form": "반반 정도에요",
          "speaker_id": "2",
          "time": "20210824 07:50"
        },
        {
          "id": "MDRW2100010926.1.12",
          "form": "비슷하네요",
          "original_form": "비슷하네요",
          "speaker_id": "1",
          "time": "20210824 07:51"
        },
        {
          "id": "MDRW2100010926.1.13",
          "form": "그런 주변상황도 중요하려나요 ㅎㅎ",
          "original_form": "그런 주변상황도 중요하려나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 07:51"
        },
        {
          "id": "MDRW2100010926.1.14",
          "form": "저도 반반",
          "original_form": "저도 반반",
          "speaker_id": "1",
          "time": "20210824 07:51"
        },
        {
          "id": "MDRW2100010926.1.15",
          "form": "일찍 결혼한 친구는 아이가 이제 9살인가 그런데",
          "original_form": "일찍 결혼한 친구는 아이가 이제 9살인가 그런데",
          "speaker_id": "1",
          "time": "20210824 07:51"
        },
        {
          "id": "MDRW2100010926.1.16",
          "form": "가끔 그런거보면 신기하기도 하고",
          "original_form": "가끔 그런거보면 신기하기도 하고",
          "speaker_id": "1",
          "time": "20210824 07:52"
        },
        {
          "id": "MDRW2100010926.1.17",
          "form": "시간이 벌써 이렇게 되었나 싶기도하고",
          "original_form": "시간이 벌써 이렇게 되었나 싶기도하고",
          "speaker_id": "1",
          "time": "20210824 07:52"
        },
        {
          "id": "MDRW2100010926.1.18",
          "form": "기분이 묘해요",
          "original_form": "기분이 묘해요",
          "speaker_id": "1",
          "time": "20210824 07:52"
        },
        {
          "id": "MDRW2100010926.1.19",
          "form": "저도 대학 바로 졸업하고 결혼한 친구는 애가 둘이나되더라고요",
          "original_form": "저도 대학 바로 졸업하고 결혼한 친구는 애가 둘이나되더라고요",
          "speaker_id": "2",
          "time": "20210824 07:52"
        },
        {
          "id": "MDRW2100010926.1.20",
          "form": "2세계획이 있으신가요",
          "original_form": "2세계획이 있으신가요",
          "speaker_id": "2",
          "time": "20210824 07:52"
        },
        {
          "id": "MDRW2100010926.1.21",
          "form": "계획은 갖고 있어요",
          "original_form": "계획은 갖고 있어요",
          "speaker_id": "1",
          "time": "20210824 07:52"
        },
        {
          "id": "MDRW2100010926.1.22",
          "form": "그런데 와이프 구직문제로",
          "original_form": "그런데 와이프 구직문제로",
          "speaker_id": "1",
          "time": "20210824 07:53"
        },
        {
          "id": "MDRW2100010926.1.23",
          "form": "일단 보류에요",
          "original_form": "일단 보류에요",
          "speaker_id": "1",
          "time": "20210824 07:53"
        },
        {
          "id": "MDRW2100010926.1.24",
          "form": "저는 아이들 좋아하는데",
          "original_form": "저는 아이들 좋아하는데",
          "speaker_id": "1",
          "time": "20210824 07:53"
        },
        {
          "id": "MDRW2100010926.1.25",
          "form": "어떠세요?",
          "original_form": "어떠세요?",
          "speaker_id": "1",
          "time": "20210824 07:53"
        },
        {
          "id": "MDRW2100010926.1.26",
          "form": "저는 썩 좋아하는편은아닌데",
          "original_form": "저는 썩 좋아하는편은아닌데",
          "speaker_id": "2",
          "time": "20210824 07:54"
        },
        {
          "id": "MDRW2100010926.1.27",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210824 07:55"
        }
      ]
    }
  ]
