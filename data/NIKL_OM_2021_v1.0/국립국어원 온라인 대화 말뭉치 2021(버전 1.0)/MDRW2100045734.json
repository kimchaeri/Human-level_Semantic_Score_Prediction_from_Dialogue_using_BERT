      "utterance": [
        {
          "id": "MDRW2100045734.1.1",
          "form": "그러게요 단가 아낀다고 따흑",
          "original_form": "그러게요 단가 아낀다고 따흑",
          "speaker_id": "2",
          "time": "20210908 06:21"
        },
        {
          "id": "MDRW2100045734.1.2",
          "form": "그냥 좀 더 비싸도 맛있는 거였으면",
          "original_form": "그냥 좀 더 비싸도 맛있는 거였으면",
          "speaker_id": "2",
          "time": "20210908 06:21"
        },
        {
          "id": "MDRW2100045734.1.3",
          "form": "학교도 좋고 학생도 좋았을 것 같아요",
          "original_form": "학교도 좋고 학생도 좋았을 것 같아요",
          "speaker_id": "2",
          "time": "20210908 06:21"
        },
        {
          "id": "MDRW2100045734.1.4",
          "form": "그르니까요.",
          "original_form": "그르니까요.",
          "speaker_id": "1",
          "time": "20210908 06:22"
        },
        {
          "id": "MDRW2100045734.1.5",
          "form": "분명 등록금도 거기에 쓰일텐데",
          "original_form": "분명 등록금도 거기에 쓰일텐데",
          "speaker_id": "1",
          "time": "20210908 06:22"
        },
        {
          "id": "MDRW2100045734.1.6",
          "form": "아까운 등록금....ㅠ",
          "original_form": "아까운 등록금....ㅠ",
          "speaker_id": "1",
          "time": "20210908 06:22"
        },
        {
          "id": "MDRW2100045734.1.7",
          "form": "긍께요!!!!",
          "original_form": "긍께요!!!!",
          "speaker_id": "2",
          "time": "20210908 06:22"
        },
        {
          "id": "MDRW2100045734.1.8",
          "form": "전공쪽으로 거지도 않아서 돌려받고 싶어욬ㅋㅋㅋ",
          "original_form": "전공쪽으로 거지도 않아서 돌려받고 싶어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:23"
        },
        {
          "id": "MDRW2100045734.1.9",
          "form": "전공이랑 딴판으로 길을 잡으셨나요",
          "original_form": "전공이랑 딴판으로 길을 잡으셨나요",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045734.1.10",
          "form": "뭔가 저도 그럴 것 같은디 ㅋㅋㅋㅋ",
          "original_form": "뭔가 저도 그럴 것 같은디 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045734.1.11",
          "form": "전공 맞추기 해볼래요?!",
          "original_form": "전공 맞추기 해볼래요?!",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045734.1.12",
          "form": "제가 그러려고 키워드를 바꿨죠",
          "original_form": "제가 그러려고 키워드를 바꿨죠",
          "speaker_id": "1",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045734.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045734.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아요",
          "speaker_id": "2",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045734.1.15",
          "form": "학교마다 다 달라서 어려울것 같은데 이번꺼 티키타카로 넘기고 바로 이어해요!!",
          "original_form": "학교마다 다 달라서 어려울것 같은데 이번꺼 티키타카로 넘기고 바로 이어해요!!",
          "speaker_id": "1",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045734.1.16",
          "form": "그럴까요!",
          "original_form": "그럴까요!",
          "speaker_id": "2",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045734.1.17",
          "form": "학교 편의점 써보셨어요?",
          "original_form": "학교 편의점 써보셨어요?",
          "speaker_id": "2",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045734.1.18",
          "form": "가끔요ㅠㅠ",
          "original_form": "가끔요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045734.1.19",
          "form": "카페도 하나 있었는데 한 번싸봤어요",
          "original_form": "카페도 하나 있었는데 한 번싸봤어요",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045734.1.20",
          "form": "오 카페는 좀 잘 되어있었나요",
          "original_form": "오 카페는 좀 잘 되어있었나요",
          "speaker_id": "2",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045734.1.21",
          "form": "와...이렇게 말하고 보니 학교 진짜 겆이같아ㅛ네눀ㅋㅋㅋㅋㅋ",
          "original_form": "와...이렇게 말하고 보니 학교 진짜 겆이같아ㅛ네눀ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045734.1.22",
          "form": "그나마 나았어요 근데자리가 옶.....",
          "original_form": "그나마 나았어요 근데자리가 옶.....",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045734.1.23",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045734.1.24",
          "form": "학식 식당, 편의점, 카페 끝ㅋㅋㅋㅋ",
          "original_form": "학식 식당, 편의점, 카페 끝ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045734.1.25",
          "form": "아나 진짜 더시생각해도 욕나오게 구리네욬ㅋㅋ",
          "original_form": "아나 진짜 더시생각해도 욕나오게 구리네욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045734.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ카페가 제일 핫플레이스였나봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ카페가 제일 핫플레이스였나봐요",
          "speaker_id": "2",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045734.1.27",
          "form": "다들 돈까스를 피해 커피집으로 간 게야",
          "original_form": "다들 돈까스를 피해 커피집으로 간 게야",
          "speaker_id": "2",
          "time": "20210908 06:28"
        },
        {
          "id": "MDRW2100045734.1.28",
          "form": "그런가봐욬ㅋㅋㅋ 카페엔 디저트 메뉴가 있었으니까요",
          "original_form": "그런가봐욬ㅋㅋㅋ 카페엔 디저트 메뉴가 있었으니까요",
