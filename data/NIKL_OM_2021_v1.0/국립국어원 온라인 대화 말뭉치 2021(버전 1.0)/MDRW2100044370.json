      "utterance": [
        {
          "id": "MDRW2100044370.1.1",
          "form": "티비는 잘 안봐요",
          "original_form": "티비는 잘 안봐요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044370.1.2",
          "form": "요즘은 영상컨텐츠에서 바로 다시보기 서비스도 되니까",
          "original_form": "요즘은 영상컨텐츠에서 바로 다시보기 서비스도 되니까",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044370.1.3",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044370.1.4",
          "form": "넷플 쓰세요?",
          "original_form": "넷플 쓰세요?",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044370.1.5",
          "form": "네 넷플이랑 티빙",
          "original_form": "네 넷플이랑 티빙",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044370.1.6",
          "form": "넷플 머보세용",
          "original_form": "넷플 머보세용",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044370.1.7",
          "form": "요즘은 잘 못보긴 하는데",
          "original_form": "요즘은 잘 못보긴 하는데",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044370.1.8",
          "form": "맛녀석이나 슬의생 봐요",
          "original_form": "맛녀석이나 슬의생 봐요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044370.1.9",
          "form": "다시보기용으로",
          "original_form": "다시보기용으로",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044370.1.10",
          "form": "앗 슬의 저도보는뎅",
          "original_form": "앗 슬의 저도보는뎅",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044370.1.11",
          "form": "ㅋㅋ 캐릭 머조아하세요",
          "original_form": "ㅋㅋ 캐릭 머조아하세요",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044370.1.12",
          "form": "이익준이요ㅋㅋ",
          "original_form": "이익준이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044370.1.13",
          "form": "오저도",
          "original_form": "오저도",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044370.1.14",
          "form": "조정석 조아하세요?",
          "original_form": "조정석 조아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044370.1.15",
          "form": "네 능청스러운 연기가",
          "original_form": "네 능청스러운 연기가",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044370.1.16",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044370.1.17",
          "form": "ㅋㅋㅋㅋ 너무 연기잘해요 끼도많고",
          "original_form": "ㅋㅋㅋㅋ 너무 연기잘해요 끼도많고",
          "speaker_id": "1",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044370.1.18",
          "form": "네네 천상 연예인",
          "original_form": "네네 천상 연예인",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044370.1.19",
          "form": "혹시 질투의화신 보셧는지?",
          "original_form": "혹시 질투의화신 보셧는지?",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044370.1.20",
          "form": "네네 봤어요",
          "original_form": "네네 봤어요",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044370.1.21",
          "form": "공효진과 나온거",
          "original_form": "공효진과 나온거",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044370.1.22",
          "form": "네네 아 그거 재밋지않앗어요",
          "original_form": "네네 아 그거 재밋지않앗어요",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044370.1.23",
          "form": "네 정주행했죠",
          "original_form": "네 정주행했죠",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044370.1.24",
          "form": "ㅋㅋㅋㅋ 저 블레도 삿어요 인생드라마",
          "original_form": "ㅋㅋㅋㅋ 저 블레도 삿어요 인생드라마",
          "speaker_id": "1",
          "time": "20210907 22:38"
        }
      ]
