      "utterance": [
        {
          "id": "MDRW2100032909.1.1",
          "form": "이 주제는 좀 무거운데??",
          "original_form": "이 주제는 좀 무거운데??",
          "speaker_id": "2",
          "time": "20210906 04:59"
        },
        {
          "id": "MDRW2100032909.1.2",
          "form": "궁금해서..",
          "original_form": "궁금해서..",
          "speaker_id": "1",
          "time": "20210906 05:00"
        },
        {
          "id": "MDRW2100032909.1.3",
          "form": "요즘 결혼생활에 만족하십니까??",
          "original_form": "요즘 결혼생활에 만족하십니까??",
          "speaker_id": "1",
          "time": "20210906 05:00"
        },
        {
          "id": "MDRW2100032909.1.4",
          "form": "아니요?????",
          "original_form": "아니요?????",
          "speaker_id": "2",
          "time": "20210906 05:00"
        },
        {
          "id": "MDRW2100032909.1.5",
          "form": "악ㅋㅋㅋㅋㅋㅋ",
          "original_form": "악ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 05:00"
        },
        {
          "id": "MDRW2100032909.1.6",
          "form": "왜죠???",
          "original_form": "왜죠???",
          "speaker_id": "1",
          "time": "20210906 05:00"
        },
        {
          "id": "MDRW2100032909.1.7",
          "form": "너무 피곤해서요..",
          "original_form": "너무 피곤해서요..",
          "speaker_id": "2",
          "time": "20210906 05:00"
        },
        {
          "id": "MDRW2100032909.1.8",
          "form": "아기 키우느라 힘들지??",
          "original_form": "아기 키우느라 힘들지??",
          "speaker_id": "1",
          "time": "20210906 05:00"
        },
        {
          "id": "MDRW2100032909.1.9",
          "form": "힘든거는 모르겠는데.. 만성 피로야..",
          "original_form": "힘든거는 모르겠는데.. 만성 피로야..",
          "speaker_id": "2",
          "time": "20210906 05:00"
        },
        {
          "id": "MDRW2100032909.1.10",
          "form": "휴. 문자하지 말고 잘까???",
          "original_form": "휴. 문자하지 말고 잘까???",
          "speaker_id": "1",
          "time": "20210906 05:00"
        },
        {
          "id": "MDRW2100032909.1.11",
          "form": "30분 있으면 또 수유시간이다...",
          "original_form": "30분 있으면 또 수유시간이다...",
          "speaker_id": "2",
          "time": "20210906 05:01"
        },
        {
          "id": "MDRW2100032909.1.12",
          "form": "잘 수도 없네..",
          "original_form": "잘 수도 없네..",
