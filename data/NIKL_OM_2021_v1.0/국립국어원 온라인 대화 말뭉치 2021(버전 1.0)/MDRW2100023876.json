      "utterance": [
        {
          "id": "MDRW2100023876.1.1",
          "form": "전 까르보나라 붉닭볶음면 좋아해요",
          "original_form": "전 까르보나라 붉닭볶음면 좋아해요",
          "speaker_id": "2",
          "time": "20210830 19:32"
        },
        {
          "id": "MDRW2100023876.1.2",
          "form": "헐ㅋㅋㅋㅋ 저도 일반 불닭은 못먹는데",
          "original_form": "헐ㅋㅋㅋㅋ 저도 일반 불닭은 못먹는데",
          "speaker_id": "1",
          "time": "20210830 19:32"
        },
        {
          "id": "MDRW2100023876.1.3",
          "form": "그냥 불닭볶음면은 밥없이는 힘들더라구요",
          "original_form": "그냥 불닭볶음면은 밥없이는 힘들더라구요",
          "speaker_id": "2",
          "time": "20210830 19:32"
        },
        {
          "id": "MDRW2100023876.1.4",
          "form": "까르보나라 불닭은 잘먹어요!",
          "original_form": "까르보나라 불닭은 잘먹어요!",
          "speaker_id": "1",
          "time": "20210830 19:32"
        },
        {
          "id": "MDRW2100023876.1.5",
          "form": "그건 맛있어요!",
          "original_form": "그건 맛있어요!",
          "speaker_id": "2",
          "time": "20210830 19:32"
        },
        {
          "id": "MDRW2100023876.1.6",
          "form": "이것도 사실 제가 먹을수있는",
          "original_form": "이것도 사실 제가 먹을수있는",
          "speaker_id": "1",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100023876.1.7",
          "form": "매운맛의 한계치이긴한데ㅋㅋㅋㅋ",
          "original_form": "매운맛의 한계치이긴한데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100023876.1.8",
          "form": "아 ㅎㅎㅎ소스를 반만 넣으면 가끔 먹기에는 괜찮아요ㅎㅎ",
          "original_form": "아 ㅎㅎㅎ소스를 반만 넣으면 가끔 먹기에는 괜찮아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100023876.1.9",
          "form": "근데 저도 1년에 한번먹을까 싶더라구요ㅎ",
          "original_form": "근데 저도 1년에 한번먹을까 싶더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100023876.1.10",
          "form": "아 그런방법이 있었구나...! 전 바보같이 몰랐네요ㅋㅋㅋ",
          "original_form": "아 그런방법이 있었구나...! 전 바보같이 몰랐네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100023876.1.11",
          "form": "라면 좋아하세요?",
          "original_form": "라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100023876.1.12",
          "form": "많이는 안좋아하는데 가끔 멸치칼국수는 생각나더라구요",
          "original_form": "많이는 안좋아하는데 가끔 멸치칼국수는 생각나더라구요",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023876.1.13",
          "form": "헉ㅋㅋㅋ 이거 넘 맛있죠",
          "original_form": "헉ㅋㅋㅋ 이거 넘 맛있죠",
          "speaker_id": "1",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023876.1.14",
          "form": "멸치칼국수라고 하는데 멸치맛은 많이 안나서 시원하고",
          "original_form": "멸치칼국수라고 하는데 멸치맛은 많이 안나서 시원하고",
          "speaker_id": "1",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023876.1.15",
          "form": "그쵸?\n마지막에 밥을 뭉게지도록 불리고 계란 휘휘젓고 참기름으로 마무리해주면 정말 최고죠",
          "original_form": "그쵸?\n마지막에 밥을 뭉게지도록 불리고 계란 휘휘젓고 참기름으로 마무리해주면 정말 최고죠",
          "speaker_id": "2",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023876.1.16",
          "form": "헐ㅠㅠ 그럼 완전 샤브샤브 마지막에 먹는 죽되잖아용",
          "original_form": "헐ㅠㅠ 그럼 완전 샤브샤브 마지막에 먹는 죽되잖아용",
          "speaker_id": "1",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023876.1.17",
          "form": "한그릇 뚝딱이네요ㅋㅋ",
          "original_form": "한그릇 뚝딱이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023876.1.18",
          "form": "그쵸!\n집에서 채선당샤브샤브를 느낄 수 있죠ㅎㅎㅎ",
          "original_form": "그쵸!\n집에서 채선당샤브샤브를 느낄 수 있죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:38"
        }
      ]
