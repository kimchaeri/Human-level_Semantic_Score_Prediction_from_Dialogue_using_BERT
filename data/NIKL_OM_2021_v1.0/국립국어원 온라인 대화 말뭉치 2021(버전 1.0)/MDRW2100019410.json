      "utterance": [
        {
          "id": "MDRW2100019410.1.1",
          "form": "넼ㅋㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
