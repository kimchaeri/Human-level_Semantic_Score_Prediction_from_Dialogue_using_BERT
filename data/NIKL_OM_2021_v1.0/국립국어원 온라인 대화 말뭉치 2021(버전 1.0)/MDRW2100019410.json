        },
        {
          "id": "MDRW2100019410.1.2",
          "form": "제일 좋아하는 빵은 무엇이신가요",
          "original_form": "제일 좋아하는 빵은 무엇이신가요",
          "speaker_id": "2",
          "time": "20210827 19:02"
        },
        {
          "id": "MDRW2100019410.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ음 마늘빵이 요새 좋은거같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ음 마늘빵이 요새 좋은거같아요",
          "speaker_id": "1",
          "time": "20210827 19:02"
        },
        {
          "id": "MDRW2100019410.1.4",
          "form": "마카롱도 좋구요",
          "original_form": "마카롱도 좋구요",
          "speaker_id": "1",
          "time": "20210827 19:02"
        },
        {
          "id": "MDRW2100019410.1.5",
          "form": "아 마늘빵 맛있져......",
          "original_form": "아 마늘빵 맛있져......",
          "speaker_id": "2",
          "time": "20210827 19:04"
        },
        {
          "id": "MDRW2100019410.1.6",
          "form": "마늘들어가면 다 맛난거같아요",
          "original_form": "마늘들어가면 다 맛난거같아요",
          "speaker_id": "1",
          "time": "20210827 19:04"
        },
        {
          "id": "MDRW2100019410.1.7",
          "form": "빵 좋아하세요?",
          "original_form": "빵 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 19:04"
        },
        {
          "id": "MDRW2100019410.1.8",
          "form": "역시 뼛 속 한국인이시군요",
          "original_form": "역시 뼛 속 한국인이시군요",
          "speaker_id": "2",
          "time": "20210827 19:04"
        },
        {
          "id": "MDRW2100019410.1.9",
          "form": "네 저 빵 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 저 빵 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:04"
        },
        {
          "id": "MDRW2100019410.1.10",
          "form": "마늘도 좋아해욬ㅋㅋㅋㅋ",
          "original_form": "마늘도 좋아해욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:04"
        },
        {
          "id": "MDRW2100019410.1.11",
          "form": "ㅋㅋㅋㅋㅋ 이상하게 마늘은 별론데 저는",
          "original_form": "ㅋㅋㅋㅋㅋ 이상하게 마늘은 별론데 저는",
          "speaker_id": "1",
          "time": "20210827 19:05"
        },
        {
          "id": "MDRW2100019410.1.12",
          "form": "갈릭은 좋아요... 뭔지아시죠?ㅋㅋㅋㅋㅋㅋ",
          "original_form": "갈릭은 좋아요... 뭔지아시죠?ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:05"
        },
        {
          "id": "MDRW2100019410.1.13",
          "form": "엌ㅋㅋㅋ넼ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋ넼ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100019410.1.14",
          "form": "갈릭파스타...이런거요....?",
          "original_form": "갈릭파스타...이런거요....?",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100019410.1.15",
          "form": "갈릭토스트....",
          "original_form": "갈릭토스트....",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100019410.1.16",
          "form": "갈릭쨈?ㅋㅋㅋㅋㅋ",
          "original_form": "갈릭쨈?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100019410.1.17",
          "form": "네 ㅋㅋㅋㅋㅋ 뭔가",
          "original_form": "네 ㅋㅋㅋㅋㅋ 뭔가",
          "speaker_id": "1",
          "time": "20210827 19:07"
        },
        {
          "id": "MDRW2100019410.1.18",
          "form": "고기구울때 먹는 마늘 ..이렁건 안먹는데",
          "original_form": "고기구울때 먹는 마늘 ..이렁건 안먹는데",
          "speaker_id": "1",
          "time": "20210827 19:07"
        },
        {
          "id": "MDRW2100019410.1.19",
          "form": "마늘빵이나 파스타는 좋아요 ㅋㅋㅋ",
          "original_form": "마늘빵이나 파스타는 좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:07"
        },
        {
          "id": "MDRW2100019410.1.20",
          "form": "파스타는 뭘 제일 좋아하시나요!",
          "original_form": "파스타는 뭘 제일 좋아하시나요!",
          "speaker_id": "2",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100019410.1.21",
          "form": "저는 크림이요!",
          "original_form": "저는 크림이요!",
          "speaker_id": "1",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100019410.1.22",
          "form": "뭐좋아하세요?",
          "original_form": "뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100019410.1.23",
          "form": "저는 알리오올리오가 좋아졌어요!",
          "original_form": "저는 알리오올리오가 좋아졌어요!",
          "speaker_id": "2",
          "time": "20210827 19:16"
        },
        {
          "id": "MDRW2100019410.1.24",
          "form": "그거도 맛있죠 ㅎㅎ",
          "original_form": "그거도 맛있죠 ㅎㅎ",
