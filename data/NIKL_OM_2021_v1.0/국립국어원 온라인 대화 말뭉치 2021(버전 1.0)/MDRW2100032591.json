      "utterance": [
        {
          "id": "MDRW2100032591.1.1",
          "form": "ㅎㅎ바움쿠헨뜻이 넘 귀여운디저트에요",
          "original_form": "ㅎㅎ바움쿠헨뜻이 넘 귀여운디저트에요",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100032591.1.2",
          "form": "뜻이 나무테였던가요..? 나무랑 비슷하다고 이름 붙여졌던 걸로 기억해요 ㅋㅋ",
          "original_form": "뜻이 나무테였던가요..? 나무랑 비슷하다고 이름 붙여졌던 걸로 기억해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100032591.1.3",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "1",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100032591.1.4",
          "form": "오! 제가 맞게 기억하고 있었군요!! ㅎㅎ",
          "original_form": "오! 제가 맞게 기억하고 있었군요!! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100032591.1.5",
          "form": "네 ㅎㅎ 각국디저트 여행도 좋을거같아요",
          "original_form": "네 ㅎㅎ 각국디저트 여행도 좋을거같아요",
          "speaker_id": "1",
          "time": "20210905 21:42"
        },
        {
          "id": "MDRW2100032591.1.6",
          "form": "앗 너무 좋아요ㅜㅜ",
          "original_form": "앗 너무 좋아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032591.1.7",
          "form": "정말 기회만 되면 각국 돌아다니면서 디저트 먹어보고 싶어요..",
          "original_form": "정말 기회만 되면 각국 돌아다니면서 디저트 먹어보고 싶어요..",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032591.1.8",
          "form": "맞아요 ㅎㅎ빨리 코로나만 끝나라",
          "original_form": "맞아요 ㅎㅎ빨리 코로나만 끝나라",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032591.1.9",
          "form": "진짜 내년엔 이시국 끝나면 좋겠어요ㅜ",
          "original_form": "진짜 내년엔 이시국 끝나면 좋겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032591.1.10",
          "form": "정말요 제발",
          "original_form": "정말요 제발",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032591.1.11",
          "form": "변이좀 고만..",
          "original_form": "변이좀 고만..",
          "speaker_id": "1",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100032591.1.12",
          "form": "대체 어디까지 갈 생각인지 모르겠어요...",
          "original_form": "대체 어디까지 갈 생각인지 모르겠어요...",
          "speaker_id": "2",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032591.1.13",
          "form": "꼭 내년엔 비행기타길",
          "original_form": "꼭 내년엔 비행기타길",
          "speaker_id": "1",
          "time": "20210906 00:35"
        },
        {
          "id": "MDRW2100032591.1.14",
          "form": "기원해봅니닷",
          "original_form": "기원해봅니닷",
          "speaker_id": "1",
          "time": "20210906 00:35"
        },
        {
          "id": "MDRW2100032591.1.15",
          "form": "저두요 ㅋㅋㅋ 내년엔 부디 풀리길 바라고 있어요",
          "original_form": "저두요 ㅋㅋㅋ 내년엔 부디 풀리길 바라고 있어요",
          "speaker_id": "2",
          "time": "20210906 00:52"
        },
        {
          "id": "MDRW2100032591.1.16",
          "form": "남미 여행가보고싶으세요?",
          "original_form": "남미 여행가보고싶으세요?",
          "speaker_id": "1",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100032591.1.17",
          "form": "친구가 다녀왔는데 신기해요",
          "original_form": "친구가 다녀왔는데 신기해요",
          "speaker_id": "1",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100032591.1.18",
          "form": "헐 네 저 남미도 가보고 싶어요!",
          "original_form": "헐 네 저 남미도 가보고 싶어요!",
          "speaker_id": "2",
          "time": "20210907 08:44"
        },
        {
          "id": "MDRW2100032591.1.19",
          "form": "와 정말요?",
          "original_form": "와 정말요?",
          "speaker_id": "1",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100032591.1.20",
          "form": "전 무섭더라고요..",
          "original_form": "전 무섭더라고요..",
          "speaker_id": "1",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100032591.1.21",
          "form": "음식도 걱정이고 ㅠ",
          "original_form": "음식도 걱정이고 ㅠ",
          "speaker_id": "1",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100032591.1.22",
          "form": "음식은 아무래도 걱정안하기가 어렵죠..ㅜ",
          "original_form": "음식은 아무래도 걱정안하기가 어렵죠..ㅜ",
          "speaker_id": "2",
          "time": "20210907 13:07"
        },
        {
          "id": "MDRW2100032591.1.23",
          "form": "그래도 기회되면 가보고 싶은 마음은 있어서 ㅎㅎ",
          "original_form": "그래도 기회되면 가보고 싶은 마음은 있어서 ㅎㅎ",
