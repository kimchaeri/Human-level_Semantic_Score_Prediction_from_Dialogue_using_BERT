      "utterance": [
        {
          "id": "MDRW2100047148.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100047148.1.2",
          "form": "당근해요",
          "original_form": "당근해요",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100047148.1.3",
          "form": "당근거래요",
          "original_form": "당근거래요",
          "speaker_id": "2",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100047148.1.4",
          "form": "저도 당근",
          "original_form": "저도 당근",
          "speaker_id": "1",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100047148.1.5",
          "form": "지역 반경 기반 중고거래 좋더라구요",
          "original_form": "지역 반경 기반 중고거래 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100047148.1.6",
          "form": "저는 제라늄 키우는데 제라늄 당근으로 구매도 하고 판매도 해요.",
          "original_form": "저는 제라늄 키우는데 제라늄 당근으로 구매도 하고 판매도 해요.",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100047148.1.7",
          "form": "제라늄이 머죠?",
          "original_form": "제라늄이 머죠?",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100047148.1.8",
          "form": "아..식물이름이요",
          "original_form": "아..식물이름이요",
          "speaker_id": "2",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100047148.1.9",
          "form": "직구하세요?",
          "original_form": "직구하세요?",
          "speaker_id": "2",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100047148.1.10",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100047148.1.11",
          "form": "당근거래에서 어떤거 사세요?",
          "original_form": "당근거래에서 어떤거 사세요?",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100047148.1.12",
          "form": "팔기만요",
          "original_form": "팔기만요",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100047148.1.13",
          "form": "아.. 그러시구나",
          "original_form": "아.. 그러시구나",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100047148.1.14",
          "form": "저는 최근 아들이 타던 자전거 팔았어요.",
          "original_form": "저는 최근 아들이 타던 자전거 팔았어요.",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100047148.1.15",
          "form": "7~8년전에 45만원주고 샀는데 10만원 받고 팔았네요. 팔고나니 넘 싸게 팔았나 싶고 좀 아쉽기도 했어요.",
          "original_form": "7~8년전에 45만원주고 샀는데 10만원 받고 팔았네요. 팔고나니 넘 싸게 팔았나 싶고 좀 아쉽기도 했어요.",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100047148.1.16",
          "form": "어떤물건 파세요?",
          "original_form": "어떤물건 파세요?",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100047148.1.17",
          "form": "어떤물건 파셨어요?",
          "original_form": "어떤물건 파셨어요?",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100047148.1.18",
          "form": "공기청정기요",
          "original_form": "공기청정기요",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100047148.1.19",
          "form": "중고거래 재미있어서 물건 잘못 사도 중고로 팔면되지 라는 마음에 소비가 좀 늘어나는거 같인요.",
          "original_form": "중고거래 재미있어서 물건 잘못 사도 중고로 팔면되지 라는 마음에 소비가 좀 늘어나는거 같인요.",
          "speaker_id": "2",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100047148.1.20",
          "form": "그러니까됴",
          "original_form": "그러니까됴",
