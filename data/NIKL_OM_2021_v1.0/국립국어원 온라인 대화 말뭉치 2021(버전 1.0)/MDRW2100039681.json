      "utterance": [
        {
          "id": "MDRW2100039681.1.1",
          "form": "안녕하세요! 저녁 혹시 드셨나요?",
          "original_form": "안녕하세요! 저녁 혹시 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039681.1.2",
          "form": "네ㅎㅎ 닭갈비먹었어요",
          "original_form": "네ㅎㅎ 닭갈비먹었어요",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039681.1.3",
          "form": "뭐드셨나요,",
          "original_form": "뭐드셨나요,",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039681.1.4",
          "form": "오 저도 닭시켜먹었어요 ㅎㅎ 푸라닭 고추마요 먹었어요!",
          "original_form": "오 저도 닭시켜먹었어요 ㅎㅎ 푸라닭 고추마요 먹었어요!",
          "speaker_id": "1",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100039681.1.5",
          "form": "푸라닭 맛있죠ㅎㅎ",
          "original_form": "푸라닭 맛있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039681.1.6",
          "form": "순살로드시나요?",
          "original_form": "순살로드시나요?",
          "speaker_id": "2",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100039681.1.7",
          "form": "네 역시 순살이 맛있어가지구 ㅋㅋㅋ",
          "original_form": "네 역시 순살이 맛있어가지구 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100039681.1.8",
          "form": "치킨지주드세요 ㅎㅎ?",
          "original_form": "치킨지주드세요 ㅎㅎ?",
          "speaker_id": "2",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100039681.1.9",
          "form": "네 저 최애메뉴여서요!",
          "original_form": "네 저 최애메뉴여서요!",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039681.1.10",
          "form": "님은 배달음식 머 젤 좋아하세요??",
          "original_form": "님은 배달음식 머 젤 좋아하세요??",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039681.1.11",
          "form": "저는 피자였다가 치킨이었다가",
          "original_form": "저는 피자였다가 치킨이었다가",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039681.1.12",
          "form": "매일 바뀌는거같아요ㅎㅎ",
          "original_form": "매일 바뀌는거같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039681.1.13",
          "form": "무슨 치킨 젤 좋아하시는지 궁금해요!",
          "original_form": "무슨 치킨 젤 좋아하시는지 궁금해요!",
          "speaker_id": "1",
          "time": "20210903 22:13"
        },
        {
          "id": "MDRW2100039681.1.14",
          "form": "푸라닭 많이먹었어요ㅎㅎㅎ",
          "original_form": "푸라닭 많이먹었어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039681.1.15",
          "form": "그런데 자주먹으니까 좀 질리기도하고 그렇더라구요",
          "original_form": "그런데 자주먹으니까 좀 질리기도하고 그렇더라구요",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039681.1.16",
          "form": "푸라닭중엔 그..약간 깐풍소스느낌나는거 좋아해요",
          "original_form": "푸라닭중엔 그..약간 깐풍소스느낌나는거 좋아해요",
          "speaker_id": "2",
          "time": "20210903 22:14"
        },
        {
          "id": "MDRW2100039681.1.17",
          "form": "갑자기이름이생각안나네요",
          "original_form": "갑자기이름이생각안나네요",
          "speaker_id": "2",
          "time": "20210903 22:15"
        },
        {
          "id": "MDRW2100039681.1.18",
          "form": "헉 ㄱ깐풍소스느낌나는건 안먹어봐서 잘 모르겠어욬 뭘까요?? 궁금하네요ㅠㅋㅋㅋㅋ",
          "original_form": "헉 ㄱ깐풍소스느낌나는건 안먹어봐서 잘 모르겠어욬 뭘까요?? 궁금하네요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039681.1.19",
          "form": "전 좀 매콤한 거 좋아해서 고추바사삭도 좋아해요!ㅋㅋㅋ",
          "original_form": "전 좀 매콤한 거 좋아해서 고추바사삭도 좋아해요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039681.1.20",
          "form": "제너럴핫치킨인거같아요!",
          "original_form": "제너럴핫치킨인거같아요!",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039681.1.21",
          "form": "저는 볼케이노도 맛있더라구요",
          "original_form": "저는 볼케이노도 맛있더라구요",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039681.1.22",
          "form": "고 추..바사삭..안쳐지네요...",
          "original_form": "고 추..바사삭..안쳐지네요...",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039681.1.23",
          "form": "ㅋㅋ큐ㅠㅜㅜ 그것도 맛있죠",
          "original_form": "ㅋㅋ큐ㅠㅜㅜ 그것도 맛있죠",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039681.1.24",
          "form": "악 ㅠㅠ이름이! 제너럴핫치킨 기억해둘게여!!",
          "original_form": "악 ㅠㅠ이름이! 제너럴핫치킨 기억해둘게여!!",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039681.1.25",
          "form": "굽네거가 웬만하면 다 맛난것같아요",
          "original_form": "굽네거가 웬만하면 다 맛난것같아요",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039681.1.26",
          "form": "볼케이노 위에 치즈올려먹는거 개인적으로 좋아하는데요.. 크..",
          "original_form": "볼케이노 위에 치즈올려먹는거 개인적으로 좋아하는데요.. 크..",
          "speaker_id": "1",
          "time": "20210903 22:31"
        }
      ]
