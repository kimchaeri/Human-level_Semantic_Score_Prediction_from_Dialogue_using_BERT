      "utterance": [
        {
          "id": "MDRW2100044586.1.1",
          "form": "오늘 비옴?",
          "original_form": "오늘 비옴?",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044586.1.2",
          "form": "비 살짝왔나?",
          "original_form": "비 살짝왔나?",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044586.1.3",
          "form": "일기예보보니 전국에 비긴하더라",
          "original_form": "일기예보보니 전국에 비긴하더라",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044586.1.4",
          "form": "여긴 아침점심저녁",
          "original_form": "여긴 아침점심저녁",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044586.1.5",
          "form": "딱 중간중간 해뜨고 날씨 괜찮고ㅋ",
          "original_form": "딱 중간중간 해뜨고 날씨 괜찮고ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044586.1.6",
          "form": "아점저에 맞춰 비온듯ㅋ",
          "original_form": "아점저에 맞춰 비온듯ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044586.1.7",
          "form": "맞나 ㅋㅋ 전국적으로 비오긴했나보더라",
          "original_form": "맞나 ㅋㅋ 전국적으로 비오긴했나보더라",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044586.1.8",
          "form": "비가 많이 내리는 지역도 있나보더라고",
          "original_form": "비가 많이 내리는 지역도 있나보더라고",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044586.1.9",
          "form": "장마가 장마같지않고",
          "original_form": "장마가 장마같지않고",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044586.1.10",
          "form": "지금은 장마 끝났는데 비 찔끔 계속 오고",
          "original_form": "지금은 장마 끝났는데 비 찔끔 계속 오고",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044586.1.11",
          "form": "장마인지도모르겠어",
          "original_form": "장마인지도모르겠어",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044586.1.12",
          "form": "ㅇㅇ비가 찔끔찔끔 계속와",
          "original_form": "ㅇㅇ비가 찔끔찔끔 계속와",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044586.1.13",
          "form": "날씨도습도덕에더워",
          "original_form": "날씨도습도덕에더워",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044586.1.14",
          "form": "아~  습도때문에 더웠던가",
          "original_form": "아~  습도때문에 더웠던가",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044586.1.15",
          "form": "ㅇㅇ습도때문에 덥더라",
          "original_form": "ㅇㅇ습도때문에 덥더라",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044586.1.16",
          "form": "나 요즘 계속 더워서 회사서 선풍기 달고 살자나\n근데 오늘 고장남ㅜㅜ",
          "original_form": "나 요즘 계속 더워서 회사서 선풍기 달고 살자나\n근데 오늘 고장남ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044586.1.17",
          "form": "ㅋㄱㅋ",
          "original_form": "ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044586.1.18",
          "form": "밥먹는다고 요리했는데 더워서환장",
          "original_form": "밥먹는다고 요리했는데 더워서환장",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044586.1.19",
          "form": "그걸 몰랐네",
          "original_form": "그걸 몰랐네",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044586.1.20",
          "form": "ㅜㅜ 선풍기 내일도 피료할텐데",
          "original_form": "ㅜㅜ 선풍기 내일도 피료할텐데",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044586.1.21",
          "form": "어이구 많이 더웠겠다\n땀안났어?",
          "original_form": "어이구 많이 더웠겠다\n땀안났어?",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044586.1.22",
          "form": "우째",
          "original_form": "우째",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044586.1.23",
          "form": "땀 살짝났징 ㅋㅋ",
          "original_form": "땀 살짝났징 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:16"
        }
      ]
