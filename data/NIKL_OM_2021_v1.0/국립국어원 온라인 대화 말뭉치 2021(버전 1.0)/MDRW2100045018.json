      "utterance": [
        {
          "id": "MDRW2100045018.1.1",
          "form": "일반은머사셨어요?",
          "original_form": "일반은머사셨어요?",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045018.1.2",
          "form": "원래 주식 쇼핑도 자주 하시나요?",
          "original_form": "원래 주식 쇼핑도 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045018.1.3",
          "form": "네~~작년부터 가끔사요ㅋㅋ",
          "original_form": "네~~작년부터 가끔사요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045018.1.4",
          "form": "전 다양하게 있고 목요일 선물옵션만기일과 추석 대비해서 돈 뺀둔 상태예요 지금은 단타로 사요",
          "original_form": "전 다양하게 있고 목요일 선물옵션만기일과 추석 대비해서 돈 뺀둔 상태예요 지금은 단타로 사요",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045018.1.5",
          "form": "와 엄청 전문가시군요ㅋㅋ",
          "original_form": "와 엄청 전문가시군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045018.1.6",
          "form": "누구나 삼성전자는 품어두죠",
          "original_form": "누구나 삼성전자는 품어두죠",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045018.1.7",
          "form": "삼전 계속 떨어져서",
          "original_form": "삼전 계속 떨어져서",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045018.1.8",
          "form": "아니에요 저도 작년부터 사기 시작했어요",
          "original_form": "아니에요 저도 작년부터 사기 시작했어요",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045018.1.9",
          "form": "물타기하다가 대주주될꺼같아요ㅋㅋ",
          "original_form": "물타기하다가 대주주될꺼같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045018.1.10",
          "form": "선물옵션은 저는 뭔지 넘 어려워요",
          "original_form": "선물옵션은 저는 뭔지 넘 어려워요",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045018.1.11",
          "form": "롯데렌탈 샀었는데 계속 내려가는것 같아서 손절하고 단타로 메꿨어요",
          "original_form": "롯데렌탈 샀었는데 계속 내려가는것 같아서 손절하고 단타로 메꿨어요",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045018.1.12",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045018.1.13",
          "form": "손절하셨군요",
          "original_form": "손절하셨군요",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045018.1.14",
          "form": "아직못했어요",
          "original_form": "아직못했어요",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045018.1.15",
          "form": "첫날걍팔껄ㅡㅡ후회가",
          "original_form": "첫날걍팔껄ㅡㅡ후회가",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045018.1.16",
          "form": "만기일 전후로 지수가 많이 빠져요 외국인이 물건 다 팔거든요",
          "original_form": "만기일 전후로 지수가 많이 빠져요 외국인이 물건 다 팔거든요",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045018.1.17",
          "form": "롯데는 물건을 왜 판건지 이해 못하겠어요",
          "original_form": "롯데는 물건을 왜 판건지 이해 못하겠어요",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045018.1.18",
          "form": "그러게요 주린이라 분위기도모르고넣은내탓ㅠㅠ",
          "original_form": "그러게요 주린이라 분위기도모르고넣은내탓ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045018.1.19",
          "form": "네임밸류 있다고 봤는데 역시 공모주 사는건 기관 수요가 중요하단걸 알았어요 존버해요",
          "original_form": "네임밸류 있다고 봤는데 역시 공모주 사는건 기관 수요가 중요하단걸 알았어요 존버해요",
          "speaker_id": "2",
          "time": "20210908 00:42"
        }
      ]
