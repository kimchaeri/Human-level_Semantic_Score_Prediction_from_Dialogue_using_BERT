      "utterance": [
        {
          "id": "MDRW2100020868.1.1",
          "form": "원래 운동을 많이하세여?",
          "original_form": "원래 운동을 많이하세여?",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100020868.1.2",
          "form": "운동많이하는편은 아니였어요",
          "original_form": "운동많이하는편은 아니였어요",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020868.1.3",
          "form": "근데 산행은 지나가다가 어느멋진남자분이 등산복입고 가시는거보고",
          "original_form": "근데 산행은 지나가다가 어느멋진남자분이 등산복입고 가시는거보고",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020868.1.4",
          "form": "그 산악회 갔는데 ㅋㅋㅋ",
          "original_form": "그 산악회 갔는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100020868.1.5",
          "form": "제가 가는 산에는 그분이 안오시는거 같더라구요",
          "original_form": "제가 가는 산에는 그분이 안오시는거 같더라구요",
          "speaker_id": "2",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100020868.1.6",
          "form": "ㅋㅋㅋ 운동이 목적이 아니셨구나.. ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 운동이 목적이 아니셨구나.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020868.1.7",
          "form": "근데 막상 산행다니니 좋더라구요",
          "original_form": "근데 막상 산행다니니 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020868.1.8",
          "form": "그래서 혼자다니다가 사촌언니 끌여들여서",
          "original_form": "그래서 혼자다니다가 사촌언니 끌여들여서",
          "speaker_id": "2",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100020868.1.9",
          "form": "힘든데 많이 다니셧나봐요?",
          "original_form": "힘든데 많이 다니셧나봐요?",
          "speaker_id": "1",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100020868.1.10",
          "form": "같이 많이 다녔죠 ㅎ",
          "original_form": "같이 많이 다녔죠 ㅎ",
          "speaker_id": "2",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100020868.1.11",
          "form": "산행가서 먹는  김밥이 너무 맛나요ㅎ",
          "original_form": "산행가서 먹는  김밥이 너무 맛나요ㅎ",
          "speaker_id": "2",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100020868.1.12",
          "form": "등산은 사실 운동이 아니라 먹으러 다니는거죠..",
          "original_form": "등산은 사실 운동이 아니라 먹으러 다니는거죠..",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020868.1.13",
          "form": "진짜 뭘 먹어도 꿀맛.. ㅋㅋㅋㅋ",
          "original_form": "진짜 뭘 먹어도 꿀맛.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100020868.1.14",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100020868.1.15",
          "form": "하산해서 시원한 맥주한캔도 딱 좋아요",
          "original_form": "하산해서 시원한 맥주한캔도 딱 좋아요",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020868.1.16",
          "form": "그건 술이아니라.. 신의 선물이죠.. ㅋㅋㅋ",
          "original_form": "그건 술이아니라.. 신의 선물이죠.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100020868.1.17",
          "form": "이러니 운동한다고 산가도 살만쪄요.. ㅋㅋ",
          "original_form": "이러니 운동한다고 산가도 살만쪄요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100020868.1.18",
          "form": "안빠지죠 ㅎㅎ",
          "original_form": "안빠지죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020868.1.19",
          "form": "그래도 운동하고 먹는건 죄책감이 덜하죠 ㅎ",
          "original_form": "그래도 운동하고 먹는건 죄책감이 덜하죠 ㅎ",
          "speaker_id": "2",
          "time": "20210828 13:54"
        },
        {
          "id": "MDRW2100020868.1.20",
          "form": "ㅋㅋ 아까는 전문 운동인 같았는데..",
          "original_form": "ㅋㅋ 아까는 전문 운동인 같았는데..",
          "speaker_id": "1",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100020868.1.21",
          "form": "ㅋㅋㅋ 저 운동마니아 아니예요",
          "original_form": "ㅋㅋㅋ 저 운동마니아 아니예요",
          "speaker_id": "2",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100020868.1.22",
          "form": "지리산 한라산 다님.. 등산인이죠.. ㅋㅋ",
          "original_form": "지리산 한라산 다님.. 등산인이죠.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:57"
        },
        {
          "id": "MDRW2100020868.1.23",
          "form": "산행시작은 남자였으나 결국 재미로 다녔어요",
