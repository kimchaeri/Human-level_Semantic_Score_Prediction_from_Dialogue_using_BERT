      "utterance": [
        {
          "id": "MDRW2100026644.1.1",
          "form": "혹시 디저트먹고 운동하시나요?",
          "original_form": "혹시 디저트먹고 운동하시나요?",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026644.1.2",
          "form": "운동은 평생 해왔죠",
          "original_form": "운동은 평생 해왔죠",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026644.1.3",
          "form": "숨쉬기 운동...",
          "original_form": "숨쉬기 운동...",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026644.1.4",
          "form": "님은 하시나요?",
          "original_form": "님은 하시나요?",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026644.1.5",
          "form": "전 야식먹으면 꼭 운동합니다",
          "original_form": "전 야식먹으면 꼭 운동합니다",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026644.1.6",
          "form": "빨래판 복근이네요",
          "original_form": "빨래판 복근이네요",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026644.1.7",
          "form": "아 고전 사과합니다",
          "original_form": "아 고전 사과합니다",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026644.1.8",
          "form": "아내가 정말 욕을 안하시나용",
          "original_form": "아내가 정말 욕을 안하시나용",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026644.1.9",
          "form": "진정 에인절ㅎㅎ",
          "original_form": "진정 에인절ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026644.1.10",
          "form": "운동 뭐하세요?",
          "original_form": "운동 뭐하세요?",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026644.1.11",
          "form": "철봉 팔굽혀펴기 윗몸일으키기",
          "original_form": "철봉 팔굽혀펴기 윗몸일으키기",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026644.1.12",
          "form": "합니다",
          "original_form": "합니다",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026644.1.13",
          "form": "늙으니까 몸이 아파서요",
          "original_form": "늙으니까 몸이 아파서요",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026644.1.14",
          "form": "헉 철봉이라면",
          "original_form": "헉 철봉이라면",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026644.1.15",
          "form": "밖에 나가서 하시나요!",
          "original_form": "밖에 나가서 하시나요!",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026644.1.16",
          "form": "아 제방입구에 설치해놧습니다",
          "original_form": "아 제방입구에 설치해놧습니다",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026644.1.17",
          "form": "철봉 하고 나서 꼭꼭 계란 삶은 거를 먹어야 됩니다",
          "original_form": "철봉 하고 나서 꼭꼭 계란 삶은 거를 먹어야 됩니다",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026644.1.18",
          "form": "계란은  끓는 물에 15분",
          "original_form": "계란은  끓는 물에 15분",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026644.1.19",
          "form": "맙소사",
          "original_form": "맙소사",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026644.1.20",
          "form": "계란은 흰자만 먹습니다",
          "original_form": "계란은 흰자만 먹습니다",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026644.1.21",
          "form": "그렇게 안봤는데 독한 분이셨군요ㄷㄷ",
          "original_form": "그렇게 안봤는데 독한 분이셨군요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026644.1.22",
          "form": "아 그렇지 않아요",
          "original_form": "아 그렇지 않아요",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026644.1.23",
          "form": "지금 주님을 찾고 있습니다ㄷㄷㄷ",
          "original_form": "지금 주님을 찾고 있습니다ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026644.1.24",
          "form": "ㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026644.1.25",
          "form": "아 저 무교임",
          "original_form": "아 저 무교임",
          "speaker_id": "1",
          "time": "20210901 23:30"
        }
