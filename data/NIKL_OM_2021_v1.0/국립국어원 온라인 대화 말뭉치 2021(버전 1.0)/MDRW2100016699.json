      "utterance": [
        {
          "id": "MDRW2100016699.1.1",
          "form": "너희 집 설거지는 누구누구해?",
          "original_form": "너희 집 설거지는 누구누구해?",
          "speaker_id": "2",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016699.1.2",
          "form": "우리 집은 엄마하고 내가 해.. 너희 집은.",
          "original_form": "우리 집은 엄마하고 내가 해.. 너희 집은.",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016699.1.3",
          "form": "누가 하니?",
          "original_form": "누가 하니?",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016699.1.4",
          "form": "우리도...ㅋㅋㅋㅋㅋ....",
          "original_form": "우리도...ㅋㅋㅋㅋㅋ....",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016699.1.5",
          "form": "결혼에 대해 회의적인 이유 한가지죠 ^.^",
          "original_form": "결혼에 대해 회의적인 이유 한가지죠 ^.^",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016699.1.6",
          "form": "ㅎㅎ아무래도 그렇지 ..",
          "original_form": "ㅎㅎ아무래도 그렇지 ..",
          "speaker_id": "1",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016699.1.7",
          "form": "오빠는 가끔 와서 안도와줘?",
          "original_form": "오빠는 가끔 와서 안도와줘?",
          "speaker_id": "2",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016699.1.8",
          "form": "오빠는 아예 독립한거라 좀 상황이 예외긴 한데",
          "original_form": "오빠는 아예 독립한거라 좀 상황이 예외긴 한데",
          "speaker_id": "1",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016699.1.9",
          "form": "평소에 있엇을 때는?",
          "original_form": "평소에 있엇을 때는?",
          "speaker_id": "2",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016699.1.10",
          "form": "집에",
          "original_form": "집에",
          "speaker_id": "2",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016699.1.11",
          "form": "명절이나 좀 다같이 밥먹고하면 가위바위보 해서 해",
          "original_form": "명절이나 좀 다같이 밥먹고하면 가위바위보 해서 해",
          "speaker_id": "1",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016699.1.12",
          "form": "글쿠나 그래도 명절땐 다가팅 하네",
          "original_form": "글쿠나 그래도 명절땐 다가팅 하네",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016699.1.13",
          "form": "그때는 그래도 오빠랑 하나씩 나눠서 했던듯??",
          "original_form": "그때는 그래도 오빠랑 하나씩 나눠서 했던듯??",
          "speaker_id": "1",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016699.1.14",
          "form": "우리엄만 동생..잘 안시켜... 너무어이없음 ㅎ",
          "original_form": "우리엄만 동생..잘 안시켜... 너무어이없음 ㅎ",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016699.1.15",
          "form": "그나마 낫네..너넨",
          "original_form": "그나마 낫네..너넨",
          "speaker_id": "2",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016699.1.16",
          "form": "앗 그렇구나..ㅎㅎ...",
          "original_form": "앗 그렇구나..ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210826 14:53"
        },
        {
          "id": "MDRW2100016699.1.17",
          "form": "엄마가 정말 나한테 잘해주는 건 알겠는데",
          "original_form": "엄마가 정말 나한테 잘해주는 건 알겠는데",
