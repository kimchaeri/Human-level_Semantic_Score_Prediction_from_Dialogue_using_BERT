      "utterance": [
        {
          "id": "MDRW2100042757.1.1",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 08:35"
        },
        {
          "id": "MDRW2100042757.1.2",
          "form": "전 아침겸 점심만 ㅎㅎ",
          "original_form": "전 아침겸 점심만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042757.1.3",
          "form": "아침 뭐 드셨어요??",
          "original_form": "아침 뭐 드셨어요??",
          "speaker_id": "2",
          "time": "20210907 11:30"
        },
        {
          "id": "MDRW2100042757.1.4",
          "form": "빵먹었어요ㅠ",
          "original_form": "빵먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210907 11:31"
        },
        {
          "id": "MDRW2100042757.1.5",
          "form": "점심이 걱정이네여",
          "original_form": "점심이 걱정이네여",
          "speaker_id": "1",
          "time": "20210907 11:31"
        },
        {
          "id": "MDRW2100042757.1.6",
          "form": "저는 계란간장밥 먹으려고요++",
          "original_form": "저는 계란간장밥 먹으려고요++",
          "speaker_id": "2",
          "time": "20210907 11:32"
        },
        {
          "id": "MDRW2100042757.1.7",
          "form": "후라이로요?",
          "original_form": "후라이로요?",
          "speaker_id": "1",
          "time": "20210907 11:32"
        },
        {
          "id": "MDRW2100042757.1.8",
          "form": "네 후라이해서!",
          "original_form": "네 후라이해서!",
          "speaker_id": "2",
          "time": "20210907 11:32"
        },
        {
          "id": "MDRW2100042757.1.9",
          "form": "맛있겠어요ㅠ",
          "original_form": "맛있겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042757.1.10",
          "form": "저는둘째가 자면",
          "original_form": "저는둘째가 자면",
          "speaker_id": "1",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042757.1.11",
          "form": "아 두명이시구나",
          "original_form": "아 두명이시구나",
          "speaker_id": "2",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042757.1.12",
          "form": "기저귀하는 애라니 한창 힘드시겠어요",
          "original_form": "기저귀하는 애라니 한창 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042757.1.13",
          "form": "밥 잘 챙겨드시고요 엄마 화이팅",
          "original_form": "밥 잘 챙겨드시고요 엄마 화이팅",
          "speaker_id": "2",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042757.1.14",
          "form": "ㅠㅠ 안자고 또 우유드시고 계시네여ㅠ",
          "original_form": "ㅠㅠ 안자고 또 우유드시고 계시네여ㅠ",
          "speaker_id": "1",
          "time": "20210907 12:35"
        },
        {
          "id": "MDRW2100042757.1.15",
          "form": "ㅠㅠ 낮잠 많이 잘 시기 아닌가여 오늘은 자고싶은 날이 아닌가",
          "original_form": "ㅠㅠ 낮잠 많이 잘 시기 아닌가여 오늘은 자고싶은 날이 아닌가",
          "speaker_id": "2",
          "time": "20210907 12:36"
        },
        {
          "id": "MDRW2100042757.1.16",
          "form": "이제 재워봐야져 ㅠㅠㅠ",
          "original_form": "이제 재워봐야져 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042757.1.17",
          "form": "엄마도 밥 좀 먹자ㅠㅠ",
          "original_form": "엄마도 밥 좀 먹자ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042757.1.18",
          "form": "진짜 화이팅이에여",
          "original_form": "진짜 화이팅이에여",
          "speaker_id": "2",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042757.1.19",
          "form": "계란간장밥 드셨나영",
          "original_form": "계란간장밥 드셨나영",
          "speaker_id": "1",
          "time": "20210907 12:39"
        },
        {
          "id": "MDRW2100042757.1.20",
          "form": "네 이제 다 먹었어요",
          "original_form": "네 이제 다 먹었어요",
          "speaker_id": "2",
          "time": "20210907 12:39"
        },
        {
          "id": "MDRW2100042757.1.21",
          "form": "ㅠㅠ 후식도 드셔야져 ㅎㅎ",
          "original_form": "ㅠㅠ 후식도 드셔야져 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 12:39"
        }
