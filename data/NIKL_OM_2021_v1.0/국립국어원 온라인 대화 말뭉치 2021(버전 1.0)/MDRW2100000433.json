      "utterance": [
        {
          "id": "MDRW2100000433.1.1",
          "form": "올림픽 봤음?",
          "original_form": "올림픽 봤음?",
          "speaker_id": "1",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000433.1.2",
          "form": "봤어 재밌드라",
          "original_form": "봤어 재밌드라",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000433.1.3",
          "form": "나는 배구가 좋더라",
          "original_form": "나는 배구가 좋더라",
          "speaker_id": "1",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000433.1.4",
          "form": "김연경 넘조아",
          "original_form": "김연경 넘조아",
          "speaker_id": "2",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000433.1.5",
          "form": "연경신 사랑해",
          "original_form": "연경신 사랑해",
          "speaker_id": "1",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000433.1.6",
          "form": "배구 보러가고싶다",
          "original_form": "배구 보러가고싶다",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000433.1.7",
          "form": "맞아 가서 보고싶다",
          "original_form": "맞아 가서 보고싶다",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000433.1.8",
          "form": "심심하면 야구나 봐",
          "original_form": "심심하면 야구나 봐",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000433.1.9",
          "form": "중계 언제하지?",
          "original_form": "중계 언제하지?",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000433.1.10",
          "form": "기다려 찾아보고 올게",
          "original_form": "기다려 찾아보고 올게",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000433.1.11",
          "form": "찾았어?",
          "original_form": "찾았어?",
          "speaker_id": "1",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000433.1.12",
          "form": "6시30분 중계 앱깔고 보자",
          "original_form": "6시30분 중계 앱깔고 보자",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000433.1.13",
          "form": "나는 기아타이거즈",
          "original_form": "나는 기아타이거즈",
          "speaker_id": "1",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000433.1.14",
          "form": "한다 앱깔고 경기보자",
          "original_form": "한다 앱깔고 경기보자",
          "speaker_id": "2",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000433.1.15",
          "form": "확실히 ㅊ가 출루하니까 뒤에  득점으로 이어지더라\n진짜 얘가 발도 빠르고 해서 출루하는게 중요함...제발 정신좀 차리고 잘 좀 해라...",
          "original_form": "확실히 ㅊ가 출루하니까 뒤에  득점으로 이어지더라\n진짜 얘가 발도 빠르고 해서 출루하는게 중요함...제발 정신좀 차리고 잘 좀 해라...",
          "speaker_id": "2",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100000433.1.16",
          "form": "출루하니까 득점이 되잖아",
          "original_form": "출루하니까 득점이 되잖아",
          "speaker_id": "1",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100000433.1.17",
          "form": "그러고보니 필승조 이틀연속 썼네.... 내일까지 나오면 3연투......",
          "original_form": "그러고보니 필승조 이틀연속 썼네.... 내일까지 나오면 3연투......",
          "speaker_id": "2",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100000433.1.18",
          "form": "내일 이의리 득점지원 10점 받게해주셔요..",
          "original_form": "내일 이의리 득점지원 10점 받게해주셔요..",
          "speaker_id": "1",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100000433.1.19",
          "form": "퉁 진짜 살아난건가? 일단 타구가 작년 느낌 나고 있는거 같긴 한데...",
          "original_form": "퉁 진짜 살아난건가? 일단 타구가 작년 느낌 나고 있는거 같긴 한데...",
          "speaker_id": "2",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100000433.1.20",
          "form": "터커 내일도 쓰겠지?ㅡㅡ",
          "original_form": "터커 내일도 쓰겠지?ㅡㅡ",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100000433.1.21",
          "form": "아픈 경우 아니면 계속 쓸듯",
