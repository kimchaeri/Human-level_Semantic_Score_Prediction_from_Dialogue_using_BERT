      "utterance": [
        {
          "id": "MDRW2100000372.1.1",
          "form": "안녕하세요;)",
          "original_form": "안녕하세요;)",
          "speaker_id": "1",
          "time": "20210819 15:39"
        },
        {
          "id": "MDRW2100000372.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000372.1.3",
          "form": "다이어트 하고 싶어요 ㅠ",
          "original_form": "다이어트 하고 싶어요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 15:41"
        },
        {
          "id": "MDRW2100000372.1.4",
          "form": "살은 너무 쉽게 찌고 빠질 기미는 안보여요",
          "original_form": "살은 너무 쉽게 찌고 빠질 기미는 안보여요",
          "speaker_id": "2",
          "time": "20210819 15:41"
        },
        {
          "id": "MDRW2100000372.1.5",
          "form": "ㅜㅜ맞아요",
          "original_form": "ㅜㅜ맞아요",
          "speaker_id": "1",
          "time": "20210819 15:41"
        },
        {
          "id": "MDRW2100000372.1.6",
          "form": "다이어트 어떻게하세요?",
          "original_form": "다이어트 어떻게하세요?",
          "speaker_id": "1",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000372.1.7",
          "form": "운동도 한때같고 이제 약을 먹어볼까 싶어요",
          "original_form": "운동도 한때같고 이제 약을 먹어볼까 싶어요",
          "speaker_id": "2",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000372.1.8",
          "form": "게을러졌어요 ㅠ",
          "original_form": "게을러졌어요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000372.1.9",
          "form": "헉 ㅜㅜ 약이 마지막이잖아요",
          "original_form": "헉 ㅜㅜ 약이 마지막이잖아요",
          "speaker_id": "1",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000372.1.10",
          "form": "name1씨는 체중관리하나요?",
          "original_form": "&name1&씨는 체중관리하나요?",
          "speaker_id": "2",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000372.1.11",
          "form": "전 장이안좋아서ㅜㅜ",
          "original_form": "전 장이안좋아서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000372.1.12",
          "form": "날씬하시겠어요",
          "original_form": "날씬하시겠어요",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000372.1.13",
          "form": "흡수안되는 체질이에요 날씬한건아니고요",
          "original_form": "흡수안되는 체질이에요 날씬한건아니고요",
          "speaker_id": "1",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000372.1.14",
          "form": "장이 예민한 사람들은 살이 안붙던데",
          "original_form": "장이 예민한 사람들은 살이 안붙던데",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000372.1.15",
          "form": "어릴때는 그랬는데 요즘은 또 나이들어서그런지",
          "original_form": "어릴때는 그랬는데 요즘은 또 나이들어서그런지",
          "speaker_id": "1",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000372.1.16",
          "form": "전 먹성이 너무 좋고 비움은 안되는 체질이에요",
          "original_form": "전 먹성이 너무 좋고 비움은 안되는 체질이에요",
          "speaker_id": "2",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000372.1.17",
          "form": "엉엉 서럽게",
          "original_form": "엉엉 서럽게",
          "speaker_id": "2",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000372.1.18",
          "form": "많이먹질못하겠더라구요 속이안받아주니까ㅜ",
          "original_form": "많이먹질못하겠더라구요 속이안받아주니까ㅜ",
          "speaker_id": "1",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000372.1.19",
          "form": "ㅜㅜ건강때문에 운동도 해야되는데 영 체력이 안좋아서 매번시작하고 포기하고 그러기도하고요",
          "original_form": "ㅜㅜ건강때문에 운동도 해야되는데 영 체력이 안좋아서 매번시작하고 포기하고 그러기도하고요",
          "speaker_id": "1",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000372.1.20",
          "form": "맞아요 꾸준한 사람이 승리하는것 같아요",
          "original_form": "맞아요 꾸준한 사람이 승리하는것 같아요",
          "speaker_id": "2",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000372.1.21",
          "form": "나이드니까 피부도 탄력잃고 몸도 자꾸 찌고 ㅠ",
          "original_form": "나이드니까 피부도 탄력잃고 몸도 자꾸 찌고 ㅠ",
          "speaker_id": "2",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000372.1.22",
          "form": "맞아요 늙어가는게 점점 보여요ㅜㅜ흑흑",
          "original_form": "맞아요 늙어가는게 점점 보여요ㅜㅜ흑흑",
          "speaker_id": "1",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000372.1.23",
          "form": "세월흘러가는거 붙잡고싶어도 붙잡을수없네요",
          "original_form": "세월흘러가는거 붙잡고싶어도 붙잡을수없네요",
          "speaker_id": "1",
          "time": "20210819 15:47"
        },
        {
          "id": "MDRW2100000372.1.24",
          "form": "그래서 요즘은 비우기 하려고요 옷같은거 가방이랑 모자도",
          "original_form": "그래서 요즘은 비우기 하려고요 옷같은거 가방이랑 모자도",
          "speaker_id": "2",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000372.1.25",
          "form": "그냥 수수하게 입고 가리는거 위주로 ㅎ",
          "original_form": "그냥 수수하게 입고 가리는거 위주로 ㅎ",
          "speaker_id": "2",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000372.1.26",
          "form": "맞아요 저도 옷장정리싹하고 진짜 몇가지만 남겨뒀어요",
          "original_form": "맞아요 저도 옷장정리싹하고 진짜 몇가지만 남겨뒀어요",
          "speaker_id": "1",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000372.1.27",
          "form": "앗...잘못눌렀네요 ㅎㅎ..",
          "original_form": "앗...잘못눌렀네요 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000372.1.28",
          "form": "요즘 머리도 짧게 단발로티고 다녀서 머리에도 큰돈안들이고",
          "original_form": "요즘 머리도 짧게 단발로티고 다녀서 머리에도 큰돈안들이고",
          "speaker_id": "1",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000372.1.29",
          "form": "가볍게 다니고있어요",
          "original_form": "가볍게 다니고있어요",
          "speaker_id": "1",
          "time": "20210819 15:51"
        }
      ]
    }
