      "utterance": [
        {
          "id": "MDRW2100033552.1.1",
          "form": "결국 스벅을 와버렷네요 ㅎ",
          "original_form": "결국 스벅을 와버렷네요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100033552.1.2",
          "form": "쿠키머핀 시식해요 ㅎ",
          "original_form": "쿠키머핀 시식해요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100033552.1.3",
          "form": "쿠키라 바삭하게 맛있네요",
          "original_form": "쿠키라 바삭하게 맛있네요",
          "speaker_id": "1",
          "time": "20210824 16:44"
        },
        {
          "id": "MDRW2100033552.1.4",
          "form": "네 위가 쿠키라 맛나요",
          "original_form": "네 위가 쿠키라 맛나요",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100033552.1.5",
          "form": "속에 블루 벨히쨈",
          "original_form": "속에 블루 벨히쨈",
          "speaker_id": "2",
          "time": "20210824 16:48"
        },
        {
          "id": "MDRW2100033552.1.6",
          "form": "네 데워달라해서 더 맛나요",
          "original_form": "네 데워달라해서 더 맛나요",
          "speaker_id": "1",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100033552.1.7",
          "form": "이런머핀은 또 첨이네요",
          "original_form": "이런머핀은 또 첨이네요",
          "speaker_id": "1",
          "time": "20210824 16:49"
        },
        {
          "id": "MDRW2100033552.1.8",
          "form": "맨날 퍽퍽한 머핀만 먹다보니 ㅋ",
          "original_form": "맨날 퍽퍽한 머핀만 먹다보니 ㅋ",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100033552.1.9",
          "form": "그렇죠 안은 촉촉 위는 바삭 넘맛네요",
          "original_form": "그렇죠 안은 촉촉 위는 바삭 넘맛네요",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033552.1.10",
          "form": "저도 요번에 알았네요 ㅎㅎ",
          "original_form": "저도 요번에 알았네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033552.1.11",
          "form": "뭐 추천메뉴 있나요?",
          "original_form": "뭐 추천메뉴 있나요?",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033552.1.12",
          "form": "오늘 다른것도 시켜 드셨나요?",
          "original_form": "오늘 다른것도 시켜 드셨나요?",
          "speaker_id": "2",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100033552.1.13",
          "form": "저녁메뉴요?",
          "original_form": "저녁메뉴요?",
          "speaker_id": "1",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100033552.1.14",
          "form": "저는 모듬전찾다가 돼지김치찌개보고 같이 시키려구요 ㅋㅋ",
          "original_form": "저는 모듬전찾다가 돼지김치찌개보고 같이 시키려구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100033552.1.15",
          "form": "애들은 카레에 김주려구요",
          "original_form": "애들은 카레에 김주려구요",
          "speaker_id": "1",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100033552.1.16",
          "form": "카레 애들 먹어야할텐데 걱정 두근두근하네요",
          "original_form": "카레 애들 먹어야할텐데 걱정 두근두근하네요",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100033552.1.17",
          "form": "저녁 머드세요?",
          "original_form": "저녁 머드세요?",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100033552.1.18",
          "form": "오 찌개에 요새 세트예요 전하고 어울려요",
          "original_form": "오 찌개에 요새 세트예요 전하고 어울려요",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100033552.1.19",
          "form": "좋은데요 ㅎㅎ",
          "original_form": "좋은데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100033552.1.20",
          "form": "카레에 김 ㅎㅎ아이들 좋은데요",
          "original_form": "카레에 김 ㅎㅎ아이들 좋은데요",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100033552.1.21",
          "form": "전 불고기하려고 사러 나왔어요",
          "original_form": "전 불고기하려고 사러 나왔어요",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100033552.1.22",
          "form": "마트에서 장보고 있어요",
          "original_form": "마트에서 장보고 있어요",
          "speaker_id": "2",
          "time": "20210824 17:42"
        },
        {
          "id": "MDRW2100033552.1.23",
          "form": "전불고기에 된장 찌개요 ㅎㅎ",
          "original_form": "전불고기에 된장 찌개요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:43"
        },
        {
          "id": "MDRW2100033552.1.24",
          "form": "매운것도 포함된 세트라 좋은데요 ㅎㅎ",
          "original_form": "매운것도 포함된 세트라 좋은데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100033552.1.25",
          "form": "전 그때 찌개는 안했지만 담엔 찌개 포함 해야겠네요 ㅎㅎ",
          "original_form": "전 그때 찌개는 안했지만 담엔 찌개 포함 해야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100033552.1.26",
          "form": "불고기에 된장도 좋은데요 ㅎㅎ",
          "original_form": "불고기에 된장도 좋은데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100033552.1.27",
          "form": "왜 남의 떡이 커보이는거같이",
          "original_form": "왜 남의 떡이 커보이는거같이",
          "speaker_id": "1",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100033552.1.28",
          "form": "불고기가 탐나는지요 ㅎ",
          "original_form": "불고기가 탐나는지요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100033552.1.29",
          "form": "그죠 오늘 저녁 메뉴 다 좋네요 ㅎㅎ",
          "original_form": "그죠 오늘 저녁 메뉴 다 좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100033552.1.30",
          "form": "성공적이예요 ㅎ",
          "original_form": "성공적이예요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100033552.1.31",
          "form": "김치찌개는 반덜어서 남겨놔야겠어요",
          "original_form": "김치찌개는 반덜어서 남겨놔야겠어요",
          "speaker_id": "1",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100033552.1.32",
          "form": "주말에 신랑용으로 ㅎㅎ",
          "original_form": "주말에 신랑용으로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100033552.1.33",
          "form": "오 딱이네요",
          "original_form": "오 딱이네요",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100033552.1.34",
          "form": "주말까지 먹고 ㅋㅋ",
          "original_form": "주말까지 먹고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100033552.1.35",
          "form": "최고의 저녁이예요 ㅎㅎ",
          "original_form": "최고의 저녁이예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100033552.1.36",
          "form": "일타 투피라고 ㅋㅋ",
          "original_form": "일타 투피라고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100033552.1.37",
          "form": "이런게 최고죠",
          "original_form": "이런게 최고죠",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100033552.1.38",
          "form": "며칠 먹을수 있는게 사실 좋아요",
          "original_form": "며칠 먹을수 있는게 사실 좋아요",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100033552.1.39",
          "form": "얼마예요?",
          "original_form": "얼마예요?",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100033552.1.40",
          "form": "15000원이요",
          "original_form": "15000원이요",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100033552.1.41",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100033552.1.42",
          "form": "뭐가 그리싸요?",
          "original_form": "뭐가 그리싸요?",
          "speaker_id": "2",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100033552.1.43",
          "form": "반찬집에서 배달시킨거라 ㅋ",
          "original_form": "반찬집에서 배달시킨거라 ㅋ",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100033552.1.44",
          "form": "오 저도 담에 그래야겠네요",
          "original_form": "오 저도 담에 그래야겠네요",
          "speaker_id": "2",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100033552.1.45",
          "form": "여긴 모듬전으로 검색하니 소담푸드",
          "original_form": "여긴 모듬전으로 검색하니 소담푸드",
          "speaker_id": "1",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100033552.1.46",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100033552.1.47",
          "form": "반찬집이 젤위에뜨더라구요",
          "original_form": "반찬집이 젤위에뜨더라구요",
          "speaker_id": "1",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100033552.1.48",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100033552.1.49",
          "form": "지역따라 틀리네요",
          "original_form": "지역따라 틀리네요",
          "speaker_id": "2",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100033552.1.50",
          "form": "그런가봐요",
