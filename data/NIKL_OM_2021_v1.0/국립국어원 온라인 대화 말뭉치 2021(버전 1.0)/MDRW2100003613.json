        {
          "id": "MDRW2100003613.1.1",
          "form": "좋아하는 음식 있어요?",
          "original_form": "좋아하는 음식 있어요?",
          "speaker_id": "1",
          "time": "20210821 08:32"
        },
        {
          "id": "MDRW2100003613.1.2",
          "form": "아니면 지금 가장 먹고싶은 음식이라던가",
          "original_form": "아니면 지금 가장 먹고싶은 음식이라던가",
          "speaker_id": "1",
          "time": "20210821 08:33"
        },
        {
          "id": "MDRW2100003613.1.3",
          "form": "김치부침개",
          "original_form": "김치부침개",
          "speaker_id": "2",
          "time": "20210821 08:33"
        },
        {
          "id": "MDRW2100003613.1.4",
          "form": "감자전",
          "original_form": "감자전",
          "speaker_id": "2",
          "time": "20210821 08:33"
        },
        {
          "id": "MDRW2100003613.1.5",
          "form": "비오니까?",
          "original_form": "비오니까?",
          "speaker_id": "1",
          "time": "20210821 08:33"
        },
        {
          "id": "MDRW2100003613.1.6",
          "form": "땡기네요",
