        },
        {
          "id": "MDRW2100003613.1.7",
          "form": "부침개 맛있지",
          "original_form": "부침개 맛있지",
          "speaker_id": "1",
          "time": "20210821 08:33"
        },
        {
          "id": "MDRW2100003613.1.8",
          "form": "가장 좋아하는 부침개는 뭐에요 김치전?",
          "original_form": "가장 좋아하는 부침개는 뭐에요 김치전?",
          "speaker_id": "1",
          "time": "20210821 08:33"
        },
        {
          "id": "MDRW2100003613.1.9",
          "form": "저는 부추전 좋아해요",
          "original_form": "저는 부추전 좋아해요",
          "speaker_id": "1",
          "time": "20210821 08:34"
        },
        {
          "id": "MDRW2100003613.1.10",
          "form": "오 부추전",
          "original_form": "오 부추전",
          "speaker_id": "2",
          "time": "20210821 08:34"
        },
        {
          "id": "MDRW2100003613.1.11",
          "form": "전 파전 좋아하는데",
          "original_form": "전 파전 좋아하는데",
          "speaker_id": "2",
          "time": "20210821 08:34"
        },
        {
          "id": "MDRW2100003613.1.12",
          "form": "오 파전도 좋아요",
