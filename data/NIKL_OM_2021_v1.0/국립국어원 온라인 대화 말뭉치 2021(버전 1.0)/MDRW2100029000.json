      "utterance": [
        {
          "id": "MDRW2100029000.1.1",
          "form": "언닌 종교 가질 생각 있어?",
          "original_form": "언닌 종교 가질 생각 있어?",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100029000.1.2",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100029000.1.3",
          "form": "난 없어 ㅎㅎ",
          "original_form": "난 없어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100029000.1.4",
          "form": "평생 무교로 살거야",
          "original_form": "평생 무교로 살거야",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100029000.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100029000.1.6",
          "form": "그렇구나!!",
          "original_form": "그렇구나!!",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100029000.1.7",
          "form": "나는 불교가 좀 관심이 가",
          "original_form": "나는 불교가 좀 관심이 가",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100029000.1.8",
          "form": "불교인 애들 보면",
          "original_form": "불교인 애들 보면",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100029000.1.9",
          "form": "진짜 뭔가 깨달은거같더라구",
          "original_form": "진짜 뭔가 깨달은거같더라구",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.10",
          "form": "글쿤 ㅋㅋ",
          "original_form": "글쿤 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.11",
          "form": "흥미롭다",
          "original_form": "흥미롭다",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.12",
          "form": "같이 대화하면 마음이 편안해져..",
          "original_form": "같이 대화하면 마음이 편안해져..",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.13",
          "form": "천주교도 관심 잇어",
          "original_form": "천주교도 관심 잇어",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.14",
          "form": "천주교?!?!",
          "original_form": "천주교?!?!",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.15",
          "form": "그거 알아?",
          "original_form": "그거 알아?",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.16",
          "form": "나두 !!!",
          "original_form": "나두 !!!",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.17",
          "form": "뭐?",
          "original_form": "뭐?",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.18",
          "form": "천주교 성당은",
          "original_form": "천주교 성당은",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.19",
          "form": "웅!",
          "original_form": "웅!",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.20",
          "form": "한 동네에 하나만 세울 수 있게 되어잇대",
          "original_form": "한 동네에 하나만 세울 수 있게 되어잇대",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.21",
          "form": "헐!!",
          "original_form": "헐!!",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.22",
          "form": "왜지!!",
          "original_form": "왜지!!",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.23",
          "form": "그래서 교회에 비해 성당이 적은가봐",
          "original_form": "그래서 교회에 비해 성당이 적은가봐",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.24",
          "form": "교회는 엄청 많잖아!",
          "original_form": "교회는 엄청 많잖아!",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.25",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.26",
          "form": "신기하다..",
          "original_form": "신기하다..",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.27",
          "form": "물론 사람수가 적기도 하겠지만..",
          "original_form": "물론 사람수가 적기도 하겠지만..",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.28",
          "form": "하긴 난 태어나거 성당을 본적이",
          "original_form": "하긴 난 태어나거 성당을 본적이",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100029000.1.29",
          "form": "한번도없어.",
          "original_form": "한번도없어.",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100029000.1.30",
          "form": "흠..",
          "original_form": "흠..",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100029000.1.31",
          "form": "너가 놓친걸수도 잇어",
          "original_form": "너가 놓친걸수도 잇어",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100029000.1.32",
          "form": "교회랑 비슷하게 생기지 않았나?",
          "original_form": "교회랑 비슷하게 생기지 않았나?",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100029000.1.33",
          "form": "그럴수도있징",
          "original_form": "그럴수도있징",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100029000.1.34",
          "form": "흠..그런가?",
          "original_form": "흠..그런가?",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100029000.1.35",
          "form": "본적이없어서 모르겠는데?",
          "original_form": "본적이없어서 모르겠는데?",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100029000.1.36",
          "form": "ㅋㅋㅋ그럴수있지",
          "original_form": "ㅋㅋㅋ그럴수있지",
          "speaker_id": "2",
          "time": "20210902 23:18"
        }
      ]
    }
