      "utterance": [
        {
          "id": "MDRW2100021569.1.1",
          "form": "한식 좋아해요?",
          "original_form": "한식 좋아해요?",
          "speaker_id": "2",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100021569.1.2",
          "form": "네 한식 젤 좋아해요!",
          "original_form": "네 한식 젤 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021569.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021569.1.4",
          "form": "저 한식 좋아해요 ㅜㅜ 너무 맛있어요",
          "original_form": "저 한식 좋아해요 ㅜㅜ 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100021569.1.5",
          "form": "한식 중에 어떤 음식들 좋아해요?",
          "original_form": "한식 중에 어떤 음식들 좋아해요?",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100021569.1.6",
          "form": "진짜 가리지 않고 다 좋아하는데 굳이 따지면 찌개종류 좋아하는 것 같아요!",
          "original_form": "진짜 가리지 않고 다 좋아하는데 굳이 따지면 찌개종류 좋아하는 것 같아요!",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021569.1.7",
          "form": "한식 어떤 음식 주로 드시나요",
          "original_form": "한식 어떤 음식 주로 드시나요",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021569.1.8",
          "form": "앗 저도 찌개요~",
          "original_form": "앗 저도 찌개요~",
          "speaker_id": "2",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021569.1.9",
          "form": "고기 들어있는 김치찌개 최애에요 ㅎㅎ",
          "original_form": "고기 들어있는 김치찌개 최애에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021569.1.10",
          "form": "여기에 계란말이나 계란찜 필수!",
          "original_form": "여기에 계란말이나 계란찜 필수!",
          "speaker_id": "2",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021569.1.11",
          "form": "오오 그쵸ㅋㅋ 돼지고기김치찌개에 계란말이 완전 밥 두공기 먹을 수 있는 조합이에요 ㅠㅠ",
          "original_form": "오오 그쵸ㅋㅋ 돼지고기김치찌개에 계란말이 완전 밥 두공기 먹을 수 있는 조합이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021569.1.12",
          "form": "ㅋㅋㅋ 순두부찌개도 진짜 맛있는데 좋아해요?",
          "original_form": "ㅋㅋㅋ 순두부찌개도 진짜 맛있는데 좋아해요?",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100021569.1.13",
          "form": "해물순두부찌개 캬",
          "original_form": "해물순두부찌개 캬",
          "speaker_id": "2",
          "time": "20210828 23:19"
        },
        {
          "id": "MDRW2100021569.1.14",
          "form": "진짜 좋아하죠.. 칼칼하니 바지락이나 새우 넣고 끓여서 먹으면 .. 죽음이에요..",
          "original_form": "진짜 좋아하죠.. 칼칼하니 바지락이나 새우 넣고 끓여서 먹으면 .. 죽음이에요..",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100021569.1.15",
          "form": "맞아요 ㅜㅜ 거기에 계란 꼭 있어야함",
          "original_form": "맞아요 ㅜㅜ 거기에 계란 꼭 있어야함",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021569.1.16",
          "form": "된장찌개도 두부랑 차돌박이 필수!!",
          "original_form": "된장찌개도 두부랑 차돌박이 필수!!",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021569.1.17",
          "form": "맛있죠맛있죠.. 된장찌개는 두부 많이 들어가야 최고예요..",
          "original_form": "맛있죠맛있죠.. 된장찌개는 두부 많이 들어가야 최고예요..",
          "speaker_id": "1",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021569.1.18",
          "form": "ㅎㅎㅎ두부 하니까 순두부찌개도 생각나네요!",
          "original_form": "ㅎㅎㅎ두부 하니까 순두부찌개도 생각나네요!",
          "speaker_id": "2",
          "time": "20210829 09:52"
        },
        {
          "id": "MDRW2100021569.1.19",
          "form": "해물순두부찌개 칼칼하니 좋아요 ㅜ 어때요?",
          "original_form": "해물순두부찌개 칼칼하니 좋아요 ㅜ 어때요?",
          "speaker_id": "2",
          "time": "20210829 09:52"
        },
        {
          "id": "MDRW2100021569.1.20",
          "form": "네네 ..순두부맛있죠 ㅜㅜ 저는 그 하얀순두부찌개더 좋더라구요",
          "original_form": "네네 ..순두부맛있죠 ㅜㅜ 저는 그 하얀순두부찌개더 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 13:26"
        },
        {
          "id": "MDRW2100021569.1.21",
          "form": "그런 것도 있어요?",
          "original_form": "그런 것도 있어요?",
          "speaker_id": "2",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100021569.1.22",
          "form": "그건 뭐에요?",
          "original_form": "그건 뭐에요?",
          "speaker_id": "2",
          "time": "20210829 13:52"
        }
      ]
    }
