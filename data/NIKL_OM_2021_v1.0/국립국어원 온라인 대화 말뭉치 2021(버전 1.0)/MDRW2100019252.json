      "utterance": [
        {
          "id": "MDRW2100019252.1.1",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019252.1.2",
          "form": "나도 미칠거같음",
          "original_form": "나도 미칠거같음",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019252.1.3",
          "form": "아시아나 마일리지쓰고싶다",
          "original_form": "아시아나 마일리지쓰고싶다",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019252.1.4",
          "form": "어디가고십ㅇ븜?",
          "original_form": "어디가고십ㅇ븜?",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019252.1.5",
          "form": "태국이랑 대만 가거싶어",
          "original_form": "태국이랑 대만 가거싶어",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019252.1.6",
          "form": "대만 ㅠㅠㅠㅠㅜ",
          "original_form": "대만 ㅠㅠㅠㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019252.1.7",
          "form": "태국은 너무 더워",
          "original_form": "태국은 너무 더워",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019252.1.8",
          "form": "대만나도",
          "original_form": "대만나도",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019252.1.9",
          "form": "대만 진짜 좋았는대 ㅜ",
          "original_form": "대만 진짜 좋았는대 ㅜ",
          "speaker_id": "1",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019252.1.10",
          "form": "음식이 맛잇어ㅋㅋ",
          "original_form": "음식이 맛잇어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019252.1.11",
          "form": "분위기도 좋고",
          "original_form": "분위기도 좋고",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019252.1.12",
          "form": "진짜 ㅜㅜ 나 대만에서 산 손수건 완전 아직도 잘쓰자나",
          "original_form": "진짜 ㅜㅜ 나 대만에서 산 손수건 완전 아직도 잘쓰자나",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019252.1.13",
          "form": "손수건 삿엇어?",
          "original_form": "손수건 삿엇어?",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019252.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019252.1.15",
          "form": "도시락 포장 손수건",
          "original_form": "도시락 포장 손수건",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019252.1.16",
          "form": "야ㅋㅋㄱ 그건 도시락밥을 먹은거지..",
          "original_form": "야ㅋㅋㄱ 그건 도시락밥을 먹은거지..",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019252.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ광산이었나..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ광산이었나..",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019252.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019252.1.19",
          "form": "머야ㅋㅋㅋㅋㅋ 혼자 손수건삿는줄",
          "original_form": "머야ㅋㅋㅋㅋㅋ 혼자 손수건삿는줄",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019252.1.20",
          "form": "그것도 손수건 재질이더만..",
          "original_form": "그것도 손수건 재질이더만..",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019252.1.21",
          "form": "대체 무슨돈으로 삿지 이생각ㅋㅋㅋ",
