      "utterance": [
        {
          "id": "MDRW2100010666.1.1",
          "form": "게임을 좋아한대서",
          "original_form": "게임을 좋아한대서",
          "speaker_id": "1",
          "time": "20210823 20:28"
        },
        {
          "id": "MDRW2100010666.1.2",
