      "utterance": [
        {
          "id": "MDRW2100026127.1.1",
          "form": "추석앞두고 쇼핑하세요?",
          "original_form": "추석앞두고 쇼핑하세요?",
          "speaker_id": "1",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026127.1.2",
          "form": "추석 직전에는 오히려 물가가 올라서",
          "original_form": "추석 직전에는 오히려 물가가 올라서",
          "speaker_id": "2",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100026127.1.3",
          "form": "뭘 안사고",
          "original_form": "뭘 안사고",
          "speaker_id": "2",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100026127.1.4",
          "form": "음식물가는 많이 올라도",
          "original_form": "음식물가는 많이 올라도",
          "speaker_id": "1",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026127.1.5",
          "form": "쇼핑몰보니 할인많이행ᆢ",
          "original_form": "쇼핑몰보니 할인많이행ᆢ",
          "speaker_id": "1",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026127.1.6",
          "form": "대목이라서^^",
          "original_form": "대목이라서^^",
          "speaker_id": "1",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026127.1.7",
          "form": "음 그러고보니 설에는 득템했던기억이나요",
          "original_form": "음 그러고보니 설에는 득템했던기억이나요",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026127.1.8",
          "form": "그때 옥션이랑 지마켓에서 세일해서",
          "original_form": "그때 옥션이랑 지마켓에서 세일해서",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026127.1.9",
          "form": "오늘부터 빅세일해요",
          "original_form": "오늘부터 빅세일해요",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026127.1.10",
          "form": "옥션이요ㅎ",
          "original_form": "옥션이요ㅎ",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026127.1.11",
          "form": "저 벌써 후라이팬 샀어요",
          "original_form": "저 벌써 후라이팬 샀어요",
          "speaker_id": "1",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026127.1.12",
          "form": "오 뭐 필요한거있나 생각해봐야겠어요",
          "original_form": "오 뭐 필요한거있나 생각해봐야겠어요",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026127.1.13",
          "form": "그때 설날에는 소스티커 붙은건 전부 20프로인가 그랬는데",
          "original_form": "그때 설날에는 소스티커 붙은건 전부 20프로인가 그랬는데",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026127.1.14",
          "form": "이번에도 똑같아요",
          "original_form": "이번에도 똑같아요",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026127.1.15",
          "form": "근데 할인해도 다른데보다 비싼것도 많대요",
          "original_form": "근데 할인해도 다른데보다 비싼것도 많대요",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026127.1.16",
          "form": "얌체같이 슬쩍 가격올리기도하니",
          "original_form": "얌체같이 슬쩍 가격올리기도하니",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026127.1.17",
          "form": "초반에 사시는게 나아요",
          "original_form": "초반에 사시는게 나아요",
          "speaker_id": "1",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026127.1.18",
          "form": "오 글쿤요 지금 옥션 켰네요 ㅋㅋ",
          "original_form": "오 글쿤요 지금 옥션 켰네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100026127.1.19",
          "form": "꿀정보 감사합니다",
          "original_form": "꿀정보 감사합니다",
          "speaker_id": "2",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100026127.1.20",
          "form": "옥션은 배송비쿠폰 따로 적용돼서 지마켓보다 저렴할수도 있어요ㅎ",
          "original_form": "옥션은 배송비쿠폰 따로 적용돼서 지마켓보다 저렴할수도 있어요ㅎ",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026127.1.21",
          "form": "지마켓은 할인쿠폰과 배송쿠폰중 택1",
          "original_form": "지마켓은 할인쿠폰과 배송쿠폰중 택1",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026127.1.22",
          "form": "스마일클럽 무배 쿠폰이요",
          "original_form": "스마일클럽 무배 쿠폰이요",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026127.1.23",
          "form": "오 대박이네요 ㅋㅋ",
          "original_form": "오 대박이네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100026127.1.24",
          "form": "지금 막 두유보고있었어요 ㅋㅋㅋ",
          "original_form": "지금 막 두유보고있었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100026127.1.25",
          "form": "두유가 여성에게 안좋단얘기있어서 혹시 근종이나 그런거있음 안좋아요ㅎ",
          "original_form": "두유가 여성에게 안좋단얘기있어서 혹시 근종이나 그런거있음 안좋아요ㅎ",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100026127.1.26",
          "form": "아 정말요? 이런 그런거면 치우고",
          "original_form": "아 정말요? 이런 그런거면 치우고",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026127.1.27",
          "form": "아 저희 얼마전에 선풍기샀는데",
          "original_form": "아 저희 얼마전에 선풍기샀는데",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026127.1.28",
          "form": "검색안하려고요ㅜ",
          "original_form": "검색안하려고요ㅜ",
          "speaker_id": "2",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026127.1.29",
          "form": "ㅋㅋㅋ 잘쓰고있으심 됐져",
          "original_form": "ㅋㅋㅋ 잘쓰고있으심 됐져",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100026127.1.30",
          "form": "머 맛난거 택배 받을때가 젤 행복해요",
          "original_form": "머 맛난거 택배 받을때가 젤 행복해요",
          "speaker_id": "1",
          "time": "20210901 20:55"
        }
      ]
