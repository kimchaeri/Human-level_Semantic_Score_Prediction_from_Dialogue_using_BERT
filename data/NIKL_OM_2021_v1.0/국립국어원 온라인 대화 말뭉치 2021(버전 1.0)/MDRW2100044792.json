      "utterance": [
        {
          "id": "MDRW2100044792.1.1",
          "form": "그때는 말 할수가 없었대",
          "original_form": "그때는 말 할수가 없었대",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044792.1.2",
          "form": "나혼산도 진짜 작위적일때 많더라",
          "original_form": "나혼산도 진짜 작위적일때 많더라",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044792.1.3",
          "form": "계약인가",
          "original_form": "계약인가",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044792.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044792.1.5",
          "form": "그건모르겠네ㅜㅜ",
          "original_form": "그건모르겠네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044792.1.6",
          "form": "그 웹툰작가",
          "original_form": "그 웹툰작가",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044792.1.7",
          "form": "진심같아 보이던데",
          "original_form": "진심같아 보이던데",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044792.1.8",
          "form": "이번 사건?",
          "original_form": "이번 사건?",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044792.1.9",
          "form": "걔는 진짜 같아 ㅋㅋ",
          "original_form": "걔는 진짜 같아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044792.1.10",
          "form": "안쓰러워보이긴하더라",
          "original_form": "안쓰러워보이긴하더라",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044792.1.11",
          "form": "근데 너무 욕하더라",
          "original_form": "근데 너무 욕하더라",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044792.1.12",
          "form": "작가가 짜줬을텐데",
          "original_form": "작가가 짜줬을텐데",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044792.1.13",
          "form": "전현무는 열심히 한 죄밖에없고",
          "original_form": "전현무는 열심히 한 죄밖에없고",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044792.1.14",
          "form": "그렇긴한데 한번씩 보면 무시하듯하는게 있긴했어서",
          "original_form": "그렇긴한데 한번씩 보면 무시하듯하는게 있긴했어서",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044792.1.15",
          "form": "다른 출연자들도 작가가 짜줬을텐데",
          "original_form": "다른 출연자들도 작가가 짜줬을텐데",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044792.1.16",
          "form": "그 사람 안쓰럽긴했어",
          "original_form": "그 사람 안쓰럽긴했어",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044792.1.17",
          "form": "다 사이좋지않아?",
          "original_form": "다 사이좋지않아?",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044792.1.18",
          "form": "예능으로 뭐하하는거던데",
          "original_form": "예능으로 뭐하하는거던데",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044792.1.19",
          "form": "전현무는 거기서 매번 욕먹엉 ㅋㅋ",
          "original_form": "전현무는 거기서 매번 욕먹엉 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044792.1.20",
          "form": "긍가 난 한번씩 좀 어?하는부분이 있었어",
          "original_form": "긍가 난 한번씩 좀 어?하는부분이 있었어",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044792.1.21",
          "form": "전현무는 쫌ㅋㅋ",
          "original_form": "전현무는 쫌ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044792.1.22",
          "form": "장난같아ㅋㅋㅋㅋ",
          "original_form": "장난같아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044792.1.23",
          "form": "단체로 하는거봄ㅎㅎ",
          "original_form": "단체로 하는거봄ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044792.1.24",
          "form": "아마 기안은 일반인 느낌이많이나니까 그렇게 느끼나봐",
          "original_form": "아마 기안은 일반인 느낌이많이나니까 그렇게 느끼나봐",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044792.1.25",
          "form": "애가 공황있다고 알고 봐서 긍가ㅎ",
          "original_form": "애가 공황있다고 알고 봐서 긍가ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044792.1.26",
          "form": "너무 헌실같기도 하고\n저렇게 더럽나싶기도하고\n그사람은 보면 좀 감정 동요가 생기게돼",
          "original_form": "너무 헌실같기도 하고\n저렇게 더럽나싶기도하고\n그사람은 보면 좀 감정 동요가 생기게돼",
          "speaker_id": "1",
          "time": "20210907 23:51"
        }
      ]
    }
