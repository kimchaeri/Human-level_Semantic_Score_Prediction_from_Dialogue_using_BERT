      "utterance": [
        {
          "id": "MDRW2100005980.1.1",
          "form": "피곤한가 보다",
          "original_form": "피곤한가 보다",
          "speaker_id": "2",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005980.1.2",
          "form": "오늘 뭐 암것두 안했는데 피곤한가보네 ㅋㅋ",
          "original_form": "오늘 뭐 암것두 안했는데 피곤한가보네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005980.1.3",
          "form": "아까 산책 다녀와서도 그렇지",
          "original_form": "아까 산책 다녀와서도 그렇지",
          "speaker_id": "2",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005980.1.4",
          "form": "아 맞네 ㅎㅎ",
          "original_form": "아 맞네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005980.1.5",
          "form": "이젠 나이 먹어서 오래 운동 못하나봐",
          "original_form": "이젠 나이 먹어서 오래 운동 못하나봐",
          "speaker_id": "2",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005980.1.6",
          "form": "그런듯 ㅠㅠ 안타깝",
          "original_form": "그런듯 ㅠㅠ 안타깝",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005980.1.7",
          "form": "잘 때는 잘 자게 냅둬라",
          "original_form": "잘 때는 잘 자게 냅둬라",
          "speaker_id": "2",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005980.1.8",
          "form": "웅웅 가만히 둔다 ㅋㅋ",
          "original_form": "웅웅 가만히 둔다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005980.1.9",
          "form": "그래 잘했다",
          "original_form": "그래 잘했다",
          "speaker_id": "2",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005980.1.10",
          "form": "근데 이따 안자겠다 이러다가",
          "original_form": "근데 이따 안자겠다 이러다가",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005980.1.11",
          "form": "잘 잔다 밤에도",
          "original_form": "잘 잔다 밤에도",
          "speaker_id": "2",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005980.1.12",
