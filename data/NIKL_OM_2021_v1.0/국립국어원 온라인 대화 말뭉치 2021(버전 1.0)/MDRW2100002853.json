      "utterance": [
        {
          "id": "MDRW2100002853.1.1",
          "form": "가족이랑 같이 사세요?",
          "original_form": "가족이랑 같이 사세요?",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002853.1.2",
          "form": "저는 하숙하고 있어서",
          "original_form": "저는 하숙하고 있어서",
          "speaker_id": "2",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002853.1.3",
          "form": "가족 아니고 집주인이랑 같이 살고 있어요",
          "original_form": "가족 아니고 집주인이랑 같이 살고 있어요",
          "speaker_id": "2",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002853.1.4",
          "form": "학생이신가요? 저도 학생때 룸메랑 같이 하숙한적 있었네요 ㅎㅎ",
          "original_form": "학생이신가요? 저도 학생때 룸메랑 같이 하숙한적 있었네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002853.1.5",
          "form": "네 학생이에요",
          "original_form": "네 학생이에요",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002853.1.6",
          "form": "하숙 해보신분 처음 봐요",
          "original_form": "하숙 해보신분 처음 봐요",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002853.1.7",
          "form": "하숙 나름 좋은거같아요~",
          "original_form": "하숙 나름 좋은거같아요~",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002853.1.8",
          "form": "전 남동생이랑 같이 사는데",
          "original_form": "전 남동생이랑 같이 사는데",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002853.1.9",
          "form": "마치 제가 하숙집 주인 같은 기분..ㅎ",
          "original_form": "마치 제가 하숙집 주인 같은 기분..ㅎ",
          "speaker_id": "1",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002853.1.10",
          "form": "앗...!",
          "original_form": "앗...!",
          "speaker_id": "2",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002853.1.11",
          "form": "동생이랑 살면 그런 느낌이 될 수 있겠네요",
          "original_form": "동생이랑 살면 그런 느낌이 될 수 있겠네요",
          "speaker_id": "2",
          "time": "20210820 16:47"
        },
        {
          "id": "MDRW2100002853.1.12",
          "form": "네.. 동생은 무료 하숙생 ㅠ 부러워요..^^",
          "original_form": "네.. 동생은 무료 하숙생 ㅠ 부러워요..^^",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002853.1.13",
          "form": "제가 집안일을 좋아하는게 그나마 다행이죠..",
          "original_form": "제가 집안일을 좋아하는게 그나마 다행이죠..",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002853.1.14",
          "form": "청소가 스트레스 해소법이거든요",
          "original_form": "청소가 스트레스 해소법이거든요",
          "speaker_id": "1",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002853.1.15",
          "form": "우와 진짜 부럽네요!",
          "original_form": "우와 진짜 부럽네요!",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002853.1.16",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002853.1.17",
          "form": "너무 건전한 해소법이에요",
          "original_form": "너무 건전한 해소법이에요",
          "speaker_id": "2",
          "time": "20210820 16:49"
        },
        {
          "id": "MDRW2100002853.1.18",
          "form": "짱이다",
          "original_form": "짱이다",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002853.1.19",
          "form": "진짜 세상이 다 싫고 미울땐 욕실청소가 최고예요",
          "original_form": "진짜 세상이 다 싫고 미울땐 욕실청소가 최고예요",
          "speaker_id": "1",
          "time": "20210820 16:51"
        },
        {
          "id": "MDRW2100002853.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ수도요금이 좀 많이 나오는게 단점..ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ수도요금이 좀 많이 나오는게 단점..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002853.1.21",
          "form": "우와..! 청소만 하면 세상 우울해지는 저랑 반대에요",
          "original_form": "우와..! 청소만 하면 세상 우울해지는 저랑 반대에요",
          "speaker_id": "2",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002853.1.22",
          "form": "남동생분이 절해야겠는데요",
          "original_form": "남동생분이 절해야겠는데요",
          "speaker_id": "2",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002853.1.23",
          "form": "청소 억지로하려고하면 힘들죠ㅠㅎㅎㅎ",
          "original_form": "청소 억지로하려고하면 힘들죠ㅠㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 19:36"
        },
        {
          "id": "MDRW2100002853.1.24",
          "form": "남동생은 혼자 살아봐야 정신을 차릴거같아요 ..ㅎ",
          "original_form": "남동생은 혼자 살아봐야 정신을 차릴거같아요 ..ㅎ",
          "speaker_id": "1",
          "time": "20210820 19:36"
        },
        {
          "id": "MDRW2100002853.1.25",
          "form": "진짜 인정이요",
          "original_form": "진짜 인정이요",
          "speaker_id": "2",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100002853.1.26",
          "form": "그래도 동생이랑 같이 살면 좋은것도 있을거 같아요",
          "original_form": "그래도 동생이랑 같이 살면 좋은것도 있을거 같아요",
          "speaker_id": "2",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100002853.1.27",
          "form": "혼자 사는 것 보단 덜 외롭고 덜 심심하고",
          "original_form": "혼자 사는 것 보단 덜 외롭고 덜 심심하고",
          "speaker_id": "1",
          "time": "20210820 23:17"
        },
        {
          "id": "MDRW2100002853.1.28",
          "form": "재밌긴 해요 ㅋㅋ 부모님이랑 같이 살때랑은 또 다른 느김..",
          "original_form": "재밌긴 해요 ㅋㅋ 부모님이랑 같이 살때랑은 또 다른 느김..",
          "speaker_id": "1",
