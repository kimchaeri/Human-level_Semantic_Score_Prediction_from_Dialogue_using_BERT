      "utterance": [
        {
          "id": "MDRW2100033840.1.1",
          "form": "부먹은정말",
          "original_form": "부먹은정말",
          "speaker_id": "1",
          "time": "20210825 12:01"
        },
        {
          "id": "MDRW2100033840.1.2",
          "form": "용서할수없어요!",
          "original_form": "용서할수없어요!",
          "speaker_id": "1",
          "time": "20210825 12:01"
        },
        {
          "id": "MDRW2100033840.1.3",
          "form": "눅눅한 걸 안 좋아하시나봐요ㅋㅋㅋ",
          "original_form": "눅눅한 걸 안 좋아하시나봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:02"
        },
        {
          "id": "MDRW2100033840.1.4",
          "form": "ㅋㅋㅋ 부먹은 진짜 탕수육먹으면 안돼요",
          "original_form": "ㅋㅋㅋ 부먹은 진짜 탕수육먹으면 안돼요",
          "speaker_id": "1",
          "time": "20210825 12:05"
        },
        {
          "id": "MDRW2100033840.1.5",
          "form": "멸치칼국수vs 생생우동?",
          "original_form": "멸치칼국수vs 생생우동?",
          "speaker_id": "1",
          "time": "20210825 12:05"
        },
        {
          "id": "MDRW2100033840.1.6",
          "form": "아~ 둘 다 맛있는데~",
          "original_form": "아~ 둘 다 맛있는데~",
          "speaker_id": "2",
          "time": "20210825 12:05"
        },
        {
          "id": "MDRW2100033840.1.7",
          "form": "저는 칼국수 쪽이요",
          "original_form": "저는 칼국수 쪽이요",
          "speaker_id": "2",
          "time": "20210825 12:05"
        },
        {
          "id": "MDRW2100033840.1.8",
          "form": "라면 자주 드세요?",
          "original_form": "라면 자주 드세요?",
          "speaker_id": "2",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100033840.1.9",
          "form": "ㅋㅋㅋ저도 칼국수요 가끔 먹는답니다",
          "original_form": "ㅋㅋㅋ저도 칼국수요 가끔 먹는답니다",
          "speaker_id": "1",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100033840.1.10",
          "form": "커피는 좋아하세요?\r\n아메리카노vs푸라푸치노?",
          "original_form": "커피는 좋아하세요?\r\n아메리카노vs푸라푸치노?",
          "speaker_id": "1",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100033840.1.11",
          "form": "아메리카노요!",
          "original_form": "아메리카노요!",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100033840.1.12",
          "form": "아아인가요 뜨아인가요?",
          "original_form": "아아인가요 뜨아인가요?",
          "speaker_id": "2",
          "time": "20210825 12:08"
        },
        {
          "id": "MDRW2100033840.1.13",
          "form": "아메는 아아죠",
          "original_form": "아메는 아아죠",
          "speaker_id": "1",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100033840.1.14",
          "form": "뜨아가 아메인가요?",
          "original_form": "뜨아가 아메인가요?",
          "speaker_id": "1",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100033840.1.15",
          "form": "뜨거운 아메리카노요!",
          "original_form": "뜨거운 아메리카노요!",
          "speaker_id": "2",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100033840.1.16",
          "form": "아이스가 짱이죠~",
          "original_form": "아이스가 짱이죠~",
          "speaker_id": "2",
          "time": "20210825 12:12"
        },
        {
          "id": "MDRW2100033840.1.17",
          "form": "아 제의미는",
          "original_form": "아 제의미는",
          "speaker_id": "1",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100033840.1.18",
          "form": "뜨아도 아메로취급되나",
          "original_form": "뜨아도 아메로취급되나",
          "speaker_id": "1",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100033840.1.19",
          "form": "이런거였죠ㅋㅋㅋㅋ",
          "original_form": "이런거였죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100033840.1.20",
          "form": "얼죽아입니다",
          "original_form": "얼죽아입니다",
          "speaker_id": "1",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100033840.1.21",
          "form": "맞죠ㅋㅋㅋ",
          "original_form": "맞죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:22"
        },
        {
          "id": "MDRW2100033840.1.22",
          "form": "디저트는 좋아하시나요?",
          "original_form": "디저트는 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 12:23"
        },
        {
          "id": "MDRW2100033840.1.23",
          "form": "좋아하죠ㅋㅋㅋ",
          "original_form": "좋아하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:29"
        },
        {
          "id": "MDRW2100033840.1.24",
          "form": "디저트 뭐좋아하나요?",
          "original_form": "디저트 뭐좋아하나요?",
          "speaker_id": "1",
          "time": "20210825 12:29"
        },
        {
          "id": "MDRW2100033840.1.25",
          "form": "치즈 케이크 좋아해요!",
          "original_form": "치즈 케이크 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100033840.1.26",
          "form": "어떤 디저트 좋아하세요?",
          "original_form": "어떤 디저트 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100033840.1.27",
          "form": "전 다좋아해요ㅋㅋㅋ",
          "original_form": "전 다좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100033840.1.28",
          "form": "티라미스도좋고요",
          "original_form": "티라미스도좋고요",
          "speaker_id": "1",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100033840.1.29",
          "form": "티라미수도 맛있죠~",
          "original_form": "티라미수도 맛있죠~",
