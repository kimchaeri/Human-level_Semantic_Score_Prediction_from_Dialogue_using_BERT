      "utterance": [
        {
          "id": "MDRW2100027861.1.1",
          "form": "전 바닷가요 휴양지나",
          "original_form": "전 바닷가요 휴양지나",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100027861.1.2",
          "form": "음 전 무전여행 해봣어요",
          "original_form": "음 전 무전여행 해봣어요",
          "speaker_id": "1",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100027861.1.3",
          "form": "헉 어디로 해보셨나요",
          "original_form": "헉 어디로 해보셨나요",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027861.1.4",
          "form": "전남 지역요",
          "original_form": "전남 지역요",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027861.1.5",
          "form": "대단하시네요 전 요즘 도시로 가고 싶어요",
          "original_form": "대단하시네요 전 요즘 도시로 가고 싶어요",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027861.1.6",
          "form": "도시여행도 좋죠",
          "original_form": "도시여행도 좋죠",
          "speaker_id": "1",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027861.1.7",
          "form": "지하철 타고 걷고 놀이동산도 가고",
          "original_form": "지하철 타고 걷고 놀이동산도 가고",
          "speaker_id": "2",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027861.1.8",
          "form": "아하 그런식의 여행도 좋죠",
          "original_form": "아하 그런식의 여행도 좋죠",
          "speaker_id": "1",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100027861.1.9",
          "form": "넹 문화생활도 하구 야경도 보구 자유롭게 다니고 싶어요",
          "original_form": "넹 문화생활도 하구 야경도 보구 자유롭게 다니고 싶어요",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027861.1.10",
          "form": "상상만으로도 즐겁네요",
          "original_form": "상상만으로도 즐겁네요",
          "speaker_id": "1",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027861.1.11",
          "form": "넹ㅎ 정말 그런 날이 빨리 왔으면 좋겠어요 여행 다니게",
          "original_form": "넹ㅎ 정말 그런 날이 빨리 왔으면 좋겠어요 여행 다니게",
          "speaker_id": "2",
          "time": "20210902 16:18"
        },
        {
          "id": "MDRW2100027861.1.12",
          "form": "여행 좋아하시나봐요",
          "original_form": "여행 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027861.1.13",
          "form": "그러진않은데 요즘 답답해서여",
          "original_form": "그러진않은데 요즘 답답해서여",
          "speaker_id": "2",
          "time": "20210902 16:22"
