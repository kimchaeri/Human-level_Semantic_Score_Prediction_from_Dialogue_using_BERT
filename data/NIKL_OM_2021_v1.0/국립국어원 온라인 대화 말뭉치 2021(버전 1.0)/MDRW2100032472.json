        },
        {
          "id": "MDRW2100032472.1.2",
          "form": "네 콘서트좋아해요ㅋㅋ",
          "original_form": "네 콘서트좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:06"
        },
        {
          "id": "MDRW2100032472.1.3",
          "form": "가보셨어요?",
          "original_form": "가보셨어요?",
          "speaker_id": "2",
          "time": "20210905 19:06"
        },
        {
          "id": "MDRW2100032472.1.4",
          "form": "네 김동률 콘서트가 기억에 남아요",
          "original_form": "네 김동률 콘서트가 기억에 남아요",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100032472.1.5",
          "form": "오...대박..김동률ㅋ ㅋㅋ",
          "original_form": "오...대박..김동률ㅋ ㅋㅋ",
