      "utterance": [
        {
          "id": "MDRW2100011709.1.1",
          "form": "오 네 카우에 올려보는거도 좋은거같아요",
          "original_form": "오 네 카우에 올려보는거도 좋은거같아요",
          "speaker_id": "2",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011709.1.2",
          "form": "카우가 되게 유용하네요",
          "original_form": "카우가 되게 유용하네요",
          "speaker_id": "1",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011709.1.3",
          "form": "카우가 다른 커뮤니티보다 고민같은걸 진지하게 들어주시는거같아요",
          "original_form": "카우가 다른 커뮤니티보다 고민같은걸 진지하게 들어주시는거같아요",
          "speaker_id": "2",
          "time": "20210824 15:17"
        },
        {
          "id": "MDRW2100011709.1.4",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011709.1.5",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011709.1.6",
          "form": "제가 카우 정보만 얻거나 올리고 활동을 안해서ㅋㅋㅋ",
          "original_form": "제가 카우 정보만 얻거나 올리고 활동을 안해서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011709.1.7",
          "form": "주식이나 코인 카우에서 배우셨어요?",
          "original_form": "주식이나 코인 카우에서 배우셨어요?",
          "speaker_id": "1",
          "time": "20210824 15:18"
        },
        {
          "id": "MDRW2100011709.1.8",
          "form": "저도 활동을 열심히하는편은아니에요ㅋㅋㅋ",
          "original_form": "저도 활동을 열심히하는편은아니에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011709.1.9",
          "form": "네 카우분들이 주식입문서나 유튜브영상 추천해주시는 글보고",
          "original_form": "네 카우분들이 주식입문서나 유튜브영상 추천해주시는 글보고",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011709.1.10",
          "form": "저도 ㅋㅋㅋㅋㅋ 커뮤니티 힘들어요",
          "original_form": "저도 ㅋㅋㅋㅋㅋ 커뮤니티 힘들어요",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011709.1.11",
          "form": "따라읽거나 봤어요",
          "original_form": "따라읽거나 봤어요",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011709.1.12",
          "form": "오 주식입문서 따라읽으셨어요? 대박이다",
          "original_form": "오 주식입문서 따라읽으셨어요? 대박이다",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011709.1.13",
          "form": "커뮤로 공부를 하셨군요",
          "original_form": "커뮤로 공부를 하셨군요",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011709.1.14",
          "form": "사실 책은 두권밖에 안읽었어요...ㅎ",
          "original_form": "사실 책은 두권밖에 안읽었어요...ㅎ",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011709.1.15",
          "form": "앞으로 같이 공부 열심히해봐요",
          "original_form": "앞으로 같이 공부 열심히해봐요",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011709.1.16",
          "form": "두권이나!",
          "original_form": "두권이나!",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011709.1.17",
          "form": "카우를 교과서삼아 같이 열심히해봐요 ㅋㅋㅋ",
          "original_form": "카우를 교과서삼아 같이 열심히해봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011709.1.18",
          "form": "좋아요ㅋㅋㅋㅋ효ㅏ이팅!!",
          "original_form": "좋아요ㅋㅋㅋㅋ효ㅏ이팅!!",
          "speaker_id": "2",
          "time": "20210824 15:25"
        },
        {
          "id": "MDRW2100011709.1.19",
          "form": "커뮤에서 실제로 사람 만나보신 적 있으세요?",
          "original_form": "커뮤에서 실제로 사람 만나보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210824 15:26"
        },
        {
          "id": "MDRW2100011709.1.20",
          "form": "아뇨 아직 한번도없어요..",
          "original_form": "아뇨 아직 한번도없어요..",
