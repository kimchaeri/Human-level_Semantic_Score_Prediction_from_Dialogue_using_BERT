      "utterance": [
        {
          "id": "MDRW2100034912.1.1",
          "form": "프로그램 뭐 보새요?",
          "original_form": "프로그램 뭐 보새요?",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100034912.1.2",
          "form": "저는 tv가 없어서 잘안봐요",
          "original_form": "저는 tv가 없어서 잘안봐요",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100034912.1.3",
          "form": "드라마 어떤거 좋아하셨어요?",
          "original_form": "드라마 어떤거 좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100034912.1.4",
          "form": "저 슬의 열심히보고있어요",
          "original_form": "저 슬의 열심히보고있어요",
          "speaker_id": "2",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100034912.1.5",
          "form": "그전에 응답하라 시리즈도 열심히 봤고요",
          "original_form": "그전에 응답하라 시리즈도 열심히 봤고요",
          "speaker_id": "2",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100034912.1.6",
          "form": "둘다 재밌으면서도 마음이 따땃해져서",
          "original_form": "둘다 재밌으면서도 마음이 따땃해져서",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100034912.1.7",
          "form": "언제봐도 좋은 드라마인거 같아요",
          "original_form": "언제봐도 좋은 드라마인거 같아요",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100034912.1.8",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 07:06"
        },
        {
          "id": "MDRW2100034912.1.9",
          "form": "저는 마음이 따땃해지는것애.. 환장하는것같아요",
          "original_form": "저는 마음이 따땃해지는것애.. 환장하는것같아요",
          "speaker_id": "2",
          "time": "20210829 07:06"
        },
        {
          "id": "MDRW2100034912.1.10",
          "form": "눈물이 많은편 이신가요?",
          "original_form": "눈물이 많은편 이신가요?",
          "speaker_id": "1",
          "time": "20210829 10:35"
        },
        {
          "id": "MDRW2100034912.1.11",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100034912.1.12",
          "form": "저 맨날 울어요",
          "original_form": "저 맨날 울어요",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100034912.1.13",
          "form": "공감능력이 좋으신가보다",
          "original_form": "공감능력이 좋으신가보다",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100034912.1.14",
          "form": "따땃 하신 분 이시군요",
          "original_form": "따땃 하신 분 이시군요",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100034912.1.15",
          "form": "ㅋㅋㅋㅋㅋ그런가여",
          "original_form": "ㅋㅋㅋㅋㅋ그런가여",
          "speaker_id": "2",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100034912.1.16",
          "form": "몰입을 잘해서요",
          "original_form": "몰입을 잘해서요",
          "speaker_id": "2",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100034912.1.17",
          "form": "드라마보면서 욕하는 스타일",
          "original_form": "드라마보면서 욕하는 스타일",
          "speaker_id": "2",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100034912.1.18",
          "form": "안해볼땐 몰랐는데 욕하면서 보니까 더 재밌던데요ㅋㅋㅋㅋ",
          "original_form": "안해볼땐 몰랐는데 욕하면서 보니까 더 재밌던데요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100034912.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ그건맞죠 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그건맞죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100034912.1.20",
          "form": "저는 로스쿨볼때 아쥬..",
          "original_form": "저는 로스쿨볼때 아쥬..",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100034912.1.21",
          "form": "누굴 그렇게 욕하면서 보셨나요ㅋㅋㅋ",
          "original_form": "누굴 그렇게 욕하면서 보셨나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100034912.1.22",
          "form": "슈레기 부자 나오는데 ㅠ 그 둘 엄청 욕하면서 봤어요",
