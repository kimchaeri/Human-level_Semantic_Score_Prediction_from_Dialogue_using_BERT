      "utterance": [
        {
          "id": "MDRW2100014188.1.1",
          "form": "오오 저도욬ㅋㅋㅋ로판 뭐보세용?",
          "original_form": "오오 저도욬ㅋㅋㅋ로판 뭐보세용?",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014188.1.2",
          "form": "네이버랑 카카페에서 봐요!",
          "original_form": "네이버랑 카카페에서 봐요!",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014188.1.3",
          "form": "좀많아서 작품명이ㅋㅋㅋ..",
          "original_form": "좀많아서 작품명이ㅋㅋㅋ..",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014188.1.4",
          "form": "앜ㅋㅋㅋㅋ 천천히..이야기를 ...해볼까요",
          "original_form": "앜ㅋㅋㅋㅋ 천천히..이야기를 ...해볼까요",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014188.1.5",
          "form": "글고 작품명이 워낙 길어섴ㅋㅋㅋㅋ",
          "original_form": "글고 작품명이 워낙 길어섴ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014188.1.6",
          "form": "저는 어.. 당이돕도 보고..이가주도 보고",
          "original_form": "저는 어.. 당이돕도 보고..이가주도 보고",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014188.1.7",
          "form": "장르를 바꿔보겠습니다도 잼나더라고요",
          "original_form": "장르를 바꿔보겠습니다도 잼나더라고요",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014188.1.8",
          "form": "남미숨도 재미있어요",
          "original_form": "남미숨도 재미있어요",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014188.1.9",
          "form": "요즘 작명너무 주저리st에요ㅋㅋㅋㅋ",
          "original_form": "요즘 작명너무 주저리st에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014188.1.10",
          "form": "저도 당이돕봐요!",
          "original_form": "저도 당이돕봐요!",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014188.1.11",
          "form": "남미숨도요!",
          "original_form": "남미숨도요!",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014188.1.12",
          "form": "뭐 갑자기.악녀가 됐는데 어쩌구 이래요 제목이 다",
          "original_form": "뭐 갑자기.악녀가 됐는데 어쩌구 이래요 제목이 다",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014188.1.13",
          "form": "헉 남미숨..!! 그림체.넘 장난아니지 않나요",
          "original_form": "헉 남미숨..!! 그림체.넘 장난아니지 않나요",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014188.1.14",
          "form": "에르덴 볼 때마다 심장뛰어요",
          "original_form": "에르덴 볼 때마다 심장뛰어요",
          "speaker_id": "2",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014188.1.15",
          "form": "헐 마자요!!!",
          "original_form": "헐 마자요!!!",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014188.1.16",
          "form": "존예탱이에요ㅋㅋㅋ",
          "original_form": "존예탱이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014188.1.17",
          "form": "벨레알?악마 이겨먹을때 너무 재밌었어요ㅋㅋㅋㅋ",
          "original_form": "벨레알?악마 이겨먹을때 너무 재밌었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014188.1.18",
          "form": "마장욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "마장욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014188.1.19",
          "form": "스토리도 자본주의 스토리고(?)",
          "original_form": "스토리도 자본주의 스토리고(?)",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014188.1.20",
          "form": "에르덴..하..",
          "original_form": "에르덴..하..",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014188.1.21",
          "form": "마자욬 ㅋ ㅋㅋㅋㅋㅋ",
          "original_form": "마자욬 ㅋ ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014188.1.22",
          "form": "단행본 나오면 꼭 살거에요",
          "original_form": "단행본 나오면 꼭 살거에요",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014188.1.23",
          "form": "악아꼬도 보세요?",
          "original_form": "악아꼬도 보세요?",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014188.1.24",
          "form": "넘나 소장각",
          "original_form": "넘나 소장각",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014188.1.25",
          "form": "추천은 받았는데 아직안봦ㅎ어요..!",
          "original_form": "추천은 받았는데 아직안봦ㅎ어요..!",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014188.1.26",
          "form": "아악 꼭 보세요 악아꼬",
          "original_form": "아악 꼭 보세요 악아꼬",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014188.1.27",
          "form": "거긴 여주가 넘 사랑스렁 ㅓ요!!!!!",
          "original_form": "거긴 여주가 넘 사랑스렁 ㅓ요!!!!!",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014188.1.28",
          "form": "호르 그렇군요!",
          "original_form": "호르 그렇군요!",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014188.1.29",
          "form": "검색해보니 핑머든데",
          "original_form": "검색해보니 핑머든데",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014188.1.30",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014188.1.31",
