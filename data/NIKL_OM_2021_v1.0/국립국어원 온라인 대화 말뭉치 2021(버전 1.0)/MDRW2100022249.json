        {
          "id": "MDRW2100022249.1.1",
          "form": "음료 자주 드세요?",
          "original_form": "음료 자주 드세요?",
          "speaker_id": "2",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022249.1.2",
          "form": "음료 네 좋아해오",
          "original_form": "음료 네 좋아해오",
          "speaker_id": "1",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.3",
          "form": "어떤 음료 주로 드세요?",
          "original_form": "어떤 음료 주로 드세요?",
          "speaker_id": "2",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.4",
          "form": "탄산종류 자주먹어요",
          "original_form": "탄산종류 자주먹어요",
          "speaker_id": "1",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.5",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.6",
          "form": "오 저도 탄산 좋아해요",
          "original_form": "오 저도 탄산 좋아해요",
          "speaker_id": "2",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.7",
          "form": "근데 설탕 덩어리라 하더라구요 ㅜㅜ",
          "original_form": "근데 설탕 덩어리라 하더라구요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.8",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.9",
          "form": "당쩔..",
          "original_form": "당쩔..",
          "speaker_id": "1",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.10",
          "form": "그래서 저 제로음료만 먹어요",
          "original_form": "그래서 저 제로음료만 먹어요",
          "speaker_id": "2",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.11",
          "form": "제로음료 먹어봤어요?",
          "original_form": "제로음료 먹어봤어요?",
          "speaker_id": "2",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.12",
          "form": "ㅋㅋㅋㅋ 콜라요?",
          "original_form": "ㅋㅋㅋㅋ 콜라요?",
          "speaker_id": "1",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.13",
          "form": "콜라랑 사이다",
          "original_form": "콜라랑 사이다",
          "speaker_id": "2",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.14",
          "form": "콜라는 먹어봤어요",
          "original_form": "콜라는 먹어봤어요",
          "speaker_id": "1",
          "time": "20210829 11:37"
        },
        {
          "id": "MDRW2100022249.1.15",
          "form": "어디 브랜드꺼요?",
          "original_form": "어디 브랜드꺼요?",
          "speaker_id": "2",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100022249.1.16",
          "form": "사이다도 제로가있나요",
          "original_form": "사이다도 제로가있나요",
          "speaker_id": "1",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100022249.1.17",
          "form": "네 그럼요 스프라이트랑 칠성사이다 둘다 있던데요?",
          "original_form": "네 그럼요 스프라이트랑 칠성사이다 둘다 있던데요?",
          "speaker_id": "2",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100022249.1.18",
          "form": "콜라 보통 코카콜라먹지않나요",
          "original_form": "콜라 보통 코카콜라먹지않나요",
          "speaker_id": "1",
          "time": "20210829 11:38"
        },
        {
          "id": "MDRW2100022249.1.19",
          "form": "ㅋㅋ펩시도 제로펩시 내놨더라구요",
