        {
          "id": "MDRW2100034583.1.1",
          "form": "이천원?",
          "original_form": "이천원?",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100034583.1.2",
          "form": "ㅋㅋ이찬원이네",
          "original_form": "ㅋㅋ이찬원이네",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100034583.1.3",
          "form": "이찬원도 음방에 나오네ㅋㅋ",
          "original_form": "이찬원도 음방에 나오네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034583.1.4",
          "form": "누군데",
          "original_form": "누군데",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100034583.1.5",
          "form": "그 트롯트 대회에서 2위가 3위가 한애",
          "original_form": "그 트롯트 대회에서 2위가 3위가 한애",
          "speaker_id": "1",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100034583.1.6",
          "form": "아~",
          "original_form": "아~",
          "speaker_id": "2",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100034583.1.7",
          "form": "임영웅이 1위하고 아직도 인기가 있나보네",
          "original_form": "임영웅이 1위하고 아직도 인기가 있나보네",
          "speaker_id": "1",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100034583.1.8",
          "form": "그런가보네 나이드신분들한테",
          "original_form": "그런가보네 나이드신분들한테",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034583.1.9",
          "form": "트롯은 곡 하나 가지고 몇년을 가니깐ㅋ",
          "original_form": "트롯은 곡 하나 가지고 몇년을 가니깐ㅋ",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100034583.1.10",
          "form": "엉 한곡으로 평생하는 가수도 있잖아 김흥국",
          "original_form": "엉 한곡으로 평생하는 가수도 있잖아 김흥국",
          "speaker_id": "2",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100034583.1.11",
          "form": "ㅋㅋㅋ 저번에 문제 있었지?",
          "original_form": "ㅋㅋㅋ 저번에 문제 있었지?",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100034583.1.12",
          "form": "뺑소니 아니가?",
          "original_form": "뺑소니 아니가?",
          "speaker_id": "2",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100034583.1.13",
          "form": "보험설계사랑 어쩌고 저쩌고 아니었나",
          "original_form": "보험설계사랑 어쩌고 저쩌고 아니었나",
          "speaker_id": "1",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100034583.1.14",
          "form": "오랜만에 음악방송보는데 뭐이리 줄줄이 나오는데",
          "original_form": "오랜만에 음악방송보는데 뭐이리 줄줄이 나오는데",
