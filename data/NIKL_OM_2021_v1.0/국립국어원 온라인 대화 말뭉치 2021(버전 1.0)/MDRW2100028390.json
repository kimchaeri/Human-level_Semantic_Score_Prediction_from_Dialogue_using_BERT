      "utterance": [
        {
          "id": "MDRW2100028390.1.1",
          "form": "내부자들 보셨어요 혹시?",
          "original_form": "내부자들 보셨어요 혹시?",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028390.1.2",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028390.1.3",
          "form": "왠지 좋아하실 것 같았어요",
          "original_form": "왠지 좋아하실 것 같았어요",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028390.1.4",
          "form": "그게 너무 자극적이긴 했는데",
          "original_form": "그게 너무 자극적이긴 했는데",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028390.1.5",
          "form": "이 나라 현실을 잘 보여 준다고 생각해요",
          "original_form": "이 나라 현실을 잘 보여 준다고 생각해요",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028390.1.6",
          "form": "맞아요 정치계와 언론계와 기업의 유착관계",
          "original_form": "맞아요 정치계와 언론계와 기업의 유착관계",
          "speaker_id": "2",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028390.1.7",
          "form": "거기에 백윤식이 하는 말 있잖아요",
          "original_form": "거기에 백윤식이 하는 말 있잖아요",
          "speaker_id": "1",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100028390.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100028390.1.9",
          "form": "그게 이 나라 현실이에요 조선일보가 하는 일이고",
          "original_form": "그게 이 나라 현실이에요 조선일보가 하는 일이고",
          "speaker_id": "1",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100028390.1.10",
          "form": "그렇게 볼 수도 있죠",
          "original_form": "그렇게 볼 수도 있죠",
          "speaker_id": "2",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100028390.1.11",
          "form": "정경유착과 언론유착 끊어 내야 하는데",
          "original_form": "정경유착과 언론유착 끊어 내야 하는데",
          "speaker_id": "1",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100028390.1.12",
          "form": "영화가 때로는 현실을 더 잘 보여 주니까요",
          "original_form": "영화가 때로는 현실을 더 잘 보여 주니까요",
          "speaker_id": "2",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100028390.1.13",
          "form": "나라가 어떻게 될런지 걱정이에요",
          "original_form": "나라가 어떻게 될런지 걱정이에요",
          "speaker_id": "1",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100028390.1.14",
          "form": "저도 스크린 끝나고 나올 때 조금 씁쓸 하더라고요",
          "original_form": "저도 스크린 끝나고 나올 때 조금 씁쓸 하더라고요",
          "speaker_id": "2",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100028390.1.15",
          "form": "백윤식이 어차피 사람도 잊어버린다고",
          "original_form": "백윤식이 어차피 사람도 잊어버린다고",
          "speaker_id": "2",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100028390.1.16",
          "form": "그런 식의 대사를 했었거든요",
          "original_form": "그런 식의 대사를 했었거든요",
          "speaker_id": "2",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100028390.1.17",
          "form": "나는 황정민 나왔던 부당거래 재미있게 받고",
          "original_form": "나는 황정민 나왔던 부당거래 재미있게 받고",
          "speaker_id": "1",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100028390.1.18",
          "form": "봤고",
          "original_form": "봤고",
          "speaker_id": "1",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100028390.1.19",
          "form": "그 더킹 정우성이랑 조인성 나온 것도",
          "original_form": "그 더킹 정우성이랑 조인성 나온 것도",
          "speaker_id": "1",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100028390.1.20",
          "form": "아 저도 봤어요",
          "original_form": "아 저도 봤어요",
          "speaker_id": "2",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100028390.1.21",
          "form": "그것도 참 재미있게 봤어요",
          "original_form": "그것도 참 재미있게 봤어요",
          "speaker_id": "1",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100028390.1.22",
          "form": "영화 좋아하시네요",
          "original_form": "영화 좋아하시네요",
          "speaker_id": "2",
          "time": "20210902 19:42"
        }
      ]
