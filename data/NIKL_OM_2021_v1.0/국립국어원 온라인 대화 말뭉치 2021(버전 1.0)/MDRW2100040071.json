      "utterance": [
        {
          "id": "MDRW2100040071.1.1",
          "form": "시리얼 중에",
          "original_form": "시리얼 중에",
          "speaker_id": "1",
          "time": "20210904 04:06"
        },
        {
          "id": "MDRW2100040071.1.2",
          "form": "애들땜에 자주 못들어왓음ㅜㅜ",
          "original_form": "애들땜에 자주 못들어왓음ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 04:06"
        },
        {
          "id": "MDRW2100040071.1.3",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 04:06"
        },
        {
          "id": "MDRW2100040071.1.4",
          "form": "뮤즐리? 는 안먹음?",
          "original_form": "뮤즐리? 는 안먹음?",
          "speaker_id": "1",
          "time": "20210904 04:06"
        },
        {
          "id": "MDRW2100040071.1.5",
          "form": "이런종류를 뭐라하지? 저번에 엄마가 사온거 맛있있는데 무슨 지퍼백에 들고",
          "original_form": "이런종류를 뭐라하지? 저번에 엄마가 사온거 맛있있는데 무슨 지퍼백에 들고",
          "speaker_id": "1",
          "time": "20210904 04:06"
        },
        {
          "id": "MDRW2100040071.1.6",
          "form": "맛이없을까봐ㅜ",
          "original_form": "맛이없을까봐ㅜ",
          "speaker_id": "2",
          "time": "20210904 04:07"
        },
        {
          "id": "MDRW2100040071.1.7",
          "form": "아 그래놀라!",
          "original_form": "아 그래놀라!",
          "speaker_id": "1",
          "time": "20210904 04:07"
        },
        {
          "id": "MDRW2100040071.1.8",
          "form": "맛있던데",
          "original_form": "맛있던데",
          "speaker_id": "1",
          "time": "20210904 04:07"
        },
        {
          "id": "MDRW2100040071.1.9",
          "form": "막 그런거 달달한건 좋아하는데",
          "original_form": "막 그런거 달달한건 좋아하는데",
          "speaker_id": "2",
          "time": "20210904 04:07"
        },
        {
          "id": "MDRW2100040071.1.10",
          "form": "뮤즐리도 단거아니야? ㅋㅋㅋ",
          "original_form": "뮤즐리도 단거아니야? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 04:08"
        },
        {
          "id": "MDRW2100040071.1.11",
          "form": "아닌가? 난 두개 비슷하다고 생각했음 ㅋㅋㅋㅋ",
          "original_form": "아닌가? 난 두개 비슷하다고 생각했음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 04:08"
        },
        {
          "id": "MDRW2100040071.1.12",
          "form": "ㅋㅋ나는 달달한거 잇고 다이어트용 따로있는줄 알고잇음ㅋㅋ",
          "original_form": "ㅋㅋ나는 달달한거 잇고 다이어트용 따로있는줄 알고잇음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 04:09"
        },
        {
          "id": "MDRW2100040071.1.13",
          "form": "잘몰라ㅋㅋㅋㅋㅋ",
          "original_form": "잘몰라ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 04:09"
        },
        {
          "id": "MDRW2100040071.1.14",
          "form": "ㅋㅋㅋㅋㅋ 다이어트용은 알아보지도 않음ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 다이어트용은 알아보지도 않음ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 04:10"
        },
        {
          "id": "MDRW2100040071.1.15",
          "form": "나지금 그거 궁굼함 ㅋㅋㅋ올영에 베이글칩 아나? ㅋㅋ 그거 크림스프맛 옥수수 스프맛 나왔더라? ㅋㅋ",
          "original_form": "나지금 그거 궁굼함 ㅋㅋㅋ올영에 베이글칩 아나? ㅋㅋ 그거 크림스프맛 옥수수 스프맛 나왔더라? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 04:10"
        },
        {
          "id": "MDRW2100040071.1.16",
          "form": "배이글칩 몰라...지금올영 세일기간이던데",
          "original_form": "배이글칩 몰라...지금올영 세일기간이던데",
          "speaker_id": "2",
          "time": "20210904 04:11"
        },
        {
          "id": "MDRW2100040071.1.17",
          "form": "오늘 너봄",
          "original_form": "오늘 너봄",
          "speaker_id": "2",
          "time": "20210904 04:11"
        },
        {
          "id": "MDRW2100040071.1.18",
          "form": "어제라도 해야하나 ㅡㅋㅋ",
          "original_form": "어제라도 해야하나 ㅡㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 04:11"
        },
        {
          "id": "MDRW2100040071.1.19",
          "form": "엥 어디서 ㅋㅋㅋㅋ",
          "original_form": "엥 어디서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 04:11"
        },
        {
          "id": "MDRW2100040071.1.20",
          "form": "올영세일이라고?!",
          "original_form": "올영세일이라고?!",
          "speaker_id": "1",
          "time": "20210904 04:11"
        },
        {
          "id": "MDRW2100040071.1.21",
          "form": "베이글칩도 세일하려나 ㅋㅋ",
          "original_form": "베이글칩도 세일하려나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 04:11"
        },
        {
          "id": "MDRW2100040071.1.22",
          "form": "알파쪽에서 웬 여학생이랑 막 말하면서 가던데ㅋㅋ",
          "original_form": "알파쪽에서 웬 여학생이랑 막 말하면서 가던데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 04:12"
        },
        {
          "id": "MDRW2100040071.1.23",
          "form": "오늘 지나가면서 보니까 세일이라고 난리를 해놨던데ㅋㅋ",
          "original_form": "오늘 지나가면서 보니까 세일이라고 난리를 해놨던데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 04:12"
        },
        {
          "id": "MDRW2100040071.1.24",
          "form": "알파??",
          "original_form": "알파??",
          "speaker_id": "1",
          "time": "20210904 04:27"
        },
        {
          "id": "MDRW2100040071.1.25",
          "form": "난오늘그쪽을 지나간적이없는데? ㅋㅋㅋㅋ",
          "original_form": "난오늘그쪽을 지나간적이없는데? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 04:27"
        },
        {
          "id": "MDRW2100040071.1.26",
          "form": "올영 함가봐야겄네 ㅋㅋㅋ",
          "original_form": "올영 함가봐야겄네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 04:27"
        },
        {
          "id": "MDRW2100040071.1.27",
          "form": "응응 세일이엇음ㅋㅋ",
          "original_form": "응응 세일이엇음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100040071.1.28",
          "form": "향수세일하면좋겠당",
          "original_form": "향수세일하면좋겠당",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040071.1.29",
          "form": "지마켓세일하잖아",
          "original_form": "지마켓세일하잖아",
          "speaker_id": "1",
          "time": "20210904 15:18"
        }
      ]
    }
