      "utterance": [
        {
          "id": "MDRW2100023074.1.1",
          "form": "오늘 못자는거 아니에요?ㅋㅋㅋ",
          "original_form": "오늘 못자는거 아니에요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100023074.1.2",
          "form": "ㅋㅋㅋㅋ 그럴지도....",
          "original_form": "ㅋㅋㅋㅋ 그럴지도....",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023074.1.3",
          "form": "이정도면 꿈꿀거 같은데",
          "original_form": "이정도면 꿈꿀거 같은데",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023074.1.4",
          "form": "휴 근데 그렇게 좀 후끈 진도나가다가",
          "original_form": "휴 근데 그렇게 좀 후끈 진도나가다가",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100023074.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023074.1.6",
          "form": "진정하자해서",
          "original_form": "진정하자해서",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023074.1.7",
          "form": "한 5뷴 10분 명상하다가 왔어요",
          "original_form": "한 5뷴 10분 명상하다가 왔어요",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023074.1.8",
          "form": "차에서 계속 조용히",
          "original_form": "차에서 계속 조용히",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023074.1.9",
          "form": "누가요?",
          "original_form": "누가요?",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100023074.1.10",
          "form": "둘다",
          "original_form": "둘다",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023074.1.11",
          "form": "너무 타올라서요?",
          "original_form": "너무 타올라서요?",
          "speaker_id": "2",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100023074.1.12",
          "form": "네내",
          "original_form": "네내",
          "speaker_id": "1",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023074.1.13",
          "form": "그렇게 명상타임 가져보셨어요?",
          "original_form": "그렇게 명상타임 가져보셨어요?",
          "speaker_id": "1",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023074.1.14",
          "form": "아뇨....",
          "original_form": "아뇨....",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023074.1.15",
          "form": "전 이전에 안달나서",
          "original_form": "전 이전에 안달나서",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023074.1.16",
          "form": "그냥 직진했는데",
          "original_form": "그냥 직진했는데",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023074.1.17",
          "form": "뽀뽀거절당해서",
          "original_form": "뽀뽀거절당해서",
          "speaker_id": "2",
          "time": "20210829 21:29"
        },
        {
          "id": "MDRW2100023074.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023074.1.19",
          "form": "명상은 갑자기 모에요",
          "original_form": "명상은 갑자기 모에요",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023074.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023074.1.21",
          "form": "아니 몰라요 남친이 갑자기",
          "original_form": "아니 몰라요 남친이 갑자기",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023074.1.22",
          "form": "안돼 하면서 자제하더라구요",
          "original_form": "안돼 하면서 자제하더라구요",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023074.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023074.1.24",
          "form": "그래서 잠깐 명상하다가",
          "original_form": "그래서 잠깐 명상하다가",
          "speaker_id": "1",
          "time": "20210829 21:30"
        },
        {
          "id": "MDRW2100023074.1.25",
          "form": "한번 더 할까? 하길래",
          "original_form": "한번 더 할까? 하길래",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023074.1.26",
          "form": "다시 했다요?",
          "original_form": "다시 했다요?",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023074.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ이렇게 끊어가는 스킨십타임 뭐냐고요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이렇게 끊어가는 스킨십타임 뭐냐고요",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023074.1.28",
          "form": "잉?",
          "original_form": "잉?",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023074.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023074.1.30",
          "form": "끝이아니었네",
          "original_form": "끝이아니었네",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023074.1.31",
          "form": "나름대로의 진도표가 있는것인가",
          "original_form": "나름대로의 진도표가 있는것인가",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023074.1.32",
          "form": "다시했다구요?ㅋㅋ",
          "original_form": "다시했다구요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023074.1.33",
          "form": "네네 그냥 한번 뽀뽀에서 키스로 하고 한참 하다가 끊더니 명상타임을 갖고",
          "original_form": "네네 그냥 한번 뽀뽀에서 키스로 하고 한참 하다가 끊더니 명상타임을 갖고",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100023074.1.34",
          "form": "한번 더 할까 해서 다시 키스를 해보다가 다시 끊고",
          "original_form": "한번 더 할까 해서 다시 키스를 해보다가 다시 끊고",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100023074.1.35",
          "form": "명상타임 가지다가 집왔져",
          "original_form": "명상타임 가지다가 집왔져",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100023074.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023074.1.37",
          "form": "모지",
          "original_form": "모지",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023074.1.38",
          "form": "진도표가 있는 거 같다니까뇨",
          "original_form": "진도표가 있는 거 같다니까뇨",
          "speaker_id": "1",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023074.1.39",
          "form": "물어봐요",
          "original_form": "물어봐요",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023074.1.40",
          "form": "담에 만나면",
          "original_form": "담에 만나면",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023074.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023074.1.42",
          "form": "진도가 너무 빨랐나",
          "original_form": "진도가 너무 빨랐나",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023074.1.43",
          "form": "근데 그러기엔 본인이 먼저 하자고 했다면서요",
          "original_form": "근데 그러기엔 본인이 먼저 하자고 했다면서요",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023074.1.44",
          "form": "넼ㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023074.1.45",
          "form": "가슴이 너무 두근두근거렸나",
          "original_form": "가슴이 너무 두근두근거렸나",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023074.1.46",
          "form": "이제 진정좀 됐어요?",
          "original_form": "이제 진정좀 됐어요?",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023074.1.47",
          "form": "진도표가 있다기엔 오늘 다했는데 ㅋㅋㅋㅋㅋ",
          "original_form": "진도표가 있다기엔 오늘 다했는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023074.1.48",
          "form": "네 이제 찬물샤워해서 진정이요",
          "original_form": "네 이제 찬물샤워해서 진정이요",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023074.1.49",
          "form": "찬물요?ㅋㅋㅋㅋ",
          "original_form": "찬물요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023074.1.50",
          "form": "열난거 맞네",
          "original_form": "열난거 맞네",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023074.1.51",
          "form": "근데 잼있네요",
          "original_form": "근데 잼있네요",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023074.1.52",
          "form": "이렇게 연애담을 들으니 ㅋㅋㅋ",
          "original_form": "이렇게 연애담을 들으니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023074.1.53",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023074.1.54",
          "form": "원래 남 연애사 재미없는데",
          "original_form": "원래 남 연애사 재미없는데",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023074.1.55",
          "form": "전 잼나는데요?",
          "original_form": "전 잼나는데요?",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023074.1.56",
          "form": "ㅋㅋㅋㅋ 추억소환되고",
          "original_form": "ㅋㅋㅋㅋ 추억소환되고",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023074.1.57",
          "form": "그런거 있자나요",
          "original_form": "그런거 있자나요",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023074.1.58",
          "form": "ㅋㅋㅋㅋㅋㅋ그러게요 잘 들어주시니 얘기하는 게 재미있네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러게요 잘 들어주시니 얘기하는 게 재미있네요",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023074.1.59",
          "form": "스킨십은 남자한테 맡기세요",
          "original_form": "스킨십은 남자한테 맡기세요",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023074.1.60",
          "form": "리드당해주세요 ㅋㅋㅋ",
          "original_form": "리드당해주세요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023074.1.61",
          "form": "넹ㅋㅋㅋㅋㅋ",
          "original_form": "넹ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023074.1.62",
          "form": "그러고있어욬ㅋㅋㅋ",
          "original_form": "그러고있어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023074.1.63",
          "form": "다음엔 언제 만나요?",
          "original_form": "다음엔 언제 만나요?",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023074.1.64",
          "form": "수요일에 만나요 ㅎㅎ",
          "original_form": "수요일에 만나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023074.1.65",
          "form": "수요일이 기대되네요 ㅎㅎㅎㅎ",
          "original_form": "수요일이 기대되네요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023074.1.66",
          "form": "서로 연애하는사람끼리 얘기하니까",
          "original_form": "서로 연애하는사람끼리 얘기하니까",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
