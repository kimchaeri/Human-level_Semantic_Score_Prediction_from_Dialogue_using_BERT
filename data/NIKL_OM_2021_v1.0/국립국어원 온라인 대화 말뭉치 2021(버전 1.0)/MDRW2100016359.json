      "utterance": [
        {
          "id": "MDRW2100016359.1.1",
          "form": "마자마자 그렇더라 나는 샤이니 노래 좋아해!",
          "original_form": "마자마자 그렇더라 나는 샤이니 노래 좋아해!",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016359.1.2",
          "form": "수록곡 맛집이야",
          "original_form": "수록곡 맛집이야",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016359.1.3",
          "form": "오 샤이니!! 최근노래는 많이못들었는데",
          "original_form": "오 샤이니!! 최근노래는 많이못들었는데",
          "speaker_id": "1",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016359.1.4",
          "form": "옛날앨범 수록곡은 많이 들었던 기억이있네",
          "original_form": "옛날앨범 수록곡은 많이 들었던 기억이있네",
          "speaker_id": "1",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016359.1.5",
          "form": "누구 제일 좋아해?",
          "original_form": "누구 제일 좋아해?",
          "speaker_id": "1",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016359.1.6",
          "form": "나는 키 좋아해! 키 솔로앨범도 명곡 대잔치야! 추천!",
          "original_form": "나는 키 좋아해! 키 솔로앨범도 명곡 대잔치야! 추천!",
          "speaker_id": "2",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016359.1.7",
          "form": "오ㅋㅋㅋ 추천받았으니 들어볼게",
          "original_form": "오ㅋㅋㅋ 추천받았으니 들어볼게",
          "speaker_id": "1",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016359.1.8",
          "form": "영상은 주로 뭐 이용해?",
          "original_form": "영상은 주로 뭐 이용해?",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016359.1.9",
          "form": "나는 유튜브랑 가끔 넷플!",
          "original_form": "나는 유튜브랑 가끔 넷플!",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016359.1.10",
          "form": "나도 유툽이랑 왓챠!",
          "original_form": "나도 유툽이랑 왓챠!",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016359.1.11",
          "form": "근데 이번에 디즈니플러스나오면",
          "original_form": "근데 이번에 디즈니플러스나오면",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016359.1.12",
          "form": "거기로 갈아탈거같아",
          "original_form": "거기로 갈아탈거같아",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016359.1.13",
          "form": "오오 디즈니 영화 좋아해?",
          "original_form": "오오 디즈니 영화 좋아해?",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016359.1.14",
          "form": "응! 디즈니 마블 이쪽 좋아해서ㅎㅎ",
          "original_form": "응! 디즈니 마블 이쪽 좋아해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016359.1.15",
          "form": "나오는거 거진 다보는거같아ㅋㅋㅋ",
          "original_form": "나오는거 거진 다보는거같아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016359.1.16",
          "form": "오오 그렇구나 나도 마블 좋아해!!!",
          "original_form": "오오 그렇구나 나도 마블 좋아해!!!",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016359.1.17",
          "form": "이번에 스파이더맨 예고봤어?",
          "original_form": "이번에 스파이더맨 예고봤어?",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016359.1.18",
          "form": "ㅋㅋㅋㅋ사실 오늘집가서 놋북으로볼려고 아끼는중",
          "original_form": "ㅋㅋㅋㅋ사실 오늘집가서 놋북으로볼려고 아끼는중",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016359.1.19",
          "form": "어때!???나완전기대중이야",
          "original_form": "어때!???나완전기대중이야",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016359.1.20",
          "form": "ㅋㅋㅋㅋㅋ예고 장난아니야 ㅠㅠㅠ빨리 12월 왔으면좋겟어",
          "original_form": "ㅋㅋㅋㅋㅋ예고 장난아니야 ㅠㅠㅠ빨리 12월 왔으면좋겟어",
          "speaker_id": "2",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016359.1.21",
          "form": "헐 나도빨리 봐야겠다ㅜㅜㅜ",
          "original_form": "헐 나도빨리 봐야겠다ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100016359.1.22",
          "form": "옹옹 빨리봐 ㅜㅜ 12웡 빨리왔으면 좋겟다",
