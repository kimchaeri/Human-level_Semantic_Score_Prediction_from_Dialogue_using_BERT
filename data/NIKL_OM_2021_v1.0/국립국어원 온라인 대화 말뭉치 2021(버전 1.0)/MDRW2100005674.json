      "utterance": [
        {
          "id": "MDRW2100005674.1.1",
          "form": "강아지 키워?",
          "original_form": "강아지 키워?",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100005674.1.2",
          "form": "고양이 한 마리 키워!\r\n강아지 키워??",
          "original_form": "고양이 한 마리 키워!\r\n강아지 키워??",
          "speaker_id": "2",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100005674.1.3",
          "form": "웅 나 강아지 키워 암컷 페키니즈 키워",
          "original_form": "웅 나 강아지 키워 암컷 페키니즈 키워",
          "speaker_id": "1",
          "time": "20210824 01:01"
        },
        {
          "id": "MDRW2100005674.1.4",
          "form": "페키니즈 알아?",
          "original_form": "페키니즈 알아?",
          "speaker_id": "1",
          "time": "20210824 01:01"
        },
        {
          "id": "MDRW2100005674.1.5",
          "form": "알아! 시츄랑 닮은 애기! 아직 입모양이랑 코모양? 보고 완벽하게 구분은 못하는데 얼추 잘 알아보는 것 같아 ㅋㅋㅋㅋㅋ\r\n귀엽겠다ㅜㅜ",
          "original_form": "알아! 시츄랑 닮은 애기! 아직 입모양이랑 코모양? 보고 완벽하게 구분은 못하는데 얼추 잘 알아보는 것 같아 ㅋㅋㅋㅋㅋ\r\n귀엽겠다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 09:55"
        },
        {
          "id": "MDRW2100005674.1.6",
          "form": "헐 아는 구나 ㅠㅠㅠ 알아줘서 고마워",
          "original_form": "헐 아는 구나 ㅠㅠㅠ 알아줘서 고마워",
          "speaker_id": "1",
          "time": "20210825 04:12"
        },
        {
          "id": "MDRW2100005674.1.7",
          "form": "주름강아지라 불리지 ㅎㅎ",
          "original_form": "주름강아지라 불리지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 04:12"
        },
        {
          "id": "MDRW2100005674.1.8",
          "form": "주름강아지 귀엽다 ㅋㅋㅋㅋㅋ\r\n페키니즈는 모종이 하얀 애랑 까만 애만 봤는데 어느 쪽이야??",
          "original_form": "주름강아지 귀엽다 ㅋㅋㅋㅋㅋ\r\n페키니즈는 모종이 하얀 애랑 까만 애만 봤는데 어느 쪽이야??",
          "speaker_id": "2",
          "time": "20210825 09:28"
        },
        {
          "id": "MDRW2100005674.1.9",
          "form": "하얀 애야!! 나도까만애는 실제로 본적이 없네?",
          "original_form": "하얀 애야!! 나도까만애는 실제로 본적이 없네?",
          "speaker_id": "1",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100005674.1.10",
          "form": "너는 어떤 고양이 키워?",
          "original_form": "너는 어떤 고양이 키워?",
          "speaker_id": "1",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100005674.1.11",
          "form": "코숏이야!",
          "original_form": "코숏이야!",
          "speaker_id": "2",
          "time": "20210830 19:07"
        },
        {
          "id": "MDRW2100005674.1.12",
          "form": "삼색 무늬 코숏 ㅎㅎ",
          "original_form": "삼색 무늬 코숏 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:08"
        },
        {
          "id": "MDRW2100005674.1.13",
          "form": "헉 처음들어보는데",
          "original_form": "헉 처음들어보는데",
          "speaker_id": "1",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100005674.1.14",
          "form": "삼색이면 무슨  색 무슨 색이야?",
          "original_form": "삼색이면 무슨  색 무슨 색이야?",
          "speaker_id": "1",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100005674.1.15",
          "form": "검정색 갈색 흰색이 섞인거야!",
          "original_form": "검정색 갈색 흰색이 섞인거야!",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100005674.1.16",
          "form": "사진 보면 딱 알거야!",
          "original_form": "사진 보면 딱 알거야!",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100005674.1.17",
          "form": "오 뭐지 혹시 길고양이 중에도 코숏이 있나?",
          "original_form": "오 뭐지 혹시 길고양이 중에도 코숏이 있나?",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100005674.1.18",
          "form": "코가 짧은 종은 아니겠찌..?",
          "original_form": "코가 짧은 종은 아니겠찌..?",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100005674.1.19",
          "form": "코숏은 코리안숏헤어라고 하는데",
          "original_form": "코숏은 코리안숏헤어라고 하는데",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100005674.1.20",
          "form": "길냥이의 90퍼는 코숏이야!",
          "original_form": "길냥이의 90퍼는 코숏이야!",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100005674.1.21",
          "form": "코가 짧은.. 은 말이 좀 이상한데 코가 들창코처럼 생긴 고양이는 거의 폴드쪽일거야!",
          "original_form": "코가 짧은.. 은 말이 좀 이상한데 코가 들창코처럼 생긴 고양이는 거의 폴드쪽일거야!",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100005674.1.22",
          "form": "헉 그렇구낭!!!!!",
          "original_form": "헉 그렇구낭!!!!!",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100005674.1.23",
          "form": "헉 고양이 종은 그렇구나 너무 귀엽겠다 ㅎㅎ",
          "original_form": "헉 고양이 종은 그렇구나 너무 귀엽겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100005674.1.24",
