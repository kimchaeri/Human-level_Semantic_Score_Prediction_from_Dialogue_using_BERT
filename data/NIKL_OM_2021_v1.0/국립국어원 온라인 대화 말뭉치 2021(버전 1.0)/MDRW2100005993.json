      "utterance": [
        {
          "id": "MDRW2100005993.1.1",
          "form": "아프다기보단 발에 검은 점들이 막 생겨서",
          "original_form": "아프다기보단 발에 검은 점들이 막 생겨서",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100005993.1.2",
          "form": "놀랐죠...",
          "original_form": "놀랐죠...",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100005993.1.3",
          "form": "근처 튜브대여점인가? 거기 직원분이 성게라고 알려주셨어요",
          "original_form": "근처 튜브대여점인가? 거기 직원분이 성게라고 알려주셨어요",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100005993.1.4",
          "form": "발에 부으라고 식초같은거 주시고...",
          "original_form": "발에 부으라고 식초같은거 주시고...",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100005993.1.5",
          "form": "헐 대박 진짜요??? 검은점이 왜생기는거지!!!!",
          "original_form": "헐 대박 진짜요??? 검은점이 왜생기는거지!!!!",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100005993.1.6",
          "form": "조그마한 가시들이 발바닥에 박혀서 검은 점처럼 보였어요...",
          "original_form": "조그마한 가시들이 발바닥에 박혀서 검은 점처럼 보였어요...",
          "speaker_id": "2",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100005993.1.7",
          "form": "와.....다 가시였군요.....",
          "original_form": "와.....다 가시였군요.....",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100005993.1.8",
          "form": "식초 붓고 하니까 다 빠졌어요? ㅠㅠㅠ",
          "original_form": "식초 붓고 하니까 다 빠졌어요? ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100005993.1.9",
          "form": "네 며칠 지나니까 다 빠졌어요",
          "original_form": "네 며칠 지나니까 다 빠졌어요",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100005993.1.10",
          "form": "다행이에요ㅠ 저도 이제 바다 여행가면 바위 조심해야겠어요! ㅋㅋㅋㅋ",
          "original_form": "다행이에요ㅠ 저도 이제 바다 여행가면 바위 조심해야겠어요! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100005993.1.11",
          "form": "맞아요 조심해야 해요 ㅠㅠ",
          "original_form": "맞아요 조심해야 해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100005993.1.12",
          "form": "한 번 찔려보니까 바다 멀리 나가는게 무섭더라고요",
          "original_form": "한 번 찔려보니까 바다 멀리 나가는게 무섭더라고요",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100005993.1.13",
          "form": "그럴것같아요 ㅠㅠㅠ 진짜 멍게라니ㅠㅠㅠ",
          "original_form": "그럴것같아요 ㅠㅠㅠ 진짜 멍게라니ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100005993.1.14",
          "form": "근데 바다얘기하니까 서핑 배우고싶어요ㅠㅠ",
          "original_form": "근데 바다얘기하니까 서핑 배우고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100005993.1.15",
          "form": "맞아요! 서핑 타는 사람들 보니까 재밌어 보이더라고요",
          "original_form": "맞아요! 서핑 타는 사람들 보니까 재밌어 보이더라고요",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100005993.1.16",
          "form": "약간 스키타는 거랑 비슷하려나...?",
          "original_form": "약간 스키타는 거랑 비슷하려나...?",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100005993.1.17",
          "form": "그쵸그쳐 진짜 재밌어보여요!!!",
          "original_form": "그쵸그쳐 진짜 재밌어보여요!!!",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100005993.1.18",
          "form": "저는 중심 잘 못잡을것같아서 무섭기도해요 ㅋㅋㅋ",
          "original_form": "저는 중심 잘 못잡을것같아서 무섭기도해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100005993.1.19",
          "form": "name1님은 가보시고 싶으신 여행지 있으신가요??",
          "original_form": "&name1&님은 가보시고 싶으신 여행지 있으신가요??",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100005993.1.20",
