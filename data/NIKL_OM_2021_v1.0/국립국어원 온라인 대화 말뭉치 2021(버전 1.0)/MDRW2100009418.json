      "utterance": [
        {
          "id": "MDRW2100009418.1.1",
          "form": "name3이는 계속94썼는데ㅠ",
          "original_form": "&name3&이는 계속94썼는데ㅠ",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009418.1.2",
          "form": "이제덜더우니 kf로 갈아타ㅜ",
          "original_form": "이제덜더우니 kf로 갈아타ㅜ",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009418.1.3",
          "form": "kf로 다시쓰고 보냈다 아휴ㅜㅋㅋ",
          "original_form": "kf로 다시쓰고 보냈다 아휴ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 08:38"
        },
        {
          "id": "MDRW2100009418.1.4",
          "form": "비도오고 꿀꿀하구만",
          "original_form": "비도오고 꿀꿀하구만",
          "speaker_id": "1",
          "time": "20210823 10:46"
        },
        {
          "id": "MDRW2100009418.1.5",
          "form": "응응 알바다녀오고 ㅋㅋ밥먹었다ㅋ",
          "original_form": "응응 알바다녀오고 ㅋㅋ밥먹었다ㅋ",
          "speaker_id": "2",
          "time": "20210823 12:49"
        },
        {
          "id": "MDRW2100009418.1.6",
          "form": "비오고 습하고 덥다ㅜㅠ에어컨틈",
          "original_form": "비오고 습하고 덥다ㅜㅠ에어컨틈",
          "speaker_id": "1",
          "time": "20210823 12:57"
        },
        {
          "id": "MDRW2100009418.1.7",
          "form": "나두 에어컨틀었다ㅜㅜㅋㅋ",
          "original_form": "나두 에어컨틀었다ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009418.1.8",
          "form": "밥은먹었어?",
          "original_form": "밥은먹었어?",
          "speaker_id": "2",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100009418.1.9",
          "form": "응 국수로ㅋㅋ",
          "original_form": "응 국수로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:59"
        },
        {
          "id": "MDRW2100009418.1.10",
          "form": "국수좋지ㅋㅋㅋ",
          "original_form": "국수좋지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009418.1.11",
          "form": "나는 김밥먹었어ㅋ",
          "original_form": "나는 김밥먹었어ㅋ",
          "speaker_id": "2",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009418.1.12",
          "form": "포장해왔엉ㅋㅋ",
          "original_form": "포장해왔엉ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:00"
        },
        {
          "id": "MDRW2100009418.1.13",
          "form": "좋다~ 김밥 먹고싶다ㅋㅋ",
          "original_form": "좋다~ 김밥 먹고싶다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100009418.1.14",
          "form": "김밥 자주먹어서 그런지 좀 안땡겼는데ㅋㅋ",
          "original_form": "김밥 자주먹어서 그런지 좀 안땡겼는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100009418.1.15",
          "form": "금방먹을수있는거라서 ㅋㅋ 또 먹음",
          "original_form": "금방먹을수있는거라서 ㅋㅋ 또 먹음",
          "speaker_id": "2",
          "time": "20210823 13:45"
        },
        {
          "id": "MDRW2100009418.1.16",
          "form": "맞다 간단하게 한끼 떼우고 딱이지",
          "original_form": "맞다 간단하게 한끼 떼우고 딱이지",
          "speaker_id": "1",
          "time": "20210823 14:10"
        },
        {
          "id": "MDRW2100009418.1.17",
          "form": "응응 비가와서 그런가 살짝 피곤하네ㅜㅋㅋ",
          "original_form": "응응 비가와서 그런가 살짝 피곤하네ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100009418.1.18",
          "form": "name4는 또 응가와의 사투중",
          "original_form": "&name4&는 또 응가와의 사투중",
          "speaker_id": "1",
          "time": "20210823 16:07"
        },
        {
          "id": "MDRW2100009418.1.19",
          "form": "끙아쥬스안먹는다고ㅠ",
          "original_form": "끙아쥬스안먹는다고ㅠ",
