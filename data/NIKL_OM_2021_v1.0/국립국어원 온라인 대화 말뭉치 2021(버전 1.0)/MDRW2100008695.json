      "utterance": [
        {
          "id": "MDRW2100008695.1.1",
          "form": "어쨋든여행가고싶다",
          "original_form": "어쨋든여행가고싶다",
          "speaker_id": "2",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008695.1.2",
          "form": "ㅇㅈ 여행 갈래",
          "original_form": "ㅇㅈ 여행 갈래",
          "speaker_id": "1",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008695.1.3",
          "form": "코로나만아니면갈텐데",
          "original_form": "코로나만아니면갈텐데",
          "speaker_id": "2",
          "time": "20210822 17:31"
        },
        {
          "id": "MDRW2100008695.1.4",
          "form": "국내말고해외여행가고싶음",
          "original_form": "국내말고해외여행가고싶음",
          "speaker_id": "2",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008695.1.5",
          "form": "해외 어디???",
          "original_form": "해외 어디???",
          "speaker_id": "1",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008695.1.6",
          "form": "일본이나북유럽쪽",
          "original_form": "일본이나북유럽쪽",
          "speaker_id": "2",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008695.1.7",
          "form": "캐나다도좋음",
          "original_form": "캐나다도좋음",
          "speaker_id": "2",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008695.1.8",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210822 17:33"
        },
        {
          "id": "MDRW2100008695.1.9",
          "form": "아 나는 거기 아이슬란드",
          "original_form": "아 나는 거기 아이슬란드",
          "speaker_id": "1",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008695.1.10",
          "form": "아이슬란드가유럽쪽이야아니면북미쪽이야",
          "original_form": "아이슬란드가유럽쪽이야아니면북미쪽이야",
          "speaker_id": "2",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008695.1.11",
          "form": "유럽 쪽이래",
          "original_form": "유럽 쪽이래",
          "speaker_id": "1",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008695.1.12",
          "form": "북근 바로 밑쪽에 있는 나라래",
          "original_form": "북근 바로 밑쪽에 있는 나라래",
          "speaker_id": "1",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100008695.1.13",
          "form": "극",
          "original_form": "극",
          "speaker_id": "1",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100008695.1.14",
          "form": "아대충어딘지알거같음",
          "original_form": "아대충어딘지알거같음",
          "speaker_id": "2",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100008695.1.15",
          "form": "추운나라아님?",
          "original_form": "추운나라아님?",
          "speaker_id": "2",
          "time": "20210822 17:52"
        },
        {
          "id": "MDRW2100008695.1.16",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008695.1.17",
          "form": "거기 오로라가 기가막힌대",
          "original_form": "거기 오로라가 기가막힌대",
          "speaker_id": "1",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008695.1.18",
          "form": "오로라실제로보고싶긴함",
          "original_form": "오로라실제로보고싶긴함",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008695.1.19",
          "form": "멋있을거같음",
          "original_form": "멋있을거같음",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008695.1.20",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008695.1.21",
          "form": "근데코로나만",
          "original_form": "근데코로나만",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008695.1.22",
          "form": "아니면방학에갈만한데",
          "original_form": "아니면방학에갈만한데",
          "speaker_id": "2",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008695.1.23",
          "form": "코로나때문에 아무데도 못가고 있단게 참",
          "original_form": "코로나때문에 아무데도 못가고 있단게 참",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008695.1.24",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008695.1.25",
          "form": "나동아리중에",
          "original_form": "나동아리중에",
          "speaker_id": "2",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008695.1.26",
          "form": "세계대학이랑연결된거하나있는데",
          "original_form": "세계대학이랑연결된거하나있는데",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008695.1.27",
          "form": "이번에모로코가는거신청받았었는데",
          "original_form": "이번에모로코가는거신청받았었는데",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008695.1.28",
          "form": "갑자기모로코에코로나심해졌다고",
          "original_form": "갑자기모로코에코로나심해졌다고",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008695.1.29",
          "form": "연기됨",
          "original_form": "연기됨",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008695.1.30",
          "form": "헐 진짜 아깝네",
          "original_form": "헐 진짜 아깝네",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008695.1.31",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008695.1.32",
          "form": "톡인줄알고욕할뻔",
          "original_form": "톡인줄알고욕할뻔",
          "speaker_id": "2",
          "time": "20210822 17:58"
        },
        {
