      "utterance": [
        {
          "id": "MDRW2100042570.1.1",
          "form": "내가 초기부터 염불을 욌는데",
          "original_form": "내가 초기부터 염불을 욌는데",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042570.1.2",
          "form": "아무도 안들엇지",
          "original_form": "아무도 안들엇지",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042570.1.3",
          "form": "ㅇㅇ 난 아직도 얘가 뭔 겜인지 모르겠어 ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 난 아직도 얘가 뭔 겜인지 모르겠어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042570.1.4",
          "form": "로오히 노잼!!!!!",
          "original_form": "로오히 노잼!!!!!",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042570.1.5",
          "form": "대사도 완전 라노벨이라며",
          "original_form": "대사도 완전 라노벨이라며",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042570.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042570.1.7",
          "form": "ㅇㅇ걍 적절한 여초코인과 착즙과",
          "original_form": "ㅇㅇ걍 적절한 여초코인과 착즙과",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042570.1.8",
          "form": "그 우리가 공격받는다",
          "original_form": "그 우리가 공격받는다",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042570.1.9",
          "form": "할때 뭉치게되는?",
          "original_form": "할때 뭉치게되는?",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042570.1.10",
          "form": "그거가 잘 맞물려서",
          "original_form": "그거가 잘 맞물려서",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042570.1.11",
          "form": "살아남은듯",
          "original_form": "살아남은듯",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042570.1.12",
          "form": "ㅇㅇ 로미오와 줄리엣 증후군 느낌 ㅋㅋㅋ",
          "original_form": "ㅇㅇ 로미오와 줄리엣 증후군 느낌 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042570.1.13",
          "form": "아ㅎ",
          "original_form": "아ㅎ",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042570.1.14",
          "form": "뭔 증후군인지 모르지만 알겠네",
          "original_form": "뭔 증후군인지 모르지만 알겠네",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042570.1.15",
          "form": "냅둿으면 걍 적당히 사귀다 헤어졋을 애들을",
          "original_form": "냅둿으면 걍 적당히 사귀다 헤어졋을 애들을",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042570.1.16",
          "form": "자꾸 땔감 넣어서 천년의 사랑 하게 만드는...",
          "original_form": "자꾸 땔감 넣어서 천년의 사랑 하게 만드는...",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042570.1.17",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042570.1.18",
          "form": "그게 없어서",
          "original_form": "그게 없어서",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042570.1.19",
          "form": "더 죽은것같기도 해",
          "original_form": "더 죽은것같기도 해",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042570.1.20",
          "form": "계속 뭔가 뻘짓해서",
          "original_form": "계속 뭔가 뻘짓해서",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042570.1.21",
          "form": "계속 탔으면",
          "original_form": "계속 탔으면",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042570.1.22",
          "form": "코어팬층 더 두꺼워졌을듯",
          "original_form": "코어팬층 더 두꺼워졌을듯",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042570.1.23",
          "form": "ㅇㅇ 이젠 외부에선 거의 노관심이고",
          "original_form": "ㅇㅇ 이젠 외부에선 거의 노관심이고",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042570.1.24",
          "form": "걍 클겜 똥운영만 반복이니까...",
          "original_form": "걍 클겜 똥운영만 반복이니까...",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042570.1.25",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042570.1.26",
          "form": "운영 진짜 못하긴 하더라",
          "original_form": "운영 진짜 못하긴 하더라",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042570.1.27",
          "form": "난 고객센터가 그따위로 운영되는 겜은",
          "original_form": "난 고객센터가 그따위로 운영되는 겜은",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042570.1.28",
          "form": "첨봤잖아",
          "original_form": "첨봤잖아",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042570.1.29",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042570.1.30",
          "form": "ㅇㅇ 못하는 것도 못하는거지만 되도않는 컨셉질은 뭐야 진짜 ㅋㅋㅋ",
          "original_form": "ㅇㅇ 못하는 것도 못하는거지만 되도않는 컨셉질은 뭐야 진짜 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042570.1.31",
          "form": "공사구분 못하는 오타쿠같음",
          "original_form": "공사구분 못하는 오타쿠같음",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042570.1.32",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042570.1.33",
          "form": "진심 사회성 떨어지니까 이거좋다고 알티 태우지 마라...",
          "original_form": "진심 사회성 떨어지니까 이거좋다고 알티 태우지 마라...",
