      "utterance": [
        {
          "id": "MDRW2100014969.1.1",
          "form": "저 어제 화장품 쇼핑 했어요~",
          "original_form": "저 어제 화장품 쇼핑 했어요~",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014969.1.2",
          "form": "오 뭐사셨어요???",
          "original_form": "오 뭐사셨어요???",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014969.1.3",
          "form": "스킨 로션도 다 떨어져가는 데다 할인까지 하길래!",
          "original_form": "스킨 로션도 다 떨어져가는 데다 할인까지 하길래!",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014969.1.4",
          "form": "와 어디꺼요?",
          "original_form": "와 어디꺼요?",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014969.1.5",
          "form": "가.. 가츠비거요;;",
          "original_form": "가.. 가츠비거요;;",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014969.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014969.1.7",
          "form": "올인원이랑 왁스랑 헤어 영양 주는 그런 것도 샀어요",
          "original_form": "올인원이랑 왁스랑 헤어 영양 주는 그런 것도 샀어요",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014969.1.8",
          "form": "천원하더라고요..ㅋㅋㅋㅋ",
          "original_form": "천원하더라고요..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014969.1.9",
          "form": "맞아요 저도 샀어요...",
          "original_form": "맞아요 저도 샀어요...",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014969.1.10",
          "form": "좋더라구요",
          "original_form": "좋더라구요",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014969.1.11",
          "form": "어?! 알고 계시네요 대박 ㅋㅋ",
          "original_form": "어?! 알고 계시네요 대박 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014969.1.12",
          "form": "전 처음 사봤는데 필링패드 진짜 대박 40매에 천원",
          "original_form": "전 처음 사봤는데 필링패드 진짜 대박 40매에 천원",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014969.1.13",
          "form": "네!! 전 폼클렌징 샀어요!!",
          "original_form": "네!! 전 폼클렌징 샀어요!!",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014969.1.14",
          "form": "오~ 좋아요 ?",
          "original_form": "오~ 좋아요 ?",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014969.1.15",
          "form": "그냥...그냥 그래요 ㅋㅋㅋ",
          "original_form": "그냥...그냥 그래요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014969.1.16",
          "form": "잘모르겠어요",
          "original_form": "잘모르겠어요",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014969.1.17",
          "form": "전 잘 몰라서 종류별로 그냥 하나씩 사봤어요",
          "original_form": "전 잘 몰라서 종류별로 그냥 하나씩 사봤어요",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014969.1.18",
          "form": "패드는 좋은거같아요",
          "original_form": "패드는 좋은거같아요",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014969.1.19",
          "form": "맨날 아껴쓰다가 유통기한 지나고 그랬는데",
          "original_form": "맨날 아껴쓰다가 유통기한 지나고 그랬는데",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014969.1.20",
          "form": "하나사봤는데",
          "original_form": "하나사봤는데",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014969.1.21",
          "form": "펑펑 써보려구요...",
          "original_form": "펑펑 써보려구요...",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014969.1.22",
          "form": "좋더라구요",
          "original_form": "좋더라구요",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014969.1.23",
          "form": "맞아요 아끼면 똥 돼요 진짜",
          "original_form": "맞아요 아끼면 똥 돼요 진짜",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014969.1.24",
          "form": "유통기한 짧을까봐 많이 못샀는데 후기 보니까 긴 편인가봐요?",
          "original_form": "유통기한 짧을까봐 많이 못샀는데 후기 보니까 긴 편인가봐요?",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014969.1.25",
          "form": "생각보다 길어요!",
          "original_form": "생각보다 길어요!",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014969.1.26",
          "form": "쟁여놓고 회사에도 두고 쓸까 생각중이에요",
          "original_form": "쟁여놓고 회사에도 두고 쓸까 생각중이에요",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014969.1.27",
          "form": "그래도 반년은 다 되는거같은..?",
          "original_form": "그래도 반년은 다 되는거같은..?",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014969.1.28",
          "form": "오 좋네요 펑펑 써요 그냥",
          "original_form": "오 좋네요 펑펑 써요 그냥",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014969.1.29",
          "form": "코로나 시국이라 내친김에 건강도 챙길겸 운동하고 다이어트도 하려구요~!",
          "original_form": "코로나 시국이라 내친김에 건강도 챙길겸 운동하고 다이어트도 하려구요~!",
          "speaker_id": "2",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014969.1.30",
          "form": "건강검진 때 충격받아서...",
          "original_form": "건강검진 때 충격받아서...",
          "speaker_id": "2",
          "time": "20210825 21:26"
        }
      ]
