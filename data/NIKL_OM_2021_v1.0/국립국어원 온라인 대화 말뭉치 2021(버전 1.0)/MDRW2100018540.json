      "utterance": [
        {
          "id": "MDRW2100018540.1.1",
          "form": "밀떡볶이요?",
          "original_form": "밀떡볶이요?",
          "speaker_id": "2",
          "time": "20210827 08:02"
        },
        {
          "id": "MDRW2100018540.1.2",
          "form": "네 밀떡이요",
          "original_form": "네 밀떡이요",
          "speaker_id": "1",
          "time": "20210827 08:02"
        },
        {
          "id": "MDRW2100018540.1.3",
          "form": "밀떡 쌀떡 뭘 좋으시나요",
          "original_form": "밀떡 쌀떡 뭘 좋으시나요",
          "speaker_id": "1",
          "time": "20210827 08:02"
        },
        {
          "id": "MDRW2100018540.1.4",
          "form": "밀떡이요ㅎㅎ",
          "original_form": "밀떡이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:03"
        },
        {
          "id": "MDRW2100018540.1.5",
          "form": "밀떡 드실 때 진간장 찍어드셔보세요",
          "original_form": "밀떡 드실 때 진간장 찍어드셔보세요",
          "speaker_id": "2",
          "time": "20210827 08:03"
        },
        {
          "id": "MDRW2100018540.1.6",
          "form": "요새 밀떡 쓰는곳 없죠ㅜ",
          "original_form": "요새 밀떡 쓰는곳 없죠ㅜ",
          "speaker_id": "1",
          "time": "20210827 08:03"
        },
        {
          "id": "MDRW2100018540.1.7",
          "form": "풍미가 캬~~~~",
          "original_form": "풍미가 캬~~~~",
          "speaker_id": "2",
          "time": "20210827 08:03"
        },
        {
          "id": "MDRW2100018540.1.8",
          "form": "있어요 아딸",
          "original_form": "있어요 아딸",
          "speaker_id": "2",
          "time": "20210827 08:03"
        },
        {
          "id": "MDRW2100018540.1.9",
          "form": "오 꿀팁이네요",
          "original_form": "오 꿀팁이네요",
          "speaker_id": "1",
          "time": "20210827 08:03"
        },
        {
          "id": "MDRW2100018540.1.10",
          "form": "헉 아딸에 밀떡 있어요?",
          "original_form": "헉 아딸에 밀떡 있어요?",
          "speaker_id": "1",
          "time": "20210827 08:03"
        },
        {
          "id": "MDRW2100018540.1.11",
          "form": "왜 몰랐지ㅜ",
          "original_form": "왜 몰랐지ㅜ",
          "speaker_id": "1",
          "time": "20210827 08:04"
        },
        {
          "id": "MDRW2100018540.1.12",
          "form": "밀떡매니아층이 은근히 많다구요",
          "original_form": "밀떡매니아층이 은근히 많다구요",
          "speaker_id": "2",
          "time": "20210827 08:04"
        },
        {
          "id": "MDRW2100018540.1.13",
          "form": "저와 name2 포함해서요ㅎ",
          "original_form": "저와 &name2& 포함해서요ㅎ",
          "speaker_id": "1",
          "time": "20210827 08:05"
        },
        {
          "id": "MDRW2100018540.1.14",
          "form": "전 이제 쌀떡만 나오는줄ㅜ",
          "original_form": "전 이제 쌀떡만 나오는줄ㅜ",
          "speaker_id": "1",
          "time": "20210827 08:05"
        },
        {
          "id": "MDRW2100018540.1.15",
          "form": "아마 다른 브랜드에서도 있을거에요ㅎㅎ",
          "original_form": "아마 다른 브랜드에서도 있을거에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:05"
        },
        {
          "id": "MDRW2100018540.1.16",
          "form": "ㅎㅎ name2 믿고 한번 찾아볼게요",
          "original_form": "ㅎㅎ &name2& 믿고 한번 찾아볼게요",
          "speaker_id": "1",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018540.1.17",
          "form": "아 그 떡국떡으로 해도 좋아요",
          "original_form": "아 그 떡국떡으로 해도 좋아요",
          "speaker_id": "1",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018540.1.18",
          "form": "네. 오늘 점심은 떡볶이로",
          "original_form": "네. 오늘 점심은 떡볶이로",
          "speaker_id": "2",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018540.1.19",
          "form": "전 가래떡 굵게 하는거요",
          "original_form": "전 가래떡 굵게 하는거요",
          "speaker_id": "2",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100018540.1.20",
          "form": "아예 가래떡볶이도 있더라고욪ㅎㅎ",
          "original_form": "아예 가래떡볶이도 있더라고욪ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100018540.1.21",
          "form": "가래떡을 면처럼 해서요",
          "original_form": "가래떡을 면처럼 해서요",
          "speaker_id": "1",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100018540.1.22",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "1",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100018540.1.23",
          "form": "맞아요. 약간 부산스타일?",
          "original_form": "맞아요. 약간 부산스타일?",
          "speaker_id": "2",
          "time": "20210827 08:08"
        },
        {
          "id": "MDRW2100018540.1.24",
