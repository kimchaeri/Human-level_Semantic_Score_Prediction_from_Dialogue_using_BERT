      "utterance": [
        {
          "id": "MDRW2100026008.1.1",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100026008.1.2",
          "form": "사무직하다 거기서 하루종일 서있으려니... ㅋㅋ",
          "original_form": "사무직하다 거기서 하루종일 서있으려니... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026008.1.3",
          "form": "너무 힘들었어요ㅋㅋ",
          "original_form": "너무 힘들었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026008.1.4",
          "form": "지금 다시 직장인입니다ㅎㅎ",
          "original_form": "지금 다시 직장인입니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026008.1.5",
          "form": "아~ 사무직이 짱이죠?",
          "original_form": "아~ 사무직이 짱이죠?",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026008.1.6",
          "form": "전그래요ㅋㅋ",
          "original_form": "전그래요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100026008.1.7",
          "form": "그니까요 아오 육체노동...ㅜ",
          "original_form": "그니까요 아오 육체노동...ㅜ",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100026008.1.8",
          "form": "화장실청소 현타 대박이에요 내가 이 나이에 타국에서 굳이????",
          "original_form": "화장실청소 현타 대박이에요 내가 이 나이에 타국에서 굳이????",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100026008.1.9",
          "form": "카페일하다 한국왔어요ㅋㅋ",
          "original_form": "카페일하다 한국왔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100026008.1.10",
          "form": "와 진짜요?",
          "original_form": "와 진짜요?",
          "speaker_id": "1",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100026008.1.11",
          "form": "그래도  돈 마니 벌어오셨어요?ㅋㅋ",
          "original_form": "그래도  돈 마니 벌어오셨어요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100026008.1.12",
          "form": "시급이얼마에요?",
          "original_form": "시급이얼마에요?",
          "speaker_id": "1",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100026008.1.13",
          "form": "ㅋㅋ돈은... ㅋㅋ",
          "original_form": "ㅋㅋ돈은... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100026008.1.14",
          "form": "그때 12.5불인가.",
          "original_form": "그때 12.5불인가.",
          "speaker_id": "2",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026008.1.15",
          "form": "쎄긴해요",
          "original_form": "쎄긴해요",
          "speaker_id": "1",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026008.1.16",
          "form": "이제 가물가물하네요ㅋㅋ",
          "original_form": "이제 가물가물하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026008.1.17",
          "form": "여행이나갑시다ㅋㅋㅋ",
          "original_form": "여행이나갑시다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026008.1.18",
          "form": "휴일에 돈 더주고 팁 받는 재미죠! ㅋㅋ",
          "original_form": "휴일에 돈 더주고 팁 받는 재미죠! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026008.1.19",
          "form": "워킹은 모국에서",
          "original_form": "워킹은 모국에서",
          "speaker_id": "1",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026008.1.20",
          "form": "네 외국은 여행으로!",
          "original_form": "네 외국은 여행으로!",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100026008.1.21",
          "form": "돈쓰러가는겁니다ㅋㅋ",
          "original_form": "돈쓰러가는겁니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100026008.1.22",
          "form": "8시간 엉덩이 붙이고 인터넷을 같이 할수있는\r\n사무직이 최고죠!",
          "original_form": "8시간 엉덩이 붙이고 인터넷을 같이 할수있는\r\n사무직이 최고죠!",
          "speaker_id": "2",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100026008.1.23",
          "form": "맞아여ㅋㅋ 진짜 가끔 고생해봐야 아 감사하다 느껴요",
          "original_form": "맞아여ㅋㅋ 진짜 가끔 고생해봐야 아 감사하다 느껴요",
          "speaker_id": "1",
          "time": "20210901 18:55"
        }
      ]
