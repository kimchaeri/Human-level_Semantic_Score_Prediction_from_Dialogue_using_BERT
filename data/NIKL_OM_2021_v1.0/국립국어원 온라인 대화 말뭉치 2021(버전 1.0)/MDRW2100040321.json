      "utterance": [
        {
          "id": "MDRW2100040321.1.1",
          "form": "아 저 오늘 저녁 외식해요",
          "original_form": "아 저 오늘 저녁 외식해요",
          "speaker_id": "1",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100040321.1.2",
          "form": "우와 뭐 드시러 가나요",
          "original_form": "우와 뭐 드시러 가나요",
          "speaker_id": "2",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100040321.1.3",
          "form": "제가 순순히 알려드릴 것 같나요!",
          "original_form": "제가 순순히 알려드릴 것 같나요!",
          "speaker_id": "1",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100040321.1.4",
          "form": "이럴수가 그럼 제가 맞춰볼게요",
          "original_form": "이럴수가 그럼 제가 맞춰볼게요",
          "speaker_id": "2",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100040321.1.5",
          "form": "면 요리인가요?",
          "original_form": "면 요리인가요?",
          "speaker_id": "2",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100040321.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ면 요리 아닙니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ면 요리 아닙니다",
          "speaker_id": "1",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100040321.1.7",
          "form": "빵 요리인가요!",
          "original_form": "빵 요리인가요!",
          "speaker_id": "2",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100040321.1.8",
          "form": "빵요리도 아닙니다!",
          "original_form": "빵요리도 아닙니다!",
          "speaker_id": "1",
          "time": "20210904 16:15"
        },
        {
          "id": "MDRW2100040321.1.9",
          "form": "그럼 쌀 요리 맞지요?!",
          "original_form": "그럼 쌀 요리 맞지요?!",
          "speaker_id": "2",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100040321.1.10",
          "form": "쌀이랑 같이 먹긴 하네요!",
          "original_form": "쌀이랑 같이 먹긴 하네요!",
          "speaker_id": "1",
          "time": "20210904 16:17"
        },
        {
          "id": "MDRW2100040321.1.11",
          "form": "엇 쌀이 메인이 아닌가보네요",
          "original_form": "엇 쌀이 메인이 아닌가보네요",
          "speaker_id": "2",
          "time": "20210904 16:18"
        },
        {
          "id": "MDRW2100040321.1.12",
          "form": "야채요리인가요?",
          "original_form": "야채요리인가요?",
          "speaker_id": "2",
          "time": "20210904 16:18"
        },
        {
          "id": "MDRW2100040321.1.13",
          "form": "야채요리 아닙니다!",
          "original_form": "야채요리 아닙니다!",
          "speaker_id": "1",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100040321.1.14",
          "form": "좀만 더 힘을내요",
          "original_form": "좀만 더 힘을내요",
          "speaker_id": "1",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100040321.1.15",
          "form": "ㅋㅋㅋㅋㅋㅌ오늘도 고기군요! 돼지고기인가요?",
          "original_form": "ㅋㅋㅋㅋㅋㅌ오늘도 고기군요! 돼지고기인가요?",
          "speaker_id": "2",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100040321.1.16",
          "form": "넵 돼지고기 맞아요! ㅎㅎ",
          "original_form": "넵 돼지고기 맞아요! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:20"
        },
