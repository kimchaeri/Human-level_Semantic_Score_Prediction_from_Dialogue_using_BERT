      "utterance": [
        {
          "id": "MDRW2100002404.1.1",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "2",
          "time": "20210820 11:15"
        },
        {
          "id": "MDRW2100002404.1.2",
          "form": "나도 가고 싶었는데 코로나 끝나면 같이 갈래?",
          "original_form": "나도 가고 싶었는데 코로나 끝나면 같이 갈래?",
          "speaker_id": "1",
          "time": "20210820 11:15"
        },
        {
          "id": "MDRW2100002404.1.3",
          "form": "헐 완전 좋지",
          "original_form": "헐 완전 좋지",
          "speaker_id": "2",
          "time": "20210820 11:15"
        },
        {
          "id": "MDRW2100002404.1.4",
          "form": "어디로 여행갈까?",
          "original_form": "어디로 여행갈까?",
          "speaker_id": "2",
          "time": "20210820 11:15"
        },
        {
          "id": "MDRW2100002404.1.5",
          "form": "해외 여행? 국내 여행? 어느쪽으로 갈래??",
          "original_form": "해외 여행? 국내 여행? 어느쪽으로 갈래??",
          "speaker_id": "1",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002404.1.6",
          "form": "둘 다 가자",
          "original_form": "둘 다 가자",
          "speaker_id": "2",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002404.1.7",
          "form": "국내여행은 가볍게 가기 좋고",
          "original_form": "국내여행은 가볍게 가기 좋고",
          "speaker_id": "2",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002404.1.8",
          "form": "해외여행은 계획 빡 세워서 가자",
          "original_form": "해외여행은 계획 빡 세워서 가자",
          "speaker_id": "2",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002404.1.9",
          "form": "너는 어디 가고 싶어?",
          "original_form": "너는 어디 가고 싶어?",
          "speaker_id": "2",
          "time": "20210820 11:16"
        },
        {
          "id": "MDRW2100002404.1.10",
          "form": "젤 가까운 곳은 일본이긴한데 일본 온천이나  보라카이쪽 같은 바다도 가고 싶고",
          "original_form": "젤 가까운 곳은 일본이긴한데 일본 온천이나  보라카이쪽 같은 바다도 가고 싶고",
          "speaker_id": "1",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002404.1.11",
          "form": "넌 어디로 가고 싶어?",
          "original_form": "넌 어디로 가고 싶어?",
          "speaker_id": "1",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002404.1.12",
          "form": "나도 일본이 여행가기 만만하네",
          "original_form": "나도 일본이 여행가기 만만하네",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002404.1.13",
          "form": "유럽 한번도 안가봐서 유럽도 가보고싶긴 해",
          "original_form": "유럽 한번도 안가봐서 유럽도 가보고싶긴 해",
          "speaker_id": "2",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002404.1.14",
          "form": "나도 유럽 한번도 안가봐서 가보고 싶다",
          "original_form": "나도 유럽 한번도 안가봐서 가보고 싶다",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002404.1.15",
          "form": "근데 영어를 못하니까 가기 무서워",
          "original_form": "근데 영어를 못하니까 가기 무서워",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002404.1.16",
          "form": "내가 영어공부 열심히 할게",
          "original_form": "내가 영어공부 열심히 할게",
          "speaker_id": "2",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002404.1.17",
          "form": "여행가서 잘 말할 수 있게",
          "original_form": "여행가서 잘 말할 수 있게",
          "speaker_id": "2",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002404.1.18",
          "form": "구랭 그럼 같이 가보자!",
          "original_form": "구랭 그럼 같이 가보자!",
          "speaker_id": "1",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002404.1.19",
          "form": "코로나 끝나고 계획 잡자",
          "original_form": "코로나 끝나고 계획 잡자",
          "speaker_id": "2",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002404.1.20",
          "form": "응응 코로나가 끝나기만 한다면 ㅠㅠ",
          "original_form": "응응 코로나가 끝나기만 한다면 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 11:20"
        }
      ]
