      "utterance": [
        {
          "id": "MDRW2100021474.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100021474.1.2",
          "form": "최근에 영화 뭐 보셨어요?",
          "original_form": "최근에 영화 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100021474.1.3",
          "form": "안녕하세요 ㅎㅎ\n저는 영화관 가서 본건 모가디슈요",
          "original_form": "안녕하세요 ㅎㅎ\n저는 영화관 가서 본건 모가디슈요",
          "speaker_id": "2",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021474.1.4",
          "form": "오! 그거 아직 못봤는데 재밌나요?",
          "original_form": "오! 그거 아직 못봤는데 재밌나요?",
          "speaker_id": "1",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021474.1.5",
          "form": "네 진짜 재밌어요 ㅎㅎ",
          "original_form": "네 진짜 재밌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021474.1.6",
          "form": "전 영화관은 못갈것같아서",
          "original_form": "전 영화관은 못갈것같아서",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021474.1.7",
          "form": "요즘 아프간 사건이랑 비슷한 사건을 다룬 영화라 더 재밌더라구요",
          "original_form": "요즘 아프간 사건이랑 비슷한 사건을 다룬 영화라 더 재밌더라구요",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021474.1.8",
          "form": "vod로 열릴때까지",
          "original_form": "vod로 열릴때까지",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021474.1.9",
          "form": "기다리고있어요ㅠㅠ",
          "original_form": "기다리고있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021474.1.10",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021474.1.11",
          "form": "담에 오티티 나오면 보시면 될거 같아요",
          "original_form": "담에 오티티 나오면 보시면 될거 같아요",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021474.1.12",
          "form": "꼭 보세요!!!",
          "original_form": "꼭 보세요!!!",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021474.1.13",
          "form": "네네!ㅎㅎ 저는 영화관에서 본 마지막 영화가",
          "original_form": "네네!ㅎㅎ 저는 영화관에서 본 마지막 영화가",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021474.1.14",
          "form": "킬러의보디가드2",
          "original_form": "킬러의보디가드2",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021474.1.15",
          "form": "이거에용",
          "original_form": "이거에용",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021474.1.16",
          "form": "킬러의보디가드 이거 재밌나요?",
          "original_form": "킬러의보디가드 이거 재밌나요?",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021474.1.17",
          "form": "네네! 라이언레이놀즈랑 사무엘잭슨 나오는 영화인데",
          "original_form": "네네! 라이언레이놀즈랑 사무엘잭슨 나오는 영화인데",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021474.1.18",
          "form": "둘다 마블 작품에 출연한 배우들이라",
          "original_form": "둘다 마블 작품에 출연한 배우들이라",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021474.1.19",
          "form": "배우 좋아하시면 잘보실거에용",
          "original_form": "배우 좋아하시면 잘보실거에용",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021474.1.20",
          "form": "아 그거 원 괜찮게 본거 같아요! 투도 재밌나요?",
          "original_form": "아 그거 원 괜찮게 본거 같아요! 투도 재밌나요?",
          "speaker_id": "2",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100021474.1.21",
          "form": "원보다는 개인적으로 재미는 덜하지만",
          "original_form": "원보다는 개인적으로 재미는 덜하지만",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100021474.1.22",
          "form": "여전히 재미있어용 ㅡㅋㅋㅋㅋ",
          "original_form": "여전히 재미있어용 ㅡㅋㅋㅋㅋ",
