      "utterance": [
        {
          "id": "MDRW2100024313.1.1",
          "form": "최근에 쇼핑 뭐 하셨어요?",
          "original_form": "최근에 쇼핑 뭐 하셨어요?",
          "speaker_id": "1",
          "time": "20210831 00:35"
        },
        {
          "id": "MDRW2100024313.1.2",
          "form": "저는 청소기 샀는데",
          "original_form": "저는 청소기 샀는데",
          "speaker_id": "1",
          "time": "20210831 00:35"
        },
        {
          "id": "MDRW2100024313.1.3",
          "form": "15만원 주고 샀어요ㅜ근데 너무 무겁",
          "original_form": "15만원 주고 샀어요ㅜ근데 너무 무겁",
          "speaker_id": "1",
          "time": "20210831 00:35"
        },
        {
          "id": "MDRW2100024313.1.4",
          "form": "저는 벌제잡는 거 샀어요!",
          "original_form": "저는 벌제잡는 거 샀어요!",
          "speaker_id": "2",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100024313.1.5",
          "form": "벌레잡는거요! 편의점에 있는 파란빛나는거요!",
          "original_form": "벌레잡는거요! 편의점에 있는 파란빛나는거요!",
          "speaker_id": "2",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100024313.1.6",
          "form": "새장같이생겼는데 빛으로 유인해서 벌레가 많이줄었어요!",
          "original_form": "새장같이생겼는데 빛으로 유인해서 벌레가 많이줄었어요!",
          "speaker_id": "2",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100024313.1.7",
          "form": "아아 벌레 닿으면",
          "original_form": "아아 벌레 닿으면",
          "speaker_id": "1",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100024313.1.8",
          "form": "타닥타닥 하는거?",
          "original_form": "타닥타닥 하는거?",
          "speaker_id": "1",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100024313.1.9",
          "form": "네! 오징어탄내 나는거요",
          "original_form": "네! 오징어탄내 나는거요",
          "speaker_id": "2",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100024313.1.10",
          "form": "밤에 불빛역할도 해줘요 ㅋㅋㅋ",
          "original_form": "밤에 불빛역할도 해줘요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100024313.1.11",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100024313.1.12",
          "form": "그게 집에 필요해요?",
          "original_form": "그게 집에 필요해요?",
          "speaker_id": "1",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100024313.1.13",
          "form": "층이 낮아서 벌레가 많아요...",
          "original_form": "층이 낮아서 벌레가 많아요...",
          "speaker_id": "2",
          "time": "20210831 00:41"
        },
        {
          "id": "MDRW2100024313.1.14",
          "form": "방충망도 허술한거 같고요...ㅠㅠ",
          "original_form": "방충망도 허술한거 같고요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 00:41"
        },
        {
          "id": "MDRW2100024313.1.15",
          "form": "으.....벌레 싫어요",
          "original_form": "으.....벌레 싫어요",
          "speaker_id": "1",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100024313.1.16",
          "form": "효과 좋아요?",
          "original_form": "효과 좋아요?",
          "speaker_id": "1",
          "time": "20210831 00:44"
        },
        {
          "id": "MDRW2100024313.1.17",
          "form": "정말좋아요!! 벌써 30마리 넘게 잡혔어요!",
          "original_form": "정말좋아요!! 벌써 30마리 넘게 잡혔어요!",
          "speaker_id": "2",
          "time": "20210831 01:43"
        },
        {
          "id": "MDRW2100024313.1.18",
          "form": "정말 추천드리는 제품이에요!",
          "original_form": "정말 추천드리는 제품이에요!",
          "speaker_id": "2",
          "time": "20210831 01:43"
        },
        {
          "id": "MDRW2100024313.1.19",
          "form": "저는 그거면 돼요",
          "original_form": "저는 그거면 돼요",
          "speaker_id": "1",
          "time": "20210831 01:51"
        },
        {
          "id": "MDRW2100024313.1.20",
          "form": "전기파리채ㅋㅋㅋㅋ",
          "original_form": "전기파리채ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:51"
        },
        {
          "id": "MDRW2100024313.1.21",
          "form": "저희집에는 날벌레가 너무 많아서......",
          "original_form": "저희집에는 날벌레가 너무 많아서......",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024313.1.22",
          "form": "얼마예요?",
          "original_form": "얼마예요?",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100024313.1.23",
          "form": "만웡대요",
          "original_form": "만웡대요",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100024313.1.24",
          "form": "작은거예요~",
          "original_form": "작은거예요~",
          "speaker_id": "2",
          "time": "20210831 10:25"
        }
      ]
    }
