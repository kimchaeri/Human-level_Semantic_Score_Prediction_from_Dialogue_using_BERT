      "utterance": [
        {
          "id": "MDRW2100005692.1.1",
          "form": "오늘 비많이오던데",
          "original_form": "오늘 비많이오던데",
          "speaker_id": "2",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100005692.1.2",
          "form": "괜찮으셨나요??",
          "original_form": "괜찮으셨나요??",
          "speaker_id": "2",
          "time": "20210824 21:22"
        },
        {
          "id": "MDRW2100005692.1.3",
          "form": "오전에만 잠깐 나갔다오고 내내 집에 있어서 괜찮았어요",
          "original_form": "오전에만 잠깐 나갔다오고 내내 집에 있어서 괜찮았어요",
          "speaker_id": "1",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100005692.1.4",
          "form": "엄청나 님은 괜찮으셨나요?",
          "original_form": "엄청나 님은 괜찮으셨나요?",
          "speaker_id": "1",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100005692.1.5",
          "form": "저도 집에만 있어서 괜찮았어요!",
          "original_form": "저도 집에만 있어서 괜찮았어요!",
          "speaker_id": "2",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100005692.1.6",
          "form": "비가 많이와서그런지 창문을 열어놓으면 시끄럽더라구요 ㅎㅎ",
          "original_form": "비가 많이와서그런지 창문을 열어놓으면 시끄럽더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:25"
        },
        {
          "id": "MDRW2100005692.1.7",
          "form": "맞아요 엄청 쏟아지더라고요",
          "original_form": "맞아요 엄청 쏟아지더라고요",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100005692.1.8",
          "form": "기온도 정말 많이 내려갔더라고요",
          "original_form": "기온도 정말 많이 내려갔더라고요",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100005692.1.9",
          "form": "긴팔 옷 슬슬 꺼내야겠어요",
          "original_form": "긴팔 옷 슬슬 꺼내야겠어요",
          "speaker_id": "2",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100005692.1.10",
          "form": "아 정말요~ 낮 시간 아니면 정말 추워요",
          "original_form": "아 정말요~ 낮 시간 아니면 정말 추워요",
          "speaker_id": "1",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100005692.1.11",
          "form": "맞아요 에어컨켜서 창문닫고 자던게 엊그제같은데",
          "original_form": "맞아요 에어컨켜서 창문닫고 자던게 엊그제같은데",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100005692.1.12",
          "form": "이젠 추워서 창문 닫고 자요 ㅋㅋ",
          "original_form": "이젠 추워서 창문 닫고 자요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100005692.1.13",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100005692.1.14",
          "form": "창문 닫고 이불 덮고ㅋㅋㅋㅋ",
          "original_form": "창문 닫고 이불 덮고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100005692.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ좀 있으면 보일러도 틀겠네요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ좀 있으면 보일러도 틀겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100005692.1.16",
          "form": "아 그러네요 가을은 특히 금방 지나가니까요",
          "original_form": "아 그러네요 가을은 특히 금방 지나가니까요",
          "speaker_id": "1",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100005692.1.17",
          "form": "시간이 너무 빨라요...ㅠㅠ",
          "original_form": "시간이 너무 빨라요...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100005692.1.18",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100005692.1.19",
          "form": "더워지기 시작해서",
          "original_form": "더워지기 시작해서",
          "speaker_id": "2",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100005692.1.20",
          "form": "여름 제발 천천히오라고 했던 게 어제같은데 ㅋㅋㅠ",
          "original_form": "여름 제발 천천히오라고 했던 게 어제같은데 ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100005692.1.21",
          "form": "지옥같던 여름도 끝났네요",
          "original_form": "지옥같던 여름도 끝났네요",
          "speaker_id": "1",
          "time": "20210824 21:37"
        },
        {
          "id": "MDRW2100005692.1.22",
          "form": "너무 더워서 빨리 시원해졌으면하고 바라긴 했는데... 막상 시간이 너무 빨리 가니까 아쉽네요",
          "original_form": "너무 더워서 빨리 시원해졌으면하고 바라긴 했는데... 막상 시간이 너무 빨리 가니까 아쉽네요",
          "speaker_id": "1",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100005692.1.23",
          "form": "정말요",
          "original_form": "정말요",
          "speaker_id": "2",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100005692.1.24",
          "form": "막상 여름이 가니까 올해도 거진 다 간거같아 속상해요",
          "original_form": "막상 여름이 가니까 올해도 거진 다 간거같아 속상해요",
          "speaker_id": "2",
          "time": "20210824 22:09"
        }
      ]
    }
