      "utterance": [
        {
          "id": "MDRW2100033573.1.1",
          "form": "언니가 쓸거같아여 ㅋㅋ",
          "original_form": "언니가 쓸거같아여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100033573.1.2",
          "form": "아 잘못보냈네요 ㅠㅠ ㅋㅋ",
          "original_form": "아 잘못보냈네요 ㅠㅠ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100033573.1.3",
          "form": "아아 네 ㅋㅋ",
          "original_form": "아아 네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100033573.1.4",
          "form": "저도 그부분은 동감해요 ㅋㅋ 저도 자꾸 싸다 싶으면 사니까",
          "original_form": "저도 그부분은 동감해요 ㅋㅋ 저도 자꾸 싸다 싶으면 사니까",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100033573.1.5",
          "form": "네 그렇게되더라구요",
          "original_form": "네 그렇게되더라구요",
          "speaker_id": "2",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100033573.1.6",
          "form": "그냥 그런정보에 아예 노출안되면",
          "original_form": "그냥 그런정보에 아예 노출안되면",
          "speaker_id": "2",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100033573.1.7",
          "form": "근데 또 사놓으면 다 쓰게 되는데",
          "original_form": "근데 또 사놓으면 다 쓰게 되는데",
          "speaker_id": "1",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100033573.1.8",
          "form": "안쓰는것도 있긴하더라구요 ㅋ 그런건 당근으로...ㅋ",
          "original_form": "안쓰는것도 있긴하더라구요 ㅋ 그런건 당근으로...ㅋ",
          "speaker_id": "1",
          "time": "20210824 14:57"
        },
        {
          "id": "MDRW2100033573.1.9",
          "form": "몰라서 못사니까 나중에 세일정보 모르고 비싸게 사도 총 소비액은 적은거같아요",
          "original_form": "몰라서 못사니까 나중에 세일정보 모르고 비싸게 사도 총 소비액은 적은거같아요",
          "speaker_id": "2",
          "time": "20210824 14:57"
        },
        {
          "id": "MDRW2100033573.1.10",
          "form": "네 ㅋㅋ 당근 좋죠",
          "original_form": "네 ㅋㅋ 당근 좋죠",
          "speaker_id": "2",
          "time": "20210824 14:57"
        },
        {
          "id": "MDRW2100033573.1.11",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210824 14:57"
        },
        {
          "id": "MDRW2100033573.1.12",
          "form": "그거보면 광고를 왜하는지 알거같아요",
          "original_form": "그거보면 광고를 왜하는지 알거같아요",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100033573.1.13",
          "form": "사람 꼬시는거죠",
          "original_form": "사람 꼬시는거죠",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100033573.1.14",
          "form": "광고보면 다 좋아보이고 ㅋㅋ",
          "original_form": "광고보면 다 좋아보이고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100033573.1.15",
          "form": "그니까요 ㅋㅋ",
          "original_form": "그니까요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100033573.1.16",
          "form": "요즘엔 리뷰들도 다 그래서",
          "original_form": "요즘엔 리뷰들도 다 그래서",
          "speaker_id": "2",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100033573.1.17",
          "form": "가끔 진짜 빙 둘러서 말해주는 분 있구요\r\n그게 다 작성가이드가 있던데요",
          "original_form": "가끔 진짜 빙 둘러서 말해주는 분 있구요\r\n그게 다 작성가이드가 있던데요",
          "speaker_id": "2",
          "time": "20210824 15:47"
        },
        {
          "id": "MDRW2100033573.1.18",
          "form": "저도 리뷰는 사실 너무 이상한거 아니면 거의 좋게 쓰긴해요",
          "original_form": "저도 리뷰는 사실 너무 이상한거 아니면 거의 좋게 쓰긴해요",
          "speaker_id": "1",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100033573.1.19",
          "form": "네 그죠",
          "original_form": "네 그죠",
          "speaker_id": "2",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100033573.1.20",
          "form": "리뷰읽는 문해력이 필요하더라구요 ㅋㅋ",
          "original_form": "리뷰읽는 문해력이 필요하더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100033573.1.21",
          "form": "블로그는 거의ㅠㅠ 협찬이라 패스.하구요 ㅋㅋ",
          "original_form": "블로그는 거의ㅠㅠ 협찬이라 패스.하구요 ㅋㅋ",
          "speaker_id": "1",
