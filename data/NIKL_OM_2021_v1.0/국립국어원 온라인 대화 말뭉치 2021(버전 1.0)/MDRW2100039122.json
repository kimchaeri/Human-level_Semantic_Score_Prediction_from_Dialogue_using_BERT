        {
          "id": "MDRW2100039122.1.1",
          "form": "오늘은 일안해요?",
          "original_form": "오늘은 일안해요?",
          "speaker_id": "1",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100039122.1.2",
          "form": "할라구용",
          "original_form": "할라구용",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100039122.1.3",
          "form": "name1님은여",
          "original_form": "&name1&님은여",
          "speaker_id": "2",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100039122.1.4",
          "form": "뭐해요?",
          "original_form": "뭐해요?",
          "speaker_id": "1",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100039122.1.5",
          "form": "전 좀있다",
          "original_form": "전 좀있다",
          "speaker_id": "1",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100039122.1.6",
          "form": "주식공부좀하려구요",
          "original_form": "주식공부좀하려구요",
          "speaker_id": "1",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100039122.1.7",
          "form": "와~~",
          "original_form": "와~~",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100039122.1.8",
          "form": "대단하당",
          "original_form": "대단하당",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039122.1.9",
          "form": "아니에요",
          "original_form": "아니에요",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039122.1.10",
          "form": "나논 영상편집허요",
          "original_form": "나논 영상편집허요",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039122.1.11",
          "form": "name2님도 공모주공부는 하면좋겠어요",
          "original_form": "&name2&님도 공모주공부는 하면좋겠어요",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039122.1.12",
          "form": "이건 소액으로",
          "original_form": "이건 소액으로",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039122.1.13",
          "form": "공보주가?",
          "original_form": "공보주가?",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039122.1.14",
          "form": "중국집술이름이시",
          "original_form": "중국집술이름이시",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039122.1.15",
          "form": "이주에 한번씩 치킨값만 버는거라",
          "original_form": "이주에 한번씩 치킨값만 버는거라",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039122.1.16",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100039122.1.17",
          "form": "관심있으시면 주말에 갈켜드릴게요",
          "original_form": "관심있으시면 주말에 갈켜드릴게요",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100039122.1.18",
          "form": "조으네요 이주한번",
          "original_form": "조으네요 이주한번",
          "speaker_id": "2",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100039122.1.19",
          "form": "치킨먹는거야요?",
          "original_form": "치킨먹는거야요?",
          "speaker_id": "2",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100039122.1.20",
          "form": "소액으로 하는거라 괜찮아요",
          "original_form": "소액으로 하는거라 괜찮아요",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100039122.1.21",
          "form": "고액은 부담스럽고요",
          "original_form": "고액은 부담스럽고요",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100039122.1.22",
          "form": "없기도 하고요",
          "original_form": "없기도 하고요",
          "speaker_id": "1",
          "time": "20210903 10:48"
        }
      ]
