      "utterance": [
        {
          "id": "MDRW2100024820.1.1",
          "form": "저는 카톨릭이에요",
          "original_form": "저는 카톨릭이에요",
          "speaker_id": "2",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024820.1.2",
          "form": "무늬만?",
          "original_form": "무늬만?",
          "speaker_id": "2",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024820.1.3",
          "form": "그러시군요 세례명도 있나요",
          "original_form": "그러시군요 세례명도 있나요",
          "speaker_id": "1",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024820.1.4",
          "form": "센스있으신분",
          "original_form": "센스있으신분",
          "speaker_id": "2",
          "time": "20210831 19:22"
        },
        {
          "id": "MDRW2100024820.1.5",
          "form": "오 제 세례명을 물어봐주시다니",
          "original_form": "오 제 세례명을 물어봐주시다니",
          "speaker_id": "2",
          "time": "20210831 19:22"
        },
        {
          "id": "MDRW2100024820.1.6",
          "form": "제 세례명예뻐요",
          "original_form": "제 세례명예뻐요",
          "speaker_id": "2",
          "time": "20210831 19:22"
        },
        {
          "id": "MDRW2100024820.1.7",
          "form": "플로라 입니다",
          "original_form": "플로라 입니다",
          "speaker_id": "2",
          "time": "20210831 19:23"
        },
        {
          "id": "MDRW2100024820.1.8",
          "form": "오 정말 예뻐요!",
          "original_form": "오 정말 예뻐요!",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024820.1.9",
          "form": "세례명은 보통 어떻게 정하나요?",
          "original_form": "세례명은 보통 어떻게 정하나요?",
          "speaker_id": "1",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100024820.1.10",
          "form": "생일 에 맞춰서 정하는거에요",
          "original_form": "생일 에 맞춰서 정하는거에요",
          "speaker_id": "2",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024820.1.11",
          "form": "별자리처러묘",
          "original_form": "별자리처러묘",
          "speaker_id": "2",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024820.1.12",
          "form": "처러묘",
          "original_form": "처러묘",
          "speaker_id": "2",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024820.1.13",
          "form": "보통 그날 순교하신거라하더라고요",
          "original_form": "보통 그날 순교하신거라하더라고요",
          "speaker_id": "2",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100024820.1.14",
          "form": "아 어머 신기해요!! 처음 알게됐네요!!!",
          "original_form": "아 어머 신기해요!! 처음 알게됐네요!!!",
          "speaker_id": "1",
          "time": "20210831 19:41"
        },
        {
          "id": "MDRW2100024820.1.15",
          "form": "계속 종교가 없으셨던건가요",
          "original_form": "계속 종교가 없으셨던건가요",
          "speaker_id": "2",
          "time": "20210831 19:44"
        },
        {
          "id": "MDRW2100024820.1.16",
          "form": "아 저는 기독교예요",
          "original_form": "아 저는 기독교예요",
          "speaker_id": "1",
          "time": "20210831 19:44"
        },
        {
          "id": "MDRW2100024820.1.17",
          "form": "요즘 교회는 어떻게되나요?",
          "original_form": "요즘 교회는 어떻게되나요?",
          "speaker_id": "2",
          "time": "20210831 19:45"
        },
        {
          "id": "MDRW2100024820.1.18",
          "form": "예배 못보고있나요?",
          "original_form": "예배 못보고있나요?",
          "speaker_id": "2",
          "time": "20210831 19:46"
        },
        {
          "id": "MDRW2100024820.1.19",
          "form": "그렇죠 영상예배로 한지 오래됐어요\n카톨릭은 어떨까요?",
          "original_form": "그렇죠 영상예배로 한지 오래됐어요\n카톨릭은 어떨까요?",
          "speaker_id": "1",
          "time": "20210831 19:47"
        },
        {
          "id": "MDRW2100024820.1.20",
          "form": "저흰 아예 폐쇄가 되었고 성당이",
          "original_form": "저흰 아예 폐쇄가 되었고 성당이",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024820.1.21",
          "form": "이후부턴 그냥 집에서 엄마 혼자 묵주기도 드리고계세요",
          "original_form": "이후부턴 그냥 집에서 엄마 혼자 묵주기도 드리고계세요",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024820.1.22",
          "form": "그렇군요 코로나로 예배참석도 어렵고 종교활동도 많이 달라지는듯해요",
          "original_form": "그렇군요 코로나로 예배참석도 어렵고 종교활동도 많이 달라지는듯해요",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024820.1.23",
          "form": "저는 사실 저 필요할때만 하느님 찾는거같아서 가끔 죄송해요ㅜ",
          "original_form": "저는 사실 저 필요할때만 하느님 찾는거같아서 가끔 죄송해요ㅜ",
          "speaker_id": "2",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100024820.1.24",
          "form": "그리고 기도도 남을위한기도도해야하는데",
          "original_form": "그리고 기도도 남을위한기도도해야하는데",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024820.1.25",
          "form": "그렇지도 못하고",
          "original_form": "그렇지도 못하고",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024820.1.26",
          "form": "하..그렇게 말씀하시니 저도 부끄러워집니다 ㅜㅜ",
          "original_form": "하..그렇게 말씀하시니 저도 부끄러워집니다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 21:27"
        }
      ]
    }
