      "utterance": [
        {
          "id": "MDRW2100045782.1.1",
          "form": "정확히는 요리도 품고 있는 거죠!",
          "original_form": "정확히는 요리도 품고 있는 거죠!",
          "speaker_id": "2",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100045782.1.2",
          "form": "으으으음 요리 품은 조리..",
          "original_form": "으으으음 요리 품은 조리..",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100045782.1.3",
          "form": "힌트를 드리자면 저희가 밖에서 사먹는걸 뭐라고하죠?",
          "original_form": "힌트를 드리자면 저희가 밖에서 사먹는걸 뭐라고하죠?",
          "speaker_id": "2",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100045782.1.4",
          "form": "아 외식",
          "original_form": "아 외식",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045782.1.5",
          "form": "외식 외식 외식..학과?",
          "original_form": "외식 외식 외식..학과?",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045782.1.6",
          "form": "외식 땡땡 학과인가요?",
          "original_form": "외식 땡땡 학과인가요?",
          "speaker_id": "1",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045782.1.7",
          "form": "네!! 맞아요!!",
          "original_form": "네!! 맞아요!!",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045782.1.8",
          "form": "외식조리학과..?",
          "original_form": "외식조리학과..?",
          "speaker_id": "1",
          "time": "20210908 07:11"
        },
        {
          "id": "MDRW2100045782.1.9",
          "form": "아쉽지만 땽!!!!",
          "original_form": "아쉽지만 땽!!!!",
          "speaker_id": "2",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100045782.1.10",
          "form": "그래도 많이 맞추셨어요ㅠㅠ 외식학과까지 맞추셨으니까요!",
          "original_form": "그래도 많이 맞추셨어요ㅠㅠ 외식학과까지 맞추셨으니까요!",
          "speaker_id": "2",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100045782.1.11",
          "form": "으아아아 외식 뭐지",
          "original_form": "으아아아 외식 뭐지",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100045782.1.12",
          "form": "두 글자인가요?",
          "original_form": "두 글자인가요?",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100045782.1.13",
          "form": "외식 땡땡 학과?",
          "original_form": "외식 땡땡 학과?",
          "speaker_id": "1",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100045782.1.14",
          "form": "맞아요! 두글자!!!",
          "original_form": "맞아요! 두글자!!!",
          "speaker_id": "2",
          "time": "20210908 07:13"
        },
        {
          "id": "MDRW2100045782.1.15",
          "form": "아 뭐지 힌트!!!!!",
          "original_form": "아 뭐지 힌트!!!!!",
          "speaker_id": "1",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045782.1.16",
          "form": "아 힌트짐짜 너무 어렵다ㅠㅠ 사회시간에 저희는 배웠죠!!!",
          "original_form": "아 힌트짐짜 너무 어렵다ㅠㅠ 사회시간에 저희는 배웠죠!!!",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045782.1.17",
          "form": "1차 2차 3차!!",
          "original_form": "1차 2차 3차!!",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045782.1.18",
          "form": "사람이 하던걸 기계가 하게 됐죠!!",
          "original_form": "사람이 하던걸 기계가 하게 됐죠!!",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
