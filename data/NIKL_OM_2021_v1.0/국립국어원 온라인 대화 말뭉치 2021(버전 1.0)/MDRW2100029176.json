      "utterance": [
        {
          "id": "MDRW2100029176.1.1",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "1",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029176.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100029176.1.3",
          "form": "여행 다니는거 좋아하세요??",
          "original_form": "여행 다니는거 좋아하세요??",
          "speaker_id": "1",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029176.1.4",
          "form": "네 좋아해요ㅎㅎ 여행 못 간지 오래되었지만요ㅜㅜ",
          "original_form": "네 좋아해요ㅎㅎ 여행 못 간지 오래되었지만요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029176.1.5",
          "form": "저두요ㅠㅠ 전 혼자가는것도 진짜 좋아하는데 요즘은 집앞 한번 나가는걸로 만족중이에요...ㅋㅋㅠ",
          "original_form": "저두요ㅠㅠ 전 혼자가는것도 진짜 좋아하는데 요즘은 집앞 한번 나가는걸로 만족중이에요...ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029176.1.6",
          "form": "오 여행 혼자서도 다니시는군요!!",
          "original_form": "오 여행 혼자서도 다니시는군요!!",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100029176.1.7",
          "form": "가아끔씩 그냥 국내에서만 왔다갔다해요ㅎㅎ",
          "original_form": "가아끔씩 그냥 국내에서만 왔다갔다해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029176.1.8",
          "form": "어디 여행가보셨어요??",
          "original_form": "어디 여행가보셨어요??",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029176.1.9",
          "form": "전 국내만 여행다녀봤는데 \n지금 떠오르는건 서울 가평 파주 전주 충주 해남 정도 가본거 같아요 \n강원도 지역도 대부분은 다 가본것 같구요",
          "original_form": "전 국내만 여행다녀봤는데 \n지금 떠오르는건 서울 가평 파주 전주 충주 해남 정도 가본거 같아요 \n강원도 지역도 대부분은 다 가본것 같구요",
          "speaker_id": "2",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029176.1.10",
          "form": "와 엄청 많이 가셨는데요???",
          "original_form": "와 엄청 많이 가셨는데요???",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029176.1.11",
          "form": "저는 거의...아랫지역만 다녔어요",
          "original_form": "저는 거의...아랫지역만 다녔어요",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029176.1.12",
          "form": "바다보는걸 좋아해서 동해남해 많이 간거같아요ㅋㅋ",
          "original_form": "바다보는걸 좋아해서 동해남해 많이 간거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029176.1.13",
          "form": "근데 저는 길게 다니기보다 짧게 다녀왔던 곳이 많아요ㅎㅎ",
          "original_form": "근데 저는 길게 다니기보다 짧게 다녀왔던 곳이 많아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029176.1.14",
          "form": "바다 좋아하시는군요!",
          "original_form": "바다 좋아하시는군요!",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029176.1.15",
          "form": "네!! 약간 쌀쌀할때 바닷바람이 너무 시원해서 괜히 가서 멍때리는거 좋아해요ㅋㅋ",
          "original_form": "네!! 약간 쌀쌀할때 바닷바람이 너무 시원해서 괜히 가서 멍때리는거 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029176.1.16",
          "form": "해외로는 가고싶은곳 없으세요??",
          "original_form": "해외로는 가고싶은곳 없으세요??",
          "speaker_id": "1",
          "time": "20210903 00:27"
        },
        {
          "id": "MDRW2100029176.1.17",
          "form": "있죠! 저 유럽쪽 가보고 싶어요",
          "original_form": "있죠! 저 유럽쪽 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029176.1.18",
          "form": "헉 저두요!! 일본이랑 중국 같은 근처만 가서",
          "original_form": "헉 저두요!! 일본이랑 중국 같은 근처만 가서",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029176.1.19",
          "form": "코로나 끝나면 진짜 가야지...하고 벼르고있어요",
          "original_form": "코로나 끝나면 진짜 가야지...하고 벼르고있어요",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029176.1.20",
          "form": "사실 영어도 잘못하지만ㅋㅋㅠ 그래도 가고싶어요",
          "original_form": "사실 영어도 잘못하지만ㅋㅋㅠ 그래도 가고싶어요",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029176.1.21",
          "form": "저도 영어 못 해요ㅋㅋㅋ",
          "original_form": "저도 영어 못 해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029176.1.22",
          "form": "하지만 예전부터 해외여행 생각하면 유럽이 가고 싶더라구요ㅎㅎ",
          "original_form": "하지만 예전부터 해외여행 생각하면 유럽이 가고 싶더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:35"
        }
      ]
    }
