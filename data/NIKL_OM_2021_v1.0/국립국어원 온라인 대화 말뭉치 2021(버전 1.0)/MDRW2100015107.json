      "utterance": [
        {
          "id": "MDRW2100015107.1.1",
          "form": "미용 필요없다",
          "original_form": "미용 필요없다",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015107.1.2",
          "form": "왜 포메는 곰돌이컷 하면 넘 귀엽잖아",
          "original_form": "왜 포메는 곰돌이컷 하면 넘 귀엽잖아",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015107.1.3",
          "form": "귀엽기만 하지 관리하기 어렵다",
          "original_form": "귀엽기만 하지 관리하기 어렵다",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015107.1.4",
          "form": "아닌뎅 자른대로 깔끔하게 자란다",
          "original_form": "아닌뎅 자른대로 깔끔하게 자란다",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015107.1.5",
          "form": "됐다 비싸다",
          "original_form": "됐다 비싸다",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015107.1.6",
          "form": "ㅠㅠ그래 비싸다고 하긴하더랑 ㅋ",
          "original_form": "ㅠㅠ그래 비싸다고 하긴하더랑 ㅋ",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015107.1.7",
          "form": "가위로 일일이 자르는 거라서 그렇다",
          "original_form": "가위로 일일이 자르는 거라서 그렇다",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015107.1.8",
          "form": "그르게..내가 배워서 잘라줄까?ㅋ",
          "original_form": "그르게..내가 배워서 잘라줄까?ㅋ",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015107.1.9",
          "form": "배울게 뭐 있노 그냥 잘라 봐라",
          "original_form": "배울게 뭐 있노 그냥 잘라 봐라",
          "speaker_id": "2",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015107.1.10",
          "form": "ㅋㅋ그러다 망한당 우리 강아지들 삐뚤빼뚤해진당",
          "original_form": "ㅋㅋ그러다 망한당 우리 강아지들 삐뚤빼뚤해진당",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015107.1.11",
          "form": "그러면 그냥 놔둬라",
          "original_form": "그러면 그냥 놔둬라",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015107.1.12",
          "form": "ㅠㅠ 구냥 내가 몰래 델꼬가서 잘라올거얌",
          "original_form": "ㅠㅠ 구냥 내가 몰래 델꼬가서 잘라올거얌",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015107.1.13",
          "form": "안 된다 털 아깝다",
          "original_form": "안 된다 털 아깝다",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100015107.1.14",
          "form": "쳇",
          "original_form": "쳇",
