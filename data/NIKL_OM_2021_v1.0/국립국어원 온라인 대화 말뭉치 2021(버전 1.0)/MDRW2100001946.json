      "utterance": [
        {
          "id": "MDRW2100001946.1.1",
          "form": "최근에 올림픽 경기 봤어?? 스포츠에 관심 생기더라",
          "original_form": "최근에 올림픽 경기 봤어?? 스포츠에 관심 생기더라",
          "speaker_id": "1",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001946.1.2",
          "form": "완전 챙겨봄 ㅜㅜ",
          "original_form": "완전 챙겨봄 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 23:08"
        },
        {
          "id": "MDRW2100001946.1.3",
          "form": "특히 배구.....",
          "original_form": "특히 배구.....",
          "speaker_id": "2",
          "time": "20210819 23:08"
        },
        {
          "id": "MDRW2100001946.1.4",
          "form": "10월에 리그 개막이라더라구",
          "original_form": "10월에 리그 개막이라더라구",
          "speaker_id": "2",
          "time": "20210819 23:08"
        },
        {
          "id": "MDRW2100001946.1.5",
          "form": "맞아 나 안그래도 배구 보러 가려고 고민중",
          "original_form": "맞아 나 안그래도 배구 보러 가려고 고민중",
          "speaker_id": "1",
          "time": "20210819 23:09"
        },
        {
          "id": "MDRW2100001946.1.6",
          "form": "나도 갈 마음 있어",
          "original_form": "나도 갈 마음 있어",
          "speaker_id": "2",
          "time": "20210819 23:09"
        },
        {
          "id": "MDRW2100001946.1.7",
          "form": "희진언니 봐야돼",
          "original_form": "희진언니 봐야돼",
          "speaker_id": "2",
          "time": "20210819 23:09"
        },
        {
          "id": "MDRW2100001946.1.8",
          "form": "진짜 ㅠ 이번에 국대 유니폼 샀는데 그거 오면 직관 바로 가고 싶다",
          "original_form": "진짜 ㅠ 이번에 국대 유니폼 샀는데 그거 오면 직관 바로 가고 싶다",
          "speaker_id": "1",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001946.1.9",
          "form": "서울이면 잠실쪽 가야하나",
          "original_form": "서울이면 잠실쪽 가야하나",
          "speaker_id": "1",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001946.1.10",
          "form": "아 유니폼 플렉스 했구나 ㅋㅋㅋㅋ",
          "original_form": "아 유니폼 플렉스 했구나 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001946.1.11",
          "form": "궁금.. 내일 본격적으로 좀 찾아볼까봐",
          "original_form": "궁금.. 내일 본격적으로 좀 찾아볼까봐",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001946.1.12",
          "form": "올림픽 끝났는데..유니폼 안오는거 실화니 ㅜ",
          "original_form": "올림픽 끝났는데..유니폼 안오는거 실화니 ㅜ",
          "speaker_id": "1",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001946.1.13",
          "form": "아 ㅋㅋㅋㅋ 주문 폭주인가봐 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ 주문 폭주인가봐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001946.1.14",
          "form": "선수 이름 써있는거야??",
          "original_form": "선수 이름 써있는거야??",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001946.1.15",
          "form": "맞아 주문제작이라서 그런가봐",
          "original_form": "맞아 주문제작이라서 그런가봐",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001946.1.16",
          "form": "연경언니 번호랑 이름 써있는거로 주문했어",
          "original_form": "연경언니 번호랑 이름 써있는거로 주문했어",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001946.1.17",
          "form": "오오 굿 ㅋㅋㅋㅋ",
          "original_form": "오오 굿 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100001946.1.18",
          "form": "그거 입고 경기 보러가면 굿이다",
          "original_form": "그거 입고 경기 보러가면 굿이다",
          "speaker_id": "2",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100001946.1.19",
          "form": "그치 ㅎㅎㅎ 바로 경기장 달려간다ㅋㅋㅋㅋㅋ",
          "original_form": "그치 ㅎㅎㅎ 바로 경기장 달려간다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:18"
        },
        {
          "id": "MDRW2100001946.1.20",
          "form": "배구는 직접보러 간적이 없어서 기대",
          "original_form": "배구는 직접보러 간적이 없어서 기대",
          "speaker_id": "1",
          "time": "20210819 23:18"
        },
        {
          "id": "MDRW2100001946.1.21",
          "form": "나도 ㅋㅋㅋㅋ",
          "original_form": "나도 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100001946.1.22",
          "form": "주변에 가본적 있는 사람한테 물어봐야지...",
          "original_form": "주변에 가본적 있는 사람한테 물어봐야지...",
          "speaker_id": "2",
          "time": "20210819 23:20"
        },
        {
