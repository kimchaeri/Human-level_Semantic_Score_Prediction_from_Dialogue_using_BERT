      "utterance": [
        {
          "id": "MDRW2100005486.1.1",
          "form": "쇼핑좋아하시나요",
          "original_form": "쇼핑좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005486.1.2",
          "form": "네 너무좋아하는데 돈이 ㅎㅎ",
          "original_form": "네 너무좋아하는데 돈이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005486.1.3",
          "form": "얼마전에 워치4 샀어요 기대되요",
          "original_form": "얼마전에 워치4 샀어요 기대되요",
          "speaker_id": "2",
          "time": "20210821 23:44"
        },
        {
          "id": "MDRW2100005486.1.4",
          "form": "워치3산지 얼마안되서 4출시했더라고요",
          "original_form": "워치3산지 얼마안되서 4출시했더라고요",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005486.1.5",
          "form": "가격도 엄청내렸고ㅠㅠ",
          "original_form": "가격도 엄청내렸고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005486.1.6",
          "form": "네 가격땜에 4샀어요",
          "original_form": "네 가격땜에 4샀어요",
          "speaker_id": "2",
          "time": "20210821 23:45"
        },
        {
          "id": "MDRW2100005486.1.7",
          "form": "마음 좀 안좋으시겠어요 ㅜㅠ\r\n사신지 얼마안되셨으면",
          "original_form": "마음 좀 안좋으시겠어요 ㅜㅠ\r\n사신지 얼마안되셨으면",
          "speaker_id": "2",
          "time": "20210821 23:46"
        },
        {
          "id": "MDRW2100005486.1.8",
          "form": "워치 좋긴한가요?",
          "original_form": "워치 좋긴한가요?",
          "speaker_id": "2",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005486.1.9",
          "form": "괜찮아요ㅋㅋ 근데 비싼만보기산듯",
          "original_form": "괜찮아요ㅋㅋ 근데 비싼만보기산듯",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005486.1.10",
          "form": "굳이필요했나는 싶어요",
          "original_form": "굳이필요했나는 싶어요",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005486.1.11",
          "form": "간지는나요ㅋㅋ",
          "original_form": "간지는나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:47"
        },
        {
          "id": "MDRW2100005486.1.12",
          "form": "비싼만보기 ㅋㅋ 딱인데요 ㅎ",
          "original_form": "비싼만보기 ㅋㅋ 딱인데요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 23:48"
        },
        {
          "id": "MDRW2100005486.1.13",
          "form": "버즈는 있으세요?",
          "original_form": "버즈는 있으세요?",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005486.1.14",
          "form": "네 버즈2있어용",
          "original_form": "네 버즈2있어용",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005486.1.15",
          "form": "20년도 2월에 샀어요",
          "original_form": "20년도 2월에 샀어요",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005486.1.16",
          "form": "이번에 나온게 버즈2  아니예요?",
          "original_form": "이번에 나온게 버즈2  아니예요?",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005486.1.17",
          "form": "버즈 프로 살까 지금 나온거 사야되나 고민중이예요",
          "original_form": "버즈 프로 살까 지금 나온거 사야되나 고민중이예요",
          "speaker_id": "2",
          "time": "20210821 23:50"
        },
        {
          "id": "MDRW2100005486.1.18",
          "form": "아 그럼 제꺼는 버즈플러스인가봐",
          "original_form": "아 그럼 제꺼는 버즈플러스인가봐",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005486.1.19",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005486.1.20",
          "form": "아...좋은가요? 잘빠지지않아요?",
          "original_form": "아...좋은가요? 잘빠지지않아요?",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005486.1.21",
          "form": "잘 안빠져요",
          "original_form": "잘 안빠져요",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005486.1.22",
          "form": "근데 잘 안써서 당근에 팔았어요",
          "original_form": "근데 잘 안써서 당근에 팔았어요",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100005486.1.23",
          "form": "당근 너무 좋죠 ㅎ",
          "original_form": "당근 너무 좋죠 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005486.1.24",
          "form": "저도 당근 잘 이용해요",
          "original_form": "저도 당근 잘 이용해요",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100005486.1.25",
          "form": "주로 모 봐요?",
          "original_form": "주로 모 봐요?",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100005486.1.26",
          "form": "저는 주로 육아용품 봐요",
          "original_form": "저는 주로 육아용품 봐요",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100005486.1.27",
          "form": "저두요 육아용품만요 ㅎ",
          "original_form": "저두요 육아용품만요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100005486.1.28",
          "form": "보통 출산전에 애기용품 왕창사죠?",
          "original_form": "보통 출산전에 애기용품 왕창사죠?",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100005486.1.29",
          "form": "안쓰는거 천지기도하고",
          "original_form": "안쓰는거 천지기도하고",
          "speaker_id": "1",
          "time": "20210822 01:58"
        },
        {
          "id": "MDRW2100005486.1.30",
          "form": "쓰다가 둘째계획없어서 팔기도해요",
          "original_form": "쓰다가 둘째계획없어서 팔기도해요",
          "speaker_id": "1",
          "time": "20210822 01:58"
        }
      ]
    }
