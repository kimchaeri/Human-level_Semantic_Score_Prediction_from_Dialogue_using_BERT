      "utterance": [
        {
          "id": "MDRW2100000773.1.1",
          "form": "영화 보세요?",
          "original_form": "영화 보세요?",
          "speaker_id": "1",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100000773.1.2",
          "form": "아이들이 어려서 잘못봐요ㅜㅜ",
          "original_form": "아이들이 어려서 잘못봐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100000773.1.3",
          "form": "이하늬 나왔던 극한의 직업을 틀어놨는데",
          "original_form": "이하늬 나왔던 극한의 직업을 틀어놨는데",
          "speaker_id": "2",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100000773.1.4",
          "form": "욕을따라해서 껐네요 ㅍㅎㅎ",
          "original_form": "욕을따라해서 껐네요 ㅍㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100000773.1.5",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000773.1.6",
          "form": "정말 웃겼는디",
          "original_form": "정말 웃겼는디",
          "speaker_id": "1",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000773.1.7",
          "form": "그쵸 너무 재밌게 봤었어요^^  초보운전 스티커도 나와서 빵터졌었는데",
          "original_form": "그쵸 너무 재밌게 봤었어요^^  초보운전 스티커도 나와서 빵터졌었는데",
          "speaker_id": "2",
          "time": "20210819 19:14"
        },
        {
          "id": "MDRW2100000773.1.8",
          "form": "요즘 영화는 뭐가 재밌나요^^",
          "original_form": "요즘 영화는 뭐가 재밌나요^^",
          "speaker_id": "2",
          "time": "20210819 19:15"
        },
        {
          "id": "MDRW2100000773.1.9",
          "form": "모가디슈 잼나다거 하던데ㅇ요",
          "original_form": "모가디슈 잼나다거 하던데ㅇ요",
          "speaker_id": "1",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000773.1.10",
          "form": "아하! 모가디슈 예고편 봤었어요!",
          "original_form": "아하! 모가디슈 예고편 봤었어요!",
          "speaker_id": "2",
          "time": "20210819 19:16"
        },
        {
          "id": "MDRW2100000773.1.11",
          "form": "저도 예고편맘",
          "original_form": "저도 예고편맘",
          "speaker_id": "1",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000773.1.12",
          "form": "도라에몽도 예고편만 봤는데",
          "original_form": "도라에몽도 예고편만 봤는데",
          "speaker_id": "2",
          "time": "20210819 19:20"
        },
        {
          "id": "MDRW2100000773.1.13",
          "form": "어른을 위한 애니메이션 같았어요! 네이버 평점이 좋으네요 ^^",
          "original_form": "어른을 위한 애니메이션 같았어요! 네이버 평점이 좋으네요 ^^",
          "speaker_id": "2",
          "time": "20210819 19:20"
        },
        {
          "id": "MDRW2100000773.1.14",
          "form": "미니특공대 많이 봤어요",
          "original_form": "미니특공대 많이 봤어요",
          "speaker_id": "1",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000773.1.15",
          "form": "미니특공대는 본적없는데 그것도 애니메이션인가봐요 ㅎㅎ",
          "original_form": "미니특공대는 본적없는데 그것도 애니메이션인가봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 19:22"
        },
        {
          "id": "MDRW2100000773.1.16",
          "form": "네 얘들이 좋아해요",
          "original_form": "네 얘들이 좋아해요",
          "speaker_id": "1",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000773.1.17",
          "form": "오^^ 혹시 애니메이션  더 추천해주실거 있나요?ㅎㅎ",
          "original_form": "오^^ 혹시 애니메이션  더 추천해주실거 있나요?ㅎㅎ",
