        {
          "id": "MDRW2100045809.1.2",
          "form": "이제 가을옷 사려고",
          "original_form": "이제 가을옷 사려고",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045809.1.3",
          "form": "긴팔 셔츠?",
          "original_form": "긴팔 셔츠?",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045809.1.4",
          "form": "맞아 긴팔 좀 필요해",
          "original_form": "맞아 긴팔 좀 필요해",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045809.1.5",
          "form": "ㅋㅋㅋ 후드는 그만 사",
          "original_form": "ㅋㅋㅋ 후드는 그만 사",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045809.1.6",
          "form": "ㅜㅜ 후드가 얼마나 편한데",
          "original_form": "ㅜㅜ 후드가 얼마나 편한데",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045809.1.7",
          "form": "너무 많으니까 그렇지 ㅋㅋㅋ",
          "original_form": "너무 많으니까 그렇지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045809.1.8",
          "form": "그건 그래 ㅋㅋ 셔츠나 사야겠다",
          "original_form": "그건 그래 ㅋㅋ 셔츠나 사야겠다",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045809.1.9",
          "form": "나는 장갑 살거야",
          "original_form": "나는 장갑 살거야",
          "speaker_id": "2",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100045809.1.10",
          "form": "ㅋㅋㅋ 겨울대비?",
          "original_form": "ㅋㅋㅋ 겨울대비?",
          "speaker_id": "1",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100045809.1.11",
          "form": "응응 장갑이 하나도 없어",
          "original_form": "응응 장갑이 하나도 없어",
          "speaker_id": "2",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100045809.1.12",
          "form": "폰 터치 되는거로 사 ㅋㅋ",
          "original_form": "폰 터치 되는거로 사 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100045809.1.13",
          "form": "ㅋㅋㅋ못생겼던데 그런건",
          "original_form": "ㅋㅋㅋ못생겼던데 그런건",
          "speaker_id": "2",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100045809.1.14",
