        },
        {
          "id": "MDRW2100044897.1.2",
          "form": "내가 여성향은 진짜 엥간해선 안까는데..",
          "original_form": "내가 여성향은 진짜 엥간해선 안까는데..",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044897.1.3",
          "form": "ㅇㅇ.......",
          "original_form": "ㅇㅇ.......",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044897.1.4",
          "form": "근데 이건 진짜 좀 심해ㅋㅋ;;;",
          "original_form": "근데 이건 진짜 좀 심해ㅋㅋ;;;",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044897.1.5",
          "form": "그치 ㅋㅋㅋㅋㅋ 이정도는 진짜 처음봄",
          "original_form": "그치 ㅋㅋㅋㅋㅋ 이정도는 진짜 처음봄",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044897.1.6",
          "form": "그렇게 욕먹던 애프터라이프도 이정돈 아녓음",
          "original_form": "그렇게 욕먹던 애프터라이프도 이정돈 아녓음",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044897.1.7",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ애프터라이프도",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ애프터라이프도",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044897.1.8",
          "form": "1년은 운영함",
          "original_form": "1년은 운영함",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044897.1.9",
          "form": "얘넨 한달이나 할런지 모르겠네",
          "original_form": "얘넨 한달이나 할런지 모르겠네",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044897.1.10",
          "form": "놀랍게도",
          "original_form": "놀랍게도",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044897.1.11",
          "form": "벌써 거의 3주 지났을걸? ㅋㅋㅋㅋㅋ",
          "original_form": "벌써 거의 3주 지났을걸? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044897.1.12",
          "form": "그치만 지금도 반쯤",
          "original_form": "그치만 지금도 반쯤",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044897.1.13",
          "form": "버린 기분인데ㅋㅋㅋㅋㄱㅋ",
          "original_form": "버린 기분인데ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044897.1.14",
          "form": "ㅇㅇ 점검만 하다가 3주 지남 ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 점검만 하다가 3주 지남 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044897.1.15",
          "form": "진짜 실망스럽다",
          "original_form": "진짜 실망스럽다",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044897.1.16",
          "form": "ㄹㅇ난 기대도 안했거든?",
          "original_form": "ㄹㅇ난 기대도 안했거든?",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044897.1.17",
          "form": "근데ㅎ",
          "original_form": "근데ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044897.1.18",
          "form": "그정도도",
          "original_form": "그정도도",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044897.1.19",
          "form": "못따라가고",
          "original_form": "못따라가고",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044897.1.20",
          "form": "ㅇㅇ 나도 기대 안했는데....ㅎ...",
          "original_form": "ㅇㅇ 나도 기대 안했는데....ㅎ...",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044897.1.21",
          "form": "지하까지 뚫고감",
          "original_form": "지하까지 뚫고감",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044897.1.22",
          "form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋ 진짜",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋㅋㅋ 진짜",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044897.1.23",
          "form": "게임회사운영에",
          "original_form": "게임회사운영에",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044897.1.24",
          "form": "심지어 오픈한지",
          "original_form": "심지어 오픈한지",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044897.1.25",
          "form": "한달도 안됐는디",
          "original_form": "한달도 안됐는디",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044897.1.26",
          "form": "이렇게 불만 얘기해본적 처음",
          "original_form": "이렇게 불만 얘기해본적 처음",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044897.1.27",
          "form": "ㅇㅇㅋㅋㅋㅋㄱㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㄱㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044897.1.28",
          "form": "스카이도 욕",
          "original_form": "스카이도 욕",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044897.1.29",
          "form": "오지게 했었는데",
          "original_form": "오지게 했었는데",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044897.1.30",
          "form": "다시보니 천사같네",
          "original_form": "다시보니 천사같네",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044897.1.31",
          "form": "구관이 명관이고",
          "original_form": "구관이 명관이고",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044897.1.32",
          "form": "그치 ㅋㅋㅋ 거기도 운영대행사라 엄청 뭐라했는데",
          "original_form": "그치 ㅋㅋㅋ 거기도 운영대행사라 엄청 뭐라했는데",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044897.1.33",
          "form": "그정도는 양반이었음을...",
          "original_form": "그정도는 양반이었음을...",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044897.1.34",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044897.1.35",
          "form": "아니 진짜 할 말이 없는게",
          "original_form": "아니 진짜 할 말이 없는게",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044897.1.36",
          "form": "뭐가 있어야",
          "original_form": "뭐가 있어야",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044897.1.37",
          "form": "불만을 토로하던",
          "original_form": "불만을 토로하던",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044897.1.38",
          "form": "착즙을 해보건",
          "original_form": "착즙을 해보건",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044897.1.39",
          "form": "하지",
          "original_form": "하지",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044897.1.40",
          "form": "뭐 있는게 없어",
          "original_form": "뭐 있는게 없어",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044897.1.41",
          "form": "ㅇㅇㅋㅋㅋㅋ 심지어",
          "original_form": "ㅇㅇㅋㅋㅋㅋ 심지어",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044897.1.42",
          "form": "오픈 예정인거 대략 얘기했는데",
          "original_form": "오픈 예정인거 대략 얘기했는데",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044897.1.43",
          "form": "루트가 왠지",
          "original_form": "루트가 왠지",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044897.1.44",
          "form": "남자주인공 시점의 스토리 어쩌구 하는걸보니",
          "original_form": "남자주인공 시점의 스토리 어쩌구 하는걸보니",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044897.1.45",
          "form": "내가 생각하는 그 연애겜이 아닌 느낌 ㅎ",
          "original_form": "내가 생각하는 그 연애겜이 아닌 느낌 ㅎ",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044897.1.46",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044897.1.47",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044897.1.48",
          "form": "그런거 필요없어",
          "original_form": "그런거 필요없어",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044897.1.49",
          "form": "ㅇㅇ 내가 그런게 하고싶음",
          "original_form": "ㅇㅇ 내가 그런게 하고싶음",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044897.1.50",
          "form": "인디겜을 하겟냨",
          "original_form": "인디겜을 하겟냨",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044897.1.51",
          "form": "이시점에 남주시점이",
          "original_form": "이시점에 남주시점이",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044897.1.52",
          "form": "왜 필요해",
          "original_form": "왜 필요해",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044897.1.53",
          "form": "봐 바야 쟤가.....다음 달의신 후보...? 쟤가....?",
          "original_form": "봐 바야 쟤가.....다음 달의신 후보...? 쟤가....?",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044897.1.54",
          "form": "정도겠지",
          "original_form": "정도겠지",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044897.1.55",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
