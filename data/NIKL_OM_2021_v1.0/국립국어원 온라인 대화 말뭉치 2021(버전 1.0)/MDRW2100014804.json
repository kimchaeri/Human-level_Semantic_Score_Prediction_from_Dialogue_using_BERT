      "utterance": [
        {
          "id": "MDRW2100014804.1.1",
          "form": "여행 못간지 오래ㅜㅜ",
          "original_form": "여행 못간지 오래ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014804.1.2",
          "form": "여행 가곱",
          "original_form": "여행 가곱",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014804.1.3",
          "form": "나도 여행 가고싶당",
          "original_form": "나도 여행 가고싶당",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014804.1.4",
          "form": "차만있어도 갔을듯",
          "original_form": "차만있어도 갔을듯",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014804.1.5",
          "form": "글서 그런가 차 사려는 사람 부쩍 생긴듯",
          "original_form": "글서 그런가 차 사려는 사람 부쩍 생긴듯",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014804.1.6",
          "form": "요즘은 그리 옛날에했던",
          "original_form": "요즘은 그리 옛날에했던",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014804.1.7",
          "form": "걸어서 세계속으로",
          "original_form": "걸어서 세계속으로",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014804.1.8",
          "form": "프로 종종 보게됨ㅎ",
          "original_form": "프로 종종 보게됨ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014804.1.9",
          "form": "여행  가곱",
          "original_form": "여행  가곱",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014804.1.10",
          "form": "나도 걸어서 세계속으로 본당 재밌엉!!",
          "original_form": "나도 걸어서 세계속으로 본당 재밌엉!!",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014804.1.11",
          "form": "너랑 태국가고싶당 재밌을거같아",
          "original_form": "너랑 태국가고싶당 재밌을거같아",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014804.1.12",
          "form": "그치? 재밌어\r\n지금 시기라 마스크 안쓰고 다른나라 풍습 하는거보고 재밌어",
          "original_form": "그치? 재밌어\r\n지금 시기라 마스크 안쓰고 다른나라 풍습 하는거보고 재밌어",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014804.1.13",
          "form": "코시국 끝나고 여행가자~",
          "original_form": "코시국 끝나고 여행가자~",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014804.1.14",
          "form": "넌 액티비티 좋아하지?",
          "original_form": "넌 액티비티 좋아하지?",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014804.1.15",
          "form": "ㅋㅋ응 은근?아닌듯한데 좋아하는거같어",
          "original_form": "ㅋㅋ응 은근?아닌듯한데 좋아하는거같어",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014804.1.16",
          "form": "태국에 유명한 액티비티 있어?",
          "original_form": "태국에 유명한 액티비티 있어?",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014804.1.17",
          "form": "아니 난 좋아하는편은 아니라서",
          "original_form": "아니 난 좋아하는편은 아니라서",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014804.1.18",
          "form": "서로 하고싶은거 절충해서 가야겠다 싶어성 ㅋㅋ",
          "original_form": "서로 하고싶은거 절충해서 가야겠다 싶어성 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014804.1.19",
          "form": "놀이기구는 좋아하는데",
          "original_form": "놀이기구는 좋아하는데",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014804.1.20",
          "form": "앗ㅋㅋ놀이기구",
          "original_form": "앗ㅋㅋ놀이기구",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014804.1.21",
          "form": "이번달엔 안가겠지?",
          "original_form": "이번달엔 안가겠지?",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014804.1.22",
          "form": "나 대구 이원드 표있음ㅋㅋ",
          "original_form": "나 대구 이원드 표있음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014804.1.23",
          "form": "나 몸무게 불어난 뒤로는 안가봐서 괜찮을까 몰겠네 ㅋㅋ",
          "original_form": "나 몸무게 불어난 뒤로는 안가봐서 괜찮을까 몰겠네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014804.1.24",
          "form": "그리고 시외버스 오래타는거 무섭기도 하고",
          "original_form": "그리고 시외버스 오래타는거 무섭기도 하고",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014804.1.25",
          "form": "가고싶긴하다",
          "original_form": "가고싶긴하다",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014804.1.26",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014804.1.27",
          "form": "난 혼자 가볼까하다",
          "original_form": "난 혼자 가볼까하다",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014804.1.28",
          "form": "그래도 같이 가는게 더 재밌겠다싶고 그래",
          "original_form": "그래도 같이 가는게 더 재밌겠다싶고 그래",
          "speaker_id": "1",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014804.1.29",
          "form": "오 진짜? 멋지당 ㅋㅋ 놀이동산 혼자가는거",
          "original_form": "오 진짜? 멋지당 ㅋㅋ 놀이동산 혼자가는거",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014804.1.30",
          "form": "이번주말있어서 아직 고민중이긴해ㅎ",
          "original_form": "이번주말있어서 아직 고민중이긴해ㅎ",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014804.1.31",
          "form": "얼마할인 되는거야?",
          "original_form": "얼마할인 되는거야?",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014804.1.32",
          "form": "모르겠어ㅎ ㅎ저렴하다했어ㅎㅎ",
          "original_form": "모르겠어ㅎ ㅎ저렴하다했어ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100014804.1.33",
          "form": "글쿠만 ㅎㅎ 가면 재밌겠다 나도 안 간지 오래됨",
          "original_form": "글쿠만 ㅎㅎ 가면 재밌겠다 나도 안 간지 오래됨",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014804.1.34",
          "form": "그체ㅎㅎ",
          "original_form": "그체ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014804.1.35",
          "form": "나도ㅋ",
          "original_form": "나도ㅋ",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014804.1.36",
          "form": "놀이동산 꽤된듯해서",
          "original_form": "놀이동산 꽤된듯해서",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014804.1.37",
          "form": "이번달 안까지 가능하니\r\n진짜 갈꺼면 얘기해ㅎ",
          "original_form": "이번달 안까지 가능하니\r\n진짜 갈꺼면 얘기해ㅎ",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100014804.1.38",
          "form": "나 24살 ? 25? 그때 가보고 못감",
          "original_form": "나 24살 ? 25? 그때 가보고 못감",
          "speaker_id": "2",
