      "utterance": [
        {
          "id": "MDRW2100034162.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 19:08"
        },
        {
          "id": "MDRW2100034162.1.2",
          "form": "안녕하세요 날씨가 많이 흐리죠?ㅠ",
          "original_form": "안녕하세요 날씨가 많이 흐리죠?ㅠ",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100034162.1.3",
          "form": "그러게요ㅠ 비 쏟아질 것 같아요",
          "original_form": "그러게요ㅠ 비 쏟아질 것 같아요",
          "speaker_id": "1",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100034162.1.4",
          "form": "가을이 오려다 마네요ㅠ",
          "original_form": "가을이 오려다 마네요ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100034162.1.5",
          "form": "비 그치고 나면 진짜 추울거 같아요",
          "original_form": "비 그치고 나면 진짜 추울거 같아요",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100034162.1.6",
          "form": "빨리 가을옷 준비해야할듯요",
          "original_form": "빨리 가을옷 준비해야할듯요",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100034162.1.7",
          "form": "가을옷 꺼내려면 여름옷들 정리해야하는데",
          "original_form": "가을옷 꺼내려면 여름옷들 정리해야하는데",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100034162.1.8",
          "form": "날씨가 계속 이래서 빨래하기가 쉽지않네요ㅠ",
          "original_form": "날씨가 계속 이래서 빨래하기가 쉽지않네요ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100034162.1.9",
          "form": "지금 빨래하면 안마를거 같아요...ㅋㅋ",
          "original_form": "지금 빨래하면 안마를거 같아요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100034162.1.10",
          "form": "습하고 춥고ㅠ",
          "original_form": "습하고 춥고ㅠ",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100034162.1.11",
          "form": "냄새도 나겠죠... ㅎ",
          "original_form": "냄새도 나겠죠... ㅎ",
          "speaker_id": "1",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100034162.1.12",
          "form": "어느 계절 좋아하세요?",
          "original_form": "어느 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100034162.1.13",
          "form": "저는 겨울 좋아해요 name1님은요?",
          "original_form": "저는 겨울 좋아해요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100034162.1.14",
          "form": "저도 겨울 좋아해요!",
          "original_form": "저도 겨울 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100034162.1.15",
          "form": "집에서도 이불, 밖에서도 이불 같은 롱패딩에 파묻혀있어도 돼서 좋아요 ㅎㅎ",
          "original_form": "집에서도 이불, 밖에서도 이불 같은 롱패딩에 파묻혀있어도 돼서 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100034162.1.16",
          "form": "맞아요 겨울 덕분에 느끼는 포근함이라는게 있죠ㅋㅋ",
          "original_form": "맞아요 겨울 덕분에 느끼는 포근함이라는게 있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100034162.1.17",
          "form": "올 겨울 많이 추울거라던데 걱정이긴 하네요 ㅎ",
          "original_form": "올 겨울 많이 추울거라던데 걱정이긴 하네요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100034162.1.18",
          "form": "저번 겨울의 영하 20도도 뛰어넘겠죠?ㅠ",
          "original_form": "저번 겨울의 영하 20도도 뛰어넘겠죠?ㅠ",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100034162.1.19",
          "form": "넘을 것 같아요 ㅎㅎ",
          "original_form": "넘을 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:53"
        }
