      "utterance": [
        {
          "id": "MDRW2100002360.1.1",
          "form": "전 종교가 없어요",
          "original_form": "전 종교가 없어요",
          "speaker_id": "2",
          "time": "20210820 10:21"
        },
        {
          "id": "MDRW2100002360.1.2",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210820 10:21"
        },
        {
          "id": "MDRW2100002360.1.3",
          "form": "앗 저도 무교요 ㅎㅎ",
          "original_form": "앗 저도 무교요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:21"
        },
        {
          "id": "MDRW2100002360.1.4",
          "form": "전 종교를 가지면 주말에 가야해서 안갖고 있어요",
          "original_form": "전 종교를 가지면 주말에 가야해서 안갖고 있어요",
          "speaker_id": "2",
          "time": "20210820 10:22"
        },
        {
          "id": "MDRW2100002360.1.5",
          "form": "코로나로 더 꺼려져요",
          "original_form": "코로나로 더 꺼려져요",
          "speaker_id": "2",
          "time": "20210820 10:22"
        },
        {
          "id": "MDRW2100002360.1.6",
          "form": "전 어릴때만 모태신앙이다가\r\n20살이후로 는 무교로 ㅎㅎ",
          "original_form": "전 어릴때만 모태신앙이다가\r\n20살이후로 는 무교로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:35"
        },
        {
          "id": "MDRW2100002360.1.7",
          "form": "아 그러셨군요 전 딱히 종교 없고 가끔 절에는 가요",
          "original_form": "아 그러셨군요 전 딱히 종교 없고 가끔 절에는 가요",
          "speaker_id": "2",
          "time": "20210820 10:36"
        },
        {
          "id": "MDRW2100002360.1.8",
          "form": "왠지 종교는 자유가 없어서 ㅡㅡ",
          "original_form": "왠지 종교는 자유가 없어서 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210820 10:36"
        },
        {
          "id": "MDRW2100002360.1.9",
          "form": "근데 저희 시댁은 다 교회다녀요 둘째 서방님이 목사세요",
          "original_form": "근데 저희 시댁은 다 교회다녀요 둘째 서방님이 목사세요",
          "speaker_id": "2",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002360.1.10",
          "form": "헉ㅋㅋㅋㅋ",
          "original_form": "헉ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002360.1.11",
          "form": "다행히 강요는 안하세요",
          "original_form": "다행히 강요는 안하세요",
          "speaker_id": "2",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002360.1.12",
          "form": "저희도 새언니 전도사님이세요",
          "original_form": "저희도 새언니 전도사님이세요",
          "speaker_id": "1",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002360.1.13",
          "form": "헉 전도 하시죠?",
          "original_form": "헉 전도 하시죠?",
          "speaker_id": "2",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002360.1.14",
          "form": "첨에 하시다가 이제 안해요",
          "original_form": "첨에 하시다가 이제 안해요",
          "speaker_id": "1",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002360.1.15",
          "form": "그냥 인사치레로",
          "original_form": "그냥 인사치레로",
          "speaker_id": "1",
          "time": "20210820 10:37"
        },
        {
          "id": "MDRW2100002360.1.16",
          "form": "아가씨 올해는 교회 나가세요 정도  ㅎㅎ",
          "original_form": "아가씨 올해는 교회 나가세요 정도  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 10:38"
        },
        {
          "id": "MDRW2100002360.1.17",
          "form": "ㅋㅋ그정도는 괜찮죠 울 둘째 서방님도 암말 안해요",
          "original_form": "ㅋㅋ그정도는 괜찮죠 울 둘째 서방님도 암말 안해요",
          "speaker_id": "2",
          "time": "20210820 10:38"
        },
        {
          "id": "MDRW2100002360.1.18",
          "form": "ㅎㅎ 다행이네요 \r\n새언니가 결혼전엔 강요 많이 하셧거든욮ㅋㅋ",
          "original_form": "ㅎㅎ 다행이네요 \r\n새언니가 결혼전엔 강요 많이 하셧거든욮ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 10:42"
        },
        {
          "id": "MDRW2100002360.1.19",
          "form": "저도요 그러다 결혼후 안해요",
          "original_form": "저도요 그러다 결혼후 안해요",
          "speaker_id": "2",
          "time": "20210820 10:42"
        },
        {
          "id": "MDRW2100002360.1.20",
          "form": "그게 참 불편해요 ..싫다는데도 계속 그럴때는요",
          "original_form": "그게 참 불편해요 ..싫다는데도 계속 그럴때는요",
          "speaker_id": "2",
          "time": "20210820 10:42"
        },
        {
          "id": "MDRW2100002360.1.21",
          "form": "전 굳건하게 거절했어요 ㅋㅋ",
          "original_form": "전 굳건하게 거절했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 10:43"
        },
        {
          "id": "MDRW2100002360.1.22",
          "form": "이상하게 절은 가면 맘이 편해서 가끔 산채 비빔밥도 먹을겸 가요",
          "original_form": "이상하게 절은 가면 맘이 편해서 가끔 산채 비빔밥도 먹을겸 가요",
          "speaker_id": "2",
          "time": "20210820 10:43"
        },
        {
          "id": "MDRW2100002360.1.23",
          "form": "저도 절은 가끔가요 힐링되자나요\r\n조용하고 풍경소리에 새소리에",
          "original_form": "저도 절은 가끔가요 힐링되자나요\r\n조용하고 풍경소리에 새소리에",
          "speaker_id": "1",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002360.1.24",
          "form": "맞아 요 숲냄새도 좋아요",
          "original_form": "맞아 요 숲냄새도 좋아요",
          "speaker_id": "2",
          "time": "20210820 11:06"
        },
        {
          "id": "MDRW2100002360.1.25",
          "form": "향도 좋게 느껴지고 ㅋㅋ",
          "original_form": "향도 좋게 느껴지고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:07"
        },
        {
          "id": "MDRW2100002360.1.26",
          "form": "구속이 없는게 좋은거 같아요",
          "original_form": "구속이 없는게 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100002360.1.27",
          "form": "맞아요 ㅎㅎ 절은 뭔가 부담이? 없어요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ 절은 뭔가 부담이? 없어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:12"
        },
        {
          "id": "MDRW2100002360.1.28",
          "form": "네 교회는 꼭 주말 가야하고 절운 아니라 좋죠",
          "original_form": "네 교회는 꼭 주말 가야하고 절운 아니라 좋죠",
          "speaker_id": "2",
          "time": "20210820 11:12"
        },
        {
          "id": "MDRW2100002360.1.29",
          "form": "코로나에 확진자 많이 나와서 교회가 더 싫어요",
          "original_form": "코로나에 확진자 많이 나와서 교회가 더 싫어요",
          "speaker_id": "2",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002360.1.30",
          "form": "무교는 아닌데  기도는 가끔 하네요 ㅎㅎ",
          "original_form": "무교는 아닌데  기도는 가끔 하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002360.1.31",
