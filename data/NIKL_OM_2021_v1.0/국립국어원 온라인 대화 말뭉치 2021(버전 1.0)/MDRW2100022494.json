      "utterance": [
        {
          "id": "MDRW2100022494.1.1",
          "form": "곰표 드셔보셨나요?",
          "original_form": "곰표 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100022494.1.2",
          "form": "아니여 드셔보셨어요?",
          "original_form": "아니여 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210829 15:11"
        },
        {
          "id": "MDRW2100022494.1.3",
          "form": "저는 곰표 맥주만 먹어봤어요ㅎㅎ",
          "original_form": "저는 곰표 맥주만 먹어봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100022494.1.4",
          "form": "저는 밀가루만 있는 줄 알았어요ㅋㅋㅋ",
          "original_form": "저는 밀가루만 있는 줄 알았어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022494.1.5",
          "form": "팝콘도 있네요?",
          "original_form": "팝콘도 있네요?",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022494.1.6",
          "form": "팝콘은 일반 짭짭한 팝콘 맛이래요!",
          "original_form": "팝콘은 일반 짭짭한 팝콘 맛이래요!",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100022494.1.7",
          "form": "아~ 캬라멜이 아니네요",
          "original_form": "아~ 캬라멜이 아니네요",
          "speaker_id": "2",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022494.1.8",
          "form": "요즘 콜라보 하는거보면 은근 눈이 즐거워요ㅋㅋ",
          "original_form": "요즘 콜라보 하는거보면 은근 눈이 즐거워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022494.1.9",
          "form": "디자인이 워낙 독특한 경우가 많으니까여ㅋㅋ",
          "original_form": "디자인이 워낙 독특한 경우가 많으니까여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100022494.1.10",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022494.1.11",
          "form": "마케팅이 빠질 수 없는 세상이긴 하죠",
          "original_form": "마케팅이 빠질 수 없는 세상이긴 하죠",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100022494.1.12",
          "form": "여유 되시면 곰표 맥주는 드셔보세요ㅎㅎ",
          "original_form": "여유 되시면 곰표 맥주는 드셔보세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022494.1.13",
          "form": "오 맛있나요?",
          "original_form": "오 맛있나요?",
          "speaker_id": "2",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022494.1.14",
          "form": "맥주는 마시던것만 마셔봐서 궁금하네요",
          "original_form": "맥주는 마시던것만 마셔봐서 궁금하네요",
          "speaker_id": "2",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022494.1.15",
          "form": "가볍고 끝맛이 좋았어요!",
          "original_form": "가볍고 끝맛이 좋았어요!",
          "speaker_id": "1",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022494.1.16",
          "form": "끝맛이 좋은게 많이 없었는데 더 매력적이네욥",
          "original_form": "끝맛이 좋은게 많이 없었는데 더 매력적이네욥",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022494.1.17",
          "form": "졔님 입맛에도 맞으셨음 좋겠네요ㅎㅎ",
          "original_form": "졔님 입맛에도 맞으셨음 좋겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 17:56"
        }
      ]
