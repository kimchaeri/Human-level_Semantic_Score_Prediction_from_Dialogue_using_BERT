      "utterance": [
        {
          "id": "MDRW2100044032.1.1",
          "form": "허니콤보랑 굉장히 흡사한데",
          "original_form": "허니콤보랑 굉장히 흡사한데",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044032.1.2",
          "form": "매운맛으로 먹을수도있어요",
          "original_form": "매운맛으로 먹을수도있어요",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044032.1.3",
          "form": "와 좋네요... 매운 허니콤보라니..",
          "original_form": "와 좋네요... 매운 허니콤보라니..",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044032.1.4",
          "form": "맵 단 짠을 다 느낄 수 있는 거군요",
          "original_form": "맵 단 짠을 다 느낄 수 있는 거군요",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044032.1.5",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044032.1.6",
          "form": "순한맛으로 드시면",
          "original_form": "순한맛으로 드시면",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044032.1.7",
          "form": "안맵게도 가능해요!",
          "original_form": "안맵게도 가능해요!",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044032.1.8",
          "form": "매운맛으로 먹어봐야겠어요",
          "original_form": "매운맛으로 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044032.1.9",
          "form": "매운거 너무 좋아요",
          "original_form": "매운거 너무 좋아요",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044032.1.10",
          "form": "앗..!",
          "original_form": "앗..!",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044032.1.11",
          "form": "아마도",
          "original_form": "아마도",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044032.1.12",
          "form": "매운맛은 고",
          "original_form": "매운맛은 고",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044032.1.13",
          "form": "추랑 같이 튀기는것같아요",
          "original_form": "추랑 같이 튀기는것같아요",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100044032.1.14",
          "form": "오 ㅁ.. 맛있을 것 같아요 저 굽네친 고 추 바사삭 좋아하거든요",
          "original_form": "오 ㅁ.. 맛있을 것 같아요 저 굽네친 고 추 바사삭 좋아하거든요",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044032.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044032.1.16",
          "form": "그거도 너무 맛잇죠ㅠㅠㅠ",
          "original_form": "그거도 너무 맛잇죠ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044032.1.17",
          "form": "그럼 혹시 닭똥집을 마늘이랑 같이 튀기는 갓도 좋아하시나요",
          "original_form": "그럼 혹시 닭똥집을 마늘이랑 같이 튀기는 갓도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044032.1.18",
          "form": "오 그렇게는 안먹어봤어요",
          "original_form": "오 그렇게는 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044032.1.19",
          "form": "마늘과 튀긴다니 맛없을수가 없겠군요",
          "original_form": "마늘과 튀긴다니 맛없을수가 없겠군요",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044032.1.20",
          "form": "그렇게 튀겨주는집이 있더라고요",
          "original_form": "그렇게 튀겨주는집이 있더라고요",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044032.1.21",
          "form": "진ㅉ ㅏ 환상의맛 ㅠㅠㅠ",
          "original_form": "진ㅉ ㅏ 환상의맛 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044032.1.22",
          "form": "근처에 있음 가보고싶어요.. 마늘 사랑해요 정말..",
          "original_form": "근처에 있음 가보고싶어요.. 마늘 사랑해요 정말..",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044032.1.23",
          "form": "혹시 맵슐랭 드셔보셨나요?",
          "original_form": "혹시 맵슐랭 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044032.1.24",
          "form": "엇 아니요!",
          "original_form": "엇 아니요!",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044032.1.25",
          "form": "매운 걸 잘 먹지는 못해서ㅠㅠ",
          "original_form": "매운 걸 잘 먹지는 못해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044032.1.26",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "1",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044032.1.27",
          "form": "아녀 .. 저는 매운건 좋아하는데 아직 먹어보진 못했어요",
          "original_form": "아녀 .. 저는 매운건 좋아하는데 아직 먹어보진 못했어요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044032.1.28",
          "form": "후기가 좀 .. 배아프고 다음날 화장실 심판을 받는다는 후기아 많아서",
          "original_form": "후기가 좀 .. 배아프고 다음날 화장실 심판을 받는다는 후기아 많아서",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044032.1.29",
          "form": "망설여지더라구요 ㅋㅋ",
          "original_form": "망설여지더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044032.1.30",
          "form": "아앜ㅋㅋㅋㅋ",
          "original_form": "아앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044032.1.31",
          "form": "맵슐랭은 어디 치킨집 치킨인가요?",
          "original_form": "맵슐랭은 어디 치킨집 치킨인가요?",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044032.1.32",
          "form": "자담치킨이요 !! ㅋㅋ 마일드 맛이 있긴한데 이것도 속에서 맵다고 하더라구요",
          "original_form": "자담치킨이요 !! ㅋㅋ 마일드 맛이 있긴한데 이것도 속에서 맵다고 하더라구요",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044032.1.33",
          "form": "아아 거기 거였군요",
          "original_form": "아아 거기 거였군요",
          "speaker_id": "1",
          "time": "20210907 21:34"
        }
      ]
