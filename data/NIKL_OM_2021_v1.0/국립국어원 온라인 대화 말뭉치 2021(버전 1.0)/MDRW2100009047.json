      "utterance": [
        {
          "id": "MDRW2100009047.1.1",
          "form": "넷플릭스 보세요?",
          "original_form": "넷플릭스 보세요?",
          "speaker_id": "1",
          "time": "20210823 15:51"
        },
        {
          "id": "MDRW2100009047.1.2",
          "form": "아뇽ㅋㅋ넷플릭스 요즘 딱히 볼 만한 게 없는 거 같아서ㅜ안봐용",
          "original_form": "아뇽ㅋㅋ넷플릭스 요즘 딱히 볼 만한 게 없는 거 같아서ㅜ안봐용",
          "speaker_id": "2",
          "time": "20210823 16:13"
        },
        {
          "id": "MDRW2100009047.1.3",
          "form": "전 티빙으로 신서유기 정주행중입니다",
          "original_form": "전 티빙으로 신서유기 정주행중입니다",
          "speaker_id": "1",
          "time": "20210823 16:24"
        },
        {
          "id": "MDRW2100009047.1.4",
          "form": "신서유기 재밌죠ㅋㅋ전 이미 본방으로 봐가지구ㅋㅋ",
          "original_form": "신서유기 재밌죠ㅋㅋ전 이미 본방으로 봐가지구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:47"
        },
        {
          "id": "MDRW2100009047.1.5",
          "form": "영화같은것도 많이 보나요 티빙으루",
          "original_form": "영화같은것도 많이 보나요 티빙으루",
          "speaker_id": "2",
          "time": "20210823 16:48"
        },
        {
          "id": "MDRW2100009047.1.6",
          "form": "이제 막 설치해서ㅋㅋㅋ",
          "original_form": "이제 막 설치해서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100009047.1.7",
          "form": "이것저것 찾아봐야겠어요",
          "original_form": "이것저것 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100009047.1.8",
          "form": "넷플릭스도 보고싶은데 추전할만한거 있나요",
          "original_form": "넷플릭스도 보고싶은데 추전할만한거 있나요",
          "speaker_id": "1",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100009047.1.9",
          "form": "저도 잘...ㅋㅋㅋㅋ넷플릭스 하면 킹덤 아닌가요? 전 보진 않았지만 인기가 좋던데요?",
          "original_form": "저도 잘...ㅋㅋㅋㅋ넷플릭스 하면 킹덤 아닌가요? 전 보진 않았지만 인기가 좋던데요?",
          "speaker_id": "2",
          "time": "20210823 17:09"
        },
        {
          "id": "MDRW2100009047.1.10",
          "form": "킹덤은 근데 진짜",
          "original_form": "킹덤은 근데 진짜",
          "speaker_id": "1",
          "time": "20210823 17:30"
        },
        {
          "id": "MDRW2100009047.1.11",
          "form": "주변사람들도 다 들어는 보고 실제론 못봤다네요",
          "original_form": "주변사람들도 다 들어는 보고 실제론 못봤다네요",
          "speaker_id": "1",
          "time": "20210823 17:30"
        },
        {
          "id": "MDRW2100009047.1.12",
          "form": "실존하는게 맞긴 한건가",
          "original_form": "실존하는게 맞긴 한건가",
          "speaker_id": "1",
          "time": "20210823 17:31"
        },
        {
          "id": "MDRW2100009047.1.13",
          "form": "ㅋㅋㅋㅋ다들 안봤나봐요 듣기만하고...",
          "original_form": "ㅋㅋㅋㅋ다들 안봤나봐요 듣기만하고...",
          "speaker_id": "2",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100009047.1.14",
          "form": "미드는 안보세요?",
          "original_form": "미드는 안보세요?",
          "speaker_id": "1",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100009047.1.15",
          "form": "네ㅋㅋㅋ드라마 잘 안보는...",
          "original_form": "네ㅋㅋㅋ드라마 잘 안보는...",
          "speaker_id": "2",
          "time": "20210823 17:44"
        },
        {
          "id": "MDRW2100009047.1.16",
          "form": "덱스터는 나중에 한번 보세요",
          "original_form": "덱스터는 나중에 한번 보세요",
          "speaker_id": "1",
          "time": "20210823 17:45"
        },
        {
          "id": "MDRW2100009047.1.17",
          "form": "완전재밌음",
          "original_form": "완전재밌음",
          "speaker_id": "1",
          "time": "20210823 17:45"
        },
        {
          "id": "MDRW2100009047.1.18",
          "form": "덱스터요?",
          "original_form": "덱스터요?",
          "speaker_id": "2",
          "time": "20210823 17:48"
        },
        {
          "id": "MDRW2100009047.1.19",
          "form": "옛날에 보던  애니메이션 덱스터의 실험실인줄 알았네요ㅋㅋ",
          "original_form": "옛날에 보던  애니메이션 덱스터의 실험실인줄 알았네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100009047.1.20",
          "form": "으르신 티가 많이 납니다",
          "original_form": "으르신 티가 많이 납니다",
          "speaker_id": "1",
          "time": "20210823 17:53"
        },
        {
          "id": "MDRW2100009047.1.21",
          "form": "그레 약간 비슷한것같긴 해요",
          "original_form": "그레 약간 비슷한것같긴 해요",
          "speaker_id": "1",
          "time": "20210823 17:53"
        },
        {
          "id": "MDRW2100009047.1.22",
          "form": "미드 덱스터 로코쪽인데 스윗해요 엄청",
          "original_form": "미드 덱스터 로코쪽인데 스윗해요 엄청",
          "speaker_id": "1",
          "time": "20210823 17:54"
        },
        {
          "id": "MDRW2100009047.1.23",
          "form": "ㅋㅋㅋㅋ찾아보니까 연쇄살인범 잡는 싸이코패스 연쇄살인범의 내용이라는데 맞나요...",
          "original_form": "ㅋㅋㅋㅋ찾아보니까 연쇄살인범 잡는 싸이코패스 연쇄살인범의 내용이라는데 맞나요...",
          "speaker_id": "2",
          "time": "20210823 18:00"
        },
        {
          "id": "MDRW2100009047.1.24",
          "form": "스윗한 거 맞아요...?",
          "original_form": "스윗한 거 맞아요...?",
          "speaker_id": "2",
          "time": "20210823 18:00"
        },
        {
          "id": "MDRW2100009047.1.25",
          "form": "아 그거 그사람들이 거짓말하는거예요",
          "original_form": "아 그거 그사람들이 거짓말하는거예요",
          "speaker_id": "1",
          "time": "20210823 18:10"
        },
        {
          "id": "MDRW2100009047.1.26",
          "form": "남자친구랑 분위기 좋게 한번 보세요",
          "original_form": "남자친구랑 분위기 좋게 한번 보세요",
          "speaker_id": "1",
          "time": "20210823 18:10"
        },
        {
          "id": "MDRW2100009047.1.27",
          "form": "ㅋㅋㅋㅋㄱㅋㅋ진짜요?ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋ진짜요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:26"
        },
        {
          "id": "MDRW2100009047.1.28",
          "form": "저는 분위기잡고 와인한잔 하면서 봤어요",
          "original_form": "저는 분위기잡고 와인한잔 하면서 봤어요",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100009047.1.29",
          "form": "...? 아닌거 같은데...",
          "original_form": "...? 아닌거 같은데...",
          "speaker_id": "2",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100009047.1.30",
          "form": "사진만 봐도...ㄷㄷ한데요....?",
          "original_form": "사진만 봐도...ㄷㄷ한데요....?",
          "speaker_id": "2",
          "time": "20210823 18:32"
        }
      ]
