        },
        {
          "id": "MDRW2100024562.1.2",
          "form": "축구장 가봤어?",
          "original_form": "축구장 가봤어?",
          "speaker_id": "1",
          "time": "20210831 14:14"
        },
        {
          "id": "MDRW2100024562.1.3",
          "form": "응 예전에",
          "original_form": "응 예전에",
          "speaker_id": "2",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100024562.1.4",
          "form": "난 축구장은 한 번도 안 가본 거 같애",
          "original_form": "난 축구장은 한 번도 안 가본 거 같애",
          "speaker_id": "1",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100024562.1.5",
          "form": "그럴 수 있지",
          "original_form": "그럴 수 있지",
          "speaker_id": "2",
          "time": "20210831 14:15"
        },
        {
          "id": "MDRW2100024562.1.6",
          "form": "축구도 재밌긴 하겠다",
          "original_form": "축구도 재밌긴 하겠다",
          "speaker_id": "1",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100024562.1.7",
          "form": "응 재밌어",
          "original_form": "응 재밌어",
          "speaker_id": "2",
          "time": "20210831 14:57"
        },
        {
          "id": "MDRW2100024562.1.8",
          "form": "직관으로 보면 전술 같은 거 잘 보여?",
          "original_form": "직관으로 보면 전술 같은 거 잘 보여?",
          "speaker_id": "1",
          "time": "20210831 14:58"
        },
        {
          "id": "MDRW2100024562.1.9",
          "form": "그런 것까지는 잘 모르겠어",
          "original_form": "그런 것까지는 잘 모르겠어",
          "speaker_id": "2",
          "time": "20210831 14:58"
        },
        {
          "id": "MDRW2100024562.1.10",
          "form": "축구 유튜브 많이 본다며",
          "original_form": "축구 유튜브 많이 본다며",
          "speaker_id": "1",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100024562.1.11",
          "form": "대충은 보이는데 전문가는 아니니까",
          "original_form": "대충은 보이는데 전문가는 아니니까",
          "speaker_id": "2",
          "time": "20210831 14:59"
        },
        {
          "id": "MDRW2100024562.1.12",
          "form": "나는 뭉쳐야 찬다보면서 축구도 전술을 많이 짜는구나 했거든",
