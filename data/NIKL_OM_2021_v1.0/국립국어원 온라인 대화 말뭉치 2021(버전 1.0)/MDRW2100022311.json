        {
          "id": "MDRW2100022311.1.1",
          "form": "ㅎㅎ 맛있었어?",
          "original_form": "ㅎㅎ 맛있었어?",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022311.1.2",
          "form": "ㅇㅇ 마늘 넣어서 더 맛있었어 ㅎㅎ",
          "original_form": "ㅇㅇ 마늘 넣어서 더 맛있었어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022311.1.3",
          "form": "근데 좀 매웠지?",
          "original_form": "근데 좀 매웠지?",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022311.1.4",
          "form": "약간 맵긴 했는데 괜찮았어",
          "original_form": "약간 맵긴 했는데 괜찮았어",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022311.1.5",
          "form": "마지막에 참기름 넣는 걸 깜박해서 ㅋㅋ",
          "original_form": "마지막에 참기름 넣는 걸 깜박해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022311.1.6",
          "form": "아 그래서 약간 매콤했구나 ㅎㅎ",
          "original_form": "아 그래서 약간 매콤했구나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022311.1.7",
          "form": "게다 김가루도 빼먹었어 ㅋㅋㅋ",
          "original_form": "게다 김가루도 빼먹었어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022311.1.8",
          "form": "아 맞네~ 어쩐지 평소보다 약간 매콤하다 했더니",
          "original_form": "아 맞네~ 어쩐지 평소보다 약간 매콤하다 했더니",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022311.1.9",
          "form": "볶다보니 다 빼먹었네",
          "original_form": "볶다보니 다 빼먹었네",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022311.1.10",
          "form": "그래도 맛있었어~ 야채랑 마늘넣어서 그런가 ㅎㅎ",
          "original_form": "그래도 맛있었어~ 야채랑 마늘넣어서 그런가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022311.1.11",
          "form": "잘 먹었다니 다행이네",
          "original_form": "잘 먹었다니 다행이네",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022311.1.12",
          "form": "다음에 또 부탁해용",
          "original_form": "다음에 또 부탁해용",
