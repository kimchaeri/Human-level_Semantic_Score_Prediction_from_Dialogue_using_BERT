      "utterance": [
        {
          "id": "MDRW2100025136.1.1",
          "form": "모임 좋아하시는",
          "original_form": "모임 좋아하시는",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100025136.1.2",
          "form": "느낌은 아니신데ㅎㅎ",
          "original_form": "느낌은 아니신데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100025136.1.3",
          "form": "앗 텍스트로도 느껴지시는 군요ㅋㅋㅋ",
          "original_form": "앗 텍스트로도 느껴지시는 군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:58"
        },
        {
          "id": "MDRW2100025136.1.4",
          "form": "친구들 빼고는 따로 참여하는 모임이 없네요^//^",
          "original_form": "친구들 빼고는 따로 참여하는 모임이 없네요^//^",
          "speaker_id": "2",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100025136.1.5",
          "form": "따로 모임 참여하시는 거 있으신가요?",
          "original_form": "따로 모임 참여하시는 거 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 00:59"
        },
        {
          "id": "MDRW2100025136.1.6",
          "form": "저는 뭐 친구들 모임하고",
          "original_form": "저는 뭐 친구들 모임하고",
          "speaker_id": "1",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100025136.1.7",
          "form": "회사동기들?ㅋㅋ",
          "original_form": "회사동기들?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100025136.1.8",
          "form": "오 회사분들이랑 돈독 하시군요ㅋㅋ",
          "original_form": "오 회사분들이랑 돈독 하시군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100025136.1.9",
          "form": "(물론 동기분들 한정이실수도 있지만..!",
          "original_form": "(물론 동기분들 한정이실수도 있지만..!",
          "speaker_id": "2",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100025136.1.10",
          "form": "회사도 돈독한 편이에요ㅋㅋ",
          "original_form": "회사도 돈독한 편이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:07"
        },
        {
          "id": "MDRW2100025136.1.11",
          "form": "교대근무조 같이 돌아서",
          "original_form": "교대근무조 같이 돌아서",
          "speaker_id": "1",
          "time": "20210901 01:07"
        },
        {
          "id": "MDRW2100025136.1.12",
          "form": "거의 가족만큼 보는듯요ㅋㅋ",
          "original_form": "거의 가족만큼 보는듯요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:07"
        },
        {
          "id": "MDRW2100025136.1.13",
          "form": "다행이네요ㅎㅎ",
          "original_form": "다행이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 10:07"
        },
        {
          "id": "MDRW2100025136.1.14",
          "form": "회사 사람들이랑 모이면 특별히 하시는 거 있으신가요?",
          "original_form": "회사 사람들이랑 모이면 특별히 하시는 거 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 10:12"
        },
        {
          "id": "MDRW2100025136.1.15",
          "form": "뭐 보통 맛있는거 먹죠?ㅋㅋ",
          "original_form": "뭐 보통 맛있는거 먹죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025136.1.16",
          "form": "요새는 코로나 심해서 뭐",
          "original_form": "요새는 코로나 심해서 뭐",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025136.1.17",
          "form": "시켜먹습니닷 보통ㅋㅋ",
          "original_form": "시켜먹습니닷 보통ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025136.1.18",
          "form": "좋네요..ㅎㅎ  배달 최고..!!",
          "original_form": "좋네요..ㅎㅎ  배달 최고..!!",
          "speaker_id": "2",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100025136.1.19",
          "form": "저도 거의 만나면 카페같은 곳에서 수다만 떨고 오는 것 같네요ㅋㅋ",
          "original_form": "저도 거의 만나면 카페같은 곳에서 수다만 떨고 오는 것 같네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025136.1.20",
          "form": "요새는 진짜 예전에 비해 뜸해요",
          "original_form": "요새는 진짜 예전에 비해 뜸해요",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025136.1.21",
          "form": "카드 실적도 못채워서",
          "original_form": "카드 실적도 못채워서",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100025136.1.22",
          "form": "하나 잘랐어요ㅎㅎ",
          "original_form": "하나 잘랐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100025136.1.23",
          "form": "아앟....ㅎㅎㅎ",
          "original_form": "아앟....ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100025136.1.24",
          "form": "돈은 절약이 됩니다만 ㅎㅎ",
          "original_form": "돈은 절약이 됩니다만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100025136.1.25",
          "form": "그,그렇죠..ㅎㅎ 확실히 예전에는 단체로 우르르가서 밥 먹는 자리도 빈번했는데 이제는 그런게 아얘 없으니까..",
          "original_form": "그,그렇죠..ㅎㅎ 확실히 예전에는 단체로 우르르가서 밥 먹는 자리도 빈번했는데 이제는 그런게 아얘 없으니까..",
          "speaker_id": "2",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100025136.1.26",
          "form": "조..좋네요..!!ㅠ",
          "original_form": "조..좋네요..!!ㅠ",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100025136.1.27",
          "form": "ㅋㅋ진짜 좋으신건가요",
          "original_form": "ㅋㅋ진짜 좋으신건가요",
          "speaker_id": "1",
          "time": "20210901 15:40"
        },
        {
