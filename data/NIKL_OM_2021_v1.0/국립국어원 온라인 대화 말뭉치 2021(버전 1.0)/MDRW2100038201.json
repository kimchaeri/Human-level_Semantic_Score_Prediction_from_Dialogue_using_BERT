      "utterance": [
        {
          "id": "MDRW2100038201.1.1",
          "form": "웹드 에이틴 보셨어여?ㅋ",
          "original_form": "웹드 에이틴 보셨어여?ㅋ",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038201.1.2",
          "form": "ㅋㅋㅋ하이틴드라마긴한데",
          "original_form": "ㅋㅋㅋ하이틴드라마긴한데",
          "speaker_id": "2",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038201.1.3",
          "form": "유명한건 알아요! 보진않았어요!",
          "original_form": "유명한건 알아요! 보진않았어요!",
          "speaker_id": "1",
          "time": "20210902 00:38"
        },
        {
          "id": "MDRW2100038201.1.4",
          "form": "하이틴 그거 그나름대로의 매력이 있지 않나요 ㅋㅋㅋㅋ 가끔 하이틴 영화 보고싶고 그런데 ㅋㅋㅋ",
          "original_form": "하이틴 그거 그나름대로의 매력이 있지 않나요 ㅋㅋㅋㅋ 가끔 하이틴 영화 보고싶고 그런데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100038201.1.5",
          "form": "저도 유명하다해서 봤는데ㅋㅋㅋ",
          "original_form": "저도 유명하다해서 봤는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100038201.1.6",
          "form": "풋풋하면서 오글거리면서 봤어요",
          "original_form": "풋풋하면서 오글거리면서 봤어요",
          "speaker_id": "2",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100038201.1.7",
          "form": "매력이있던가요 ㅋㅋㅋ",
          "original_form": "매력이있던가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100038201.1.8",
          "form": "그게 매력이죠 ㅋㅋㅋ/",
          "original_form": "그게 매력이죠 ㅋㅋㅋ/",
          "speaker_id": "1",
          "time": "20210902 00:42"
        },
        {
          "id": "MDRW2100038201.1.9",
          "form": "그런맛으로 보는거죠 ㅋㅋㅋㅋ",
          "original_form": "그런맛으로 보는거죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100038201.1.10",
          "form": "웹드 엑스엑스도 재밌었어요 ㅋㅋ",
          "original_form": "웹드 엑스엑스도 재밌었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100038201.1.11",
          "form": "첨들어봐요ㅋㅋ",
          "original_form": "첨들어봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038201.1.12",
          "form": "하니 나왔네요!",
          "original_form": "하니 나왔네요!",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100038201.1.13",
          "form": "네네 하니 거기서 엄청 예뻐요 ㅋㅋㅋ",
          "original_form": "네네 하니 거기서 엄청 예뻐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100038201.1.14",
          "form": "아예 배우로 전향했나봐요",
          "original_form": "아예 배우로 전향했나봐요",
          "speaker_id": "2",
          "time": "20210902 02:12"
        },
        {
          "id": "MDRW2100038201.1.15",
          "form": "영화도 찍었던데",
          "original_form": "영화도 찍었던데",
          "speaker_id": "2",
          "time": "20210902 02:12"
        },
        {
          "id": "MDRW2100038201.1.16",
          "form": "박화영2였나?!",
          "original_form": "박화영2였나?!",
          "speaker_id": "2",
          "time": "20210902 02:12"
        },
        {
          "id": "MDRW2100038201.1.17",
          "form": "뭐얔ㅋㅋㅋ하석진 면접상대가 전소민이네욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭐얔ㅋㅋㅋ하석진 면접상대가 전소민이네욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 02:13"
        },
        {
          "id": "MDRW2100038201.1.18",
          "form": "아 너무웃겨",
          "original_form": "아 너무웃겨",
          "speaker_id": "2",
          "time": "20210902 02:13"
        },
        {
          "id": "MDRW2100038201.1.19",
          "form": "그거 진짜깨알같은 부분들이 너무많아서 ㅋㅋㅋ 진짜 재밌지 않나요 ㅋㅋㅋ",
          "original_form": "그거 진짜깨알같은 부분들이 너무많아서 ㅋㅋㅋ 진짜 재밌지 않나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 03:09"
        },
        {
          "id": "MDRW2100038201.1.20",
          "form": "전소민 오로라공주때처럼 하고 나와서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전소민 오로라공주때처럼 하고 나와서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 06:19"
        },
        {
          "id": "MDRW2100038201.1.21",
          "form": "드라마 전개가 딱 tvn 느낌이 나요",
          "original_form": "드라마 전개가 딱 tvn 느낌이 나요",
          "speaker_id": "2",
          "time": "20210902 06:19"
        },
        {
          "id": "MDRW2100038201.1.22",
          "form": "오로라공주를 안봐서 몰랐네요 ㅋㅋㅋㅋ 조만간 혼술남녀 재탕한번 해야겠어요 ㅋㅋ",
          "original_form": "오로라공주를 안봐서 몰랐네요 ㅋㅋㅋㅋ 조만간 혼술남녀 재탕한번 해야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:48"
        },
        {
          "id": "MDRW2100038201.1.23",
          "form": "마지막화만 남았어요ㅋㅋㅋ",
          "original_form": "마지막화만 남았어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038201.1.24",
          "form": "오늘 술 약속있어서",
          "original_form": "오늘 술 약속있어서",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038201.1.25",
          "form": "먹고 와서 밤에 보려고 아껴두고있어요",
          "original_form": "먹고 와서 밤에 보려고 아껴두고있어요",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038201.1.26",
          "form": "어머 진짜 빨리 보셨어요! ㅋㅋㅋ",
          "original_form": "어머 진짜 빨리 보셨어요! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100038201.1.27",
          "form": "배속해서 보시나요?!",
          "original_form": "배속해서 보시나요?!",
          "speaker_id": "1",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100038201.1.28",
          "form": "네네ㅋㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:04"
        }
