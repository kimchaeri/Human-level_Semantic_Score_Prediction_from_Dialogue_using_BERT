      "utterance": [
        {
          "id": "MDRW2100005121.1.1",
          "form": "안녕하세요!!!",
          "original_form": "안녕하세요!!!",
          "speaker_id": "1",
          "time": "20210821 20:39"
        },
        {
          "id": "MDRW2100005121.1.2",
          "form": "안녕하세용!!",
          "original_form": "안녕하세용!!",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005121.1.3",
          "form": "와 좋아하는 음식 너무 많은데",
          "original_form": "와 좋아하는 음식 너무 많은데",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005121.1.4",
          "form": "매운 거 좋아하세요?",
          "original_form": "매운 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005121.1.5",
          "form": "와 저 매운거 너무 사랑해요",
          "original_form": "와 저 매운거 너무 사랑해요",
          "speaker_id": "1",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005121.1.6",
          "form": "제 베스트 영혼의 음식 매운떡볶이",
          "original_form": "제 베스트 영혼의 음식 매운떡볶이",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005121.1.7",
          "form": "와 저도 떡볶이랑 마라탕이랑...",
          "original_form": "와 저도 떡볶이랑 마라탕이랑...",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005121.1.8",
          "form": "하튼 매운 거 좋아해요",
          "original_form": "하튼 매운 거 좋아해요",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005121.1.9",
          "form": "맵찔이긴한데ㅎㅎㅎ",
          "original_form": "맵찔이긴한데ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005121.1.10",
          "form": "와 저랑 취향 똑같으시네요... 저 일주일에 최소 두번은 마라탕 먹어야해요....",
          "original_form": "와 저랑 취향 똑같으시네요... 저 일주일에 최소 두번은 마라탕 먹어야해요....",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005121.1.11",
          "form": "혈관에 혈액대신 마라탕이 흐르겠어요",
          "original_form": "혈관에 혈액대신 마라탕이 흐르겠어요",
          "speaker_id": "2",
          "time": "20210821 20:44"
        },
        {
          "id": "MDRW2100005121.1.12",
          "form": "전 떡볶이 국물이 흘러요",
          "original_form": "전 떡볶이 국물이 흘러요",
          "speaker_id": "2",
          "time": "20210821 20:45"
        },
        {
          "id": "MDRW2100005121.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저도 마찬가지입니다... 아마 전 마라맛 떡볶이가 흐를거에요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저도 마찬가지입니다... 아마 전 마라맛 떡볶이가 흐를거에요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005121.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ보통 배달시켜 드시나요? 저는 요즘에는 거의 배달만 시켜먹어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ보통 배달시켜 드시나요? 저는 요즘에는 거의 배달만 시켜먹어요",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005121.1.15",
          "form": "저는 반반이에요! 애인이 요리를 잘해서 해주기도 하고 귀찮을 땐 시키고 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 반반이에요! 애인이 요리를 잘해서 해주기도 하고 귀찮을 땐 시키고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005121.1.16",
          "form": "우와앙 너무 좋으시겠다",
          "original_form": "우와앙 너무 좋으시겠다",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005121.1.17",
          "form": "오늘 저녁은 뭐 드셨나요?",
          "original_form": "오늘 저녁은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005121.1.18",
          "form": "전 사실 자영업자라 ㅋㅋㅋㅋㅋ 가게에서 메뉴 빼고 남은 반죽으로 김치전 먹었슴당...",
          "original_form": "전 사실 자영업자라 ㅋㅋㅋㅋㅋ 가게에서 메뉴 빼고 남은 반죽으로 김치전 먹었슴당...",
          "speaker_id": "1",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005121.1.19",
          "form": "헉 김치전 좋아하는데 맛있겠다",
          "original_form": "헉 김치전 좋아하는데 맛있겠다",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005121.1.20",
          "form": "저는 카레 먹었습니당",
          "original_form": "저는 카레 먹었습니당",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005121.1.21",
          "form": "오늘 유독 김치전이 맛있게 됐더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "오늘 유독 김치전이 맛있게 됐더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005121.1.22",
          "form": "카레!!!! 왕맛있갰다....",
          "original_form": "카레!!!! 왕맛있갰다....",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005121.1.23",
          "form": "김치전은 가장자리가 바삭바삭한 게 넘 좋아요",
          "original_form": "김치전은 가장자리가 바삭바삭한 게 넘 좋아요",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005121.1.24",
          "form": "카레는 야채 큼직한 게 좋구여",
          "original_form": "카레는 야채 큼직한 게 좋구여",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005121.1.25",
          "form": "그냥 제 취향이에요ㅋㄱㅋㅋㄱ",
          "original_form": "그냥 제 취향이에요ㅋㄱㅋㅋㄱ",
          "speaker_id": "2",
