        },
        {
          "id": "MDRW2100029893.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029893.1.3",
          "form": "좋아하시는 음식 있으신가요",
          "original_form": "좋아하시는 음식 있으신가요",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029893.1.4",
          "form": "저는 피자 좋아해여~",
          "original_form": "저는 피자 좋아해여~",
          "speaker_id": "1",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100029893.1.5",
          "form": "오 피자 좋죠",
          "original_form": "오 피자 좋죠",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100029893.1.6",
          "form": "어떤 피자 좋아하시나요",
          "original_form": "어떤 피자 좋아하시나요",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029893.1.7",
          "form": "저는 파파존스 여기꺼만시켜먹어요ㅋㅋㅋㅋ",
          "original_form": "저는 파파존스 여기꺼만시켜먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029893.1.8",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "2",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029893.1.9",
          "form": "조금 의외네요?",
          "original_form": "조금 의외네요?",
          "speaker_id": "2",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029893.1.10",
          "form": "아니 의외랄건 없지만",
          "original_form": "아니 의외랄건 없지만",
          "speaker_id": "2",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100029893.1.11",
          "form": "아니 유명하긴 한데",
          "original_form": "아니 유명하긴 한데",
          "speaker_id": "2",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100029893.1.12",
          "form": "뭔가 뭔가",
          "original_form": "뭔가 뭔가",
          "speaker_id": "2",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100029893.1.13",
          "form": "앜ㅋㅋㅋㅋㅋ 그렁가여??",
          "original_form": "앜ㅋㅋㅋㅋㅋ 그렁가여??",
          "speaker_id": "1",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100029893.1.14",
          "form": "혹시 드셔보셧어요??",
          "original_form": "혹시 드셔보셧어요??",
          "speaker_id": "1",
          "time": "20210903 16:19"
        },
        {
          "id": "MDRW2100029893.1.15",
          "form": "아 그러고보니",
          "original_form": "아 그러고보니",
          "speaker_id": "2",
          "time": "20210903 16:20"
        },
        {
          "id": "MDRW2100029893.1.16",
          "form": "제가 마지막으로 먹은게",
          "original_form": "제가 마지막으로 먹은게",
          "speaker_id": "2",
          "time": "20210903 16:20"
        },
        {
          "id": "MDRW2100029893.1.17",
          "form": "3년전인가 할정도로",
          "original_form": "3년전인가 할정도로",
          "speaker_id": "2",
          "time": "20210903 16:20"
        },
        {
          "id": "MDRW2100029893.1.18",
          "form": "안 친해서 그런가;;",
          "original_form": "안 친해서 그런가;;",
          "speaker_id": "2",
          "time": "20210903 16:20"
        },
        {
          "id": "MDRW2100029893.1.19",
          "form": "앜ㅋㅋㅋㅋㅋ 되게오래되셧네요!!",
          "original_form": "앜ㅋㅋㅋㅋㅋ 되게오래되셧네요!!",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029893.1.20",
          "form": "전 미국맛피자좋아해서 여기꺼잘먹어요~",
          "original_form": "전 미국맛피자좋아해서 여기꺼잘먹어요~",
          "speaker_id": "1",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029893.1.21",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029893.1.22",
          "form": "씬 피자가",
          "original_form": "씬 피자가",
          "speaker_id": "2",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029893.1.23",
          "form": "이탈리아 식인가였죠?",
          "original_form": "이탈리아 식인가였죠?",
          "speaker_id": "2",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029893.1.24",
          "form": "미국식하면 시카고 피자부터 그려지는데",
          "original_form": "미국식하면 시카고 피자부터 그려지는데",
          "speaker_id": "2",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100029893.1.25",
          "form": "맞아여 ㅋㅋㅋㅋ 아 미국도 동부서부 많이다르니깐여~",
          "original_form": "맞아여 ㅋㅋㅋㅋ 아 미국도 동부서부 많이다르니깐여~",
          "speaker_id": "1",
          "time": "20210903 16:26"
        },
        {
          "id": "MDRW2100029893.1.26",
          "form": "그러면",
          "original_form": "그러면",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029893.1.27",
          "form": "메뉴는 어떤거",
          "original_form": "메뉴는 어떤거",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029893.1.28",
          "form": "좋아하시나요",
          "original_form": "좋아하시나요",
          "speaker_id": "2",
          "time": "20210903 16:27"
        },
        {
          "id": "MDRW2100029893.1.29",
          "form": "저는 매번 존스페이버릿이나 스치랜 두개 자주먹어요",
          "original_form": "저는 매번 존스페이버릿이나 스치랜 두개 자주먹어요",
          "speaker_id": "1",
          "time": "20210903 16:28"
        },
        {
          "id": "MDRW2100029893.1.30",
          "form": "스치랜은",
          "original_form": "스치랜은",
          "speaker_id": "2",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100029893.1.31",
          "form": "줄임말인가요?",
          "original_form": "줄임말인가요?",
          "speaker_id": "2",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100029893.1.32",
          "form": "넼ㅋㅋㅋ 스파이시치킨랜치 이걸꺼에요 ㅋㅋ",
          "original_form": "넼ㅋㅋㅋ 스파이시치킨랜치 이걸꺼에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100029893.1.33",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100029893.1.34",
          "form": "이름만 봐도",
          "original_form": "이름만 봐도",
          "speaker_id": "2",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029893.1.35",
