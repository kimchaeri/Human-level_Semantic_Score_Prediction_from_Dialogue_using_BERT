      "utterance": [
        {
          "id": "MDRW2100042995.1.1",
          "form": "name2님은 빵좋아하시나요",
          "original_form": "&name2&님은 빵좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 12:40"
        },
        {
          "id": "MDRW2100042995.1.2",
          "form": "초코빵 좋아해요!",
          "original_form": "초코빵 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100042995.1.3",
          "form": "개맛있죠 초코빵",
          "original_form": "개맛있죠 초코빵",
          "speaker_id": "1",
          "time": "20210907 12:57"
        },
        {
          "id": "MDRW2100042995.1.4",
          "form": "단거 좋아하시나봐요",
          "original_form": "단거 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210907 12:57"
        },
        {
          "id": "MDRW2100042995.1.5",
          "form": "마카롱도 좋아하시나요",
          "original_form": "마카롱도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 12:57"
        },
        {
          "id": "MDRW2100042995.1.6",
          "form": "완전 좋아해요! 초코 관련된 건 다 좋아해요",
          "original_form": "완전 좋아해요! 초코 관련된 건 다 좋아해요",
          "speaker_id": "2",
          "time": "20210907 12:58"
        },
        {
          "id": "MDRW2100042995.1.7",
          "form": "ㅋㅋㅋㅋ아ㅋㅋname2님의 베스트초코과자는 뭔가요",
          "original_form": "ㅋㅋㅋㅋ아ㅋㅋ&name2&님의 베스트초코과자는 뭔가요",
          "speaker_id": "1",
          "time": "20210907 13:00"
        },
        {
          "id": "MDRW2100042995.1.8",
          "form": "칙촉이요ㅋㅋ 맛있어용",
          "original_form": "칙촉이요ㅋㅋ 맛있어용",
          "speaker_id": "2",
          "time": "20210907 13:02"
        },
        {
          "id": "MDRW2100042995.1.9",
          "form": "아ㅠ넘좋아요 초코칩쿠키같은",
          "original_form": "아ㅠ넘좋아요 초코칩쿠키같은",
          "speaker_id": "1",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042995.1.10",
          "form": "버석함과다른 촉촉함 ㄹㅇ좋아요",
          "original_form": "버석함과다른 촉촉함 ㄹㅇ좋아요",
          "speaker_id": "1",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042995.1.11",
          "form": "맞아요ㅜㅜㅜ 촉촉바삭이어서 정말 맛있어요",
          "original_form": "맞아요ㅜㅜㅜ 촉촉바삭이어서 정말 맛있어요",
          "speaker_id": "2",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100042995.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100042995.1.13",
          "form": "저는 홈런볼!",
          "original_form": "저는 홈런볼!",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100042995.1.14",
          "form": "바삭한데 초코드간게 손이 잘가요",
          "original_form": "바삭한데 초코드간게 손이 잘가요",
          "speaker_id": "1",
          "time": "20210907 13:06"
        },
        {
          "id": "MDRW2100042995.1.15",
          "form": "허........ 맞아요.....사실 그거 어제 먹었아요...넘 맛있죠ㅜㅜ",
          "original_form": "허........ 맞아요.....사실 그거 어제 먹었아요...넘 맛있죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 13:08"
        },
        {
          "id": "MDRW2100042995.1.16",
          "form": "아ㅠ맛잇엇겟다",
          "original_form": "아ㅠ맛잇엇겟다",
          "speaker_id": "1",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100042995.1.17",
          "form": "얼려드시나요 구워드시나요",
          "original_form": "얼려드시나요 구워드시나요",
          "speaker_id": "1",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100042995.1.18",
          "form": "헉 홈런볼 구워먹나요?!",
          "original_form": "헉 홈런볼 구워먹나요?!",
          "speaker_id": "2",
          "time": "20210907 13:11"
        },
        {
          "id": "MDRW2100042995.1.19",
          "form": "에프 160도 5분돌리면",
          "original_form": "에프 160도 5분돌리면",
          "speaker_id": "1",
          "time": "20210907 13:13"
        },
        {
          "id": "MDRW2100042995.1.20",
          "form": "따끈바삭함에 살살녹아요",
          "original_form": "따끈바삭함에 살살녹아요",
          "speaker_id": "1",
          "time": "20210907 13:13"
        },
        {
          "id": "MDRW2100042995.1.21",
          "form": "추천합니당",
          "original_form": "추천합니당",
          "speaker_id": "1",
          "time": "20210907 13:13"
        },
        {
          "id": "MDRW2100042995.1.22",
          "form": "와... 저 상상도 못해본 방법이에요... 하나 더 사서 그렇게 먹어봐야겠어요",
          "original_form": "와... 저 상상도 못해본 방법이에요... 하나 더 사서 그렇게 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 13:13"
        },
        {
          "id": "MDRW2100042995.1.23",
          "form": "바로먹음 초코땜에 뜨거운데",
          "original_form": "바로먹음 초코땜에 뜨거운데",
          "speaker_id": "1",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100042995.1.24",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100042995.1.25",
          "form": "기영이처럼 후불어드세요ㄹㅇ",
          "original_form": "기영이처럼 후불어드세요ㄹㅇ",
          "speaker_id": "1",
          "time": "20210907 13:15"
        }
      ]
    }
