      "utterance": [
        {
          "id": "MDRW2100021075.1.1",
          "form": "전 피자는 피자헛걸 주로 먹어요",
          "original_form": "전 피자는 피자헛걸 주로 먹어요",
          "speaker_id": "1",
          "time": "20210828 15:38"
        },
        {
          "id": "MDRW2100021075.1.2",
          "form": "테이크아웃 할인률이좋아서요",
          "original_form": "테이크아웃 할인률이좋아서요",
          "speaker_id": "1",
          "time": "20210828 15:38"
        },
        {
          "id": "MDRW2100021075.1.3",
          "form": "가끔 통신사 할인 할때 좋아요40%할인",
          "original_form": "가끔 통신사 할인 할때 좋아요40%할인",
          "speaker_id": "2",
          "time": "20210828 15:39"
        },
        {
          "id": "MDRW2100021075.1.4",
          "form": "음식은 주로 어떤것 해서 드시나요?",
          "original_form": "음식은 주로 어떤것 해서 드시나요?",
          "speaker_id": "2",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100021075.1.5",
          "form": "전 좀 쉬운거요 토마토 계란볶음 같은거요 name2님은요?",
          "original_form": "전 좀 쉬운거요 토마토 계란볶음 같은거요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100021075.1.6",
          "form": "전 부대찌개. 미역국. 김치찌개",
          "original_form": "전 부대찌개. 미역국. 김치찌개",
          "speaker_id": "2",
          "time": "20210828 15:41"
        },
        {
          "id": "MDRW2100021075.1.7",
          "form": "우와 국종류를 잘하시는 구나 국 저도 좋아해요",
          "original_form": "우와 국종류를 잘하시는 구나 국 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210828 15:41"
        },
        {
          "id": "MDRW2100021075.1.8",
          "form": "다른 반찬많이 필요하지 않은. 음식 위주로 해요",
          "original_form": "다른 반찬많이 필요하지 않은. 음식 위주로 해요",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021075.1.9",
          "form": "메인 음식 김치",
          "original_form": "메인 음식 김치",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021075.1.10",
          "form": "아하 전 좀 편한 메인반찬만드는걸 좋아해요",
          "original_form": "아하 전 좀 편한 메인반찬만드는걸 좋아해요",
          "speaker_id": "1",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100021075.1.11",
          "form": "토마토 볶음ㄱㅌ은,,",
          "original_form": "토마토 볶음ㄱㅌ은,,",
          "speaker_id": "1",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100021075.1.12",
          "form": "토마토 계란볶음도 맛있죠",
          "original_form": "토마토 계란볶음도 맛있죠",
          "speaker_id": "2",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100021075.1.13",
          "form": "쉽고 편하구요 ㅋㅋ 너무 조 ㅎ아요",
          "original_form": "쉽고 편하구요 ㅋㅋ 너무 조 ㅎ아요",
          "speaker_id": "1",
          "time": "20210828 15:51"
        },
        {
          "id": "MDRW2100021075.1.14",
          "form": "김치볶음밥도 즐겨해먹어요",
          "original_form": "김치볶음밥도 즐겨해먹어요",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100021075.1.15",
          "form": "저도요 편하고 김치처리도 하고,,, 아무거나 넣어도 ㅁ사잇구요",
          "original_form": "저도요 편하고 김치처리도 하고,,, 아무거나 넣어도 ㅁ사잇구요",
          "speaker_id": "1",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100021075.1.16",
          "form": "김볶은 참치파? 돼지고기파? 햄파? 뭐세요?",
          "original_form": "김볶은 참치파? 돼지고기파? 햄파? 뭐세요?",
          "speaker_id": "1",
          "time": "20210828 15:54"
        },
        {
          "id": "MDRW2100021075.1.17",
          "form": "스팸도 넣고 참치도넣고가끔 고기도 넣는데 스팸이 젤 맛나요",
          "original_form": "스팸도 넣고 참치도넣고가끔 고기도 넣는데 스팸이 젤 맛나요",
          "speaker_id": "2",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100021075.1.18",
          "form": "그렇군요 짭잘하고 돼지라서 좋아요",
          "original_form": "그렇군요 짭잘하고 돼지라서 좋아요",
          "speaker_id": "1",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100021075.1.19",
          "form": "참치나 일반 고기는 간이 좀 덜된느낌이죠",
          "original_form": "참치나 일반 고기는 간이 좀 덜된느낌이죠",
          "speaker_id": "1",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100021075.1.20",
          "form": "굴소스를 넣어보세요. 볶음요리에 굴소스 넣으면 맛있어요",
          "original_form": "굴소스를 넣어보세요. 볶음요리에 굴소스 넣으면 맛있어요",
