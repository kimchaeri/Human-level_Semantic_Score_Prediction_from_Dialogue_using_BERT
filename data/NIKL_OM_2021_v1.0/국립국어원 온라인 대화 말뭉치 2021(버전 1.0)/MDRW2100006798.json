      "utterance": [
        {
          "id": "MDRW2100006798.1.1",
          "form": "계란 좋죠",
          "original_form": "계란 좋죠",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006798.1.2",
          "form": "혹시 미나리 넣어보셨어요??",
          "original_form": "혹시 미나리 넣어보셨어요??",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100006798.1.3",
          "form": "가끔 치즈도 널어요 !!",
          "original_form": "가끔 치즈도 널어요 !!",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006798.1.4",
          "form": "오 미나리는 아직 ..!!",
          "original_form": "오 미나리는 아직 ..!!",
          "speaker_id": "1",
          "time": "20210908 09:41"
        },
        {
          "id": "MDRW2100006798.1.5",
          "form": "인터넷 보고 한번 시도해 봤는데",
          "original_form": "인터넷 보고 한번 시도해 봤는데",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006798.1.6",
          "form": "의외로 맛있더라고요",
          "original_form": "의외로 맛있더라고요",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100006798.1.7",
          "form": "오오 잘어울릴거같아요!!!",
          "original_form": "오오 잘어울릴거같아요!!!",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100006798.1.8",
          "form": "예전에 붓기 뺀다구 우유도 넣엇어요 ㅋㅋ",
          "original_form": "예전에 붓기 뺀다구 우유도 넣엇어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100006798.1.9",
          "form": "저 그거 스펀지에서 봤는데 ㅋㅋㅋ",
          "original_form": "저 그거 스펀지에서 봤는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100006798.1.10",
          "form": "맛은 어떻던가요?",
          "original_form": "맛은 어떻던가요?",
          "speaker_id": "2",
          "time": "20210908 09:44"
        },
        {
          "id": "MDRW2100006798.1.11",
          "form": "ㅋㅋㅋ 약간 고소한..??",
          "original_form": "ㅋㅋㅋ 약간 고소한..??",
          "speaker_id": "1",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100006798.1.12",
          "form": "그냥 안먹는게 나을거같아요 ㅋㅋ",
          "original_form": "그냥 안먹는게 나을거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:47"
        },
        {
          "id": "MDRW2100006798.1.13",
          "form": "아하 ㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100006798.1.14",
          "form": "저는 치즈도 시도 해 본 적 없어서...",
          "original_form": "저는 치즈도 시도 해 본 적 없어서...",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100006798.1.15",
          "form": "아 근데 이거 나쁜말 필터 이상하네요 ㅠㅠ",
          "original_form": "아 근데 이거 나쁜말 필터 이상하네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100006798.1.16",
          "form": "남이 끓여주는 라면이 젤 좋은거같아요 ㅋㅋ",
          "original_form": "남이 끓여주는 라면이 젤 좋은거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100006798.1.17",
          "form": "역시 그렇죠 ㅋㅋㅋ",
          "original_form": "역시 그렇죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100006798.1.18",
          "form": "분식집 라면이 진짜 맛있드라구용",
          "original_form": "분식집 라면이 진짜 맛있드라구용",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100006798.1.19",
          "form": "분식집 라면 진짜 맛있죠",
          "original_form": "분식집 라면 진짜 맛있죠",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100006798.1.20",
          "form": "근데 좀 비싼 거 같아요 ㅠㅠ",
          "original_form": "근데 좀 비싼 거 같아요 ㅠㅠ",
