      "utterance": [
        {
          "id": "MDRW2100039758.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039758.1.2",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039758.1.3",
          "form": "네! 6~7개월된강아지키우고잇어용",
          "original_form": "네! 6~7개월된강아지키우고잇어용",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039758.1.4",
          "form": "어머나!",
          "original_form": "어머나!",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039758.1.5",
          "form": "너무 귀엽겠다 ㅠㅠ",
          "original_form": "너무 귀엽겠다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100039758.1.6",
          "form": "무슨 종이예요?",
          "original_form": "무슨 종이예요?",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039758.1.7",
          "form": "프렌치불독이용!",
          "original_form": "프렌치불독이용!",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039758.1.8",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039758.1.9",
          "form": "불독 털 많이 안빠지죠?",
          "original_form": "불독 털 많이 안빠지죠?",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100039758.1.10",
          "form": "애교 많아요??ㅠㅠ",
          "original_form": "애교 많아요??ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100039758.1.11",
          "form": "털 꽤 빠지는 편이에요 ㅠ 근데 짧아서 티는 많이안나는거같구 애교엄청많아요 ㅋㅋ 사람을 너무 조하앻서",
          "original_form": "털 꽤 빠지는 편이에요 ㅠ 근데 짧아서 티는 많이안나는거같구 애교엄청많아요 ㅋㅋ 사람을 너무 조하앻서",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039758.1.12",
          "form": "으아 너무 부럽네요 ㅠㅠㅠㅠ",
          "original_form": "으아 너무 부럽네요 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039758.1.13",
          "form": "이름이 뭐예요? 애기",
          "original_form": "이름이 뭐예요? 애기",
          "speaker_id": "1",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039758.1.14",
          "form": "고미요!",
          "original_form": "고미요!",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039758.1.15",
          "form": "쪼꼬미해서 고미!",
          "original_form": "쪼꼬미해서 고미!",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039758.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:18"
        },
        {
          "id": "MDRW2100039758.1.17",
          "form": "으악",
          "original_form": "으악",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039758.1.18",
          "form": "너무 귀여워요!!!><><",
          "original_form": "너무 귀여워요!!!><><",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039758.1.19",
          "form": "고미는 어떻게 입양하게 되셨어요??",
          "original_form": "고미는 어떻게 입양하게 되셨어요??",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039758.1.20",
          "form": "원래 직접 키우는 애기는 아니구 부모님이 데려오셨는데 부모님 일하는 사정상 두세달만 잠깐 제가 맡기로 했어요!",
          "original_form": "원래 직접 키우는 애기는 아니구 부모님이 데려오셨는데 부모님 일하는 사정상 두세달만 잠깐 제가 맡기로 했어요!",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039758.1.21",
          "form": "우와 그러시구나",
          "original_form": "우와 그러시구나",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039758.1.22",
          "form": "귀엽겠다...ㅠㅠ",
          "original_form": "귀엽겠다...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039758.1.23",
          "form": "앉아 할줄 알아요?ㅠㅠ",
          "original_form": "앉아 할줄 알아요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039758.1.24",
          "form": "넵! 하우스도 할줄알아요 ㅎㅎ",
          "original_form": "넵! 하우스도 할줄알아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039758.1.25",
          "form": "우와 짱 똑똑해 ㅠㅠㅠㅠ",
          "original_form": "우와 짱 똑똑해 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039758.1.26",
          "form": "산책 자주 가세요??",
          "original_form": "산책 자주 가세요??",
          "speaker_id": "1",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039758.1.27",
          "form": "넵 매일매일 나가요!",
          "original_form": "넵 매일매일 나가요!",
          "speaker_id": "2",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039758.1.28",
          "form": "우와 되게 좋은 반려주인 이네요ㅠㅠ",
          "original_form": "우와 되게 좋은 반려주인 이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039758.1.29",
          "form": "고미가 좋아하겠어요 ㅠㅠ귀엽겠다",
          "original_form": "고미가 좋아하겠어요 ㅠㅠ귀엽겠다",
          "speaker_id": "1",
          "time": "20210903 22:27"
        }
      ]
    }
