      "utterance": [
        {
          "id": "MDRW2100006009.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100006009.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100006009.1.3",
          "form": "네 여행 좋아합니다",
          "original_form": "네 여행 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 18:50"
        },
        {
          "id": "MDRW2100006009.1.4",
          "form": "코로나 이전에는 1년에 1번은 해외 나갔었어요",
          "original_form": "코로나 이전에는 1년에 1번은 해외 나갔었어요",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100006009.1.5",
          "form": "정말 좋아하셨네요 마지막 여행지는요?",
          "original_form": "정말 좋아하셨네요 마지막 여행지는요?",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100006009.1.6",
          "form": "기억이 가물가물한데",
          "original_form": "기억이 가물가물한데",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100006009.1.7",
          "form": "아마 일본같네요",
          "original_form": "아마 일본같네요",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100006009.1.8",
          "form": "일본 어느지역이요?",
          "original_form": "일본 어느지역이요?",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100006009.1.9",
          "form": "오사카 교토요",
          "original_form": "오사카 교토요",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100006009.1.10",
          "form": "여행 자주 다니시나요?",
          "original_form": "여행 자주 다니시나요?",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100006009.1.11",
          "form": "저도 일본 초밥 맛보고 싶어요..\r\n가족여행만 소소하게 다니는 편입니다",
          "original_form": "저도 일본 초밥 맛보고 싶어요..\r\n가족여행만 소소하게 다니는 편입니다",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100006009.1.12",
          "form": "ㅇㅎ 그렇군요 해외는 안나가보신거에요?",
          "original_form": "ㅇㅎ 그렇군요 해외는 안나가보신거에요?",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100006009.1.13",
          "form": "해외는 4-5번정도 가봤어요 아이 생기고는 괌만...",
          "original_form": "해외는 4-5번정도 가봤어요 아이 생기고는 괌만...",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100006009.1.14",
          "form": "아... 아이가 있으셨군요",
          "original_form": "아... 아이가 있으셨군요",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100006009.1.15",
          "form": "그러면 어쩔수없이..",
          "original_form": "그러면 어쩔수없이..",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100006009.1.16",
          "form": "괌 좋죠 레저하기 딱인데",
          "original_form": "괌 좋죠 레저하기 딱인데",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006009.1.17",
          "form": "쇼핑이랑",
          "original_form": "쇼핑이랑",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006009.1.18",
          "form": "괌 가보셨나요?",
          "original_form": "괌 가보셨나요?",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100006009.1.19",
          "form": "넵 가봤어요ㅎㅎ",
          "original_form": "넵 가봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100006009.1.20",
          "form": "리티디안 비치가 젤 기억에 남네요",
          "original_form": "리티디안 비치가 젤 기억에 남네요",
          "speaker_id": "2",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100006009.1.21",
          "form": "저는 못갔어요 거기 가는길이 제법 험하다고 본 것 같은데ㅜㅜ",
          "original_form": "저는 못갔어요 거기 가는길이 제법 험하다고 본 것 같은데ㅜㅜ",
          "speaker_id": "1",
