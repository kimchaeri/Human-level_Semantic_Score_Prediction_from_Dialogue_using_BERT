      "utterance": [
        {
          "id": "MDRW2100011091.1.1",
          "form": "헉 아직도요?",
          "original_form": "헉 아직도요?",
          "speaker_id": "2",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011091.1.2",
          "form": "여기는 비 그쳐서 귀뚜라미 소리만 들려요",
          "original_form": "여기는 비 그쳐서 귀뚜라미 소리만 들려요",
          "speaker_id": "2",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011091.1.3",
          "form": "지금은 괜찮네요 낮은 고도는 물이 잠겼다던데 거긴 괜찮나요?",
          "original_form": "지금은 괜찮네요 낮은 고도는 물이 잠겼다던데 거긴 괜찮나요?",
          "speaker_id": "1",
          "time": "20210824 08:59"
        },
        {
          "id": "MDRW2100011091.1.4",
          "form": "여기는 비가 많이오진 않아서 괜찮은것같아요",
          "original_form": "여기는 비가 많이오진 않아서 괜찮은것같아요",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011091.1.5",
          "form": "뒤늦은 물난리네요 ㅠㅠ",
          "original_form": "뒤늦은 물난리네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011091.1.6",
          "form": "어우 그러게요 지금도 계속와요 여긴",
          "original_form": "어우 그러게요 지금도 계속와요 여긴",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011091.1.7",
          "form": "밖은 시원한데 안이 덥네요ㅠ",
          "original_form": "밖은 시원한데 안이 덥네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011091.1.8",
          "form": "아무래도 창문을 못열어듀니까요 ㅠ",
          "original_form": "아무래도 창문을 못열어듀니까요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011091.1.9",
          "form": "그래도 이번만 지나면 시원해지니까요",
          "original_form": "그래도 이번만 지나면 시원해지니까요",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011091.1.10",
          "form": "가을 좋아하세요?",
          "original_form": "가을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011091.1.11",
          "form": "네! 가을에 산책하는거 너무 좋아해요",
          "original_form": "네! 가을에 산책하는거 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011091.1.12",
          "form": "가을 산책하기 너무 좋은 계절이죠",
          "original_form": "가을 산책하기 너무 좋은 계절이죠",
          "speaker_id": "2",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011091.1.13",
          "form": "볼것도 많고!",
          "original_form": "볼것도 많고!",
          "speaker_id": "2",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011091.1.14",
          "form": "궁궐이나 공원가면 낙엽도 이쁘고 !",
          "original_form": "궁궐이나 공원가면 낙엽도 이쁘고 !",
          "speaker_id": "1",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100011091.1.15",
          "form": "얼른 가을 왔으면 좋겠어요",
          "original_form": "얼른 가을 왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100011091.1.16",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100011091.1.17",
          "form": "코시국만 아니면",
          "original_form": "코시국만 아니면",
          "speaker_id": "2",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100011091.1.18",
          "form": "오뎅도 열심히 먹을 계절인데",
          "original_form": "오뎅도 열심히 먹을 계절인데",
          "speaker_id": "2",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100011091.1.19",
          "form": "오뎅 타코야끼 붕어빵의 계절이 오고있어여",
          "original_form": "오뎅 타코야끼 붕어빵의 계절이 오고있어여",
          "speaker_id": "1",
          "time": "20210824 20:23"
        },
        {
          "id": "MDRW2100011091.1.20",
          "form": "하아",
          "original_form": "하아",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100011091.1.21",
          "form": "너무 맛있겠다...",
          "original_form": "너무 맛있겠다...",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100011091.1.22",
          "form": "길거리 떡볶이 못먹는개 한이에요",
          "original_form": "길거리 떡볶이 못먹는개 한이에요",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100011091.1.23",
          "form": "예전처럼 오백원치 컵에 담아서 팔아주면. 좋겠어요",
          "original_form": "예전처럼 오백원치 컵에 담아서 팔아주면. 좋겠어요",
          "speaker_id": "1",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011091.1.24",
          "form": "추울때 딱 그만큼 먹고싶은때가 있는데...",
          "original_form": "추울때 딱 그만큼 먹고싶은때가 있는데...",
          "speaker_id": "1",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100011091.1.25",
          "form": "아..너무 좋죠..",
          "original_form": "아..너무 좋죠..",
          "speaker_id": "2",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100011091.1.26",
          "form": "코로나만아니면..",
          "original_form": "코로나만아니면..",
          "speaker_id": "2",
          "time": "20210824 20:53"
        }
      ]
    }
