      "utterance": [
        {
          "id": "MDRW2100043495.1.1",
          "form": "추리영화나 드라마도 좋아하시나요?",
          "original_form": "추리영화나 드라마도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043495.1.2",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043495.1.3",
          "form": "그런.. 스릴러? 좋아해요",
          "original_form": "그런.. 스릴러? 좋아해요",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043495.1.4",
          "form": "근데 잘 만들어야제 보다보면 다 알겠더라구요",
          "original_form": "근데 잘 만들어야제 보다보면 다 알겠더라구요",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043495.1.5",
          "form": ";-;",
          "original_form": ";-;",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043495.1.6",
          "form": "아 맞아요ㅋㅋㅋㅋ 허술한 거.. 정말 .. 김빠져요",
          "original_form": "아 맞아요ㅋㅋㅋㅋ 허술한 거.. 정말 .. 김빠져요",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043495.1.7",
          "form": "재밌게 보신거 어떤거 있으신가요?",
          "original_form": "재밌게 보신거 어떤거 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100043495.1.8",
          "form": "가장 최근에 나이브스 아웃 봤어요 ㅋㅋ 재미있더라구요",
          "original_form": "가장 최근에 나이브스 아웃 봤어요 ㅋㅋ 재미있더라구요",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043495.1.9",
          "form": "저도 그거 재밌게봤어요 영화관가서 봤었는데 결말이 기억안나서 또 봤는데 재밌더라구요 ㅋㅋ",
          "original_form": "저도 그거 재밌게봤어요 영화관가서 봤었는데 결말이 기억안나서 또 봤는데 재밌더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100043495.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그쳐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그쳐",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043495.1.11",
          "form": "아 영화관 안 단지도 꽤 돾내요ㅠㅠ",
          "original_form": "아 영화관 안 단지도 꽤 돾내요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043495.1.12",
          "form": "넷플 추천에 떠서 봤어요",
          "original_form": "넷플 추천에 떠서 봤어요",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043495.1.13",
          "form": "그럼 셜록홈즈도 보셨나요??",
          "original_form": "그럼 셜록홈즈도 보셨나요??",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043495.1.14",
          "form": "네 드라마영화 다 봤어요 ㅋㅋㅋ",
          "original_form": "네 드라마영화 다 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043495.1.15",
          "form": "진짜 드라마 너무 재밌어요 ㅋㅋㅋ",
          "original_form": "진짜 드라마 너무 재밌어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043495.1.16",
          "form": "ㅋㅋㅋㅋ시즌 3까지는요...",
          "original_form": "ㅋㅋㅋㅋ시즌 3까지는요...",
          "speaker_id": "2",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043495.1.17",
          "form": "아이린애들러 편 .. 진짜 대박이에요",
          "original_form": "아이린애들러 편 .. 진짜 대박이에요",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043495.1.18",
          "form": "마자여",
          "original_form": "마자여",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043495.1.19",
          "form": "아이넴락드 이거 진짜 센세이션",
          "original_form": "아이넴락드 이거 진짜 센세이션",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043495.1.20",
          "form": "진짜... 감탄하면서 봤네요 ㅋㅋ",
          "original_form": "진짜... 감탄하면서 봤네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043495.1.21",
          "form": "그때 진짜 그 테마? 배경화면도 인기 많았던 거 같아요",
          "original_form": "그때 진짜 그 테마? 배경화면도 인기 많았던 거 같아요",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043495.1.22",
          "form": "베네딕트 컴버배치가 그걸로 확 뜨지 않았나요,",
          "original_form": "베네딕트 컴버배치가 그걸로 확 뜨지 않았나요,",
          "speaker_id": "2",
