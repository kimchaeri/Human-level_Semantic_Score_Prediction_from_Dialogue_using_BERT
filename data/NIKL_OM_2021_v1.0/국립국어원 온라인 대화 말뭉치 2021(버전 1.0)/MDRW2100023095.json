      "utterance": [
        {
          "id": "MDRW2100023095.1.1",
          "form": "야구",
          "original_form": "야구",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023095.1.2",
          "form": "오오ㅎ 반려동물입니까?",
          "original_form": "오오ㅎ 반려동물입니까?",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023095.1.3",
          "form": "지금키우세요?",
          "original_form": "지금키우세요?",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023095.1.4",
          "form": "아뇨 궁금해요",
          "original_form": "아뇨 궁금해요",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023095.1.5",
          "form": "요새 주변에 강아지들도 많이보이고 ㅋㅋ",
          "original_form": "요새 주변에 강아지들도 많이보이고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023095.1.6",
          "form": "아ㅎㅎㅈ",
          "original_form": "아ㅎㅎㅈ",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023095.1.7",
          "form": "저 비글 키웠었어요ㅎ",
          "original_form": "저 비글 키웠었어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023095.1.8",
          "form": "오오 귀여웠을것같아요 엄청 똑똑하다고도 들은듯 ㅎㅎ",
          "original_form": "오오 귀여웠을것같아요 엄청 똑똑하다고도 들은듯 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023095.1.9",
          "form": "엄청 똑똑해요ㅎㅎ",
          "original_form": "엄청 똑똑해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023095.1.10",
          "form": "완전ㅎ 근데 불쌍해요ㅠㅍ",
          "original_form": "완전ㅎ 근데 불쌍해요ㅠㅍ",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023095.1.11",
          "form": "비글이 사람들엄청좋아하고 충성심이 강해요",
          "original_form": "비글이 사람들엄청좋아하고 충성심이 강해요",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023095.1.12",
          "form": "그래서 시험견으로 쓰인대요",
          "original_form": "그래서 시험견으로 쓰인대요",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023095.1.13",
          "form": "면역력도 쎄서 별도의 건강문제도없어서그런지 시험견으로 쓰니ㅠ",
          "original_form": "면역력도 쎄서 별도의 건강문제도없어서그런지 시험견으로 쓰니ㅠ",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023095.1.14",
          "form": "기르시던 강아지는 이름이 뭐였어요? ㅎㅎ",
          "original_form": "기르시던 강아지는 이름이 뭐였어요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023095.1.15",
          "form": "남자는 구글이고 여자는 베이글이요ㅎㅎㅎ",
          "original_form": "남자는 구글이고 여자는 베이글이요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023095.1.16",
          "form": "글자돌림ㅎ",
          "original_form": "글자돌림ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023095.1.17",
          "form": "어머 두마리나 키우셨구나 어쩌다가 키우시게되신건가요 ㅎㅎ",
          "original_form": "어머 두마리나 키우셨구나 어쩌다가 키우시게되신건가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023095.1.18",
          "form": "혼자사는데 강아지 기르고싶었어요ㅎ",
          "original_form": "혼자사는데 강아지 기르고싶었어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023095.1.19",
          "form": "그리고 비글이 운동하기 너무좋아요ㅎ 에너지가 넘쳐서ㅎㅎ",
          "original_form": "그리고 비글이 운동하기 너무좋아요ㅎ 에너지가 넘쳐서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023095.1.20",
          "form": "맨날 두마리데리고 산책갔었어요ㅎ",
          "original_form": "맨날 두마리데리고 산책갔었어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023095.1.21",
          "form": "에고 지금은 없어서 보고싶으시겠어요",
          "original_form": "에고 지금은 없어서 보고싶으시겠어요",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023095.1.22",
          "form": "글죠 보고싶어요ㅎ",
          "original_form": "글죠 보고싶어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023095.1.23",
          "form": "저는 제가 작아서 작은개를키우고싶더라고요",
          "original_form": "저는 제가 작아서 작은개를키우고싶더라고요",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023095.1.24",
          "form": "마음은 차우차우도 키우고싶지잉ㆍㄴ",
          "original_form": "마음은 차우차우도 키우고싶지잉ㆍㄴ",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023095.1.25",
          "form": "지만 ㅋㅋ",
          "original_form": "지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023095.1.26",
          "form": "리트리버도",
          "original_form": "리트리버도",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023095.1.27",
          "form": "산책을 시키는게 아니라 당할듯 ㅋㅋ",
          "original_form": "산책을 시키는게 아니라 당할듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023095.1.28",
          "form": "작은개가 좋아요ㅎㅎㅎㅎ",
          "original_form": "작은개가 좋아요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023095.1.29",
          "form": "똥....ㅠ",
          "original_form": "똥....ㅠ",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023095.1.30",
          "form": "앗죄송요ㅎㅎㅎ",
          "original_form": "앗죄송요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023095.1.31",
          "form": "사람만하게 싸요ㅎ",
          "original_form": "사람만하게 싸요ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100023095.1.32",
          "form": "그죠 ㅋㅋ뭔가 영화와 현실은 다르겠져 ㅋㅋㅋ",
          "original_form": "그죠 ㅋㅋ뭔가 영화와 현실은 다르겠져 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:54"
        }
      ]
