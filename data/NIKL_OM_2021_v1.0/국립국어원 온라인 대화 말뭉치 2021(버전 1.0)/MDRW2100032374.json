      "utterance": [
        {
          "id": "MDRW2100032374.1.1",
          "form": "요즘에 탄산수 많이 먹잖아",
          "original_form": "요즘에 탄산수 많이 먹잖아",
          "speaker_id": "2",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100032374.1.2",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100032374.1.3",
          "form": "이유가 있나?",
          "original_form": "이유가 있나?",
          "speaker_id": "2",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100032374.1.4",
          "form": "시럽 안들어가서 칼로리가 낮으니까?",
          "original_form": "시럽 안들어가서 칼로리가 낮으니까?",
          "speaker_id": "1",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100032374.1.5",
          "form": "탄산수 브랜드도 엄청 많더라",
          "original_form": "탄산수 브랜드도 엄청 많더라",
          "speaker_id": "2",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100032374.1.6",
          "form": "먹어봤어?",
          "original_form": "먹어봤어?",
          "speaker_id": "1",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100032374.1.7",
          "form": "응 근데 나랑은 안맞더라고 ㅋㅋㅋ",
          "original_form": "응 근데 나랑은 안맞더라고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100032374.1.8",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100032374.1.9",
          "form": "따끔거리기만 하고 맛도 없고",
          "original_form": "따끔거리기만 하고 맛도 없고",
          "speaker_id": "2",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100032374.1.10",
          "form": "아 탄산 별로라고 했지",
          "original_form": "아 탄산 별로라고 했지",
          "speaker_id": "1",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100032374.1.11",
          "form": "응 탄산수도 별로야",
          "original_form": "응 탄산수도 별로야",
          "speaker_id": "2",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100032374.1.12",
          "form": "난 그 청량감?",
          "original_form": "난 그 청량감?",
          "speaker_id": "1",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100032374.1.13",
          "form": "그게 좋아",
          "original_form": "그게 좋아",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100032374.1.14",
          "form": "느끼한 거 먹을 때 탄산음료 먹긴 해",
          "original_form": "느끼한 거 먹을 때 탄산음료 먹긴 해",
          "speaker_id": "2",
          "time": "20210905 16:37"
