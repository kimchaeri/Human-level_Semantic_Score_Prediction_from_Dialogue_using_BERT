      "utterance": [
        {
          "id": "MDRW2100028341.1.1",
          "form": "저 진짜....열심히 해서 꽉 채웠어요ㅋㅋㅋ",
          "original_form": "저 진짜....열심히 해서 꽉 채웠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:14"
        },
        {
          "id": "MDRW2100028341.1.2",
          "form": "불교신자지만 성경내용 거의 알고있는 1인...",
          "original_form": "불교신자지만 성경내용 거의 알고있는 1인...",
          "speaker_id": "1",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028341.1.3",
          "form": "ㅋㅋㅋㅋㅋ 전 그냥 대츙 들어서",
          "original_form": "ㅋㅋㅋㅋㅋ 전 그냥 대츙 들어서",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028341.1.4",
          "form": "학점이 네...",
          "original_form": "학점이 네...",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100028341.1.5",
          "form": "ㅜㅠ 그럼 여태껏 교회나 성당이나 절에 다녀보신 적 없으신건가요",
          "original_form": "ㅜㅠ 그럼 여태껏 교회나 성당이나 절에 다녀보신 적 없으신건가요",
          "speaker_id": "1",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100028341.1.6",
          "form": "교회 진짜 어릴 때요",
          "original_form": "교회 진짜 어릴 때요",
          "speaker_id": "2",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100028341.1.7",
          "form": "여름성경학교 이런 거 아시나요ㅋㅋㅋㅋ",
          "original_form": "여름성경학교 이런 거 아시나요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100028341.1.8",
          "form": "아 뭔지 알 것 같아요ㅋㅋ",
          "original_form": "아 뭔지 알 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028341.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 그런 거 때문에",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그런 거 때문에",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028341.1.10",
          "form": "갔었어요",
          "original_form": "갔었어요",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028341.1.11",
          "form": "아아ㅋㅋㅋ",
          "original_form": "아아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100028341.1.12",
          "form": "막 찬송가부르고 손뼉치고 그러지않았나요ㅋㅋ",
          "original_form": "막 찬송가부르고 손뼉치고 그러지않았나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028341.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ어릴 때라 그건 기억 안나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ어릴 때라 그건 기억 안나요",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100028341.1.14",
          "form": "물놀이만..",
          "original_form": "물놀이만..",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100028341.1.15",
          "form": "ㅋㅋ어릴 때라 즐거웠던 기억만 나나봐요",
          "original_form": "ㅋㅋ어릴 때라 즐거웠던 기억만 나나봐요",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028341.1.16",
          "form": "ㅋㅋㅋㅋ네 그렇죠!",
          "original_form": "ㅋㅋㅋㅋ네 그렇죠!",
          "speaker_id": "2",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100028341.1.17",
          "form": "조만간 시간 나면 저는 절에 가서 머리 좀 식혀야겠어요ㅎ",
          "original_form": "조만간 시간 나면 저는 절에 가서 머리 좀 식혀야겠어요ㅎ",
