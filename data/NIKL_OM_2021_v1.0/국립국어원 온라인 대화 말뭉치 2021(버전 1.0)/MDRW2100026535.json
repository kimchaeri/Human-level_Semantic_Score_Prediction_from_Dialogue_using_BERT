      "utterance": [
        {
          "id": "MDRW2100026535.1.1",
          "form": "성산 숙소 치니까 바로 나오네요",
          "original_form": "성산 숙소 치니까 바로 나오네요",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026535.1.2",
          "form": "원래 진짜쌋는데",
          "original_form": "원래 진짜쌋는데",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026535.1.3",
          "form": "저는 담에 장기간 갈수있음",
          "original_form": "저는 담에 장기간 갈수있음",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026535.1.4",
          "form": "제차를 가져 가고 싶어요ㅋ",
          "original_form": "제차를 가져 가고 싶어요ㅋ",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026535.1.5",
          "form": "오 배타고용",
          "original_form": "오 배타고용",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026535.1.6",
          "form": "요즘엔 샌딩해준다 하더라구여",
          "original_form": "요즘엔 샌딩해준다 하더라구여",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026535.1.7",
          "form": "그거도 좋네여 오래잇으면 ㅠㅠ",
          "original_form": "그거도 좋네여 오래잇으면 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026535.1.8",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026535.1.9",
          "form": "그게 더 비싸려나ㅜ",
          "original_form": "그게 더 비싸려나ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026535.1.10",
          "form": "랜트가 하도 비싸다 하니까",
          "original_form": "랜트가 하도 비싸다 하니까",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026535.1.11",
          "form": "속편하게 그러는 사람 많아졌대여",
          "original_form": "속편하게 그러는 사람 많아졌대여",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026535.1.12",
          "form": "아아아 그럴만한거같아요 진짜 싸게하면  하루 이삼만원도 하던게 이제 그냥 십마넌 넘으니",
          "original_form": "아아아 그럴만한거같아요 진짜 싸게하면  하루 이삼만원도 하던게 이제 그냥 십마넌 넘으니",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026535.1.13",
          "form": "혹시 가시면 전기차 추천드려요",
          "original_form": "혹시 가시면 전기차 추천드려요",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026535.1.14",
          "form": "이번에 전기차 빌렸는데 4박5일동안 충전 없이 잘썼어요",
          "original_form": "이번에 전기차 빌렸는데 4박5일동안 충전 없이 잘썼어요",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026535.1.15",
          "form": "코로나라 어디 안가고 해안드라이브 많이 했는데",
          "original_form": "코로나라 어디 안가고 해안드라이브 많이 했는데",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026535.1.16",
          "form": "오 ?전기차 오래가네요",
          "original_form": "오 ?전기차 오래가네요",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026535.1.17",
          "form": "어디해안 젤 조으셧어요",
          "original_form": "어디해안 젤 조으셧어요",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026535.1.18",
          "form": "저는 돌고래도 봤어요",
          "original_form": "저는 돌고래도 봤어요",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026535.1.19",
          "form": "오 돌고래!!!",
          "original_form": "오 돌고래!!!",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026535.1.20",
          "form": "그 풍차있는덴데",
          "original_form": "그 풍차있는덴데",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026535.1.21",
          "form": "조으셨겟다ㅠㅠ",
          "original_form": "조으셨겟다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026535.1.22",
