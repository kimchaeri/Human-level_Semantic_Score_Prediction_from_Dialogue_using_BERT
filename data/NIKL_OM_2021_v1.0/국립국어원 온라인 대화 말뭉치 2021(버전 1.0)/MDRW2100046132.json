      "utterance": [
        {
          "id": "MDRW2100046132.1.1",
          "form": "근데 백신예약도 카카오로 하는 거 보고",
          "original_form": "근데 백신예약도 카카오로 하는 거 보고",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046132.1.2",
          "form": "카카오대체채팅앱도 이제 나왔음좋겠네요",
          "original_form": "카카오대체채팅앱도 이제 나왔음좋겠네요",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046132.1.3",
          "form": "좀 무서워졌어요",
          "original_form": "좀 무서워졌어요",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046132.1.4",
          "form": "백신예약을카카오로하다니",
          "original_form": "백신예약을카카오로하다니",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046132.1.5",
          "form": "나중에 춘식이가 신호등에도 박히진 않겠죠...?",
          "original_form": "나중에 춘식이가 신호등에도 박히진 않겠죠...?",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046132.1.6",
          "form": "저는걍인터넷으루했는데",
          "original_form": "저는걍인터넷으루했는데",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046132.1.7",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046132.1.8",
          "form": "막 표지판에도 나오고...",
          "original_form": "막 표지판에도 나오고...",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046132.1.9",
          "form": "끔찍ㅎㅡ하네요",
          "original_form": "끔찍ㅎㅡ하네요",
          "speaker_id": "1",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100046132.1.10",
          "form": "카카오국가",
          "original_form": "카카오국가",
          "speaker_id": "1",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100046132.1.11",
          "form": "아ㅜㅜㅜ",
          "original_form": "아ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100046132.1.12",
          "form": "노란색 구려서 싫은데",
          "original_form": "노란색 구려서 싫은데",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100046132.1.13",
          "form": "아진짜 라이언이랑 주입식귀여움이에요",
          "original_form": "아진짜 라이언이랑 주입식귀여움이에요",
          "speaker_id": "1",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100046132.1.14",
          "form": "좀 이쁜 노랑이면  몰라요",
          "original_form": "좀 이쁜 노랑이면  몰라요",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100046132.1.15",
          "form": "카카오 색이 너무 누래요",
          "original_form": "카카오 색이 너무 누래요",
          "speaker_id": "2",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100046132.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:38"
        },
        {
          "id": "MDRW2100046132.1.17",
          "form": "네이버가 열일했으면",
          "original_form": "네이버가 열일했으면",
          "speaker_id": "1",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046132.1.18",
          "form": "그러게요ㅜㅜ",
          "original_form": "그러게요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046132.1.19",
          "form": "그냥 어느 한쪽이 독점하지 않았으면...",
          "original_form": "그냥 어느 한쪽이 독점하지 않았으면...",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046132.1.20",
          "form": "그래도 네이버는 네이버페이 때문인가",
          "original_form": "그래도 네이버는 네이버페이 때문인가",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046132.1.21",
          "form": "좀 더 이미지가 좋긴 하네욬ㅋㅋㅋㅋ",
          "original_form": "좀 더 이미지가 좋긴 하네욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100046132.1.22",
          "form": "네 ㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ",
