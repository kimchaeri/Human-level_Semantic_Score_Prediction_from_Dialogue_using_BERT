      "utterance": [
        {
          "id": "MDRW2100036928.1.1",
          "form": "짬뽕밥1 간짜장1시키면",
          "original_form": "짬뽕밥1 간짜장1시키면",
          "speaker_id": "2",
          "time": "20210831 15:08"
        },
        {
          "id": "MDRW2100036928.1.2",
          "form": "와 너무 지혜로우신데 ㅋㅋㅋ",
          "original_form": "와 너무 지혜로우신데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100036928.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100036928.1.4",
          "form": "짬뽕 짜장 짬뽕밥 짜장밥 4가지먹을수있어서 글케시켜요",
          "original_form": "짬뽕 짜장 짬뽕밥 짜장밥 4가지먹을수있어서 글케시켜요",
          "speaker_id": "2",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100036928.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 진짜 너무 기발하시네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 진짜 너무 기발하시네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100036928.1.6",
          "form": "너무 좋다 ㅋㅋㅋ",
          "original_form": "너무 좋다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:09"
        },
        {
          "id": "MDRW2100036928.1.7",
          "form": "저도 만약에 그런일이 잇으면 그렇게 해봐야겟어요 ㅜㅜ\r\n아 진짜 뭐라도 그렇게파나보네요 ㅜ",
          "original_form": "저도 만약에 그런일이 잇으면 그렇게 해봐야겟어요 ㅜㅜ\r\n아 진짜 뭐라도 그렇게파나보네요 ㅜ",
          "speaker_id": "1",
          "time": "20210831 15:10"
        },
        {
          "id": "MDRW2100036928.1.8",
          "form": "네 전 다양하게먹는거좋아해서 ㅋㅋㅋㅋㅋ글고 가성비도챙기구여",
          "original_form": "네 전 다양하게먹는거좋아해서 ㅋㅋㅋㅋㅋ글고 가성비도챙기구여",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100036928.1.9",
          "form": "ㅋㅋㅋㅋㅋ 오 저랑 똑같으시다!!\r\n저도 다양하게먹는거 진짜 좋아해요 ㅋㅋㅋㅋ\r\n그래서 맥주사면 이젠 그림보고 고르죸ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 오 저랑 똑같으시다!!\r\n저도 다양하게먹는거 진짜 좋아해요 ㅋㅋㅋㅋ\r\n그래서 맥주사면 이젠 그림보고 고르죸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:13"
        },
        {
          "id": "MDRW2100036928.1.10",
          "form": "(맥주는 취향에따라다르겟져 ㅋㅋ)",
          "original_form": "(맥주는 취향에따라다르겟져 ㅋㅋ)",
          "speaker_id": "1",
          "time": "20210831 15:14"
        },
        {
          "id": "MDRW2100036928.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ오오 전 술찔이라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ오오 전 술찔이라",
          "speaker_id": "2",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100036928.1.12",
          "form": "술에대핸 잘모르는데",
          "original_form": "술에대핸 잘모르는데",
          "speaker_id": "2",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100036928.1.13",
          "form": "맥주어떤거 젤좋아하세요",
          "original_form": "맥주어떤거 젤좋아하세요",
          "speaker_id": "2",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100036928.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 빅웨이브라는 술이랑 홉고블린 이런거 좋아해용 ㅋㅋ 카스나 하이트랑은 좀 달라요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 빅웨이브라는 술이랑 홉고블린 이런거 좋아해용 ㅋㅋ 카스나 하이트랑은 좀 달라요",
          "speaker_id": "1",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100036928.1.15",
          "form": "수입맥주요 ㅎㅎ",
          "original_form": "수입맥주요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100036928.1.16",
          "form": "오 맛 차이가 어떻게 달라요?",
          "original_form": "오 맛 차이가 어떻게 달라요?",
          "speaker_id": "2",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100036928.1.17",
          "form": "전 다똑같은 시고 쓴 보리맛이던데ㅜ",
          "original_form": "전 다똑같은 시고 쓴 보리맛이던데ㅜ",
          "speaker_id": "2",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100036928.1.18",
          "form": "빅웨이브는 좀 과일향? 복숭아향 같은게나고 부두러운데",
          "original_form": "빅웨이브는 좀 과일향? 복숭아향 같은게나고 부두러운데",
          "speaker_id": "1",
          "time": "20210831 15:21"
        },
        {
          "id": "MDRW2100036928.1.19",
          "form": "카스는 좀 그냥 우리나라에서 무난히시키는 그런 맥주죠",
          "original_form": "카스는 좀 그냥 우리나라에서 무난히시키는 그런 맥주죠",
          "speaker_id": "1",
          "time": "20210831 15:21"
        },
        {
          "id": "MDRW2100036928.1.20",
          "form": "외국맥주 맛잇어요 ㅎㅎㅎ",
          "original_form": "외국맥주 맛잇어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 15:21"
        },
        {
          "id": "MDRW2100036928.1.21",
          "form": "과일향 신기하네요",
          "original_form": "과일향 신기하네요",
          "speaker_id": "2",
          "time": "20210831 15:21"
        },
        {
          "id": "MDRW2100036928.1.22",
          "form": "맛에서도 느껴지는건가요?",
          "original_form": "맛에서도 느껴지는건가요?",
          "speaker_id": "2",
          "time": "20210831 15:21"
        },
        {
          "id": "MDRW2100036928.1.23",
          "form": "ㅋㅋㅋㅋㅋ 네.. 엄청 다양하죠..\r\n그래서 수입맥주중에 맛잇는걸 잘 알아보고 사야해요 ㅎㅎ\r\nname2님은 술은 아예 안드시나봐요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 네.. 엄청 다양하죠..\r\n그래서 수입맥주중에 맛잇는걸 잘 알아보고 사야해요 ㅎㅎ\r\n&name2&님은 술은 아예 안드시나봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:23"
        },
        {
          "id": "MDRW2100036928.1.24",
          "form": "저 아예안먹는건아니고 한잔정도..ㅎㅎㅎㅎ",
          "original_form": "저 아예안먹는건아니고 한잔정도..ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100036928.1.25",
          "form": "술아예안먹는건아니고 한잔정도는 가볍게마십니다",
          "original_form": "술아예안먹는건아니고 한잔정도는 가볍게마십니다",
