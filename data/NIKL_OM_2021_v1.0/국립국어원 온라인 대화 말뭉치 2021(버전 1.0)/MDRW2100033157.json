      "utterance": [
        {
          "id": "MDRW2100033157.1.1",
          "form": "네 한마리만 키우고있어요!",
          "original_form": "네 한마리만 키우고있어요!",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100033157.1.2",
          "form": "고양이를 더 키운다던지 강아지 같은 다른 동물과 함께 키워볼  생각은 안해보셨나요?",
          "original_form": "고양이를 더 키운다던지 강아지 같은 다른 동물과 함께 키워볼  생각은 안해보셨나요?",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100033157.1.3",
          "form": "지금 여동생이 뭐 달팽이 키우는거",
          "original_form": "지금 여동생이 뭐 달팽이 키우는거",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033157.1.4",
          "form": "간혹 보고있는데",
          "original_form": "간혹 보고있는데",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033157.1.5",
          "form": "어우..ㅋㅋㅋ",
          "original_form": "어우..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033157.1.6",
          "form": "다른건 생각안하고있네요 아직까진",
          "original_form": "다른건 생각안하고있네요 아직까진",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033157.1.7",
          "form": "여동생은 달팽이 키우는 군요",
          "original_form": "여동생은 달팽이 키우는 군요",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100033157.1.8",
          "form": "고양이 하나로 만족하면 된겁니다",
          "original_form": "고양이 하나로 만족하면 된겁니다",
          "speaker_id": "1",
          "time": "20210906 17:58"
        },
        {
          "id": "MDRW2100033157.1.9",
          "form": "보면 여러마리 키우는 집들도 있길래 한번 물어봤습니다",
          "original_form": "보면 여러마리 키우는 집들도 있길래 한번 물어봤습니다",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033157.1.10",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100033157.1.11",
          "form": "여러마리는 복잡할거같아서 1마리만 ㅎㅎ",
          "original_form": "여러마리는 복잡할거같아서 1마리만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100033157.1.12",
          "form": "고양이는 강아지에 비해 손이 덜간다고 들었는데 어떤가요?",
          "original_form": "고양이는 강아지에 비해 손이 덜간다고 들었는데 어떤가요?",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033157.1.13",
          "form": "네 손이 확실히 덜가는거같아요!",
          "original_form": "네 손이 확실히 덜가는거같아요!",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100033157.1.14",
          "form": "훈련도 거의 필요없고 알아서 거의 하는 타입인지라",
          "original_form": "훈련도 거의 필요없고 알아서 거의 하는 타입인지라",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100033157.1.15",
          "form": "그런면에서 인기가 있군요",
          "original_form": "그런면에서 인기가 있군요",
          "speaker_id": "1",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100033157.1.16",
          "form": "생긴것도 예쁘구요",
          "original_form": "생긴것도 예쁘구요",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033157.1.17",
          "form": "강아지는 귀엽다는 느낌이라면",
          "original_form": "강아지는 귀엽다는 느낌이라면",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033157.1.18",
          "form": "고양이는 예쁘다는 느낌이 있지요",
          "original_form": "고양이는 예쁘다는 느낌이 있지요",
          "speaker_id": "1",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033157.1.19",
          "form": "맞아여 ㅎㅎ",
          "original_form": "맞아여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100033157.1.20",
          "form": "저도 상황이 되면 고양이를 키워보고 싶네요",
          "original_form": "저도 상황이 되면 고양이를 키워보고 싶네요",
          "speaker_id": "1",
          "time": "20210906 18:31"
        },
        {
          "id": "MDRW2100033157.1.21",
          "form": "쉽게 결정은 못하겠지만요",
          "original_form": "쉽게 결정은 못하겠지만요",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100033157.1.22",
          "form": "결정은 최대한 신중하게 하시는게 좋죠!",
          "original_form": "결정은 최대한 신중하게 하시는게 좋죠!",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100033157.1.23",
          "form": "아무래도 생명을 키우는거니..",
          "original_form": "아무래도 생명을 키우는거니..",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100033157.1.24",
          "form": "또 한두해 키우는게 아니니까요",
          "original_form": "또 한두해 키우는게 아니니까요",
          "speaker_id": "1",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100033157.1.25",
          "form": "고양이 수명도 꽤 길지요?",
          "original_form": "고양이 수명도 꽤 길지요?",
          "speaker_id": "1",
          "time": "20210906 18:43"
        },
        {
          "id": "MDRW2100033157.1.26",
          "form": "10년 이상은 우선 산다구 들었어요",
          "original_form": "10년 이상은 우선 산다구 들었어요",
          "speaker_id": "2",
          "time": "20210906 18:44"
        },
        {
          "id": "MDRW2100033157.1.27",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210906 18:46"
        }
      ]
    }
