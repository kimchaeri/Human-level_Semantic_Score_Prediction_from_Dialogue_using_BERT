      "utterance": [
        {
          "id": "MDRW2100022928.1.1",
          "form": "넌 키우는 동물 있어?",
          "original_form": "넌 키우는 동물 있어?",
          "speaker_id": "1",
          "time": "20210829 19:14"
        },
        {
          "id": "MDRW2100022928.1.2",
          "form": "난 고양이 두마리 키워",
          "original_form": "난 고양이 두마리 키워",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022928.1.3",
          "form": "난 아직 아무것도 안 키우는데",
          "original_form": "난 아직 아무것도 안 키우는데",
          "speaker_id": "1",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022928.1.4",
          "form": "고양이 키울 때 뭐가 제일 보람차?",
          "original_form": "고양이 키울 때 뭐가 제일 보람차?",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022928.1.5",
          "form": "고양이가 행복해 보일때",
          "original_form": "고양이가 행복해 보일때",
          "speaker_id": "2",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022928.1.6",
          "form": "밥먹고 잘때",
          "original_form": "밥먹고 잘때",
          "speaker_id": "2",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022928.1.7",
          "form": "귀엽겠다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "귀엽겠다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022928.1.8",
          "form": "너네 고양이들은 여자니 남자니",
          "original_form": "너네 고양이들은 여자니 남자니",
          "speaker_id": "1",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100022928.1.9",
          "form": "둘다 남자앤들이고 중성화수술했어",
          "original_form": "둘다 남자앤들이고 중성화수술했어",
          "speaker_id": "2",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100022928.1.10",
          "form": "어이쿠 그렇구나 수술 고생했네",
          "original_form": "어이쿠 그렇구나 수술 고생했네",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100022928.1.11",
          "form": "둘이 성격이 각각 어때?",
          "original_form": "둘이 성격이 각각 어때?",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100022928.1.12",
          "form": "하나는 4차원이고",
          "original_form": "하나는 4차원이고",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022928.1.13",
          "form": "한마리는 왕자병이야",
          "original_form": "한마리는 왕자병이야",
          "speaker_id": "2",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022928.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 웃긴다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 웃긴다",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022928.1.15",
          "form": "둘이 그럼 성격차로 많이 싸워?",
          "original_form": "둘이 그럼 성격차로 많이 싸워?",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100022928.1.16",
          "form": "응 엄청 자주싸워",
          "original_form": "응 엄청 자주싸워",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022928.1.17",
          "form": "혼내도 계속 싸우더라",
          "original_form": "혼내도 계속 싸우더라",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022928.1.18",
          "form": "아이고...ㅋㅋㅋㅋㅋ꾸러기들이다",
          "original_form": "아이고...ㅋㅋㅋㅋㅋ꾸러기들이다",
          "speaker_id": "1",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022928.1.19",
          "form": "싸우면 알아서 화해 잘 해? 아니면 니가 개입해야 돼?",
          "original_form": "싸우면 알아서 화해 잘 해? 아니면 니가 개입해야 돼?",
          "speaker_id": "1",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022928.1.20",
          "form": "내가 개입해서 중간에 잘말려줘야돼",
          "original_form": "내가 개입해서 중간에 잘말려줘야돼",
          "speaker_id": "2",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022928.1.21",
          "form": "안그럼 사이가 더 안좋아진대",
          "original_form": "안그럼 사이가 더 안좋아진대",
          "speaker_id": "2",
          "time": "20210829 19:20"
        },
        {
          "id": "MDRW2100022928.1.22",
          "form": "아 정말? 헐 진심 처음 듣는 팁이다",
          "original_form": "아 정말? 헐 진심 처음 듣는 팁이다",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022928.1.23",
          "form": "잘 해결해줘야겠네ㅎㅎ",
          "original_form": "잘 해결해줘야겠네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 19:21"
        }
      ]
    }
