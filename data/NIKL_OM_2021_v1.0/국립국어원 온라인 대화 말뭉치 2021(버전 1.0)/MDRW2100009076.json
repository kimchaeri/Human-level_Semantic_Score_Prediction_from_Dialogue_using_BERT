      "utterance": [
        {
          "id": "MDRW2100009076.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100009076.1.2",
          "form": "네 ㅎㅎ 오늘은 족발 먹었어요",
          "original_form": "네 ㅎㅎ 오늘은 족발 먹었어요",
          "speaker_id": "2",
          "time": "20210823 20:34"
        },
        {
          "id": "MDRW2100009076.1.3",
          "form": "name1님은 저녁 드셨나요?",
          "original_form": "&name1&님은 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210823 20:34"
        },
        {
          "id": "MDRW2100009076.1.4",
          "form": "오 족발!!! 맛있겠네요ㅜㅠㅠ",
          "original_form": "오 족발!!! 맛있겠네요ㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 00:20"
        },
        {
          "id": "MDRW2100009076.1.5",
          "form": "전 생활패턴이 망해서 지금... 저녁 먹으려고요",
          "original_form": "전 생활패턴이 망해서 지금... 저녁 먹으려고요",
          "speaker_id": "1",
          "time": "20210824 00:20"
        },
        {
          "id": "MDRW2100009076.1.6",
          "form": "헉 ㅋㅋㅋㅋ 저도 생활패턴 엉망진창인데 반갑네요 ㅋㅋㅋㅋ",
          "original_form": "헉 ㅋㅋㅋㅋ 저도 생활패턴 엉망진창인데 반갑네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:03"
        },
        {
          "id": "MDRW2100009076.1.7",
          "form": "저녁 뭐드셨나요?",
          "original_form": "저녁 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210824 10:03"
        },
        {
          "id": "MDRW2100009076.1.8",
          "form": "어제는 떡볶ㅇ ㅣ 먹었어요",
          "original_form": "어제는 떡볶ㅇ ㅣ 먹었어요",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100009076.1.9",
          "form": "오늘은 뭐 먹을지",
          "original_form": "오늘은 뭐 먹을지",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100009076.1.10",
          "form": "떡볶이 맛있겠네요",
          "original_form": "떡볶이 맛있겠네요",
          "speaker_id": "2",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100009076.1.11",
          "form": "저는 족발 남은거 먹으려구요 ㅋㅋㅋㅋ",
          "original_form": "저는 족발 남은거 먹으려구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100009076.1.12",
          "form": "오 좋아요ㅋㅋㅋㅋ",
          "original_form": "오 좋아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100009076.1.13",
          "form": "저는 마라탕 먹었어요",
          "original_form": "저는 마라탕 먹었어요",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100009076.1.14",
          "form": "크어어 부러워요",
          "original_form": "크어어 부러워요",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100009076.1.15",
          "form": "하지만 저도 주말에 먹기로 했으니 주말 전까지만 부러워하겠습니다....",
          "original_form": "하지만 저도 주말에 먹기로 했으니 주말 전까지만 부러워하겠습니다....",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100009076.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100009076.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아욧",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아욧",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100009076.1.18",
          "form": "오늘은 뭐 드시나요",
          "original_form": "오늘은 뭐 드시나요",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100009076.1.19",
          "form": "저는 깻잎장아찌랑 스팸 구워서 먹을거예요",
          "original_form": "저는 깻잎장아찌랑 스팸 구워서 먹을거예요",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100009076.1.20",
          "form": "앗 백신 맞고 잠이 많아져서 답을 못했네요...",
          "original_form": "앗 백신 맞고 잠이 많아져서 답을 못했네요...",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100009076.1.21",
          "form": "오늘은 삼겹살에 구운 김치 먹었어요!",
          "original_form": "오늘은 삼겹살에 구운 김치 먹었어요!",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100009076.1.22",
          "form": "으악 너무 부럽네요",
          "original_form": "으악 너무 부럽네요",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100009076.1.23",
          "form": "삼겹살 먹은지가 언젠지..",
          "original_form": "삼겹살 먹은지가 언젠지..",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100009076.1.24",
          "form": "크흐 저도 오랜만에 먹었어요",
          "original_form": "크흐 저도 오랜만에 먹었어요",
          "speaker_id": "1",
          "time": "20210828 20:59"
        }
      ]
