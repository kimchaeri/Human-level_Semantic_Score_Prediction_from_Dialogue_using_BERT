      "utterance": [
        {
          "id": "MDRW2100000330.1.1",
          "form": "치과하니 돈 잘버는 듯",
          "original_form": "치과하니 돈 잘버는 듯",
          "speaker_id": "2",
          "time": "20210818 15:43"
        },
        {
          "id": "MDRW2100000330.1.2",
          "form": "페닥 쓰면서 3일만 일함",
          "original_form": "페닥 쓰면서 3일만 일함",
          "speaker_id": "2",
          "time": "20210818 15:43"
        },
        {
          "id": "MDRW2100000330.1.3",
          "form": "아 치과의사면 뭐",
          "original_form": "아 치과의사면 뭐",
          "speaker_id": "1",
          "time": "20210818 15:43"
        },
        {
          "id": "MDRW2100000330.1.4",
          "form": "살아있네",
          "original_form": "살아있네",
          "speaker_id": "1",
          "time": "20210818 15:44"
        },
        {
          "id": "MDRW2100000330.1.5",
          "form": "서울에 슈퍼카 두대 놓고 오피스 잡고",
          "original_form": "서울에 슈퍼카 두대 놓고 오피스 잡고",
          "speaker_id": "2",
          "time": "20210818 15:44"
        },
        {
          "id": "MDRW2100000330.1.6",
          "form": "놀러 다녀",
          "original_form": "놀러 다녀",
          "speaker_id": "2",
          "time": "20210818 15:44"
        },
        {
          "id": "MDRW2100000330.1.7",
          "form": "다 대출이지만",
          "original_form": "다 대출이지만",
          "speaker_id": "2",
          "time": "20210818 15:44"
        },
        {
          "id": "MDRW2100000330.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210818 15:44"
        },
        {
          "id": "MDRW2100000330.1.9",
          "form": "치과의사가 그정도까지 돼?",
          "original_form": "치과의사가 그정도까지 돼?",
          "speaker_id": "1",
          "time": "20210818 15:44"
        },
        {
          "id": "MDRW2100000330.1.10",
          "form": "원래 잘 살던거 아냐?",
          "original_form": "원래 잘 살던거 아냐?",
          "speaker_id": "1",
          "time": "20210818 15:44"
        },
        {
          "id": "MDRW2100000330.1.11",
          "form": "파나메라?",
          "original_form": "파나메라?",
          "speaker_id": "2",
          "time": "20210818 15:44"
        },
        {
          "id": "MDRW2100000330.1.12",
          "form": "차이름이야?",
          "original_form": "차이름이야?",
          "speaker_id": "1",
          "time": "20210818 15:45"
        },
        {
          "id": "MDRW2100000330.1.13",
          "form": "한대는 얼마 안하는구나 2억 짜리랑",
          "original_form": "한대는 얼마 안하는구나 2억 짜리랑",
          "speaker_id": "2",
          "time": "20210818 15:45"
        },
        {
          "id": "MDRW2100000330.1.14",
          "form": "아우디 중고",
          "original_form": "아우디 중고",
          "speaker_id": "2",
          "time": "20210818 15:45"
        },
        {
          "id": "MDRW2100000330.1.15",
          "form": "슈퍼카가 아니네",
          "original_form": "슈퍼카가 아니네",
          "speaker_id": "2",
          "time": "20210818 15:45"
        },
        {
          "id": "MDRW2100000330.1.16",
          "form": "그래도 비싼대",
          "original_form": "그래도 비싼대",
          "speaker_id": "1",
          "time": "20210818 15:45"
        },
        {
          "id": "MDRW2100000330.1.17",
          "form": "5년전에 페닥할때",
          "original_form": "5년전에 페닥할때",
          "speaker_id": "2",
          "time": "20210818 15:45"
        },
        {
          "id": "MDRW2100000330.1.18",
          "form": "나 운동같이하는 치과부부도",
          "original_form": "나 운동같이하는 치과부부도",
          "speaker_id": "1",
          "time": "20210818 15:45"
        },
        {
          "id": "MDRW2100000330.1.19",
          "form": "한달에 1400 받았나",
          "original_form": "한달에 1400 받았나",
          "speaker_id": "2",
          "time": "20210818 15:46"
        },
        {
          "id": "MDRW2100000330.1.20",
          "form": "포르쉐 끄는데",
          "original_form": "포르쉐 끄는데",
          "speaker_id": "1",
          "time": "20210818 15:46"
        },
        {
          "id": "MDRW2100000330.1.21",
          "form": "7년전이겠다",
          "original_form": "7년전이겠다",
          "speaker_id": "2",
          "time": "20210818 15:46"
        },
        {
          "id": "MDRW2100000330.1.22",
          "form": "의사가 좋긴 좋네",
          "original_form": "의사가 좋긴 좋네",
          "speaker_id": "1",
          "time": "20210818 15:46"
        },
        {
          "id": "MDRW2100000330.1.23",
          "form": "세전 연봉 2억 이상",
          "original_form": "세전 연봉 2억 이상",
