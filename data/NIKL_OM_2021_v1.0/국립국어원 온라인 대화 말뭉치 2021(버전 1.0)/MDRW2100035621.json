      "utterance": [
        {
          "id": "MDRW2100035621.1.1",
          "form": "고양이 키워보셨나용?",
          "original_form": "고양이 키워보셨나용?",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100035621.1.2",
          "form": "어릴때 강아지 키워봤어요",
          "original_form": "어릴때 강아지 키워봤어요",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035621.1.3",
          "form": "고양이는 키우고싶어요ㅠ",
          "original_form": "고양이는 키우고싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035621.1.4",
          "form": "앗 무슨 종이었어요?",
          "original_form": "앗 무슨 종이었어요?",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035621.1.5",
          "form": "근데 책임져야하는게 무거우니까 참고있어요",
          "original_form": "근데 책임져야하는게 무거우니까 참고있어요",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035621.1.6",
          "form": "시고르자브종 백구였어요ㅋㅋ",
          "original_form": "시고르자브종 백구였어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035621.1.7",
          "form": "아 맞아요ㅠㅠ 책임져야하는 무게가 무겁죠",
          "original_form": "아 맞아요ㅠㅠ 책임져야하는 무게가 무겁죠",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035621.1.8",
          "form": "아아 너무 귀엽다",
          "original_form": "아아 너무 귀엽다",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035621.1.9",
          "form": "속쌍커풀도 있었나요?",
          "original_form": "속쌍커풀도 있었나요?",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035621.1.10",
          "form": "으앙ㅋㅋㅋ",
          "original_form": "으앙ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100035621.1.11",
          "form": "그죠 아침마다 밥주는 권력자(엄마) 방 앞에 앉아있었습니다",
          "original_form": "그죠 아침마다 밥주는 권력자(엄마) 방 앞에 앉아있었습니다",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100035621.1.12",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100035621.1.13",
          "form": "진도 아이들은",
          "original_form": "진도 아이들은",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100035621.1.14",
          "form": "서열 정리가 확실하고",
          "original_form": "서열 정리가 확실하고",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100035621.1.15",
          "form": "초등생때라 잘 못해줬어요 지금이면ㅇ진짜 산책도 잘시켜주고",
          "original_form": "초등생때라 잘 못해줬어요 지금이면ㅇ진짜 산책도 잘시켜주고",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035621.1.16",
          "form": "주인한테 순종적이라고 들었는데",
          "original_form": "주인한테 순종적이라고 들었는데",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035621.1.17",
          "form": "잘해줄텐데 ㅋㅋㅇ",
          "original_form": "잘해줄텐데 ㅋㅋㅇ",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035621.1.18",
          "form": "맞아요 저는 서열밖이었습니다^^",
          "original_form": "맞아요 저는 서열밖이었습니다^^",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035621.1.19",
          "form": "아아ㅠㅠ 아쉬움이 남으시나봐용",
          "original_form": "아아ㅠㅠ 아쉬움이 남으시나봐용",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035621.1.20",
          "form": "하핰ㅋㅋㅋㅋ",
          "original_form": "하핰ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035621.1.21",
          "form": "그래도 서열은 그럴지라도",
          "original_form": "그래도 서열은 그럴지라도",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035621.1.22",
          "form": "그녀에게 저는 군식구일뿐ㅋㅋ",
          "original_form": "그녀에게 저는 군식구일뿐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035621.1.23",
          "form": "자기 가족이다 라는 개념은 강하다고 하더라구요",
          "original_form": "자기 가족이다 라는 개념은 강하다고 하더라구요",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035621.1.24",
          "form": "아마 다른 외부인이 name2님 건들었으면",
          "original_form": "아마 다른 외부인이 &name2&님 건들었으면",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035621.1.25",
          "form": "보호하려고 했을거예요",
          "original_form": "보호하려고 했을거예요",
          "speaker_id": "1",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035621.1.26",
          "form": "밥주는 엄마가 짱이였죠ㅋㅋ",
          "original_form": "밥주는 엄마가 짱이였죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035621.1.27",
          "form": "그랬겠죠?",
          "original_form": "그랬겠죠?",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035621.1.28",
          "form": "ㅋㅋㅋㅋ 말만 들어도 왜케 귀엽져ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 말만 들어도 왜케 귀엽져ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:26"
        }
      ]
    }
