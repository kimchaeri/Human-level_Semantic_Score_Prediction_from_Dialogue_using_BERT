      "utterance": [
        {
          "id": "MDRW2100027690.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027690.1.2",
          "form": "사실 땀내는걸 엄청 좋아하진 않는데 자전거같이 타는건 좋아해요!",
          "original_form": "사실 땀내는걸 엄청 좋아하진 않는데 자전거같이 타는건 좋아해요!",
          "speaker_id": "2",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027690.1.3",
          "form": "헉 자전거",
          "original_form": "헉 자전거",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027690.1.4",
          "form": "저는 운동신경이 정말 없어서.. 자전거를 못타요",
          "original_form": "저는 운동신경이 정말 없어서.. 자전거를 못타요",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027690.1.5",
          "form": "저도 어릴때 배운게 아니었으면 못 탔을거예요 ㅜㅜ 몸치라서",
          "original_form": "저도 어릴때 배운게 아니었으면 못 탔을거예요 ㅜㅜ 몸치라서",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027690.1.6",
          "form": "전에 방송댄스 학원도 친구 손잡고 가봤는데",
          "original_form": "전에 방송댄스 학원도 친구 손잡고 가봤는데",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027690.1.7",
          "form": "몸치 고쳐준다더니 공약 실천 못했습니다ㅋㅋ",
          "original_form": "몸치 고쳐준다더니 공약 실천 못했습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027690.1.8",
          "form": "오 방송댄스!",
          "original_form": "오 방송댄스!",
          "speaker_id": "1",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027690.1.9",
          "form": "배우러 가보고싶은데 부끄러워서...",
          "original_form": "배우러 가보고싶은데 부끄러워서...",
          "speaker_id": "1",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027690.1.10",
          "form": "특히 웨이브 들어가는 동작을 하는데",
          "original_form": "특히 웨이브 들어가는 동작을 하는데",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027690.1.11",
          "form": "학원에 전신거울이 사방에 있거든요",
          "original_form": "학원에 전신거울이 사방에 있거든요",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027690.1.12",
          "form": "제 모습이 다각도로 웃기더라고요",
          "original_form": "제 모습이 다각도로 웃기더라고요",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027690.1.13",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027690.1.14",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027690.1.15",
          "form": "전 웃겨서 못할거같아요",
          "original_form": "전 웃겨서 못할거같아요",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027690.1.16",
          "form": "저 필라테스 하다가도 피식피식 웃어요",
          "original_form": "저 필라테스 하다가도 피식피식 웃어요",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027690.1.17",
          "form": "선생님도 웃고 저도 웃고",
          "original_form": "선생님도 웃고 저도 웃고",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027690.1.18",
          "form": "내 몸은 왜 이따위인가",
          "original_form": "내 몸은 왜 이따위인가",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027690.1.19",
          "form": "와 필라테스ㅋㅋ 공감입니다",
          "original_form": "와 필라테스ㅋㅋ 공감입니다",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027690.1.20",
          "form": "선생님이 자꾸 놀라시고",
          "original_form": "선생님이 자꾸 놀라시고",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027690.1.21",
          "form": "어머 이게 왜 안되시지...? 하시면서요ㅋㅋ",
          "original_form": "어머 이게 왜 안되시지...? 하시면서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027690.1.22",
          "form": "ㅋㅋㄱㅋㄱㅋㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋㄱㅋㅋㄱㅋㄱㄱㅋㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027690.1.23",
          "form": "전 안되고있는거같은데 됐다고 하고",
          "original_form": "전 안되고있는거같은데 됐다고 하고",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027690.1.24",
          "form": "저 집중하고있는데 집중하라고 하고",
          "original_form": "저 집중하고있는데 집중하라고 하고",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027690.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027690.1.26",
          "form": "필라테스 얘기나온 김에 목 스트레칭 한번 합니다ㅋㅋ",
          "original_form": "필라테스 얘기나온 김에 목 스트레칭 한번 합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027690.1.27",
          "form": "어깨에 힘빼는게 세상에서 제일 어려워요",
          "original_form": "어깨에 힘빼는게 세상에서 제일 어려워요",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027690.1.28",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027690.1.29",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027690.1.30",
          "form": "글고 목 자꾸 튀어나와요",
          "original_form": "글고 목 자꾸 튀어나와요",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027690.1.31",
          "form": "맞아요! 거북이도 아닌데!",
          "original_form": "맞아요! 거북이도 아닌데!",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027690.1.32",
          "form": "전 거북이가 맞는거같아요..",
          "original_form": "전 거북이가 맞는거같아요..",
          "speaker_id": "1",
          "time": "20210902 14:30"
        },
        {
          "id": "MDRW2100027690.1.33",
          "form": "얼마전에 도수치료받는데",
          "original_form": "얼마전에 도수치료받는데",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027690.1.34",
          "form": "필라테스할때는 배 집어넣으면서 숨 쉬라고 배웠었거든요",
          "original_form": "필라테스할때는 배 집어넣으면서 숨 쉬라고 배웠었거든요",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027690.1.35",
          "form": "근데 도수치료 선생님이 배꼽을 내민다는 느낌으로 숨 들이쉬고 숨 뱉을때 어깨를 내리면서 힘을 빼라고 하시더라고요",
          "original_form": "근데 도수치료 선생님이 배꼽을 내민다는 느낌으로 숨 들이쉬고 숨 뱉을때 어깨를 내리면서 힘을 빼라고 하시더라고요",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100027690.1.36",
          "form": "잘 때 그렇게 호흡하고 자니까 목이 한결 편해요",
          "original_form": "잘 때 그렇게 호흡하고 자니까 목이 한결 편해요",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100027690.1.37",
          "form": "아 맞아요 필라테스는 등을 부풀리면서 숨을 들이쉬고 내뱉으면서 배에 힘을 꽉 주고 넣으라고 하죵",
          "original_form": "아 맞아요 필라테스는 등을 부풀리면서 숨을 들이쉬고 내뱉으면서 배에 힘을 꽉 주고 넣으라고 하죵",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100027690.1.38",
          "form": "저는 근데.. 이상해요 숨 들이쉴때 배를 넣고 내뱉을때 배가 나와요",
          "original_form": "저는 근데.. 이상해요 숨 들이쉴때 배를 넣고 내뱉을때 배가 나와요",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027690.1.39",
          "form": "필라테스하면서 그나마 조금 고쳤는데",
          "original_form": "필라테스하면서 그나마 조금 고쳤는데",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027690.1.40",
          "form": "복근을 느끼고 힘주는거 정말 낯설더라고요",
          "original_form": "복근을 느끼고 힘주는거 정말 낯설더라고요",
          "speaker_id": "2",
          "time": "20210902 14:34"
        }
      ]
