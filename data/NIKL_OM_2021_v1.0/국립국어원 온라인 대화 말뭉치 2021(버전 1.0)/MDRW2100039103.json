      "utterance": [
        {
          "id": "MDRW2100039103.1.1",
          "form": "영양제 드시나요",
          "original_form": "영양제 드시나요",
          "speaker_id": "1",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100039103.1.2",
          "form": "영양제는 먹다가 최근에 끊었어요",
          "original_form": "영양제는 먹다가 최근에 끊었어요",
          "speaker_id": "2",
          "time": "20210903 10:05"
        },
        {
          "id": "MDRW2100039103.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 10:05"
        },
        {
          "id": "MDRW2100039103.1.4",
          "form": "저도 먹어요  ...안꾸준히",
          "original_form": "저도 먹어요  ...안꾸준히",
          "speaker_id": "1",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100039103.1.5",
          "form": "왜꾾으셧어요",
          "original_form": "왜꾾으셧어요",
          "speaker_id": "1",
          "time": "20210903 10:08"
        },
        {
          "id": "MDRW2100039103.1.6",
          "form": "언젠가부터 속이 안받더라구요",
          "original_form": "언젠가부터 속이 안받더라구요",
          "speaker_id": "2",
          "time": "20210903 10:09"
        },
        {
          "id": "MDRW2100039103.1.7",
          "form": "아아ㅜㅜ 그럴수잇죵",
          "original_form": "아아ㅜㅜ 그럴수잇죵",
          "speaker_id": "1",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100039103.1.8",
          "form": "위장에좀 무리가 가나",
          "original_form": "위장에좀 무리가 가나",
          "speaker_id": "1",
          "time": "20210903 10:13"
        },
        {
          "id": "MDRW2100039103.1.9",
          "form": "그런거 같아요ㅠ",
          "original_form": "그런거 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210903 10:14"
        },
        {
          "id": "MDRW2100039103.1.10",
          "form": "머머 드셧엇어요",
          "original_form": "머머 드셧엇어요",
          "speaker_id": "1",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100039103.1.11",
          "form": "센트룸 먹었었어요",
          "original_form": "센트룸 먹었었어요",
          "speaker_id": "2",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100039103.1.12",
          "form": "아아 비타민만요?",
          "original_form": "아아 비타민만요?",
          "speaker_id": "1",
          "time": "20210903 10:18"
        },
        {
          "id": "MDRW2100039103.1.13",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:19"
        },
        {
          "id": "MDRW2100039103.1.14",
          "form": "저도 뭔가 큰거먹을땐 속 안조앗던거같아요...",
          "original_form": "저도 뭔가 큰거먹을땐 속 안조앗던거같아요...",
          "speaker_id": "1",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100039103.1.15",
          "form": "걍 임팩타민 이런거머거요ㅋㅋ",
          "original_form": "걍 임팩타민 이런거머거요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:21"
        },
        {
          "id": "MDRW2100039103.1.16",
          "form": "임팩타민도 많이들 드시는군요",
          "original_form": "임팩타민도 많이들 드시는군요",
          "speaker_id": "2",
          "time": "20210903 10:24"
        },
        {
          "id": "MDRW2100039103.1.17",
          "form": "비타민 비가 피로회복에 좋대요",
          "original_form": "비타민 비가 피로회복에 좋대요",
          "speaker_id": "1",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100039103.1.18",
          "form": "비타민 비 위주로 먹어야겠어요ㅎㅎ",
          "original_form": "비타민 비 위주로 먹어야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:26"
        },
        {
          "id": "MDRW2100039103.1.19",
          "form": "넹 비를 많이 넣은게 잇더라구요 아마 임팩타민도 그런거인듯",
          "original_form": "넹 비를 많이 넣은게 잇더라구요 아마 임팩타민도 그런거인듯",
          "speaker_id": "1",
          "time": "20210903 10:27"
        },
        {
          "id": "MDRW2100039103.1.20",
          "form": "다시 영양제 먹기 시작하면 임팩타민으로 가야겠어요",
          "original_form": "다시 영양제 먹기 시작하면 임팩타민으로 가야겠어요",
          "speaker_id": "2",
          "time": "20210903 10:28"
        },
        {
          "id": "MDRW2100039103.1.21",
          "form": "넹 나쁘지않아요 ㅋㅋ",
          "original_form": "넹 나쁘지않아요 ㅋㅋ",
