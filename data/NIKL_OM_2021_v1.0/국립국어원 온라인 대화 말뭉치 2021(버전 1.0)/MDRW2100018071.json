      "utterance": [
        {
          "id": "MDRW2100018071.1.1",
          "form": "난 고양이",
          "original_form": "난 고양이",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018071.1.2",
          "form": "고양이 말고 키워보고 싶은 동물 없어?",
          "original_form": "고양이 말고 키워보고 싶은 동물 없어?",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100018071.1.3",
          "form": "고슴도치",
          "original_form": "고슴도치",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100018071.1.4",
          "form": "고슴도치 귀엽지",
          "original_form": "고슴도치 귀엽지",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100018071.1.5",
          "form": "그니까 키워보고 싶은 동물 있어?",
          "original_form": "그니까 키워보고 싶은 동물 있어?",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100018071.1.6",
          "form": "미니돼지 있으면 키워보고 싶어",
          "original_form": "미니돼지 있으면 키워보고 싶어",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100018071.1.7",
          "form": "미니돼지 커도 별로 안커?",
          "original_form": "미니돼지 커도 별로 안커?",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100018071.1.8",
          "form": "미니 이니까 그러겠지?",
          "original_form": "미니 이니까 그러겠지?",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100018071.1.9",
          "form": "냄새 많이 나지 않을까?",
          "original_form": "냄새 많이 나지 않을까?",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100018071.1.10",
          "form": "근데 돼지가 우리 생각보다 되게 깨끗한 동물이래",
          "original_form": "근데 돼지가 우리 생각보다 되게 깨끗한 동물이래",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100018071.1.11",
          "form": "나도 어디서 듣긴 들었었어",
          "original_form": "나도 어디서 듣긴 들었었어",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100018071.1.12",
          "form": "그리고 엄청 똑똑하대",
          "original_form": "그리고 엄청 똑똑하대",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100018071.1.13",
          "form": "맞아 내가 알기론 고양이보다 더 똑똑하대",
          "original_form": "맞아 내가 알기론 고양이보다 더 똑똑하대",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100018071.1.14",
          "form": "맞아 개보다 더 똑똑할걸?",
          "original_form": "맞아 개보다 더 똑똑할걸?",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100018071.1.15",
          "form": "개인가 고양인가 보다 똑똑하다 그랬어",
          "original_form": "개인가 고양인가 보다 똑똑하다 그랬어",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100018071.1.16",
          "form": "응 생각보다 엄청 똑똑하대",
          "original_form": "응 생각보다 엄청 똑똑하대",
