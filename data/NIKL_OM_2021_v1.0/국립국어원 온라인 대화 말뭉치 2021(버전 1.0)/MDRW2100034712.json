      "utterance": [
        {
          "id": "MDRW2100034712.1.1",
          "form": "어떤거 추가해서 드세여",
          "original_form": "어떤거 추가해서 드세여",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100034712.1.2",
          "form": "분모자랑 당면이랑 이런걸로 선택해서 먹었어여",
          "original_form": "분모자랑 당면이랑 이런걸로 선택해서 먹었어여",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100034712.1.3",
          "form": "저는 사리추가같은건 안하구",
          "original_form": "저는 사리추가같은건 안하구",
          "speaker_id": "1",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100034712.1.4",
          "form": "감자튀김만요 ㅋㅋㅋ",
          "original_form": "감자튀김만요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100034712.1.5",
          "form": "오 감자튀김도 있군여",
          "original_form": "오 감자튀김도 있군여",
          "speaker_id": "2",
          "time": "20210828 18:03"
        },
        {
          "id": "MDRW2100034712.1.6",
          "form": "네 그 해쉬브라운 있더라구요",
          "original_form": "네 그 해쉬브라운 있더라구요",
          "speaker_id": "1",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100034712.1.7",
          "form": "해쉬브라운 좋죠! 담에는 추가해서 먹어볼게요 ㅋㅋ",
          "original_form": "해쉬브라운 좋죠! 담에는 추가해서 먹어볼게요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100034712.1.8",
          "form": "네 ㅋㅋㅋㅋ 그거 으깨서 국물에 비벼먹으면 최고에요 ㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ 그거 으깨서 국물에 비벼먹으면 최고에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100034712.1.9",
          "form": "으와 크와 맛잘알이실거 같네여",
          "original_form": "으와 크와 맛잘알이실거 같네여",
          "speaker_id": "2",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100034712.1.10",
          "form": "쩝쩝박사가 장래희망입니다",
          "original_form": "쩝쩝박사가 장래희망입니다",
          "speaker_id": "1",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100034712.1.11",
          "form": "ㅋㅋㅋㅋ어우 세상에서 젤 훌륭한 박사",
          "original_form": "ㅋㅋㅋㅋ어우 세상에서 젤 훌륭한 박사",
          "speaker_id": "2",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100034712.1.12",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100034712.1.13",
          "form": "좋아하시는 맛집 있으세요?",
          "original_form": "좋아하시는 맛집 있으세요?",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100034712.1.14",
          "form": "저는 사실 뭐든 잘 먹어서 웬만하면 다 맛있다해서 사람들이 잘 안믿어주는데",
          "original_form": "저는 사실 뭐든 잘 먹어서 웬만하면 다 맛있다해서 사람들이 잘 안믿어주는데",
          "speaker_id": "2",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034712.1.15",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034712.1.16",
          "form": "저희동네에 진짜 맛집있었거든요 유명쉐프였는데",
          "original_form": "저희동네에 진짜 맛집있었거든요 유명쉐프였는데",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034712.1.17",
          "form": "서울쪽 식당이 너무 잘되어서 바빠져서",
          "original_form": "서울쪽 식당이 너무 잘되어서 바빠져서",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034712.1.18",
          "form": "동네 식당 접고 서울로 갔어요",
          "original_form": "동네 식당 접고 서울로 갔어요",
          "speaker_id": "2",
          "time": "20210828 18:11"
        }
      ]
