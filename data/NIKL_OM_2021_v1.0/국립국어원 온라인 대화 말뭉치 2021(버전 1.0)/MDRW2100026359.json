      "utterance": [
        {
          "id": "MDRW2100026359.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ 제발 빨리끝나라!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 제발 빨리끝나라!!",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026359.1.2",
          "form": "ㅋㅋㅋㅋㅋ 대학원 수업은 뒷전이 되네요 신기하게",
          "original_form": "ㅋㅋㅋㅋㅋ 대학원 수업은 뒷전이 되네요 신기하게",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026359.1.3",
          "form": "성적 잘받아야되는데",
          "original_form": "성적 잘받아야되는데",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026359.1.4",
          "form": "언니는 긴장 안되세요??",
          "original_form": "언니는 긴장 안되세요??",
          "speaker_id": "2",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026359.1.5",
          "form": "그래도 name2씨 하나만 들으면 되니까",
          "original_form": "그래도 &name2&씨 하나만 들으면 되니까",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026359.1.6",
          "form": "괜찮을거예요!!!!",
          "original_form": "괜찮을거예요!!!!",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026359.1.7",
          "form": "저는 긴장까진 안되지만",
          "original_form": "저는 긴장까진 안되지만",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026359.1.8",
          "form": "그냥 잘해야겠다! 는 생각이 강해요!!!",
          "original_form": "그냥 잘해야겠다! 는 생각이 강해요!!!",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026359.1.9",
          "form": "아 맞네요 언니 생화학도 들으시지!!",
          "original_form": "아 맞네요 언니 생화학도 들으시지!!",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026359.1.10",
          "form": "생화학랩분들도 많겠다 ㅋㅋㅋㅋ",
          "original_form": "생화학랩분들도 많겠다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026359.1.11",
          "form": "같이 파이팅해요!",
          "original_form": "같이 파이팅해요!",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026359.1.12",
          "form": "이번학기끝나면 연말이네요 저 연말 좋아해요...",
          "original_form": "이번학기끝나면 연말이네요 저 연말 좋아해요...",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026359.1.13",
          "form": "그때까지 잘 버티길...!!",
          "original_form": "그때까지 잘 버티길...!!",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026359.1.14",
          "form": "맞아요 몽글몽글한 연말 분위기....",
          "original_form": "맞아요 몽글몽글한 연말 분위기....",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026359.1.15",
          "form": "생화학 학점 따기 힘들 것 같은데ㅠ",
          "original_form": "생화학 학점 따기 힘들 것 같은데ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026359.1.16",
          "form": "졸업시험 잘 보려면 수업 잘 들어야겠어요,,",
          "original_form": "졸업시험 잘 보려면 수업 잘 들어야겠어요,,",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026359.1.17",
          "form": "잘 할 수 있을거예요!!!",
          "original_form": "잘 할 수 있을거예요!!!",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026359.1.18",
          "form": "합.. 졸업시험... 그렇네요",
          "original_form": "합.. 졸업시험... 그렇네요",
          "speaker_id": "2",
          "time": "20210902 08:27"
        },
        {
          "id": "MDRW2100026359.1.19",
          "form": "와 언니... 저 어제 씻고 잠깐 누웠는데 그대로 잠들어서 또 불켜놓고 잤어요",
          "original_form": "와 언니... 저 어제 씻고 잠깐 누웠는데 그대로 잠들어서 또 불켜놓고 잤어요",
          "speaker_id": "2",
          "time": "20210902 08:27"
        },
        {
          "id": "MDRW2100026359.1.20",
          "form": "늦잠자서 빨래도 못말리고ㅜㅜ 하 이런..",
          "original_form": "늦잠자서 빨래도 못말리고ㅜㅜ 하 이런..",
          "speaker_id": "2",
          "time": "20210902 08:27"
        },
        {
          "id": "MDRW2100026359.1.21",
          "form": "학교가야되는데... ",
          "original_form": "학교가야되는데... ",
          "speaker_id": "2",
          "time": "20210902 08:28"
        },
        {
          "id": "MDRW2100026359.1.22",
          "form": "헉 피곤하셨나보다",
          "original_form": "헉 피곤하셨나보다",
          "speaker_id": "1",
          "time": "20210902 08:43"
        },
        {
          "id": "MDRW2100026359.1.23",
          "form": "어제 첫날부터 조교 일 때문에 스트레스 많이 받았죠 ㅠㅠ",
          "original_form": "어제 첫날부터 조교 일 때문에 스트레스 많이 받았죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 08:43"
        },
        {
          "id": "MDRW2100026359.1.24",
          "form": "뭔가 한건 없어도 첫날이라 좀 긴장했던 건 있는거 같아요ㅜㅜ",
          "original_form": "뭔가 한건 없어도 첫날이라 좀 긴장했던 건 있는거 같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 09:02"
        },
        {
          "id": "MDRW2100026359.1.25",
          "form": "조교도 그렇고ㅠㅠ",
          "original_form": "조교도 그렇고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 09:02"
        },
        {
          "id": "MDRW2100026359.1.26",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210902 09:02"
        },
        {
          "id": "MDRW2100026359.1.27",
          "form": "저도 첫 한 달이 힘들었어요ㅠ",
          "original_form": "저도 첫 한 달이 힘들었어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 09:03"
        },
        {
          "id": "MDRW2100026359.1.28",
          "form": "6월에 힘들었던 거랑은 다른 맥락이었달까?",
          "original_form": "6월에 힘들었던 거랑은 다른 맥락이었달까?",
          "speaker_id": "1",
          "time": "20210902 09:03"
        },
        {
          "id": "MDRW2100026359.1.29",
          "form": "그러셨군요...!!",
          "original_form": "그러셨군요...!!",
          "speaker_id": "2",
          "time": "20210902 09:03"
        },
        {
          "id": "MDRW2100026359.1.30",
          "form": "적응기간이 필요한가봐요ㅜㅜ",
          "original_form": "적응기간이 필요한가봐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 09:03"
        },
        {
          "id": "MDRW2100026359.1.31",
          "form": "언니는 인턴도 오래하셨는데ㅠㅠ",
          "original_form": "언니는 인턴도 오래하셨는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 09:03"
        },
        {
          "id": "MDRW2100026359.1.32",
          "form": "네네 이게",
          "original_form": "네네 이게",
          "speaker_id": "1",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100026359.1.33",
          "form": "인턴 때랑은 또 다르더라구요...",
          "original_form": "인턴 때랑은 또 다르더라구요...",
          "speaker_id": "1",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100026359.1.34",
          "form": "그렇구나...ㅜㅜ",
          "original_form": "그렇구나...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 09:06"
        },
        {
          "id": "MDRW2100026359.1.35",
          "form": "저도 곧 적응하겠죠!!",
          "original_form": "저도 곧 적응하겠죠!!",
          "speaker_id": "2",
          "time": "20210902 09:06"
        },
        {
          "id": "MDRW2100026359.1.36",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210902 09:06"
        },
        {
          "id": "MDRW2100026359.1.37",
          "form": "금방 하실거예요!!!",
          "original_form": "금방 하실거예요!!!",
          "speaker_id": "1",
          "time": "20210902 09:06"
        },
        {
          "id": "MDRW2100026359.1.38",
          "form": "오늘은 개별 과제 연구를",
          "original_form": "오늘은 개별 과제 연구를",
          "speaker_id": "1",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100026359.1.39",
          "form": "신청해봅시다!",
          "original_form": "신청해봅시다!",
          "speaker_id": "1",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100026359.1.40",
          "form": "아맞다!! 네!",
          "original_form": "아맞다!! 네!",
          "speaker_id": "2",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100026359.1.41",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100026359.1.42",
          "form": "언니 오늘 실험도 하시나요??",
          "original_form": "언니 오늘 실험도 하시나요??",
          "speaker_id": "2",
          "time": "20210902 09:07"
        }
      ]
