      "utterance": [
        {
          "id": "MDRW2100019576.1.1",
          "form": "오늘 저녁은 뭐 드셨나요?",
          "original_form": "오늘 저녁은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019576.1.2",
          "form": "전 아까 유가네 닭갈비 시켜먹었어요 ㅋㅋㅋ",
          "original_form": "전 아까 유가네 닭갈비 시켜먹었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019576.1.3",
          "form": "오 요새 유가네 닭갈비 잘 안 보이도라구요? ㅠㅠ",
          "original_form": "오 요새 유가네 닭갈비 잘 안 보이도라구요? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019576.1.4",
          "form": "저희 동네만 없는건지",
          "original_form": "저희 동네만 없는건지",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019576.1.5",
          "form": "그래요? 다 망했나 ㅜㅜ",
          "original_form": "그래요? 다 망했나 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019576.1.6",
          "form": "저희동네는 세개나 있어서 좋더라구여",
          "original_form": "저희동네는 세개나 있어서 좋더라구여",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019576.1.7",
          "form": "오오 유세권이시군요!",
          "original_form": "오오 유세권이시군요!",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019576.1.8",
          "form": "닭갈비 먹고싶네요 ㅠㅠ",
          "original_form": "닭갈비 먹고싶네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019576.1.9",
          "form": "닭갈비 ..소울푸드에요 ㅠㅜ",
          "original_form": "닭갈비 ..소울푸드에요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019576.1.10",
          "form": "닭갈비 춘천에서도 드셔보셨나요?",
          "original_form": "닭갈비 춘천에서도 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019576.1.11",
          "form": "아 한번 먹어봤는데",
          "original_form": "아 한번 먹어봤는데",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019576.1.12",
          "form": "유가네가 훨 맛있었어요..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "유가네가 훨 맛있었어요..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019576.1.13",
          "form": "앗ㅋㅋㅋㅋㅋㅋ 역시 신뢰의 프차...",
          "original_form": "앗ㅋㅋㅋㅋㅋㅋ 역시 신뢰의 프차...",
          "speaker_id": "1",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019576.1.14",
          "form": "저도 닭갈비 춘천에서 먹은것 중에 맛있었던건 없는 거 같아요",
          "original_form": "저도 닭갈비 춘천에서 먹은것 중에 맛있었던건 없는 거 같아요",
          "speaker_id": "1",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019576.1.15",
          "form": "별과달인가 그 프차 아세요?",
          "original_form": "별과달인가 그 프차 아세요?",
          "speaker_id": "1",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019576.1.16",
          "form": "닭갈비 맛있더라구요",
          "original_form": "닭갈비 맛있더라구요",
          "speaker_id": "1",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019576.1.17",
          "form": "처음 들어봐요 ㅋㅋㅋ",
          "original_form": "처음 들어봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019576.1.18",
          "form": "오그래여? 닭갈비 넘 좋아하는데",
          "original_form": "오그래여? 닭갈비 넘 좋아하는데",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019576.1.19",
          "form": "검색해볼게요!",
          "original_form": "검색해볼게요!",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019576.1.20",
          "form": "고구마 무스가 있어서 맛있었어요!",
          "original_form": "고구마 무스가 있어서 맛있었어요!",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019576.1.21",
          "form": "고구마 대신 고구마 무스를 뿌려주더라구요....",
          "original_form": "고구마 대신 고구마 무스를 뿌려주더라구요....",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019576.1.22",
          "form": "신세계!",
          "original_form": "신세계!",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019576.1.23",
          "form": "헉 맛잇겠어요",
          "original_form": "헉 맛잇겠어요",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019576.1.24",
          "form": "고구마무스는...맛없없이죠",
          "original_form": "고구마무스는...맛없없이죠",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019576.1.25",
          "form": "고구마 무스랑 치즈을 이렇게 세줄인가 해주고 가운데에도 짜주고",
          "original_form": "고구마 무스랑 치즈을 이렇게 세줄인가 해주고 가운데에도 짜주고",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019576.1.26",
          "form": "너무 맛있었어요ㅠㅠㅋㅋㅋ",
          "original_form": "너무 맛있었어요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:38"
        }
      ]
    }
