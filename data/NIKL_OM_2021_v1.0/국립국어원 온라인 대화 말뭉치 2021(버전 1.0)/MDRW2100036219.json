      "utterance": [
        {
          "id": "MDRW2100036219.1.1",
          "form": "팝콘치킨 튀겨 먹었어요!",
          "original_form": "팝콘치킨 튀겨 먹었어요!",
          "speaker_id": "2",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100036219.1.2",
          "form": "저녁 뭐 드세요?",
          "original_form": "저녁 뭐 드세요?",
          "speaker_id": "2",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100036219.1.3",
          "form": "저는 고구마맛탕 먹었어요!",
          "original_form": "저는 고구마맛탕 먹었어요!",
          "speaker_id": "1",
          "time": "20210830 19:15"
        },
        {
          "id": "MDRW2100036219.1.4",
          "form": "맛탕은 촉촉한걸 좋아하시나요 퍽퍽한걸 좋아하시나요?",
          "original_form": "맛탕은 촉촉한걸 좋아하시나요 퍽퍽한걸 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210830 19:16"
        },
        {
          "id": "MDRW2100036219.1.5",
          "form": "전 퍽퍽파입니다 ㅎㅎ 어떤거 좋아하세요?!",
          "original_form": "전 퍽퍽파입니다 ㅎㅎ 어떤거 좋아하세요?!",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100036219.1.6",
          "form": "저는 촉촉파에요 ㅎㅎ",
          "original_form": "저는 촉촉파에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:18"
        },
        {
          "id": "MDRW2100036219.1.7",
          "form": "저희 같이 먹으면 되겠어요 ㅋㅋㅋ !",
          "original_form": "저희 같이 먹으면 되겠어요 ㅋㅋㅋ !",
          "speaker_id": "1",
          "time": "20210830 19:18"
        },
        {
          "id": "MDRW2100036219.1.8",
          "form": "싸울 일 없이 먹겠네요 ㅎㅎ",
          "original_form": "싸울 일 없이 먹겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100036219.1.9",
          "form": "치킨은 어느 부위가 더 좋으세요?",
          "original_form": "치킨은 어느 부위가 더 좋으세요?",
          "speaker_id": "2",
          "time": "20210830 19:20"
        },
        {
          "id": "MDRW2100036219.1.10",
          "form": "저 치킨은 다 저랑 먹으려고 할껄요",
          "original_form": "저 치킨은 다 저랑 먹으려고 할껄요",
          "speaker_id": "1",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100036219.1.11",
          "form": "닭가 슴 살 파입니다 ㅎㅎ",
          "original_form": "닭가 슴 살 파입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100036219.1.12",
          "form": "앗 라이벌을 만났습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 라이벌을 만났습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:21"
        },
        {
          "id": "MDRW2100036219.1.13",
          "form": "앗 헐 ㅋㅋㅋㅋ",
          "original_form": "앗 헐 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:22"
        },
        {
          "id": "MDRW2100036219.1.14",
          "form": "취향 겹치기 쉽지않은데 !",
          "original_form": "취향 겹치기 쉽지않은데 !",
          "speaker_id": "1",
          "time": "20210830 19:22"
        },
        {
          "id": "MDRW2100036219.1.15",
          "form": "평화롭게 두 마리 시켜요 ㅋㅋㅋㅋㅋ",
          "original_form": "평화롭게 두 마리 시켜요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:22"
        },
        {
          "id": "MDRW2100036219.1.16",
          "form": "ㅡ그거 좋은방법이네욬ㅋㅋ",
          "original_form": "ㅡ그거 좋은방법이네욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:22"
        },
        {
          "id": "MDRW2100036219.1.17",
          "form": "전 닭다리를 싫어해요 ㅜ",
          "original_form": "전 닭다리를 싫어해요 ㅜ",
          "speaker_id": "1",
          "time": "20210830 19:23"
        },
        {
          "id": "MDRW2100036219.1.18",
          "form": "입에 안 맞나요??",
          "original_form": "입에 안 맞나요??",
          "speaker_id": "2",
          "time": "20210830 19:23"
        },
        {
          "id": "MDRW2100036219.1.19",
          "form": "저는 목 빼고는 그래도 잘 먹어요 ㅎㅎ",
          "original_form": "저는 목 빼고는 그래도 잘 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:24"
        },
        {
          "id": "MDRW2100036219.1.20",
          "form": "닭날개도 요즘 좋더라구요 ㅎㅎ",
          "original_form": "닭날개도 요즘 좋더라구요 ㅎㅎ",
