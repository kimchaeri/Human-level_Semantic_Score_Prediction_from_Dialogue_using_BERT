      "utterance": [
        {
          "id": "MDRW2100008779.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008779.1.2",
          "form": "딱히 좋아하는 편도 싫어하는 편도 아니에요",
          "original_form": "딱히 좋아하는 편도 싫어하는 편도 아니에요",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008779.1.3",
          "form": "ㅎㅎ그렇군용",
          "original_form": "ㅎㅎ그렇군용",
          "speaker_id": "1",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008779.1.4",
          "form": "갔던 여행지중에 좋은 곳 있을까요",
          "original_form": "갔던 여행지중에 좋은 곳 있을까요",
          "speaker_id": "2",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008779.1.5",
          "form": "울릉도욯ㅎ",
          "original_form": "울릉도욯ㅎ",
          "speaker_id": "1",
          "time": "20210822 18:34"
        },
        {
          "id": "MDRW2100008779.1.6",
          "form": "울릉도 얘기는 부모님한테 들은적 있어요",
          "original_form": "울릉도 얘기는 부모님한테 들은적 있어요",
          "speaker_id": "2",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100008779.1.7",
          "form": "바람이 심하면 접근이 힘들다고 하시던데",
          "original_form": "바람이 심하면 접근이 힘들다고 하시던데",
          "speaker_id": "2",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100008779.1.8",
          "form": "ㅋㅋ맞아요 배 핵 흔들려요",
          "original_form": "ㅋㅋ맞아요 배 핵 흔들려요",
          "speaker_id": "1",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100008779.1.9",
          "form": "뱃멀미가 심해서 전 못가겠네요ㅠ",
          "original_form": "뱃멀미가 심해서 전 못가겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100008779.1.10",
          "form": "네 어디 가고 싶으세요?",
          "original_form": "네 어디 가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210823 00:11"
        },
        {
          "id": "MDRW2100008779.1.11",
          "form": "저는 뜬금없지만 쿠바 가보고 싶어요",
          "original_form": "저는 뜬금없지만 쿠바 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100008779.1.12",
          "form": "오 쿠바 이쁠듯해요",
          "original_form": "오 쿠바 이쁠듯해요",
          "speaker_id": "1",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100008779.1.13",
          "form": "분위기가 묘하더라구요",
          "original_form": "분위기가 묘하더라구요",
          "speaker_id": "2",
          "time": "20210823 08:06"
        },
        {
          "id": "MDRW2100008779.1.14",
          "form": "ㅋㅋ버킷리스트에 저장",
          "original_form": "ㅋㅋ버킷리스트에 저장",
          "speaker_id": "1",
          "time": "20210823 09:19"
        },
        {
          "id": "MDRW2100008779.1.15",
          "form": "버킷리스트가 여행지로 가득차겠어요ㅋㅋ",
          "original_form": "버킷리스트가 여행지로 가득차겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:23"
        },
        {
          "id": "MDRW2100008779.1.16",
          "form": "코로나 때문에",
          "original_form": "코로나 때문에",
          "speaker_id": "1",
          "time": "20210823 09:27"
        },
        {
          "id": "MDRW2100008779.1.17",
          "form": "버킷만 가득차겠어요 진짜ㅎㅎ",
