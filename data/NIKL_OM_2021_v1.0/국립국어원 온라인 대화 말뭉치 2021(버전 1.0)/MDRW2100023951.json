      "utterance": [
        {
          "id": "MDRW2100023951.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023951.1.2",
          "form": "자주보는 유튜브 채널 있으세요?",
          "original_form": "자주보는 유튜브 채널 있으세요?",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023951.1.3",
          "form": "저는 솧디 자주봐요",
          "original_form": "저는 솧디 자주봐요",
          "speaker_id": "2",
          "time": "20210901 15:02"
        },
        {
          "id": "MDRW2100023951.1.4",
          "form": "곡 소개 채널이요",
          "original_form": "곡 소개 채널이요",
          "speaker_id": "2",
          "time": "20210901 15:02"
        },
        {
          "id": "MDRW2100023951.1.5",
          "form": "노래좋아하시는구나",
          "original_form": "노래좋아하시는구나",
          "speaker_id": "1",
          "time": "20210901 15:09"
        },
        {
          "id": "MDRW2100023951.1.6",
          "form": "요즘은 어떤노래들으세요?",
          "original_form": "요즘은 어떤노래들으세요?",
          "speaker_id": "1",
          "time": "20210901 15:09"
        },
        {
          "id": "MDRW2100023951.1.7",
          "form": "Alec Benjamin노래 듣고있어요 요즘",
          "original_form": "Alec Benjamin노래 듣고있어요 요즘",
          "speaker_id": "2",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100023951.1.8",
          "form": "name1님은 어떤 노래 좋아하세요?",
          "original_form": "&name1&님은 어떤 노래 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100023951.1.9",
          "form": "저는 요즘 통 노래를 안듣긴했는데 최근에 악뮤 낙화나 안예은 창귀 몇번들었어요",
          "original_form": "저는 요즘 통 노래를 안듣긴했는데 최근에 악뮤 낙화나 안예은 창귀 몇번들었어요",
          "speaker_id": "1",
          "time": "20210901 15:14"
        },
        {
          "id": "MDRW2100023951.1.10",
          "form": "유독 좋아하시는 노래 장르있으세요?",
          "original_form": "유독 좋아하시는 노래 장르있으세요?",
          "speaker_id": "1",
          "time": "20210901 15:14"
        },
        {
          "id": "MDRW2100023951.1.11",
          "form": "저도 악뮤 진짜 좋아해요",
          "original_form": "저도 악뮤 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100023951.1.12",
          "form": "케이팝시절부터 쭉 응원했는데 한결같은 뮤지션인것 같아요",
          "original_form": "케이팝시절부터 쭉 응원했는데 한결같은 뮤지션인것 같아요",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100023951.1.13",
          "form": "특유의 분위기가 너무 좋아요",
          "original_form": "특유의 분위기가 너무 좋아요",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100023951.1.14",
          "form": "먹방 같은건 안보시나요?",
          "original_form": "먹방 같은건 안보시나요?",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100023951.1.15",
          "form": "네 ㅋㅋㅋ보고있음 넘 배고파져요",
          "original_form": "네 ㅋㅋㅋ보고있음 넘 배고파져요",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100023951.1.16",
          "form": "저는 안보다가도 식이할때면 나도모르게 보고있더라구요",
          "original_form": "저는 안보다가도 식이할때면 나도모르게 보고있더라구요",
          "speaker_id": "1",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100023951.1.17",
          "form": "대리만족돼서요?",
          "original_form": "대리만족돼서요?",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100023951.1.18",
          "form": "네ㅋㅋ대리만족으로 보다가 배고파져서 괴로워해요",
          "original_form": "네ㅋㅋ대리만족으로 보다가 배고파져서 괴로워해요",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100023951.1.19",
          "form": "ㅋㅋㅋ식단조절하고 계셔요?",
          "original_form": "ㅋㅋㅋ식단조절하고 계셔요?",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
