      "utterance": [
        {
          "id": "MDRW2100014834.1.1",
          "form": "애들 자나??",
          "original_form": "애들 자나??",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014834.1.2",
          "form": "아니 언니. ^^ 너무 ...잘 노네?",
          "original_form": "아니 언니. ^^ 너무 ...잘 노네?",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014834.1.3",
          "form": "언니네는 애들 자?",
          "original_form": "언니네는 애들 자?",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014834.1.4",
          "form": "하아...",
          "original_form": "하아...",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014834.1.5",
          "form": "여기도",
          "original_form": "여기도",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014834.1.6",
          "form": "잠을 안자네",
          "original_form": "잠을 안자네",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014834.1.7",
          "form": "내일 개학이라",
          "original_form": "내일 개학이라",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014834.1.8",
          "form": "일찍 잤으면 좋겠는데",
          "original_form": "일찍 잤으면 좋겠는데",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014834.1.9",
          "form": "그러니깐은 ㅋㅋㅋ",
          "original_form": "그러니깐은 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014834.1.10",
          "form": "개학이라도.....평소와 똑같다.",
          "original_form": "개학이라도.....평소와 똑같다.",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014834.1.11",
          "form": "그래 내 마음만 개학이지",
          "original_form": "그래 내 마음만 개학이지",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014834.1.12",
          "form": "나 이제 재우러 간다",
          "original_form": "나 이제 재우러 간다",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014834.1.13",
          "form": "빠른 육퇴 바래보자",
          "original_form": "빠른 육퇴 바래보자",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014834.1.14",
          "form": "재우고 만나자",
          "original_form": "재우고 만나자",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014834.1.15",
          "form": "응 알았어. 언니 좀따 봐 ㅋ",
          "original_form": "응 알았어. 언니 좀따 봐 ㅋ",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014834.1.16",
          "form": "어제 애들 재우면서 잠들었었네 ㅋㅋ",
          "original_form": "어제 애들 재우면서 잠들었었네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 07:57"
        },
        {
          "id": "MDRW2100014834.1.17",
          "form": "지안이 알러지약 먹고, 약 바르고 잤더니 한결낫다.",
          "original_form": "지안이 알러지약 먹고, 약 바르고 잤더니 한결낫다.",
          "speaker_id": "1",
          "time": "20210826 07:58"
        },
        {
          "id": "MDRW2100014834.1.18",
          "form": "안과나 피부과갔다가 학교를 갈 지,",
          "original_form": "안과나 피부과갔다가 학교를 갈 지,",
          "speaker_id": "1",
          "time": "20210826 07:58"
        },
        {
          "id": "MDRW2100014834.1.19",
          "form": "학교 갔다가 병원갈지 고민이네",
          "original_form": "학교 갔다가 병원갈지 고민이네",
          "speaker_id": "1",
          "time": "20210826 07:58"
        },
        {
          "id": "MDRW2100014834.1.20",
          "form": "알러지는 맞는것같아.",
          "original_form": "알러지는 맞는것같아.",
          "speaker_id": "1",
          "time": "20210826 07:58"
        },
        {
          "id": "MDRW2100014834.1.21",
          "form": "좀 나아서 다행이네",
          "original_form": "좀 나아서 다행이네",
          "speaker_id": "2",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100014834.1.22",
          "form": "병원 갔나??",
          "original_form": "병원 갔나??",
          "speaker_id": "2",
          "time": "20210826 08:51"
        },
        {
          "id": "MDRW2100014834.1.23",
          "form": "아니 이제가려고ㅋㅋ",
          "original_form": "아니 이제가려고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:04"
        },
        {
          "id": "MDRW2100014834.1.24",
          "form": "병원갔다 학교가야겠네",
          "original_form": "병원갔다 학교가야겠네",
          "speaker_id": "2",
          "time": "20210826 09:14"
        },
        {
          "id": "MDRW2100014834.1.25",
          "form": "응응 안과는 안되겠어서 피부과옴. ㅜㅋㅋ",
          "original_form": "응응 안과는 안되겠어서 피부과옴. ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:52"
        },
        {
          "id": "MDRW2100014834.1.26",
          "form": "아 피부과",
          "original_form": "아 피부과",
          "speaker_id": "2",
          "time": "20210826 10:21"
        },
        {
          "id": "MDRW2100014834.1.27",
          "form": "진료봤나??",
          "original_form": "진료봤나??",
          "speaker_id": "2",
          "time": "20210826 10:21"
        },
        {
          "id": "MDRW2100014834.1.28",
          "form": "첫째 학교 간겨??",
          "original_form": "첫째 학교 간겨??",
          "speaker_id": "2",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100014834.1.29",
          "form": "응 언니 ㅋㅋㅋ비염알러지래. 그래서 약 처방받고 학교 갔어",
          "original_form": "응 언니 ㅋㅋㅋ비염알러지래. 그래서 약 처방받고 학교 갔어",
          "speaker_id": "1",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100014834.1.30",
          "form": "아 진짜??",
          "original_form": "아 진짜??",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100014834.1.31",
          "form": "비염이라서 눈주위가 올라왔구먼",
          "original_form": "비염이라서 눈주위가 올라왔구먼",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100014834.1.32",
          "form": "우리원도",
          "original_form": "우리원도",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100014834.1.33",
          "form": "감기걸려서 코막히면",
          "original_form": "감기걸려서 코막히면",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100014834.1.34",
          "form": "눈주위에 시퍼렇게 되더라",
          "original_form": "눈주위에 시퍼렇게 되더라",
          "speaker_id": "2",
          "time": "20210826 11:10"
        },
        {
          "id": "MDRW2100014834.1.35",
          "form": "맞어맞어 우리애들도 그렇거든",
          "original_form": "맞어맞어 우리애들도 그렇거든",
          "speaker_id": "1",
          "time": "20210826 11:11"
        },
        {
          "id": "MDRW2100014834.1.36",
          "form": "여튼 진짜 그래도 약 먹으면 금방 좋아진다고 하니 다행이지뭐",
          "original_form": "여튼 진짜 그래도 약 먹으면 금방 좋아진다고 하니 다행이지뭐",
          "speaker_id": "1",
          "time": "20210826 11:12"
        }
      ]
    }
