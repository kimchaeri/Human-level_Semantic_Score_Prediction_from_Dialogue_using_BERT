        },
        {
          "id": "MDRW2100014571.1.2",
          "form": "예전에 동전을 ㅋㅋ",
          "original_form": "예전에 동전을 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014571.1.3",
          "form": "3만원사서",
          "original_form": "3만원사서",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014571.1.4",
          "form": "사신거예요?",
          "original_form": "사신거예요?",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014571.1.5",
          "form": "9만원에 팔고",
          "original_form": "9만원에 팔고",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014571.1.6",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014571.1.7",
          "form": "좋았어요 ㅋㅋ",
          "original_form": "좋았어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014571.1.8",
          "form": "근대",
          "original_form": "근대",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014571.1.9",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014571.1.10",
          "form": "아들 16네 팔고",
          "original_form": "아들 16네 팔고",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014571.1.11",
          "form": "친구 30에 팔고 ㅡㅡㅈ",
          "original_form": "친구 30에 팔고 ㅡㅡㅈ",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014571.1.12",
          "form": "저만 9만 바보예요",
          "original_form": "저만 9만 바보예요",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014571.1.13",
          "form": "오잉?",
          "original_form": "오잉?",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014571.1.14",
          "form": "말안듣고",
          "original_form": "말안듣고",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014571.1.15",
          "form": "그렇게도 팔려요?",
          "original_form": "그렇게도 팔려요?",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014571.1.16",
          "form": "한정판요",
          "original_form": "한정판요",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014571.1.17",
          "form": "네 수집용 재테크여",
          "original_form": "네 수집용 재테크여",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014571.1.18",
          "form": "아...한정판",
          "original_form": "아...한정판",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014571.1.19",
          "form": "희귀물이라 비싸게 팔리는군요",
          "original_form": "희귀물이라 비싸게 팔리는군요",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014571.1.20",
          "form": "네 참제가 땅을치고 후회했죠",
          "original_form": "네 참제가 땅을치고 후회했죠",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014571.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014571.1.22",
          "form": "살깨도 당첨되야 사요 ㅋㅋ",
          "original_form": "살깨도 당첨되야 사요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014571.1.23",
          "form": "살때도",
          "original_form": "살때도",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014571.1.24",
          "form": "옷도 그래요",
          "original_form": "옷도 그래요",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014571.1.25",
          "form": "한정판 3만원",
          "original_form": "한정판 3만원",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014571.1.26",
          "form": "바로 13만원 팔아요",
          "original_form": "바로 13만원 팔아요",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014571.1.27",
          "form": "와..그런정보는 어디서 구하세요?ㅎ",
          "original_form": "와..그런정보는 어디서 구하세요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014571.1.28",
          "form": "저도 옷 팔아 봤네요 ㅋㅋ",
          "original_form": "저도 옷 팔아 봤네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014571.1.29",
          "form": "3만원에 판사람은 우째요",
          "original_form": "3만원에 판사람은 우째요",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014571.1.30",
          "form": "아 그게 사이트라 메이커에서 그리 팔아요",
          "original_form": "아 그게 사이트라 메이커에서 그리 팔아요",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014571.1.31",
          "form": "추첨으로",
          "original_form": "추첨으로",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014571.1.32",
          "form": "아...추첨 운도 따라야하는군요",
          "original_form": "아...추첨 운도 따라야하는군요",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014571.1.33",
          "form": "네 잘안돼요",
          "original_form": "네 잘안돼요",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014571.1.34",
          "form": "아주 전산 마비돼요",
          "original_form": "아주 전산 마비돼요",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014571.1.35",
          "form": "정보 아들이 알려줘요",
          "original_form": "정보 아들이 알려줘요",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014571.1.36",
          "form": "공부빼고 잘해요 ㅋㅌ",
          "original_form": "공부빼고 잘해요 ㅋㅌ",
