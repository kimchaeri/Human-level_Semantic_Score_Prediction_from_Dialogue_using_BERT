      "utterance": [
        {
          "id": "MDRW2100016893.1.1",
          "form": "게임계에 요즘 메타버스 소리 많이 나오는데",
          "original_form": "게임계에 요즘 메타버스 소리 많이 나오는데",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016893.1.2",
          "form": "이게뭐지",
          "original_form": "이게뭐지",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016893.1.3",
          "form": "그거 아녀? 로오히 프라우같은거",
          "original_form": "그거 아녀? 로오히 프라우같은거",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016893.1.4",
          "form": "그건 캐릭이 메타 드립을 하는거고",
          "original_form": "그건 캐릭이 메타 드립을 하는거고",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016893.1.5",
          "form": "찾아보니 가상현실에서",
          "original_form": "찾아보니 가상현실에서",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016893.1.6",
          "form": "그럼 모르겠다",
          "original_form": "그럼 모르겠다",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016893.1.7",
          "form": "경제문화사회 교육등 활동을 게임에서 다 할수있는거를 말한다네",
          "original_form": "경제문화사회 교육등 활동을 게임에서 다 할수있는거를 말한다네",
          "speaker_id": "2",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016893.1.8",
          "form": "오.. 싱기하네",
          "original_form": "오.. 싱기하네",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016893.1.9",
          "form": "너 name3한테 왜 답장 안해",
          "original_form": "너 &name3&한테 왜 답장 안해",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016893.1.10",
          "form": "ㅣ?잊고있었어",
          "original_form": "ㅣ?잊고있었어",
          "speaker_id": "2",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016893.1.11",
          "form": "자기가하고싶은 말만해",
          "original_form": "자기가하고싶은 말만해",
          "speaker_id": "2",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016893.1.12",
          "form": "난 그냥 게임이야기나 할래",
          "original_form": "난 그냥 게임이야기나 할래",
          "speaker_id": "2",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016893.1.13",
          "form": "카톡에서 널 원망하고 있어",
          "original_form": "카톡에서 널 원망하고 있어",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016893.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016893.1.15",
          "form": "제2의 나라 천천히 망하고 있는데",
          "original_form": "제2의 나라 천천히 망하고 있는데",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016893.1.16",
          "form": "넷마블이라 오래갈듯",
          "original_form": "넷마블이라 오래갈듯",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016893.1.17",
          "form": "2년은 가려나?",
          "original_form": "2년은 가려나?",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016893.1.18",
          "form": "ㅇㅇ... 7대죄 그것도 오래가는거보면",
          "original_form": "ㅇㅇ... 7대죄 그것도 오래가는거보면",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016893.1.19",
          "form": "그거 오래가는거 신기함ㅋㅋㅋ",
          "original_form": "그거 오래가는거 신기함ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016893.1.20",
          "form": "요즘 폰게임 하는사람들은 사실 비슷한걸",
          "original_form": "요즘 폰게임 하는사람들은 사실 비슷한걸",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016893.1.21",
          "form": "계속하는걸 선호하는거 같아보여",
          "original_form": "계속하는걸 선호하는거 같아보여",
          "speaker_id": "2",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016893.1.22",
          "form": "리니지라이크 라면서도 결국 제2의나라 흥하고",
          "original_form": "리니지라이크 라면서도 결국 제2의나라 흥하고",
