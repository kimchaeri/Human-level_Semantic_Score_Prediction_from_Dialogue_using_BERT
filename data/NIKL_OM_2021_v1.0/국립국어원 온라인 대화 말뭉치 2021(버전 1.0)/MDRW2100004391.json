      "utterance": [
        {
          "id": "MDRW2100004391.1.1",
          "form": "메밀국수도 밀키트가 있어요?",
          "original_form": "메밀국수도 밀키트가 있어요?",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004391.1.2",
          "form": "남친도 자취를 안해서...",
          "original_form": "남친도 자취를 안해서...",
          "speaker_id": "1",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004391.1.3",
          "form": "아항...",
          "original_form": "아항...",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004391.1.4",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004391.1.5",
          "form": "메밀국수 광화문에",
          "original_form": "메밀국수 광화문에",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004391.1.6",
          "form": "데이트하는데 돈 엄청 나가겠네요",
          "original_form": "데이트하는데 돈 엄청 나가겠네요",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004391.1.7",
          "form": "미진이라고 유명 맛집있는데",
          "original_form": "미진이라고 유명 맛집있는데",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004391.1.8",
          "form": "아 거기서 밀키트 판매하나요?",
          "original_form": "아 거기서 밀키트 판매하나요?",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004391.1.9",
          "form": "음 일단 마켓컬리에서 팔아요",
          "original_form": "음 일단 마켓컬리에서 팔아요",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004391.1.10",
          "form": "아 여름에 먹어야되는데 너무 늦게알았다",
          "original_form": "아 여름에 먹어야되는데 너무 늦게알았다",
          "speaker_id": "2",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004391.1.11",
          "form": "국물이 진해서 맛있죠ㅋ",
          "original_form": "국물이 진해서 맛있죠ㅋ",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004391.1.12",
          "form": "컬리에서 짜장면같은건 시켜먹어봤어요",
          "original_form": "컬리에서 짜장면같은건 시켜먹어봤어요",
          "speaker_id": "2",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004391.1.13",
          "form": "그것도 제가 그 팩 중탕해가지고",
          "original_form": "그것도 제가 그 팩 중탕해가지고",
          "speaker_id": "2",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004391.1.14",
          "form": "비벼서",
          "original_form": "비벼서",
          "speaker_id": "2",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004391.1.15",
          "form": "가져다드렸는데",
          "original_form": "가져다드렸는데",
          "speaker_id": "2",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004391.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맛이 좋구나 하셨나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맛이 좋구나 하셨나요",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004391.1.17",
          "form": "그날은 안때리더라구요...",
          "original_form": "그날은 안때리더라구요...",
          "speaker_id": "2",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004391.1.18",
          "form": "따흐흑...ㅜ",
          "original_form": "따흐흑...ㅜ",
          "speaker_id": "1",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004391.1.19",
          "form": "전 데이트때 맛집다니면 비용이 꽤 나가긴하는데",
          "original_form": "전 데이트때 맛집다니면 비용이 꽤 나가긴하는데",
          "speaker_id": "1",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004391.1.20",
          "form": "요즘은 그러질못하니까",
          "original_form": "요즘은 그러질못하니까",
          "speaker_id": "1",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004391.1.21",
          "form": "남치니네 가게에서 간단한음식 배달시켜먹어서 비용이 꽤 줄었어요",
          "original_form": "남치니네 가게에서 간단한음식 배달시켜먹어서 비용이 꽤 줄었어요",
          "speaker_id": "1",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004391.1.22",
          "form": "오... 따장님이신가봐요",
          "original_form": "오... 따장님이신가봐요",
          "speaker_id": "2",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004391.1.23",
          "form": "넹 따장님이세영",
          "original_form": "넹 따장님이세영",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004391.1.24",
          "form": "주말에는 가게를 안해서 거기서 만나영",
          "original_form": "주말에는 가게를 안해서 거기서 만나영",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004391.1.25",
          "form": "부르마블도 하고 영화도 보고",
          "original_form": "부르마블도 하고 영화도 보고",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004391.1.26",
          "form": "맛있는 것도 해줘여",
          "original_form": "맛있는 것도 해줘여",
          "speaker_id": "1",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004391.1.27",
          "form": "행복하시겠네요...",
          "original_form": "행복하시겠네요...",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004391.1.28",
          "form": "빌게이츠한테 어제 오백원 주웠다고 자랑한 기분이 드네요 갑자기",
          "original_form": "빌게이츠한테 어제 오백원 주웠다고 자랑한 기분이 드네요 갑자기",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004391.1.29",
          "form": "왜지...",
          "original_form": "왜지...",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004391.1.30",
          "form": "ㅋㅋㅋㅋㅋ가게는 제 것도 아니고... 남친거고...건물주느님이 있으시니...",
          "original_form": "ㅋㅋㅋㅋㅋ가게는 제 것도 아니고... 남친거고...건물주느님이 있으시니...",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004391.1.31",
          "form": "또르륵...",
          "original_form": "또르륵...",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004391.1.32",
          "form": "다 같죠 뭐...",
          "original_form": "다 같죠 뭐...",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004391.1.33",
          "form": "일식 좋아하시나여",
          "original_form": "일식 좋아하시나여",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004391.1.34",
          "form": "닉네임 보시면 아시겠지만",
          "original_form": "닉네임 보시면 아시겠지만",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004391.1.35",
          "form": "일식 전반으로 다 좋아합니다",
          "original_form": "일식 전반으로 다 좋아합니다",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004391.1.36",
          "form": "튀김요리 킬러예요",
          "original_form": "튀김요리 킬러예요",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004391.1.37",
          "form": "이자카야도 좋아하시나여",
          "original_form": "이자카야도 좋아하시나여",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004391.1.38",
          "form": "예에전에 친구가 알바하는 가게 갔었는데",
          "original_form": "예에전에 친구가 알바하는 가게 갔었는데",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004391.1.39",
          "form": "가격이...",
          "original_form": "가격이...",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004391.1.40",
          "form": "음...? 제 톡이 안갔나요? 가격이 좀 비싸긴하다는 톡이요",
          "original_form": "음...? 제 톡이 안갔나요? 가격이 좀 비싸긴하다는 톡이요",
          "speaker_id": "1",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004391.1.41",
          "form": "안왔어용",
          "original_form": "안왔어용",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004391.1.42",
          "form": "저는 이자카야에서 모듬회 먹어봤는데",
          "original_form": "저는 이자카야에서 모듬회 먹어봤는데",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004391.1.43",
          "form": "좀 실망해가지구",
          "original_form": "좀 실망해가지구",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004391.1.44",
          "form": "엄청 비싼데 엄청 사각사각했어요ㅋㅋㅋㅋㅋ",
          "original_form": "엄청 비싼데 엄청 사각사각했어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004391.1.45",
          "form": "덜녹아서",
          "original_form": "덜녹아서",
          "speaker_id": "2",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004391.1.46",
          "form": "으앗...",
          "original_form": "으앗...",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004391.1.47",
          "form": "전 회를 못먹어요",
          "original_form": "전 회를 못먹어요",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004391.1.48",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004391.1.49",
          "form": "남치니도 일식요리사인데 회를 안먹어요 하도 먹어서",
          "original_form": "남치니도 일식요리사인데 회를 안먹어요 하도 먹어서",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004391.1.50",
          "form": "으르신 못먹는것도 있으셨군요",
          "original_form": "으르신 못먹는것도 있으셨군요",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004391.1.51",
          "form": "ㅋㅋㅋㅋㅋ회 못먹...",
          "original_form": "ㅋㅋㅋㅋㅋ회 못먹...",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004391.1.52",
          "form": "고수좋아하시나요",
          "original_form": "고수좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004391.1.53",
          "form": "제가 고수 냄새를 못맡아서",
          "original_form": "제가 고수 냄새를 못맡아서",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004391.1.54",
          "form": "전 쌀국수랑 똠얌꿍 좋아하는데 고수는 진짜...못먹겠어요",
          "original_form": "전 쌀국수랑 똠얌꿍 좋아하는데 고수는 진짜...못먹겠어요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004391.1.55",
          "form": "전 쌀국수에 한웅큼",
          "original_form": "전 쌀국수에 한웅큼",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004391.1.56",
          "form": "넣어먹어요",
          "original_form": "넣어먹어요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004391.1.57",
          "form": "우와...전 고수씹으면 샴푸먹는거 같아서 못먹겠던데",
          "original_form": "우와...전 고수씹으면 샴푸먹는거 같아서 못먹겠던데",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004391.1.58",
          "form": "냄새 못맡아서 어떤 느낌인지도 몰라요ㅜㅜ",
          "original_form": "냄새 못맡아서 어떤 느낌인지도 몰라요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004391.1.59",
          "form": "딴건 다 잘 맡는데",
          "original_form": "딴건 다 잘 맡는데",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004391.1.60",
          "form": "고수만 유독 냄새를 몰라요",
          "original_form": "고수만 유독 냄새를 몰라요",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004391.1.61",
          "form": "그게 좋은거죠ㅜ 고수가 몸에 좋다던데",
          "original_form": "그게 좋은거죠ㅜ 고수가 몸에 좋다던데",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004391.1.62",
          "form": "먹방유튜브 얘기한 김에 먹는거 얘기하죠",
          "original_form": "먹방유튜브 얘기한 김에 먹는거 얘기하죠",
          "speaker_id": "1",
          "time": "20210826 12:29"
        }
      ]
