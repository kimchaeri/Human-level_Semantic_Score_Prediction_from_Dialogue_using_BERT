      "utterance": [
        {
          "id": "MDRW2100046162.1.1",
          "form": "오 순두부찌개 너무 맛있죠",
          "original_form": "오 순두부찌개 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046162.1.2",
          "form": "한식 좋아하시나요?",
          "original_form": "한식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046162.1.3",
          "form": "네 너무 좋아해요! 저는 선지찌개도 땡기네요",
          "original_form": "네 너무 좋아해요! 저는 선지찌개도 땡기네요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046162.1.4",
          "form": "크 요즘 국밥류가 끌리더라구요",
          "original_form": "크 요즘 국밥류가 끌리더라구요",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046162.1.5",
          "form": "그죠그죠 국물있는 게 최고예요ㅠ",
          "original_form": "그죠그죠 국물있는 게 최고예요ㅠ",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046162.1.6",
          "form": "어제밤에 비와서 쌀쌀해서그런가봐요",
          "original_form": "어제밤에 비와서 쌀쌀해서그런가봐요",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046162.1.7",
          "form": "맞아요ㅠㅠ 이제 완전 가을이에요",
          "original_form": "맞아요ㅠㅠ 이제 완전 가을이에요",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046162.1.8",
          "form": "가을되면 커피도 따뜻한거마시게되요",
          "original_form": "가을되면 커피도 따뜻한거마시게되요",
          "speaker_id": "1",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046162.1.9",
          "form": "맞아요ㅋㅋㅋ 따뜻한 라떼면 천국이죠",
          "original_form": "맞아요ㅋㅋㅋ 따뜻한 라떼면 천국이죠",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046162.1.10",
          "form": "크 저도 아메리카노는 아이스로 먹는데 라떼는따뜻한게좋아요",
          "original_form": "크 저도 아메리카노는 아이스로 먹는데 라떼는따뜻한게좋아요",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046162.1.11",
          "form": "저도요ㅠㅠ 얼죽아지만 라떼는 핫으로!",
          "original_form": "저도요ㅠㅠ 얼죽아지만 라떼는 핫으로!",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046162.1.12",
          "form": "크 뭘좀아시네요",
          "original_form": "크 뭘좀아시네요",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046162.1.13",
          "form": "라떼사먹어야겠어요",
          "original_form": "라떼사먹어야겠어요",
          "speaker_id": "1",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046162.1.14",
          "form": "저도요! 갑자기 땡기네요ㅋㅋ",
          "original_form": "저도요! 갑자기 땡기네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:00"
        }
