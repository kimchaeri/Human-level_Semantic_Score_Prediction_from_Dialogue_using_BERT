      "utterance": [
        {
          "id": "MDRW2100038533.1.1",
          "form": "노랑 ㅠㅠ",
          "original_form": "노랑 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100038533.1.2",
          "form": "흰색 옷은 정말... 예쁘긴 한데",
          "original_form": "흰색 옷은 정말... 예쁘긴 한데",
          "speaker_id": "1",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100038533.1.3",
          "form": "세탁도 잘 해야하고",
          "original_form": "세탁도 잘 해야하고",
          "speaker_id": "1",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100038533.1.4",
          "form": "까다로워요!",
          "original_form": "까다로워요!",
          "speaker_id": "1",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100038533.1.5",
          "form": "맞어요 ㅠㅠㅠ",
          "original_form": "맞어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100038533.1.6",
          "form": "진짜마음아파요",
          "original_form": "진짜마음아파요",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100038533.1.7",
          "form": "그래서 가끔은",
          "original_form": "그래서 가끔은",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100038533.1.8",
          "form": "아이보리색으로 합의볼 때도 있는 것 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "아이보리색으로 합의볼 때도 있는 것 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100038533.1.9",
          "form": "저도요 ㅋㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100038533.1.10",
          "form": "이번여름엔 아이보리마니산거같아요",
          "original_form": "이번여름엔 아이보리마니산거같아요",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100038533.1.11",
          "form": "오오 어떤 물건을 사셨나요?!",
          "original_form": "오오 어떤 물건을 사셨나요?!",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100038533.1.12",
          "form": "저는 베이지색 반팔 니트요 ㅎㅎ",
          "original_form": "저는 베이지색 반팔 니트요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100038533.1.13",
          "form": "치마나원피스??",
          "original_form": "치마나원피스??",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100038533.1.14",
          "form": "ㅌㅌㅌㅌ",
          "original_form": "ㅌㅌㅌㅌ",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100038533.1.15",
          "form": "아아 그러시구나!!!",
          "original_form": "아아 그러시구나!!!",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100038533.1.16",
          "form": "하의는 흰색 사입기가 참 부담스러운거같아요ㅠ",
          "original_form": "하의는 흰색 사입기가 참 부담스러운거같아요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100038533.1.17",
          "form": "ㅋㅋㅋㅋㅋ그래됴 치마는 조꼼갠춘한거같아요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그래됴 치마는 조꼼갠춘한거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100038533.1.18",
          "form": "그쵸그쵸 저는 치마 안입은지 오래 되었네욬ㅋㅋ ㅠ",
          "original_form": "그쵸그쵸 저는 치마 안입은지 오래 되었네욬ㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:49"
        },
        {
          "id": "MDRW2100038533.1.19",
          "form": "요즘은 운동화를 장만하고 싶어요!",
          "original_form": "요즘은 운동화를 장만하고 싶어요!",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100038533.1.20",
          "form": "오오!!!사면좋쵸!!!",
          "original_form": "오오!!!사면좋쵸!!!",
          "speaker_id": "2",
          "time": "20210903 06:39"
        },
        {
          "id": "MDRW2100038533.1.21",
          "form": "name2님은 운동화 어디꺼 신으시나요?!",
          "original_form": "&name2&님은 운동화 어디꺼 신으시나요?!",
          "speaker_id": "1",
          "time": "20210903 08:06"
        },
        {
          "id": "MDRW2100038533.1.22",
          "form": "전휠라 신어요!!",
          "original_form": "전휠라 신어요!!",
          "speaker_id": "2",
