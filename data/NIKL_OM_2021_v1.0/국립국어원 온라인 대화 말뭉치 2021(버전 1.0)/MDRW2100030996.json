      "utterance": [
        {
          "id": "MDRW2100030996.1.1",
          "form": "멋있어요 듀얼컴퓨터",
          "original_form": "멋있어요 듀얼컴퓨터",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100030996.1.2",
          "form": "일의 효율 높여줬나요?",
          "original_form": "일의 효율 높여줬나요?",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100030996.1.3",
          "form": "네네, 넓게 볼 수 있어서 좋더라구요",
          "original_form": "네네, 넓게 볼 수 있어서 좋더라구요",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030996.1.4",
          "form": "실험실에서 인턴할때",
          "original_form": "실험실에서 인턴할때",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030996.1.5",
          "form": "컴퓨터 2개 쓰시는 대학원생분들보면 뭔가",
          "original_form": "컴퓨터 2개 쓰시는 대학원생분들보면 뭔가",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030996.1.6",
          "form": "멋있어보였어요",
          "original_form": "멋있어보였어요",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030996.1.7",
          "form": "그쵸ㅋㅋㅋ 저는 교수님 두개짜리 세개짜리 모니터 볼때 신기했어요",
          "original_form": "그쵸ㅋㅋㅋ 저는 교수님 두개짜리 세개짜리 모니터 볼때 신기했어요",
          "speaker_id": "2",
          "time": "20210904 15:40"
        },
        {
          "id": "MDRW2100030996.1.8",
          "form": "비록 교수는 못됐지만 모니터는 두개",
          "original_form": "비록 교수는 못됐지만 모니터는 두개",
          "speaker_id": "2",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100030996.1.9",
          "form": "모니터값 벌려고 열심히 일하고 있습니다",
          "original_form": "모니터값 벌려고 열심히 일하고 있습니다",
          "speaker_id": "2",
          "time": "20210904 15:41"
        },
        {
          "id": "MDRW2100030996.1.10",
          "form": "와 세개짜리도있네요",
          "original_form": "와 세개짜리도있네요",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100030996.1.11",
          "form": "교수는 못됐지만ㅋㅋㅋㅋ",
          "original_form": "교수는 못됐지만ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100030996.1.12",
          "form": "교수가 꿈이셨나요",
          "original_form": "교수가 꿈이셨나요",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100030996.1.13",
          "form": "아뇨, 공부랑은 거리가 멀어섴ㅋㅋㅋ",
          "original_form": "아뇨, 공부랑은 거리가 멀어섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100030996.1.14",
          "form": "교육업은 생각도 안했습니다",
          "original_form": "교육업은 생각도 안했습니다",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100030996.1.15",
          "form": "지금도 재택 프리랜서",
          "original_form": "지금도 재택 프리랜서",
          "speaker_id": "2",
          "time": "20210904 17:52"
        },
        {
          "id": "MDRW2100030996.1.16",
          "form": "프리랜서 멋있어요",
          "original_form": "프리랜서 멋있어요",
          "speaker_id": "1",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100030996.1.17",
          "form": "능력있어야 할 수 있는...",
          "original_form": "능력있어야 할 수 있는...",
          "speaker_id": "1",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100030996.1.18",
          "form": "ㅎㅎㅎ 운이 좋아서 하고 있어요",
          "original_form": "ㅎㅎㅎ 운이 좋아서 하고 있어요",
          "speaker_id": "2",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100030996.1.19",
          "form": "운도 노력하는 사람한테 온다구요!",
          "original_form": "운도 노력하는 사람한테 온다구요!",
          "speaker_id": "1",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100030996.1.20",
          "form": "능력있는 사람중에 운있는 사람이 잘되는것같아요",
          "original_form": "능력있는 사람중에 운있는 사람이 잘되는것같아요",
          "speaker_id": "1",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100030996.1.21",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100030996.1.22",
          "form": "ㅎㅎㅎ 되고싶다 하고싶다 염불외면 그렇게 되는 것 같아요",
          "original_form": "ㅎㅎㅎ 되고싶다 하고싶다 염불외면 그렇게 되는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100030996.1.23",
          "form": "name1님은 직장인이세요?",
          "original_form": "&name1&님은 직장인이세요?",
          "speaker_id": "2",
          "time": "20210904 18:27"
        },
        {
          "id": "MDRW2100030996.1.24",
          "form": "전 대학생이요!",
          "original_form": "전 대학생이요!",
          "speaker_id": "1",
          "time": "20210904 18:48"
        },
        {
          "id": "MDRW2100030996.1.25",
          "form": "이번학기가 막학기에요ㅠ",
          "original_form": "이번학기가 막학기에요ㅠ",
          "speaker_id": "1",
          "time": "20210904 18:48"
        },
        {
          "id": "MDRW2100030996.1.26",
          "form": "22년 2월 졸업예정이랍니다",
          "original_form": "22년 2월 졸업예정이랍니다",
          "speaker_id": "1",
          "time": "20210904 18:48"
        },
        {
          "id": "MDRW2100030996.1.27",
          "form": "유예하게될수도있구요",
          "original_form": "유예하게될수도있구요",
          "speaker_id": "1",
          "time": "20210904 18:49"
        },
        {
          "id": "MDRW2100030996.1.28",
          "form": "오, 곧 졸업이시군요",
          "original_form": "오, 곧 졸업이시군요",
          "speaker_id": "2",
          "time": "20210904 18:59"
        },
        {
          "id": "MDRW2100030996.1.29",
          "form": "하고 싶은 일은 있으세요??",
          "original_form": "하고 싶은 일은 있으세요??",
          "speaker_id": "2",
          "time": "20210904 18:59"
        },
        {
          "id": "MDRW2100030996.1.30",
          "form": "반도체 소재 제작하고싶어요!!!",
          "original_form": "반도체 소재 제작하고싶어요!!!",
          "speaker_id": "1",
          "time": "20210906 11:18"
        },
        {
          "id": "MDRW2100030996.1.31",
          "form": "오, 이과시군요\r\n어느 학과세요? 반도체공학?",
          "original_form": "오, 이과시군요\r\n어느 학과세요? 반도체공학?",
          "speaker_id": "2",
          "time": "20210906 11:26"
        },
        {
