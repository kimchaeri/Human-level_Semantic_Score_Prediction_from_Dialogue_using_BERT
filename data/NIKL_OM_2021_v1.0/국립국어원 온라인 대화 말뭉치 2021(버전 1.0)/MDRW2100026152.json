      "utterance": [
        {
          "id": "MDRW2100026152.1.1",
          "form": "오늘 점심 토달볶 해먹었어",
          "original_form": "오늘 점심 토달볶 해먹었어",
          "speaker_id": "2",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100026152.1.2",
          "form": "오늘은 감자도 곁들여서 토달감볶이었단다",
          "original_form": "오늘은 감자도 곁들여서 토달감볶이었단다",
          "speaker_id": "2",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100026152.1.3",
          "form": "윽..듣기만해도 괴식이다",
          "original_form": "윽..듣기만해도 괴식이다",
          "speaker_id": "1",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100026152.1.4",
          "form": "무슨 감자까지 총출동이냐",
          "original_form": "무슨 감자까지 총출동이냐",
          "speaker_id": "1",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100026152.1.5",
          "form": "괴식이라니",
          "original_form": "괴식이라니",
          "speaker_id": "2",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100026152.1.6",
          "form": "콩토달감볶 만들어서 선물해버린다",
          "original_form": "콩토달감볶 만들어서 선물해버린다",
          "speaker_id": "2",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100026152.1.7",
          "form": "으...유리상자안에 영원히 보관만할게",
          "original_form": "으...유리상자안에 영원히 보관만할게",
          "speaker_id": "1",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100026152.1.8",
          "form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋ웃겨 증말",
          "original_form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋ웃겨 증말",
          "speaker_id": "2",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026152.1.9",
          "form": "뭐 먹었니?",
          "original_form": "뭐 먹었니?",
          "speaker_id": "2",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026152.1.10",
          "form": "싸이버거 먹는데",
          "original_form": "싸이버거 먹는데",
          "speaker_id": "1",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100026152.1.11",
          "form": "원래는 감자튀김먹어도",
          "original_form": "원래는 감자튀김먹어도",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026152.1.12",
          "form": "배가안찼는데",
          "original_form": "배가안찼는데",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026152.1.13",
          "form": "이제 햄버거 단품만먹어도",
          "original_form": "이제 햄버거 단품만먹어도",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026152.1.14",
          "form": "배불러",
          "original_form": "배불러",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100026152.1.15",
          "form": "오메 양이 확 줄었네 요즘 밥 줄이기 중이야?",
          "original_form": "오메 양이 확 줄었네 요즘 밥 줄이기 중이야?",
          "speaker_id": "2",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100026152.1.16",
          "form": "그렇게 애타게 노력안했는데",
          "original_form": "그렇게 애타게 노력안했는데",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026152.1.17",
          "form": "자연스럽게 줄었어",
          "original_form": "자연스럽게 줄었어",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100026152.1.18",
          "form": "신기하다 난 요즘 늘었는데",
          "original_form": "신기하다 난 요즘 늘었는데",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100026152.1.19",
          "form": "난 살크업 하려나봐",
          "original_form": "난 살크업 하려나봐",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100026152.1.20",
          "form": "좋다좋다 살크업에는 샤브샤브가 짱이래",
          "original_form": "좋다좋다 살크업에는 샤브샤브가 짱이래",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026152.1.21",
          "form": "건강하게 찌는건",
          "original_form": "건강하게 찌는건",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026152.1.22",
          "form": "진짜? 샤브샤브 많이 먹어야겠는걸",
          "original_form": "진짜? 샤브샤브 많이 먹어야겠는걸",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026152.1.23",
          "form": "넌 매운샤브가 좋니 일반 샤브가 좋니",
          "original_form": "넌 매운샤브가 좋니 일반 샤브가 좋니",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026152.1.24",
          "form": "둘다 매력이있지",
          "original_form": "둘다 매력이있지",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100026152.1.25",
          "form": "인정 둘 다 맛있다",
          "original_form": "인정 둘 다 맛있다",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100026152.1.26",
          "form": "그래도 난 일반 샤브가 더 좋아",
          "original_form": "그래도 난 일반 샤브가 더 좋아",
          "speaker_id": "2",
          "time": "20210901 20:39"
        },
        {
          "id": "MDRW2100026152.1.27",
          "form": "넌 샤브 다 먹고 죽이 좋니 국수가 좋니",
          "original_form": "넌 샤브 다 먹고 죽이 좋니 국수가 좋니",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100026152.1.28",
          "form": "난 무조건 죽",
          "original_form": "난 무조건 죽",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100026152.1.29",
          "form": "칼국수는 안먹어도",
          "original_form": "칼국수는 안먹어도",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100026152.1.30",
          "form": "죽은 꼭 먹어",
          "original_form": "죽은 꼭 먹어",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100026152.1.31",
          "form": "어째서죠",
          "original_form": "어째서죠",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100026152.1.32",
          "form": "죽이젤맛있어",
          "original_form": "죽이젤맛있어",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100026152.1.33",
          "form": "죽 맛있긴 혀",
          "original_form": "죽 맛있긴 혀",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100026152.1.34",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "1",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026152.1.35",
          "form": "난 죽이나 국수 말고 샤브 본품을 더 좋아해",
          "original_form": "난 죽이나 국수 말고 샤브 본품을 더 좋아해",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026152.1.36",
          "form": "고기랑 야채를 더 먹고 끝내",
          "original_form": "고기랑 야채를 더 먹고 끝내",
          "speaker_id": "2",
          "time": "20210901 20:45"
        },
        {
          "id": "MDRW2100026152.1.37",
          "form": "야채도 좋지 난 단배추",
          "original_form": "야채도 좋지 난 단배추",
          "speaker_id": "1",
          "time": "20210901 20:47"
        }
      ]
    }
