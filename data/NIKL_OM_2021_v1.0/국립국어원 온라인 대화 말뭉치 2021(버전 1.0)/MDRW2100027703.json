      "utterance": [
        {
          "id": "MDRW2100027703.1.1",
          "form": "스케일이 크시네요 ㅎㅎ",
          "original_form": "스케일이 크시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027703.1.2",
          "form": "여자입니다ㅋㅋ",
          "original_form": "여자입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027703.1.3",
          "form": "이것저것 해보는걸 좋아해요",
          "original_form": "이것저것 해보는걸 좋아해요",
          "speaker_id": "2",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027703.1.4",
          "form": "와  저도 여러게임 하긴했지만",
          "original_form": "와  저도 여러게임 하긴했지만",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027703.1.5",
          "form": "온라인게임 여러게 하셨네요",
          "original_form": "온라인게임 여러게 하셨네요",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027703.1.6",
          "form": "금방 질리는 편이기도 해서요ㅎㅎ 바짝하고 넘어가고 합니다",
          "original_form": "금방 질리는 편이기도 해서요ㅎㅎ 바짝하고 넘어가고 합니다",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027703.1.7",
          "form": "저는 스위치랑 플스 샀는데",
          "original_form": "저는 스위치랑 플스 샀는데",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100027703.1.8",
          "form": "플스는 거의 먼지가 수북",
          "original_form": "플스는 거의 먼지가 수북",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100027703.1.9",
          "form": "저도 스위치 있는데 거의 거실 장식품같아요ㅋㅋ 가끔 손님 접대용 마리오카트나 하고",
          "original_form": "저도 스위치 있는데 거의 거실 장식품같아요ㅋㅋ 가끔 손님 접대용 마리오카트나 하고",
          "speaker_id": "2",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100027703.1.10",
          "form": "동물의 숲도 조금 하다 말았어요",
          "original_form": "동물의 숲도 조금 하다 말았어요",
          "speaker_id": "2",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100027703.1.11",
          "form": "어 저 카트 하고싶었는데",
          "original_form": "어 저 카트 하고싶었는데",
          "speaker_id": "1",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027703.1.12",
          "form": "8나온지 너무 오래라",
          "original_form": "8나온지 너무 오래라",
          "speaker_id": "1",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027703.1.13",
          "form": "곧 9 나올각 ㅋㅋ",
          "original_form": "곧 9 나올각 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100027703.1.14",
          "form": "동숲 옛날거랑 별로 달라진게 없는 기분 ㅜㅜ",
          "original_form": "동숲 옛날거랑 별로 달라진게 없는 기분 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 14:41"
        },
        {
          "id": "MDRW2100027703.1.15",
          "form": "저는 닌텐도 자체가 처음이긴 한데 동숲 하다말고 마인크래프트에 빠져버렸어요ㅋㅋ",
          "original_form": "저는 닌텐도 자체가 처음이긴 한데 동숲 하다말고 마인크래프트에 빠져버렸어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100027703.1.16",
          "form": "아무래도 어릴때부터 PC게임만 해와서 콘솔이 손에 잘 안붙더라고요",
          "original_form": "아무래도 어릴때부터 PC게임만 해와서 콘솔이 손에 잘 안붙더라고요",
          "speaker_id": "2",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100027703.1.17",
          "form": "스위치로 마크 괜찮나요?!",
          "original_form": "스위치로 마크 괜찮나요?!",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100027703.1.18",
          "form": "동숲 동물만 모으다 접은 ㅋㅋㅋㅋ",
          "original_form": "동숲 동물만 모으다 접은 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027703.1.19",
          "form": "공사가 귀찮아서",
          "original_form": "공사가 귀찮아서",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027703.1.20",
          "form": "아 마크는 컴으로 했습니다ㅋㅋ",
          "original_form": "아 마크는 컴으로 했습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027703.1.21",
          "form": "저는 키보드 마우스 컨트롤 밖에 못하는것같아요ㅋㅋ",
          "original_form": "저는 키보드 마우스 컨트롤 밖에 못하는것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027703.1.22",
          "form": "아까비 저는 집에 컴퓨터가 없어요 ㅠㅠ",
          "original_form": "아까비 저는 집에 컴퓨터가 없어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100027703.1.23",
          "form": "아이고 요새 사려면 더 힘들다고는 들었어요ㅜㅜ 비트코인때문에 그래픽카드가 금값이라던데요",
          "original_form": "아이고 요새 사려면 더 힘들다고는 들었어요ㅜㅜ 비트코인때문에 그래픽카드가 금값이라던데요",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027703.1.24",
          "form": "그니까요 날이 갈수록 오르나봐요",
          "original_form": "그니까요 날이 갈수록 오르나봐요",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027703.1.25",
          "form": "그래서 게임은 방송으로 다본다는",
          "original_form": "그래서 게임은 방송으로 다본다는",
          "speaker_id": "1",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027703.1.26",
          "form": "요즘 어지간한 게임은 사양이 높아서ㅜㅜ 폰게임도 폰 엄청 뜨거워지고요",
          "original_form": "요즘 어지간한 게임은 사양이 높아서ㅜㅜ 폰게임도 폰 엄청 뜨거워지고요",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027703.1.27",
          "form": "저 원신하는데",
          "original_form": "저 원신하는데",
          "speaker_id": "1",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027703.1.28",
          "form": "뜨끈뜨끈 하더라구요",
          "original_form": "뜨끈뜨끈 하더라구요",
          "speaker_id": "1",
          "time": "20210902 15:10"
        }
      ]
