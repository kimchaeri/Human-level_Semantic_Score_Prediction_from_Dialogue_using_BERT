      "utterance": [
        {
          "id": "MDRW2100029649.1.1",
          "form": "핫딜 너무 소중해요 ㅋㅋㅋ",
          "original_form": "핫딜 너무 소중해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:20"
        },
        {
          "id": "MDRW2100029649.1.2",
          "form": "마트 가는 건 좋은데 집까지 나르는 것도 또 은근 일이더라구요ㅋㅋㅋㅋ",
          "original_form": "마트 가는 건 좋은데 집까지 나르는 것도 또 은근 일이더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029649.1.3",
          "form": "맞아요 ㅋㅋ 구경하고 카트에담고  이것까진 즐거운데 ㅋㄱㄱㅋ",
          "original_form": "맞아요 ㅋㅋ 구경하고 카트에담고  이것까진 즐거운데 ㅋㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029649.1.4",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "1",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029649.1.5",
          "form": "역시 쇼핑은 인터넷이죠 ㅋㅋ",
          "original_form": "역시 쇼핑은 인터넷이죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029649.1.6",
          "form": "그쵸그쵸 ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸그쵸 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:21"
        },
        {
          "id": "MDRW2100029649.1.7",
          "form": "무배 잘 찾아서 결제하고요 ㅋㅋㅋㅋ",
          "original_form": "무배 잘 찾아서 결제하고요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029649.1.8",
          "form": "네 맞아용ㅋㅋ",
          "original_form": "네 맞아용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029649.1.9",
          "form": "화장품도 오프라인에서 산지 백만년 된 거 같아요 ㅋㅋㅋ",
          "original_form": "화장품도 오프라인에서 산지 백만년 된 거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029649.1.10",
          "form": "근데 옷이나 신발은",
          "original_form": "근데 옷이나 신발은",
          "speaker_id": "2",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029649.1.11",
          "form": "입어보고 신어보고",
          "original_form": "입어보고 신어보고",
          "speaker_id": "2",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029649.1.12",
          "form": "필요할때가 있어서 좀 아쉬워요ㅜ",
          "original_form": "필요할때가 있어서 좀 아쉬워요ㅜ",
          "speaker_id": "2",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029649.1.13",
          "form": "저도 화장품 오프안간지 오래됐네요 ㅎㅎ;",
          "original_form": "저도 화장품 오프안간지 오래됐네요 ㅎㅎ;",
          "speaker_id": "2",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029649.1.14",
          "form": "아 맞아요 신발은 특히 더 그렇죠",
          "original_form": "아 맞아요 신발은 특히 더 그렇죠",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029649.1.15",
          "form": "혹시 커블써보셨나요",
          "original_form": "혹시 커블써보셨나요",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029649.1.16",
          "form": "애들 옷이랑 신발은 더 사이즈가 중요할 것 같아요",
          "original_form": "애들 옷이랑 신발은 더 사이즈가 중요할 것 같아요",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029649.1.17",
          "form": "커블체어요??",
          "original_form": "커블체어요??",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029649.1.18",
          "form": "네^ ^ 그거용!ㅋㅋ",
          "original_form": "네^ ^ 그거용!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029649.1.19",
          "form": "커블은 아니고 비슷한 것만 써봤어요",
          "original_form": "커블은 아니고 비슷한 것만 써봤어요",
          "speaker_id": "1",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029649.1.20",
          "form": "지금 그게 딜떴는데",
          "original_form": "지금 그게 딜떴는데",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029649.1.21",
          "form": "사볼까말까 고민되서요 ㅋ",
          "original_form": "사볼까말까 고민되서요 ㅋ",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029649.1.22",
          "form": "저희아이들이 쌍둥이거든요",
          "original_form": "저희아이들이 쌍둥이거든요",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029649.1.23",
          "form": "아이들용으로 키즈사놨는데",
          "original_form": "아이들용으로 키즈사놨는데",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029649.1.24",
          "form": "급 제것도 사고싶어져서ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "급 제것도 사고싶어져서ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029649.1.25",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029649.1.26",
          "form": "한번 질러보시는 것도...!",
          "original_form": "한번 질러보시는 것도...!",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029649.1.27",
          "form": "별로면 당근 어떠세요??",
          "original_form": "별로면 당근 어떠세요??",
          "speaker_id": "1",
          "time": "20210903 11:25"
        },
        {
          "id": "MDRW2100029649.1.28",
          "form": "ㅋㅋ오 왜 전그생각을 못했죠?ㅋㅋㅋ",
          "original_form": "ㅋㅋ오 왜 전그생각을 못했죠?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029649.1.29",
          "form": "일단 구매해보겠습니닼ㅋㅋ",
          "original_form": "일단 구매해보겠습니닼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029649.1.30",
