      "utterance": [
        {
          "id": "MDRW2100044002.1.1",
          "form": "날씨얘기했었습니닿",
          "original_form": "날씨얘기했었습니닿",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100044002.1.2",
          "form": "그럼 후드티가 많으신가용",
          "original_form": "그럼 후드티가 많으신가용",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044002.1.3",
          "form": "후드티가 대부분이죠ㅎㅎ",
          "original_form": "후드티가 대부분이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100044002.1.4",
          "form": "따로 사고싶은 옷은 요즘 있으신가요?",
          "original_form": "따로 사고싶은 옷은 요즘 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044002.1.5",
          "form": "슬슬 가을이니 코트하나 사고싶네요ㅎㅎ",
          "original_form": "슬슬 가을이니 코트하나 사고싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044002.1.6",
          "form": "오 트렌치코트요?",
          "original_form": "오 트렌치코트요?",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044002.1.7",
          "form": "트렌치코트는 키가좀 작아서 도전할 용기가 안생기더라구요 ㅠㅜ",
          "original_form": "트렌치코트는 키가좀 작아서 도전할 용기가 안생기더라구요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044002.1.8",
          "form": "그럼 짧은코트보시는건가용",
          "original_form": "그럼 짧은코트보시는건가용",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044002.1.9",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044002.1.10",
          "form": "저도 저번에 역시즌방송에서 양털반코트하나 샀는데",
          "original_form": "저도 저번에 역시즌방송에서 양털반코트하나 샀는데",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044002.1.11",
          "form": "짧은게 또 이뻐보이긴해요!",
          "original_form": "짧은게 또 이뻐보이긴해요!",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044002.1.12",
          "form": "그쵸그쵸ㅠ",
          "original_form": "그쵸그쵸ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044002.1.13",
          "form": "요즘엔 또 짧은게 이뻐보여서 하나 장만하고싶네요",
          "original_form": "요즘엔 또 짧은게 이뻐보여서 하나 장만하고싶네요",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044002.1.14",
          "form": "색깔은 어떤걸ㄷ 보시나요",
          "original_form": "색깔은 어떤걸ㄷ 보시나요",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044002.1.15",
          "form": "좀 무난한 갈색~검정계열 좋아해요",
          "original_form": "좀 무난한 갈색~검정계열 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044002.1.16",
          "form": "밝은건 좀 안어울리더라구요ㅎㅎ",
          "original_form": "밝은건 좀 안어울리더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044002.1.17",
          "form": "곧 가을겨울이라 갈색이나 검정계열이 딱 맞는 색깔이죠!",
          "original_form": "곧 가을겨울이라 갈색이나 검정계열이 딱 맞는 색깔이죠!",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044002.1.18",
          "form": "그쵸 그쵸ㅎㅎ",
          "original_form": "그쵸 그쵸ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044002.1.19",
          "form": "저는...그냥 색깔이 좋아서 사긴샀는데...좀 많이 튀는거같아요ㅜㅜ 핑크라...",
          "original_form": "저는...그냥 색깔이 좋아서 사긴샀는데...좀 많이 튀는거같아요ㅜㅜ 핑크라...",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044002.1.20",
          "form": "핑크도 잘입으면 진짜 예쁘긴하더라구요ㅎㅎ",
          "original_form": "핑크도 잘입으면 진짜 예쁘긴하더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044002.1.21",
          "form": "조금 튀긴하지만 그래도!",
          "original_form": "조금 튀긴하지만 그래도!",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044002.1.22",
          "form": "처음 핑크 도전해봐요ㅜ",
          "original_form": "처음 핑크 도전해봐요ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044002.1.23",
          "form": "헉 그렇군요!",
          "original_form": "헉 그렇군요!",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044002.1.24",
          "form": "잘 어울리실거예요~",
          "original_form": "잘 어울리실거예요~",
          "speaker_id": "2",
          "time": "20210907 22:25"
        }
      ]
    }
  ]
