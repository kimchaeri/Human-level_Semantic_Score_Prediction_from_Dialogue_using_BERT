        },
        {
          "id": "MDRW2100047186.1.3",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100047186.1.4",
          "form": "공연~따로 본 적은 없어요",
          "original_form": "공연~따로 본 적은 없어요",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100047186.1.5",
          "form": "공연 본거 있으세요?",
          "original_form": "공연 본거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100047186.1.6",
          "form": "콘서트 가봤어요",
          "original_form": "콘서트 가봤어요",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100047186.1.7",
          "form": "오 어떤 콘서트요?",
          "original_form": "오 어떤 콘서트요?",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100047186.1.8",
          "form": "아이유요",
          "original_form": "아이유요",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100047186.1.9",
          "form": "와 아이유 콘서트 진짜 가고싶었는데",
          "original_form": "와 아이유 콘서트 진짜 가고싶었는데",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100047186.1.10",
          "form": "한번 가보세요",
          "original_form": "한번 가보세요",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100047186.1.11",
          "form": "코로나 끝나면 가봐야겠어요",
          "original_form": "코로나 끝나면 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100047186.1.12",
          "form": "전시회 가보셨나요",
          "original_form": "전시회 가보셨나요",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100047186.1.13",
          "form": "그럼요 최근엔 요시고 사진 전시회 다녀왔어요",
          "original_form": "그럼요 최근엔 요시고 사진 전시회 다녀왔어요",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100047186.1.14",
          "form": "다녀와보신곳 있으세요?",
          "original_form": "다녀와보신곳 있으세요?",
          "speaker_id": "2",
          "time": "20210826 00:23"
        },
        {
          "id": "MDRW2100047186.1.15",
          "form": "모마 전시장에서",
