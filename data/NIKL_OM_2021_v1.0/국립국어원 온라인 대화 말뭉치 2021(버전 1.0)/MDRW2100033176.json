      "utterance": [
        {
          "id": "MDRW2100033176.1.1",
          "form": "최근에 뭐 산거 있어??",
          "original_form": "최근에 뭐 산거 있어??",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100033176.1.2",
          "form": "하이루",
          "original_form": "하이루",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100033176.1.3",
          "form": "나 올리브영에서 부승관템 샀늠",
          "original_form": "나 올리브영에서 부승관템 샀늠",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100033176.1.4",
          "form": "헐 그 수분크림??",
          "original_form": "헐 그 수분크림??",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100033176.1.5",
          "form": "나는 그린민트 샀는데",
          "original_form": "나는 그린민트 샀는데",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100033176.1.6",
          "form": "그 토너랑 수분크림 샀는딩",
          "original_form": "그 토너랑 수분크림 샀는딩",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100033176.1.7",
          "form": "아 마자마자",
          "original_form": "아 마자마자",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100033176.1.8",
          "form": "수분크림은 사야하고",
          "original_form": "수분크림은 사야하고",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100033176.1.9",
          "form": "폼클렌징 없어서 그거 먼저 샀음",
          "original_form": "폼클렌징 없어서 그거 먼저 샀음",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100033176.1.10",
          "form": "아항 써보니까 어때?",
          "original_form": "아항 써보니까 어때?",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100033176.1.11",
          "form": "음.... 아직 나쁘지않은것같긴한데",
          "original_form": "음.... 아직 나쁘지않은것같긴한데",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100033176.1.12",
          "form": "일단 효과 보려면 일주일 정도는 더 써봐야할듯...",
          "original_form": "일단 효과 보려면 일주일 정도는 더 써봐야할듯...",
          "speaker_id": "1",
          "time": "20210906 18:07"
        },
        {
          "id": "MDRW2100033176.1.13",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033176.1.14",
          "form": "나중에 후기 알려줘",
          "original_form": "나중에 후기 알려줘",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033176.1.15",
          "form": "오키오키",
          "original_form": "오키오키",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033176.1.16",
          "form": "그거 말고 사고싶은건 없어?",
          "original_form": "그거 말고 사고싶은건 없어?",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033176.1.17",
          "form": "음..... 파데랑 립스틱 정도?",
          "original_form": "음..... 파데랑 립스틱 정도?",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033176.1.18",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033176.1.19",
          "form": "나는 잠옷ㅠㅠ",
          "original_form": "나는 잠옷ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033176.1.20",
          "form": "아 트윗 봤던거같아",
          "original_form": "아 트윗 봤던거같아",
          "speaker_id": "1",
          "time": "20210906 18:08"
        },
        {
          "id": "MDRW2100033176.1.21",
          "form": "원피스 잠옷인데 완전 귀여워",
          "original_form": "원피스 잠옷인데 완전 귀여워",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100033176.1.22",
          "form": "그 솜 원피스?같은",
          "original_form": "그 솜 원피스?같은",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100033176.1.23",
          "form": "맞아!!",
          "original_form": "맞아!!",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100033176.1.24",
          "form": "월급 받으면 바로 살거야",
          "original_form": "월급 받으면 바로 살거야",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100033176.1.25",
          "form": "그거 예쁘더랑",
          "original_form": "그거 예쁘더랑",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100033176.1.26",
          "form": "좋은 구매일듯",
          "original_form": "좋은 구매일듯",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100033176.1.27",
          "form": "그치!! 나중에 사면 보여줄게",
          "original_form": "그치!! 나중에 사면 보여줄게",
          "speaker_id": "2",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100033176.1.28",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100033176.1.29",
          "form": "꼭 보여줘",
          "original_form": "꼭 보여줘",
          "speaker_id": "1",
          "time": "20210906 18:10"
        }
      ]
    }
