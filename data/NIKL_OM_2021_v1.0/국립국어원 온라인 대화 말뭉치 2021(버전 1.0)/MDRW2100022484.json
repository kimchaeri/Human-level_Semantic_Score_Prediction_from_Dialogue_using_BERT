        },
        {
          "id": "MDRW2100022484.1.2",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100022484.1.3",
          "form": "1교시 짱 많아...",
          "original_form": "1교시 짱 많아...",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100022484.1.4",
          "form": "1교시면 몇시에 일어나야해?",
          "original_form": "1교시면 몇시에 일어나야해?",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100022484.1.5",
          "form": "8시 정도에 일어나서 씻고 옷 갈아잎어야한다유",
          "original_form": "8시 정도에 일어나서 씻고 옷 갈아잎어야한다유",
          "speaker_id": "2",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100022484.1.6",
          "form": "헐 기숙사지?",
          "original_form": "헐 기숙사지?",
          "speaker_id": "1",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100022484.1.7",
          "form": "나 긱사 떨어졌어ㅠ",
          "original_form": "나 긱사 떨어졌어ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100022484.1.8",
          "form": "아 그럼 자취하는구나?",
          "original_form": "아 그럼 자취하는구나?",
          "speaker_id": "1",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100022484.1.9",
          "form": "통학해..",
          "original_form": "통학해..",
          "speaker_id": "2",
          "time": "20210831 00:19"
        },
        {
          "id": "MDRW2100022484.1.10",
          "form": "엥 정말?",
          "original_form": "엥 정말?",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100022484.1.11",
          "form": "아 학교가 가깝구나?",
          "original_form": "아 학교가 가깝구나?",
          "speaker_id": "1",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100022484.1.12",
          "form": "아니 난 서울 살고 학교는 천안이라서 짱 멀다!",
          "original_form": "아니 난 서울 살고 학교는 천안이라서 짱 멀다!",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100022484.1.13",
          "form": "헐 근데 1교시라고..?",
          "original_form": "헐 근데 1교시라고..?",
          "speaker_id": "1",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100022484.1.14",
          "form": "그럼 몇시에 일어나야해?",
          "original_form": "그럼 몇시에 일어나야해?",
          "speaker_id": "1",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100022484.1.15",
          "form": "흠 6시?",
          "original_form": "흠 6시?",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100022484.1.16",
          "form": "헐 일어날 수 있어?",
          "original_form": "헐 일어날 수 있어?",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100022484.1.17",
          "form": "그래서 맨날 통학버스에서 침흘리고 자ㅋㅋㅋ",
          "original_form": "그래서 맨날 통학버스에서 침흘리고 자ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100022484.1.18",
          "form": "으악 더러워",
          "original_form": "으악 더러워",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100022484.1.19",
          "form": "뭐????",
          "original_form": "뭐????",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100022484.1.20",
          "form": "그럼 집에는 몇시에 도착해?",
          "original_form": "그럼 집에는 몇시에 도착해?",
          "speaker_id": "1",
