      "utterance": [
        {
          "id": "MDRW2100023523.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 10:57"
        },
        {
          "id": "MDRW2100023523.1.2",
          "form": "네 좋아합니다 선선해지면 경주가고싶어요",
          "original_form": "네 좋아합니다 선선해지면 경주가고싶어요",
          "speaker_id": "2",
          "time": "20210830 12:21"
        },
        {
          "id": "MDRW2100023523.1.3",
          "form": "어디 여행 가보셨나요?",
          "original_form": "어디 여행 가보셨나요?",
          "speaker_id": "1",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023523.1.4",
          "form": "경주 좋아요 예전에 갔는데 고즈넉하니 좋았어요",
          "original_form": "경주 좋아요 예전에 갔는데 고즈넉하니 좋았어요",
          "speaker_id": "1",
          "time": "20210830 13:46"
        },
        {
          "id": "MDRW2100023523.1.5",
          "form": "저는 월정교랑 동궁과월지? 어디가보셨어요",
          "original_form": "저는 월정교랑 동궁과월지? 어디가보셨어요",
          "speaker_id": "2",
          "time": "20210830 17:25"
        },
        {
          "id": "MDRW2100023523.1.6",
          "form": "20대 초반에 친구들이랑 갔는데 기억은 안나요 ㅋㅋㅋㅋㅋ",
          "original_form": "20대 초반에 친구들이랑 갔는데 기억은 안나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100023523.1.7",
          "form": "야간투어 해주는 게스트하우스가 있더라구요",
          "original_form": "야간투어 해주는 게스트하우스가 있더라구요",
          "speaker_id": "2",
          "time": "20210831 06:35"
        },
        {
          "id": "MDRW2100023523.1.8",
          "form": "오 재밌겠다 야간투어때 뭐 했어요?",
          "original_form": "오 재밌겠다 야간투어때 뭐 했어요?",
          "speaker_id": "1",
          "time": "20210831 10:04"
        },
        {
          "id": "MDRW2100023523.1.9",
          "form": "안압지 둘러보고 월정교 사진 촬영 같은거했어요",
          "original_form": "안압지 둘러보고 월정교 사진 촬영 같은거했어요",
          "speaker_id": "2",
          "time": "20210831 11:01"
        },
        {
          "id": "MDRW2100023523.1.10",
          "form": "야간밤산책이랑",
          "original_form": "야간밤산책이랑",
          "speaker_id": "2",
          "time": "20210831 11:01"
        },
        {
          "id": "MDRW2100023523.1.11",
          "form": "안압지는 가본것 같아요 연못? 엄청 큰거요",
          "original_form": "안압지는 가본것 같아요 연못? 엄청 큰거요",
          "speaker_id": "1",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100023523.1.12",
          "form": "되게 재밌게 가셨네요!",
          "original_form": "되게 재밌게 가셨네요!",
          "speaker_id": "1",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100023523.1.13",
          "form": "맞아요 큰 연못 맞아요",
          "original_form": "맞아요 큰 연못 맞아요",
          "speaker_id": "2",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100023523.1.14",
          "form": "거기 밤에 보면 이쁘던데 전 일정상 낮에 본것 같아요",
          "original_form": "거기 밤에 보면 이쁘던데 전 일정상 낮에 본것 같아요",
          "speaker_id": "1",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100023523.1.15",
          "form": "오 낮도 이쁘죠",
          "original_form": "오 낮도 이쁘죠",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100023523.1.16",
          "form": "경주는 낮과밤 다 좋아요",
          "original_form": "경주는 낮과밤 다 좋아요",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100023523.1.17",
          "form": "가고픈 곳 있으세요",
          "original_form": "가고픈 곳 있으세요",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100023523.1.18",
          "form": "전 강릉이나 속초 가고 싶어요",
          "original_form": "전 강릉이나 속초 가고 싶어요",
          "speaker_id": "1",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100023523.1.19",
          "form": "강원쪽은 안가봐서요",
          "original_form": "강원쪽은 안가봐서요",
          "speaker_id": "1",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100023523.1.20",
          "form": "저듀요 넘 멀어서 못가봤어요",
          "original_form": "저듀요 넘 멀어서 못가봤어요",
          "speaker_id": "2",
          "time": "20210831 17:35"
        }
      ]
