      "utterance": [
        {
          "id": "MDRW2100012952.1.1",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "2",
          "time": "20210825 09:27"
        },
        {
          "id": "MDRW2100012952.1.2",
          "form": "아직 키우지는 않는데 준비되면 키우고 싶어요!",
          "original_form": "아직 키우지는 않는데 준비되면 키우고 싶어요!",
          "speaker_id": "1",
          "time": "20210825 09:29"
        },
        {
          "id": "MDRW2100012952.1.3",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210825 09:29"
        },
        {
          "id": "MDRW2100012952.1.4",
          "form": "아니요ㅠㅠ 신혼이라",
          "original_form": "아니요ㅠㅠ 신혼이라",
          "speaker_id": "2",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012952.1.5",
          "form": "아 맞다 ㅎㅎ",
          "original_form": "아 맞다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012952.1.6",
          "form": "얼마전에 친구가 강아지를 데려왔는데",
          "original_form": "얼마전에 친구가 강아지를 데려왔는데",
          "speaker_id": "1",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012952.1.7",
          "form": "너무 귀엽더라고요",
          "original_form": "너무 귀엽더라고요",
          "speaker_id": "1",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012952.1.8",
          "form": "강아지파세요 고양이파세요?",
          "original_form": "강아지파세요 고양이파세요?",
          "speaker_id": "1",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012952.1.9",
          "form": "전 강아지요",
          "original_form": "전 강아지요",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012952.1.10",
          "form": "고양이는 우리를",
          "original_form": "고양이는 우리를",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012952.1.11",
          "form": "밑으로 보는거 같아서",
          "original_form": "밑으로 보는거 같아서",
          "speaker_id": "2",
          "time": "20210825 09:31"
        },
        {
          "id": "MDRW2100012952.1.12",
          "form": "근데 어렸을때는 고양이 키우는 사람 보기 힘들었는데",
          "original_form": "근데 어렸을때는 고양이 키우는 사람 보기 힘들었는데",
          "speaker_id": "1",
          "time": "20210825 09:39"
        },
        {
          "id": "MDRW2100012952.1.13",
          "form": "요새는 진짜 많은거 같더라구요",
          "original_form": "요새는 진짜 많은거 같더라구요",
          "speaker_id": "1",
          "time": "20210825 09:39"
        },
        {
          "id": "MDRW2100012952.1.14",
          "form": "음 그러게요",
          "original_form": "음 그러게요",
          "speaker_id": "2",
          "time": "20210825 09:52"
        },
        {
          "id": "MDRW2100012952.1.15",
          "form": "방송에 노출도 많이되고",
          "original_form": "방송에 노출도 많이되고",
          "speaker_id": "1",
          "time": "20210825 10:15"
        },
        {
          "id": "MDRW2100012952.1.16",
          "form": "신기해요 뭔가 친근해진 느낌이에요",
          "original_form": "신기해요 뭔가 친근해진 느낌이에요",
          "speaker_id": "1",
          "time": "20210825 10:15"
        },
        {
          "id": "MDRW2100012952.1.17",
          "form": "맞아요 개는 뭐 시골에 많았는데",
          "original_form": "맞아요 개는 뭐 시골에 많았는데",
          "speaker_id": "2",
          "time": "20210825 11:37"
        },
        {
          "id": "MDRW2100012952.1.18",
          "form": "맞아요 고양이는 진짜 드물었는데",
          "original_form": "맞아요 고양이는 진짜 드물었는데",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100012952.1.19",
          "form": "친구는 고양이 사람을 밑으로 보는 그 성격이 좋더고하더러고요 ㅋㅋ",
          "original_form": "친구는 고양이 사람을 밑으로 보는 그 성격이 좋더고하더러고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100012952.1.20",
          "form": "ㅋㅋ전 하인되기 싫어서",
          "original_form": "ㅋㅋ전 하인되기 싫어서",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100012952.1.21",
          "form": "진짜 호불호가 딱 갈리는거 같아요",
          "original_form": "진짜 호불호가 딱 갈리는거 같아요",
          "speaker_id": "1",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100012952.1.22",
          "form": "애기 키워야죠 ㅎㅎ",
          "original_form": "애기 키워야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        }
      ]
