      "utterance": [
        {
          "id": "MDRW2100022234.1.1",
          "form": "즐겨보는 드라마있으신가요",
          "original_form": "즐겨보는 드라마있으신가요",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022234.1.2",
          "form": "아뇨 저는 챙겨보는거 귀찮아서 드라마는 잘 안 보는편이에요",
          "original_form": "아뇨 저는 챙겨보는거 귀찮아서 드라마는 잘 안 보는편이에요",
          "speaker_id": "2",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022234.1.3",
          "form": "다 끝나고 몰아보는걸 선호해요",
          "original_form": "다 끝나고 몰아보는걸 선호해요",
          "speaker_id": "2",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022234.1.4",
          "form": "name1님은 요즘 보는 드라마 있으신가요??",
          "original_form": "&name1&님은 요즘 보는 드라마 있으신가요??",
          "speaker_id": "2",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022234.1.5",
          "form": "아..그럼 예능은요?",
          "original_form": "아..그럼 예능은요?",
          "speaker_id": "1",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022234.1.6",
          "form": "저 요즘 펜트하우스",
          "original_form": "저 요즘 펜트하우스",
          "speaker_id": "1",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022234.1.7",
          "form": "보고있어요",
          "original_form": "보고있어요",
          "speaker_id": "1",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022234.1.8",
          "form": "들어보셨나요",
          "original_form": "들어보셨나요",
          "speaker_id": "1",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022234.1.9",
          "form": "아아 네 완전 인기많지않나요??",
          "original_form": "아아 네 완전 인기많지않나요??",
          "speaker_id": "2",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022234.1.10",
          "form": "저도 유튜브로 잘린거 보는데",
          "original_form": "저도 유튜브로 잘린거 보는데",
          "speaker_id": "2",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022234.1.11",
          "form": "재밌더라구요",
          "original_form": "재밌더라구요",
          "speaker_id": "2",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022234.1.12",
          "form": "네 아무래도 주위에서 많이 보니까 저도 보게되는게 있어요",
          "original_form": "네 아무래도 주위에서 많이 보니까 저도 보게되는게 있어요",
          "speaker_id": "1",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022234.1.13",
          "form": "제 주변에도 보는 친구들 많더라구요",
          "original_form": "제 주변에도 보는 친구들 많더라구요",
          "speaker_id": "2",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022234.1.14",
          "form": "내용이 완전 막장이던데요??ㅋㅋㅋㅋㅋㅋ",
          "original_form": "내용이 완전 막장이던데요??ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022234.1.15",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022234.1.16",
          "form": "이래도되나싶을정도로",
          "original_form": "이래도되나싶을정도로",
          "speaker_id": "1",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022234.1.17",
          "form": "죽었다 살았다 난리더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "죽었다 살았다 난리더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022234.1.18",
          "form": "막장이라ㅋㅋ 피로감이느껴지더라구요",
          "original_form": "막장이라ㅋㅋ 피로감이느껴지더라구요",
          "speaker_id": "1",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100022234.1.19",
          "form": "그건그래요ㅋㅋ 정확히 보셨습니다",
          "original_form": "그건그래요ㅋㅋ 정확히 보셨습니다",
          "speaker_id": "1",
          "time": "20210829 11:40"
        },
        {
          "id": "MDRW2100022234.1.20",
          "form": "마지막에 보신 드라마는 무엇인강ᆢ",
          "original_form": "마지막에 보신 드라마는 무엇인강ᆢ",
          "speaker_id": "1",
          "time": "20210829 11:41"
        },
        {
          "id": "MDRW2100022234.1.21",
          "form": "스카이캐슬인거같아요",
          "original_form": "스카이캐슬인거같아요",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022234.1.22",
          "form": "저는 티비는 잘 안 보고 폰을 더 많이 해서",
          "original_form": "저는 티비는 잘 안 보고 폰을 더 많이 해서",
          "speaker_id": "2",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022234.1.23",
          "form": "스카이캐슬 전국민이 난리였죠",
          "original_form": "스카이캐슬 전국민이 난리였죠",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022234.1.24",
          "form": "드라마 본지가 오래 됐네요ㅋㅋㅋㅋ",
          "original_form": "드라마 본지가 오래 됐네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022234.1.25",
          "form": "어머님 전적으로 저를 믿으셔야합니다",
