      "utterance": [
        {
          "id": "MDRW2100013870.1.1",
          "form": "요즘 비가자주와요",
          "original_form": "요즘 비가자주와요",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013870.1.2",
          "form": "name1 지역도 비가오고있나요",
          "original_form": "&name1& 지역도 비가오고있나요",
          "speaker_id": "2",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013870.1.3",
          "form": "지금은 엄청 맑아요ㅎㅎ",
          "original_form": "지금은 엄청 맑아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013870.1.4",
          "form": "어제는 조금씩 왔는데",
          "original_form": "어제는 조금씩 왔는데",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013870.1.5",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013870.1.6",
          "form": "어제 진짜 하늘에 구멍뚫린 줄",
          "original_form": "어제 진짜 하늘에 구멍뚫린 줄",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013870.1.7",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013870.1.8",
          "form": "장마에 태풍까지 겹쳐서 더 그런가봐요",
          "original_form": "장마에 태풍까지 겹쳐서 더 그런가봐요",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013870.1.9",
          "form": "저희쪽은 그냥 평범하게 내렸는데",
          "original_form": "저희쪽은 그냥 평범하게 내렸는데",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013870.1.10",
          "form": "북서쪽 지역이라 태풍이 빗겨갔거든요",
          "original_form": "북서쪽 지역이라 태풍이 빗겨갔거든요",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013870.1.11",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013870.1.12",
          "form": "저는 관통한듯해요",
          "original_form": "저는 관통한듯해요",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013870.1.13",
          "form": "아직도 비가오거든요",
          "original_form": "아직도 비가오거든요",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013870.1.14",
          "form": "아하 ㅠㅠㅠ 큰 피해는 없으셨죠ㅠ?",
          "original_form": "아하 ㅠㅠㅠ 큰 피해는 없으셨죠ㅠ?",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013870.1.15",
          "form": "저는 피해본거 없긴한데",
          "original_form": "저는 피해본거 없긴한데",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013870.1.16",
          "form": "아무래도 농민들이..",
          "original_form": "아무래도 농민들이..",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013870.1.17",
          "form": "ㅠㅠㅠㅠ 그렇긴 하죠... 밑에지방은 완전 난리났다던데",
          "original_form": "ㅠㅠㅠㅠ 그렇긴 하죠... 밑에지방은 완전 난리났다던데",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013870.1.18",
          "form": "그러니까요ㅠ",
          "original_form": "그러니까요ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013870.1.19",
          "form": "비오는 날 어떠세요",
          "original_form": "비오는 날 어떠세요",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013870.1.20",
          "form": "저는 대부분 집안에 있으니까 괜찮은데",
          "original_form": "저는 대부분 집안에 있으니까 괜찮은데",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013870.1.21",
          "form": "가끔 비오는데 나가야되면..",
          "original_form": "가끔 비오는데 나가야되면..",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013870.1.22",
          "form": "최악이죠ㅋ",
          "original_form": "최악이죠ㅋ",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013870.1.23",
          "form": "끔찍..",
          "original_form": "끔찍..",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013870.1.24",
          "form": "우산써도 젖고",
          "original_form": "우산써도 젖고",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013870.1.25",
          "form": "축축하고..",
          "original_form": "축축하고..",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013870.1.26",
