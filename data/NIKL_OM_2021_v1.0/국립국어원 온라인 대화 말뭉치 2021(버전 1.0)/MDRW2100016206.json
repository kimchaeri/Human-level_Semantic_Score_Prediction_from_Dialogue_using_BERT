      "utterance": [
        {
          "id": "MDRW2100016206.1.1",
          "form": "작은 아들 공기 연습하러 나왔는데 하나도 못하네 우짜나 ㅋㅋㅋ",
          "original_form": "작은 아들 공기 연습하러 나왔는데 하나도 못하네 우짜나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016206.1.2",
          "form": "이런이런 평소에 할 일이 하나도 없으니",
          "original_form": "이런이런 평소에 할 일이 하나도 없으니",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016206.1.3",
          "form": "공기 연습하다가 안잡히니 울상이야",
          "original_form": "공기 연습하다가 안잡히니 울상이야",
          "speaker_id": "1",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100016206.1.4",
          "form": "ㅎㅎ 그냥 즐기라고 해",
          "original_form": "ㅎㅎ 그냥 즐기라고 해",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016206.1.5",
          "form": "곧 공기 시합 한다는데 내가 괜히 긴장되네",
          "original_form": "곧 공기 시합 한다는데 내가 괜히 긴장되네",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016206.1.6",
          "form": "하나도 못하는데 긴장할 게 있나 ㅋㅋㅋ",
          "original_form": "하나도 못하는데 긴장할 게 있나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016206.1.7",
          "form": "ㅋㅋㅋ 내가 괜히 얼굴이 화끈거려서",
          "original_form": "ㅋㅋㅋ 내가 괜히 얼굴이 화끈거려서",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016206.1.8",
          "form": "작은 아들은 쿨해서 그런거 신경안써",
          "original_form": "작은 아들은 쿨해서 그런거 신경안써",
          "speaker_id": "2",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016206.1.9",
          "form": "ㅎㅎ 평소에 연습 좀 해야하나",
          "original_form": "ㅎㅎ 평소에 연습 좀 해야하나",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016206.1.10",
          "form": "할 일도 많은데 뭘 공기까지 ㅋㅋ",
          "original_form": "할 일도 많은데 뭘 공기까지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016206.1.11",
          "form": "못하지만 재미있어 하더라고",
          "original_form": "못하지만 재미있어 하더라고",
          "speaker_id": "1",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016206.1.12",
          "form": "방에서 할만한 공간이 없을텐데",
          "original_form": "방에서 할만한 공간이 없을텐데",
          "speaker_id": "2",
          "time": "20210826 11:06"
        },
        {
          "id": "MDRW2100016206.1.13",
          "form": "침대 위에서 하라고는 했는데 힘들듯",
          "original_form": "침대 위에서 하라고는 했는데 힘들듯",
          "speaker_id": "1",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016206.1.14",
          "form": "ㅎㅎ 잘하는 애들이 하면 되지 뭐",
          "original_form": "ㅎㅎ 잘하는 애들이 하면 되지 뭐",
          "speaker_id": "2",
          "time": "20210826 11:07"
        },
        {
          "id": "MDRW2100016206.1.15",
          "form": "ㅇㅇ 울 아들은 이번 시간에 할 게 없겠어 ㅋㅋㅋ",
