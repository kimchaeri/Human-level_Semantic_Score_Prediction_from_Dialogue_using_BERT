      "utterance": [
        {
          "id": "MDRW2100015056.1.1",
          "form": "2021년 배우들 출연료 순위라고 있다",
          "original_form": "2021년 배우들 출연료 순위라고 있다",
          "speaker_id": "2",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015056.1.2",
          "form": "우리나라 순위가?",
          "original_form": "우리나라 순위가?",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015056.1.3",
          "form": "17위 로버트 패터슨",
          "original_form": "17위 로버트 패터슨",
          "speaker_id": "2",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015056.1.4",
          "form": "로다주도 있지 않나?",
          "original_form": "로다주도 있지 않나?",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015056.1.5",
          "form": "300만달러라는데 우리나라 돈으로 한 40억이가",
          "original_form": "300만달러라는데 우리나라 돈으로 한 40억이가",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015056.1.6",
          "form": "외국배우",
          "original_form": "외국배우",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015056.1.7",
          "form": "맞다 40 억 정도다. 17위도 40 억?",
          "original_form": "맞다 40 억 정도다. 17위도 40 억?",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015056.1.8",
          "form": "배트맨 시리즈를 찍었나보다 내년에 개봉하네",
          "original_form": "배트맨 시리즈를 찍었나보다 내년에 개봉하네",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015056.1.9",
          "form": "아 좀 안 어울린다ㅋ",
          "original_form": "아 좀 안 어울린다ㅋ",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015056.1.10",
          "form": "16위 르리스 파인",
          "original_form": "16위 르리스 파인",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015056.1.11",
          "form": "크리스 파인",
          "original_form": "크리스 파인",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015056.1.12",
          "form": "누군지 모르겠다. 어디 나왔어?",
          "original_form": "누군지 모르겠다. 어디 나왔어?",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015056.1.13",
          "form": "던전 앤 드래곤이라고 판타지물을 찍었네",
          "original_form": "던전 앤 드래곤이라고 판타지물을 찍었네",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015056.1.14",
          "form": "영화와 배우를 전혀 모르겠다ㅋㅋ",
          "original_form": "영화와 배우를 전혀 모르겠다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015056.1.15",
          "form": "아~원더우면 찍었네 저번에 얘기했던거",
          "original_form": "아~원더우면 찍었네 저번에 얘기했던거",
