      "utterance": [
        {
          "id": "MDRW2100042772.1.1",
          "form": "반려동물 키워본 경험 있으세요?",
          "original_form": "반려동물 키워본 경험 있으세요?",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100042772.1.2",
          "form": "네 지금 강아지 키워여ㅎ",
          "original_form": "네 지금 강아지 키워여ㅎ",
          "speaker_id": "2",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100042772.1.3",
          "form": "강아지 너무 귀엽겠어요",
          "original_form": "강아지 너무 귀엽겠어요",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100042772.1.4",
          "form": "저도 예전에 키웠었는데",
          "original_form": "저도 예전에 키웠었는데",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100042772.1.5",
          "form": "말티즈였어요",
          "original_form": "말티즈였어요",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100042772.1.6",
          "form": "또자엄마는 어느 종을 키우시나요?",
          "original_form": "또자엄마는 어느 종을 키우시나요?",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100042772.1.7",
          "form": "저는 비숑이여ㅎ",
          "original_form": "저는 비숑이여ㅎ",
          "speaker_id": "2",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100042772.1.8",
          "form": "털뭉치",
          "original_form": "털뭉치",
          "speaker_id": "2",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100042772.1.9",
          "form": "비숑 요즘 인기종이잖아요",
          "original_form": "비숑 요즘 인기종이잖아요",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100042772.1.10",
          "form": "엄청 귀엽구요",
          "original_form": "엄청 귀엽구요",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100042772.1.11",
          "form": "이름은 뭔가요?",
          "original_form": "이름은 뭔가요?",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100042772.1.12",
          "form": "이름이 털뭉치인가요?",
          "original_form": "이름이 털뭉치인가요?",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
          "id": "MDRW2100042772.1.13",
          "form": "이름은 뽕이",
          "original_form": "이름은 뽕이",
          "speaker_id": "2",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100042772.1.14",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100042772.1.15",
          "form": "아 이름은 뽕이군요",
          "original_form": "아 이름은 뽕이군요",
          "speaker_id": "1",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100042772.1.16",
          "form": "이름도 귀엽네요",
          "original_form": "이름도 귀엽네요",
          "speaker_id": "1",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100042772.1.17",
          "form": "몇살이에요?",
          "original_form": "몇살이에요?",
          "speaker_id": "1",
          "time": "20210907 15:01"
        },
        {
          "id": "MDRW2100042772.1.18",
          "form": "1살이여",
          "original_form": "1살이여",
          "speaker_id": "2",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100042772.1.19",
          "form": "귀여워요ㅎ",
          "original_form": "귀여워요ㅎ",
          "speaker_id": "2",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100042772.1.20",
          "form": "진짜 귀여울 때네요",
          "original_form": "진짜 귀여울 때네요",
          "speaker_id": "1",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100042772.1.21",
          "form": "가족들이 다들 좋아하겠어요",
          "original_form": "가족들이 다들 좋아하겠어요",
          "speaker_id": "1",
          "time": "20210907 15:06"
        },
        {
          "id": "MDRW2100042772.1.22",
          "form": "젤 사랑받는것같아요ㅎ",
          "original_form": "젤 사랑받는것같아요ㅎ",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100042772.1.23",
          "form": "강아지가 그렇죠. 애교도 많고",
          "original_form": "강아지가 그렇죠. 애교도 많고",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100042772.1.24",
          "form": "저도 말티즈 키웠을 때 가족들이 걔만 처다보느라 바빴죠",
          "original_form": "저도 말티즈 키웠을 때 가족들이 걔만 처다보느라 바빴죠",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100042772.1.25",
          "form": "ㅎㅎ 그렇게되더라구요",
          "original_form": "ㅎㅎ 그렇게되더라구요",
          "speaker_id": "2",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100042772.1.26",
          "form": "약간 아기 같죠 집안에 아기 있으면 온 시구가 아기 처다보고 있고",
          "original_form": "약간 아기 같죠 집안에 아기 있으면 온 시구가 아기 처다보고 있고",
          "speaker_id": "1",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100042772.1.27",
          "form": "아기 재롱 부리면 좋아하고",
          "original_form": "아기 재롱 부리면 좋아하고",
          "speaker_id": "1",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100042772.1.28",
          "form": "손은 많이 가고 ㅎㅎ",
          "original_form": "손은 많이 가고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:54"
        }
      ]
    }
  ]
}
