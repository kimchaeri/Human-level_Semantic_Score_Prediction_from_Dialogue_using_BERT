      "utterance": [
        {
          "id": "MDRW2100027074.1.1",
          "form": "전주에서 한옥마을가고",
          "original_form": "전주에서 한옥마을가고",
          "speaker_id": "2",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100027074.1.2",
          "form": "성당??",
          "original_form": "성당??",
          "speaker_id": "2",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100027074.1.3",
          "form": "한복입고 놀았죠머..",
          "original_form": "한복입고 놀았죠머..",
          "speaker_id": "2",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100027074.1.4",
          "form": "아 한복입고",
          "original_form": "아 한복입고",
          "speaker_id": "1",
          "time": "20210902 09:03"
        },
        {
          "id": "MDRW2100027074.1.5",
          "form": "사진 많이",
          "original_form": "사진 많이",
          "speaker_id": "1",
          "time": "20210902 09:03"
        },
        {
          "id": "MDRW2100027074.1.6",
          "form": "찍으셧나유",
          "original_form": "찍으셧나유",
          "speaker_id": "1",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100027074.1.7",
          "form": "맞아요당일치기라서",
          "original_form": "맞아요당일치기라서",
          "speaker_id": "2",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100027074.1.8",
          "form": "조꼼 버거웠지만",
          "original_form": "조꼼 버거웠지만",
          "speaker_id": "2",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100027074.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100027074.1.10",
          "form": "아 당일치기로 다녀오셨구낰ㅋㅋㅋㅋ 엄청 일정 빡셋겟는데요",
          "original_form": "아 당일치기로 다녀오셨구낰ㅋㅋㅋㅋ 엄청 일정 빡셋겟는데요",
          "speaker_id": "1",
          "time": "20210902 12:11"
        },
        {
          "id": "MDRW2100027074.1.11",
          "form": "그래도 재미있었어요",
          "original_form": "그래도 재미있었어요{emoji:•}",
          "speaker_id": "2",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100027074.1.12",
          "form": "전주에 그 길거리야 아시나요",
          "original_form": "전주에 그 길거리야 아시나요",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027074.1.13",
          "form": "잘모르겠네용",
          "original_form": "잘모르겠네용",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027074.1.14",
          "form": "그음식많은곳먼 가서 ㅋㅋㅋ",
          "original_form": "그음식많은곳먼 가서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027074.1.15",
          "form": "아 음식점 이름이 길거리야 인데 존맛이거든요",
          "original_form": "아 음식점 이름이 길거리야 인데 존맛이거든요",
          "speaker_id": "1",
          "time": "20210902 12:45"
        },
        {
          "id": "MDRW2100027074.1.16",
          "form": "아진짜요??",
          "original_form": "아진짜요??",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027074.1.17",
          "form": "다음에 가볼게요!!!",
          "original_form": "다음에 가볼게요!!!",
          "speaker_id": "2",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027074.1.18",
          "form": "방탈출이런거좋아하시나요??",
          "original_form": "방탈출이런거좋아하시나요??",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100027074.1.19",
          "form": "네! 가본적은 별로없지만 재밋어요!",
          "original_form": "네! 가본적은 별로없지만 재밋어요!",
          "speaker_id": "1",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100027074.1.20",
          "form": "전진짜좋아햐요",
          "original_form": "전진짜좋아햐요",
          "speaker_id": "2",
          "time": "20210902 13:01"
        },
        {
          "id": "MDRW2100027074.1.21",
          "form": "방탈출카페 자주가세요?",
          "original_form": "방탈출카페 자주가세요?",
          "speaker_id": "1",
          "time": "20210902 13:02"
        },
        {
          "id": "MDRW2100027074.1.22",
          "form": "네네 진짜 좋아해요 ㅠㅠ",
          "original_form": "네네 진짜 좋아해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:04"
        }
      ]
