        {
          "id": "MDRW2100034304.1.2",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034304.1.3",
          "form": "가을 옷 살 거야?",
          "original_form": "가을 옷 살 거야?",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034304.1.4",
          "form": "이번 가을은 안 사고 있는 옷 입으려고",
          "original_form": "이번 가을은 안 사고 있는 옷 입으려고",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034304.1.5",
          "form": "살 빼고",
          "original_form": "살 빼고",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034304.1.6",
          "form": "너도 그럴 듯",
          "original_form": "너도 그럴 듯",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034304.1.7",
          "form": "내년 봄부터",
          "original_form": "내년 봄부터",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034304.1.8",
          "form": "아니 나도",
          "original_form": "아니 나도",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034304.1.9",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034304.1.10",
          "form": "옷 많이 사야지",
          "original_form": "옷 많이 사야지",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034304.1.11",
          "form": "이게 왜 나쁜 말이야",
          "original_form": "이게 왜 나쁜 말이야",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034304.1.12",
          "form": "왕창",
          "original_form": "왕창",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034304.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034304.1.14",
          "form": "나도 그냥 봄에 입던 거 입으려고",
          "original_form": "나도 그냥 봄에 입던 거 입으려고",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034304.1.15",
          "form": "긴팔이랑 겨울옷은 많아서",
          "original_form": "긴팔이랑 겨울옷은 많아서",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034304.1.16",
          "form": "있는 걸로 버텨야지",
          "original_form": "있는 걸로 버텨야지",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034304.1.17",
          "form": "나갈 일도 별로 없고...ㅎ",
          "original_form": "나갈 일도 별로 없고...ㅎ",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
