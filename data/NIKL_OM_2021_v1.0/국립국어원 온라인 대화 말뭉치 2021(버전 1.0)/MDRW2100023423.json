      "utterance": [
        {
          "id": "MDRW2100023423.1.1",
          "form": "넹 한번 잃어버리면 힘들어여...ㅋㅋㅋ",
          "original_form": "넹 한번 잃어버리면 힘들어여...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100023423.1.2",
          "form": "이불 빨래는 하셨나용?",
          "original_form": "이불 빨래는 하셨나용?",
          "speaker_id": "1",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100023423.1.3",
          "form": "허겅.. 요즘 너무 흐리구 비와서",
          "original_form": "허겅.. 요즘 너무 흐리구 비와서",
          "speaker_id": "2",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100023423.1.4",
          "form": "실내건조로 ㅠㅠ",
          "original_form": "실내건조로 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100023423.1.5",
          "form": "어찌저찌.. 님은요??",
          "original_form": "어찌저찌.. 님은요??",
          "speaker_id": "2",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023423.1.6",
          "form": "저도 엊그제밤에 빨래해서 널어놨는데",
          "original_form": "저도 엊그제밤에 빨래해서 널어놨는데",
          "speaker_id": "1",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023423.1.7",
          "form": "오늘 낮되서야 겨우 말랐어요",
          "original_form": "오늘 낮되서야 겨우 말랐어요",
          "speaker_id": "1",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023423.1.8",
          "form": "너무 습해서",
          "original_form": "너무 습해서",
          "speaker_id": "1",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023423.1.9",
          "form": "안말라요ㅠㅠㅠ",
          "original_form": "안말라요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023423.1.10",
          "form": "이번주말그나마",
          "original_form": "이번주말그나마",
          "speaker_id": "2",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023423.1.11",
          "form": "비가안와서쪼금나았죠",
          "original_form": "비가안와서쪼금나았죠",
          "speaker_id": "2",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023423.1.12",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "1",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023423.1.13",
          "form": "근데 오늘도 오전에는",
          "original_form": "근데 오늘도 오전에는",
          "speaker_id": "1",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023423.1.14",
          "form": "남부지방에는 천둥번개치고",
          "original_form": "남부지방에는 천둥번개치고",
          "speaker_id": "1",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023423.1.15",
          "form": "비왔다고 하더라구여",
          "original_form": "비왔다고 하더라구여",
          "speaker_id": "1",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023423.1.16",
          "form": "헐 중부는 해났어요! 잠깐이지만",
          "original_form": "헐 중부는 해났어요! 잠깐이지만",
          "speaker_id": "2",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023423.1.17",
          "form": "오오 다행이네용!",
          "original_form": "오오 다행이네용!",
          "speaker_id": "1",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023423.1.18",
          "form": "저는 경기도인데",
          "original_form": "저는 경기도인데",
          "speaker_id": "1",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023423.1.19",
          "form": "하루종일 흐렸어요ㅠㅠ",
          "original_form": "하루종일 흐렸어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023423.1.20",
          "form": "비오는거 좋아하세용?",
          "original_form": "비오는거 좋아하세용?",
          "speaker_id": "1",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023423.1.21",
          "form": "오잉 저도 경기도인데",
          "original_form": "오잉 저도 경기도인데",
          "speaker_id": "2",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023423.1.22",
          "form": "저는 정확히 북부지역이에용",
          "original_form": "저는 정확히 북부지역이에용",
          "speaker_id": "1",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023423.1.23",
          "form": "파주쪽?",
          "original_form": "파주쪽?",
          "speaker_id": "1",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023423.1.24",
          "form": "비오는거 좋아요~~ 빨래는 너무 슬프지만",
          "original_form": "비오는거 좋아요~~ 빨래는 너무 슬프지만",
          "speaker_id": "2",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023423.1.25",
          "form": "아하 저는 남부예요",
          "original_form": "아하 저는 남부예요",
          "speaker_id": "2",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023423.1.26",
          "form": "그래서 그런가봐용!ㅎㅎ",
          "original_form": "그래서 그런가봐용!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023423.1.27",
          "form": "저는 비오는날",
          "original_form": "저는 비오는날",
          "speaker_id": "1",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023423.1.28",
          "form": "집안에 있는거 좋아해용ㅎㅎ",
          "original_form": "집안에 있는거 좋아해용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023423.1.29",
          "form": "그 특유의 차분하고 잠 잘올것같은 분위기가 좋아요",
          "original_form": "그 특유의 차분하고 잠 잘올것같은 분위기가 좋아요",
          "speaker_id": "1",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023423.1.30",
          "form": "와 저두요 멀좀아시는군여!! 비오는날 커피 캬~",
          "original_form": "와 저두요 멀좀아시는군여!! 비오는날 커피 캬~",
          "speaker_id": "2",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100023423.1.31",
          "form": "최고져...",
          "original_form": "최고져...",
          "speaker_id": "2",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100023423.1.32",
          "form": "넹넹ㅋㅋㅋ 근데 나가서 막 돌아다니고 그런건 음...ㅋㅋㅋ",
          "original_form": "넹넹ㅋㅋㅋ 근데 나가서 막 돌아다니고 그런건 음...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100023423.1.33",
          "form": "ㅠ 이제 자러 가야겠어요 낼 출근이...",
          "original_form": "ㅠ 이제 자러 가야겠어요 낼 출근이...",
          "speaker_id": "2",
          "time": "20210830 02:04"
        }
      ]
    }
