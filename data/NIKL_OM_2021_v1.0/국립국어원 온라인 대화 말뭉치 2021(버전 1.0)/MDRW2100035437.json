        {
          "id": "MDRW2100035437.1.1",
          "form": "일단 무조건 해피엔딩이 좋더라 난",
          "original_form": "일단 무조건 해피엔딩이 좋더라 난",
          "speaker_id": "2",
          "time": "20210829 17:07"
        },
        {
          "id": "MDRW2100035437.1.2",
          "form": "그래 맞다~",
          "original_form": "그래 맞다~",
          "speaker_id": "1",
          "time": "20210829 17:07"
        },
        {
          "id": "MDRW2100035437.1.3",
          "form": "발레강사분도 쿨하시다 ㅋㅋ",
          "original_form": "발레강사분도 쿨하시다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:07"
        },
        {
          "id": "MDRW2100035437.1.4",
          "form": "그러네. 본인만 서운",
          "original_form": "그러네. 본인만 서운",
          "speaker_id": "1",
          "time": "20210829 17:08"
        },
        {
          "id": "MDRW2100035437.1.5",
          "form": "그러게 ㅋㅋ",
          "original_form": "그러게 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:08"
        },
        {
          "id": "MDRW2100035437.1.6",
          "form": "할머니 웃긴다",
          "original_form": "할머니 웃긴다",
          "speaker_id": "1",
          "time": "20210829 17:08"
        },
        {
          "id": "MDRW2100035437.1.7",
          "form": "응 포옹하고 바로 밀쳐버림",
          "original_form": "응 포옹하고 바로 밀쳐버림",
          "speaker_id": "2",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100035437.1.8",
          "form": "내말이..  친구 나간다",
          "original_form": "내말이..  친구 나간다",
          "speaker_id": "1",
          "time": "20210829 17:09"
        },
        {
          "id": "MDRW2100035437.1.9",
          "form": "친구가 이쁘게 생겼다",
          "original_form": "친구가 이쁘게 생겼다",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100035437.1.10",
          "form": "진짜 여자같이 생김",
          "original_form": "진짜 여자같이 생김",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100035437.1.11",
          "form": "저리 헤어지니 특히 남자들이라 표현도 안하고",
          "original_form": "저리 헤어지니 특히 남자들이라 표현도 안하고",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100035437.1.12",
          "form": "그래..  그래도 다 알지",
          "original_form": "그래..  그래도 다 알지",
          "speaker_id": "1",
          "time": "20210829 17:11"
