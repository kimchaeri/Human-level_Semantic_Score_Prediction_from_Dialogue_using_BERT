      "utterance": [
        {
          "id": "MDRW2100045286.1.1",
          "form": "오 아녀 그거 아직 못먹어봤어요",
          "original_form": "오 아녀 그거 아직 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045286.1.2",
          "form": "어떤가요..? 드셔보셨나요?",
          "original_form": "어떤가요..? 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045286.1.3",
          "form": "아녀! 저도 안먹어봐서 궁금해서 물어봤어요 ㅋㅋㅋ",
          "original_form": "아녀! 저도 안먹어봐서 궁금해서 물어봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045286.1.4",
          "form": "어떤맛일까요",
          "original_form": "어떤맛일까요",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045286.1.5",
          "form": "그니까요 생으로 먹던데... 무슨 맛일지 궁금하네요",
          "original_form": "그니까요 생으로 먹던데... 무슨 맛일지 궁금하네요",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045286.1.6",
          "form": "생으로 먹어요????",
          "original_form": "생으로 먹어요????",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045286.1.7",
          "form": "어디 넣거나 데쳐먹는거 아니고요?",
          "original_form": "어디 넣거나 데쳐먹는거 아니고요?",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045286.1.8",
          "form": "제가 본건 다 생으로 먹더라구요...",
          "original_form": "제가 본건 다 생으로 먹더라구요...",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045286.1.9",
          "form": "그 소금장에 찍어사요...!",
          "original_form": "그 소금장에 찍어사요...!",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045286.1.10",
          "form": "오오... 신기하네요",
          "original_form": "오오... 신기하네요",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045286.1.11",
          "form": "송이 드셔보셨어요?",
          "original_form": "송이 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045286.1.12",
          "form": "그것도 생으로 먹는다던게",
          "original_form": "그것도 생으로 먹는다던게",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045286.1.13",
          "form": "네 .. 근데 사실.송이.. 제스타일이 아니었네요",
          "original_form": "네 .. 근데 사실.송이.. 제스타일이 아니었네요",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045286.1.14",
          "form": "향이 넘 진해서.. ㅜㅜ 별로열어요 송이 좋아하시나요",
          "original_form": "향이 넘 진해서.. ㅜㅜ 별로열어요 송이 좋아하시나요",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045286.1.15",
          "form": "저는 어렸을때 엄마가 먹어봤다는데 기억에 없어요 ..ㅋㅋㅋㅋ",
          "original_form": "저는 어렸을때 엄마가 먹어봤다는데 기억에 없어요 ..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045286.1.16",
          "form": "그게 뭐가 얼마나 좋길래 그렇게 비싼걸까요?",
          "original_form": "그게 뭐가 얼마나 좋길래 그렇게 비싼걸까요?",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045286.1.17",
          "form": "그러니까요.. 트러플 버섯도 그렇구요",
          "original_form": "그러니까요.. 트러플 버섯도 그렇구요",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045286.1.18",
          "form": "근데 송이는 인공재배가 안되서 비싼거라네요...",
          "original_form": "근데 송이는 인공재배가 안되서 비싼거라네요...",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045286.1.19",
          "form": "참 버섯은 신기해요..",
          "original_form": "참 버섯은 신기해요..",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045286.1.20",
          "form": "근데 인공재배가 안되도 다른 버섯들이 많은데 굳이 비쌀필요가 있나 싶어요",
          "original_form": "근데 인공재배가 안되도 다른 버섯들이 많은데 굳이 비쌀필요가 있나 싶어요",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045286.1.21",
          "form": "특별한 뭔가가 있으면 모르는데",
          "original_form": "특별한 뭔가가 있으면 모르는데",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045286.1.22",
          "form": "그런건 아니잖아요",
          "original_form": "그런건 아니잖아요",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045286.1.23",
          "form": "뭔가 사람들은 희귀하고.. 그런걸 좋아하는 것 같아요..",
          "original_form": "뭔가 사람들은 희귀하고.. 그런걸 좋아하는 것 같아요..",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045286.1.24",
          "form": "그래서 송이나 트러플도 인기가 있는게 아닐까요..",
          "original_form": "그래서 송이나 트러플도 인기가 있는게 아닐까요..",
          "speaker_id": "2",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045286.1.25",
          "form": "저는 약간 이해못해요 ..ㅋㅋㅋㅋ 그냥저냥 다 좋다 이런 사람이라 그런가봐요",
          "original_form": "저는 약간 이해못해요 ..ㅋㅋㅋㅋ 그냥저냥 다 좋다 이런 사람이라 그런가봐요",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045286.1.26",
          "form": "저도 가끔 이해안될때가 있어욬ㅋㅋ",
          "original_form": "저도 가끔 이해안될때가 있어욬ㅋㅋ",
          "speaker_id": "2",
