      "utterance": [
        {
          "id": "MDRW2100020588.1.1",
          "form": "저도 따라먹었는데 맛있더라구요",
          "original_form": "저도 따라먹었는데 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020588.1.2",
          "form": "아니 근데 베라에서 요거트만 한통이라니",
          "original_form": "아니 근데 베라에서 요거트만 한통이라니",
          "speaker_id": "2",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020588.1.3",
          "form": "오 저도 도전해봐야겠어요",
          "original_form": "오 저도 도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020588.1.4",
          "form": "뭔가 부자같네요,,,",
          "original_form": "뭔가 부자같네요,,,",
          "speaker_id": "2",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020588.1.5",
          "form": "진정한광기",
          "original_form": "진정한광기",
          "speaker_id": "1",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020588.1.6",
          "form": "아닙니다..",
          "original_form": "아닙니다..",
          "speaker_id": "1",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020588.1.7",
          "form": "저는 약간 원재료가 비싸보이는 애들로....",
          "original_form": "저는 약간 원재료가 비싸보이는 애들로....",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020588.1.8",
          "form": "그럼 6가지맛을 담는다면?",
          "original_form": "그럼 6가지맛을 담는다면?",
          "speaker_id": "1",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020588.1.9",
          "form": "퍼담는답니다..",
          "original_form": "퍼담는답니다..",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020588.1.10",
          "form": "아 뉴치케 넣으셔야죠",
          "original_form": "아 뉴치케 넣으셔야죠",
          "speaker_id": "1",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020588.1.11",
          "form": "원가 제일비쌈",
          "original_form": "원가 제일비쌈",
          "speaker_id": "1",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020588.1.12",
          "form": "앗 의도한건 아니지만",
          "original_form": "앗 의도한건 아니지만",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020588.1.13",
          "form": "뉴치케를 담으시나요",
          "original_form": "뉴치케를 담으시나요",
          "speaker_id": "1",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020588.1.14",
          "form": "제가 소비를 잘하고 있었네요",
          "original_form": "제가 소비를 잘하고 있었네요",
          "speaker_id": "2",
          "time": "20210828 10:12"
        },
        {
          "id": "MDRW2100020588.1.15",
          "form": "종종 뉴치케도 넣는 편이죠",
          "original_form": "종종 뉴치케도 넣는 편이죠",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020588.1.16",
          "form": "ㅋㅋㅋㅋㅋ 역시 먹잘알은 본능이군요",
          "original_form": "ㅋㅋㅋㅋㅋ 역시 먹잘알은 본능이군요",
          "speaker_id": "1",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020588.1.17",
          "form": "6가지면 흠",
          "original_form": "6가지면 흠",
          "speaker_id": "2",
          "time": "20210828 10:13"
        },
        {
          "id": "MDRW2100020588.1.18",
          "form": "알갱이가 많은 걸 좀 넣고 싶네요",
          "original_form": "알갱이가 많은 걸 좀 넣고 싶네요",
          "speaker_id": "2",
          "time": "20210828 10:13"
        }
      ]
