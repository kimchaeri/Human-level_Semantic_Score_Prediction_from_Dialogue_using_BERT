        {
          "id": "MDRW2100043372.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043372.1.2",
          "form": "찬물은 심지어 때도 안 불어난다! 우우",
          "original_form": "찬물은 심지어 때도 안 불어난다! 우우",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043372.1.3",
          "form": "ㅋㅋㅋ찬물샤워 극혐파",
          "original_form": "ㅋㅋㅋ찬물샤워 극혐파",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043372.1.4",
          "form": "아 너무 웃겨요",
          "original_form": "아 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043372.1.5",
          "form": "우우",
          "original_form": "우우",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043372.1.6",
          "form": "엇 근데 백신 맞으셨으면 샤워 한동안 못 하시겠네요",
          "original_form": "엇 근데 백신 맞으셨으면 샤워 한동안 못 하시겠네요",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043372.1.7",
          "form": "아닌가 하루만 건너뛰면 되나요?",
          "original_form": "아닌가 하루만 건너뛰면 되나요?",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100043372.1.8",
          "form": "당일만 안하면돼요!",
          "original_form": "당일만 안하면돼요!",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043372.1.9",
          "form": "오늘 일어나자마자 했답니다",
          "original_form": "오늘 일어나자마자 했답니다",
          "speaker_id": "1",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100043372.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ뜨거운 물로 하셨나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ뜨거운 물로 하셨나요",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043372.1.11",
          "form": "네 두피가 익을뻔했답니다ㅠ",
          "original_form": "네 두피가 익을뻔했답니다ㅠ",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043372.1.12",
          "form": "하루건너뛰어서",
          "original_form": "하루건너뛰어서",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043372.1.13",
          "form": "온도조절 정도를 까먹었어요ㅠ",
          "original_form": "온도조절 정도를 까먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043372.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:37"
        },
        {
          "id": "MDRW2100043372.1.15",
          "form": "삶아지셨군요",
          "original_form": "삶아지셨군요",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043372.1.16",
          "form": "저도 오늘 두피 삶아야 하는데",
          "original_form": "저도 오늘 두피 삶아야 하는데",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043372.1.17",
          "form": "저녁 먹고 삶을 예정이어유",
          "original_form": "저녁 먹고 삶을 예정이어유",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043372.1.18",
          "form": "저녁 뭐 먹을까 고민이 되네요 요즘 건강식으로 챙겨먹으려고 노력하고 있거든요!",
          "original_form": "저녁 뭐 먹을까 고민이 되네요 요즘 건강식으로 챙겨먹으려고 노력하고 있거든요!",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100043372.1.19",
          "form": "name1님은 다이어트 해본 적 있으세요?",
          "original_form": "&name1&님은 다이어트 해본 적 있으세요?",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
