      "utterance": [
        {
          "id": "MDRW2100025544.1.1",
          "form": "뭔가 뀨는 상상이 안돼",
          "original_form": "뭔가 뀨는 상상이 안돼",
          "speaker_id": "2",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025544.1.2",
          "form": "내 이미지랑 매치가 안돼",
          "original_form": "내 이미지랑 매치가 안돼",
          "speaker_id": "2",
          "time": "20210901 13:06"
        },
        {
          "id": "MDRW2100025544.1.3",
          "form": "뀨는 약간 술먹고 헤롱거리는",
          "original_form": "뀨는 약간 술먹고 헤롱거리는",
          "speaker_id": "1",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025544.1.4",
          "form": "드렁큰헤드윅같은느낌",
          "original_form": "드렁큰헤드윅같은느낌",
          "speaker_id": "1",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025544.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그건 걍 슬빵 마약 중독자 아니야..?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그건 걍 슬빵 마약 중독자 아니야..?",
          "speaker_id": "2",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025544.1.6",
          "form": "ㅋㅋㄱㅋㄱㅋㅋㄱㅋㅋ헤롱이랑 비슷한데 쫌 달라",
          "original_form": "ㅋㅋㄱㅋㄱㅋㅋㄱㅋㅋ헤롱이랑 비슷한데 쫌 달라",
          "speaker_id": "1",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025544.1.7",
          "form": "그리고 인성이터졌어",
          "original_form": "그리고 인성이터졌어",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025544.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 뀨는 인성 터져도 귀여울듯",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ근데 뀨는 인성 터져도 귀여울듯",
          "speaker_id": "2",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025544.1.9",
          "form": "조드윅은 인성이안터지고",
          "original_form": "조드윅은 인성이안터지고",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025544.1.10",
          "form": "캐해가 좀 많이다르더라구용",
          "original_form": "캐해가 좀 많이다르더라구용",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025544.1.11",
          "form": "사람 자체에서 풍기는 러블리함이 있어",
          "original_form": "사람 자체에서 풍기는 러블리함이 있어",
          "speaker_id": "2",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025544.1.12",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025544.1.13",
          "form": "뀨 완전러블리",
          "original_form": "뀨 완전러블리",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025544.1.14",
          "form": "사실 저도 젠틀맨스가이드",
          "original_form": "사실 저도 젠틀맨스가이드",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025544.1.15",
          "form": "보고싶었는데",
          "original_form": "보고싶었는데",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025544.1.16",
          "form": "당시 하도 난리라",
          "original_form": "당시 하도 난리라",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025544.1.17",
          "form": "ㅜ 아쉽",
          "original_form": "ㅜ 아쉽",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025544.1.18",
          "form": "캐해 다른 맛인거 좋아ㅜㅜㅜㅜㅜ 같은 캐릭 연기해도 다들 초점이 다르니깐",
          "original_form": "캐해 다른 맛인거 좋아ㅜㅜㅜㅜㅜ 같은 캐릭 연기해도 다들 초점이 다르니깐",
          "speaker_id": "2",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025544.1.19",
          "form": "ㅇㅇㅇㅇㅇㅇ그래서 ㅋㅋㅋ원래 이츠학이랑 애증관계인데",
          "original_form": "ㅇㅇㅇㅇㅇㅇ그래서 ㅋㅋㅋ원래 이츠학이랑 애증관계인데",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025544.1.20",
          "form": "젠가는 그 때 이상이 것도 보고 싶엇는데",
          "original_form": "젠가는 그 때 이상이 것도 보고 싶엇는데",
          "speaker_id": "2",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025544.1.21",
          "form": "여긴 이츠학이 다 좋아서 곁에 붙어있는거 같은",
          "original_form": "여긴 이츠학이 다 좋아서 곁에 붙어있는거 같은",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025544.1.22",
          "form": "지갑 가난해서 못본거 아쉬움",
          "original_form": "지갑 가난해서 못본거 아쉬움",
          "speaker_id": "2",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025544.1.23",
          "form": "오...이상이가 했었어....?",
          "original_form": "오...이상이가 했었어....?",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025544.1.24",
          "form": "첨듣는 뉴우스",
          "original_form": "첨듣는 뉴우스",
          "speaker_id": "1",
          "time": "20210901 13:10"
        }
      ]
