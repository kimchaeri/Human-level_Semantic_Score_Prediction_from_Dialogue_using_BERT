        {
          "id": "MDRW2100018092.1.2",
          "form": "필승!",
          "original_form": "필승!",
          "speaker_id": "2",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100018092.1.3",
          "form": "군대 다녀오셨습니까?",
          "original_form": "군대 다녀오셨습니까?",
          "speaker_id": "1",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100018092.1.4",
          "form": "아뇨ㅋㅋ 다녀오셨어요?",
          "original_form": "아뇨ㅋㅋ 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100018092.1.5",
          "form": "다녀오지 않았습니다..",
          "original_form": "다녀오지 않았습니다..",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100018092.1.6",
          "form": "미필끼리 군대 이야기 한번 해볼까요?ㅋㅋ",
          "original_form": "미필끼리 군대 이야기 한번 해볼까요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100018092.1.7",
          "form": "ㅋㅋㅋ좋네용",
          "original_form": "ㅋㅋㅋ좋네용",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100018092.1.8",
          "form": "요즘 여자들이 군대 가야한다는 주장이 나오는데 어떻게 생각하세요?",
          "original_form": "요즘 여자들이 군대 가야한다는 주장이 나오는데 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100018092.1.9",
          "form": "가도 변할게 없지 않나 싶어요",
          "original_form": "가도 변할게 없지 않나 싶어요",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100018092.1.10",
          "form": "아 어떤 것들이요?",
          "original_form": "아 어떤 것들이요?",
          "speaker_id": "1",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100018092.1.11",
          "form": "서로를 향한 날이 랄까요",
          "original_form": "서로를 향한 날이 랄까요",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100018092.1.12",
          "form": "좁혀지지 않을거겉아요",
          "original_form": "좁혀지지 않을거겉아요",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100018092.1.13",
          "form": "맞죠 ㅠㅠ 이건 강제할 사항이 아닌 것 같아요.",
          "original_form": "맞죠 ㅠㅠ 이건 강제할 사항이 아닌 것 같아요.",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100018092.1.14",
          "form": "근데 군대는 미뤄두신거에요?",
          "original_form": "근데 군대는 미뤄두신거에요?",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100018092.1.15",
          "form": "아뇨 저는 여자라ㅠ",
          "original_form": "아뇨 저는 여자라ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100018092.1.16",
          "form": "곧 군대 가시나요?",
          "original_form": "곧 군대 가시나요?",
          "speaker_id": "2",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100018092.1.17",
          "form": "앗 제가 실례 했습니다...",
          "original_form": "앗 제가 실례 했습니다...",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100018092.1.18",
          "form": "저는 면제에요!",
          "original_form": "저는 면제에요!",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100018092.1.19",
          "form": "아녜요",
          "original_form": "아녜요",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100018092.1.20",
          "form": "아하 그러시균요!",
          "original_form": "아하 그러시균요!",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100018092.1.21",
          "form": "그럼 남사친이 군대 얘기할 때 기분이 어때요?",
          "original_form": "그럼 남사친이 군대 얘기할 때 기분이 어때요?",
          "speaker_id": "1",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100018092.1.22",
          "form": "음 어색?이랄까요",
          "original_form": "음 어색?이랄까요",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100018092.1.23",
          "form": "이미 군대 다 다녀와서ㅋㅋ 사실 그냥 그렇구나싶어오ㅋㅋ",
          "original_form": "이미 군대 다 다녀와서ㅋㅋ 사실 그냥 그렇구나싶어오ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100018092.1.24",
          "form": "저도 친구들이 군대 이야기 하면 무슨 소리 하는 지 하나도 모르겠더라구요",
          "original_form": "저도 친구들이 군대 이야기 하면 무슨 소리 하는 지 하나도 모르겠더라구요",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100018092.1.25",
          "form": "근데 자기네끼리 신나게 하는 거 보면 가만히 있어요 그냥 ㅠ",
          "original_form": "근데 자기네끼리 신나게 하는 거 보면 가만히 있어요 그냥 ㅠ",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100018092.1.26",
          "form": "저도..ㅠ 잘 모르니까요ㅠ",
          "original_form": "저도..ㅠ 잘 모르니까요ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100018092.1.27",
          "form": "그래도 군인들이 있어서 한국이 안전한 것 같네요",
          "original_form": "그래도 군인들이 있어서 한국이 안전한 것 같네요",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100018092.1.28",
          "form": "워낙 한국 노리는 국가들이 많다보니..",
          "original_form": "워낙 한국 노리는 국가들이 많다보니..",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100018092.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100018092.1.30",
          "form": "그건 그렇죠",
          "original_form": "그건 그렇죠",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100018092.1.31",
          "form": "편해졌다해도 쉽지않은걸요",
          "original_form": "편해졌다해도 쉽지않은걸요",
          "speaker_id": "2",
          "time": "20210827 21:58"
        }
      ]
