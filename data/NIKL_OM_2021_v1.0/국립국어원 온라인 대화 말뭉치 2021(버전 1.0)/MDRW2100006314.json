      "utterance": [
        {
          "id": "MDRW2100006314.1.1",
          "form": "오늘 비왔나요?",
          "original_form": "오늘 비왔나요?",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006314.1.2",
          "form": "네 아침부터 계속오네요",
          "original_form": "네 아침부터 계속오네요",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006314.1.3",
          "form": "여긴지금 그쳤어요",
          "original_form": "여긴지금 그쳤어요",
          "speaker_id": "1",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006314.1.4",
          "form": "날씨도 쌀쌀해요",
          "original_form": "날씨도 쌀쌀해요",
          "speaker_id": "2",
          "time": "20210907 22:43"
        },
        {
          "id": "MDRW2100006314.1.5",
          "form": "여기도 쌀쌀해요",
          "original_form": "여기도 쌀쌀해요",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006314.1.6",
          "form": "집은 근데 왜더울까요ㅎㅎ",
          "original_form": "집은 근데 왜더울까요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006314.1.7",
          "form": "전 선풍기 안키고있어요",
          "original_form": "전 선풍기 안키고있어요",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006314.1.8",
          "form": "선풍기틀고 있는데",
          "original_form": "선풍기틀고 있는데",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006314.1.9",
          "form": "전 이불덥고있어요",
          "original_form": "전 이불덥고있어요",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006314.1.10",
          "form": "헉 ㅎ아파트라 시원한가봐요",
          "original_form": "헉 ㅎ아파트라 시원한가봐요",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006314.1.11",
          "form": "주택이요",
          "original_form": "주택이요",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100006314.1.12",
          "form": "오피스텔이라 환기가 잘안돼서",
          "original_form": "오피스텔이라 환기가 잘안돼서",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006314.1.13",
          "form": "강옆이라 그런지  선선하네요",
          "original_form": "강옆이라 그런지  선선하네요",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006314.1.14",
          "form": "바깥공기가 잘 안들어오네요",
          "original_form": "바깥공기가 잘 안들어오네요",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006314.1.15",
          "form": "맞바람불어야 환기도 잘되더라구요",
          "original_form": "맞바람불어야 환기도 잘되더라구요",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006314.1.16",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006314.1.17",
          "form": "습해서 빨래말리기 힘드네요",
          "original_form": "습해서 빨래말리기 힘드네요",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100006314.1.18",
          "form": "비 지긋지긋한데 태풍 북상중이래요",
          "original_form": "비 지긋지긋한데 태풍 북상중이래요",
