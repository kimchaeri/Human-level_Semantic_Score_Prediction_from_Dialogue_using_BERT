      "utterance": [
        {
          "id": "MDRW2100029783.1.1",
          "form": "저승가라고 함",
          "original_form": "저승가라고 함",
          "speaker_id": "1",
          "time": "20210903 14:45"
        },
        {
          "id": "MDRW2100029783.1.2",
          "form": "신함얘기했더니",
          "original_form": "신함얘기했더니",
          "speaker_id": "1",
          "time": "20210903 14:45"
        },
        {
          "id": "MDRW2100029783.1.3",
          "form": "저승가라는게",
          "original_form": "저승가라는게",
          "speaker_id": "1",
          "time": "20210903 14:45"
        },
        {
          "id": "MDRW2100029783.1.4",
          "form": "그렇게 큰의미가있는지도 잘 모르겄네",
          "original_form": "그렇게 큰의미가있는지도 잘 모르겄네",
          "speaker_id": "1",
          "time": "20210903 14:45"
        },
        {
          "id": "MDRW2100029783.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029783.1.6",
          "form": "저승에 가려면",
          "original_form": "저승에 가려면",
          "speaker_id": "2",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029783.1.7",
          "form": "사망을 해야 하는데",
          "original_form": "사망을 해야 하는데",
          "speaker_id": "2",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029783.1.8",
          "form": "그 얘길 못하네...",
          "original_form": "그 얘길 못하네...",
          "speaker_id": "2",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100029783.1.9",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210903 14:53"
        },
        {
          "id": "MDRW2100029783.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ여기석 ㄴ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ여기석 ㄴ",
          "speaker_id": "1",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029783.1.11",
          "form": "아 그러고보니",
          "original_form": "아 그러고보니",
          "speaker_id": "2",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100029783.1.12",
          "form": "샹치 반응 좋던데",
          "original_form": "샹치 반응 좋던데",
          "speaker_id": "2",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100029783.1.13",
          "form": "샹치가 좋다기보단",
          "original_form": "샹치가 좋다기보단",
          "speaker_id": "2",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100029783.1.14",
          "form": "양조위가 좋더라...",
          "original_form": "양조위가 좋더라...",
          "speaker_id": "2",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100029783.1.15",
          "form": "반응이...",
          "original_form": "반응이...",
          "speaker_id": "2",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100029783.1.16",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100029783.1.17",
          "form": "근데주인공 너무내취향아닌네",
          "original_form": "근데주인공 너무내취향아닌네",
          "speaker_id": "1",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100029783.1.18",
          "form": "평식옹 평이나쁘지않은거보고",
          "original_form": "평식옹 평이나쁘지않은거보고",
          "speaker_id": "1",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100029783.1.19",
          "form": "올ㅋ했음",
          "original_form": "올ㅋ했음",
          "speaker_id": "1",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100029783.1.20",
          "form": "ㅇㅇㅇ 생각보다 영화 자체도 나쁘지 않다는 모양..?",
          "original_form": "ㅇㅇㅇ 생각보다 영화 자체도 나쁘지 않다는 모양..?",
          "speaker_id": "2",
          "time": "20210903 15:18"
        },
        {
          "id": "MDRW2100029783.1.21",
          "form": "중뽕도 저정도면 그냥 참고 봐줄만한 정도라고..",
          "original_form": "중뽕도 저정도면 그냥 참고 봐줄만한 정도라고..",
          "speaker_id": "2",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100029783.1.22",
          "form": "오.....",
          "original_form": "오.....",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029783.1.23",
          "form": "한번보ㅏ볼까",
          "original_form": "한번보ㅏ볼까",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029783.1.24",
          "form": "블위도안봤는데",
          "original_form": "블위도안봤는데",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029783.1.25",
          "form": "샹치부터보게생겼네",
          "original_form": "샹치부터보게생겼네",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100029783.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그니까여...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그니까여...",
          "speaker_id": "2",
          "time": "20210903 16:02"
        },
        {
          "id": "MDRW2100029783.1.27",
          "form": "블위부터봐야할텐데",
          "original_form": "블위부터봐야할텐데",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100029783.1.28",
          "form": "또잘안봐지더라고...",
          "original_form": "또잘안봐지더라고...",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100029783.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이게 약간",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이게 약간",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029783.1.30",
          "form": "극장 가서 보는 영화는",
          "original_form": "극장 가서 보는 영화는",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029783.1.31",
          "form": "상영 스케줄이랑 상영기간이 딱 정해져있으니까",
          "original_form": "상영 스케줄이랑 상영기간이 딱 정해져있으니까",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029783.1.32",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029783.1.33",
          "form": "의무감? 의지? 를 갖고 가게되는데",
          "original_form": "의무감? 의지? 를 갖고 가게되는데",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029783.1.34",
          "form": "iptv나.. 넷플같은 플랫폼에 들어가있는 영화는 도리어",
          "original_form": "iptv나.. 넷플같은 플랫폼에 들어가있는 영화는 도리어",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029783.1.35",
          "form": "아무때나 볼 수 있으니까",
          "original_form": "아무때나 볼 수 있으니까",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029783.1.36",
          "form": "잘끊어버리고",
          "original_form": "잘끊어버리고",
          "speaker_id": "1",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029783.1.37",
          "form": "나태해져서 오히려",
          "original_form": "나태해져서 오히려",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029783.1.38",
          "form": "내의지대로",
          "original_form": "내의지대로",
          "speaker_id": "1",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029783.1.39",
          "form": "잘 안 보게 되는...",
          "original_form": "잘 안 보게 되는...",
          "speaker_id": "2",
          "time": "20210903 16:11"
