        },
        {
          "id": "MDRW2100035120.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100035120.1.3",
          "form": "점심식사하셨나요?",
          "original_form": "점심식사하셨나요?",
          "speaker_id": "2",
          "time": "20210829 13:32"
        },
        {
          "id": "MDRW2100035120.1.4",
          "form": "아니요.. name2님은요?",
          "original_form": "아니요.. &name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100035120.1.5",
          "form": "전 먹었어요",
          "original_form": "전 먹었어요",
          "speaker_id": "2",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100035120.1.6",
          "form": "오 뭐드셧어요?",
          "original_form": "오 뭐드셧어요?",
          "speaker_id": "1",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100035120.1.7",
          "form": "엄마가 순대국 포장해오셔서 먹었어요",
          "original_form": "엄마가 순대국 포장해오셔서 먹었어요",
          "speaker_id": "2",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100035120.1.8",
          "form": "오.. 맛잇엇겠네요. 사골국같은 느낌인가요?",
          "original_form": "오.. 맛잇엇겠네요. 사골국같은 느낌인가요?",
          "speaker_id": "1",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100035120.1.9",
          "form": "사골국과는 느낌이 다른거같아요",
          "original_form": "사골국과는 느낌이 다른거같아요",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100035120.1.10",
          "form": "무슨국 베이스인데요?",
          "original_form": "무슨국 베이스인데요?",
          "speaker_id": "1",
          "time": "20210829 14:52"
        },
        {
          "id": "MDRW2100035120.1.11",
          "form": "잘 모르겠어요ㅜㅜ",
          "original_form": "잘 모르겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 15:16"
        },
        {
          "id": "MDRW2100035120.1.12",
          "form": "오 그러시군요..\r\n안먹어본지 오래라 저도 먹고싶네요",
          "original_form": "오 그러시군요..\r\n안먹어본지 오래라 저도 먹고싶네요",
          "speaker_id": "1",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100035120.1.13",
          "form": "저희집 동네에 순대국집이 하나 있어 가끔 먹게 되더라구요",
          "original_form": "저희집 동네에 순대국집이 하나 있어 가끔 먹게 되더라구요",
          "speaker_id": "2",
          "time": "20210829 15:23"
        },
        {
          "id": "MDRW2100035120.1.14",
          "form": "저는 어제 유투브에서 순대튀김을 봣는데\r\n맛잇겟더라고요",
          "original_form": "저는 어제 유투브에서 순대튀김을 봣는데\r\n맛잇겟더라고요",
          "speaker_id": "1",
          "time": "20210829 15:24"
        },
        {
          "id": "MDRW2100035120.1.15",
          "form": "집에 반찬들이 없다고 엄마가 친구분과 식사하고, 점심먹으라고 따로 포장해오셨어요",
          "original_form": "집에 반찬들이 없다고 엄마가 친구분과 식사하고, 점심먹으라고 따로 포장해오셨어요",
          "speaker_id": "2",
          "time": "20210829 15:24"
        }
      ]
