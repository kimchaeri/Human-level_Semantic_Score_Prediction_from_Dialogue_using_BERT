      "utterance": [
        {
          "id": "MDRW2100011999.1.1",
          "form": "학교 생활 어때 오늘 뭐했어",
          "original_form": "학교 생활 어때 오늘 뭐했어",
          "speaker_id": "2",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011999.1.2",
          "form": "오늘 수업만 7번 했어",
          "original_form": "오늘 수업만 7번 했어",
          "speaker_id": "1",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011999.1.3",
          "form": "와 오늘 한 교시도 안 쉬었어?",
          "original_form": "와 오늘 한 교시도 안 쉬었어?",
          "speaker_id": "2",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011999.1.4",
          "form": "쉬는 시간 10분이랑",
          "original_form": "쉬는 시간 10분이랑",
          "speaker_id": "1",
          "time": "20210824 18:22"
        },
        {
          "id": "MDRW2100011999.1.5",
          "form": "점심 시간 빼고는 다 안 쉬었어",
          "original_form": "점심 시간 빼고는 다 안 쉬었어",
          "speaker_id": "1",
          "time": "20210824 18:22"
        },
        {
          "id": "MDRW2100011999.1.6",
          "form": "쉬는 시간에는 뭐해?",
          "original_form": "쉬는 시간에는 뭐해?",
          "speaker_id": "2",
          "time": "20210824 18:22"
        },
        {
          "id": "MDRW2100011999.1.7",
          "form": "친구들이랑 루미큐브 하거나",
          "original_form": "친구들이랑 루미큐브 하거나",
          "speaker_id": "1",
          "time": "20210824 18:22"
        },
        {
          "id": "MDRW2100011999.1.8",
          "form": "다른 반에 놀러가거나",
          "original_form": "다른 반에 놀러가거나",
          "speaker_id": "1",
          "time": "20210824 18:22"
        },
        {
          "id": "MDRW2100011999.1.9",
          "form": "자거나...?",
          "original_form": "자거나...?",
          "speaker_id": "1",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100011999.1.10",
          "form": "점심시간은 몇 분이야?",
          "original_form": "점심시간은 몇 분이야?",
          "speaker_id": "2",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100011999.1.11",
          "form": "1시간 25분!",
          "original_form": "1시간 25분!",
          "speaker_id": "1",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100011999.1.12",
          "form": "헐 엄청 기네",
          "original_form": "헐 엄청 기네",
          "speaker_id": "2",
          "time": "20210824 18:23"
        },
        {
          "id": "MDRW2100011999.1.13",
          "form": "근데 밥 먹고 좀 쉬면 빨리 지나가",
          "original_form": "근데 밥 먹고 좀 쉬면 빨리 지나가",
          "speaker_id": "1",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100011999.1.14",
          "form": "급식은 맛있어?",
          "original_form": "급식은 맛있어?",
          "speaker_id": "2",
          "time": "20210824 18:24"
        },
        {
          "id": "MDRW2100011999.1.15",
          "form": "급식 쥬스 나오면 맛있어",
          "original_form": "급식 쥬스 나오면 맛있어",
          "speaker_id": "1",
          "time": "20210824 18:33"
        }
      ]
