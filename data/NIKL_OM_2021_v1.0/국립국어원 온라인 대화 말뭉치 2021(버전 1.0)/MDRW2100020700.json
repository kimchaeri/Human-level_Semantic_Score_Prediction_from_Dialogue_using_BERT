        {
          "id": "MDRW2100020700.1.1",
          "form": "아뇽 안키워요",
          "original_form": "아뇽 안키워요",
          "speaker_id": "2",
          "time": "20210830 13:17"
        },
        {
          "id": "MDRW2100020700.1.2",
          "form": "아 저두요ㅠ",
          "original_form": "아 저두요ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100020700.1.3",
          "form": "그럼 키우고 싶은 동물은 있으신가요?",
          "original_form": "그럼 키우고 싶은 동물은 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100020700.1.4",
          "form": "아직이용 ㅠㅠ 있으세여?",
          "original_form": "아직이용 ㅠㅠ 있으세여?",
          "speaker_id": "2",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100020700.1.5",
          "form": "저는 강아지나 고양이 한번 키워보고 싶어요ㅎㅎ",
          "original_form": "저는 강아지나 고양이 한번 키워보고 싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:20"
        },
        {
          "id": "MDRW2100020700.1.6",
          "form": "너무 귀여울 것같아요",
          "original_form": "너무 귀여울 것같아요",
          "speaker_id": "1",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100020700.1.7",
          "form": "동물들은 안가리고 다좋아하세영?",
          "original_form": "동물들은 안가리고 다좋아하세영?",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100020700.1.8",
          "form": "네! 대부분 다 좋아해요",
          "original_form": "네! 대부분 다 좋아해요",
          "speaker_id": "1",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100020700.1.9",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100020700.1.10",
          "form": "저는 쏘쏘 ..보통이에영 ㅎㅎ",
          "original_form": "저는 쏘쏘 ..보통이에영 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 07:57"
        },
        {
          "id": "MDRW2100020700.1.11",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100020700.1.12",
          "form": "혹시 비염있으세요?",
          "original_form": "혹시 비염있으세요?",
          "speaker_id": "1",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100020700.1.13",
          "form": "아뇨 그건아닌뎅 ㅎㅎ 원래는 무서워서 싫었는데 그래도 점점 귀엽게보이더라구요 나이들면서 바뀌나봐요",
          "original_form": "아뇨 그건아닌뎅 ㅎㅎ 원래는 무서워서 싫었는데 그래도 점점 귀엽게보이더라구요 나이들면서 바뀌나봐요",
          "speaker_id": "2",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100020700.1.14",
          "form": "아ㅏ",
          "original_form": "아ㅏ",
          "speaker_id": "1",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100020700.1.15",
          "form": "저희 엄마두 고양이 눈이 무섭다면서 고양이 안좋아했는데",
          "original_form": "저희 엄마두 고양이 눈이 무섭다면서 고양이 안좋아했는데",
          "speaker_id": "1",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100020700.1.16",
          "form": "요즘은 귀엽다하세요ㅎㅎ",
          "original_form": "요즘은 귀엽다하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100020700.1.17",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100020700.1.18",
          "form": "맞아요 ㅋㅋㅋ변하나봐요 정말",
          "original_form": "맞아요 ㅋㅋㅋ변하나봐요 정말",
          "speaker_id": "2",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100020700.1.19",
          "form": "반려동물 생긴다면 지어주고 싶은 이름 있으세요?",
