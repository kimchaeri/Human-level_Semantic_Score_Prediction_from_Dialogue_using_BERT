          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 부럽다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아 부럽다",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.5",
          "form": "문학 수행으로 앍돈 책에",
          "original_form": "문학 수행으로 앍돈 책에",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.6",
          "form": "만화랑 웹툰 비교하는 내용 나와서",
          "original_form": "만화랑 웹툰 비교하는 내용 나와서",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.7",
          "form": "그거 적으려구",
          "original_form": "그거 적으려구",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.9",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.10",
          "form": "나 문학 수행 책",
          "original_form": "나 문학 수행 책",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.11",
          "form": "나는 유시민 작가의 국가란 무엇인가",
          "original_form": "나는 유시민 작가의 국가란 무엇인가",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.12",
          "form": "읽고 독서록 써야 함",
          "original_form": "읽고 독서록 써야 함",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.13",
          "form": "책 이름 웹툰 스쿨임",
          "original_form": "책 이름 웹툰 스쿨임",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 국가란 무엇인가... 들어만봐도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 국가란 무엇인가... 들어만봐도",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030266.1.16",
          "form": "어려워 보인다..",
          "original_form": "어려워 보인다..",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030266.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030266.1.18",
          "form": "정치와 법치",
          "original_form": "정치와 법치",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030266.1.19",
          "form": "이것보단 괜찮아..",
          "original_form": "이것보단 괜찮아..",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030266.1.20",
          "form": "ㅈ,진짜루..^^",
          "original_form": "ㅈ,진짜루..^^",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030266.1.21",
          "form": "ㅇ하 나 내년 정법하는데",
          "original_form": "ㅇ하 나 내년 정법하는데",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030266.1.22",
          "form": "어카지. ...",
          "original_form": "어카지. ...",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030266.1.23",
