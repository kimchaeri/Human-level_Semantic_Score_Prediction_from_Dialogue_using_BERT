      "utterance": [
        {
          "id": "MDRW2100033248.1.1",
          "form": "비와요",
          "original_form": "비와요",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100033248.1.2",
          "form": "다시 비의 날씨가 되었어요",
          "original_form": "다시 비의 날씨가 되었어요",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100033248.1.3",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033248.1.4",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033248.1.5",
          "form": "더위가 덜해서",
          "original_form": "더위가 덜해서",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033248.1.6",
          "form": "공원 오가지 나쁘지 않네요",
          "original_form": "공원 오가지 나쁘지 않네요",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033248.1.7",
          "form": "라고 할려했는데",
          "original_form": "라고 할려했는데",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033248.1.8",
          "form": "마침 비가 오네요",
          "original_form": "마침 비가 오네요",
          "speaker_id": "1",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100033248.1.9",
          "form": "산책하기 좋은 날씨라 매일 갔었는데",
          "original_form": "산책하기 좋은 날씨라 매일 갔었는데",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033248.1.10",
          "form": "지난 가을장마때는 못나갔어요",
          "original_form": "지난 가을장마때는 못나갔어요",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033248.1.11",
          "form": "오늘은 잠깐 안올때 나갔다오긴 했는데",
          "original_form": "오늘은 잠깐 안올때 나갔다오긴 했는데",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033248.1.12",
          "form": "내일은 못나가겠네요",
          "original_form": "내일은 못나가겠네요",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033248.1.13",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100033248.1.14",
          "form": "오늘 계속 비오네요",
          "original_form": "오늘 계속 비오네요",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100033248.1.15",
          "form": "그래도 출근할 때 보니까",
          "original_form": "그래도 출근할 때 보니까",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100033248.1.16",
          "form": "좀 잠잠해진 거 같긴 했지만요",
          "original_form": "좀 잠잠해진 거 같긴 했지만요",
          "speaker_id": "1",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100033248.1.17",
          "form": "보니까 우리 지녁은 수요일 새벽까지 오는 걸로 되어있네요",
          "original_form": "보니까 우리 지녁은 수요일 새벽까지 오는 걸로 되어있네요",
          "speaker_id": "2",
          "time": "20210907 09:28"
        },
        {
          "id": "MDRW2100033248.1.18",
          "form": "아직도 장맛비라고 하던데 진짜 오래가네요",
          "original_form": "아직도 장맛비라고 하던데 진짜 오래가네요",
          "speaker_id": "2",
          "time": "20210907 09:28"
        },
        {
          "id": "MDRW2100033248.1.19",
          "form": "그래도 작년보다",
          "original_form": "그래도 작년보다",
          "speaker_id": "1",
          "time": "20210907 10:56"
        },
        {
          "id": "MDRW2100033248.1.20",
          "form": "여름 중에는 덜 온 거 같긴 한데...",
          "original_form": "여름 중에는 덜 온 거 같긴 한데...",
          "speaker_id": "1",
          "time": "20210907 10:56"
        },
        {
          "id": "MDRW2100033248.1.21",
          "form": "그러니까 말이에요",
          "original_form": "그러니까 말이에요",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100033248.1.22",
          "form": "전 중부지방 살고 있는데 여기는 마른 장마라고 그랬거든요",
          "original_form": "전 중부지방 살고 있는데 여기는 마른 장마라고 그랬거든요",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100033248.1.23",
          "form": "그러더니 오히려 요즘 비가 계속 오네요",
          "original_form": "그러더니 오히려 요즘 비가 계속 오네요",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100033248.1.24",
          "form": "물론",
          "original_form": "물론",
          "speaker_id": "1",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100033248.1.25",
          "form": "요즘 비는",
          "original_form": "요즘 비는",
          "speaker_id": "1",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100033248.1.26",
          "form": "더위를 가시게 해줘서",
          "original_form": "더위를 가시게 해줘서",
          "speaker_id": "1",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100033248.1.27",
          "form": "마냥 나쁜건 아닌것 같지만서도",
          "original_form": "마냥 나쁜건 아닌것 같지만서도",
          "speaker_id": "1",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100033248.1.28",
          "form": "뭔가 우중충한건 어쩔 수 없네요",
          "original_form": "뭔가 우중충한건 어쩔 수 없네요",
          "speaker_id": "1",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100033248.1.29",
          "form": "날이 흐리니까 가을 같지 않아요",
          "original_form": "날이 흐리니까 가을 같지 않아요",
          "speaker_id": "2",
          "time": "20210907 16:16"
        },
        {
          "id": "MDRW2100033248.1.30",
          "form": "9월이면 맑고 푸른 하늘이 기대되는데",
          "original_form": "9월이면 맑고 푸른 하늘이 기대되는데",
          "speaker_id": "2",
          "time": "20210907 16:16"
        },
        {
          "id": "MDRW2100033248.1.31",
          "form": "확실히 그렇죠",
          "original_form": "확실히 그렇죠",
          "speaker_id": "1",
          "time": "20210907 16:18"
        },
        {
          "id": "MDRW2100033248.1.32",
          "form": "빨리 비가 그치고 단풍이 들었으면 좋겠어요",
          "original_form": "빨리 비가 그치고 단풍이 들었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100033248.1.33",
          "form": "아 확실히",
          "original_form": "아 확실히",
          "speaker_id": "1",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100033248.1.34",
          "form": "맑은 하늘 시원한 온도 적당한 바람 알록달록한 풍광",
          "original_form": "맑은 하늘 시원한 온도 적당한 바람 알록달록한 풍광",
          "speaker_id": "1",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100033248.1.35",
          "form": "얼른 왔으면 좋겠네요",
          "original_form": "얼른 왔으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100033248.1.36",
          "form": "네 주말에 화창하길래 그렇게 가나했떠니",
          "original_form": "네 주말에 화창하길래 그렇게 가나했떠니",
          "speaker_id": "2",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100033248.1.37",
          "form": "또 비가 오네요",
          "original_form": "또 비가 오네요",
          "speaker_id": "2",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100033248.1.38",
          "form": "비가 그치고 이제는 완연한 가을 날씨가 되길 기대해봅니다",
          "original_form": "비가 그치고 이제는 완연한 가을 날씨가 되길 기대해봅니다",
          "speaker_id": "2",
          "time": "20210907 17:03"
        }
