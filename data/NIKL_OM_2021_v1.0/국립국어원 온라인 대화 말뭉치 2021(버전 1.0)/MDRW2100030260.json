      "utterance": [
        {
          "id": "MDRW2100030260.1.1",
          "form": "츄우르는 어디서 사나요??",
          "original_form": "츄우르는 어디서 사나요??",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030260.1.2",
          "form": "츄우르는 요즘 파는 곳이 많습니댜",
          "original_form": "츄우르는 요즘 파는 곳이 많습니댜",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030260.1.3",
          "form": "마트 편의점 다이소 등등 유통된 곳이 많아영",
          "original_form": "마트 편의점 다이소 등등 유통된 곳이 많아영",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030260.1.4",
          "form": "다이소도?????",
          "original_form": "다이소도?????",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030260.1.5",
          "form": "아 저 나중에 만나면 츄우르를 드리겠습니다",
          "original_form": "아 저 나중에 만나면 츄우르를 드리겠습니다",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030260.1.6",
          "form": "작년에 사둔게 아직도 있어오....",
          "original_form": "작년에 사둔게 아직도 있어오....",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030260.1.7",
          "form": "오오 유통기한 아직 괜찮나요?",
          "original_form": "오오 유통기한 아직 괜찮나요?",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030260.1.8",
          "form": "드셔보고 알려주세오",
          "original_form": "드셔보고 알려주세오",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030260.1.9",
          "form": "그리고 주십셔",
          "original_form": "그리고 주십셔",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030260.1.10",
          "form": "네ㅠ알겠습니다",
          "original_form": "네ㅠ알겠습니다",
          "speaker_id": "1",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030260.1.11",
          "form": "제가 대표로 먹어보겠습니댕",
          "original_form": "제가 대표로 먹어보겠습니댕",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030260.1.12",
          "form": "멍멍",
          "original_form": "멍멍",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030260.1.13",
          "form": "드셔보는....?",
          "original_form": "드셔보는....?",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030260.1.14",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030260.1.15",
          "form": "댕댕이가 타자를 치넹",
          "original_form": "댕댕이가 타자를 치넹",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030260.1.16",
          "form": "헥헥헥헥",
          "original_form": "헥헥헥헥",
          "speaker_id": "1",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100030260.1.17",
          "form": "와 이건 나쁜말 안걸린다 역시 댕댕짱",
          "original_form": "와 이건 나쁜말 안걸린다 역시 댕댕짱",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030260.1.18",
          "form": "아잌ㅋㅋㅋ",
          "original_form": "아잌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030260.1.19",
          "form": "츄우르 유통기한 아직 남았던 거 같음",
          "original_form": "츄우르 유통기한 아직 남았던 거 같음",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030260.1.20",
          "form": "오오 다행이댜",
          "original_form": "오오 다행이댜",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030260.1.21",
          "form": "그럼 댕댕이가 와서 냥이 츄우르 주십셔",
          "original_form": "그럼 댕댕이가 와서 냥이 츄우르 주십셔",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030260.1.22",
          "form": "내가 먹을 수도 없어서...",
          "original_form": "내가 먹을 수도 없어서...",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030260.1.23",
          "form": "알겠댕",
          "original_form": "알겠댕",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030260.1.24",
          "form": "댕댕이 들어가는 순간",
          "original_form": "댕댕이 들어가는 순간",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030260.1.25",
          "form": "저 근데 당댕이가 냥이만나면 죽는 거 아닌가요",
          "original_form": "저 근데 당댕이가 냥이만나면 죽는 거 아닌가요",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030260.1.26",
          "form": "히이익",
          "original_form": "히이익",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030260.1.27",
          "form": "거기는 파이트장 되는겨",
          "original_form": "거기는 파이트장 되는겨",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030260.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030260.1.29",
          "form": "츄우르를 가져왔는데 왜 먹질 못하니",
          "original_form": "츄우르를 가져왔는데 왜 먹질 못하니",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030260.1.30",
          "form": "점점 주제 멀어지는데 까일려나...",
          "original_form": "점점 주제 멀어지는데 까일려나...",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030260.1.31",
          "form": "댕첨지...!!",
          "original_form": "댕첨지...!!",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030260.1.32",
          "form": "요새 추우르 말고 고양이 간식 뭐사??",
          "original_form": "요새 추우르 말고 고양이 간식 뭐사??",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030260.1.33",
          "form": "아 뇌-절을 하고 말았어 ㅁㅇㅁㅇ",
          "original_form": "아 뇌-절을 하고 말았어 ㅁㅇㅁㅇ",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030260.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030260.1.35",
          "form": "난 거의 안사구 가서 봤떠니",
          "original_form": "난 거의 안사구 가서 봤떠니",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030260.1.36",
          "form": "뭐 트릿도 있고 국산 츄우르도 많고",
          "original_form": "뭐 트릿도 있고 국산 츄우르도 많고",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030260.1.37",
          "form": "무슨 사료처럼 생긴 간식도 있더랑",
          "original_form": "무슨 사료처럼 생긴 간식도 있더랑",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030260.1.38",
          "form": "헐 사료 간식은 몰까",
          "original_form": "헐 사료 간식은 몰까",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030260.1.39",
          "form": "장난감은??",
          "original_form": "장난감은??",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030260.1.40",
          "form": "장난감 겁나 많ㅋㅋㅋ",
          "original_form": "장난감 겁나 많ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030260.1.41",
          "form": "한마리가 장난감 집착러라 무서웠당..",
          "original_form": "한마리가 장난감 집착러라 무서웠당..",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030260.1.42",
          "form": "그 다이소에 파는 장난감들 다 있고",
          "original_form": "그 다이소에 파는 장난감들 다 있고",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030260.1.43",
          "form": "뭔가 종류가 다양했음",
          "original_form": "뭔가 종류가 다양했음",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030260.1.44",
          "form": "물고기 인형도 있꾸",
          "original_form": "물고기 인형도 있꾸",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030260.1.45",
          "form": "장난감 집착러 ㅋㅋㅋㅋ혹시 저 이쁜이???",
          "original_form": "장난감 집착러 ㅋㅋㅋㅋ혹시 저 이쁜이???",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030260.1.46",
          "form": "다이소 장난감 퀄리티 괜찮아???",
          "original_form": "다이소 장난감 퀄리티 괜찮아???",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030260.1.47",
          "form": "다이소 장난감 다양하긴 하더라",
          "original_form": "다이소 장난감 다양하긴 하더라",
          "speaker_id": "1",
          "time": "20210903 23:35"
        }
      ]
