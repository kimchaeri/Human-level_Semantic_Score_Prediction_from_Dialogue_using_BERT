      "utterance": [
        {
          "id": "MDRW2100038272.1.1",
          "form": "학창시절 좋아하는 과목이 뭐였나요?",
          "original_form": "학창시절 좋아하는 과목이 뭐였나요?",
          "speaker_id": "1",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100038272.1.2",
          "form": "전 국어요!",
          "original_form": "전 국어요!",
          "speaker_id": "2",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100038272.1.3",
          "form": "어떤거 좋아하셨어요?",
          "original_form": "어떤거 좋아하셨어요?",
          "speaker_id": "2",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100038272.1.4",
          "form": "문학소녀 느낌이네요",
          "original_form": "문학소녀 느낌이네요",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100038272.1.5",
          "form": "전 체육이요",
          "original_form": "전 체육이요",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100038272.1.6",
          "form": "운동장에서 뛰노는 걸 좋아했어요",
          "original_form": "운동장에서 뛰노는 걸 좋아했어요",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100038272.1.7",
          "form": "아 체육소녀!",
          "original_form": "아 체육소녀!",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100038272.1.8",
          "form": "운동 잘하셨군요!",
          "original_form": "운동 잘하셨군요!",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100038272.1.9",
          "form": "운동신경이 없는 편은 아니었고 그냥 공부보다는 몸으로 하는 걸 좋아하는 타입이었죠 ㅎㅎ",
          "original_form": "운동신경이 없는 편은 아니었고 그냥 공부보다는 몸으로 하는 걸 좋아하는 타입이었죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100038272.1.10",
          "form": "전 체력장 늘 꼴찌ㅋㅋ",
          "original_form": "전 체력장 늘 꼴찌ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100038272.1.11",
          "form": "저랑 반대인 듯 하네요",
          "original_form": "저랑 반대인 듯 하네요",
          "speaker_id": "1",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100038272.1.12",
          "form": "책을 좋아하니 성적 좋았을 거 같아요",
          "original_form": "책을 좋아하니 성적 좋았을 거 같아요",
          "speaker_id": "1",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100038272.1.13",
          "form": "전 성적 기억도 안나네요 ㅎㅎ",
          "original_form": "전 성적 기억도 안나네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100038272.1.14",
          "form": "ㅋㅋ 뭐든 잘하면 장땡이죠ㅋㅋ",
          "original_form": "ㅋㅋ 뭐든 잘하면 장땡이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100038272.1.15",
          "form": "학교때 추억이 진짜 많아요",
          "original_form": "학교때 추억이 진짜 많아요",
          "speaker_id": "2",
          "time": "20210906 19:59"
        },
        {
          "id": "MDRW2100038272.1.16",
          "form": "어떤게 또 기억이 나나요?",
          "original_form": "어떤게 또 기억이 나나요?",
          "speaker_id": "1",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100038272.1.17",
          "form": "전 야자때 밥먹고 잤던 기억ㅋㅋ",
          "original_form": "전 야자때 밥먹고 잤던 기억ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:01"
        },
        {
          "id": "MDRW2100038272.1.18",
          "form": "그랬군요",
          "original_form": "그랬군요",
          "speaker_id": "1",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100038272.1.19",
          "form": "생각해보면 어린 시절이 순수해서 그런건지",
          "original_form": "생각해보면 어린 시절이 순수해서 그런건지",
          "speaker_id": "1",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100038272.1.20",
          "form": "아니면 지나간 추억이라서 그런건지",
          "original_form": "아니면 지나간 추억이라서 그런건지",
          "speaker_id": "1",
          "time": "20210906 20:03"
        },
        {
          "id": "MDRW2100038272.1.21",
          "form": "다닐땐 몰랐는데 지금에 와서 돌이켜보면 참 그립네요",
          "original_form": "다닐땐 몰랐는데 지금에 와서 돌이켜보면 참 그립네요",
          "speaker_id": "1",
          "time": "20210906 20:04"
        },
        {
          "id": "MDRW2100038272.1.22",
          "form": "그러니까요 그때 친구들 만나도 그때 이야기하고ㅎㅎ",
          "original_form": "그러니까요 그때 친구들 만나도 그때 이야기하고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:05"
        },
        {
          "id": "MDRW2100038272.1.23",
          "form": "이런 시절도 있었지 하면서 대화하면 동심으로 돌아간 느낌이죠",
          "original_form": "이런 시절도 있었지 하면서 대화하면 동심으로 돌아간 느낌이죠",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100038272.1.24",
          "form": "그래서 요즘 학생들이 좀 안돼 보여요",
          "original_form": "그래서 요즘 학생들이 좀 안돼 보여요",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100038272.1.25",
          "form": "코로나로 마스크 쓰고 수업받고 마음껏 뛰놀지도 못하고",
          "original_form": "코로나로 마스크 쓰고 수업받고 마음껏 뛰놀지도 못하고",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100038272.1.26",
          "form": "그러니까요 애기들 너무 불쌍해요 새내기들도 그러구요ㅜㅠ",
          "original_form": "그러니까요 애기들 너무 불쌍해요 새내기들도 그러구요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210906 20:10"
        }
      ]
    }
