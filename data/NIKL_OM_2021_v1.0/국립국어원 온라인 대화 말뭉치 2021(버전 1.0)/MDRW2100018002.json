      "utterance": [
        {
          "id": "MDRW2100018002.1.1",
          "form": "옥수수가 약간 겉이 말랐어",
          "original_form": "옥수수가 약간 겉이 말랐어",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100018002.1.2",
          "form": "옥수수에 토마토주스 조합 괜찮은거 같은데요",
          "original_form": "옥수수에 토마토주스 조합 괜찮은거 같은데요",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100018002.1.3",
          "form": "옥수수가 마른건 어쩔수가 없답니다",
          "original_form": "옥수수가 마른건 어쩔수가 없답니다",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018002.1.4",
          "form": "우리가 키운 유기농이라 맛있지",
          "original_form": "우리가 키운 유기농이라 맛있지",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018002.1.5",
          "form": "토마토주스도 갈기는 귀찮지만 맛은 좋아요",
          "original_form": "토마토주스도 갈기는 귀찮지만 맛은 좋아요",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018002.1.6",
          "form": "나는 그냥 토마토는 별로야 갈아야 먹지",
          "original_form": "나는 그냥 토마토는 별로야 갈아야 먹지",
          "speaker_id": "2",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018002.1.7",
          "form": "신선은 한데 좀 귀찮아요",
          "original_form": "신선은 한데 좀 귀찮아요",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018002.1.8",
          "form": "그냥 먹어도 맛있는데요",
          "original_form": "그냥 먹어도 맛있는데요",
          "speaker_id": "1",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100018002.1.9",
          "form": "그래도 많이 먹기엔 갈아 마시는게 좋아",
          "original_form": "그래도 많이 먹기엔 갈아 마시는게 좋아",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100018002.1.10",
          "form": "설탕 소금 어느쪽이 좋으신가요",
          "original_form": "설탕 소금 어느쪽이 좋으신가요",
          "speaker_id": "1",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100018002.1.11",
          "form": "나는 단게 싫어",
          "original_form": "나는 단게 싫어",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100018002.1.12",
          "form": "예전에는 설탕 많이 넣어 먹었는데 기억안나시나보네",
          "original_form": "예전에는 설탕 많이 넣어 먹었는데 기억안나시나보네",
          "speaker_id": "1",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100018002.1.13",
          "form": "예전엔 그랬지 국물 마시고 달달했지",
          "original_form": "예전엔 그랬지 국물 마시고 달달했지",
          "speaker_id": "2",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100018002.1.14",
          "form": "소금은 좀 별로일거 같아요 설탕이 낫지요",
          "original_form": "소금은 좀 별로일거 같아요 설탕이 낫지요",
          "speaker_id": "1",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100018002.1.15",
          "form": "토마토 달걀 넣고 볶은거 그것도 괜찮던데",
          "original_form": "토마토 달걀 넣고 볶은거 그것도 괜찮던데",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100018002.1.16",
          "form": "맞아요 토달볶 좀 맛있어요 귀찮아서 그렇지만 맛있죠",
          "original_form": "맞아요 토달볶 좀 맛있어요 귀찮아서 그렇지만 맛있죠",
          "speaker_id": "1",
          "time": "20210826 22:09"
        }
