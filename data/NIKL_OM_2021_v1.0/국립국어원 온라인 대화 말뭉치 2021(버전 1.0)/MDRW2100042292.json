      "utterance": [
        {
          "id": "MDRW2100042292.1.1",
          "form": "육퇴했나?",
          "original_form": "육퇴했나?",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042292.1.2",
          "form": "오늘 저녁은 뭐 줬어요 애들??",
          "original_form": "오늘 저녁은 뭐 줬어요 애들??",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042292.1.3",
          "form": "지는 예전에 육했지요",
          "original_form": "지는 예전에 육했지요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042292.1.4",
          "form": "지코 맛있겠어요",
          "original_form": "지코 맛있겠어요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042292.1.5",
          "form": "지코바",
          "original_form": "지코바",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042292.1.6",
          "form": "지코바 맛있겠네",
          "original_form": "지코바 맛있겠네",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042292.1.7",
          "form": "지금 이 시간에 뭘 먹어도 맛있지요",
          "original_form": "지금 이 시간에 뭘 먹어도 맛있지요",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042292.1.8",
          "form": "갑자기 지코바고ㅋㅋㅋ",
          "original_form": "갑자기 지코바고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042292.1.9",
          "form": "언니 저녁 뭐 먹었어요",
          "original_form": "언니 저녁 뭐 먹었어요",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042292.1.10",
          "form": "갑자기 지코가 먹고 싶었어 용",
          "original_form": "갑자기 지코가 먹고 싶었어 용",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042292.1.11",
          "form": "난 일단 씻고싶다ㅋ",
          "original_form": "난 일단 씻고싶다ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042292.1.12",
          "form": "우리,",
          "original_form": "우리,",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042292.1.13",
          "form": "분식먹음",
          "original_form": "분식먹음",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042292.1.14",
          "form": "뭐한다고 아직 씻지도 않았어요",
          "original_form": "뭐한다고 아직 씻지도 않았어요",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042292.1.15",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042292.1.16",
          "form": "애들은 숙제하고 잘건데",
          "original_form": "애들은 숙제하고 잘건데",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042292.1.17",
          "form": "자는듸ㅣ시간이 많이걸려서 내일부터 30분 늦출까싶다",
          "original_form": "자는듸ㅣ시간이 많이걸려서 내일부터 30분 늦출까싶다",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042292.1.18",
          "form": "누워서 뒹글거리는시간이 너무많음",
          "original_form": "누워서 뒹글거리는시간이 너무많음",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042292.1.19",
          "form": "잠이 안오는데 잘하면 그렇죠",
          "original_form": "잠이 안오는데 잘하면 그렇죠",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042292.1.20",
          "form": "그래 내일부터 좀더 잠이오도록",
          "original_form": "그래 내일부터 좀더 잠이오도록",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042292.1.21",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100042292.1.22",
