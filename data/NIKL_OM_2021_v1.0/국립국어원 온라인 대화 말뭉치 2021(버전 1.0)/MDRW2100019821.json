          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019821.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019821.1.13",
          "form": "뭐 설마 그거?",
          "original_form": "뭐 설마 그거?",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019821.1.14",
          "form": "이나영 나오는 드라마?",
          "original_form": "이나영 나오는 드라마?",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019821.1.15",
          "form": "ㄴㄴㄴㄴ당잠사",
          "original_form": "ㄴㄴㄴㄴ당잠사",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019821.1.16",
          "form": "계란후라이 맛있겠다",
          "original_form": "계란후라이 맛있겠다",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019821.1.17",
          "form": "요새 계란이 금값이야~",
          "original_form": "요새 계란이 금값이야~",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019821.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019821.1.19",
