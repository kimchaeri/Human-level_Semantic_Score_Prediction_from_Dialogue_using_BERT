      "utterance": [
        {
          "id": "MDRW2100020661.1.1",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "1",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020661.1.2",
          "form": "네 드셨어요?",
          "original_form": "네 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020661.1.3",
          "form": "네 친구가 베이글해줘서",
          "original_form": "네 친구가 베이글해줘서",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020661.1.4",
          "form": "먹었네요",
          "original_form": "먹었네요",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020661.1.5",
          "form": "오 맛있었겠네요",
          "original_form": "오 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020661.1.6",
          "form": "점심은 뭐드세요?",
          "original_form": "점심은 뭐드세요?",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020661.1.7",
          "form": "떡볶이 먹을꺼같앙ㆍ",
          "original_form": "떡볶이 먹을꺼같앙ㆍ",
          "speaker_id": "1",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020661.1.8",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020661.1.9",
          "form": "오! 저도 떡볶이 먹으러 가려구요",
          "original_form": "오! 저도 떡볶이 먹으러 가려구요",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020661.1.10",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020661.1.11",
          "form": "여기양념이단거같기도하고",
          "original_form": "여기양념이단거같기도하고",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020661.1.12",
          "form": "어떤 스타일 좋아하시나요?",
          "original_form": "어떤 스타일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 13:05"
        },
        {
          "id": "MDRW2100020661.1.13",
          "form": "달지않은거",
          "original_form": "달지않은거",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020661.1.14",
          "form": "좋아행ᆢ",
          "original_form": "좋아행ᆢ",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020661.1.15",
          "form": "많이 다셨나봐요",
          "original_form": "많이 다셨나봐요",
          "speaker_id": "2",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100020661.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100020661.1.17",
          "form": "물을 어찌 그리많이 마셨는지",
          "original_form": "물을 어찌 그리많이 마셨는지",
          "speaker_id": "1",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100020661.1.18",
          "form": "ㅜㅜ거기 가심 안되겠네요",
          "original_form": "ㅜㅜ거기 가심 안되겠네요",
          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020661.1.19",
          "form": "점심인데",
          "original_form": "점심인데",
          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020661.1.20",
          "form": "그러겟네요",
          "original_form": "그러겟네요",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020661.1.21",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020661.1.22",
          "form": "제가 다 속상하네요",
          "original_form": "제가 다 속상하네요",
          "speaker_id": "2",
          "time": "20210828 13:33"
        },
        {
          "id": "MDRW2100020661.1.23",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "1",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100020661.1.24",
          "form": "지금은 뭐 드세요?",
          "original_form": "지금은 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100020661.1.25",
          "form": "저 막국수 먹으러 왔어요ㅋㅋㅋ",
          "original_form": "저 막국수 먹으러 왔어요ㅋㅋㅋ",
