      "utterance": [
        {
          "id": "MDRW2100044841.1.1",
          "form": "요즘 배달비가 오천원도 넘는 곳이 있더라고요",
          "original_form": "요즘 배달비가 오천원도 넘는 곳이 있더라고요",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044841.1.2",
          "form": "그러게 말이에요",
          "original_form": "그러게 말이에요",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044841.1.3",
          "form": "그게 거리가 멀면",
          "original_form": "그게 거리가 멀면",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044841.1.4",
          "form": "그렇게 나오더라구요",
          "original_form": "그렇게 나오더라구요",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044841.1.5",
          "form": "엽떡 방문포장 할인 없어진거 아셨어요?",
          "original_form": "엽떡 방문포장 할인 없어진거 아셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044841.1.6",
          "form": "몰랐어요",
          "original_form": "몰랐어요",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044841.1.7",
          "form": "와 너무하네요 찾아가면 할인좀 해줘야되는거 아닌가여",
          "original_form": "와 너무하네요 찾아가면 할인좀 해줘야되는거 아닌가여",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044841.1.8",
          "form": "원래 3천원 빼줬는데",
          "original_form": "원래 3천원 빼줬는데",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044841.1.9",
          "form": "6월말일자로 없어졌어여ㅠ",
          "original_form": "6월말일자로 없어졌어여ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044841.1.10",
          "form": "와 많이 빼줬네요",
          "original_form": "와 많이 빼줬네요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044841.1.11",
          "form": "저 어느 떡볶이집인지 기억은 안나는데 거기 오백원 빼줬어요",
          "original_form": "저 어느 떡볶이집인지 기억은 안나는데 거기 오백원 빼줬어요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044841.1.12",
          "form": "오..백원이라니...ㅠㅠ",
          "original_form": "오..백원이라니...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044841.1.13",
          "form": "근데 사라졌다니...",
          "original_form": "근데 사라졌다니...",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044841.1.14",
          "form": "본사에서 몇년해준것 같았는데",
          "original_form": "본사에서 몇년해준것 같았는데",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044841.1.15",
          "form": "치사하죠??",
          "original_form": "치사하죠??",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044841.1.16",
          "form": "지난주 토요일에 가니깐...",
          "original_form": "지난주 토요일에 가니깐...",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044841.1.17",
          "form": "없어졌다고 매번 시켜먹다가ㅠㅠ",
          "original_form": "없어졌다고 매번 시켜먹다가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044841.1.18",
          "form": "오백원은 배떡 원조라고 불리는 떡볶이 체인이었어요",
          "original_form": "오백원은 배떡 원조라고 불리는 떡볶이 체인이었어요",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044841.1.19",
          "form": "그나마 배달비가 천원이라서ㅠㅠ 다행이지.ㅠㅠ",
          "original_form": "그나마 배달비가 천원이라서ㅠㅠ 다행이지.ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044841.1.20",
          "form": "아 !!!",
          "original_form": "아 !!!",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044841.1.21",
          "form": "슬퍼여 ㅠㅠ",
          "original_form": "슬퍼여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044841.1.22",
          "form": "천원이어도 천원인데",
          "original_form": "천원이어도 천원인데",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044841.1.23",
          "form": "아 ㅋㅋ 누가 그럼 포장하나여ㅠㅠ",
          "original_form": "아 ㅋㅋ 누가 그럼 포장하나여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044841.1.24",
          "form": "배달로 묵고 말지,",
          "original_form": "배달로 묵고 말지,",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044841.1.25",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044841.1.26",
          "form": "얍떡 먹고싶네여ㅠㅠ 목요일에 친구랑 먹기로 했는데",
          "original_form": "얍떡 먹고싶네여ㅠㅠ 목요일에 친구랑 먹기로 했는데",
          "speaker_id": "1",
          "time": "20210908 00:00"
        }
      ]
    }
