      "utterance": [
        {
          "id": "MDRW2100013321.1.1",
          "form": "저는 고양이 키우고싶아요",
          "original_form": "저는 고양이 키우고싶아요",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013321.1.2",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013321.1.3",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013321.1.4",
          "form": "털이 복실복실하고 부드럽고 껴안으면 푸짐한 그런고양이 없나요",
          "original_form": "털이 복실복실하고 부드럽고 껴안으면 푸짐한 그런고양이 없나요",
          "speaker_id": "2",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013321.1.5",
          "form": "냥냥",
          "original_form": "냥냥",
          "speaker_id": "1",
          "time": "20210825 14:02"
        },
        {
          "id": "MDRW2100013321.1.6",
          "form": "없네요",
          "original_form": "없네요",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013321.1.7",
          "form": "유쾌하신 분이시네요",
          "original_form": "유쾌하신 분이시네요",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013321.1.8",
          "form": "얼마전에 유투브에서 고양이영상을봤는데",
          "original_form": "얼마전에 유투브에서 고양이영상을봤는데",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013321.1.9",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013321.1.10",
          "form": "너무 귀엽더라고요.볼때마다 키우고싶고",
          "original_form": "너무 귀엽더라고요.볼때마다 키우고싶고",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013321.1.11",
          "form": "어떤 품종 좋아하시는지요?",
          "original_form": "어떤 품종 좋아하시는지요?",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013321.1.12",
          "form": "고민되는 일화들이 은근 있는것같아요",
          "original_form": "고민되는 일화들이 은근 있는것같아요",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013321.1.13",
          "form": "와웅",
          "original_form": "와웅",
          "speaker_id": "1",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013321.1.14",
          "form": "저는 귀여우면 다 오케이입니다",
          "original_form": "저는 귀여우면 다 오케이입니다",
          "speaker_id": "2",
          "time": "20210825 14:03"
        },
        {
          "id": "MDRW2100013321.1.15",
          "form": "브리티쉬 쇼트 고양이 좋아해요",
          "original_form": "브리티쉬 쇼트 고양이 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013321.1.16",
          "form": "스핑크스도 귀여우면 어때요?",
          "original_form": "스핑크스도 귀여우면 어때요?",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013321.1.17",
          "form": "좋나요?",
          "original_form": "좋나요?",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013321.1.18",
          "form": "깔끔하긴하겠네요",
          "original_form": "깔끔하긴하겠네요",
          "speaker_id": "2",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013321.1.19",
          "form": "스핑크스도 털이 짧지 않나요?",
          "original_form": "스핑크스도 털이 짧지 않나요?",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013321.1.20",
          "form": "털이 짧으면 따가울텐데",
          "original_form": "털이 짧으면 따가울텐데",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013321.1.21",
          "form": "그래도 저는 털있는 고양이가 더 좋아요",
          "original_form": "그래도 저는 털있는 고양이가 더 좋아요",
