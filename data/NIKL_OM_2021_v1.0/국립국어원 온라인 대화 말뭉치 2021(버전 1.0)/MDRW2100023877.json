      "utterance": [
        {
          "id": "MDRW2100023877.1.1",
          "form": "안녕하세용 저녁은 드셨나요~?",
          "original_form": "안녕하세용 저녁은 드셨나요~?",
          "speaker_id": "1",
          "time": "20210830 19:33"
        },
        {
          "id": "MDRW2100023877.1.2",
          "form": "네~~~~~~~ name1도 드셨나요?",
          "original_form": "네~~~~~~~ &name1&도 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100023877.1.3",
          "form": "네네! 저는 카레 먹었어요호",
          "original_form": "네네! 저는 카레 먹었어요호",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100023877.1.4",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210830 19:34"
        },
        {
          "id": "MDRW2100023877.1.5",
          "form": "저는 치킨먹었어요 ㅎㅎ",
          "original_form": "저는 치킨먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023877.1.6",
          "form": "이야아",
          "original_form": "이야아",
          "speaker_id": "1",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023877.1.7",
          "form": "어디치킨 드셨나요~?",
          "original_form": "어디치킨 드셨나요~?",
          "speaker_id": "1",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023877.1.8",
          "form": "페리카나요!!!",
          "original_form": "페리카나요!!!",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023877.1.9",
          "form": "좋아하는 치킨브랜드 있으세요?",
          "original_form": "좋아하는 치킨브랜드 있으세요?",
          "speaker_id": "2",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023877.1.10",
          "form": "페리카나면 양념인가여??",
          "original_form": "페리카나면 양념인가여??",
          "speaker_id": "1",
          "time": "20210830 19:35"
        },
        {
          "id": "MDRW2100023877.1.11",
          "form": "저는 치킨매니아 새우치킨 좋아해요!",
          "original_form": "저는 치킨매니아 새우치킨 좋아해요!",
          "speaker_id": "1",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023877.1.12",
          "form": "넵~",
          "original_form": "넵~",
          "speaker_id": "2",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023877.1.13",
          "form": "오 그거 먹어보고 싶었는데 ...!!",
          "original_form": "오 그거 먹어보고 싶었는데 ...!!",
          "speaker_id": "2",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023877.1.14",
          "form": "크으 역시 저도 양념은 무조건 페리카나시켜요ㅋㅋㅋ",
          "original_form": "크으 역시 저도 양념은 무조건 페리카나시켜요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023877.1.15",
          "form": "이거 되게 맛있어요 새우랑 궁합도 잘맞고ㅎㅎ",
          "original_form": "이거 되게 맛있어요 새우랑 궁합도 잘맞고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023877.1.16",
          "form": "다음에 시켜먹어봐야겠어요",
          "original_form": "다음에 시켜먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023877.1.17",
          "form": "궁금하네요ㅎㅎ",
          "original_form": "궁금하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:36"
        },
        {
          "id": "MDRW2100023877.1.18",
          "form": "추천드립니당ㅎㅎ",
          "original_form": "추천드립니당ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023877.1.19",
          "form": "카레에 고기들어간거 좋아하세요?",
          "original_form": "카레에 고기들어간거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023877.1.20",
          "form": "네! 카레에 고기 필수 아닌가요ㅎㅎ",
          "original_form": "네! 카레에 고기 필수 아닌가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:37"
        },
        {
          "id": "MDRW2100023877.1.21",
          "form": "오 역시 맛잘알이시네요ㅎㅎㅎ",
          "original_form": "오 역시 맛잘알이시네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023877.1.22",
          "form": "저도 고기가득 넣어먹었어요~",
          "original_form": "저도 고기가득 넣어먹었어요~",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023877.1.23",
          "form": "맛있으셨겠어요~~",
          "original_form": "맛있으셨겠어요~~",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023877.1.24",
          "form": "네네ㅋㅋㅋㅋ 맛있더라구요 오랜만에 먹어서 그런가",
          "original_form": "네네ㅋㅋㅋㅋ 맛있더라구요 오랜만에 먹어서 그런가",
          "speaker_id": "1",
