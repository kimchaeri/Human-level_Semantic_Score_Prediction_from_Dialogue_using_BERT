      "utterance": [
        {
          "id": "MDRW2100030574.1.1",
          "form": "문송안함도 보려고 하는데",
          "original_form": "문송안함도 보려고 하는데",
          "speaker_id": "1",
          "time": "20210904 11:48"
        },
        {
          "id": "MDRW2100030574.1.2",
          "form": "이제 드래곤길들이기 책 3권만 읽으면 됨 ㅋㅋㅋㅋ",
          "original_form": "이제 드래곤길들이기 책 3권만 읽으면 됨 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:48"
        },
        {
          "id": "MDRW2100030574.1.3",
          "form": "오 거의 다 봣네 ㅋㅋㅋㅋ",
          "original_form": "오 거의 다 봣네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100030574.1.4",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 13:08"
        },
        {
          "id": "MDRW2100030574.1.5",
          "form": "30분이면 읽는데",
          "original_form": "30분이면 읽는데",
          "speaker_id": "1",
          "time": "20210904 13:08"
        },
        {
          "id": "MDRW2100030574.1.6",
          "form": "내용이 어려운게 아니니까",
          "original_form": "내용이 어려운게 아니니까",
          "speaker_id": "1",
          "time": "20210904 13:08"
        },
        {
          "id": "MDRW2100030574.1.7",
          "form": "장수가 많아서 그렇지 ㅋㅋ",
          "original_form": "장수가 많아서 그렇지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:11"
        },
        {
          "id": "MDRW2100030574.1.8",
          "form": "그림이 절반이야 ㅋㅋㅋ",
          "original_form": "그림이 절반이야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030574.1.9",
          "form": "어린이가 그려둔것 같은 그런 그림이라 엄청 예쁜 그림은 아니거든",
          "original_form": "어린이가 그려둔것 같은 그런 그림이라 엄청 예쁜 그림은 아니거든",
          "speaker_id": "1",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030574.1.10",
          "form": "이거 내용이 되게 재미진게",
          "original_form": "이거 내용이 되게 재미진게",
          "speaker_id": "1",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030574.1.11",
          "form": "히컵이 드래곤 말을 할줄알아",
          "original_form": "히컵이 드래곤 말을 할줄알아",
          "speaker_id": "1",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030574.1.12",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030574.1.13",
          "form": "서로 언어로 소통을 해?",
          "original_form": "서로 언어로 소통을 해?",
          "speaker_id": "2",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030574.1.14",
          "form": "영화버전은 확실히 캐릭터만 차용한거 같더라",
          "original_form": "영화버전은 확실히 캐릭터만 차용한거 같더라",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030574.1.15",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030574.1.16",
          "form": "히컵이 드래곤 언어로 대화를해",
          "original_form": "히컵이 드래곤 언어로 대화를해",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030574.1.17",
          "form": "유일하게 드래곤이랑 말을 할줄알아",
          "original_form": "유일하게 드래곤이랑 말을 할줄알아",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030574.1.18",
          "form": "그래서 드래곤들이 말하는거도 묘사가 되어있거든",
          "original_form": "그래서 드래곤들이 말하는거도 묘사가 되어있거든",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030574.1.19",
          "form": "재능잇는 아이엿네 ㅋㅋㅋ",
          "original_form": "재능잇는 아이엿네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030574.1.20",
          "form": "그것도 유일무이한ㅋㅋ",
          "original_form": "그것도 유일무이한ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030574.1.21",
          "form": "그리고 칼싸움을 못하고",
          "original_form": "그리고 칼싸움을 못하고",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030574.1.22",
          "form": "작고 볼품없어보인다고 하는데",
          "original_form": "작고 볼품없어보인다고 하는데",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030574.1.23",
          "form": "그게 사실 우리애가 왼손잡이에요",
          "original_form": "그게 사실 우리애가 왼손잡이에요",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030574.1.24",
          "form": "불세출 천잰데?",
          "original_form": "불세출 천잰데?",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030574.1.25",
          "form": "오른손으로 할라니까 못하는거였어",
          "original_form": "오른손으로 할라니까 못하는거였어",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030574.1.26",
          "form": "알고보니 이세계 힘숨찐이었다",
          "original_form": "알고보니 이세계 힘숨찐이었다",
          "speaker_id": "2",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100030574.1.27",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030574.1.28",
          "form": "책 표지 보면",
          "original_form": "책 표지 보면",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030574.1.29",
          "form": "위대한 바이킹이라고 하거든",
          "original_form": "위대한 바이킹이라고 하거든",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030574.1.30",
          "form": "위대한 해적의 후손이자 후계자로",
          "original_form": "위대한 해적의 후손이자 후계자로",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030574.1.31",
          "form": "피시래그는 되게 마르고",
          "original_form": "피시래그는 되게 마르고",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030574.1.32",
          "form": "눈이 사시야",
          "original_form": "눈이 사시야",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030574.1.33",
          "form": "역시 그아빠에 그아들인거지",
          "original_form": "역시 그아빠에 그아들인거지",
          "speaker_id": "2",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100030574.1.34",
          "form": "영화에선 퉁퉁이 아니엇나",
          "original_form": "영화에선 퉁퉁이 아니엇나",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030574.1.35",
          "form": "이친구 몸집이 크긴 했지",
          "original_form": "이친구 몸집이 크긴 했지",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030574.1.36",
          "form": "미니 드래곤 안고 다니고 ㅋㅋㅋ",
          "original_form": "미니 드래곤 안고 다니고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030574.1.37",
          "form": "겁도 많고",
          "original_form": "겁도 많고",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030574.1.38",
          "form": "아는거 많은 사전급인 친구지",
          "original_form": "아는거 많은 사전급인 친구지",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030574.1.39",
          "form": "책에선 여자 등장인물을 아직 못봤거든",
          "original_form": "책에선 여자 등장인물을 아직 못봤거든",
          "speaker_id": "1",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030574.1.40",
          "form": "그래도 생긴게 퉁퉁이 정도인 친구는 아닌데..",
          "original_form": "그래도 생긴게 퉁퉁이 정도인 친구는 아닌데..",
          "speaker_id": "1",
          "time": "20210904 13:26"
        }
      ]
    }
