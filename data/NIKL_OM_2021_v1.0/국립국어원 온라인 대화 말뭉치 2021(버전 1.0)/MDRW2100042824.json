      "utterance": [
        {
          "id": "MDRW2100042824.1.1",
          "form": "일하고 있어요?",
          "original_form": "일하고 있어요?",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042824.1.2",
          "form": "일하고 있지",
          "original_form": "일하고 있지",
          "speaker_id": "1",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042824.1.3",
          "form": "지는 도착요 ㅋㅋㅋㅋ",
          "original_form": "지는 도착요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042824.1.4",
          "form": "도착했나??",
          "original_form": "도착했나??",
          "speaker_id": "1",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042824.1.5",
          "form": "10시까지 10분만 쉬어야지",
          "original_form": "10시까지 10분만 쉬어야지",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042824.1.6",
          "form": "그래 이제 열심히 박스를 열어라",
          "original_form": "그래 이제 열심히 박스를 열어라",
          "speaker_id": "1",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042824.1.7",
          "form": "언니 덥네요ㅋㅋㅋ",
          "original_form": "언니 덥네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042824.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042824.1.9",
          "form": "그래 10분 은 쉬어야지",
          "original_form": "그래 10분 은 쉬어야지",
          "speaker_id": "1",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042824.1.10",
          "form": "오늘도 결국은 마스크 찾았네",
          "original_form": "오늘도 결국은 마스크 찾았네",
          "speaker_id": "2",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042824.1.11",
          "form": "정 시부터 일해라",
          "original_form": "정 시부터 일해라",
          "speaker_id": "1",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042824.1.12",
          "form": "마스크",
          "original_form": "마스크",
          "speaker_id": "2",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042824.1.13",
          "form": "마스크를 찾았다고??",
          "original_form": "마스크를 찾았다고??",
          "speaker_id": "1",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042824.1.14",
          "form": "축툭해 졌어요",
          "original_form": "축툭해 졌어요",
          "speaker_id": "2",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042824.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㄱㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㄱㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042824.1.16",
          "form": "그래 ㅋㅋㅋㅋㄱ알아들었다",
          "original_form": "그래 ㅋㅋㅋㅋㄱ알아들었다",
          "speaker_id": "1",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042824.1.17",
          "form": "그 글씨가 또 안되네요ㅋㅋㅋㅋ",
          "original_form": "그 글씨가 또 안되네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042824.1.18",
          "form": "꼬로 마다 꼬로 네",
          "original_form": "꼬로 마다 꼬로 네",
          "speaker_id": "2",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042824.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ쉽지않다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ쉽지않다",
          "speaker_id": "1",
          "time": "20210907 09:52"
        },
        {
          "id": "MDRW2100042824.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:52"
        },
        {
          "id": "MDRW2100042824.1.21",
          "form": "나도 계속 말했는데",
          "original_form": "나도 계속 말했는데",
          "speaker_id": "1",
          "time": "20210907 09:52"
        },
        {
          "id": "MDRW2100042824.1.22",
          "form": "나쁘다고 해서",
          "original_form": "나쁘다고 해서",
          "speaker_id": "1",
          "time": "20210907 09:52"
        },
        {
          "id": "MDRW2100042824.1.23",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210907 09:52"
        },
        {
          "id": "MDRW2100042824.1.24",
          "form": "단어를 찼다 찼다",
          "original_form": "단어를 찼다 찼다",
          "speaker_id": "2",
          "time": "20210907 09:52"
        },
        {
          "id": "MDRW2100042824.1.25",
          "form": "진이 빠지는구만요ㅋㅋ",
          "original_form": "진이 빠지는구만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:52"
        },
        {
          "id": "MDRW2100042824.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:53"
        },
        {
          "id": "MDRW2100042824.1.27",
          "form": "이참에 언어 순화를 해보자",
          "original_form": "이참에 언어 순화를 해보자",
          "speaker_id": "1",
          "time": "20210907 09:53"
        },
        {
          "id": "MDRW2100042824.1.28",
          "form": "그때 해 보았잖아요 언니",
          "original_form": "그때 해 보았잖아요 언니",
          "speaker_id": "2",
          "time": "20210907 09:53"
        },
        {
          "id": "MDRW2100042824.1.29",
          "form": "너무나 스트레스였네요ㅋㅋㅋㅋㅋ",
