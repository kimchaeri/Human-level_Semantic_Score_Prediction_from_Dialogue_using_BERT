      "utterance": [
        {
          "id": "MDRW2100030239.1.1",
          "form": "냉장고는 뭘로 채워져있는게 좋아?",
          "original_form": "냉장고는 뭘로 채워져있는게 좋아?",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.2",
          "form": "1. 맛있는 음료수로 채워져잇다",
          "original_form": "1. 맛있는 음료수로 채워져잇다",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.3",
          "form": "음.. 냉장고가 고기로 채워져잇으면",
          "original_form": "음.. 냉장고가 고기로 채워져잇으면",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.4",
          "form": "2. 과자로 채워져잇다",
          "original_form": "2. 과자로 채워져잇다",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.5",
          "form": "되게만족스러울것같애 ㅋㅋ",
          "original_form": "되게만족스러울것같애 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.6",
          "form": "고기는 일부러 뺏어",
          "original_form": "고기는 일부러 뺏어",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.7",
          "form": "밸런스 파괴잖아",
          "original_form": "밸런스 파괴잖아",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.8",
          "form": "그럿구만 ㅜ 그게 중요한디",
          "original_form": "그럿구만 ㅜ 그게 중요한디",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.9",
          "form": "난 1번 ㅋㅋ",
          "original_form": "난 1번 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.10",
          "form": "과자는 안시원해도 맛잇어",
          "original_form": "과자는 안시원해도 맛잇어",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.11",
          "form": "음료수는 시원해야지 ㅋㅋ",
          "original_form": "음료수는 시원해야지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.12",
          "form": "흠.. 그렇지만 무한대로 나온다는게",
          "original_form": "흠.. 그렇지만 무한대로 나온다는게",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.13",
          "form": "중요한거아냐?",
          "original_form": "중요한거아냐?",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030239.1.14",
          "form": "그런감?",
          "original_form": "그런감?",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030239.1.15",
          "form": "머 각자 좋아하는게 다르갯지 ㅎㅎ",
          "original_form": "머 각자 좋아하는게 다르갯지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030239.1.16",
          "form": "웅 음료수는 뭐든지 거기서 거기지만은 과자는",
          "original_form": "웅 음료수는 뭐든지 거기서 거기지만은 과자는",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030239.1.17",
          "form": "여러종류 먹음 좋자나 ㅎㅎ",
          "original_form": "여러종류 먹음 좋자나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030239.1.18",
          "form": "그럴수있지 ㅎㅎ",
          "original_form": "그럴수있지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030239.1.19",
          "form": "이런 사고방식!!!!!",
          "original_form": "이런 사고방식!!!!!",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030239.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030239.1.21",
          "form": "난 음료수는 커피가 좋아ㅋ",
          "original_form": "난 음료수는 커피가 좋아ㅋ",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030239.1.22",
          "form": "젤 좋아하는 과자는 모야?",
          "original_form": "젤 좋아하는 과자는 모야?",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030239.1.23",
          "form": "감자칩이 좋와",
          "original_form": "감자칩이 좋와",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030239.1.24",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "1",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100030239.1.25",
          "form": "그첳구나!!",
          "original_form": "그첳구나!!",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030239.1.26",
          "form": "난 사과파이",
          "original_form": "난 사과파이",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030239.1.27",
          "form": "글쿤 ㅋㅋ",
          "original_form": "글쿤 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030239.1.28",
          "form": "너무좋아",
          "original_form": "너무좋아",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030239.1.29",
          "form": "나두 좋아함",
          "original_form": "나두 좋아함",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030239.1.30",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030239.1.31",
          "form": "과자는 다 맛잇어",
          "original_form": "과자는 다 맛잇어",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030239.1.32",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030239.1.33",
          "form": "얘기하니깐 먹구싶당",
          "original_form": "얘기하니깐 먹구싶당",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100030239.1.34",
          "form": "음식도 나라별로 특색이있잖아..",
          "original_form": "음식도 나라별로 특색이있잖아..",
