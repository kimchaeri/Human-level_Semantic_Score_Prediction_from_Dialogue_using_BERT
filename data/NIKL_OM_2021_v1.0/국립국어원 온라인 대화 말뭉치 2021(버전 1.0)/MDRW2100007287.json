      "utterance": [
        {
          "id": "MDRW2100007287.1.1",
          "form": "혼자 사셔도",
          "original_form": "혼자 사셔도",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007287.1.2",
          "form": "즐겁게 본인 일 하시면서 잘 사실 것 같아요ㅋㅋㅋ",
          "original_form": "즐겁게 본인 일 하시면서 잘 사실 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007287.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007287.1.4",
          "form": "여자분생겼다고 하면 한동안 우울할것같아요",
          "original_form": "여자분생겼다고 하면 한동안 우울할것같아요",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007287.1.5",
          "form": "그분 너무 부러워서 ㅎㅎ",
          "original_form": "그분 너무 부러워서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007287.1.6",
          "form": "ㅠㅠ아무래도 그렇죠ㅠㅠ",
          "original_form": "ㅠㅠ아무래도 그렇죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007287.1.7",
          "form": "과연 어떤여자분이...",
          "original_form": "과연 어떤여자분이...",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007287.1.8",
          "form": "난 같이 맥주마셔줄수있는데 말이죠",
          "original_form": "난 같이 맥주마셔줄수있는데 말이죠",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007287.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007287.1.10",
          "form": "나도 맥주 좋아하는데  ㅋㅋㅋ",
          "original_form": "나도 맥주 좋아하는데  ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007287.1.11",
          "form": "팬들 초청해서 팬미팅? 식으로 한번 하면 좋겠어요ㅋㅋㅋ",
          "original_form": "팬들 초청해서 팬미팅? 식으로 한번 하면 좋겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007287.1.12",
          "form": "디너쇼 느낌으로",
          "original_form": "디너쇼 느낌으로",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007287.1.13",
          "form": "와 진짜 짱좋은생각이세요",
          "original_form": "와 진짜 짱좋은생각이세요",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007287.1.14",
          "form": "성시경님이 만든음식과",
          "original_form": "성시경님이 만든음식과",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007287.1.15",
          "form": "성시경님이 불러주는 노래와함케한다면",
          "original_form": "성시경님이 불러주는 노래와함케한다면",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007287.1.16",
          "form": "무조건 예매해야죠 ㅎ",
          "original_form": "무조건 예매해야죠 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007287.1.17",
          "form": "진짜 너무 행복할 것 같아요!!",
          "original_form": "진짜 너무 행복할 것 같아요!!",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007287.1.18",
          "form": "저도 가고싶네요ㅋㅋㅋ",
          "original_form": "저도 가고싶네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007287.1.19",
          "form": "꼬꼬무는 아세요?",
          "original_form": "꼬꼬무는 아세요?",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007287.1.20",
          "form": "아 그거 유튜브로 하나 봤어요!",
          "original_form": "아 그거 유튜브로 하나 봤어요!",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007287.1.21",
          "form": "삼풍백화점편",
          "original_form": "삼풍백화점편",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007287.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007287.1.23",
          "form": "삼풍편도 봤죠",
          "original_form": "삼풍편도 봤죠",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007287.1.24",
          "form": "너무 충격이였어요 ㅜㅠ",
          "original_form": "너무 충격이였어요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007287.1.25",
          "form": "저렇게 디테일하게 파헤쳐주는 프로그램 속이 다 시원해요 ㅎ",
          "original_form": "저렇게 디테일하게 파헤쳐주는 프로그램 속이 다 시원해요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007287.1.26",
          "form": "맞아요 제가 알던것보다",
          "original_form": "맞아요 제가 알던것보다",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007287.1.27",
          "form": "진짜 상세하게 알려주셔서",
          "original_form": "진짜 상세하게 알려주셔서",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007287.1.28",
          "form": "안타까우면서도 흥미롭게 봤어요",
          "original_form": "안타까우면서도 흥미롭게 봤어요",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007287.1.29",
          "form": "잊혀져가는 사건을 이렇게 리마인드시켜주는것도 좋은거같아요",
          "original_form": "잊혀져가는 사건을 이렇게 리마인드시켜주는것도 좋은거같아요",
          "speaker_id": "2",
          "time": "20210822 01:27"
        }
      ]
