      "utterance": [
        {
          "id": "MDRW2100004068.1.1",
          "form": "좋아하는 연예인이 누구에요?",
          "original_form": "좋아하는 연예인이 누구에요?",
          "speaker_id": "1",
          "time": "20210821 14:45"
        },
        {
          "id": "MDRW2100004068.1.2",
          "form": "아이돌 너무좋아하져",
          "original_form": "아이돌 너무좋아하져",
          "speaker_id": "2",
          "time": "20210821 14:46"
        },
        {
          "id": "MDRW2100004068.1.3",
          "form": "엔시티 재현이랑 도영 최차애..",
          "original_form": "엔시티 재현이랑 도영 최차애..",
          "speaker_id": "2",
          "time": "20210821 14:46"
        },
        {
          "id": "MDRW2100004068.1.4",
          "form": "아 역시나 엔시티 팬이군요 !!!!!!!!",
          "original_form": "아 역시나 엔시티 팬이군요 !!!!!!!!",
          "speaker_id": "1",
          "time": "20210821 14:47"
        },
        {
          "id": "MDRW2100004068.1.5",
          "form": "제 생각이 맞나요?",
          "original_form": "제 생각이 맞나요?",
          "speaker_id": "1",
          "time": "20210821 14:47"
        },
        {
          "id": "MDRW2100004068.1.6",
          "form": "완전 맞습니다",
          "original_form": "완전 맞습니다",
          "speaker_id": "2",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004068.1.7",
          "form": "컴백을 너무안해서 조금 식으려다가",
          "original_form": "컴백을 너무안해서 조금 식으려다가",
          "speaker_id": "2",
          "time": "20210821 14:50"
        },
        {
          "id": "MDRW2100004068.1.8",
          "form": "어제 티저가떠서..다시 불타네요",
          "original_form": "어제 티저가떠서..다시 불타네요",
          "speaker_id": "2",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004068.1.9",
          "form": "오 티저 떴어요? 한번 봐야겠어요",
          "original_form": "오 티저 떴어요? 한번 봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004068.1.10",
          "form": "저도 요새 덕질이 시들고 있거든요",
          "original_form": "저도 요새 덕질이 시들고 있거든요",
          "speaker_id": "1",
          "time": "20210821 14:51"
        },
        {
          "id": "MDRW2100004068.1.11",
          "form": "한번씩 덕질 침체기가 찾아오곤하죠..",
          "original_form": "한번씩 덕질 침체기가 찾아오곤하죠..",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004068.1.12",
          "form": "최근에 좋아하는 아이돌 있으세요?",
          "original_form": "최근에 좋아하는 아이돌 있으세요?",
          "speaker_id": "2",
          "time": "20210821 14:53"
        },
        {
          "id": "MDRW2100004068.1.13",
          "form": "아직 엔시티가 마지막이고 그냥 간간히 엔시티 챙겨보고 있어요",
          "original_form": "아직 엔시티가 마지막이고 그냥 간간히 엔시티 챙겨보고 있어요",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004068.1.14",
          "form": "온리 엔시티이신가요?",
          "original_form": "온리 엔시티이신가요?",
          "speaker_id": "1",
          "time": "20210821 14:56"
        },
        {
          "id": "MDRW2100004068.1.15",
          "form": "덕질하는 그룹은 그렇긴한데",
          "original_form": "덕질하는 그룹은 그렇긴한데",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004068.1.16",
          "form": "약간 다른 곳도 간잽을 하시는군요?",
          "original_form": "약간 다른 곳도 간잽을 하시는군요?",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004068.1.17",
          "form": "엔하이픈 투바투 더보이즈 등등도 나올때마다 한번씩 보고있긴하죠ㅋㅋ",
          "original_form": "엔하이픈 투바투 더보이즈 등등도 나올때마다 한번씩 보고있긴하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004068.1.18",
          "form": "요즘 아이돌 퀄리티가 다 괜찮아가지고..",
          "original_form": "요즘 아이돌 퀄리티가 다 괜찮아가지고..",
          "speaker_id": "2",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004068.1.19",
          "form": "아ㅋㅋㅋㅋㅋ 맞아요 다들 뭐 나쁘지 않더라구요",
          "original_form": "아ㅋㅋㅋㅋㅋ 맞아요 다들 뭐 나쁘지 않더라구요",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004068.1.20",
          "form": "더보이즈도 가끔 보긴봐요",
          "original_form": "더보이즈도 가끔 보긴봐요",
          "speaker_id": "1",
          "time": "20210821 14:59"
        },
        {
          "id": "MDRW2100004068.1.21",
          "form": "더보이즈 요즘 엄청 인기많아졌더라고요",
          "original_form": "더보이즈 요즘 엄청 인기많아졌더라고요",
          "speaker_id": "2",
          "time": "20210821 15:00"
        },
        {
          "id": "MDRW2100004068.1.22",
          "form": "초동50만장넘었다길래 놀랐어요",
          "original_form": "초동50만장넘었다길래 놀랐어요",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004068.1.23",
          "form": "초동 50만장이요? 와 대박",
          "original_form": "초동 50만장이요? 와 대박",
          "speaker_id": "1",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004068.1.24",
          "form": "인기 많네요",
          "original_form": "인기 많네요",
          "speaker_id": "1",
