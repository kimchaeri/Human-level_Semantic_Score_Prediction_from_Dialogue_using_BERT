      "utterance": [
        {
          "id": "MDRW2100036248.1.1",
          "form": "넘 좋아요 하 ,,",
          "original_form": "넘 좋아요 하 ,,",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100036248.1.2",
          "form": "저도 ㅋㅋㅋ 나왔을땐 몰랐었어요",
          "original_form": "저도 ㅋㅋㅋ 나왔을땐 몰랐었어요",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100036248.1.3",
          "form": "뒤늦게 알았는데",
          "original_form": "뒤늦게 알았는데",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100036248.1.4",
          "form": "드리미들이랑 찰떡이에요 !",
          "original_form": "드리미들이랑 찰떡이에요 !",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100036248.1.5",
          "form": "그니까요!!!!! 그 무슨",
          "original_form": "그니까요!!!!! 그 무슨",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036248.1.6",
          "form": "타이거 어저고 노래 있는데",
          "original_form": "타이거 어저고 노래 있는데",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036248.1.7",
          "form": "그거 진짜 청량 끝판왕 아닌가요!!",
          "original_form": "그거 진짜 청량 끝판왕 아닌가요!!",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036248.1.8",
          "form": "그 트더피 말씀하시는건가요?!",
          "original_form": "그 트더피 말씀하시는건가요?!",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036248.1.9",
          "form": "아마 그거 같은데 !",
          "original_form": "아마 그거 같은데 !",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036248.1.10",
          "form": "오마이갓",
          "original_form": "오마이갓",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036248.1.11",
          "form": "저 오ㅐ.타이거라 했죠",
          "original_form": "저 오ㅐ.타이거라 했죠",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036248.1.12",
          "form": "어이없어서 비명 질렀어요",
          "original_form": "어이없어서 비명 질렀어요",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036248.1.13",
          "form": "아 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036248.1.14",
          "form": "그거 맞아요",
          "original_form": "그거 맞아요",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036248.1.15",
          "form": "그래도 알아들었어욬ㅋㄱㅋ",
          "original_form": "그래도 알아들었어욬ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036248.1.16",
          "form": "비슷했다구ㅡ요!",
          "original_form": "비슷했다구ㅡ요!",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036248.1.17",
          "form": "ㅎ.ㅎ..... 부끄럽구만요",
          "original_form": "ㅎ.ㅎ..... 부끄럽구만요",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100036248.1.18",
          "form": "앗 근데 저 아렇게 엔시티 노래 잘 아시는 뷴",
          "original_form": "앗 근데 저 아렇게 엔시티 노래 잘 아시는 뷴",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100036248.1.19",
          "form": "정말 오랜만에 봐서 넘 좋아요",
          "original_form": "정말 오랜만에 봐서 넘 좋아요",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100036248.1.20",
          "form": "저두욬ㅋㅋㄱㄲ!",
          "original_form": "저두욬ㅋㅋㄱㄲ!",
          "speaker_id": "2",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100036248.1.21",
          "form": "엔시티 노래는 좀",
          "original_form": "엔시티 노래는 좀",
          "speaker_id": "2",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100036248.1.22",
          "form": "네오하다라는 편견이 많아서",
          "original_form": "네오하다라는 편견이 많아서",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100036248.1.23",
          "form": "사람들이 안듣잖아요ㅜㅜ",
          "original_form": "사람들이 안듣잖아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100036248.1.24",
          "form": "그니까요!! 막 터치같은 노래는 진짜",
          "original_form": "그니까요!! 막 터치같은 노래는 진짜",
          "speaker_id": "1",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100036248.1.25",
          "form": "대중적인대 ㅜ.^",
          "original_form": "대중적인대 ㅜ.^",
          "speaker_id": "1",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100036248.1.26",
          "form": "터치 ...하 비운의 명곡이죠",
          "original_form": "터치 ...하 비운의 명곡이죠",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100036248.1.27",
          "form": "지금쯤 냈으면 차트를 휩쓸었을 ...",
          "original_form": "지금쯤 냈으면 차트를 휩쓸었을 ...",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100036248.1.28",
          "form": "아 그니까요!!!!! 진짜 그때 다들 리즈라서 넘 예쁜데 ㅠ",
          "original_form": "아 그니까요!!!!! 진짜 그때 다들 리즈라서 넘 예쁜데 ㅠ",
          "speaker_id": "1",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100036248.1.29",
          "form": "의상도 넘 예쁘고!!",
          "original_form": "의상도 넘 예쁘고!!",
          "speaker_id": "1",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100036248.1.30",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100036248.1.31",
          "form": "진짜 다들 리즈였어요",
          "original_form": "진짜 다들 리즈였어요",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100036248.1.32",
          "form": "유타랑 도영이두 리즈였고...",
          "original_form": "유타랑 도영이두 리즈였고...",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100036248.1.33",
          "form": "윈윈이도 리즈였구 ...",
          "original_form": "윈윈이도 리즈였구 ...",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100036248.1.34",
          "form": "아 그니까요!! 진짜 터치 때 유타는",
          "original_form": "아 그니까요!! 진짜 터치 때 유타는",
          "speaker_id": "1",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100036248.1.35",
          "form": "절대 못.. 따라올 것 같어요 너무 청초했던 ㅠ.^",
          "original_form": "절대 못.. 따라올 것 같어요 너무 청초했던 ㅠ.^",
