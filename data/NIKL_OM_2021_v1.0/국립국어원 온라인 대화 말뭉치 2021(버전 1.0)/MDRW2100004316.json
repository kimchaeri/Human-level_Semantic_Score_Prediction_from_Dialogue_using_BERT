      "utterance": [
        {
          "id": "MDRW2100004316.1.1",
          "form": "요즘 날씨 정말 변덕스럽져",
          "original_form": "요즘 날씨 정말 변덕스럽져",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004316.1.2",
          "form": "오늘은 비가 죽어라 내리네요",
          "original_form": "오늘은 비가 죽어라 내리네요",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004316.1.3",
          "form": "그러니까요 비가 왜이리 많이 내리는지 모르겠어요",
          "original_form": "그러니까요 비가 왜이리 많이 내리는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004316.1.4",
          "form": "또 가을장마? 인가 온다고 하더라구요",
          "original_form": "또 가을장마? 인가 온다고 하더라구요",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004316.1.5",
          "form": "축축해요",
          "original_form": "축축해요",
          "speaker_id": "2",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004316.1.6",
          "form": "오마이갓",
          "original_form": "오마이갓",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004316.1.7",
          "form": "비가 계속 온다니",
          "original_form": "비가 계속 온다니",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004316.1.8",
          "form": "근데 쫌 시원하기도 해요",
          "original_form": "근데 쫌 시원하기도 해요",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004316.1.9",
          "form": "어차피 거리두기 때문에 계속 집에만 있어서ㅋㅋㅋ",
          "original_form": "어차피 거리두기 때문에 계속 집에만 있어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004316.1.10",
          "form": "맞아요 비가 많이오니 날씨가 풀려서",
          "original_form": "맞아요 비가 많이오니 날씨가 풀려서",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004316.1.11",
          "form": "시원하니 좋아요",
          "original_form": "시원하니 좋아요",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004316.1.12",
          "form": "더워서 항상 선풍기 틀고지냈는데",
          "original_form": "더워서 항상 선풍기 틀고지냈는데",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004316.1.13",
          "form": "이른아침에는 이제 좀 추워요",
          "original_form": "이른아침에는 이제 좀 추워요",
          "speaker_id": "2",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004316.1.14",
          "form": "일교차가 크죠",
          "original_form": "일교차가 크죠",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004316.1.15",
          "form": "이런 날씨 감기걸리기 딱 좋은데",
          "original_form": "이런 날씨 감기걸리기 딱 좋은데",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004316.1.16",
          "form": "그래서 저는 두툼한 이불 꺼냈어요...",
          "original_form": "그래서 저는 두툼한 이불 꺼냈어요...",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004316.1.17",
          "form": "나갈때 긴팔 입을정도인가요 요즘?",
          "original_form": "나갈때 긴팔 입을정도인가요 요즘?",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004316.1.18",
          "form": "맞아요 저는 끄고 귀찮아서 꿋꿋이 틀어놨다가",
          "original_form": "맞아요 저는 끄고 귀찮아서 꿋꿋이 틀어놨다가",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004316.1.19",
          "form": "오늘 목이 좀 칼칼하네요ㅜㅠ",
          "original_form": "오늘 목이 좀 칼칼하네요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004316.1.20",
          "form": "그래도 안더워서 좋아요",
          "original_form": "그래도 안더워서 좋아요",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004316.1.21",
          "form": "저는 더위를 많이타서 아직 짧은 옷차림이에요",
          "original_form": "저는 더위를 많이타서 아직 짧은 옷차림이에요",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004316.1.22",
          "form": "아하 그러시군요",
          "original_form": "아하 그러시군요",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004316.1.23",
          "form": "저는 추운 날씨를 좋아하는데",
          "original_form": "저는 추운 날씨를 좋아하는데",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004316.1.24",
          "form": "점점 설레면서",
          "original_form": "점점 설레면서",
          "speaker_id": "1",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004316.1.25",
          "form": "낮이 짧아져서 아쉽기도 하네요.",
          "original_form": "낮이 짧아져서 아쉽기도 하네요.",
          "speaker_id": "1",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004316.1.26",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004316.1.27",
          "form": "저는 여름이 좋아요 풍경이 다채로워서 예뻐서 좋더라구요",
          "original_form": "저는 여름이 좋아요 풍경이 다채로워서 예뻐서 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004316.1.28",
          "form": "그런데 막상 여름되면 가을이랑 겨울이 좋아요",
          "original_form": "그런데 막상 여름되면 가을이랑 겨울이 좋아요",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004316.1.29",
          "form": "그러네요 낮이 짧아지는건 아쉽죠ㅜㅜ",
          "original_form": "그러네요 낮이 짧아지는건 아쉽죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004316.1.30",
          "form": "name1님은 어떤 계절을 좋아하시나요",
          "original_form": "&name1&님은 어떤 계절을 좋아하시나요",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004316.1.31",
          "form": "여름 초록초록하고 꽃도 많이 펴서 좋죠",
          "original_form": "여름 초록초록하고 꽃도 많이 펴서 좋죠",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004316.1.32",
          "form": "저는 겨울 좋아합니다",
          "original_form": "저는 겨울 좋아합니다",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004316.1.33",
          "form": "연말 분위기를 좋아하고 추운게 좋아서요",
          "original_form": "연말 분위기를 좋아하고 추운게 좋아서요",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004316.1.34",
          "form": "겨울 냄새도 좋아하고",
          "original_form": "겨울 냄새도 좋아하고",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004316.1.35",
          "form": "옷 껴입는것도 좋고ㅎㅎ",
          "original_form": "옷 껴입는것도 좋고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004316.1.36",
          "form": "와 맞아요 겨울만의 그 분위기도 너무 좋아요",
          "original_form": "와 맞아요 겨울만의 그 분위기도 너무 좋아요",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004316.1.37",
          "form": "크리스마스!! 연말!! 올 겨울은 눈도 오고 안추웠으면 좋겠어요",
          "original_form": "크리스마스!! 연말!! 올 겨울은 눈도 오고 안추웠으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004316.1.38",
          "form": "눈이 오는데 안 추울 수가 있나요?",
          "original_form": "눈이 오는데 안 추울 수가 있나요?",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004316.1.39",
          "form": "오류가 있는데요",
          "original_form": "오류가 있는데요",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004316.1.40",
          "form": "농담입니다..",
          "original_form": "농담입니다..",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004316.1.41",
          "form": "저도 눈내리면 좋겠어요",
          "original_form": "저도 눈내리면 좋겠어요",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004316.1.42",
          "form": "못본지 너무 오래됐네요..",
          "original_form": "못본지 너무 오래됐네요..",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004316.1.43",
          "form": "암튼",
          "original_form": "암튼",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004316.1.44",
          "form": "얼마뒤에 태풍이 온다더라고요",
          "original_form": "얼마뒤에 태풍이 온다더라고요",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004316.1.45",
          "form": "ㅋㄱㅋㄱㅋㄱ 저의 그냥 바람일 뿐이었어요 부산이라 눈이 안와서 슬퍼요",
          "original_form": "ㅋㄱㅋㄱㅋㄱ 저의 그냥 바람일 뿐이었어요 부산이라 눈이 안와서 슬퍼요",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004316.1.46",
          "form": "장마도오고 태풍도 온다니 날씨가 정말 이상하네요ㅠㅠ",
          "original_form": "장마도오고 태풍도 온다니 날씨가 정말 이상하네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004316.1.47",
          "form": "그쵸.. 날씨가 너무 궂어요.",
          "original_form": "그쵸.. 날씨가 너무 궂어요.",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004316.1.48",
          "form": "변동도 많고",
          "original_form": "변동도 많고",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004316.1.49",
          "form": "평소에 우산 잘 챙기고 다녀야될것같아요",
          "original_form": "평소에 우산 잘 챙기고 다녀야될것같아요",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004316.1.50",
          "form": "그러게나말이에요ㅜㅠ 햇빛나는데 천둥칠땐 깜짝놀랐어요",
