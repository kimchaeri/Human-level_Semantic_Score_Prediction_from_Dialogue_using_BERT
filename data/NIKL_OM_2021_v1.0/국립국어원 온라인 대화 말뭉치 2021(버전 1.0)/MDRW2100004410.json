      "utterance": [
        {
          "id": "MDRW2100004410.1.1",
          "form": "이번 올림픽보셨어요?",
          "original_form": "이번 올림픽보셨어요?",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004410.1.2",
          "form": "네~~ 역대급으로 재미있었던거같아요",
          "original_form": "네~~ 역대급으로 재미있었던거같아요",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004410.1.3",
          "form": "저는 특히 양궁이요 안산진짜대단한거같아요",
          "original_form": "저는 특히 양궁이요 안산진짜대단한거같아요",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004410.1.4",
          "form": "심박수 나오는거 보셨어요?",
          "original_form": "심박수 나오는거 보셨어요?",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004410.1.5",
          "form": "네.  김우진인가는 60회인가? 엄정나더라구요",
          "original_form": "네.  김우진인가는 60회인가? 엄정나더라구요",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004410.1.6",
          "form": "헉 60밖에 안나왔나요?",
          "original_form": "헉 60밖에 안나왔나요?",
          "speaker_id": "1",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004410.1.7",
          "form": "설마 잘때는 안뛰는거 아니겠죠?ㅎㅎ",
          "original_form": "설마 잘때는 안뛰는거 아니겠죠?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004410.1.8",
          "form": "ㅎㅎ 그니깐요.  긴장이라는게 없나봐요",
          "original_form": "ㅎㅎ 그니깐요.  긴장이라는게 없나봐요",
          "speaker_id": "2",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004410.1.9",
          "form": "안산도 대단하다싶었는데김우진도 대단하네요",
          "original_form": "안산도 대단하다싶었는데김우진도 대단하네요",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004410.1.10",
          "form": "배구도 보셨어요?",
          "original_form": "배구도 보셨어요?",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004410.1.11",
          "form": "김연경짱~~ 다른선수들도 잘했구요",
          "original_form": "김연경짱~~ 다른선수들도 잘했구요",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004410.1.12",
          "form": "올림픽대비하려고 연봉깍아서 들어온거래요",
          "original_form": "올림픽대비하려고 연봉깍아서 들어온거래요",
          "speaker_id": "2",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004410.1.13",
          "form": "맞아요 근데 편파판정하는거보니 속이 부글부글하더라구요",
          "original_form": "맞아요 근데 편파판정하는거보니 속이 부글부글하더라구요",
          "speaker_id": "1",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004410.1.14",
          "form": "대단한사람같아요",
          "original_form": "대단한사람같아요",
          "speaker_id": "2",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004410.1.15",
          "form": "어디서나 편파는 나오니...",
          "original_form": "어디서나 편파는 나오니...",
          "speaker_id": "2",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004410.1.16",
          "form": "보는 우리도 그런데 선수들은 오죽 답답할까싶더라구요",
          "original_form": "보는 우리도 그런데 선수들은 오죽 답답할까싶더라구요",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004410.1.17",
          "form": "그러게요.  속상할꺼같아요",
          "original_form": "그러게요.  속상할꺼같아요",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004410.1.18",
          "form": "이번에 높이뛰기선수 넘 행복하게 뛰어서 보는 내내 같이 즐거웠어요",
          "original_form": "이번에 높이뛰기선수 넘 행복하게 뛰어서 보는 내내 같이 즐거웠어요",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004410.1.19",
          "form": "티비에 나왔음 좋겠는데 군인이라 못나오나봐요",
          "original_form": "티비에 나왔음 좋겠는데 군인이라 못나오나봐요",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004410.1.20",
          "form": "거의 다보셨어요? 저는 양궁만본거같아요",
          "original_form": "거의 다보셨어요? 저는 양궁만본거같아요",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004410.1.21",
          "form": "배구랑",
          "original_form": "배구랑",
          "speaker_id": "1",
