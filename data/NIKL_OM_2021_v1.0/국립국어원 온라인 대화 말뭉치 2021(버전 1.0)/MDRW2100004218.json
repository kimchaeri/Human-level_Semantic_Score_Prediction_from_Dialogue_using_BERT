      "utterance": [
        {
          "id": "MDRW2100004218.1.1",
          "form": "좋아하는 게임이 있어?",
          "original_form": "좋아하는 게임이 있어?",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004218.1.2",
          "form": "요즘은 배틀로얄게임 하고잇어ㅎ",
          "original_form": "요즘은 배틀로얄게임 하고잇어ㅎ",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004218.1.3",
          "form": "게임 좋아해?",
          "original_form": "게임 좋아해?",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004218.1.4",
          "form": "싫어하지는 않는데 엄청 못 하는 편이야ㅠㅠ",
          "original_form": "싫어하지는 않는데 엄청 못 하는 편이야ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004218.1.5",
          "form": "카트라이더도 튜토리얼에서 버벅거리는 수준!",
          "original_form": "카트라이더도 튜토리얼에서 버벅거리는 수준!",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004218.1.6",
          "form": "아 글쿠나ㅠㅠ",
          "original_form": "아 글쿠나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004218.1.7",
          "form": "그래서 맨날 쉬운 게임만 하고 있어ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 맨날 쉬운 게임만 하고 있어ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004218.1.8",
          "form": "ㅋㄱㄱㅋㅋ 무슨 게임해?",
          "original_form": "ㅋㄱㄱㅋㅋ 무슨 게임해?",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004218.1.9",
          "form": "쿠키런이나 꿈의 정원 같은 거!",
          "original_form": "쿠키런이나 꿈의 정원 같은 거!",
          "speaker_id": "2",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004218.1.10",
          "form": "근데 쿠키런도 맨날 친구가 못 한다고 놀려ㅠ",
          "original_form": "근데 쿠키런도 맨날 친구가 못 한다고 놀려ㅠ",
          "speaker_id": "2",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004218.1.11",
          "form": "앗ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004218.1.12",
          "form": "쿠키런은 템잇으면 괜찮지않아?",
          "original_form": "쿠키런은 템잇으면 괜찮지않아?",
          "speaker_id": "1",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004218.1.13",
          "form": "안한지 오래되서 잘은 모르지만",
          "original_form": "안한지 오래되서 잘은 모르지만",
          "speaker_id": "1",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004218.1.14",
          "form": "맞아ㅎㅎ 그래서 맨날 아이템 잔뜩 모아두고 쿠키도 열심히 키우고 있어",
          "original_form": "맞아ㅎㅎ 그래서 맨날 아이템 잔뜩 모아두고 쿠키도 열심히 키우고 있어",
          "speaker_id": "2",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004218.1.15",
          "form": "아이템 없었으면 이것도 못 해ㅋㅋㅋ",
          "original_form": "아이템 없었으면 이것도 못 해ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100004218.1.16",
          "form": "ㅋㅋㄱㅋ 쿠키런도 현질유도 많은 편인가?",
          "original_form": "ㅋㅋㄱㅋ 쿠키런도 현질유도 많은 편인가?",
          "speaker_id": "1",
          "time": "20210821 15:45"
        },
        {
          "id": "MDRW2100004218.1.17",
          "form": "보통 모바일이 현질유도가 좀 잇잖아",
          "original_form": "보통 모바일이 현질유도가 좀 잇잖아",
          "speaker_id": "1",
          "time": "20210821 15:46"
        },
        {
          "id": "MDRW2100004218.1.18",
          "form": "좀 있긴 하지..? 그래도 심한건 아닌 거 같아",
          "original_form": "좀 있긴 하지..? 그래도 심한건 아닌 거 같아",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004218.1.19",
          "form": "난 현질 잘 안 하는 편이거든",
          "original_form": "난 현질 잘 안 하는 편이거든",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004218.1.20",
          "form": "글쿠나",
          "original_form": "글쿠나",
          "speaker_id": "1",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004218.1.21",
          "form": "나도 현질 잘 안하는데",
          "original_form": "나도 현질 잘 안하는데",
          "speaker_id": "1",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004218.1.22",
          "form": "어떤 모바일겜은 현질안하면",
          "original_form": "어떤 모바일겜은 현질안하면",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004218.1.23",
          "form": "너무 지루해서 접게되더라고ㅋㅋ",
          "original_form": "너무 지루해서 접게되더라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004218.1.24",
          "form": "맞아 현질 안 하면 게임 따라가기 힘든 부분도 있고",
          "original_form": "맞아 현질 안 하면 게임 따라가기 힘든 부분도 있고",
          "speaker_id": "2",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004218.1.25",
          "form": "물약을 몇백개를 먹여야 레벨업을 한다니ㅠㅜ",
          "original_form": "물약을 몇백개를 먹여야 레벨업을 한다니ㅠㅜ",
          "speaker_id": "2",
          "time": "20210821 15:56"
        }
      ]
    }
  ]
