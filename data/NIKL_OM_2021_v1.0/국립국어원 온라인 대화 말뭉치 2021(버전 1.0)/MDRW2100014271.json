      "utterance": [
        {
          "id": "MDRW2100014271.1.1",
          "form": "강아지나 고양이 키우시나용??",
          "original_form": "강아지나 고양이 키우시나용??",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014271.1.2",
          "form": "아뇨ㅠㅠ키우고 계세요?",
          "original_form": "아뇨ㅠㅠ키우고 계세요?",
          "speaker_id": "2",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014271.1.3",
          "form": "저는 강아지 엄청 좋아하는데 ㅠㅠ 엄마가 알러지가 있으셔서",
          "original_form": "저는 강아지 엄청 좋아하는데 ㅠㅠ 엄마가 알러지가 있으셔서",
          "speaker_id": "1",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014271.1.4",
          "form": "못키워요 ㅠㅠ",
          "original_form": "못키워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014271.1.5",
          "form": "아 저도 부모님이 안좋아하셔서...나중에 독립하면 생각해보려구요",
          "original_form": "아 저도 부모님이 안좋아하셔서...나중에 독립하면 생각해보려구요",
          "speaker_id": "2",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014271.1.6",
          "form": "보통 부모님이 안좋아하셔서 못키우는거같아요 ㅠ",
          "original_form": "보통 부모님이 안좋아하셔서 못키우는거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014271.1.7",
          "form": "맞아요...사실 부모님이 책임자가 되버리니까ㅎㅎ",
          "original_form": "맞아요...사실 부모님이 책임자가 되버리니까ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100014271.1.8",
          "form": "맞아요 ㅎㅎ 사료나 그런걸 보통 집에 있는 사람이 주니까",
          "original_form": "맞아요 ㅎㅎ 사료나 그런걸 보통 집에 있는 사람이 주니까",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014271.1.9",
          "form": "저는 항상 다른 집가서 강아지 있으면",
          "original_form": "저는 항상 다른 집가서 강아지 있으면",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014271.1.10",
          "form": "저한테 애교부리는게 너무 좋더라구요 ㅋㅋ",
          "original_form": "저한테 애교부리는게 너무 좋더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014271.1.11",
          "form": "그니까요ㅎㅎ 참 부러운ㅎㅎㅠㅠㅜ",
          "original_form": "그니까요ㅎㅎ 참 부러운ㅎㅎㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014271.1.12",
          "form": "고양이도 좋아하세요??",
          "original_form": "고양이도 좋아하세요??",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014271.1.13",
          "form": "네ㅎㅎ 영상으로 보면 귀엽더라구요",
          "original_form": "네ㅎㅎ 영상으로 보면 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014271.1.14",
          "form": "ㅎㅎ 맞아요 그리고 약간 시크한게 매력인거같아요",
          "original_form": "ㅎㅎ 맞아요 그리고 약간 시크한게 매력인거같아요",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014271.1.15",
          "form": "맞아요ㅋㅋ그러면서도 의지되는 순간도 있구ㅎㅎ",
          "original_form": "맞아요ㅋㅋ그러면서도 의지되는 순간도 있구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014271.1.16",
          "form": "맞아요 강아지나 고양이나 둘 다 주인이 슬퍼하면 알아채고 애교ㅡ부리는거 보면",
          "original_form": "맞아요 강아지나 고양이나 둘 다 주인이 슬퍼하면 알아채고 애교ㅡ부리는거 보면",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014271.1.17",
          "form": "진짜 키우고싶어요 ㅎㅎ",
          "original_form": "진짜 키우고싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014271.1.18",
          "form": "맞아요ㅠㅠ저도 그래요",
          "original_form": "맞아요ㅠㅠ저도 그래요",
          "speaker_id": "2",
          "time": "20210825 18:17"
        }
      ]
