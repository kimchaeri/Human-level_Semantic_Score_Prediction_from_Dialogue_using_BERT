      "utterance": [
        {
          "id": "MDRW2100020313.1.1",
          "form": "안녕하세요 아하하",
          "original_form": "안녕하세요 아하하",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100020313.1.2",
          "form": "넵 ㅎㅎㅎㅎ 간만이시네요",
          "original_form": "넵 ㅎㅎㅎㅎ 간만이시네요",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100020313.1.3",
          "form": "요새 드라마 뭐 보세요",
          "original_form": "요새 드라마 뭐 보세요",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100020313.1.4",
          "form": "딱히 챙겨 보는 드라마는 없습니다",
          "original_form": "딱히 챙겨 보는 드라마는 없습니다",
          "speaker_id": "2",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100020313.1.5",
          "form": "저는 요새 메모리스트라는 드라마를 보고 있습니다",
          "original_form": "저는 요새 메모리스트라는 드라마를 보고 있습니다",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100020313.1.6",
          "form": "저도 메모 리스트 예전에 봤는데",
          "original_form": "저도 메모 리스트 예전에 봤는데",
          "speaker_id": "2",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100020313.1.7",
          "form": "유승호가 잠 잘 컸더라고요",
          "original_form": "유승호가 잠 잘 컸더라고요",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100020313.1.8",
          "form": "아 저는 유승호가 조금 아쉽더라구요 좀만 더 몸 관리를 잘했으면은 더 훨씬 더 멋졌을 거 같은데",
          "original_form": "아 저는 유승호가 조금 아쉽더라구요 좀만 더 몸 관리를 잘했으면은 더 훨씬 더 멋졌을 거 같은데",
          "speaker_id": "2",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100020313.1.9",
          "form": "그 정도면 엄청 잘 컸죠",
          "original_form": "그 정도면 엄청 잘 컸죠",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100020313.1.10",
          "form": "내 그러긴 한데 그래도 조금 더 뭐 젖었을 거 같은데 아쉽더라고요",
          "original_form": "내 그러긴 한데 그래도 조금 더 뭐 젖었을 거 같은데 아쉽더라고요",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100020313.1.11",
          "form": "멋있어졌울거",
          "original_form": "멋있어졌울거",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100020313.1.12",
          "form": "뭐 재밌는 드라마가 없을까요",
          "original_form": "뭐 재밌는 드라마가 없을까요",
          "speaker_id": "1",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100020313.1.13",
          "form": "방법은 보셨어요 영화도 나왔던데",
          "original_form": "방법은 보셨어요 영화도 나왔던데",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100020313.1.14",
          "form": "저도 영화로 보고 드라마를 다시 봤어요",
          "original_form": "저도 영화로 보고 드라마를 다시 봤어요",
          "speaker_id": "1",
          "time": "20210901 18:25"
        }
      ]
