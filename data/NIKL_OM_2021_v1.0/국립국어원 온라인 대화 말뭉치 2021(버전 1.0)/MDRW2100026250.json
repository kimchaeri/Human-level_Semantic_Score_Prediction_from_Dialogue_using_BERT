      "utterance": [
        {
          "id": "MDRW2100026250.1.1",
          "form": "나 name3name4name5 보고싶어",
          "original_form": "나 &name3&&name4&&name5& 보고싶어",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026250.1.2",
          "form": "본가에 두고 온 내... 귀염둥이들",
          "original_form": "본가에 두고 온 내... 귀염둥이들",
          "speaker_id": "2",
          "time": "20210901 21:03"
        },
        {
          "id": "MDRW2100026250.1.3",
          "form": "헐 그러고보니 나 한 번도 직접 본 적이 없어",
          "original_form": "헐 그러고보니 나 한 번도 직접 본 적이 없어",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100026250.1.4",
          "form": "엥 그랬나 우리집 놀러온적 없어?",
          "original_form": "엥 그랬나 우리집 놀러온적 없어?",
          "speaker_id": "2",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100026250.1.5",
          "form": "없어ㅋㅋㅋㅠㅠ",
          "original_form": "없어ㅋㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100026250.1.6",
          "form": "헐... 하늘 아래 아주 큰 행복을 놓쳤구나",
          "original_form": "헐... 하늘 아래 아주 큰 행복을 놓쳤구나",
          "speaker_id": "2",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100026250.1.7",
          "form": "우리 멈머들은 아주 귀여워",
          "original_form": "우리 멈머들은 아주 귀여워",
          "speaker_id": "2",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100026250.1.8",
          "form": "아ㅋㅋㅋㅋㅋ 그런 편이긴 하지",
          "original_form": "아ㅋㅋㅋㅋㅋ 그런 편이긴 하지",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100026250.1.9",
          "form": "귀엽겠다 궁금해",
          "original_form": "귀엽겠다 궁금해",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100026250.1.10",
          "form": "사진 자주 올려주잖아 우리 애들",
          "original_form": "사진 자주 올려주잖아 우리 애들",
          "speaker_id": "2",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100026250.1.11",
          "form": "그치만... 조금 더 역병 나아지면 놀러와",
          "original_form": "그치만... 조금 더 역병 나아지면 놀러와",
          "speaker_id": "2",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100026250.1.12",
          "form": "헐 완전 좋아",
          "original_form": "헐 완전 좋아",
          "speaker_id": "1",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100026250.1.13",
          "form": "세마리 키우는 거야?",
          "original_form": "세마리 키우는 거야?",
          "speaker_id": "1",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100026250.1.14",
          "form": "엉 세마리ㅣ",
          "original_form": "엉 세마리ㅣ",
          "speaker_id": "2",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100026250.1.15",
          "form": "하니 얘기만? 가끔 들어본 것 같애",
          "original_form": "하니 얘기만? 가끔 들어본 것 같애",
          "speaker_id": "1",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100026250.1.16",
          "form": "둘은 믹스견이고",
          "original_form": "둘은 믹스견이고",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100026250.1.17",
          "form": "하나는 브리타니스파니엘이라고",
          "original_form": "하나는 브리타니스파니엘이라고",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100026250.1.18",
          "form": "하는 견종잇어",
          "original_form": "하는 견종잇어",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100026250.1.19",
          "form": "우와 이름 짱 길다",
          "original_form": "우와 이름 짱 길다",
          "speaker_id": "1",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100026250.1.20",
          "form": "고급지네",
          "original_form": "고급지네",
          "speaker_id": "1",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100026250.1.21",
          "form": "아무래도 사냥개라 좀",
          "original_form": "아무래도 사냥개라 좀",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100026250.1.22",
          "form": "그런편이죠",
          "original_form": "그런편이죠",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100026250.1.23",
          "form": "우와 사냥개야?",
          "original_form": "우와 사냥개야?",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100026250.1.24",
          "form": "아 그럼 그것도 못들었겠네",
          "original_form": "아 그럼 그것도 못들었겠네",
          "speaker_id": "2",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100026250.1.25",
          "form": "사냥 구경간거",
          "original_form": "사냥 구경간거",
          "speaker_id": "2",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100026250.1.26",
          "form": "헐 못 들었어",
          "original_form": "헐 못 들었어",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100026250.1.27",
          "form": "아 근데 막 걔네가 잡고 그러는게 아니라",
          "original_form": "아 근데 막 걔네가 잡고 그러는게 아니라",
          "speaker_id": "2",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100026250.1.28",
          "form": "사람이 총 쏘면 물어오고 그런거",
          "original_form": "사람이 총 쏘면 물어오고 그런거",
          "speaker_id": "2",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100026250.1.29",
          "form": "꿩사냥개인데",
          "original_form": "꿩사냥개인데",
          "speaker_id": "2",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100026250.1.30",
          "form": "name4는 그런거 할줄 몰라서 옆에서 구경했어",
          "original_form": "&name4&는 그런거 할줄 몰라서 옆에서 구경했어",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100026250.1.31",
          "form": "귀여워ㅋㅋㅋㅋㅋㅋ",
          "original_form": "귀여워ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100026250.1.32",
          "form": "헐 그럼 꿩 사냥하는 것도 봤겠네",
          "original_form": "헐 그럼 꿩 사냥하는 것도 봤겠네",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100026250.1.33",
          "form": "요즘에도 그런 게 있구나",
          "original_form": "요즘에도 그런 게 있구나",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100026250.1.34",
          "form": "허가받으면??ㅍ할 수있대",
          "original_form": "허가받으면??ㅍ할 수있대",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100026250.1.35",
          "form": "name4랑 같은 견종들이랑 같이 갔는데",
          "original_form": "&name4&랑 같은 견종들이랑 같이 갔는데",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100026250.1.36",
          "form": "역시 우리집 애가 제일 잘생겼어",
          "original_form": "역시 우리집 애가 제일 잘생겼어",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100026250.1.37",
