      "utterance": [
        {
          "id": "MDRW2100007242.1.1",
          "form": "언니 싸이월드 세대야?",
          "original_form": "언니 싸이월드 세대야?",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007242.1.2",
          "form": "맞긴 맞는데 난 안 했어",
          "original_form": "맞긴 맞는데 난 안 했어",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007242.1.3",
          "form": "싸이월드가 버디버디 같은 건가?",
          "original_form": "싸이월드가 버디버디 같은 건가?",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007242.1.4",
          "form": "헐~ 너 몰라?",
          "original_form": "헐~ 너 몰라?",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007242.1.5",
          "form": "나는 그 세대가 아니라서",
          "original_form": "나는 그 세대가 아니라서",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007242.1.6",
          "form": "세대차이 난다...미니홈피 같은거야",
          "original_form": "세대차이 난다...미니홈피 같은거야",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007242.1.7",
          "form": "미니홈피 부활하는 걸로 왜 다 난리지?",
          "original_form": "미니홈피 부활하는 걸로 왜 다 난리지?",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007242.1.8",
          "form": "거기가 약간 '난 가끔 눈물을 흘린ㄷr....' 이런류의 글을 주로 올리는 데여서ㅋㅋㅋㅋㅋ",
          "original_form": "거기가 약간 '난 가끔 눈물을 흘린ㄷr....{emoji:☆}' 이런류의 글을 주로 올리는 데여서ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007242.1.9",
          "form": "아~ 그게 싸이월드였구나~",
          "original_form": "아~ 그게 싸이월드였구나~",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007242.1.10",
          "form": "ㅋㅋㅋㅋㅋ 사람들이 흑역사 많이 쌓았지...",
          "original_form": "ㅋㅋㅋㅋㅋ 사람들이 흑역사 많이 쌓았지...",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007242.1.11",
          "form": "언니는 안 했으면 그런거 없었겠네",
          "original_form": "언니는 안 했으면 그런거 없었겠네",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007242.1.12",
          "form": "맞아 다행임ㅎ",
          "original_form": "맞아 다행임ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007242.1.13",
          "form": "요즘으로 치면 인스타 같은건가ㅋㅋㅋㅋ",
          "original_form": "요즘으로 치면 인스타 같은건가ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007242.1.14",
          "form": "부활하면 다 쓸까?",
          "original_form": "부활하면 다 쓸까?",
