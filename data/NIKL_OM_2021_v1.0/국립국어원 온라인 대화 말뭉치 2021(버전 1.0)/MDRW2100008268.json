      "utterance": [
        {
          "id": "MDRW2100008268.1.1",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008268.1.2",
          "form": "뭐 동호회하세요?",
          "original_form": "뭐 동호회하세요?",
          "speaker_id": "1",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008268.1.3",
          "form": "오프라인은 딱히 없어요 하시는것있으세요?",
          "original_form": "오프라인은 딱히 없어요 하시는것있으세요?",
          "speaker_id": "2",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008268.1.4",
          "form": "봉사..",
          "original_form": "봉사..",
          "speaker_id": "1",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008268.1.5",
          "form": "라고 해야할까 헌혈다녀요",
          "original_form": "라고 해야할까 헌혈다녀요",
          "speaker_id": "1",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008268.1.6",
          "form": "아 저 어제 헌혈했어요",
          "original_form": "아 저 어제 헌혈했어요",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008268.1.7",
          "form": "오 헌혈 어쩌다 시작하게되셨는지",
          "original_form": "오 헌혈 어쩌다 시작하게되셨는지",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008268.1.8",
          "form": "여쭤도 될까요?",
          "original_form": "여쭤도 될까요?",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008268.1.9",
          "form": "저는 그냥 어쩌다가? 헌혈의 집 지나가다가? 하게됐는데",
          "original_form": "저는 그냥 어쩌다가? 헌혈의 집 지나가다가? 하게됐는데",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008268.1.10",
          "form": "그이후로는 꾸준히 하려고 하고 있어요",
          "original_form": "그이후로는 꾸준히 하려고 하고 있어요",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008268.1.11",
          "form": "멋지세요!",
          "original_form": "멋지세요!",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008268.1.12",
          "form": "역시 어쩌다 하게되는게",
          "original_form": "역시 어쩌다 하게되는게",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008268.1.13",
          "form": "나중에는 커지는 것같아요",
          "original_form": "나중에는 커지는 것같아요",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008268.1.14",
          "form": "뭔가 각잡고 해야지! 했던 것보다 오래가더라고요",
          "original_form": "뭔가 각잡고 해야지! 했던 것보다 오래가더라고요",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008268.1.15",
          "form": "사실 염불보다 잿밥에 관심 있기도해요 부끄럽지만",
          "original_form": "사실 염불보다 잿밥에 관심 있기도해요 부끄럽지만",
          "speaker_id": "2",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008268.1.16",
          "form": "어제도 영화티켓이 1+1이었다는ㅜ",
          "original_form": "어제도 영화티켓이 1+1이었다는ㅜ",
          "speaker_id": "2",
          "time": "20210822 15:02"
        },
        {
          "id": "MDRW2100008268.1.17",
          "form": "아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008268.1.18",
          "form": "근데 요즘은 이벤트 많이하더라고요",
          "original_form": "근데 요즘은 이벤트 많이하더라고요",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008268.1.19",
          "form": "사람들이 잘 안와서 그런가봐요",
          "original_form": "사람들이 잘 안와서 그런가봐요",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008268.1.20",
          "form": "가시는 것 만으로 대단하신거죠!",
          "original_form": "가시는 것 만으로 대단하신거죠!",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008268.1.21",
          "form": "아 상품도 엄청 좋아진것같던대",
          "original_form": "아 상품도 엄청 좋아진것같던대",
          "speaker_id": "1",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008268.1.22",
          "form": "근데 너무 튕기더라고요",
          "original_form": "근데 너무 튕기더라고요",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008268.1.23",
          "form": "매번부족한 철분 ㅋㅋ",
          "original_form": "매번부족한 철분 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008268.1.24",
          "form": "아이고 철분 열심히 섭취하셔야 겠어요",
          "original_form": "아이고 철분 열심히 섭취하셔야 겠어요",
          "speaker_id": "1",
          "time": "20210822 15:17"
        },
        {
          "id": "MDRW2100008268.1.25",
          "form": "선짓국 먹으러 갑시다! ㅋㅋ",
          "original_form": "선짓국 먹으러 갑시다! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:18"
        },
        {
          "id": "MDRW2100008268.1.26",
          "form": "저 다섯번 튕기고 어제겨우 혈장 했네여 나중엔 뭔가 넘튕기니까 가는것도 거시기했어여ㅜ",
          "original_form": "저 다섯번 튕기고 어제겨우 혈장 했네여 나중엔 뭔가 넘튕기니까 가는것도 거시기했어여ㅜ",
          "speaker_id": "2",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008268.1.27",
          "form": "진짜 고생 많으셨네요 ㅠㅠ",
          "original_form": "진짜 고생 많으셨네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:26"
        }
      ]
    }
