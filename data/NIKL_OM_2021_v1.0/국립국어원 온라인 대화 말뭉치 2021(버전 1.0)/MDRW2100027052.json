      "utterance": [
        {
          "id": "MDRW2100027052.1.1",
          "form": "중고거래 한적은있어요",
          "original_form": "중고거래 한적은있어요",
          "speaker_id": "2",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100027052.1.2",
          "form": "전 당근",
          "original_form": "전 당근",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100027052.1.3",
          "form": "당근을 중고거래하셨다구요?",
          "original_form": "당근을 중고거래하셨다구요?",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027052.1.4",
          "form": "어떻게하셨어요?",
          "original_form": "어떻게하셨어요?",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027052.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100027052.1.6",
          "form": "상하지않아요?",
          "original_form": "상하지않아요?",
          "speaker_id": "2",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100027052.1.7",
          "form": "아니여 당근 마켓해여",
          "original_form": "아니여 당근 마켓해여",
          "speaker_id": "1",
          "time": "20210902 09:55"
        },
        {
          "id": "MDRW2100027052.1.8",
          "form": "아항 그 당근ㅋㅋㅋㅋ",
          "original_form": "아항 그 당근ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027052.1.9",
          "form": "난 먹는 당근생각했어요",
          "original_form": "난 먹는 당근생각했어요",
          "speaker_id": "2",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027052.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:24"
        },
        {
          "id": "MDRW2100027052.1.11",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027052.1.12",
          "form": "중고로 판적도있고",
          "original_form": "중고로 판적도있고",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027052.1.13",
          "form": "산적도있어요",
          "original_form": "산적도있어요",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027052.1.14",
          "form": "주로 나노블럭을 많이 팔았어요",
          "original_form": "주로 나노블럭을 많이 팔았어요",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027052.1.15",
          "form": "네고는 없고요?",
          "original_form": "네고는 없고요?",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100027052.1.16",
          "form": "레고말하는거죠?",
          "original_form": "레고말하는거죠?",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027052.1.17",
          "form": "깍아달라거 하는 분들요ㅋㅋ",
          "original_form": "깍아달라거 하는 분들요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027052.1.18",
          "form": "앜ㅋㅋ그런사람있었어요",
          "original_form": "앜ㅋㅋ그런사람있었어요",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027052.1.19",
          "form": "그래서 제품은 거의 새거고 제가 많이 저렴해서하는거니깐",
          "original_form": "그래서 제품은 거의 새거고 제가 많이 저렴해서하는거니깐",
          "speaker_id": "2",
          "time": "20210902 11:29"
        },
        {
          "id": "MDRW2100027052.1.20",
          "form": "거의80%나 해줬는데",
          "original_form": "거의80%나 해줬는데",
          "speaker_id": "2",
          "time": "20210902 11:30"
