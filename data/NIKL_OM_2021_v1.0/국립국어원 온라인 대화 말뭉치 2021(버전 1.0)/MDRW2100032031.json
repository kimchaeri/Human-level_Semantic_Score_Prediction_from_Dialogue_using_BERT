      "utterance": [
        {
          "id": "MDRW2100032031.1.1",
          "form": "어제 길냥이를 보았어요",
          "original_form": "어제 길냥이를 보았어요",
          "speaker_id": "2",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032031.1.2",
          "form": "동네 터줏대감입니다",
          "original_form": "동네 터줏대감입니다",
          "speaker_id": "2",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.3",
          "form": "어 어떤 길냥이 얻나요 문의가 어쩌죠",
          "original_form": "어 어떤 길냥이 얻나요 문의가 어쩌죠",
          "speaker_id": "1",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.4",
          "form": "사람들이 밥이랑 사료를 주고요",
          "original_form": "사람들이 밥이랑 사료를 주고요",
          "speaker_id": "2",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.5",
          "form": "통통한 가요",
          "original_form": "통통한 가요",
          "speaker_id": "1",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.6",
          "form": "깨끗한 하얀색에 갈색 점박이에요",
          "original_form": "깨끗한 하얀색에 갈색 점박이에요",
          "speaker_id": "2",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.7",
          "form": "너무 너무 귀엽겠네요",
          "original_form": "너무 너무 귀엽겠네요",
          "speaker_id": "1",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.8",
          "form": "아가야 같아요 작거든요 날씬하고",
          "original_form": "아가야 같아요 작거든요 날씬하고",
          "speaker_id": "2",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.9",
          "form": "고양이 굉장히 좋아합니다",
          "original_form": "고양이 굉장히 좋아합니다",
          "speaker_id": "1",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.10",
          "form": "사람 손을 너무 너무 좋아합니다",
          "original_form": "사람 손을 너무 너무 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.11",
          "form": "사람이 만지면 고개를 내밀어요",
          "original_form": "사람이 만지면 고개를 내밀어요",
          "speaker_id": "2",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.12",
          "form": "완전 개냥이인가 봅니다",
          "original_form": "완전 개냥이인가 봅니다",
          "speaker_id": "1",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.13",
          "form": "사람들이 장난감 가지고 와 놀아 줍니다",
          "original_form": "사람들이 장난감 가지고 와 놀아 줍니다",
          "speaker_id": "2",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032031.1.14",
          "form": "누가 데려가면 안 될 거 같아요",
          "original_form": "누가 데려가면 안 될 거 같아요",
          "speaker_id": "2",
          "time": "20210905 11:13"
        },
        {
          "id": "MDRW2100032031.1.15",
          "form": "사랑받고 있군요",
          "original_form": "사랑받고 있군요",
          "speaker_id": "1",
          "time": "20210905 11:13"
        },
        {
          "id": "MDRW2100032031.1.16",
          "form": "모두의 고양이입니다",
          "original_form": "모두의 고양이입니다",
          "speaker_id": "2",
          "time": "20210905 11:13"
        },
        {
          "id": "MDRW2100032031.1.17",
          "form": "그러게 말이에요 만이네 고양이 이네요",
          "original_form": "그러게 말이에요 만이네 고양이 이네요",
          "speaker_id": "1",
