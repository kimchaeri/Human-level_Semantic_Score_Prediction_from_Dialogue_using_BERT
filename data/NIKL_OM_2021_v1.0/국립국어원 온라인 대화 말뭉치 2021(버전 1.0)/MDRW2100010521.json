      "utterance": [
        {
          "id": "MDRW2100010521.1.1",
          "form": "저녁식사는 하셨어요?",
          "original_form": "저녁식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210823 19:12"
        },
        {
          "id": "MDRW2100010521.1.2",
          "form": "아니요 아직요~이제 준비해야 되요",
          "original_form": "아니요 아직요~이제 준비해야 되요",
          "speaker_id": "2",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010521.1.3",
          "form": "직접 요리해드시는거예요?",
          "original_form": "직접 요리해드시는거예요?",
          "speaker_id": "1",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010521.1.4",
          "form": "네~근데 아까 파바에서 빵을 사와서 간단하게 준비만 하려고요",
          "original_form": "네~근데 아까 파바에서 빵을 사와서 간단하게 준비만 하려고요",
          "speaker_id": "2",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010521.1.5",
          "form": "요기요 하셨나요?ㅎ",
          "original_form": "요기요 하셨나요?ㅎ",
          "speaker_id": "1",
          "time": "20210823 19:26"
        },
        {
          "id": "MDRW2100010521.1.6",
          "form": "네~ㅎㅎㅎㅎㅎㅎ 아시는군요 ㅎㅎ",
          "original_form": "네~ㅎㅎㅎㅎㅎㅎ 아시는군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010521.1.7",
          "form": "저도 아까 아빠 주문해줬어요",
          "original_form": "저도 아까 아빠 주문해줬어요",
          "speaker_id": "1",
          "time": "20210823 19:35"
        },
        {
          "id": "MDRW2100010521.1.8",
          "form": "슈퍼클럽 가입도해서",
          "original_form": "슈퍼클럽 가입도해서",
          "speaker_id": "1",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100010521.1.9",
          "form": "오~~효녀시네요~",
          "original_form": "오~~효녀시네요~",
          "speaker_id": "2",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100010521.1.10",
          "form": "아빠가 빵을 넘 좋아해요ㅎ",
          "original_form": "아빠가 빵을 넘 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100010521.1.11",
          "form": "아~~저도 빵을 좋아하거든요",
          "original_form": "아~~저도 빵을 좋아하거든요",
          "speaker_id": "2",
          "time": "20210823 19:37"
        },
        {
          "id": "MDRW2100010521.1.12",
          "form": "5천원할인에 슈퍼클럽쓰니 만원고르고 2천원결제하니 좋아요",
          "original_form": "5천원할인에 슈퍼클럽쓰니 만원고르고 2천원결제하니 좋아요",
          "speaker_id": "1",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010521.1.13",
          "form": "샐러드 비싼데 요럴때 이용하기 좋더라구요",
          "original_form": "샐러드 비싼데 요럴때 이용하기 좋더라구요",
          "speaker_id": "1",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010521.1.14",
          "form": "무슨빵 사셨어요?",
          "original_form": "무슨빵 사셨어요?",
          "speaker_id": "1",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010521.1.15",
          "form": "네~저는 식빵이랑 연유브레드요~",
          "original_form": "네~저는 식빵이랑 연유브레드요~",
          "speaker_id": "2",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010521.1.16",
          "form": "혹시 연유브레드 드셔보셨나요?",
          "original_form": "혹시 연유브레드 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010521.1.17",
          "form": "보긴했는데 맛있어요?",
          "original_form": "보긴했는데 맛있어요?",
          "speaker_id": "1",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010521.1.18",
          "form": "전 고구마페스츄리 크림치즈빵 요런거 좋아해요",
          "original_form": "전 고구마페스츄리 크림치즈빵 요런거 좋아해요",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010521.1.19",
          "form": "부드럽고 촉촉하고 달달해요~저는 최애인데",
          "original_form": "부드럽고 촉촉하고 달달해요~저는 최애인데",
          "speaker_id": "2",
          "time": "20210823 19:42"
        }
      ]
