      "utterance": [
        {
          "id": "MDRW2100002220.1.1",
          "form": "저희둘째가 그렇게 어릴때 길에서 울었네요",
          "original_form": "저희둘째가 그렇게 어릴때 길에서 울었네요",
          "speaker_id": "1",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002220.1.2",
          "form": "저 정말 당황해서 너무 힘들었어요",
          "original_form": "저 정말 당황해서 너무 힘들었어요",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002220.1.3",
          "form": "길에서 요즘 잘 누우려고해요 저희애도ㅎㅎ",
          "original_form": "길에서 요즘 잘 누우려고해요 저희애도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002220.1.4",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002220.1.5",
          "form": "떼쟁이 시작이라",
          "original_form": "떼쟁이 시작이라",
          "speaker_id": "2",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002220.1.6",
          "form": "근데 크니까 정말 바른 아이가 됐어요",
          "original_form": "근데 크니까 정말 바른 아이가 됐어요",
          "speaker_id": "1",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002220.1.7",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "1",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002220.1.8",
          "form": "잘안나가게되여ㅜㅜ",
          "original_form": "잘안나가게되여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002220.1.9",
          "form": "시간이 약이네요",
          "original_form": "시간이 약이네요",
          "speaker_id": "2",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002220.1.10",
          "form": "저희애도 보통아니 였어요",
          "original_form": "저희애도 보통아니 였어요",
          "speaker_id": "1",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002220.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002220.1.12",
          "form": "티비 나올뻔 했어요",
          "original_form": "티비 나올뻔 했어요",
          "speaker_id": "1",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002220.1.13",
          "form": "시간이 안가요ㅠ",
          "original_form": "시간이 안가요ㅠ",
          "speaker_id": "2",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100002220.1.14",
          "form": "고생 많으셧겟어요",
          "original_form": "고생 많으셧겟어요",
          "speaker_id": "2",
          "time": "20210820 00:47"
        },
        {
          "id": "MDRW2100002220.1.15",
          "form": "말도 마세요 챙피허고 왜그러나 이해를 못하니 넘 힘들었어요",
          "original_form": "말도 마세요 챙피허고 왜그러나 이해를 못하니 넘 힘들었어요",
          "speaker_id": "1",
          "time": "20210820 00:47"
        },
        {
          "id": "MDRW2100002220.1.16",
          "form": "듣기만해도 힘드네요ㅠ",
          "original_form": "듣기만해도 힘드네요ㅠ",
          "speaker_id": "2",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002220.1.17",
          "form": "네 지금 생각해도 힘드네요",
          "original_form": "네 지금 생각해도 힘드네요",
          "speaker_id": "1",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002220.1.18",
          "form": "지금은 예의가 바른데 밖에서 주로 잘하네요 ㅋㅋ",
          "original_form": "지금은 예의가 바른데 밖에서 주로 잘하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100002220.1.19",
          "form": "그래도 다 키워놓으셔서 부러워요",
          "original_form": "그래도 다 키워놓으셔서 부러워요",
          "speaker_id": "2",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002220.1.20",
          "form": "아직 딸이 한참 남았어요",
          "original_form": "아직 딸이 한참 남았어요",
          "speaker_id": "1",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002220.1.21",
          "form": "벌써 걱정돼요 중고를 어찌 보낼지",
          "original_form": "벌써 걱정돼요 중고를 어찌 보낼지",
          "speaker_id": "1",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002220.1.22",
          "form": "매순간 아이들은 엄마에게 고민을 줘요",
          "original_form": "매순간 아이들은 엄마에게 고민을 줘요",
          "speaker_id": "1",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002220.1.23",
          "form": "전  조딩이라도 언제 만들지요ㅎㅎ",
          "original_form": "전  조딩이라도 언제 만들지요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002220.1.24",
          "form": "기가 너무 빨려여ㅜㅜ",
          "original_form": "기가 너무 빨려여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002220.1.25",
          "form": "금새돼요",
          "original_form": "금새돼요",
          "speaker_id": "1",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002220.1.26",
          "form": "시간이 안가요ㅜㅜ 다들 금새라시는데",
          "original_form": "시간이 안가요ㅜㅜ 다들 금새라시는데",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002220.1.27",
          "form": "어릴땐 크길 바라는데 또 어릴때가 생각나요",
          "original_form": "어릴땐 크길 바라는데 또 어릴때가 생각나요",
          "speaker_id": "1",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002220.1.28",
          "form": "그때 그런듯해요 조금 지나면 금새죠",
          "original_form": "그때 그런듯해요 조금 지나면 금새죠",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002220.1.29",
          "form": "저도 그맘 알거같아요",
          "original_form": "저도 그맘 알거같아요",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002220.1.30",
          "form": "신생아때는 돌만 지나라했는데",
          "original_form": "신생아때는 돌만 지나라했는데",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002220.1.31",
          "form": "네 참 예뻤는데",
          "original_form": "네 참 예뻤는데",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002220.1.32",
          "form": "지금보니 신생아가 젤 편햇어요",
          "original_form": "지금보니 신생아가 젤 편햇어요",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002220.1.33",
          "form": "맞아요 그땐 몰랐어요 ㅜㅜ",
          "original_form": "맞아요 그땐 몰랐어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002220.1.34",
          "form": "근데 지금잇는애에 신생아 추가라 무서워요ㅋ",
          "original_form": "근데 지금잇는애에 신생아 추가라 무서워요ㅋ",
          "speaker_id": "2",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002220.1.35",
          "form": "어떻게 재울지ㅋㅋ",
          "original_form": "어떻게 재울지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002220.1.36",
          "form": "헉 진짜 걱정되네요",
          "original_form": "헉 진짜 걱정되네요",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002220.1.37",
          "form": "둘다 너무 어려서",
          "original_form": "둘다 너무 어려서",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002220.1.38",
          "form": "지금 잘 자둬야 겠어요",
          "original_form": "지금 잘 자둬야 겠어요",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002220.1.39",
          "form": "잠이 안와요ㅠ",
          "original_form": "잠이 안와요ㅠ",
          "speaker_id": "2",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002220.1.40",
          "form": "큰일이네요 그때 어쩐데요",
          "original_form": "큰일이네요 그때 어쩐데요",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002220.1.41",
          "form": "그냥 뜬눈이에요ㅎㅎ",
          "original_form": "그냥 뜬눈이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002220.1.42",
          "form": "뜬눈 저도 그래요 밤새 깊이 못자요",
          "original_form": "뜬눈 저도 그래요 밤새 깊이 못자요",
          "speaker_id": "1",
          "time": "20210820 00:54"
        }
      ]
