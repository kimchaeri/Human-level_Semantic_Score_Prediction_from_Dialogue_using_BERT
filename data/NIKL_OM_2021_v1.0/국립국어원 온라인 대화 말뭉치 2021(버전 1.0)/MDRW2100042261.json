      "utterance": [
        {
          "id": "MDRW2100042261.1.1",
          "form": "넘비싸서 살수있을지ㅋㅋ",
          "original_form": "넘비싸서 살수있을지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100042261.1.2",
          "form": "그리고 이번에 올영세일있어서",
          "original_form": "그리고 이번에 올영세일있어서",
          "speaker_id": "2",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042261.1.3",
          "form": "화장품 질렀네여",
          "original_form": "화장품 질렀네여",
          "speaker_id": "2",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042261.1.4",
          "form": "오 !!! 어뜬거사셨어요.기초?",
          "original_form": "오 !!! 어뜬거사셨어요.기초?",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042261.1.5",
          "form": "올영세일할때마다 텅장 ㅠ",
          "original_form": "올영세일할때마다 텅장 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042261.1.6",
          "form": "라로슈포제 히알루론 세럼이랑",
          "original_form": "라로슈포제 히알루론 세럼이랑",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042261.1.7",
          "form": "가희 멀티밤샀어요",
          "original_form": "가희 멀티밤샀어요",
          "speaker_id": "2",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042261.1.8",
          "form": "요즘가히 많이쓰더라구요 좋나요,.",
          "original_form": "요즘가히 많이쓰더라구요 좋나요,.",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042261.1.9",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042261.1.10",
          "form": "저도 처음이요",
          "original_form": "저도 처음이요",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042261.1.11",
          "form": "5종류나 있는데",
          "original_form": "5종류나 있는데",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042261.1.12",
          "form": "기본사봤어요",
          "original_form": "기본사봤어요",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042261.1.13",
          "form": "오 그것도세일하던가요?",
          "original_form": "오 그것도세일하던가요?",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042261.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042261.1.15",
          "form": "기초는 어떤거 쓰세요",
          "original_form": "기초는 어떤거 쓰세요",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042261.1.16",
          "form": "항상 수분크림 최대관심사인데",
          "original_form": "항상 수분크림 최대관심사인데",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042261.1.17",
          "form": "전ㅠㅠ집콕이라 잘안발라여ㅠ",
          "original_form": "전ㅠㅠ집콕이라 잘안발라여ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042261.1.18",
          "form": "애도어리고 ㅠㅠ",
          "original_form": "애도어리고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042261.1.19",
          "form": "그러시군여",
          "original_form": "그러시군여",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042261.1.20",
          "form": "바를시간이 없는건 핑계겠죵ㅋ",
          "original_form": "바를시간이 없는건 핑계겠죵ㅋ",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042261.1.21",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042261.1.22",
          "form": "그래도되죠머",
          "original_form": "그래도되죠머",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042261.1.23",
          "form": "그럼 주로 식료품 구입하시겠어요",
          "original_form": "그럼 주로 식료품 구입하시겠어요",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042261.1.24",
          "form": "맞아요 ㅎㅎ 애도있고하니 ㅎ",
          "original_form": "맞아요 ㅎㅎ 애도있고하니 ㅎ",
          "speaker_id": "1",
