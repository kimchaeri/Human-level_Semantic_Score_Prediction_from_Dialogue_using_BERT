      "utterance": [
        {
          "id": "MDRW2100011254.1.1",
          "form": "비가 너무 많이와요 온니~",
          "original_form": "비가 너무 많이와요 온니~",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011254.1.2",
          "form": "운전 조심해요ㅠ",
          "original_form": "운전 조심해요ㅠ",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011254.1.3",
          "form": "비바람이네ㅜㅜ",
          "original_form": "비바람이네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100011254.1.4",
          "form": "남해쪽은 비안온데",
          "original_form": "남해쪽은 비안온데",
          "speaker_id": "2",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100011254.1.5",
          "form": "비가 어마무시 하네여~",
          "original_form": "비가 어마무시 하네여~",
          "speaker_id": "1",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100011254.1.6",
          "form": "난 이런날씨 넘나 좋은데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "난 이런날씨 넘나 좋은데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100011254.1.7",
          "form": "밖이 새까맣네요~",
          "original_form": "밖이 새까맣네요~",
          "speaker_id": "1",
          "time": "20210824 10:17"
        },
        {
          "id": "MDRW2100011254.1.8",
          "form": "애들 다젖겠다ㅜㅜ",
          "original_form": "애들 다젖겠다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 10:48"
        },
        {
          "id": "MDRW2100011254.1.9",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210824 10:49"
        },
        {
          "id": "MDRW2100011254.1.10",
          "form": "애들은 번개만 안치면 신날꺼예요 ㅋㅋㅋㅋ",
          "original_form": "애들은 번개만 안치면 신날꺼예요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100011254.1.11",
          "form": "첫찌 다른친구들 다 장화 신는다고 ㅋㅋㅋㅋ장화사달라고~",
          "original_form": "첫찌 다른친구들 다 장화 신는다고 ㅋㅋㅋㅋ장화사달라고~",
          "speaker_id": "1",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100011254.1.12",
          "form": "초딩은 장화 안신는줄 알았네요 ㅋㅋㅋ",
          "original_form": "초딩은 장화 안신는줄 알았네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100011254.1.13",
          "form": "우린 다 장화ㅋㅋ",
          "original_form": "우린 다 장화ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100011254.1.14",
          "form": "지금비안오네ㅋㅋ",
          "original_form": "지금비안오네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:42"
        },
        {
          "id": "MDRW2100011254.1.15",
          "form": "징짜여~장화 방금 샀는데 ㅋㅋㅋㅋㅋ",
          "original_form": "징짜여~장화 방금 샀는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:48"
        },
        {
          "id": "MDRW2100011254.1.16",
          "form": "날씨 뭐 이러죠??ㅋㅋㅋㅋ",
          "original_form": "날씨 뭐 이러죠??ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:48"
        },
        {
          "id": "MDRW2100011254.1.17",
          "form": "바람만분다",
          "original_form": "바람만분다",
          "speaker_id": "2",
          "time": "20210824 12:13"
        },
        {
          "id": "MDRW2100011254.1.18",
          "form": "근데덥다이",
          "original_form": "근데덥다이",
          "speaker_id": "2",
          "time": "20210824 12:13"
        },
        {
          "id": "MDRW2100011254.1.19",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 12:13"
        },
        {
          "id": "MDRW2100011254.1.20",
          "form": "ㅋㅋㅋㅋㅋ그 후덥지근한 바람여~",
          "original_form": "ㅋㅋㅋㅋㅋ그 후덥지근한 바람여~",
          "speaker_id": "1",
          "time": "20210824 12:26"
        },
        {
          "id": "MDRW2100011254.1.21",
          "form": "실외기 바람같은거 ㅋㅋㅋㅋ",
          "original_form": "실외기 바람같은거 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:27"
        },
        {
          "id": "MDRW2100011254.1.22",
          "form": "안그래도 창고에 있는데..문열리는소리나서 나왔디..",
          "original_form": "안그래도 창고에 있는데..문열리는소리나서 나왔디..",
          "speaker_id": "1",
          "time": "20210824 12:27"
        },
        {
          "id": "MDRW2100011254.1.23",
          "form": "바람이 오셨네요 ㅋㅋㅋㅋ",
          "original_form": "바람이 오셨네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:27"
        },
        {
          "id": "MDRW2100011254.1.24",
          "form": "바람ㅋㅋㅋ",
          "original_form": "바람ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:35"
        },
        {
          "id": "MDRW2100011254.1.25",
          "form": "오늘 손님 1도 없구만요 ㅋㅋㅋㅋ",
          "original_form": "오늘 손님 1도 없구만요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100011254.1.26",
          "form": "여긴 날씨영향을 진짜 많이 받네여~",
          "original_form": "여긴 날씨영향을 진짜 많이 받네여~",
          "speaker_id": "1",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100011254.1.27",
          "form": "비와서 손님안오는갑네",
          "original_form": "비와서 손님안오는갑네",
          "speaker_id": "2",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100011254.1.28",
          "form": "푹쉬어라ㅋㅋㅋㅋ",
          "original_form": "푹쉬어라ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100011254.1.29",
          "form": "오늘 4시까진데..쉬는데도 한계가 ㅋㅋㅋㅋ",
          "original_form": "오늘 4시까진데..쉬는데도 한계가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011254.1.30",
          "form": "늠나 지겹네요~자꾸 바람불어가 문은 열리고 ㅋㅋㅋㅋㅋ",
          "original_form": "늠나 지겹네요~자꾸 바람불어가 문은 열리고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011254.1.31",
          "form": "오늘 4시까지가? 오래하노",
          "original_form": "오늘 4시까지가? 오래하노",
          "speaker_id": "2",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100011254.1.32",
          "form": "화욜은 여기 언니가 다른데 알바해주는날이라ㅠ",
          "original_form": "화욜은 여기 언니가 다른데 알바해주는날이라ㅠ",
          "speaker_id": "1",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100011254.1.33",
          "form": "화욜 4시까지 목욜 쉬고 이래 된거요~",
          "original_form": "화욜 4시까지 목욜 쉬고 이래 된거요~",
          "speaker_id": "1",
          "time": "20210824 13:19"
        },
        {
          "id": "MDRW2100011254.1.34",
          "form": "목욜엔 비가 안와야 되는데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "목욜엔 비가 안와야 되는데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:20"
        },
        {
          "id": "MDRW2100011254.1.35",
          "form": "소중한 휴무니 ㅋㅋㅋㅋ",
          "original_form": "소중한 휴무니 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 13:20"
        },
        {
          "id": "MDRW2100011254.1.36",
          "form": "그래 소중한휴무 목욜에 모디지ㅡ 나중에ㅋㅋㅋ",
          "original_form": "그래 소중한휴무 목욜에 모디지ㅡ 나중에ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011254.1.37",
          "form": "비안오드라",
          "original_form": "비안오드라",
          "speaker_id": "2",
          "time": "20210824 13:37"
        },
        {
          "id": "MDRW2100011254.1.38",
          "form": "일기예보ㅋㅋ",
          "original_form": "일기예보ㅋㅋ",
