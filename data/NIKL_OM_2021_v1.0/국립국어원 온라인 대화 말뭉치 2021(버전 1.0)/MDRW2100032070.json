      "utterance": [
        {
          "id": "MDRW2100032070.1.1",
          "form": "취준생은 오늘도 자소설을 쓰는데",
          "original_form": "취준생은 오늘도 자소설을 쓰는데",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032070.1.2",
          "form": "대충쓸까",
          "original_form": "대충쓸까",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032070.1.3",
          "form": "반도체 아니고 배터리 그냥 써보게 쓰는건데",
          "original_form": "반도체 아니고 배터리 그냥 써보게 쓰는건데",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032070.1.4",
          "form": "해당 분야에 큰 욕심 없고 좀 쉬고싶다면?",
          "original_form": "해당 분야에 큰 욕심 없고 좀 쉬고싶다면?",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100032070.1.5",
          "form": "붙여주면 좋기야한데",
          "original_form": "붙여주면 좋기야한데",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100032070.1.6",
          "form": "나라도 나 안뽑아서",
          "original_form": "나라도 나 안뽑아서",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100032070.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ한게없으니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋ한게없으니까",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100032070.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러면 가볍게 써서 제출해봐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러면 가볍게 써서 제출해봐",
          "speaker_id": "2",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032070.1.9",
          "form": "웅웅 그래야겠어",
          "original_form": "웅웅 그래야겠어",
          "speaker_id": "1",
          "time": "20210905 12:03"
        },
        {
          "id": "MDRW2100032070.1.10",
          "form": "꿈이뭐라고?",
          "original_form": "꿈이뭐라고?",
          "speaker_id": "1",
          "time": "20210905 12:03"
        },
        {
          "id": "MDRW2100032070.1.11",
          "form": "렛유인 미리 해봐 진짜 좋아",
          "original_form": "렛유인 미리 해봐 진짜 좋아",
          "speaker_id": "1",
          "time": "20210905 12:03"
        },
        {
          "id": "MDRW2100032070.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100032070.1.13",
          "form": "취업 사이트인데 카페도 있고 유튜브도 있으",
          "original_form": "취업 사이트인데 카페도 있고 유튜브도 있으",
          "speaker_id": "1",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100032070.1.14",
          "form": "이공계 취업 !",
          "original_form": "이공계 취업 !",
          "speaker_id": "1",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100032070.1.15",
          "form": "그러네",
          "original_form": "그러네",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100032070.1.16",
          "form": "좋아보인다",
          "original_form": "좋아보인다",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100032070.1.17",
          "form": "아는게 많구나 너?ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아는게 많구나 너?ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:05"
        },
        {
          "id": "MDRW2100032070.1.18",
          "form": "아냐ㅠ 좀더 일찍알았어야했어",
          "original_form": "아냐ㅠ 좀더 일찍알았어야했어",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032070.1.19",
          "form": "넌 안늦었으니까",
          "original_form": "넌 안늦었으니까",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032070.1.20",
          "form": "지금부터라도 해ㅠ",
          "original_form": "지금부터라도 해ㅠ",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032070.1.21",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032070.1.22",
          "form": "고마워고마워",
          "original_form": "고마워고마워",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032070.1.23",
          "form": "자소서 쓰는건 좀 익숙해졌어?",
          "original_form": "자소서 쓰는건 좀 익숙해졌어?",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032070.1.24",
          "form": "익숙은 해졌짘ㅋㅋㅋ",
          "original_form": "익숙은 해졌짘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032070.1.25",
          "form": "능력 향상은 아니지만",
          "original_form": "능력 향상은 아니지만",
          "speaker_id": "1",
          "time": "20210905 12:13"
        },
        {
          "id": "MDRW2100032070.1.26",
          "form": "그것도 능력 향상이긴하지",
          "original_form": "그것도 능력 향상이긴하지",
          "speaker_id": "2",
          "time": "20210905 12:19"
        },
        {
          "id": "MDRW2100032070.1.27",
          "form": "오 그런가ㅎㅎㅎ",
          "original_form": "오 그런가ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100032070.1.28",
          "form": "그것참 좋은 희망 이야",
          "original_form": "그것참 좋은 희망 이야",
          "speaker_id": "1",
          "time": "20210905 12:21"
        },
        {
          "id": "MDRW2100032070.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그럼그럼",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그럼그럼",
          "speaker_id": "2",
          "time": "20210905 12:26"
        },
        {
          "id": "MDRW2100032070.1.30",
          "form": "카페에서 자소서 쓰는거야?",
          "original_form": "카페에서 자소서 쓰는거야?",
          "speaker_id": "2",
          "time": "20210905 12:27"
        },
        {
          "id": "MDRW2100032070.1.31",
          "form": "웅웅 취업ㅠ해야지",
          "original_form": "웅웅 취업ㅠ해야지",
          "speaker_id": "1",
          "time": "20210905 12:40"
        },
        {
          "id": "MDRW2100032070.1.32",
          "form": "나 삼성 이력서에 존경인물 이랑 사유적어야하는데",
          "original_form": "나 삼성 이력서에 존경인물 이랑 사유적어야하는데",
          "speaker_id": "1",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100032070.1.33",
          "form": "어떻게 하는게 좋지,..",
          "original_form": "어떻게 하는게 좋지,..",
          "speaker_id": "1",
          "time": "20210907 16:14"
        }
      ]
    }
