      "utterance": [
        {
          "id": "MDRW2100030245.1.1",
          "form": "휴... 학교 수업 듣는거 너무 싫어",
          "original_form": "휴... 학교 수업 듣는거 너무 싫어",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030245.1.2",
          "form": "무슨 과목이 제일 좋은거같애?",
          "original_form": "무슨 과목이 제일 좋은거같애?",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030245.1.3",
          "form": "학교 수업이 다 그렇지 ㅎㅎ",
          "original_form": "학교 수업이 다 그렇지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030245.1.4",
          "form": "난 급식시간 ㅋ",
          "original_form": "난 급식시간 ㅋ",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030245.1.5",
          "form": "장난이구",
          "original_form": "장난이구",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030245.1.6",
          "form": "음악이 좋아",
          "original_form": "음악이 좋아",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030245.1.7",
          "form": "그렇구나 ㅋㅋ",
          "original_form": "그렇구나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.8",
          "form": "난 음악 너무 싫어!",
          "original_form": "난 음악 너무 싫어!",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.9",
          "form": "취향존중 못하겠어",
          "original_form": "취향존중 못하겠어",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.10",
          "form": "왜!",
          "original_form": "왜!",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.11",
          "form": "음악 잼잇잖아~!!!!",
          "original_form": "음악 잼잇잖아~!!!!",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.12",
          "form": "가끔 피아노도 치구 ㅋ",
          "original_form": "가끔 피아노도 치구 ㅋ",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.13",
          "form": "음악 7시간하느니 체육 7시간을 하겠어",
          "original_form": "음악 7시간하느니 체육 7시간을 하겠어",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.14",
          "form": "헐 진좌야?",
          "original_form": "헐 진좌야?",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.15",
          "form": "그정도여",
          "original_form": "그정도여",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.16",
          "form": "웅 진짜야",
          "original_form": "웅 진짜야",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.17",
          "form": "체육 7시간이면 힘들어서 주글듯...",
          "original_form": "체육 7시간이면 힘들어서 주글듯...",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.18",
          "form": "진심 음악은 너무 싫어!",
          "original_form": "진심 음악은 너무 싫어!",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030245.1.19",
          "form": "왜케 시러하는데?!",
          "original_form": "왜케 시러하는데?!",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030245.1.20",
          "form": "음악 7시간도 힘들어서 주그니깐 걱정마",
          "original_form": "음악 7시간도 힘들어서 주그니깐 걱정마",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030245.1.21",
          "form": "노래불러야대서?",
          "original_form": "노래불러야대서?",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030245.1.22",
          "form": "웅.. 그리구 립싱크하는거도 지루해",
          "original_form": "웅.. 그리구 립싱크하는거도 지루해",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030245.1.23",
          "form": "그냥 아무덧도 안하는 시간이나 다름없잖아",
          "original_form": "그냥 아무덧도 안하는 시간이나 다름없잖아",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030245.1.24",
          "form": "헝 ㅋㅋ 슬프다",
          "original_form": "헝 ㅋㅋ 슬프다",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030245.1.25",
          "form": "나는 문학 과목이 좋아!!",
          "original_form": "나는 문학 과목이 좋아!!",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030245.1.26",
          "form": "재미써 지문이",
          "original_form": "재미써 지문이",
          "speaker_id": "1",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100030245.1.27",
          "form": "오 ㅎㅎ 나두 그래",
          "original_form": "오 ㅎㅎ 나두 그래",
