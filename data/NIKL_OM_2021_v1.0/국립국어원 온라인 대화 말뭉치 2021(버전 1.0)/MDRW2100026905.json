      "utterance": [
        {
          "id": "MDRW2100026905.1.1",
          "form": "메타버스 들어보셨나요",
          "original_form": "메타버스 들어보셨나요",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100026905.1.2",
          "form": "네 메타버스는 핫하죠",
          "original_form": "네 메타버스는 핫하죠",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100026905.1.3",
          "form": "요즘핫이슈죠",
          "original_form": "요즘핫이슈죠",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100026905.1.4",
          "form": "정해진 미래산업이라고 볼수있죠",
          "original_form": "정해진 미래산업이라고 볼수있죠",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100026905.1.5",
          "form": "게임에서도 많이 접목되더라구요",
          "original_form": "게임에서도 많이 접목되더라구요",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100026905.1.6",
          "form": "엠게임, 카카오게임즈 주식으로 수익보았습니다",
          "original_form": "엠게임, 카카오게임즈 주식으로 수익보았습니다",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100026905.1.7",
          "form": "관련주식도 많이올랐죠",
          "original_form": "관련주식도 많이올랐죠",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026905.1.8",
          "form": "선거활동도 메타버스이고요",
          "original_form": "선거활동도 메타버스이고요",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026905.1.9",
          "form": "펄어비스에서 출시되는 게임에 메타버스 적용할예정이래요",
          "original_form": "펄어비스에서 출시되는 게임에 메타버스 적용할예정이래요",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026905.1.10",
          "form": "네 신기한 미래입니다",
          "original_form": "네 신기한 미래입니다",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026905.1.11",
          "form": "이름은 도깨비",
          "original_form": "이름은 도깨비",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026905.1.12",
          "form": "기대되네요",
          "original_form": "기대되네요",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026905.1.13",
          "form": "게임들이 기대됩니다",
          "original_form": "게임들이 기대됩니다",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026905.1.14",
          "form": "안경쓰면 게임세계로 들어가겠죠?",
          "original_form": "안경쓰면 게임세계로 들어가겠죠?",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026905.1.15",
          "form": "관련 주식도요",
          "original_form": "관련 주식도요",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100026905.1.16",
          "form": "너무 멋져요",
          "original_form": "너무 멋져요",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100026905.1.17",
          "form": "관심주 있으시나요?",
          "original_form": "관심주 있으시나요?",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100026905.1.18",
          "form": "펄어비스는 사셨나요",
          "original_form": "펄어비스는 사셨나요",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100026905.1.19",
          "form": "맥스트 공모주에",
          "original_form": "맥스트 공모주에",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100026905.1.20",
          "form": "청약했었죠",
          "original_form": "청약했었죠",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
