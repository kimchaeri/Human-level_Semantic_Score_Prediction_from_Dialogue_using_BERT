      "utterance": [
        {
          "id": "MDRW2100014956.1.1",
          "form": "저는 그 기아 응원가서 치킨만 끝내주게 뜯었어요",
          "original_form": "저는 그 기아 응원가서 치킨만 끝내주게 뜯었어요",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014956.1.2",
          "form": "앗 아뇨",
          "original_form": "앗 아뇨",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014956.1.3",
          "form": "대신 누가 이길지 투표하는 이벤은 참여했어요",
          "original_form": "대신 누가 이길지 투표하는 이벤은 참여했어요",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014956.1.4",
          "form": "누구 투표하셨나요",
          "original_form": "누구 투표하셨나요",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014956.1.5",
          "form": "무족권 기아죠",
          "original_form": "무족권 기아죠",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014956.1.6",
          "form": "역쒸",
          "original_form": "역쒸",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014956.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014956.1.8",
          "form": "야구장은 약간 그 분위기가 너무 재밌는거같아요",
          "original_form": "야구장은 약간 그 분위기가 너무 재밌는거같아요",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014956.1.9",
          "form": "거기서 야구 팬들 포인트 받을 수 있게 해놨더라고요",
          "original_form": "거기서 야구 팬들 포인트 받을 수 있게 해놨더라고요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014956.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014956.1.11",
          "form": "그 다같이 응원가 부를때",
          "original_form": "그 다같이 응원가 부를때",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014956.1.12",
          "form": "응원하고 음식먹고",
          "original_form": "응원하고 음식먹고",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014956.1.13",
          "form": "넘 재밋어요",
          "original_form": "넘 재밋어요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014956.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014956.1.15",
          "form": "음식 섭취 가능이 신의 한수",
          "original_form": "음식 섭취 가능이 신의 한수",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014956.1.16",
          "form": "그러다 공 하나씩 잡고",
          "original_form": "그러다 공 하나씩 잡고",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014956.1.17",
          "form": "가끔 비빔밥 비벼드시는 분도 계시더라구요",
          "original_form": "가끔 비빔밥 비벼드시는 분도 계시더라구요",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014956.1.18",
          "form": "예? 공을요?",
          "original_form": "예? 공을요?",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014956.1.19",
          "form": "비벼먹어요??",
          "original_form": "비벼먹어요??",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014956.1.20",
          "form": "아니 아니ㅋㅋㅋ 공은 잡고",
          "original_form": "아니 아니ㅋㅋㅋ 공은 잡고",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014956.1.21",
          "form": "양푼가져와서 비빔밥해먹는 사람이 있더라구요",
          "original_form": "양푼가져와서 비빔밥해먹는 사람이 있더라구요",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014956.1.22",
          "form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014956.1.23",
          "form": "보통 치킨먹잖아요",
          "original_form": "보통 치킨먹잖아요",
          "speaker_id": "1",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014956.1.24",
          "form": "본격적이시네요 비빔밥",
          "original_form": "본격적이시네요 비빔밥",
          "speaker_id": "2",
