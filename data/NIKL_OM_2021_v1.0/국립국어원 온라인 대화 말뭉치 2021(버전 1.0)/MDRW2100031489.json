      "utterance": [
        {
          "id": "MDRW2100031489.1.1",
          "form": "넌 데바데 플레이타임이 어떻게 돼?",
          "original_form": "넌 데바데 플레이타임이 어떻게 돼?",
          "speaker_id": "1",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100031489.1.2",
          "form": "나는 아마 100시간정도 한 것 같아",
          "original_form": "나는 아마 100시간정도 한 것 같아",
          "speaker_id": "2",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100031489.1.3",
          "form": "별로 안했구나",
          "original_form": "별로 안했구나",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100031489.1.4",
          "form": "너는 얼마나 했는데?",
          "original_form": "너는 얼마나 했는데?",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031489.1.5",
          "form": "나는 1000시간 가까이 했어",
          "original_form": "나는 1000시간 가까이 했어",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031489.1.6",
          "form": "1000시간? 진짜 많이 했구나?",
          "original_form": "1000시간? 진짜 많이 했구나?",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031489.1.7",
          "form": "데바데가 고인물 게임이라 많이 한 편은 아니야",
          "original_form": "데바데가 고인물 게임이라 많이 한 편은 아니야",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100031489.1.8",
          "form": "고인물들은 몇 시간 정도 하는데?",
          "original_form": "고인물들은 몇 시간 정도 하는데?",
          "speaker_id": "2",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100031489.1.9",
          "form": "주로 7천시간 이상씩은 하고 많이 하면 만시간 정도 해",
          "original_form": "주로 7천시간 이상씩은 하고 많이 하면 만시간 정도 해",
          "speaker_id": "1",
          "time": "20210904 22:08"
        },
        {
          "id": "MDRW2100031489.1.10",
          "form": "와 진짜 대단하다",
          "original_form": "와 진짜 대단하다",
          "speaker_id": "2",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100031489.1.11",
          "form": "너 주로 보는 게임 유튜버가 누구야?",
          "original_form": "너 주로 보는 게임 유튜버가 누구야?",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031489.1.12",
          "form": "나는 데바데 방송 봐",
          "original_form": "나는 데바데 방송 봐",
          "speaker_id": "1",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100031489.1.13",
          "form": "누구 보는데?",
          "original_form": "누구 보는데?",
          "speaker_id": "2",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100031489.1.14",
          "form": "맥또마또",
          "original_form": "맥또마또",
          "speaker_id": "1",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100031489.1.15",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210904 22:18"
        },
        {
          "id": "MDRW2100031489.1.16",
          "form": "나는 살인마협회장이란 사람 좋아해",
          "original_form": "나는 살인마협회장이란 사람 좋아해",
          "speaker_id": "2",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100031489.1.17",
          "form": "그사람이 왜 좋아?",
          "original_form": "그사람이 왜 좋아?",
          "speaker_id": "1",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100031489.1.18",
          "form": "게임을 잘해?",
          "original_form": "게임을 잘해?",
          "speaker_id": "1",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100031489.1.19",
          "form": "응 특히 살인마를 엄청 잘해",
          "original_form": "응 특히 살인마를 엄청 잘해",
          "speaker_id": "2",
          "time": "20210904 22:20"
        },
        {
          "id": "MDRW2100031489.1.20",
          "form": "완전 1티어야",
          "original_form": "완전 1티어야",
          "speaker_id": "2",
          "time": "20210904 22:20"
        },
        {
          "id": "MDRW2100031489.1.21",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210904 22:20"
        },
        {
          "id": "MDRW2100031489.1.22",
          "form": "또 보는 사람 있어?",
          "original_form": "또 보는 사람 있어?",
          "speaker_id": "1",
          "time": "20210904 22:20"
        },
        {
          "id": "MDRW2100031489.1.23",
          "form": "나는 없어 너는?",
          "original_form": "나는 없어 너는?",
          "speaker_id": "2",
          "time": "20210904 22:21"
        },
        {
          "id": "MDRW2100031489.1.24",
          "form": "나는 게임 하는 사람들 트위치 방송으로 많이 봐",
          "original_form": "나는 게임 하는 사람들 트위치 방송으로 많이 봐",
          "speaker_id": "1",
          "time": "20210904 22:22"
        },
        {
          "id": "MDRW2100031489.1.25",
          "form": "아 나 두 명 더 있다!",
          "original_form": "아 나 두 명 더 있다!",
          "speaker_id": "2",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100031489.1.26",
          "form": "누구?",
          "original_form": "누구?",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100031489.1.27",
          "form": "데바데 스트리머는 아닌데 오버워치 하는 사람",
          "original_form": "데바데 스트리머는 아닌데 오버워치 하는 사람",
          "speaker_id": "2",
          "time": "20210904 22:25"
        }
      ]
    }
  ]
