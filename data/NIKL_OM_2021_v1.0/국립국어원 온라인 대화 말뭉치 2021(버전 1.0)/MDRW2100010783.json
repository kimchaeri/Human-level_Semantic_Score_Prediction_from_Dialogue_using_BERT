      "utterance": [
        {
          "id": "MDRW2100010783.1.1",
          "form": "아 백신 때문에요? 아직요! ㅎㅎ",
          "original_form": "아 백신 때문에요? 아직요! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010783.1.2",
          "form": "네네 혹시 후유증이 있으면",
          "original_form": "네네 혹시 후유증이 있으면",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010783.1.3",
          "form": "먹어야하니까요",
          "original_form": "먹어야하니까요",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010783.1.4",
          "form": "네 사둬야죠 ㅎㅎ",
          "original_form": "네 사둬야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100010783.1.5",
          "form": "2차 맞으시면 개운하겠어요 ㅎㅎ",
          "original_form": "2차 맞으시면 개운하겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 22:38"
        },
        {
          "id": "MDRW2100010783.1.6",
          "form": "뭔가 좀 불안의 굴레에서 벗어나는 기분이 들어요 ㅎㅎ",
          "original_form": "뭔가 좀 불안의 굴레에서 벗어나는 기분이 들어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010783.1.7",
          "form": "그래도 방심할순없을거같아요 변이도 많아서",
          "original_form": "그래도 방심할순없을거같아요 변이도 많아서",
          "speaker_id": "1",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010783.1.8",
          "form": "그건 그렇죠 ㅠㅠ",
          "original_form": "그건 그렇죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010783.1.9",
          "form": "세계적으로 또 조짐이 있으니..",
          "original_form": "세계적으로 또 조짐이 있으니..",
          "speaker_id": "2",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010783.1.10",
          "form": "네 이제 끼고사는수밖에 없대요 ㅋㅋㅋㅋ 섭섭하여라 삼겹살먹고 주사맞으러가시나요?",
          "original_form": "네 이제 끼고사는수밖에 없대요 ㅋㅋㅋㅋ 섭섭하여라 삼겹살먹고 주사맞으러가시나요?",
          "speaker_id": "1",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100010783.1.11",
          "form": "다음달이라 아직 계획은 없는데 ㅋㅋ 먹고가면 효과있나요? ㅎㅎ",
          "original_form": "다음달이라 아직 계획은 없는데 ㅋㅋ 먹고가면 효과있나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100010783.1.12",
          "form": "부모님도 그렇게 말하긴 하더라고요 ㅋㅋㅋ",
          "original_form": "부모님도 그렇게 말하긴 하더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100010783.1.13",
          "form": "ㅋㅋㅋ보건부에서 하도 삼겹살이야기해서 질의응답봤는데 아무효능없지만 위안이 된대요ㅋㅋ",
          "original_form": "ㅋㅋㅋ보건부에서 하도 삼겹살이야기해서 질의응답봤는데 아무효능없지만 위안이 된대요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:27"
        },
        {
          "id": "MDRW2100010783.1.14",
          "form": "아 효능없군요 ㅋㅋㅋㅋ 근데 뭔가 먹으면 든든할거 같아요 ㅋㅋㅋ",
          "original_form": "아 효능없군요 ㅋㅋㅋㅋ 근데 뭔가 먹으면 든든할거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100010783.1.15",
          "form": "그냥 부산사람들의 국밥토템같은거 아닐까요ㅋㅋㅋ",
          "original_form": "그냥 부산사람들의 국밥토템같은거 아닐까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100010783.1.16",
          "form": "ㅋㅋㅋㅋ 그럴수도 있겠네요",
          "original_form": "ㅋㅋㅋㅋ 그럴수도 있겠네요",
          "speaker_id": "2",
          "time": "20210824 11:29"
        },
        {
          "id": "MDRW2100010783.1.17",
          "form": "요새 잠 푹 주무시나요?",
          "original_form": "요새 잠 푹 주무시나요?",
          "speaker_id": "2",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100010783.1.18",
          "form": "기절잠 하는편이에요",
          "original_form": "기절잠 하는편이에요",
          "speaker_id": "1",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100010783.1.19",
          "form": "새벽두시 한시 즈음에요",
          "original_form": "새벽두시 한시 즈음에요",
          "speaker_id": "1",
          "time": "20210824 11:30"
        },
        {
          "id": "MDRW2100010783.1.20",
          "form": "오 누우면 바로 잠드시나봐요",
          "original_form": "오 누우면 바로 잠드시나봐요",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100010783.1.21",
          "form": "폰게임하다 자요ㅋㅋ",
          "original_form": "폰게임하다 자요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:32"
        }
      ]
    }
