      "utterance": [
        {
          "id": "MDRW2100033878.1.1",
          "form": "중고거래 해보셨나요?",
          "original_form": "중고거래 해보셨나요?",
          "speaker_id": "1",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100033878.1.2",
          "form": "네 당근마켓으로 많이 팔아봤어요",
          "original_form": "네 당근마켓으로 많이 팔아봤어요",
          "speaker_id": "2",
          "time": "20210825 14:32"
        },
        {
          "id": "MDRW2100033878.1.3",
          "form": "당근마켓이 만들어지고 저도 많이 거래한 것 같아요",
          "original_form": "당근마켓이 만들어지고 저도 많이 거래한 것 같아요",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100033878.1.4",
          "form": "당근마켓으로 산 것도 있으세요?",
          "original_form": "당근마켓으로 산 것도 있으세요?",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100033878.1.5",
          "form": "무선이어폰 구매해봤어요",
          "original_form": "무선이어폰 구매해봤어요",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100033878.1.6",
          "form": "저는 사본 적은 없어요 중고로 사는 건 불안해서 안 사게 되더라고요",
          "original_form": "저는 사본 적은 없어요 중고로 사는 건 불안해서 안 사게 되더라고요",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100033878.1.7",
          "form": "그렇긴 해요ㅋㅋ 저는 당분간만 사용할 걸 구하는거라 당근으로 구매했어요",
          "original_form": "그렇긴 해요ㅋㅋ 저는 당분간만 사용할 걸 구하는거라 당근으로 구매했어요",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100033878.1.8",
          "form": "그런 용도면 당근이 좋겠네요",
          "original_form": "그런 용도면 당근이 좋겠네요",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100033878.1.9",
          "form": "전 예전에 아이돌 앨범 당근으로 몇 번 팔았었어요",
          "original_form": "전 예전에 아이돌 앨범 당근으로 몇 번 팔았었어요",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100033878.1.10",
          "form": "잘 팔리셨나요?",
          "original_form": "잘 팔리셨나요?",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100033878.1.11",
          "form": "가격 조금씩 내리면서 존버해야하긴 했는데 그래도 나름 잘 팔렸어요",
          "original_form": "가격 조금씩 내리면서 존버해야하긴 했는데 그래도 나름 잘 팔렸어요",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100033878.1.12",
          "form": "인기 떨어지기 전에 팔아야죠",
          "original_form": "인기 떨어지기 전에 팔아야죠",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100033878.1.13",
          "form": "맞아요ㅋㅋㅋ 최대한 잘 나갈 때 빨리 팔아야죠",
          "original_form": "맞아요ㅋㅋㅋ 최대한 잘 나갈 때 빨리 팔아야죠",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100033878.1.14",
          "form": "name1 님은 요새 사고 싶은 거 있으세요?",
          "original_form": "&name1& 님은 요새 사고 싶은 거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100033878.1.15",
          "form": "커피 머신을 고민하고있어요",
          "original_form": "커피 머신을 고민하고있어요",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100033878.1.16",
          "form": "커피 자주 드시나보네요",
          "original_form": "커피 자주 드시나보네요",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100033878.1.17",
          "form": "커피 자주 마시는 사람들은 머신 사면 보통 만족하는 것 같아요",
