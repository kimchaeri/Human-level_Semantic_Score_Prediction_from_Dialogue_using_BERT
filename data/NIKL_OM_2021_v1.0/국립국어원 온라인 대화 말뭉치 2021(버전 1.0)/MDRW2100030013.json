      "utterance": [
        {
          "id": "MDRW2100030013.1.1",
          "form": "헐 저 마라탕 완전 좋아해용",
          "original_form": "헐 저 마라탕 완전 좋아해용",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100030013.1.2",
          "form": "규린님은용?!",
          "original_form": "규린님은용?!",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100030013.1.3",
          "form": "와 저도 마라탕 좋아해요 ! 저 샹궈도 진짜 좋아해요ㅋㅋ",
          "original_form": "와 저도 마라탕 좋아해요 ! 저 샹궈도 진짜 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030013.1.4",
          "form": "앗 만났다 영혼의 단짝!!!",
          "original_form": "앗 만났다 영혼의 단짝!!!",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100030013.1.5",
          "form": "샹궈 진짜 맛있죠...",
          "original_form": "샹궈 진짜 맛있죠...",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030013.1.6",
          "form": "저 맨날 밥이랑 같이 먹어용ㅋㅋㅋ",
          "original_form": "저 맨날 밥이랑 같이 먹어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100030013.1.7",
          "form": "으악 생각하니까 조만간 먹고싶네여ㅠㅋㅋㅋ",
          "original_form": "으악 생각하니까 조만간 먹고싶네여ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100030013.1.8",
          "form": "마라탕이랑 꿔바로우 먹는것도 진린데",
          "original_form": "마라탕이랑 꿔바로우 먹는것도 진린데",
          "speaker_id": "1",
          "time": "20210903 18:37"
        },
        {
          "id": "MDRW2100030013.1.9",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "2",
          "time": "20210903 18:38"
        },
        {
          "id": "MDRW2100030013.1.10",
          "form": "저 지금 완전 침고였어요",
          "original_form": "저 지금 완전 침고였어요",
          "speaker_id": "2",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100030013.1.11",
          "form": "꿔바로우ㅠㅠㅠ 간만에 먹구 싶네요!!",
          "original_form": "꿔바로우ㅠㅠㅠ 간만에 먹구 싶네요!!",
          "speaker_id": "2",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100030013.1.12",
          "form": "갑자기 저도 먹고싶어지네요ㅎㅎ 맵기는 어느정도로 드세여?",
          "original_form": "갑자기 저도 먹고싶어지네요ㅎㅎ 맵기는 어느정도로 드세여?",
          "speaker_id": "1",
          "time": "20210903 18:41"
        },
        {
          "id": "MDRW2100030013.1.13",
          "form": "전 3단계 쯤이요!!",
          "original_form": "전 3단계 쯤이요!!",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100030013.1.14",
          "form": "전에 0단계 시켰다가 하얀탕이 나와서",
          "original_form": "전에 0단계 시켰다가 하얀탕이 나와서",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100030013.1.15",
          "form": "담부턴 맵게 먹어요!!",
          "original_form": "담부턴 맵게 먹어요!!",
          "speaker_id": "2",
          "time": "20210903 18:49"
        },
        {
          "id": "MDRW2100030013.1.16",
          "form": "와 매운거 잘 드시는군요! 전 거의 1단계까지가 최대더라구요",
          "original_form": "와 매운거 잘 드시는군요! 전 거의 1단계까지가 최대더라구요",
          "speaker_id": "1",
          "time": "20210903 18:51"
        },
        {
          "id": "MDRW2100030013.1.17",
          "form": "그쵸 0단계 시켰다 저도 백탕나와가지고ㅋㅋㅋ",
          "original_form": "그쵸 0단계 시켰다 저도 백탕나와가지고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:52"
        },
        {
          "id": "MDRW2100030013.1.18",
          "form": "ㅋㅋㅋㅋㅋ헉 뭐뭐 넣어드세요??",
          "original_form": "ㅋㅋㅋㅋㅋ헉 뭐뭐 넣어드세요??",
          "speaker_id": "2",
          "time": "20210903 18:53"
        },
        {
          "id": "MDRW2100030013.1.19",
          "form": "전 당면이랑 청경채 숙주 많이요!!",
          "original_form": "전 당면이랑 청경채 숙주 많이요!!",
          "speaker_id": "2",
          "time": "20210903 18:54"
        },
        {
          "id": "MDRW2100030013.1.20",
          "form": "저랑 비슷해요!! 전 으루롹기님이 말하신 거에서 치즈떡이랑 푸주 분모자도 꼭 추가해줘야해요ㅎㅎ",
          "original_form": "저랑 비슷해요!! 전 으루롹기님이 말하신 거에서 치즈떡이랑 푸주 분모자도 꼭 추가해줘야해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 18:55"
        },
        {
          "id": "MDRW2100030013.1.21",
          "form": "고기는 쇠고기일까요..?!?!",
          "original_form": "고기는 쇠고기일까요..?!?!",
          "speaker_id": "1",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100030013.1.22",
          "form": "헉 소고기...! 제가 고기를 빠뜨렀네요!!",
          "original_form": "헉 소고기...! 제가 고기를 빠뜨렀네요!!",
          "speaker_id": "2",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100030013.1.23",
          "form": "ㅋㅋㅋㅋ분모자도 너무 맛있어요!",
          "original_form": "ㅋㅋㅋㅋ분모자도 너무 맛있어요!",
          "speaker_id": "2",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100030013.1.24",
          "form": "맛있져ㅎㅎ 떡볶이는 혹시 안 좋아하시나요??",
          "original_form": "맛있져ㅎㅎ 떡볶이는 혹시 안 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210903 19:01"
        },
        {
          "id": "MDRW2100030013.1.25",
          "form": "제 배민 목록엔 떡볶이와 마라탕이 쌓여있죠ㅋㅋㅋㅋ",
          "original_form": "제 배민 목록엔 떡볶이와 마라탕이 쌓여있죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100030013.1.26",
          "form": "전 신전 많이 시켜먹어요!!",
          "original_form": "전 신전 많이 시켜먹어요!!",
          "speaker_id": "2",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100030013.1.27",
          "form": "잘맞네요ㅋㅋㅋ저도 신전 엄청 좋아해요! 신전 튀김 중에 오뎅 특히 좋아해요 아 닭껍질튀김도요!",
          "original_form": "잘맞네요ㅋㅋㅋ저도 신전 엄청 좋아해요! 신전 튀김 중에 오뎅 특히 좋아해요 아 닭껍질튀김도요!",
