      "utterance": [
        {
          "id": "MDRW2100044116.1.1",
          "form": "지금 두개찌 먹었거든요",
          "original_form": "지금 두개찌 먹었거든요",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044116.1.2",
          "form": "오늘 신랑이 백신2차 맞아서 육아 제가 전담했는데 죽갓어요",
          "original_form": "오늘 신랑이 백신2차 맞아서 육아 제가 전담했는데 죽갓어요",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044116.1.3",
          "form": "그래도 신랑이 하는 일이 잇었던거네요...ㅋㅋㅋ",
          "original_form": "그래도 신랑이 하는 일이 잇었던거네요...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044116.1.4",
          "form": "신랑 진짜 아픈건 맞죠?",
          "original_form": "신랑 진짜 아픈건 맞죠?",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044116.1.5",
          "form": "체온이 좀 높아요",
          "original_form": "체온이 좀 높아요",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044116.1.6",
          "form": "글고 저도 2차ㅏㅈ고 디비자서 ㅋㅋㅋ",
          "original_form": "글고 저도 2차ㅏㅈ고 디비자서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044116.1.7",
          "form": "오늘 만 참는다 ...이러고잇어욬ㅋㅋ",
          "original_form": "오늘 만 참는다 ...이러고잇어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044116.1.8",
          "form": "왠지 아픈척 하고 육아 몰빵한거 아닌가 했는데 열은 연기할 수 없는거니까",
          "original_form": "왠지 아픈척 하고 육아 몰빵한거 아닌가 했는데 열은 연기할 수 없는거니까",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044116.1.9",
          "form": "고생 하셨네요 ㅠ",
          "original_form": "고생 하셨네요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044116.1.10",
          "form": "저희 신랑은 집에 없어요 ㅠ",
          "original_form": "저희 신랑은 집에 없어요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044116.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ글도 저녁은 안하고 치킨시켜주더라고요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ글도 저녁은 안하고 치킨시켜주더라고요",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044116.1.12",
          "form": "어디가셨나요?출장?ㅠㅠ",
          "original_form": "어디가셨나요?출장?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044116.1.13",
          "form": "술??",
          "original_form": "술??",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044116.1.14",
          "form": "저 주말부부요 ㅠ",
          "original_form": "저 주말부부요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044116.1.15",
          "form": "매일이 독박",
          "original_form": "매일이 독박",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044116.1.16",
          "form": "아아 독박 ㅠㅠ그래도 주말에만 보니 애틋하시겟어요",
          "original_form": "아아 독박 ㅠㅠ그래도 주말에만 보니 애틋하시겟어요",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044116.1.17",
          "form": "전혀요?? ㅋㅋㅋㅋ",
          "original_form": "전혀요?? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044116.1.18",
          "form": "매일보니 감흥이 없어욬ㅋㅋㅋ",
          "original_form": "매일보니 감흥이 없어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044116.1.19",
          "form": "오면 귀찮아요",
          "original_form": "오면 귀찮아요",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044116.1.20",
          "form": "ㅋㅋㅋㅋㅋ그래도 애들 봐주러 오시잖아요 웰컴해주세요",
          "original_form": "ㅋㅋㅋㅋㅋ그래도 애들 봐주러 오시잖아요 웰컴해주세요",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044116.1.21",
          "form": "집에서 저한테 잔소리 하는.사람이 하나도 없다가 신랑이 오면 생기잖아요 ㅋㅋㅋ",
          "original_form": "집에서 저한테 잔소리 하는.사람이 하나도 없다가 신랑이 오면 생기잖아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044116.1.22",
          "form": "그리고 신랑 오면 티비를.제.맘대로 못봐요",
          "original_form": "그리고 신랑 오면 티비를.제.맘대로 못봐요",
