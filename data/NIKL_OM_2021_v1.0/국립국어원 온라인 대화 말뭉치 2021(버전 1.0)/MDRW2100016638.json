        },
        {
          "id": "MDRW2100016638.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016638.1.3",
          "form": "집안일하시나요?",
          "original_form": "집안일하시나요?",
          "speaker_id": "2",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100016638.1.4",
          "form": "당연하졍",
          "original_form": "당연하졍",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016638.1.5",
          "form": "근데 해도해도 끝이없는게 너무 힘들어요",
          "original_form": "근데 해도해도 끝이없는게 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016638.1.6",
          "form": "하기 싫은 집안일이 뭔가요",
          "original_form": "하기 싫은 집안일이 뭔가요",
          "speaker_id": "2",
          "time": "20210826 14:24"
        },
        {
          "id": "MDRW2100016638.1.7",
          "form": "화장실청소",
          "original_form": "화장실청소",
          "speaker_id": "1",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016638.1.8",
          "form": "화장실청소가 하기 싫어용",
          "original_form": "화장실청소가 하기 싫어용",
          "speaker_id": "1",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016638.1.9",
          "form": "님은여",
          "original_form": "님은여",
          "speaker_id": "1",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016638.1.10",
          "form": "음쓰 배출이요",
          "original_form": "음쓰 배출이요",
          "speaker_id": "2",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016638.1.11",
          "form": "아 인정",
          "original_form": "아 인정",
          "speaker_id": "1",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016638.1.12",
          "form": "냄새 진짜 토할거같아요",
          "original_form": "냄새 진짜 토할거같아요",
          "speaker_id": "1",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016638.1.13",
          "form": "벌레도 징그러워요",
          "original_form": "벌레도 징그러워요",
          "speaker_id": "2",
          "time": "20210826 14:30"
        },
        {
          "id": "MDRW2100016638.1.14",
          "form": "누가 좋아해요",
          "original_form": "누가 좋아해요",
          "speaker_id": "1",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016638.1.15",
          "form": "조만간 전기파리채라도 사려구요",
          "original_form": "조만간 전기파리채라도 사려구요",
          "speaker_id": "1",
          "time": "20210826 14:31"
        },
        {
          "id": "MDRW2100016638.1.16",
          "form": "가족이랑 같이사세요?",
          "original_form": "가족이랑 같이사세요?",
          "speaker_id": "2",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016638.1.17",
          "form": "아녀 자취",
          "original_form": "아녀 자취",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016638.1.18",
          "form": "가족이랑 살땐 가족이 다 하줘서 편했는데",
          "original_form": "가족이랑 살땐 가족이 다 하줘서 편했는데",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016638.1.19",
          "form": "지금은 제가 해야죠",
          "original_form": "지금은 제가 해야죠",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016638.1.20",
          "form": "독립하셨군요",
          "original_form": "독립하셨군요",
          "speaker_id": "2",
          "time": "20210826 14:38"
        }
      ]
