          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100010245.1.3",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100010245.1.4",
          "form": "아뇨 무교예요",
          "original_form": "아뇨 무교예요",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100010245.1.5",
          "form": "name1님은 종교 있으신가요",
          "original_form": "&name1&님은 종교 있으신가요",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100010245.1.6",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100010245.1.7",
          "form": "아앗...",
          "original_form": "아앗...",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100010245.1.8",
          "form": "교회 가보신 적 있나요",
          "original_form": "교회 가보신 적 있나요",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100010245.1.9",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100010245.1.10",
          "form": "어릴때 친구따라서요ㅋㅋㅋㅋ",
          "original_form": "어릴때 친구따라서요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100010245.1.11",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100010245.1.12",
          "form": "저도 친구 따라서 가봤어요 ㅋㅋ",
          "original_form": "저도 친구 따라서 가봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100010245.1.13",
          "form": "바자회하러!",
          "original_form": "바자회하러!",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100010245.1.14",
          "form": "ㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋ저도요",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100010245.1.15",
          "form": "달란트시장?",
          "original_form": "달란트시장?",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100010245.1.16",
          "form": "네네 ㅋㅋ",
          "original_form": "네네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100010245.1.17",
          "form": "친구 돈으로 샀지요... ^.^",
          "original_form": "친구 돈으로 샀지요... ^.^",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100010245.1.18",
          "form": "대신 교회에 전화번호를 털렸던 기억이",
          "original_form": "대신 교회에 전화번호를 털렸던 기억이",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100010245.1.19",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100010245.1.20",
          "form": "다들어릴때",
          "original_form": "다들어릴때",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100010245.1.21",
          "form": "한번은가본것같네요",
          "original_form": "한번은가본것같네요",
          "speaker_id": "1",
          "time": "20210826 17:09"
        },
        {
          "id": "MDRW2100010245.1.22",
          "form": "워낙 맛있는 걸 많이 뿌려서...",
          "original_form": "워낙 맛있는 걸 많이 뿌려서...",
          "speaker_id": "2",
