      "utterance": [
        {
          "id": "MDRW2100031434.1.1",
          "form": "담에 머리부터 발끝까지 쫙 골라드려요? ㅎㅎ",
          "original_form": "담에 머리부터 발끝까지 쫙 골라드려요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 21:36"
        },
        {
          "id": "MDRW2100031434.1.2",
          "form": "그럼 좋지",
          "original_form": "그럼 좋지",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100031434.1.3",
          "form": "너무안비싼걸로",
          "original_form": "너무안비싼걸로",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100031434.1.4",
          "form": "근데 예쁜옷 보다보면 다 비싸던디 ㅠㅠㅋㅋㅋㅋ",
          "original_form": "근데 예쁜옷 보다보면 다 비싸던디 ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100031434.1.5",
          "form": "저렴한거 있겠지",
          "original_form": "저렴한거 있겠지",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100031434.1.6",
          "form": "엄마 은근히 밝은색 옷 어울리니까요",
          "original_form": "엄마 은근히 밝은색 옷 어울리니까요",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031434.1.7",
          "form": "맨날 검은색만 입구다니지 마시구",
          "original_form": "맨날 검은색만 입구다니지 마시구",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031434.1.8",
          "form": "검은색도 좋은데",
          "original_form": "검은색도 좋은데",
          "speaker_id": "2",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031434.1.9",
          "form": "검정은 좀 얼둘 어두어ㅓ보이니까",
          "original_form": "검정은 좀 얼둘 어두어ㅓ보이니까",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031434.1.10",
          "form": "얼굴",
          "original_form": "얼굴",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031434.1.11",
          "form": "베이지색 같은거... 슬슬 가을이니까 코트 괜찮을거같아요",
          "original_form": "베이지색 같은거... 슬슬 가을이니까 코트 괜찮을거같아요",
          "speaker_id": "1",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100031434.1.12",
          "form": "지나번에 산거 있다",
          "original_form": "지나번에 산거 있다",
          "speaker_id": "2",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100031434.1.13",
          "form": "그거 잘어울렸으니까요 ㅋㅋㅋㅋ 담에 비슷한걸로 또 사버려요",
          "original_form": "그거 잘어울렸으니까요 ㅋㅋㅋㅋ 담에 비슷한걸로 또 사버려요",
          "speaker_id": "1",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100031434.1.14",
          "form": "딸도 신발 사야하는데",
          "original_form": "딸도 신발 사야하는데",
          "speaker_id": "2",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100031434.1.15",
          "form": "저야 뭐 밖에 안나가니까",
          "original_form": "저야 뭐 밖에 안나가니까",
          "speaker_id": "1",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100031434.1.16",
          "form": "운동화 필요없어?",
          "original_form": "운동화 필요없어?",
          "speaker_id": "2",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100031434.1.17",
          "form": "샌들 있었음 좋겠어요",
          "original_form": "샌들 있었음 좋겠어요",
          "speaker_id": "1",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100031434.1.18",
          "form": "여름 다 지나가서 샌들이야",
          "original_form": "여름 다 지나가서 샌들이야",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100031434.1.19",
          "form": "슬리퍼만 신으니까 또 별로라...",
          "original_form": "슬리퍼만 신으니까 또 별로라...",
          "speaker_id": "1",
          "time": "20210904 22:45"
        }
      ]
