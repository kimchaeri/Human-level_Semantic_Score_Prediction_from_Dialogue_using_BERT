        {
          "id": "MDRW2100031500.1.1",
          "form": "name2님은 요리 직접하시나요?",
          "original_form": "&name2&님은 요리 직접하시나요?",
          "speaker_id": "1",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100031500.1.2",
          "form": "저는 본가에 살다보니 직접 요리할 일이 없어요",
          "original_form": "저는 본가에 살다보니 직접 요리할 일이 없어요",
          "speaker_id": "2",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100031500.1.3",
          "form": "그래서 요리초보",
          "original_form": "그래서 요리초보",
          "speaker_id": "2",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100031500.1.4",
          "form": "저도 요리못해요ㅜㅜ",
          "original_form": "저도 요리못해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100031500.1.5",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100031500.1.6",
          "form": "아침을 많이 먹어서 점저로 먹으려구요",
          "original_form": "아침을 많이 먹어서 점저로 먹으려구요",
          "speaker_id": "2",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100031500.1.7",
          "form": "엇 아침 뭐 드셨어요?",
          "original_form": "엇 아침 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 14:20"
        },
        {
          "id": "MDRW2100031500.1.8",
          "form": "김치찌개 먹었어용!! 어묵사리 추가ㅋㅋㅋ",
          "original_form": "김치찌개 먹었어용!! 어묵사리 추가ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100031500.1.9",
          "form": "어묵사리 맛있겠네요ㅜ",
          "original_form": "어묵사리 맛있겠네요ㅜ",
          "speaker_id": "1",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100031500.1.10",
          "form": "저는 김치국 먹었어요",
          "original_form": "저는 김치국 먹었어요",
          "speaker_id": "1",
          "time": "20210905 14:21"
        },
        {
          "id": "MDRW2100031500.1.11",
          "form": "오 둘다 김치수프 먹었네요",
          "original_form": "오 둘다 김치수프 먹었네요",
          "speaker_id": "2",
          "time": "20210905 14:22"
        },
        {
          "id": "MDRW2100031500.1.12",
          "form": "그러게요ㅎ",
          "original_form": "그러게요ㅎ",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100031500.1.13",
          "form": "마침 집에 김치도 떨어져서",
          "original_form": "마침 집에 김치도 떨어져서",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100031500.1.14",
          "form": "홈쇼핑에서 시켰어요",
          "original_form": "홈쇼핑에서 시켰어요",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100031500.1.15",
          "form": "김치 시켜드세요?",
          "original_form": "김치 시켜드세요?",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100031500.1.16",
          "form": "시키기도 하고 어무니가 담기도 하세요",
          "original_form": "시키기도 하고 어무니가 담기도 하세요",
          "speaker_id": "2",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100031500.1.17",
          "form": "저희집도 옛날에는 담았는데 힘들어서 ㅎㅎ",
          "original_form": "저희집도 옛날에는 담았는데 힘들어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100031500.1.18",
          "form": "그리고 요즘에는 사는게 비용이 덜 들기도 하더라구요",
          "original_form": "그리고 요즘에는 사는게 비용이 덜 들기도 하더라구요",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100031500.1.19",
          "form": "맞아요ㅜㅜ 보통 일이 아니죠 김장이",
          "original_form": "맞아요ㅜㅜ 보통 일이 아니죠 김장이",
