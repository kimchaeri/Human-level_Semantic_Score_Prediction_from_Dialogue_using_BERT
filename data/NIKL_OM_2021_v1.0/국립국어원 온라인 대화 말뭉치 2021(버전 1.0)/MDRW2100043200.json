      "utterance": [
        {
          "id": "MDRW2100043200.1.1",
          "form": "비가오나요?",
          "original_form": "비가오나요?",
          "speaker_id": "1",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043200.1.2",
          "form": "아직 안와욧",
          "original_form": "아직 안와욧",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043200.1.3",
          "form": "여기도그쳤어요",
          "original_form": "여기도그쳤어요",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100043200.1.4",
          "form": "퇴근길에 습할꺼같아요",
          "original_form": "퇴근길에 습할꺼같아요",
          "speaker_id": "2",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043200.1.5",
          "form": "ㅎㅎ그래도그쳐서좋아요",
          "original_form": "ㅎㅎ그래도그쳐서좋아요",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100043200.1.6",
          "form": "주말엔 비안왓음좋겟어요",
          "original_form": "주말엔 비안왓음좋겟어요",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043200.1.7",
          "form": "주말에약속있으세요?",
          "original_form": "주말에약속있으세요?",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100043200.1.8",
          "form": "데이트해야져 ㅎㅎ",
          "original_form": "데이트해야져 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100043200.1.9",
          "form": "캬ㅎㅎㅎ",
          "original_form": "캬ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043200.1.10",
          "form": "맑겠네요",
          "original_form": "맑겠네요",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043200.1.11",
          "form": "그래야합니다ㅋ",
          "original_form": "그래야합니다ㅋ",
          "speaker_id": "2",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043200.1.12",
          "form": "가을옷 사셧나유",
          "original_form": "가을옷 사셧나유",
          "speaker_id": "2",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043200.1.13",
          "form": "가디건샀어요!",
          "original_form": "가디건샀어요!",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043200.1.14",
          "form": "잘하셧어욥!!",
          "original_form": "잘하셧어욥!!",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043200.1.15",
          "form": "가을엔 가디건이쥬",
          "original_form": "가을엔 가디건이쥬",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043200.1.16",
          "form": "ㅎㅎㅎ맞아요",
          "original_form": "ㅎㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043200.1.17",
          "form": "세일가로사서",
          "original_form": "세일가로사서",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043200.1.18",
          "form": "저는 니트삿어유",
          "original_form": "저는 니트삿어유",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043200.1.19",
          "form": "만족해요",
          "original_form": "만족해요",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043200.1.20",
          "form": "가을니트인가용",
          "original_form": "가을니트인가용",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043200.1.21",
          "form": "또비와요ㅠ",
          "original_form": "또비와요ㅠ",
