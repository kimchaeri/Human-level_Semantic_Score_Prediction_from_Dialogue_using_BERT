      "utterance": [
        {
          "id": "MDRW2100002435.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002435.1.2",
          "form": "반려동물 키우고 싶은데 못키우네요ㅠ반려동물 키우세요?",
          "original_form": "반려동물 키우고 싶은데 못키우네요ㅠ반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210820 16:59"
        },
        {
          "id": "MDRW2100002435.1.3",
          "form": "아니요 별로 안좋아해요",
          "original_form": "아니요 별로 안좋아해요",
          "speaker_id": "2",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002435.1.4",
          "form": "무슨 동물 키우고 싶으시나요?",
          "original_form": "무슨 동물 키우고 싶으시나요?",
          "speaker_id": "2",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002435.1.5",
          "form": "고양이키우고 싶어요",
          "original_form": "고양이키우고 싶어요",
          "speaker_id": "1",
          "time": "20210820 17:03"
        },
        {
          "id": "MDRW2100002435.1.6",
          "form": "집사님이 되고 싶으신가보네요 ㄱ",
          "original_form": "집사님이 되고 싶으신가보네요 ㄱ",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002435.1.7",
          "form": "이름은 정하신거 있나요?",
          "original_form": "이름은 정하신거 있나요?",
          "speaker_id": "2",
          "time": "20210820 17:04"
        },
        {
          "id": "MDRW2100002435.1.8",
          "form": "name3이라고 짓고 싶네요 고양이라서요ㅋㅋ 동물 안좋아하시는 이유가 있나요?",
          "original_form": "&name3&이라고 짓고 싶네요 고양이라서요ㅋㅋ 동물 안좋아하시는 이유가 있나요?",
          "speaker_id": "1",
          "time": "20210820 17:05"
        },
        {
          "id": "MDRW2100002435.1.9",
          "form": "똥치우기도 싫고 산책도 귀찮고",
          "original_form": "똥치우기도 싫고 산책도 귀찮고",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002435.1.10",
          "form": "힘들거같아서요",
          "original_form": "힘들거같아서요",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002435.1.11",
          "form": "맞아요그렇긴하죠 강아지랑 고양이중에 뭐가 더 좋아요?",
          "original_form": "맞아요그렇긴하죠 강아지랑 고양이중에 뭐가 더 좋아요?",
          "speaker_id": "1",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002435.1.12",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "2",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100002435.1.13",
          "form": "고양이는 아직 무섭네요",
          "original_form": "고양이는 아직 무섭네요",
          "speaker_id": "2",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100002435.1.14",
          "form": "잘 키울자신 있으시나요?",
          "original_form": "잘 키울자신 있으시나요?",
          "speaker_id": "2",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100002435.1.15",
          "form": "아니요 키울자신이없어서 못키우겠어요ㅠ",
          "original_form": "아니요 키울자신이없어서 못키우겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100002435.1.16",
          "form": "책임감도 있어야해요",
          "original_form": "책임감도 있어야해요",
          "speaker_id": "2",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100002435.1.17",
          "form": "맞아요 책임감이 중요하죠 병이라도 걸리면 걱정도 많아지고요ㅠ",
          "original_form": "맞아요 책임감이 중요하죠 병이라도 걸리면 걱정도 많아지고요ㅠ",
          "speaker_id": "1",
          "time": "20210820 18:37"
        }
