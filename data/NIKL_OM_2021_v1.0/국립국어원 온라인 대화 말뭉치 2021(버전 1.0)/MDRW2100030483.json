      "utterance": [
        {
          "id": "MDRW2100030483.1.1",
          "form": "여행!",
          "original_form": "여행!",
          "speaker_id": "2",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030483.1.2",
          "form": "여행 가고 싶네요....",
          "original_form": "여행 가고 싶네요....",
          "speaker_id": "2",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030483.1.3",
          "form": "최근에 여행 간 곳 있으신가요?",
          "original_form": "최근에 여행 간 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030483.1.4",
          "form": "name1님은 혹.시.! 바다 보는 것을 좋아하시려나요 ",
          "original_form": "&name1&님은 혹.시.! 바다 보는 것을 좋아하시려나요 ",
          "speaker_id": "2",
          "time": "20210904 01:57"
        },
        {
          "id": "MDRW2100030483.1.5",
          "form": "음 저는 여행은 아니고",
          "original_form": "음 저는 여행은 아니고",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030483.1.6",
          "form": "네ㅜㅜ 바다엄청 좋아해요",
          "original_form": "네ㅜㅜ 바다엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030483.1.7",
          "form": "어머니 아버지와 함께 가까운 양수리에서 연꽃 보고 왔어요",
          "original_form": "어머니 아버지와 함께 가까운 양수리에서 연꽃 보고 왔어요",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030483.1.8",
          "form": "하 저도 좋아하는데....",
          "original_form": "하 저도 좋아하는데....",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030483.1.9",
          "form": "사실 부산을 한번도 안가봤다가",
          "original_form": "사실 부산을 한번도 안가봤다가",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030483.1.10",
          "form": "정말 맘먹고 가야하는 곳이 되었어요ㅠㅠ 왜이리 바쁜건지....",
          "original_form": "정말 맘먹고 가야하는 곳이 되었어요ㅠㅠ 왜이리 바쁜건지....",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030483.1.11",
          "form": "헉?!?! 저도요!!!",
          "original_form": "헉?!?! 저도요!!!",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030483.1.12",
          "form": "지난 달에 처음 갔거든요",
          "original_form": "지난 달에 처음 갔거든요",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030483.1.13",
          "form": "저도 부산 한 번도 안가봤어요 저.....ㅜㅜㅜㅋㅋㅋㅋ",
          "original_form": "저도 부산 한 번도 안가봤어요 저.....ㅜㅜㅜㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030483.1.14",
          "form": "해운대에 반하고왔어요",
          "original_form": "해운대에 반하고왔어요",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030483.1.15",
          "form": "와 무슨 일로",
          "original_form": "와 무슨 일로",
          "speaker_id": "2",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030483.1.16",
          "form": "헉 꼭 가보세요!",
          "original_form": "헉 꼭 가보세요!",
          "speaker_id": "1",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030483.1.17",
          "form": "다녀오셨나요?!",
          "original_form": "다녀오셨나요?!",
          "speaker_id": "2",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030483.1.18",
          "form": "센텀시티는",
          "original_form": "센텀시티는",
          "speaker_id": "2",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030483.1.19",
          "form": "제 드림시티예요",
          "original_form": "제 드림시티예요",
          "speaker_id": "2",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030483.1.20",
          "form": "친구들이랑 바람 쐬러갔다왔어요!",
          "original_form": "친구들이랑 바람 쐬러갔다왔어요!",
          "speaker_id": "1",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030483.1.21",
          "form": "저 사실.... 10월에 부산에 학회가 있었는데",
          "original_form": "저 사실.... 10월에 부산에 학회가 있었는데",
          "speaker_id": "2",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030483.1.22",
          "form": "앗 부러워요 ㅠㅠㅠㅠ",
          "original_form": "앗 부러워요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030483.1.23",
          "form": "헉 센텀시티가 어디인가했는데",
          "original_form": "헉 센텀시티가 어디인가했는데",
          "speaker_id": "1",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030483.1.24",
          "form": "그래서 일부러 백신도 일찍 맞았거든요!!",
          "original_form": "그래서 일부러 백신도 일찍 맞았거든요!!",
          "speaker_id": "2",
          "time": "20210904 01:59"
        },
        {
