      "utterance": [
        {
          "id": "MDRW2100033444.1.1",
          "form": "담에  또 걔가 뜨면 말해드릴게요 ㅋㄱ",
          "original_form": "담에  또 걔가 뜨면 말해드릴게요 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100033444.1.2",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:35"
        },
        {
          "id": "MDRW2100033444.1.3",
          "form": "여운있는 사람 ㅋㅋ",
          "original_form": "여운있는 사람 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100033444.1.4",
          "form": "아 추억의 게임 있으세요",
          "original_form": "아 추억의 게임 있으세요",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100033444.1.5",
          "form": "내 ㅎㅎ",
          "original_form": "내 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100033444.1.6",
          "form": "게임다하셨어요?",
          "original_form": "게임다하셨어요?",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100033444.1.7",
          "form": "곧 12시라 임무 완수 하셔야죠",
          "original_form": "곧 12시라 임무 완수 하셔야죠",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100033444.1.8",
          "form": "ㅋㅋ 땅파야하는데",
          "original_form": "ㅋㅋ 땅파야하는데",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100033444.1.9",
          "form": "캐시퍼",
          "original_form": "캐시퍼",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100033444.1.10",
          "form": "ㅋㅋ그땅 언제파서 금?캐나요?",
          "original_form": "ㅋㅋ그땅 언제파서 금?캐나요?",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100033444.1.11",
          "form": "다이야도 있나요?",
          "original_form": "다이야도 있나요?",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100033444.1.12",
          "form": "게임이 금은동 가격 다르죠?",
          "original_form": "게임이 금은동 가격 다르죠?",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100033444.1.13",
          "form": "철 구리인가요?",
          "original_form": "철 구리인가요?",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100033444.1.14",
          "form": "네네 달라요 ㅋㅋ",
          "original_form": "네네 달라요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100033444.1.15",
          "form": "보석류가 비싸죠",
          "original_form": "보석류가 비싸죠",
          "speaker_id": "2",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100033444.1.16",
          "form": "에메랄드 루비",
          "original_form": "에메랄드 루비",
          "speaker_id": "2",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100033444.1.17",
          "form": "다이아는 있는거같은데",
          "original_form": "다이아는 있는거같은데",
          "speaker_id": "2",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100033444.1.18",
          "form": "오 진짜 캐고싶다 주렁주렁 달고 다니게요",
          "original_form": "오 진짜 캐고싶다 주렁주렁 달고 다니게요",
          "speaker_id": "1",
          "time": "20210823 22:39"
        },
        {
          "id": "MDRW2100033444.1.19",
          "form": "아 금 게임이지만 루비 캐면 신나겠어요",
          "original_form": "아 금 게임이지만 루비 캐면 신나겠어요",
          "speaker_id": "1",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100033444.1.20",
          "form": "혹시 프린세스메이커 아세요",
          "original_form": "혹시 프린세스메이커 아세요",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100033444.1.21",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100033444.1.22",
          "form": "전 게임 잘몰라요",
          "original_form": "전 게임 잘몰라요",
          "speaker_id": "1",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100033444.1.23",
          "form": "스벅각 베라각 밖에 ㅋㅋ",
          "original_form": "스벅각 베라각 밖에 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100033444.1.24",
          "form": "음 제가 10대 때 했던 게임이에요",
          "original_form": "음 제가 10대 때 했던 게임이에요",
          "speaker_id": "2",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100033444.1.25",
          "form": "추억의 게임같은?",
          "original_form": "추억의 게임같은?",
          "speaker_id": "2",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100033444.1.26",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100033444.1.27",
          "form": "전 게임 ㅋㅋ솔리테어인가",
          "original_form": "전 게임 ㅋㅋ솔리테어인가",
          "speaker_id": "1",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100033444.1.28",
          "form": "고것밖에 몰라요",
          "original_form": "고것밖에 몰라요",
          "speaker_id": "1",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100033444.1.29",
          "form": "그 카드게임요",
          "original_form": "그 카드게임요",
          "speaker_id": "1",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100033444.1.30",
          "form": "정말 관심없었는데",
          "original_form": "정말 관심없었는데",
          "speaker_id": "1",
          "time": "20210823 22:47"
        },
        {
          "id": "MDRW2100033444.1.31",
          "form": "베라각만 손가락 부러지게 해요 ㅋㅋ",
          "original_form": "베라각만 손가락 부러지게 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:47"
        },
        {
          "id": "MDRW2100033444.1.32",
          "form": "폰을 부술듯",
          "original_form": "폰을 부술듯",
          "speaker_id": "1",
          "time": "20210823 22:47"
        },
        {
          "id": "MDRW2100033444.1.33",
          "form": "ㅋㅋ팁? 까진 아닌데",
          "original_form": "ㅋㅋ팁? 까진 아닌데",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100033444.1.34",
          "form": "넷플릭스나 웨이브 같은거보세여",
          "original_form": "넷플릭스나 웨이브 같은거보세여",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100033444.1.35",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033444.1.36",
          "form": "근데 게임은 단순한게 좋아요",
          "original_form": "근데 게임은 단순한게 좋아요",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033444.1.37",
          "form": "머리아픔 싫습니다 ㅋㅋ",
          "original_form": "머리아픔 싫습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100033444.1.38",
          "form": "전 그거 띄워놓고 보면서 해요",
          "original_form": "전 그거 띄워놓고 보면서 해요",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033444.1.39",
          "form": "팝업창으로",
          "original_form": "팝업창으로",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100033444.1.40",
          "form": "오좋은데요",
          "original_form": "오좋은데요",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100033444.1.41",
          "form": "그생걱못했네요",
          "original_form": "그생걱못했네요",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100033444.1.42",
          "form": "그럼 게임 안지루하겠어요",
          "original_form": "그럼 게임 안지루하겠어요",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100033444.1.43",
          "form": "외국꺼말고 한국예능같은거보세요",
          "original_form": "외국꺼말고 한국예능같은거보세요",
          "speaker_id": "2",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100033444.1.44",
          "form": "멀티로 내용 어렵지않은거",
          "original_form": "멀티로 내용 어렵지않은거",
          "speaker_id": "2",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100033444.1.45",
          "form": "네그래야 자막없러 가능하겠네요",
          "original_form": "네그래야 자막없러 가능하겠네요",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100033444.1.46",
          "form": "게임하면 지루해요",
          "original_form": "게임하면 지루해요",
          "speaker_id": "1",
          "time": "20210823 22:53"
        }
      ]
    }
