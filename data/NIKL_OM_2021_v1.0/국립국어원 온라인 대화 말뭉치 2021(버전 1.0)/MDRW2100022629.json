      "utterance": [
        {
          "id": "MDRW2100022629.1.1",
          "form": "음...배고플때먹으면되요",
          "original_form": "음...배고플때먹으면되요",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022629.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022629.1.3",
          "form": "악 ㅋㅋㅋ 아니 그것도 맞는 말이긴 해요",
          "original_form": "악 ㅋㅋㅋ 아니 그것도 맞는 말이긴 해요",
          "speaker_id": "1",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022629.1.4",
          "form": "저는 라면을 그렇게 먹지는 않아요 ㅋㅋㅋㅋ",
          "original_form": "저는 라면을 그렇게 먹지는 않아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022629.1.5",
          "form": "제가 말한건 라면에 넣어 먹으면 맛있는거요",
          "original_form": "제가 말한건 라면에 넣어 먹으면 맛있는거요",
          "speaker_id": "1",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022629.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ음 파나 치즈 넣어요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ음 파나 치즈 넣어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022629.1.7",
          "form": "따로 좋아하는 토핑잇으세요?",
          "original_form": "따로 좋아하는 토핑잇으세요?",
          "speaker_id": "2",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022629.1.8",
          "form": "오~ ㅋㅋ 건더기 보충!",
          "original_form": "오~ ㅋㅋ 건더기 보충!",
          "speaker_id": "1",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022629.1.9",
          "form": "저는 만두 넣는 거 좋아해요",
          "original_form": "저는 만두 넣는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022629.1.10",
          "form": "그리고 계란이요(안터지게)",
          "original_form": "그리고 계란이요(안터지게)",
          "speaker_id": "1",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022629.1.11",
          "form": "아 저도 만두 좋아해욬ㅋㅋㅋ",
          "original_form": "아 저도 만두 좋아해욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022629.1.12",
          "form": "계란필수! 저는 무조건 깨뜨려요",
          "original_form": "계란필수! 저는 무조건 깨뜨려요",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022629.1.13",
          "form": "무슨 만두 좋아해요?",
          "original_form": "무슨 만두 좋아해요?",
          "speaker_id": "1",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022629.1.14",
          "form": "고기만두 갈비만두 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "고기만두 갈비만두 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022629.1.15",
          "form": "허걱",
          "original_form": "허걱",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022629.1.16",
          "form": "갈비만두 ㅋㅋㅋ 예전에 한창 유행했는데",
          "original_form": "갈비만두 ㅋㅋㅋ 예전에 한창 유행했는데",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022629.1.17",
          "form": "요즘은 얇은피 만두가 유행인듯 해요",
          "original_form": "요즘은 얇은피 만두가 유행인듯 해요",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022629.1.18",
          "form": "그쵸 ㅋㅋㅋㅋ맛잇어요",
          "original_form": "그쵸 ㅋㅋㅋㅋ맛잇어요",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022629.1.19",
          "form": "아 안그래두 최근에 얇은피 대량구매햇어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 안그래두 최근에 얇은피 대량구매햇어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022629.1.20",
          "form": "먹어봤어요?",
          "original_form": "먹어봤어요?",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022629.1.21",
          "form": "열봉지 저렴하게나와서 쟁엿어요",
          "original_form": "열봉지 저렴하게나와서 쟁엿어요",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022629.1.22",
          "form": "아 열봉지나요?",
          "original_form": "아 열봉지나요?",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022629.1.23",
          "form": "400그램씩?",
          "original_form": "400그램씩?",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022629.1.24",
          "form": "네 ㅋㅋㅋㅋㅋ고기랑 김치조합으로샀어요",
          "original_form": "네 ㅋㅋㅋㅋㅋ고기랑 김치조합으로샀어요",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022629.1.25",
          "form": "김치가 의외로 맛잇더라구요",
          "original_form": "김치가 의외로 맛잇더라구요",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022629.1.26",
          "form": "오호 ㅎㅎ 얼마주고 사셨어요",
          "original_form": "오호 ㅎㅎ 얼마주고 사셨어요",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022629.1.27",
          "form": "열봉에 만원중반대엿던거같아요",
          "original_form": "열봉에 만원중반대엿던거같아요",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022629.1.28",
          "form": "11번가에는 8봉에 1만 8천원에 무료배송이던데",
          "original_form": "11번가에는 8봉에 1만 8천원에 무료배송이던데",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022629.1.29",
          "form": "엄청 싸게 잘 사셨네요 지금!!",
          "original_form": "엄청 싸게 잘 사셨네요 지금!!",
          "speaker_id": "1",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022629.1.30",
          "form": "요즘 얇은피 핫딜이 많잌드더라구요",
          "original_form": "요즘 얇은피 핫딜이 많잌드더라구요",
          "speaker_id": "2",
          "time": "20210829 16:37"
        },
        {
          "id": "MDRW2100022629.1.31",
          "form": "저도 사고 싶은데 너무 양이 많아요",
          "original_form": "저도 사고 싶은데 너무 양이 많아요",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022629.1.32",
          "form": "8봉 10봉 이걸 어디다 넣어요 ㅜㅜ",
          "original_form": "8봉 10봉 이걸 어디다 넣어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022629.1.33",
          "form": "그쵸 ㅠㅠㅠ 혼자사시면 힘들죠",
          "original_form": "그쵸 ㅠㅠㅠ 혼자사시면 힘들죠",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022629.1.34",
          "form": "저는 가족이랑 살아서 다행히 ㅋㅋㅋ",
          "original_form": "저는 가족이랑 살아서 다행히 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022629.1.35",
          "form": "아 그 하남쭈꾸미 드셔보셧어요?",
          "original_form": "아 그 하남쭈꾸미 드셔보셧어요?",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022629.1.36",
          "form": "아~ 가족끼리 나눠먹으면 되겠다 ㅎㅎ",
          "original_form": "아~ 가족끼리 나눠먹으면 되겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022629.1.37",
          "form": "상호인가요?",
          "original_form": "상호인가요?",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022629.1.38",
          "form": "밀키트에요!",
          "original_form": "밀키트에요!",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022629.1.39",
          "form": "앜ㅋㅋ 밀키트로는 안먹어봤네요",
          "original_form": "앜ㅋㅋ 밀키트로는 안먹어봤네요",
          "speaker_id": "1",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022629.1.40",
          "form": "가게는 가본 적 있어요",
          "original_form": "가게는 가본 적 있어요",
          "speaker_id": "1",
          "time": "20210829 16:39"
        }
      ]
