          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100020377.1.25",
          "form": "나중에 친구들이랑 해봐여",
          "original_form": "나중에 친구들이랑 해봐여",
          "speaker_id": "2",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100020377.1.26",
          "form": "그거 텔레그레이션???",
          "original_form": "그거 텔레그레이션???",
          "speaker_id": "1",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100020377.1.27",
          "form": "아심???",
          "original_form": "아심???",
          "speaker_id": "1",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100020377.1.28",
          "form": "그갓도 존웃일듯",
          "original_form": "그갓도 존웃일듯",
          "speaker_id": "1",
          "time": "20210828 02:31"
