      "utterance": [
        {
          "id": "MDRW2100020377.1.1",
          "form": "분당못벗어나잖아요",
          "original_form": "분당못벗어나잖아요",
          "speaker_id": "2",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020377.1.2",
          "form": "설대입구는 좀 멀다",
          "original_form": "설대입구는 좀 멀다",
          "speaker_id": "1",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020377.1.3",
          "form": "점심시간에",
          "original_form": "점심시간에",
          "speaker_id": "2",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020377.1.4",
          "form": "한판씩해도",
          "original_form": "한판씩해도",
          "speaker_id": "2",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020377.1.5",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "2",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020377.1.6",
          "form": "스플랜더 소요시간 얼마나대요",
          "original_form": "스플랜더 소요시간 얼마나대요",
          "speaker_id": "2",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020377.1.7",
          "form": "그그 게임하다",
          "original_form": "그그 게임하다",
          "speaker_id": "1",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020377.1.8",
          "form": "점심시간 다씀",
          "original_form": "점심시간 다씀",
          "speaker_id": "1",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020377.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020377.1.10",
          "form": "거의 30-40분이요",
          "original_form": "거의 30-40분이요",
          "speaker_id": "1",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020377.1.11",
          "form": "이번에 친구들이랑",
          "original_form": "이번에 친구들이랑",
          "speaker_id": "2",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020377.1.12",
          "form": "스페이스 뭐시기했는데",
          "original_form": "스페이스 뭐시기했는데",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020377.1.13",
          "form": "재밌었음",
          "original_form": "재밌었음",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020377.1.14",
          "form": "그건 뭔가요",
          "original_form": "그건 뭔가요",
          "speaker_id": "1",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020377.1.15",
          "form": "넷이 하면 ㄹㅇ웃긴ㅅ듯",
          "original_form": "넷이 하면 ㄹㅇ웃긴ㅅ듯",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020377.1.16",
          "form": "약간 말없이 카드랑 신호로 미션깨는건데",
          "original_form": "약간 말없이 카드랑 신호로 미션깨는건데",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020377.1.17",
          "form": "답답한 놈 한명씩나옴",
          "original_form": "답답한 놈 한명씩나옴",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020377.1.18",
          "form": "그게 본인아닌지",
          "original_form": "그게 본인아닌지",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020377.1.19",
          "form": "다시한번 생각해보셧나요",
          "original_form": "다시한번 생각해보셧나요",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020377.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020377.1.21",
          "form": "ㄴㄴ너무 확실해서",
          "original_form": "ㄴㄴ너무 확실해서",
          "speaker_id": "2",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020377.1.22",
          "form": "욕했어요",
          "original_form": "욕했어요",
          "speaker_id": "2",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020377.1.23",
          "form": "스페이스크루",
          "original_form": "스페이스크루",
          "speaker_id": "2",
          "time": "20210828 02:30"
        },
        {
