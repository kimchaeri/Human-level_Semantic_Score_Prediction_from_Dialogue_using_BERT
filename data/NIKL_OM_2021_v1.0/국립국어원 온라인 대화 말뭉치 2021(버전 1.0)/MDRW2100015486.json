      "utterance": [
        {
          "id": "MDRW2100015486.1.1",
          "form": "이제 비 좀 덜온다",
          "original_form": "이제 비 좀 덜온다",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015486.1.2",
          "form": "태풍왔을때가 진짜 엄청 내렸는데",
          "original_form": "태풍왔을때가 진짜 엄청 내렸는데",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015486.1.3",
          "form": "어제 오늘 좀 덜한듯",
          "original_form": "어제 오늘 좀 덜한듯",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015486.1.4",
          "form": "빨래 안마르고 꿉꿉한데 내일은 해 좀 났으면..",
          "original_form": "빨래 안마르고 꿉꿉한데 내일은 해 좀 났으면..",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015486.1.5",
          "form": "글게...",
          "original_form": "글게...",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015486.1.6",
          "form": "공기자체가 꿉꿉한느낌",
          "original_form": "공기자체가 꿉꿉한느낌",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015486.1.7",
          "form": "후덥지근도아니고 이상한 날씨다ㅋㅋㅋ",
          "original_form": "후덥지근도아니고 이상한 날씨다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015486.1.8",
          "form": "습해서 더 더운느낌",
          "original_form": "습해서 더 더운느낌",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015486.1.9",
          "form": "제습기 필수ㅋㅋㅋㅠㅠ",
          "original_form": "제습기 필수ㅋㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015486.1.10",
          "form": "이번주 내내 비온다는데",
          "original_form": "이번주 내내 비온다는데",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015486.1.11",
          "form": "일기예보 언제나처럼 틀렸으면...",
          "original_form": "일기예보 언제나처럼 틀렸으면...",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015486.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015486.1.13",
          "form": "내일은 잠깐 그친다고 본거같은데",
          "original_form": "내일은 잠깐 그친다고 본거같은데",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015486.1.14",
          "form": "부디...ㅋㅋㅋ 이제 습한거 물러가고 선선한 날씨가 왔으면...",
          "original_form": "부디...ㅋㅋㅋ 이제 습한거 물러가고 선선한 날씨가 왔으면...",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015486.1.15",
          "form": "밤에 선풍기틀면 약간 쌀쌀한데 안틀면 덥고",
          "original_form": "밤에 선풍기틀면 약간 쌀쌀한데 안틀면 덥고",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015486.1.16",
          "form": "애매..",
          "original_form": "애매..",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015486.1.17",
          "form": "지금날씨에 반팔에 긴바지가 딱임",
          "original_form": "지금날씨에 반팔에 긴바지가 딱임",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015486.1.18",
          "form": "적당히 얇은걸로",
          "original_form": "적당히 얇은걸로",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015486.1.19",
          "form": "반팔긴바진데",
          "original_form": "반팔긴바진데",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015486.1.20",
          "form": "대신 바지가 냉장고바지긴함ㅋㅋㅋㄱ",
          "original_form": "대신 바지가 냉장고바지긴함ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015486.1.21",
          "form": "냉장고 바지 이런거 말고ㅋㅋㅋㄱ",
          "original_form": "냉장고 바지 이런거 말고ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015486.1.22",
          "form": "그건 한여름옷아니냨ㅋㅋㅋ",
          "original_form": "그건 한여름옷아니냨ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015486.1.23",
          "form": "긴바지는 맞는디ㅋㅋㄱ",
          "original_form": "긴바지는 맞는디ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015486.1.24",
          "form": "일반 면바지 긴 건 아직 좀 답답할거같음...",
          "original_form": "일반 면바지 긴 건 아직 좀 답답할거같음...",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015486.1.25",
          "form": "그럼 반바지로 타협하고 선풍기틀고 얇은 이불덮어라ㅋㅋㅋ",
          "original_form": "그럼 반바지로 타협하고 선풍기틀고 얇은 이불덮어라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015486.1.26",
          "form": "ㅋㅋㅋㅋㄱ그건 이미...",
          "original_form": "ㅋㅋㅋㅋㄱ그건 이미...",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015486.1.27",
          "form": "여름용 시어서커 이불인데",
          "original_form": "여름용 시어서커 이불인데",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015486.1.28",
          "form": "인견이 더 시원하려나 싶기도",
          "original_form": "인견이 더 시원하려나 싶기도",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015486.1.29",
          "form": "나도 시어서커라 모르겠네",
          "original_form": "나도 시어서커라 모르겠네",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015486.1.30",
          "form": "근데 이제 거의 여름 끝이니..",
          "original_form": "근데 이제 거의 여름 끝이니..",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015486.1.31",
          "form": "도톰한 이불을 쓸시간이 다가온다",
          "original_form": "도톰한 이불을 쓸시간이 다가온다",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015486.1.32",
          "form": "ㅋㅋㅋㅋㅋ가을에도 덥긴더울건데",
          "original_form": "ㅋㅋㅋㅋㅋ가을에도 덥긴더울건데",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015486.1.33",
          "form": "너무 빠른준비 아닌교ㅋㅋㅋㄱ",
          "original_form": "너무 빠른준비 아닌교ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015486.1.34",
          "form": "ㅋㅋㅋㅋㄱ더운거 한순간이다",
          "original_form": "ㅋㅋㅋㅋㄱ더운거 한순간이다",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015486.1.35",
          "form": "태풍전에 봐라",
          "original_form": "태풍전에 봐라",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015486.1.36",
          "form": "얼마나 시원했음",
          "original_form": "얼마나 시원했음",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015486.1.37",
          "form": "그럼 뭐하나 지금 이래더운뎈ㅋㅋㅋㅋㄱ",
          "original_form": "그럼 뭐하나 지금 이래더운뎈ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015486.1.38",
          "form": "난 빨리 후덥지근한거만 없어지면 좋을듯..",
          "original_form": "난 빨리 후덥지근한거만 없어지면 좋을듯..",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015486.1.39",
          "form": "가을날씨 원츄",
          "original_form": "가을날씨 원츄",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015486.1.40",
          "form": "빨리 선선해져서 땀 안나는 계절이오길..",
          "original_form": "빨리 선선해져서 땀 안나는 계절이오길..",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015486.1.41",
          "form": "올여름 별로 안더웠다는데",
          "original_form": "올여름 별로 안더웠다는데",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015486.1.42",
          "form": "거짓말같음",
          "original_form": "거짓말같음",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015486.1.43",
          "form": "ㅋㅋㅋ올여름덜더워서 지금 모기가 기승이라는데",
          "original_form": "ㅋㅋㅋ올여름덜더워서 지금 모기가 기승이라는데",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015486.1.44",
          "form": "체감상 항상 덜더운 여름은 없고",
          "original_form": "체감상 항상 덜더운 여름은 없고",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015486.1.45",
          "form": "가장더운여름만있음",
          "original_form": "가장더운여름만있음",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015486.1.46",
          "form": "ㅋㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋㅋ맞아",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100015486.1.47",
          "form": "겪을때마다 더 더운 여름..",
          "original_form": "겪을때마다 더 더운 여름..",
          "speaker_id": "1",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100015486.1.48",
          "form": "며칠선선하다가 오늘 해 좀 났다고",
          "original_form": "며칠선선하다가 오늘 해 좀 났다고",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100015486.1.49",
          "form": "너무덥다...",
          "original_form": "너무덥다...",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100015486.1.50",
          "form": "주말엔 다시 비온다던데",
          "original_form": "주말엔 다시 비온다던데",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100015486.1.51",
          "form": "내일 오후부터 비온대..",
          "original_form": "내일 오후부터 비온대..",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100015486.1.52",
          "form": "비그치면 또 후덥지근해질까봐 걱정",
          "original_form": "비그치면 또 후덥지근해질까봐 걱정",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100015486.1.53",
          "form": "빨리 바람막이 입고싶다",
          "original_form": "빨리 바람막이 입고싶다",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100015486.1.54",
          "form": "옷고민 안하고 걸치기만하면되는데",
          "original_form": "옷고민 안하고 걸치기만하면되는데",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100015486.1.55",
          "form": "ㅋㅋㅋ맞아... 지금같이 날씨 애매할때가 제일",
          "original_form": "ㅋㅋㅋ맞아... 지금같이 날씨 애매할때가 제일",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100015486.1.56",
          "form": "옷 뭐입을지 고민...",
          "original_form": "옷 뭐입을지 고민...",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100015486.1.57",
          "form": "며칠전까진 밤엔 그래도 쌀쌀하고 그랬는데",
          "original_form": "며칠전까진 밤엔 그래도 쌀쌀하고 그랬는데",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100015486.1.58",
          "form": "오늘은 밤에 좀 더울거같은데",
          "original_form": "오늘은 밤에 좀 더울거같은데",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100015486.1.59",
          "form": "ㅇㅇ선풍기 틀고자야겠다",
          "original_form": "ㅇㅇ선풍기 틀고자야겠다",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
