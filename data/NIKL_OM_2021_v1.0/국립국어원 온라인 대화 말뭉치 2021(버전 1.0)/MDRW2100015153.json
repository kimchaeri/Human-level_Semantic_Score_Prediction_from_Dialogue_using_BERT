        },
        {
          "id": "MDRW2100015153.1.2",
          "form": "요즘 날씨 어떠세요?",
          "original_form": "요즘 날씨 어떠세요?",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015153.1.3",
          "form": "축축하죠 비가 계속 오네요",
          "original_form": "축축하죠 비가 계속 오네요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015153.1.4",
          "form": "name2 님도 그런가요?",
          "original_form": "&name2& 님도 그런가요?",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015153.1.5",
          "form": "여기도 그래요",
          "original_form": "여기도 그래요",
          "speaker_id": "2",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015153.1.6",
          "form": "더운 것보다 습한 게 더 지내기 불편한 갓 같아요",
          "original_form": "더운 것보다 습한 게 더 지내기 불편한 갓 같아요",
          "speaker_id": "2",
          "time": "20210826 00:21"
        },
        {
          "id": "MDRW2100015153.1.7",
          "form": "그렇긴 하죠",
          "original_form": "그렇긴 하죠",
          "speaker_id": "1",
          "time": "20210826 07:29"
        },
        {
          "id": "MDRW2100015153.1.8",
          "form": "빨래도 잘 안 마르네요",
          "original_form": "빨래도 잘 안 마르네요",
          "speaker_id": "1",
          "time": "20210826 07:30"
        },
        {
          "id": "MDRW2100015153.1.9",
          "form": "맞아요...오늘은 비가 안 오는 대신에 엄청 습하네요ㅠㅠ",
          "original_form": "맞아요...오늘은 비가 안 오는 대신에 엄청 습하네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:42"
        },
        {
          "id": "MDRW2100015153.1.10",
          "form": "그쵸 그래도 겨울에 엄청 건조할 거 생각하면 지금이 더 좋아요ㅋㅋㅋ",
          "original_form": "그쵸 그래도 겨울에 엄청 건조할 거 생각하면 지금이 더 좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100015153.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 08:08"
        },
        {
          "id": "MDRW2100015153.1.12",
          "form": "겨울엔 건조해서 정말 피부가....",
          "original_form": "겨울엔 건조해서 정말 피부가....",
          "speaker_id": "2",
          "time": "20210828 08:08"
        },
        {
          "id": "MDRW2100015153.1.13",
          "form": "촉촉한 지금 만족해여겠어요",
          "original_form": "촉촉한 지금 만족해여겠어요",
          "speaker_id": "2",
          "time": "20210828 08:08"
        },
        {
          "id": "MDRW2100015153.1.14",
          "form": "name2 님 계신 지역은 오늘 날씨 어떤가요?",
          "original_form": "&name2& 님 계신 지역은 오늘 날씨 어떤가요?",
          "speaker_id": "1",
          "time": "20210828 09:42"
        },
        {
          "id": "MDRW2100015153.1.15",
          "form": "흐렸는데 지금은 해가 떠 있네요!",
          "original_form": "흐렸는데 지금은 해가 떠 있네요!",
          "speaker_id": "2",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100015153.1.16",
          "form": "name1님 날씨는 어때요?",
          "original_form": "&name1&님 날씨는 어때요?",
          "speaker_id": "2",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100015153.1.17",
          "form": "저도요! 날씨 완전 좋아요",
          "original_form": "저도요! 날씨 완전 좋아요",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100015153.1.18",
          "form": "언제까지 비가 올까요",
          "original_form": "언제까지 비가 올까요",
          "speaker_id": "2",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100015153.1.19",
          "form": "태풍이 얼른 갔으면 좋겠어요",
          "original_form": "태풍이 얼른 갔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100015153.1.20",
          "form": "아 지금 태풍 지나가고 있나요?",
          "original_form": "아 지금 태풍 지나가고 있나요?",
          "speaker_id": "1",
          "time": "20210828 12:24"
        },
        {
          "id": "MDRW2100015153.1.21",
          "form": "비 그치면 완전 가을 될 것 같아요",
          "original_form": "비 그치면 완전 가을 될 것 같아요",
          "speaker_id": "1",
          "time": "20210828 12:24"
        },
        {
          "id": "MDRW2100015153.1.22",
          "form": "태풍 지나가고 있다고 들었어요!!",
          "original_form": "태풍 지나가고 있다고 들었어요!!",
          "speaker_id": "2",
          "time": "20210828 12:25"
        },
        {
          "id": "MDRW2100015153.1.23",
          "form": "마자요 어제 밤도 완전 선선하더라구요",
          "original_form": "마자요 어제 밤도 완전 선선하더라구요",
          "speaker_id": "2",
          "time": "20210828 12:25"
        },
        {
          "id": "MDRW2100015153.1.24",
          "form": "요즘 오전이랑 밤에는 되게 선선해요",
          "original_form": "요즘 오전이랑 밤에는 되게 선선해요",
          "speaker_id": "1",
          "time": "20210828 12:25"
        },
        {
          "id": "MDRW2100015153.1.25",
          "form": "원래 아침마다 운동 나가는데 요즘엔 아침에 나가기 쌀쌀할 것 같아서 안 나간지 꽤 됐어요ㅋㅋㅋ",
          "original_form": "원래 아침마다 운동 나가는데 요즘엔 아침에 나가기 쌀쌀할 것 같아서 안 나간지 꽤 됐어요ㅋㅋㅋ",
