      "utterance": [
        {
          "id": "MDRW2100029276.1.1",
          "form": "요새 게임 뭐해유",
          "original_form": "요새 게임 뭐해유",
          "speaker_id": "1",
          "time": "20210903 00:59"
        },
        {
          "id": "MDRW2100029276.1.2",
          "form": "최근엔 야숨을 했어용",
          "original_form": "최근엔 야숨을 했어용",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029276.1.3",
          "form": "스팀게임도 쪼끔 했어용",
          "original_form": "스팀게임도 쪼끔 했어용",
          "speaker_id": "2",
          "time": "20210903 01:00"
        },
        {
          "id": "MDRW2100029276.1.4",
          "form": "쿠킹덤도 햇네요",
          "original_form": "쿠킹덤도 햇네요",
          "speaker_id": "2",
          "time": "20210903 01:01"
        },
        {
          "id": "MDRW2100029276.1.5",
          "form": "와 젤다 재밌을 것 같아 스팀은 어떤건가유?",
          "original_form": "와 젤다 재밌을 것 같아 스팀은 어떤건가유?",
          "speaker_id": "1",
          "time": "20210903 01:03"
        },
        {
          "id": "MDRW2100029276.1.6",
          "form": "젤다 담에 우리만나면 직접하게해드릴수도잇사와요",
          "original_form": "젤다 담에 우리만나면 직접하게해드릴수도잇사와요",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029276.1.7",
          "form": "스팀은 그 게임 모아두는 곳 같은 거구",
          "original_form": "스팀은 그 게임 모아두는 곳 같은 거구",
          "speaker_id": "2",
          "time": "20210903 01:04"
        },
        {
          "id": "MDRW2100029276.1.8",
          "form": "최근엔 리듬닥터나 로보토미 코퍼레이션을 햇답니돠",
          "original_form": "최근엔 리듬닥터나 로보토미 코퍼레이션을 햇답니돠",
          "speaker_id": "2",
          "time": "20210903 01:05"
        },
        {
          "id": "MDRW2100029276.1.9",
          "form": "세상에 우리 꼭 만나자ㅋㅋㅋㅋ",
          "original_form": "세상에 우리 꼭 만나자ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:06"
        },
        {
          "id": "MDRW2100029276.1.10",
          "form": "나 사실 젤다가 그 남자앤줄 알았는데 알고보니 다른 이름이더라",
          "original_form": "나 사실 젤다가 그 남자앤줄 알았는데 알고보니 다른 이름이더라",
          "speaker_id": "1",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029276.1.11",
          "form": "너무 충격이였어ㅋㅋㅋㅋ당연히 젤다인줄..",
          "original_form": "너무 충격이였어ㅋㅋㅋㅋ당연히 젤다인줄..",
          "speaker_id": "1",
          "time": "20210903 01:08"
        },
        {
          "id": "MDRW2100029276.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 그 남자애는 링크!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그 남자애는 링크!!",
          "speaker_id": "2",
          "time": "20210903 01:10"
        },
        {
          "id": "MDRW2100029276.1.13",
          "form": "담에 만나서 맛난것도 먹고 겜도 하고 그럽시당",
          "original_form": "담에 만나서 맛난것도 먹고 겜도 하고 그럽시당",
          "speaker_id": "2",
          "time": "20210903 01:11"
        },
        {
          "id": "MDRW2100029276.1.14",
          "form": "링크 이름 너무 어색해ㅋㅋㅋㅋ젤다가 찰떡 아니냐며..",
          "original_form": "링크 이름 너무 어색해ㅋㅋㅋㅋ젤다가 찰떡 아니냐며..",
          "speaker_id": "1",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100029276.1.15",
          "form": "좋아요 담에 만나면 꼭 같이 해유ㅠㅠ 난 요새 쿠킹덤 밖에 안해서 뭔 게임있는지도 모르게쒀",
          "original_form": "좋아요 담에 만나면 꼭 같이 해유ㅠㅠ 난 요새 쿠킹덤 밖에 안해서 뭔 게임있는지도 모르게쒀",
          "speaker_id": "1",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100029276.1.16",
          "form": "ㅋㅋㅋㅋ젤다는 시리즈에 나오는 공주님 이름이엇다ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋ젤다는 시리즈에 나오는 공주님 이름이엇다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029276.1.17",
          "form": "쿠킹덤도 요새겜인데 머",
          "original_form": "쿠킹덤도 요새겜인데 머",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029276.1.18",
          "form": "플스론 겜 안햇어??",
          "original_form": "플스론 겜 안햇어??",
          "speaker_id": "2",
          "time": "20210903 11:15"
        },
        {
          "id": "MDRW2100029276.1.19",
          "form": "플스...한달동안 하고 그 이후로 쭉 방치돼있어...ㅋㅋㅋㅋㅋ",
          "original_form": "플스...한달동안 하고 그 이후로 쭉 방치돼있어...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:57"
        },
        {
          "id": "MDRW2100029276.1.20",
          "form": "시디를 사서 해야하다보니 재밌는 게임 뭐가 있는지도 모르겠고ㅠ.ㅠ",
          "original_form": "시디를 사서 해야하다보니 재밌는 게임 뭐가 있는지도 모르겠고ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210903 13:58"
        },
        {
          "id": "MDRW2100029276.1.21",
          "form": "ㅠㅠㅠ삿던 겜 잇지않앗나,",
          "original_form": "ㅠㅠㅠ삿던 겜 잇지않앗나,",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029276.1.22",
          "form": "그건별로엿어?",
          "original_form": "그건별로엿어?",
          "speaker_id": "2",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100029276.1.23",
          "form": "디트로이트? 맞나 이거 재밌었는데",
          "original_form": "디트로이트? 맞나 이거 재밌었는데",
          "speaker_id": "1",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100029276.1.24",
          "form": "선택지로 이야기가 흘러가다보니 뭔가 결말보고 난 후로는 지겨운 느낌이랄까ㅋㅋㅋㅋ...",
          "original_form": "선택지로 이야기가 흘러가다보니 뭔가 결말보고 난 후로는 지겨운 느낌이랄까ㅋㅋㅋㅋ...",
          "speaker_id": "1",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100029276.1.25",
          "form": "글쿠나...",
          "original_form": "글쿠나...",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100029276.1.26",
          "form": "담에 기회되면 니어오토마타두 해보는건어때",
          "original_form": "담에 기회되면 니어오토마타두 해보는건어때",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029276.1.27",
          "form": "베스타도 플스버전잇던데",
          "original_form": "베스타도 플스버전잇던데",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029276.1.28",
          "form": "처음 들어봐서 검색해봤는데 로봇친구들이 나오는구먼!",
          "original_form": "처음 들어봐서 검색해봤는데 로봇친구들이 나오는구먼!",
          "speaker_id": "1",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100029276.1.29",
          "form": "파판이랑 콜라보도 했었넹",
          "original_form": "파판이랑 콜라보도 했었넹",
          "speaker_id": "1",
          "time": "20210903 18:03"
        },
        {
          "id": "MDRW2100029276.1.30",
          "form": "마저마저 꽤 유명한 게임이야",
          "original_form": "마저마저 꽤 유명한 게임이야",
          "speaker_id": "2",
          "time": "20210903 18:46"
        },
        {
          "id": "MDRW2100029276.1.31",
          "form": "근데 컴터보단 플스가  조작도 쉽다고하더라구",
          "original_form": "근데 컴터보단 플스가  조작도 쉽다고하더라구",
          "speaker_id": "2",
          "time": "20210903 18:46"
        }
      ]
