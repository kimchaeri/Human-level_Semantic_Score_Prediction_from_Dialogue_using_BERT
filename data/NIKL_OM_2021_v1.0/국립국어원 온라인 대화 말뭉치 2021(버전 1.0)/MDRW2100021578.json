      "utterance": [
        {
          "id": "MDRW2100021578.1.1",
          "form": "치킨 추천 감사드려요! 꼭 먹어볼게요",
          "original_form": "치킨 추천 감사드려요! 꼭 먹어볼게요",
          "speaker_id": "1",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100021578.1.2",
          "form": "네네 ㅋㅋㅋㅋ한식 중에 좋아하시는 메뉴 있으세요?",
          "original_form": "네네 ㅋㅋㅋㅋ한식 중에 좋아하시는 메뉴 있으세요?",
          "speaker_id": "2",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021578.1.3",
          "form": "갈비요!!",
          "original_form": "갈비요!!",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021578.1.4",
          "form": "간장으로 된거요",
          "original_form": "간장으로 된거요",
          "speaker_id": "1",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021578.1.5",
          "form": "아아 ㅋㅋㅋㅋ그렇군요 다음으로는 뭐좋어하세요?",
          "original_form": "아아 ㅋㅋㅋㅋ그렇군요 다음으로는 뭐좋어하세요?",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021578.1.6",
          "form": "다음으로는 잡채요!",
          "original_form": "다음으로는 잡채요!",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100021578.1.7",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100021578.1.8",
          "form": "아 잡채맛잇죠 ㅠㅠㅋㅋㅋㅋ 계속 입에 들어가요",
          "original_form": "아 잡채맛잇죠 ㅠㅠㅋㅋㅋㅋ 계속 입에 들어가요",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100021578.1.9",
          "form": "음 저는 불고기나 두루치기좋아해요!",
          "original_form": "음 저는 불고기나 두루치기좋아해요!",
          "speaker_id": "2",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100021578.1.10",
          "form": "맞아요!! 끊임없이 흡입해요",
          "original_form": "맞아요!! 끊임없이 흡입해요",
          "speaker_id": "1",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100021578.1.11",
          "form": "크으 고기는 사랑이죠!!",
          "original_form": "크으 고기는 사랑이죠!!",
          "speaker_id": "1",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100021578.1.12",
          "form": "맞아요 ㅠㅠ 왤케맛잇죠ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅠㅠ 왤케맛잇죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100021578.1.13",
          "form": "파스타도 좋아하세요?",
          "original_form": "파스타도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100021578.1.14",
          "form": "헉 파스타도 너무 좋아해서 면 쌓아두고 있어오ㅋㅋㅋ",
          "original_form": "헉 파스타도 너무 좋아해서 면 쌓아두고 있어오ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100021578.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ저두요 소스 세일하면 쟁여두구요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저두요 소스 세일하면 쟁여두구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100021578.1.16",
          "form": "소스 직접 해드시나요?",
          "original_form": "소스 직접 해드시나요?",
          "speaker_id": "2",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100021578.1.17",
          "form": "안귀찮을때는 해먹어요!!",
          "original_form": "안귀찮을때는 해먹어요!!",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100021578.1.18",
          "form": "맛있는거 먹고싶을때는 소스를!!!!",
          "original_form": "맛있는거 먹고싶을때는 소스를!!!!",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100021578.1.19",
          "form": "오 그렇군요 요리 쫌 하시나봐요!",
          "original_form": "오 그렇군요 요리 쫌 하시나봐요!",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100021578.1.20",
          "form": "무슨 소스 쥬로 해드세요?",
          "original_form": "무슨 소스 쥬로 해드세요?",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100021578.1.21",
          "form": "크림을 많이 먹어요!",
          "original_form": "크림을 많이 먹어요!",
          "speaker_id": "1",
          "time": "20210829 15:01"
        },
        {
          "id": "MDRW2100021578.1.22",
          "form": "막입이라ㅋㅋ 그냥 다 잘먹어서 그렇슴닷ㅋㅋ",
          "original_form": "막입이라ㅋㅋ 그냥 다 잘먹어서 그렇슴닷ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:01"
        },
        {
          "id": "MDRW2100021578.1.23",
          "form": "아 그렇군요 ㅋㅋㅋㅋ크림이 생각보다 쉽다고하더라구요??",
          "original_form": "아 그렇군요 ㅋㅋㅋㅋ크림이 생각보다 쉽다고하더라구요??",
          "speaker_id": "2",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100021578.1.24",
          "form": "생크림넣으면된다고 본거같아요 ㅋㅋㅋ",
          "original_form": "생크림넣으면된다고 본거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:02"
        },
        {
          "id": "MDRW2100021578.1.25",
          "form": "맞아요ㅋㄱㅋㅋㅋ",
