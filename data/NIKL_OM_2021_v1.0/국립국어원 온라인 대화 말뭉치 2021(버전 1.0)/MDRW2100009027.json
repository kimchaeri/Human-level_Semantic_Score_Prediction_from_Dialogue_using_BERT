        {
          "id": "MDRW2100009027.1.1",
          "form": "싸이월드아세요?",
          "original_form": "싸이월드아세요?",
          "speaker_id": "1",
          "time": "20210822 21:02"
        },
        {
          "id": "MDRW2100009027.1.2",
          "form": "그럼요 ㅎㅎ 20대 연애기록이 그대로... 부활 했어요?",
          "original_form": "그럼요 ㅎㅎ 20대 연애기록이 그대로... 부활 했어요?",
          "speaker_id": "2",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009027.1.3",
          "form": "부활하나봐요ㅎ",
          "original_form": "부활하나봐요ㅎ",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009027.1.4",
          "form": "맨날 부활한다하고 아직도 안한거에요?",
          "original_form": "맨날 부활한다하고 아직도 안한거에요?",
          "speaker_id": "2",
          "time": "20210822 22:26"
        },
        {
          "id": "MDRW2100009027.1.5",
          "form": "그런가봐요ㄱ",
          "original_form": "그런가봐요ㄱ",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009027.1.6",
          "form": "진짜 그 사진이 남아있을런지 궁금하네요 28세정도까지 한 것 같아요",
          "original_form": "진짜 그 사진이 남아있을런지 궁금하네요 28세정도까지 한 것 같아요",
          "speaker_id": "2",
          "time": "20210822 22:53"
        },
        {
          "id": "MDRW2100009027.1.7",
          "form": "전 대학때",
          "original_form": "전 대학때",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009027.1.8",
          "form": "굴욕",
          "original_form": "굴욕",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009027.1.9",
          "form": "크흐흐 구남친들 사진 없나요",
          "original_form": "크흐흐 구남친들 사진 없나요",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009027.1.10",
          "form": "ㅋㅋ 먼가 잔뜩일것같아 열어보기가 겁나요",
          "original_form": "ㅋㅋ 먼가 잔뜩일것같아 열어보기가 겁나요",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009027.1.11",
          "form": "ㅋㅋㅋ진짜 판도라의 상자에요 저도",
          "original_form": "ㅋㅋㅋ진짜 판도라의 상자에요 저도",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009027.1.12",
          "form": "왜케 굴욕인지",
          "original_form": "왜케 굴욕인지",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009027.1.13",
          "form": "그래도 그립지 않나요? 청춘ㅋ",
          "original_form": "그래도 그립지 않나요? 청춘ㅋ",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009027.1.14",
          "form": "그때가 꿀이었는데ㅠ",
