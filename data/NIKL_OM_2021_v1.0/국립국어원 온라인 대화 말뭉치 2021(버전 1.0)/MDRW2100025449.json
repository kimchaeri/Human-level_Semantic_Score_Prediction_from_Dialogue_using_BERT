      "utterance": [
        {
          "id": "MDRW2100025449.1.1",
          "form": "배떡 블랙은 식욕이 감퇴될거 같다구여",
          "original_form": "배떡 블랙은 식욕이 감퇴될거 같다구여",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025449.1.2",
          "form": "마ㅈ완전",
          "original_form": "마ㅈ완전",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025449.1.3",
          "form": "떡을 갯벌에 샤브샤브한 너낌",
          "original_form": "떡을 갯벌에 샤브샤브한 너낌",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025449.1.4",
          "form": "음식에 장난치지말아줘",
          "original_form": "음식에 장난치지말아줘",
          "speaker_id": "2",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025449.1.5",
          "form": "민초도 그렇고",
          "original_form": "민초도 그렇고",
          "speaker_id": "2",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025449.1.6",
          "form": "아징짜",
          "original_form": "아징짜",
          "speaker_id": "1",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025449.1.7",
          "form": "뇌절메뉴 싫어 ㅠ",
          "original_form": "뇌절메뉴 싫어 ㅠ",
          "speaker_id": "1",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025449.1.8",
          "form": "그냥 원래 있는대로 먹고싶음",
          "original_form": "그냥 원래 있는대로 먹고싶음",
          "speaker_id": "2",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025449.1.9",
          "form": "초코 떡볶이 진짜",
          "original_form": "초코 떡볶이 진짜",
          "speaker_id": "2",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025449.1.10",
          "form": "누가 자꾸그런거승인하는거야",
          "original_form": "누가 자꾸그런거승인하는거야",
          "speaker_id": "1",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025449.1.11",
          "form": "최근에 본 음식중",
          "original_form": "최근에 본 음식중",
          "speaker_id": "2",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025449.1.12",
          "form": "아 그거진짜오바",
          "original_form": "아 그거진짜오바",
          "speaker_id": "1",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025449.1.13",
          "form": "짜장을하라구...",
          "original_form": "짜장을하라구...",
          "speaker_id": "1",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025449.1.14",
          "form": "누가 만든거야 진짜",
          "original_form": "누가 만든거야 진짜",
          "speaker_id": "2",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025449.1.15",
          "form": "왜떡볶이가 초코야",
          "original_form": "왜떡볶이가 초코야",
          "speaker_id": "1",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025449.1.16",
          "form": "초코라니",
          "original_form": "초코라니",
          "speaker_id": "2",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025449.1.17",
          "form": "디저트랑",
          "original_form": "디저트랑",
          "speaker_id": "1",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025449.1.18",
          "form": "식사는 분리좀해줘라",
          "original_form": "식사는 분리좀해줘라",
          "speaker_id": "1",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025449.1.19",
          "form": "떡볶이는 달면 안돼",
          "original_form": "떡볶이는 달면 안돼",
          "speaker_id": "2",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025449.1.20",
          "form": "ㅇㅇ ㄹㅇ",
          "original_form": "ㅇㅇ ㄹㅇ",
          "speaker_id": "1",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025449.1.21",
          "form": "본래의맛은 해치면안돠지",
          "original_form": "본래의맛은 해치면안돠지",
          "speaker_id": "1",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025449.1.22",
          "form": "민초떡볶이 완전별로",
          "original_form": "민초떡볶이 완전별로",
          "speaker_id": "1",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025449.1.23",
          "form": "제일 별로",
          "original_form": "제일 별로",
          "speaker_id": "2",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025449.1.24",
          "form": "민초떡볶이 민초치킨",
          "original_form": "민초떡볶이 민초치킨",
          "speaker_id": "2",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025449.1.25",
          "form": "민초돈가스",
          "original_form": "민초돈가스",
          "speaker_id": "2",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100025449.1.26",
          "form": "민초떡볶이",
          "original_form": "민초떡볶이",
          "speaker_id": "1",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100025449.1.27",
          "form": "그거떡볶퀸도",
          "original_form": "그거떡볶퀸도",
          "speaker_id": "1",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100025449.1.28",
          "form": "욕하던데",
          "original_form": "욕하던데",
          "speaker_id": "1",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100025449.1.29",
          "form": "민초돈가스는뭐야",
          "original_form": "민초돈가스는뭐야",
          "speaker_id": "1",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100025449.1.30",
          "form": "창의력을 이상하게발사하네",
          "original_form": "창의력을 이상하게발사하네",
          "speaker_id": "1",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100025449.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ아진짜 민초 너무 싫어",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아진짜 민초 너무 싫어",
          "speaker_id": "2",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100025449.1.32",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100025449.1.33",
          "form": "민초단과겸상불가",
          "original_form": "민초단과겸상불가",
          "speaker_id": "1",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100025449.1.34",
          "form": "민초단과같은아이스크림쉐어불가",
          "original_form": "민초단과같은아이스크림쉐어불가",
