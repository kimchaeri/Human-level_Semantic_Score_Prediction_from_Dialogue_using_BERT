      "utterance": [
        {
          "id": "MDRW2100019936.1.1",
          "form": "일 때려치고싶어",
          "original_form": "일 때려치고싶어",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019936.1.2",
          "form": "안돼 돈 벌어야지",
          "original_form": "안돼 돈 벌어야지",
          "speaker_id": "1",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100019936.1.3",
          "form": "급여 너무 적어",
          "original_form": "급여 너무 적어",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019936.1.4",
          "form": "대행사로 이직하고싶어",
          "original_form": "대행사로 이직하고싶어",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019936.1.5",
          "form": "난 이직 찬성이야!",
          "original_form": "난 이직 찬성이야!",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019936.1.6",
          "form": "그런데 일단 조금만 더 힘내보자",
          "original_form": "그런데 일단 조금만 더 힘내보자",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019936.1.7",
          "form": "12월 중순까진 버텨야돼,,",
          "original_form": "12월 중순까진 버텨야돼,,",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019936.1.8",
          "form": "업무 스트레스 장난 아니겠다ㅠㅠ",
          "original_form": "업무 스트레스 장난 아니겠다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019936.1.9",
          "form": "아니 자기네들이 생각한 기존 이미지랑은 다른 새롭고 돌I 스러운 사진을 찾으라는데",
          "original_form": "아니 자기네들이 생각한 기존 이미지랑은 다른 새롭고 돌I 스러운 사진을 찾으라는데",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019936.1.10",
          "form": "하,,, 빠꾸먹고 또먹고 또찾고",
          "original_form": "하,,, 빠꾸먹고 또먹고 또찾고",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019936.1.11",
          "form": "어떤 일이었는데?",
          "original_form": "어떤 일이었는데?",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019936.1.12",
          "form": "프로그램 코디?",
          "original_form": "프로그램 코디?",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019936.1.13",
          "form": "새로 들어가는 드라마 대본보고 스타일 정하는거ㅜㅡ",
          "original_form": "새로 들어가는 드라마 대본보고 스타일 정하는거ㅜㅡ",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019936.1.14",
          "form": "드라마 대본을 보고 정한다고?",
          "original_form": "드라마 대본을 보고 정한다고?",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019936.1.15",
          "form": "상상력 풍부해야겠네..",
          "original_form": "상상력 풍부해야겠네..",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019936.1.16",
          "form": "아니 기존에 떠오르는 이미지는 찾지말라는데",
          "original_form": "아니 기존에 떠오르는 이미지는 찾지말라는데",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019936.1.17",
          "form": "하,,,,,",
          "original_form": "하,,,,,",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019936.1.18",
          "form": "스트레스ㅡㅡ",
          "original_form": "스트레스ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019936.1.19",
          "form": "스트레스 장난 아니겠다",
          "original_form": "스트레스 장난 아니겠다",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019936.1.20",
          "form": "나라면 이직 벌써 찾고있을거야",
          "original_form": "나라면 이직 벌써 찾고있을거야",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019936.1.21",
          "form": "자소서 다시 고쳐써야겠다",
          "original_form": "자소서 다시 고쳐써야겠다",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019936.1.22",
          "form": "스페도 쌓아야지",
          "original_form": "스페도 쌓아야지",
          "speaker_id": "2",
          "time": "20210827 23:17"
        }
