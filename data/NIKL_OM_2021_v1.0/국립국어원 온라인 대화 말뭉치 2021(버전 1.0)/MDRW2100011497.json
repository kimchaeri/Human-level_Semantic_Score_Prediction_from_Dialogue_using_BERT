        {
          "id": "MDRW2100011497.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 13:14"
        },
        {
          "id": "MDRW2100011497.1.2",
          "form": "안녕하세여~",
          "original_form": "안녕하세여~",
          "speaker_id": "2",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100011497.1.3",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210824 13:22"
        },
        {
          "id": "MDRW2100011497.1.4",
          "form": "저는 기독교이긴 한데... 교회를 안가요..",
          "original_form": "저는 기독교이긴 한데... 교회를 안가요..",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011497.1.5",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011497.1.6",
          "form": "아뇨저는 무교랍니다",
          "original_form": "아뇨저는 무교랍니다",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011497.1.7",
          "form": "헉 교회를아예안가나요?",
          "original_form": "헉 교회를아예안가나요?",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011497.1.8",
          "form": "넹...ㅋㅋㅋㅋㅋ",
          "original_form": "넹...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011497.1.9",
          "form": "어렸을 때부터 가족따라 기독교되었는뎅",
          "original_form": "어렸을 때부터 가족따라 기독교되었는뎅",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011497.1.10",
          "form": "좀 이게 맞나 싶어서 안다니게 되었어요 ㅎㅎㅎㅎㅎ",
          "original_form": "좀 이게 맞나 싶어서 안다니게 되었어요 ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011497.1.11",
          "form": "근데 하나님은 믿어요 이상하죠 ㅌㅋㅋㅋㅋ",
          "original_form": "근데 하나님은 믿어요 이상하죠 ㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011497.1.12",
          "form": "아 그럼",
          "original_form": "아 그럼",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100011497.1.13",
          "form": "교회는아예 안가시는거예요?",
          "original_form": "교회는아예 안가시는거예요?",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100011497.1.14",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100011497.1.15",
          "form": "무교신가요?",
          "original_form": "무교신가요?",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100011497.1.16",
          "form": "거의 무교라고 봐야해요!",
          "original_form": "거의 무교라고 봐야해요!",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
