      "utterance": [
        {
          "id": "MDRW2100041255.1.1",
          "form": "집안일 좋아 하시나요",
          "original_form": "집안일 좋아 하시나요",
          "speaker_id": "2",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100041255.1.2",
          "form": "아니요 name2님은 좋아하시나요?",
          "original_form": "아니요 &name2&님은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100041255.1.3",
          "form": "전 집안일중 청소가 제일싫어요",
          "original_form": "전 집안일중 청소가 제일싫어요",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100041255.1.4",
          "form": "ㅋㅋㅋㅋ그렇죠",
          "original_form": "ㅋㅋㅋㅋ그렇죠",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100041255.1.5",
          "form": "그럼 그나마 나은 집안일은 무엇인가요?",
          "original_form": "그럼 그나마 나은 집안일은 무엇인가요?",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100041255.1.6",
          "form": "요리가 나아요",
          "original_form": "요리가 나아요",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041255.1.7",
          "form": "그리고 빨래 요",
          "original_form": "그리고 빨래 요",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041255.1.8",
          "form": "어떤가요",
          "original_form": "어떤가요",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100041255.1.9",
          "form": "저도 빨래가 그나마 괜찮고 설거지랑 청소가 정말 싫은 것 같아요",
          "original_form": "저도 빨래가 그나마 괜찮고 설거지랑 청소가 정말 싫은 것 같아요",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100041255.1.10",
          "form": "청소중 욕실 청소가 최악이죠",
          "original_form": "청소중 욕실 청소가 최악이죠",
          "speaker_id": "2",
          "time": "20210905 22:12"
        },
        {
          "id": "MDRW2100041255.1.11",
          "form": "그러니까요 찌린내가 정말 최악이죠ㅋㅋㅋㅋ",
          "original_form": "그러니까요 찌린내가 정말 최악이죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041255.1.12",
          "form": "곰팡이도 찌든때도 참 어려워요",
          "original_form": "곰팡이도 찌든때도 참 어려워요",
          "speaker_id": "2",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100041255.1.13",
          "form": "게다가 락스냄새는 정말 어지럽더라고요",
          "original_form": "게다가 락스냄새는 정말 어지럽더라고요",
          "speaker_id": "1",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100041255.1.14",
          "form": "그래도 락스가 청소엔 짱이죠",
          "original_form": "그래도 락스가 청소엔 짱이죠",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041255.1.15",
          "form": "맞아요 그래서 어지럽거나 냄새나도 쓰는거죠ㅋㅋㅋ",
          "original_form": "맞아요 그래서 어지럽거나 냄새나도 쓰는거죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100041255.1.16",
          "form": "문 활짝 열고해야지 \r\n안그렁 큰일나요",
          "original_form": "문 활짝 열고해야지 \r\n안그렁 큰일나요",
          "speaker_id": "2",
          "time": "20210905 23:00"
