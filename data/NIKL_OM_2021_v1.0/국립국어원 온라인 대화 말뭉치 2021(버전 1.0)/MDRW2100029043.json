      "utterance": [
        {
          "id": "MDRW2100029043.1.1",
          "form": "좋아하는 음식 있으세요? 단팥빵...?",
          "original_form": "좋아하는 음식 있으세요? 단팥빵...?",
          "speaker_id": "2",
          "time": "20210902 23:26"
        },
        {
          "id": "MDRW2100029043.1.2",
          "form": "저 단팥도 조아하고",
          "original_form": "저 단팥도 조아하고",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029043.1.3",
          "form": "백앙금도 좋아합니다ㅎ",
          "original_form": "백앙금도 좋아합니다ㅎ",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029043.1.4",
          "form": "그 완두앙금빵도 조아하고ㅋㅋ",
          "original_form": "그 완두앙금빵도 조아하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100029043.1.5",
          "form": "ㅋㅋㅋㅋ아 빵 완전 좋아하시네요??",
          "original_form": "ㅋㅋㅋㅋ아 빵 완전 좋아하시네요??",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029043.1.6",
          "form": "크림빵은요?",
          "original_form": "크림빵은요?",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029043.1.7",
          "form": "크림빵도 조아해요! 그 빵집에 파는 클래식한 긴 크림빵ㅋㅋ",
          "original_form": "크림빵도 조아해요! 그 빵집에 파는 클래식한 긴 크림빵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029043.1.8",
          "form": "ㅋㅋㅋㅋㅋ클래식한 취향이시군요!",
          "original_form": "ㅋㅋㅋㅋㅋ클래식한 취향이시군요!",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029043.1.9",
          "form": "네넹...ㅋㅋ맘모스 좋아하구ㅋㅋ",
          "original_form": "네넹...ㅋㅋ맘모스 좋아하구ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029043.1.10",
          "form": "저는 이름 왕창 길고 어려운 빵들이 좋아요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 이름 왕창 길고 어려운 빵들이 좋아요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029043.1.11",
          "form": "좋아하는 빵 있으세요??",
          "original_form": "좋아하는 빵 있으세요??",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029043.1.12",
          "form": "앗 뭐죠ㅋㅋ",
          "original_form": "앗 뭐죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029043.1.13",
          "form": "호두....들어간.....기억이 안나요",
          "original_form": "호두....들어간.....기억이 안나요",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029043.1.14",
          "form": "밤식빵도 좋아해요",
          "original_form": "밤식빵도 좋아해요",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029043.1.15",
          "form": "심플한게 짱이죠",
          "original_form": "심플한게 짱이죠",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029043.1.16",
          "form": "밤식빵 좋져",
          "original_form": "밤식빵 좋져",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029043.1.17",
          "form": "밤 잔뜩 들어간거 너무 맛있는ㅠ 갓 만들고ㅠㅠ",
          "original_form": "밤 잔뜩 들어간거 너무 맛있는ㅠ 갓 만들고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029043.1.18",
          "form": "마자요 달달하고 꼬소하고ㅠㅠ",
          "original_form": "마자요 달달하고 꼬소하고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029043.1.19",
          "form": "갓나온 건 촉촉하고ㅠㅠㅠ",
          "original_form": "갓나온 건 촉촉하고ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029043.1.20",
          "form": "크림빵도 좋아해요 근데 전 커스터드 크림빵...",
          "original_form": "크림빵도 좋아해요 근데 전 커스터드 크림빵...",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029043.1.21",
          "form": "먼지 아시죠 그것도 꽤 클래식...",
          "original_form": "먼지 아시죠 그것도 꽤 클래식...",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029043.1.22",
          "form": "앗 알죠 그 노란색 커스터드 크림 들어가고",
          "original_form": "앗 알죠 그 노란색 커스터드 크림 들어가고",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029043.1.23",
          "form": "동그란 모양의",
          "original_form": "동그란 모양의",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029043.1.24",
          "form": "마자요 완전 맛있죠",
          "original_form": "마자요 완전 맛있죠",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029043.1.25",
          "form": "좀 단 걸 좋아해서ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "좀 단 걸 좋아해서ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029043.1.26",
          "form": "아 혹시 그거 아세요?",
          "original_form": "아 혹시 그거 아세요?",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029043.1.27",
          "form": "예전에 나온 방울카스테라??",
          "original_form": "예전에 나온 방울카스테라??",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029043.1.28",
          "form": "앗 알아요",
          "original_form": "앗 알아요",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029043.1.29",
          "form": "그 약간 뻑뻑하고",
          "original_form": "그 약간 뻑뻑하고",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029043.1.30",
          "form": "우유랑 먹으면 진짜 맛있는데",
          "original_form": "우유랑 먹으면 진짜 맛있는데",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029043.1.31",
          "form": "마자요!!!! 우유랑 먹으면 찰떡",
          "original_form": "마자요!!!! 우유랑 먹으면 찰떡",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029043.1.32",
          "form": "빵 얘기하니까 갑자기 그게 떠오르네요",
          "original_form": "빵 얘기하니까 갑자기 그게 떠오르네요",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029043.1.33",
          "form": "어릴 때 진짜 좋아했는데",
          "original_form": "어릴 때 진짜 좋아했는데",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029043.1.34",
          "form": "요즘 파나요? 마트에서 본 기억이 읍는ㅠㅠ",
          "original_form": "요즘 파나요? 마트에서 본 기억이 읍는ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029043.1.35",
          "form": "갑자기 먹고 싶어 졌어요ㅎㅎ",
          "original_form": "갑자기 먹고 싶어 졌어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029043.1.36",
          "form": "저도 본 기억이 없어요ㅠㅠㅠㅠ",
          "original_form": "저도 본 기억이 없어요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029043.1.37",
          "form": "그르니까요... 얘기나오니까 먹고싶네요",
          "original_form": "그르니까요... 얘기나오니까 먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 23:41"
        }
      ]
    }
