      "utterance": [
        {
          "id": "MDRW2100023122.1.1",
          "form": "넌 좋아하는 과자 있어?",
          "original_form": "넌 좋아하는 과자 있어?",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023122.1.2",
          "form": "나 감자과자가 너무좋아",
          "original_form": "나 감자과자가 너무좋아",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023122.1.3",
          "form": "눈을 감자",
          "original_form": "눈을 감자",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023122.1.4",
          "form": "와 나도 감자스낵 좋아하는데",
          "original_form": "와 나도 감자스낵 좋아하는데",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023122.1.5",
          "form": "난 포카칩 어니언",
          "original_form": "난 포카칩 어니언",
          "speaker_id": "2",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100023122.1.6",
          "form": "너랑 나랑 감자만 먹겠네",
          "original_form": "너랑 나랑 감자만 먹겠네",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023122.1.7",
          "form": "감자로 이루어진 사이네",
          "original_form": "감자로 이루어진 사이네",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023122.1.8",
          "form": "초코과자도",
          "original_form": "초코과자도",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023122.1.9",
          "form": "고구마도 좋아해?",
          "original_form": "고구마도 좋아해?",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023122.1.10",
          "form": "너무 맛있어",
          "original_form": "너무 맛있어",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023122.1.11",
          "form": "나도 초코 좋아해",
          "original_form": "나도 초코 좋아해",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023122.1.12",
          "form": "고구마는 고구마깡만 먹어",
          "original_form": "고구마는 고구마깡만 먹어",
          "speaker_id": "1",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100023122.1.13",
          "form": "고구마향나는 빵도 맛있긴하더라",
          "original_form": "고구마향나는 빵도 맛있긴하더라",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023122.1.14",
          "form": "아 알지 포장마차 고구마빵도 괜찮더라",
          "original_form": "아 알지 포장마차 고구마빵도 괜찮더라",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023122.1.15",
          "form": "붕어빵 좋아해?",
          "original_form": "붕어빵 좋아해?",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023122.1.16",
          "form": "응 붕어빵 슈크림맛 완전 좋아",
          "original_form": "응 붕어빵 슈크림맛 완전 좋아",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023122.1.17",
          "form": "겨울의 낙이지",
          "original_form": "겨울의 낙이지",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023122.1.18",
