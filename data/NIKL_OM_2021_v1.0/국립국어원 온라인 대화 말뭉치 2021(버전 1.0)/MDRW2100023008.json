      "utterance": [
        {
          "id": "MDRW2100023008.1.1",
          "form": "그러게요 식당도 못가고",
          "original_form": "그러게요 식당도 못가고",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023008.1.2",
          "form": "저녁식사는 하셨어요?",
          "original_form": "저녁식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023008.1.3",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023008.1.4",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100023008.1.5",
          "form": "닭볶음탕이요 ㅎㅎ",
          "original_form": "닭볶음탕이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100023008.1.6",
          "form": "메이님은요?",
          "original_form": "메이님은요?",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100023008.1.7",
          "form": "저는 불닭볶음면이요",
          "original_form": "저는 불닭볶음면이요",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023008.1.8",
          "form": "헛 그거 너무 맵던데",
          "original_form": "헛 그거 너무 맵던데",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023008.1.9",
          "form": "아 까르보나라불닭으로 먹었어요ㅎㅎ",
          "original_form": "아 까르보나라불닭으로 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023008.1.10",
          "form": "까르보는 좀 덜 매워요?",
          "original_form": "까르보는 좀 덜 매워요?",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023008.1.11",
          "form": "네 한결 덜 매워요",
          "original_form": "네 한결 덜 매워요",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023008.1.12",
          "form": "그럼 저도 먹어봐야겠네요",
          "original_form": "그럼 저도 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023008.1.13",
          "form": "네네 저도 자주 먹진 않는데",
          "original_form": "네네 저도 자주 먹진 않는데",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023008.1.14",
          "form": "가끔 당길 때가 있더라고요",
          "original_form": "가끔 당길 때가 있더라고요",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023008.1.15",
          "form": "매운거 당길때가 있는데",
          "original_form": "매운거 당길때가 있는데",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023008.1.16",
          "form": "쉽게 못먹겠더라구요",
          "original_form": "쉽게 못먹겠더라구요",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023008.1.17",
          "form": "저도 요즘은 매운음식 잘 못먹겠어요",
          "original_form": "저도 요즘은 매운음식 잘 못먹겠어요",
          "speaker_id": "1",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023008.1.18",
          "form": "속도 쓰리고",
          "original_form": "속도 쓰리고",
          "speaker_id": "1",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023008.1.19",
          "form": "네 마자요",
          "original_form": "네 마자요",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023008.1.20",
          "form": "매운거 잘먹고싶다",
          "original_form": "매운거 잘먹고싶다",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023008.1.21",
          "form": "ㅎㅎ그래도 매운 것 잘 못먹으면 위장은 편안할 거예요",
          "original_form": "ㅎㅎ그래도 매운 것 잘 못먹으면 위장은 편안할 거예요",
          "speaker_id": "1",
          "time": "20210829 22:51"
        }
      ]
