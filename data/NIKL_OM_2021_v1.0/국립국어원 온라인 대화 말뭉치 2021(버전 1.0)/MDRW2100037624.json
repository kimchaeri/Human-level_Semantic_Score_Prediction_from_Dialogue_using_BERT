      "utterance": [
        {
          "id": "MDRW2100037624.1.1",
          "form": "국민체조 추억인데요 ㅋㅋㅋ",
          "original_form": "국민체조 추억인데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100037624.1.2",
          "form": "안한지 10년 넘었어요 ㅋㅋㅋ",
          "original_form": "안한지 10년 넘었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100037624.1.3",
          "form": "한 2년전인가..?",
          "original_form": "한 2년전인가..?",
          "speaker_id": "2",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100037624.1.4",
          "form": "네네 ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100037624.1.5",
          "form": "중국에서 온 친구랑 이야기하다고 체육시간 이야기 나오고 그래서",
          "original_form": "중국에서 온 친구랑 이야기하다고 체육시간 이야기 나오고 그래서",
          "speaker_id": "2",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100037624.1.6",
          "form": "아아 ㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100037624.1.7",
          "form": "국민운동 이야기가 나온거에요",
          "original_form": "국민운동 이야기가 나온거에요",
          "speaker_id": "2",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100037624.1.8",
          "form": "아 완전 추억이다 ㅋㅋ 네네",
          "original_form": "아 완전 추억이다 ㅋㅋ 네네",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100037624.1.9",
          "form": "그래서 유튜브에도 국민체조 치고 같이 따라해봤는데",
          "original_form": "그래서 유튜브에도 국민체조 치고 같이 따라해봤는데",
          "speaker_id": "2",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100037624.1.10",
          "form": "힘들어...!!",
          "original_form": "힘들어...!!",
          "speaker_id": "2",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100037624.1.11",
          "form": "앗 진짜여???",
          "original_form": "앗 진짜여???",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037624.1.12",
          "form": "초딩땐 분명 이게 무슨 운동이야 ㅋㅋㅋㅋ 했는데",
          "original_form": "초딩땐 분명 이게 무슨 운동이야 ㅋㅋㅋㅋ 했는데",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037624.1.13",
          "form": "운동이 되는거에요!!ㅠㅠ",
          "original_form": "운동이 되는거에요!!ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037624.1.14",
          "form": "하긴 예전엔 막 대충했었던거 같아여 ㅋㅋㄲ",
          "original_form": "하긴 예전엔 막 대충했었던거 같아여 ㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037624.1.15",
          "form": "그래서 지금도 가끔 해요",
          "original_form": "그래서 지금도 가끔 해요",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037624.1.16",
          "form": "스트레칭 겸 해서",
          "original_form": "스트레칭 겸 해서",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037624.1.17",
          "form": "저도 오늘 함 해볼까여 ㄲㅋ",
          "original_form": "저도 오늘 함 해볼까여 ㄲㅋ",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037624.1.18",
          "form": "그게 인생 첫 운동의 시작이였던거 같은데 ㅋㅋ",
          "original_form": "그게 인생 첫 운동의 시작이였던거 같은데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100037624.1.19",
          "form": "어쨌든 생각보다 잘 짜여진 체조더라구요",
          "original_form": "어쨌든 생각보다 잘 짜여진 체조더라구요",
          "speaker_id": "2",
          "time": "20210901 13:58"
        },
        {
          "id": "MDRW2100037624.1.20",
          "form": "어른돼서 하니까 느껴져요",
          "original_form": "어른돼서 하니까 느껴져요",
          "speaker_id": "2",
          "time": "20210901 13:58"
        },
        {
          "id": "MDRW2100037624.1.21",
          "form": "저 오늘 해보고 후기 알려드릴께여 ㅋㅋㅋ",
