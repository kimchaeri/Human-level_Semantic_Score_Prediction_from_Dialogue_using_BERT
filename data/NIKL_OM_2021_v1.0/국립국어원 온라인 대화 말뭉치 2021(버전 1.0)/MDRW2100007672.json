      "utterance": [
        {
          "id": "MDRW2100007672.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210822 09:23"
        },
        {
          "id": "MDRW2100007672.1.2",
          "form": "숨쉬는 운동만해요",
          "original_form": "숨쉬는 운동만해요",
          "speaker_id": "2",
          "time": "20210822 09:24"
        },
        {
          "id": "MDRW2100007672.1.3",
          "form": "가끔요가도 하고요",
          "original_form": "가끔요가도 하고요",
          "speaker_id": "2",
          "time": "20210822 09:24"
        },
        {
          "id": "MDRW2100007672.1.4",
          "form": "전 홈트해요",
          "original_form": "전 홈트해요",
          "speaker_id": "1",
          "time": "20210822 09:24"
        },
        {
          "id": "MDRW2100007672.1.5",
          "form": "요즘 코로나때문에 저도 홈트 자주 하는거 같네요",
          "original_form": "요즘 코로나때문에 저도 홈트 자주 하는거 같네요",
          "speaker_id": "2",
          "time": "20210822 09:28"
        },
        {
          "id": "MDRW2100007672.1.6",
          "form": "유튜브 뭐뭐 봐요?",
          "original_form": "유튜브 뭐뭐 봐요?",
          "speaker_id": "2",
          "time": "20210822 09:28"
        },
        {
          "id": "MDRW2100007672.1.7",
          "form": "땅크요",
          "original_form": "땅크요",
          "speaker_id": "1",
          "time": "20210822 09:30"
        },
        {
          "id": "MDRW2100007672.1.8",
          "form": "전 자세요정 같은거 봐요",
          "original_form": "전 자세요정 같은거 봐요",
          "speaker_id": "2",
          "time": "20210822 09:31"
        },
        {
          "id": "MDRW2100007672.1.9",
          "form": "몸 균형 좋으실 거 같네요",
          "original_form": "몸 균형 좋으실 거 같네요",
          "speaker_id": "1",
          "time": "20210822 09:32"
        },
        {
          "id": "MDRW2100007672.1.10",
          "form": "그렇진 않아요 ㅋㅋㅋㅋ",
          "original_form": "그렇진 않아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:37"
        },
        {
          "id": "MDRW2100007672.1.11",
          "form": "자기몸에는 다들 불만족",
          "original_form": "자기몸에는 다들 불만족",
          "speaker_id": "1",
          "time": "20210822 09:38"
        },
        {
          "id": "MDRW2100007672.1.12",
          "form": "마자요 매일 자기관리에 열심히 인거 같아요",
          "original_form": "마자요 매일 자기관리에 열심히 인거 같아요",
          "speaker_id": "2",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007672.1.13",
          "form": "꾸준히 하기가 쉽지 않아서요",
          "original_form": "꾸준히 하기가 쉽지 않아서요",
          "speaker_id": "1",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007672.1.14",
          "form": "마자요 전 옛날에 필라테스고 해봤는데",
          "original_form": "마자요 전 옛날에 필라테스고 해봤는데",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007672.1.15",
          "form": "너무 비쌌어ㅛ",
          "original_form": "너무 비쌌어ㅛ",
          "speaker_id": "2",
          "time": "20210822 10:06"
        },
        {
          "id": "MDRW2100007672.1.16",
