      "utterance": [
        {
          "id": "MDRW2100036841.1.1",
          "form": "점심은 드셨나요?",
          "original_form": "점심은 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 13:30"
        },
        {
          "id": "MDRW2100036841.1.2",
          "form": "밀프랩도 다체롭게 먹으려고",
          "original_form": "밀프랩도 다체롭게 먹으려고",
          "speaker_id": "1",
          "time": "20210831 13:30"
        },
        {
          "id": "MDRW2100036841.1.3",
          "form": "3가지를 한꺼번에 하면 냉동기간이 한달 이렇게 되어서 또 맛이없어요",
          "original_form": "3가지를 한꺼번에 하면 냉동기간이 한달 이렇게 되어서 또 맛이없어요",
          "speaker_id": "1",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100036841.1.4",
          "form": "그렇균요 ㅣㅎㅎ",
          "original_form": "그렇균요 ㅣㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100036841.1.5",
          "form": "냉동 오래하면 마르죠ㅋㅋ",
          "original_form": "냉동 오래하면 마르죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100036841.1.6",
          "form": "하하 전 샌드위치요 비와서 귀찮아서,,",
          "original_form": "하하 전 샌드위치요 비와서 귀찮아서,,",
          "speaker_id": "1",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036841.1.7",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036841.1.8",
          "form": "전 간단하게 컵라면 먹었네요 ㅎㅎ",
          "original_form": "전 간단하게 컵라면 먹었네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:41"
        },
        {
          "id": "MDRW2100036841.1.9",
          "form": "비오니깐 나가기 귀찮더라고요 젖고,,,",
          "original_form": "비오니깐 나가기 귀찮더라고요 젖고,,,",
          "speaker_id": "1",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100036841.1.10",
          "form": "집에 와서도 대충 집밥으 ㄹ먹어서 할말도 없네요 ㅋ",
          "original_form": "집에 와서도 대충 집밥으 ㄹ먹어서 할말도 없네요 ㅋ",
          "speaker_id": "1",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100036841.1.11",
          "form": "과일얘기할까요?",
          "original_form": "과일얘기할까요?",
          "speaker_id": "1",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100036841.1.12",
          "form": "ㅋㅋㅋ과일얘기 좋죠ㅎㅎ 과일 좋아하세요?",
          "original_form": "ㅋㅋㅋ과일얘기 좋죠ㅎㅎ 과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 21:04"
        },
        {
          "id": "MDRW2100036841.1.13",
          "form": "네 전 대부분 다 좋아해요 !! 싫어하는 과일은 없어요",
          "original_form": "네 전 대부분 다 좋아해요 !! 싫어하는 과일은 없어요",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100036841.1.14",
          "form": "저도 싫어하는 과일은 없어요ㅋㅋ 아 근데 포도는 껍질까먹기 귀찮아서 잘 안먹네요ㅋㅋ",
          "original_form": "저도 싫어하는 과일은 없어요ㅋㅋ 아 근데 포도는 껍질까먹기 귀찮아서 잘 안먹네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:09"
        },
        {
          "id": "MDRW2100036841.1.15",
          "form": "저도요 저도요 캡벨포도 그래서 안먹어요",
          "original_form": "저도요 저도요 캡벨포도 그래서 안먹어요",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100036841.1.16",
          "form": "씨적은 거봉이랑 샤인머스캣은 잘먹어요 ㅋㅋㅋ",
          "original_form": "씨적은 거봉이랑 샤인머스캣은 잘먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100036841.1.17",
          "form": "님도그런가요",
          "original_form": "님도그런가요",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100036841.1.18",
          "form": "저도 씨적거나 껍질도 먹을수 있는거 잘먹어요ㅋㅋㅋ",
          "original_form": "저도 씨적거나 껍질도 먹을수 있는거 잘먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100036841.1.19",
          "form": "토마토가 최애이거나 그런가요?",
          "original_form": "토마토가 최애이거나 그런가요?",
          "speaker_id": "2",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100036841.1.20",
          "form": "아 제가 마스크를 쓰고 오래 걸어다니면 얼굴이 빨개ㅐ져서",
          "original_form": "아 제가 마스크를 쓰고 오래 걸어다니면 얼굴이 빨개ㅐ져서",
          "speaker_id": "1",
          "time": "20210831 21:12"
        },
        {
          "id": "MDRW2100036841.1.21",
          "form": "제가 얼굴이 토마토 같아서 지었어요,,",
          "original_form": "제가 얼굴이 토마토 같아서 지었어요,,",
          "speaker_id": "1",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100036841.1.22",
          "form": "ㅋㅋㅋ귀여우시네여 저도 얼굴 잘 빨개지는 편이긴 해요ㅋㅋ",
          "original_form": "ㅋㅋㅋ귀여우시네여 저도 얼굴 잘 빨개지는 편이긴 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:14"
        }
      ]
