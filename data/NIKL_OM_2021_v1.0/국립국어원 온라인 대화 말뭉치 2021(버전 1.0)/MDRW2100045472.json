      "utterance": [
        {
          "id": "MDRW2100045472.1.1",
          "form": "하자는 전부 다 읶어서",
          "original_form": "하자는 전부 다 읶어서",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045472.1.2",
          "form": "안 넣으셔도 될듯",
          "original_form": "안 넣으셔도 될듯",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045472.1.3",
          "form": "그쵸 뭐...",
          "original_form": "그쵸 뭐...",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045472.1.4",
          "form": "견딜수 있는만큼 하자요",
          "original_form": "견딜수 있는만큼 하자요",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045472.1.5",
          "form": "2급상춤같은",
          "original_form": "2급상춤같은",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045472.1.6",
          "form": "사준다고 하면서 매번 지갑 놓ㄱ..",
          "original_form": "사준다고 하면서 매번 지갑 놓ㄱ..",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045472.1.7",
          "form": "폐급말고",
          "original_form": "폐급말고",
          "speaker_id": "1",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045472.1.8",
          "form": "못 견디겠네요",
          "original_form": "못 견디겠네요",
          "speaker_id": "2",
          "time": "20210908 02:45"
        },
        {
          "id": "MDRW2100045472.1.9",
          "form": "우...",
          "original_form": "우...",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045472.1.10",
          "form": "때려놓고 사과하는 사람?",
          "original_form": "때려놓고 사과하는 사람?",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045472.1.11",
          "form": "뭐 돈많고 일찍죽는 남자도 좋아요",
          "original_form": "뭐 돈많고 일찍죽는 남자도 좋아요",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045472.1.12",
          "form": "남편으로는",
          "original_form": "남편으로는",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045472.1.13",
          "form": "아녀",
          "original_form": "아녀",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045472.1.14",
          "form": "전 그런 사람이면 여자도 좋아요",
          "original_form": "전 그런 사람이면 여자도 좋아요",
          "speaker_id": "2",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045472.1.15",
          "form": "걍 견딜만한 하자?",
          "original_form": "걍 견딜만한 하자?",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045472.1.16",
          "form": "결혼못하몀 상속 못해요 그냥 사랑만 할꺼면",
          "original_form": "결혼못하몀 상속 못해요 그냥 사랑만 할꺼면",
          "speaker_id": "1",
          "time": "20210908 02:46"
        },
        {
          "id": "MDRW2100045472.1.17",
          "form": "건강한 사람이랑 하셈",
          "original_form": "건강한 사람이랑 하셈",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045472.1.18",
          "form": "평소에 엄청 다정한데 분조장 있어서 주변에 시비 터는 사람?",
          "original_form": "평소에 엄청 다정한데 분조장 있어서 주변에 시비 터는 사람?",
          "speaker_id": "2",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045472.1.19",
          "form": "그냥 밥먹을따 쩝쩝거리는데 본인이 스테이크 쏜 그런사람이요",
          "original_form": "그냥 밥먹을따 쩝쩝거리는데 본인이 스테이크 쏜 그런사람이요",
          "speaker_id": "1",
          "time": "20210908 02:47"
        },
        {
          "id": "MDRW2100045472.1.20",
          "form": "그런 조언 하기에는 본인 이상형이 하자 있는 사람이잖아요",
          "original_form": "그런 조언 하기에는 본인 이상형이 하자 있는 사람이잖아요",
          "speaker_id": "2",
          "time": "20210908 02:47"
        }
      ]
