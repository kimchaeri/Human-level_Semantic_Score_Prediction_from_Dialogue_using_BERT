      "utterance": [
        {
          "id": "MDRW2100023912.1.1",
          "form": "예랑가랑은 미대 브이로그가 재밌어요!",
          "original_form": "예랑가랑은 미대 브이로그가 재밌어요!",
          "speaker_id": "1",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023912.1.2",
          "form": "대학에서 자고 먹는 걸 보여줘서",
          "original_form": "대학에서 자고 먹는 걸 보여줘서",
          "speaker_id": "1",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023912.1.3",
          "form": "진짜 새로운 충격이었거든요",
          "original_form": "진짜 새로운 충격이었거든요",
          "speaker_id": "1",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100023912.1.4",
          "form": "와우 대학 숙식이라니",
          "original_form": "와우 대학 숙식이라니",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023912.1.5",
          "form": "짱이네요",
          "original_form": "짱이네요",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023912.1.6",
          "form": "바로 나중에 볼 영상추가해야겠어요 ㅋㅋㅋ",
          "original_form": "바로 나중에 볼 영상추가해야겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023912.1.7",
          "form": "네!!!! 진짜 재밌어요 ㅎ.ㅎ !!!",
          "original_form": "네!!!! 진짜 재밌어요 ㅎ.ㅎ !!!",
          "speaker_id": "1",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023912.1.8",
          "form": "name2은 추천해주고 싶은 영상 있으세요!?",
          "original_form": "&name2&은 추천해주고 싶은 영상 있으세요!?",
          "speaker_id": "1",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023912.1.9",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023912.1.10",
          "form": "저는 하하하의 삼색이 때문에 낚시를 못하겠다",
          "original_form": "저는 하하하의 삼색이 때문에 낚시를 못하겠다",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023912.1.11",
          "form": "추천드려요!!",
          "original_form": "추천드려요!!",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023912.1.12",
          "form": "고양이들이 넘 귀엽고 재밌어요 ㅠ!",
          "original_form": "고양이들이 넘 귀엽고 재밌어요 ㅠ!",
          "speaker_id": "2",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100023912.1.13",
          "form": "헐!!! 당장 보고 와여겠어요",
          "original_form": "헐!!! 당장 보고 와여겠어요",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100023912.1.14",
          "form": "삼색이 넘 구ㅏ여워요 ㅠㅠ",
          "original_form": "삼색이 넘 구ㅏ여워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100023912.1.15",
          "form": "제 최애 고양이는 야통이!",
          "original_form": "제 최애 고양이는 야통이!",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100023912.1.16",
          "form": "우오아ㅏ",
          "original_form": "우오아ㅏ",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023912.1.17",
          "form": "야통파이시군요!!",
          "original_form": "야통파이시군요!!",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023912.1.18",
          "form": "저는 삼색파이지만",
          "original_form": "저는 삼색파이지만",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023912.1.19",
          "form": "야통이도 사랑합니다",
          "original_form": "야통이도 사랑합니다",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023912.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100023912.1.21",
          "form": "헛 그 야통이가 우는게 너무 신기하더라고요!",
          "original_form": "헛 그 야통이가 우는게 너무 신기하더라고요!",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023912.1.22",
          "form": "야옹 안 울고 웩옹 하는개",
          "original_form": "야옹 안 울고 웩옹 하는개",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023912.1.23",
          "form": "넘 귀여웠어요",
          "original_form": "넘 귀여웠어요",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100023912.1.24",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023912.1.25",
          "form": "이얏호응 하고 우는 고양이 저 처음봤어요 ㅠ",
          "original_form": "이얏호응 하고 우는 고양이 저 처음봤어요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023912.1.26",
          "form": "너무 귀여워요 ㅠㅠ",
          "original_form": "너무 귀여워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023912.1.27",
          "form": "그 왓챠에도 하하하 콘텐츠가 있더라구요!",
          "original_form": "그 왓챠에도 하하하 콘텐츠가 있더라구요!",
          "speaker_id": "2",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100023912.1.28",
          "form": "헐 진짜요???? 신기하네요!!!",
          "original_form": "헐 진짜요???? 신기하네요!!!",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100023912.1.29",
          "form": "혹시 왓챠보세요??",
          "original_form": "혹시 왓챠보세요??",
          "speaker_id": "2",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100023912.1.30",
          "form": "유튜브 영상이랑 같은 건가요?",
          "original_form": "유튜브 영상이랑 같은 건가요?",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100023912.1.31",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100023912.1.32",
          "form": "유튜브 영상이랑 같은 것 같아요~",
          "original_form": "유튜브 영상이랑 같은 것 같아요~",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023912.1.33",
          "form": "이번에 왓챠 가입해서 해리포터 도장깨기 했는데",
          "original_form": "이번에 왓챠 가입해서 해리포터 도장깨기 했는데",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023912.1.34",
          "form": "넘 재밌었어요!!!",
          "original_form": "넘 재밌었어요!!!",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100023912.1.35",
          "form": "헐 저도 해리포터 완전 좋아해요!",
          "original_form": "헐 저도 해리포터 완전 좋아해요!",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023912.1.36",
          "form": "헛 저도 햐리포터 완전 좋아해요!",
          "original_form": "헛 저도 햐리포터 완전 좋아해요!",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100023912.1.37",
          "form": "시리즈 다 보셨나요?!",
          "original_form": "시리즈 다 보셨나요?!",
