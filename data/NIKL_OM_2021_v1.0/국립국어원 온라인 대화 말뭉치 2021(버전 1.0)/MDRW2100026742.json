        {
          "id": "MDRW2100026742.1.1",
          "form": "사고 싶은거 진짜 많음",
          "original_form": "사고 싶은거 진짜 많음",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026742.1.2",
          "form": "뭔데?",
          "original_form": "뭔데?",
          "speaker_id": "1",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026742.1.3",
          "form": "걍 돈을 쓰고 싶다 해여하나",
          "original_form": "걍 돈을 쓰고 싶다 해여하나",
          "speaker_id": "2",
          "time": "20210901 23:51"
        },
        {
          "id": "MDRW2100026742.1.4",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026742.1.5",
          "form": "우리 지금은",
          "original_form": "우리 지금은",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026742.1.6",
          "form": "모을 때여요?",
          "original_form": "모을 때여요?",
          "speaker_id": "1",
          "time": "20210901 23:52"
        },
        {
          "id": "MDRW2100026742.1.7",
          "form": "아 공모주로 금요일에 30 들어오니까",
          "original_form": "아 공모주로 금요일에 30 들어오니까",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026742.1.8",
          "form": "그거로 사고싳은거 ㄱㄱ",
          "original_form": "그거로 사고싳은거 ㄱㄱ",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026742.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026742.1.10",
          "form": "근데 사고 싶은데",
          "original_form": "근데 사고 싶은데",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026742.1.11",
          "form": "안사도 되는것들 있잖아",
          "original_form": "안사도 되는것들 있잖아",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026742.1.12",
          "form": "그런거 나중에 후회할것 같아서",
          "original_form": "그런거 나중에 후회할것 같아서",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026742.1.13",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026742.1.14",
          "form": "많지ㅋㅋㅋㅋㅋ",
          "original_form": "많지ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026742.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026742.1.16",
          "form": "일단 다이어트 열심히 한다음에",
          "original_form": "일단 다이어트 열심히 한다음에",
