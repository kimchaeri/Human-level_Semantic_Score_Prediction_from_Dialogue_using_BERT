        },
        {
          "id": "MDRW2100031722.1.3",
          "form": "저도없어요ㅠ",
          "original_form": "저도없어요ㅠ",
          "speaker_id": "1",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100031722.1.4",
          "form": "저는 개 키운적 있어요",
          "original_form": "저는 개 키운적 있어요",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031722.1.5",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031722.1.6",
          "form": "어떤,,?",
          "original_form": "어떤,,?",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031722.1.7",
          "form": "진돗개요~",
          "original_form": "진돗개요~",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031722.1.8",
          "form": "너무부러워요!!",
          "original_form": "너무부러워요!!",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031722.1.9",
          "form": "이름은 뭐였나요?!",
          "original_form": "이름은 뭐였나요?!",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031722.1.10",
          "form": "그냥 키우던 아이라 나비였어요 ㅎㅎ",
          "original_form": "그냥 키우던 아이라 나비였어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031722.1.11",
          "form": "너무 귀여워요",
          "original_form": "너무 귀여워요",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031722.1.12",
          "form": "귀여웠어요 ㅎ 듬직하고",
          "original_form": "귀여웠어요 ㅎ 듬직하고",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031722.1.13",
          "form": "백구였나요?  황구였나요?",
          "original_form": "백구였나요?  황구였나요?",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031722.1.14",
          "form": "백구도 있고, 황구도 있었네요",
          "original_form": "백구도 있고, 황구도 있었네요",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031722.1.15",
          "form": "와 한마리가 아니셨군요",
          "original_form": "와 한마리가 아니셨군요",
