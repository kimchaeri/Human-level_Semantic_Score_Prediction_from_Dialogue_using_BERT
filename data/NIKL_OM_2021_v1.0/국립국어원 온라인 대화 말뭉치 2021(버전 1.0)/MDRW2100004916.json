      "utterance": [
        {
          "id": "MDRW2100004916.1.1",
          "form": "잘 꾸미세요?",
          "original_form": "잘 꾸미세요?",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004916.1.2",
          "form": "적당히 꾸며요 직장인스타일로",
          "original_form": "적당히 꾸며요 직장인스타일로",
          "speaker_id": "1",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004916.1.3",
          "form": "날씬하신가요?",
          "original_form": "날씬하신가요?",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004916.1.4",
          "form": "저 완전 확찐자예요",
          "original_form": "저 완전 확찐자예요",
          "speaker_id": "1",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004916.1.5",
          "form": "ㅋㅋㅋㅋ저두요 ㅋㅋㅋㅋ코로나이후로 8키로쪗다가 요즘다시 노력중이라",
          "original_form": "ㅋㅋㅋㅋ저두요 ㅋㅋㅋㅋ코로나이후로 8키로쪗다가 요즘다시 노력중이라",
          "speaker_id": "2",
          "time": "20210822 10:34"
        },
        {
          "id": "MDRW2100004916.1.6",
          "form": "2.3키로 빠졋네여..",
          "original_form": "2.3키로 빠졋네여..",
          "speaker_id": "2",
          "time": "20210822 10:34"
        },
        {
          "id": "MDRW2100004916.1.7",
          "form": "와대단해요",
          "original_form": "와대단해요",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100004916.1.8",
          "form": "저는 요지부동이네요",
          "original_form": "저는 요지부동이네요",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100004916.1.9",
          "form": "어떻게빼셨나요",
          "original_form": "어떻게빼셨나요",
          "speaker_id": "1",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100004916.1.10",
          "form": "홈트요 ! 땅끄랑 타바타 해용 ㅋㅋ",
          "original_form": "홈트요 ! 땅끄랑 타바타 해용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:12"
        },
        {
          "id": "MDRW2100004916.1.11",
          "form": "저도해봐야겠어요",
          "original_form": "저도해봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100004916.1.12",
          "form": "추천하는동영상있어요?",
          "original_form": "추천하는동영상있어요?",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100004916.1.13",
          "form": "사람마다 다른거같아서 맞는거 찾아보심을 추천드려용 ㅎㅎ",
          "original_form": "사람마다 다른거같아서 맞는거 찾아보심을 추천드려용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100004916.1.14",
          "form": "좋아하는 옷스타일있으세요?",
          "original_form": "좋아하는 옷스타일있으세요?",
          "speaker_id": "2",
          "time": "20210822 13:50"
        },
        {
          "id": "MDRW2100004916.1.15",
          "form": "직장인이라 회사갈때입는옷많이사요",
          "original_form": "직장인이라 회사갈때입는옷많이사요",
          "speaker_id": "1",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100004916.1.16",
          "form": "정장스타일루요?",
          "original_form": "정장스타일루요?",
          "speaker_id": "2",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100004916.1.17",
          "form": "아니요 블라우스 슬랙스 이런식이예요",
          "original_form": "아니요 블라우스 슬랙스 이런식이예요",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100004916.1.18",
          "form": "평상복은 원피스 많이 입어요 편해서",
          "original_form": "평상복은 원피스 많이 입어요 편해서",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100004916.1.19",
          "form": "오 슬랙스도 원피스도 편하고 좋죠",
          "original_form": "오 슬랙스도 원피스도 편하고 좋죠",
          "speaker_id": "2",
          "time": "20210822 14:37"
        }
      ]
