      "utterance": [
        {
          "id": "MDRW2100044333.1.1",
          "form": "아마 가게가 사라지진 않았을 것 같은데 말이죠 ㅠ",
          "original_form": "아마 가게가 사라지진 않았을 것 같은데 말이죠 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044333.1.2",
          "form": "그쳐 요즘 이러니저러니 해도 맛집은 장사 더 잘되더라구요",
          "original_form": "그쳐 요즘 이러니저러니 해도 맛집은 장사 더 잘되더라구요",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044333.1.3",
          "form": "알려드리지 못해서 너무 안타까워요 ㅠ",
          "original_form": "알려드리지 못해서 너무 안타까워요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044333.1.4",
          "form": "다음에 그 근처 가게되면",
          "original_form": "다음에 그 근처 가게되면",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044333.1.5",
          "form": "한번 열심히 찾아볼게요!",
          "original_form": "한번 열심히 찾아볼게요!",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044333.1.6",
          "form": "빵도 좋아하시나요?",
          "original_form": "빵도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044333.1.7",
          "form": "빵도 좋아해요!",
          "original_form": "빵도 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044333.1.8",
          "form": "name1님 혹시 스콘 좋아하세요?",
          "original_form": "&name1&님 혹시 스콘 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044333.1.9",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044333.1.10",
          "form": "오목눈이 라는 가게가 있어요",
          "original_form": "오목눈이 라는 가게가 있어요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044333.1.11",
          "form": "거기 스콘 진짜! 맛있어요",
          "original_form": "거기 스콘 진짜! 맛있어요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044333.1.12",
          "form": "혹시 홍차도 좋아하신다면",
          "original_form": "혹시 홍차도 좋아하신다면",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044333.1.13",
          "form": "그집 얼그레이 차도 맛있거든요",
          "original_form": "그집 얼그레이 차도 맛있거든요",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044333.1.14",
          "form": "진짜 추천해요",
          "original_form": "진짜 추천해요",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044333.1.15",
          "form": "앗 즐겨찾기에 꼭 등록해둘게요",
          "original_form": "앗 즐겨찾기에 꼭 등록해둘게요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044333.1.16",
          "form": "제가 커피를 못마셔서 차나 에이드를 먹거든요",
          "original_form": "제가 커피를 못마셔서 차나 에이드를 먹거든요",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044333.1.17",
          "form": "앗 다행이에요 :)",
          "original_form": "앗 다행이에요 :)",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044333.1.18",
          "form": "마음에 드셨으면 좋겠어요 ㅎㅎ",
          "original_form": "마음에 드셨으면 좋겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044333.1.19",
          "form": "혹시 좀 특이한 요리 드시고싶으면",
          "original_form": "혹시 좀 특이한 요리 드시고싶으면",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044333.1.20",
          "form": "망원동에 고미태 라는 가게도 괜찮아요!",
          "original_form": "망원동에 고미태 라는 가게도 괜찮아요!",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044333.1.21",
          "form": "시즌마다 음식이 바뀌는데 직접 개발한?거고",
          "original_form": "시즌마다 음식이 바뀌는데 직접 개발한?거고",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044333.1.22",
          "form": "특이한데 제 입에는 맛있더라구요 ㅋㅋㅋ",
          "original_form": "특이한데 제 입에는 맛있더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044333.1.23",
          "form": "옹? 주로 어떤 종류의 음식인가요??",
          "original_form": "옹? 주로 어떤 종류의 음식인가요??",
          "speaker_id": "2",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044333.1.24",
          "form": "면요리예요",
          "original_form": "면요리예요",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044333.1.25",
          "form": "지난시즌은 닭콩국수 였어요!",
          "original_form": "지난시즌은 닭콩국수 였어요!",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044333.1.26",
          "form": "면요리!",
          "original_form": "면요리!",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044333.1.27",
          "form": "지난 시즌 음식도 맛이 좋았나봐요!",
          "original_form": "지난 시즌 음식도 맛이 좋았나봐요!",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044333.1.28",
          "form": "네네 처음 보는 음식인데",
          "original_form": "네네 처음 보는 음식인데",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044333.1.29",
          "form": "깔끔하고 좋았어요!",
          "original_form": "깔끔하고 좋았어요!",
          "speaker_id": "1",
          "time": "20210907 22:35"
        }
      ]
