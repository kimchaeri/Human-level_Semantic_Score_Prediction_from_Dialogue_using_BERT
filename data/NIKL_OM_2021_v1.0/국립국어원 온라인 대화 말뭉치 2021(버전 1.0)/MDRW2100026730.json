      "utterance": [
        {
          "id": "MDRW2100026730.1.1",
          "form": "운동 좋아해?",
          "original_form": "운동 좋아해?",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026730.1.2",
          "form": "운동 좋아해!",
          "original_form": "운동 좋아해!",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026730.1.3",
          "form": "근데 귀찮아서 안하지... ㅎㅎ",
          "original_form": "근데 귀찮아서 안하지... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026730.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ어떤 운동 제일 좋아해??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어떤 운동 제일 좋아해??",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026730.1.5",
          "form": "무슨 운동 좋아해??",
          "original_form": "무슨 운동 좋아해??",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026730.1.6",
          "form": "통했다 ㅋㅋㅋㅋㅋ",
          "original_form": "통했다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026730.1.7",
          "form": "음 나는 배드민턴이 제일 재밌는 것 같아!",
          "original_form": "음 나는 배드민턴이 제일 재밌는 것 같아!",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026730.1.8",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026730.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나도 배드민턴 좋아해!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 나도 배드민턴 좋아해!",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100026730.1.10",
          "form": "요즘에는 홈트 하고있어!",
          "original_form": "요즘에는 홈트 하고있어!",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026730.1.11",
          "form": "코로나 전에는 헬스장이나 요가원 다녔는데",
          "original_form": "코로나 전에는 헬스장이나 요가원 다녔는데",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026730.1.12",
          "form": "코로나 이후에는 걱정되서 못가겠더라구",
          "original_form": "코로나 이후에는 걱정되서 못가겠더라구",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100026730.1.13",
          "form": "오 멋있다!",
          "original_form": "오 멋있다!",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026730.1.14",
          "form": "마자 나도 예전엔 헬스장 다녔었는데",
          "original_form": "마자 나도 예전엔 헬스장 다녔었는데",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026730.1.15",
          "form": "확실히 헬스장 가서 운동하는게 살이 좀 빠지더라구 ㅎㅎ",
          "original_form": "확실히 헬스장 가서 운동하는게 살이 좀 빠지더라구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026730.1.16",
          "form": "맞아 확실히 헬스장이 좋긴 좋아!",
          "original_form": "맞아 확실히 헬스장이 좋긴 좋아!",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026730.1.17",
          "form": "요즘 다이어트 해??",
          "original_form": "요즘 다이어트 해??",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026730.1.18",
          "form": "거의 포기상태야... ㅎㅎ",
          "original_form": "거의 포기상태야... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:46"
        },
        {
          "id": "MDRW2100026730.1.19",
          "form": "유지하려고 노력하고 있어...",
          "original_form": "유지하려고 노력하고 있어...",
          "speaker_id": "2",
          "time": "20210902 08:46"
        },
        {
          "id": "MDRW2100026730.1.20",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "2",
          "time": "20210902 08:46"
        },
        {
          "id": "MDRW2100026730.1.21",
          "form": "나도 더 찌지만 않는 마음으로",
          "original_form": "나도 더 찌지만 않는 마음으로",
          "speaker_id": "1",
          "time": "20210902 09:11"
        },
        {
          "id": "MDRW2100026730.1.22",
          "form": "생활하고있어!",
          "original_form": "생활하고있어!",
          "speaker_id": "1",
          "time": "20210902 09:11"
        },
        {
          "id": "MDRW2100026730.1.23",
          "form": "근데 그것도 너무 힘들어ㅜㅜ",
          "original_form": "근데 그것도 너무 힘들어ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 09:11"
        },
        {
          "id": "MDRW2100026730.1.24",
          "form": "세상엔 왜 맛있는게 이렇게 많지...",
          "original_form": "세상엔 왜 맛있는게 이렇게 많지...",
          "speaker_id": "1",
          "time": "20210902 09:11"
        },
        {
          "id": "MDRW2100026730.1.25",
          "form": "맞아... 살찌기 너무 쉬워...",
          "original_form": "맞아... 살찌기 너무 쉬워...",
          "speaker_id": "2",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100026730.1.26",
          "form": "빼는 건 너무 어렵구...",
          "original_form": "빼는 건 너무 어렵구...",
          "speaker_id": "2",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100026730.1.27",
          "form": "슬프다... ㅠㅠ",
          "original_form": "슬프다... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100026730.1.28",
          "form": "그르니까ㅜㅜㅜ",
          "original_form": "그르니까ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100026730.1.29",
          "form": "건강을 위해 따로 뭐 하는게 있을까??",
          "original_form": "건강을 위해 따로 뭐 하는게 있을까??",
          "speaker_id": "1",
          "time": "20210902 09:25"
        },
        {
          "id": "MDRW2100026730.1.30",
          "form": "음... 야식 안 먹기??",
          "original_form": "음... 야식 안 먹기??",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100026730.1.31",
          "form": "늦은 시간엔 절대 안 먹으려고 노력하고 있어!",
          "original_form": "늦은 시간엔 절대 안 먹으려고 노력하고 있어!",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100026730.1.32",
          "form": "챙겨먹는 영양제 있어??",
          "original_form": "챙겨먹는 영양제 있어??",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100026730.1.33",
          "form": "응! 나 유산균이랑 오메가3 비타민 먹고있어!",
          "original_form": "응! 나 유산균이랑 오메가3 비타민 먹고있어!",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100026730.1.34",
          "form": "너는 영양제 챙겨먹어??",
          "original_form": "너는 영양제 챙겨먹어??",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100026730.1.35",
          "form": "우와 잘 챙겨먹네!",
          "original_form": "우와 잘 챙겨먹네!",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100026730.1.36",
          "form": "나는 비타민 하나밖에 안 먹어 ㅎㅎ",
          "original_form": "나는 비타민 하나밖에 안 먹어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:34"
        }
      ]
    }
