      "utterance": [
        {
          "id": "MDRW2100030819.1.1",
          "form": "레미제라블도 재밌죠",
          "original_form": "레미제라블도 재밌죠",
          "speaker_id": "2",
          "time": "20210904 12:56"
        },
        {
          "id": "MDRW2100030819.1.2",
          "form": "장발장 책 엄마가 사줬잖아",
          "original_form": "장발장 책 엄마가 사줬잖아",
          "speaker_id": "1",
          "time": "20210904 12:56"
        },
        {
          "id": "MDRW2100030819.1.3",
          "form": "맞아요 책부터 읽고 뮤지컬 본듯",
          "original_form": "맞아요 책부터 읽고 뮤지컬 본듯",
          "speaker_id": "2",
          "time": "20210904 12:57"
        },
        {
          "id": "MDRW2100030819.1.4",
          "form": "빵 훔친다고 그만큼 가둬놓고",
          "original_form": "빵 훔친다고 그만큼 가둬놓고",
          "speaker_id": "1",
          "time": "20210904 12:57"
        },
        {
          "id": "MDRW2100030819.1.5",
          "form": "근데 그거밖에 생각 안난다",
          "original_form": "근데 그거밖에 생각 안난다",
          "speaker_id": "1",
          "time": "20210904 12:57"
        },
        {
          "id": "MDRW2100030819.1.6",
          "form": "ㅋㅋㅋㅋ 시장도 되고 그래요",
          "original_form": "ㅋㅋㅋㅋ 시장도 되고 그래요",
          "speaker_id": "2",
          "time": "20210904 12:57"
        },
        {
          "id": "MDRW2100030819.1.7",
          "form": "저도 코제트 부분이랑",
          "original_form": "저도 코제트 부분이랑",
          "speaker_id": "2",
          "time": "20210904 12:57"
        },
        {
          "id": "MDRW2100030819.1.8",
          "form": "노래 하나밖에 생각 안나네요",
          "original_form": "노래 하나밖에 생각 안나네요",
          "speaker_id": "2",
          "time": "20210904 12:57"
        },
        {
          "id": "MDRW2100030819.1.9",
          "form": "코제트 양딸인가 결혼하는",
          "original_form": "코제트 양딸인가 결혼하는",
          "speaker_id": "1",
          "time": "20210904 12:57"
        },
        {
          "id": "MDRW2100030819.1.10",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210904 12:57"
        },
        {
          "id": "MDRW2100030819.1.11",
          "form": "그거랑",
          "original_form": "그거랑",
          "speaker_id": "2",
          "time": "20210904 12:57"
        },
        {
          "id": "MDRW2100030819.1.12",
          "form": "민중의 소리 어쩌구 하는 노래",
          "original_form": "민중의 소리 어쩌구 하는 노래",
          "speaker_id": "2",
          "time": "20210904 12:58"
        },
        {
          "id": "MDRW2100030819.1.13",
          "form": "재밌어서 좋아요",
          "original_form": "재밌어서 좋아요",
          "speaker_id": "2",
          "time": "20210904 12:58"
        },
        {
          "id": "MDRW2100030819.1.14",
          "form": "엄만 뮤지컬은 안봤다",
          "original_form": "엄만 뮤지컬은 안봤다",
          "speaker_id": "1",
          "time": "20210904 12:58"
        },
        {
          "id": "MDRW2100030819.1.15",
          "form": "그것도 공개되어있을거같은데",
          "original_form": "그것도 공개되어있을거같은데",
          "speaker_id": "2",
          "time": "20210904 12:58"
        },
        {
          "id": "MDRW2100030819.1.16",
          "form": "다음에 그것도 보여줘",
          "original_form": "다음에 그것도 보여줘",
          "speaker_id": "1",
          "time": "20210904 12:58"
        },
        {
          "id": "MDRW2100030819.1.17",
          "form": "집에만 있으니까 심심하신듯 ㅋㅋㅋ",
          "original_form": "집에만 있으니까 심심하신듯 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:58"
        },
        {
          "id": "MDRW2100030819.1.18",
          "form": "낮에 혼자 있으니까 할게없지",
          "original_form": "낮에 혼자 있으니까 할게없지",
          "speaker_id": "1",
          "time": "20210904 12:59"
        },
        {
          "id": "MDRW2100030819.1.19",
          "form": "주변에 뭐 많으면 보러가도 되는데",
          "original_form": "주변에 뭐 많으면 보러가도 되는데",
          "speaker_id": "2",
          "time": "20210904 12:59"
        },
        {
          "id": "MDRW2100030819.1.20",
          "form": "역시 지방이라",
          "original_form": "역시 지방이라",
          "speaker_id": "2",
          "time": "20210904 12:59"
        },
        {
          "id": "MDRW2100030819.1.21",
          "form": "그런걸로 무슨 이사를가니",
          "original_form": "그런걸로 무슨 이사를가니",
          "speaker_id": "1",
          "time": "20210904 12:59"
        },
        {
          "id": "MDRW2100030819.1.22",
          "form": "그래도 서울가면 좋은데...",
          "original_form": "그래도 서울가면 좋은데...",
          "speaker_id": "2",
          "time": "20210904 12:59"
        },
        {
          "id": "MDRW2100030819.1.23",
          "form": "집값 비싸고 애들 다컸으니",
          "original_form": "집값 비싸고 애들 다컸으니",
          "speaker_id": "1",
          "time": "20210904 12:59"
        },
        {
          "id": "MDRW2100030819.1.24",
          "form": "알아서 보고다녀야지",
          "original_form": "알아서 보고다녀야지",
          "speaker_id": "1",
          "time": "20210904 13:00"
        },
        {
          "id": "MDRW2100030819.1.25",
          "form": "뭐ㅜ요즘은 인터넷도 잘돼있으니까요",
          "original_form": "뭐ㅜ요즘은 인터넷도 잘돼있으니까요",
          "speaker_id": "2",
          "time": "20210904 13:00"
        },
        {
          "id": "MDRW2100030819.1.26",
          "form": "뮤지컬 음원도 나왔을듯!!",
          "original_form": "뮤지컬 음원도 나왔을듯!!",
          "speaker_id": "2",
          "time": "20210904 13:00"
        }
      ]
    }
  ]
