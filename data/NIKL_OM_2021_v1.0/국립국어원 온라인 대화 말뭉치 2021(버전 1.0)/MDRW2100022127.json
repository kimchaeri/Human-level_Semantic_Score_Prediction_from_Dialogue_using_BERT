      "utterance": [
        {
          "id": "MDRW2100022127.1.1",
          "form": "오늘 비온다는 예보가 있었나?",
          "original_form": "오늘 비온다는 예보가 있었나?",
          "speaker_id": "1",
          "time": "20210829 08:41"
        },
        {
          "id": "MDRW2100022127.1.2",
          "form": "이번주 내내 비온다니 장마가 맞긴맞나봐",
          "original_form": "이번주 내내 비온다니 장마가 맞긴맞나봐",
          "speaker_id": "1",
          "time": "20210829 08:42"
        },
        {
          "id": "MDRW2100022127.1.3",
          "form": "수요일까지 오는거 같아",
          "original_form": "수요일까지 오는거 같아",
          "speaker_id": "2",
          "time": "20210829 08:47"
        },
        {
          "id": "MDRW2100022127.1.4",
          "form": "여행기간엔 좋아지길 바래야지",
          "original_form": "여행기간엔 좋아지길 바래야지",
          "speaker_id": "2",
          "time": "20210829 08:48"
        },
        {
          "id": "MDRW2100022127.1.5",
          "form": "아침저녁 시원해져서 좋은데 습해서 낮에는 더워",
          "original_form": "아침저녁 시원해져서 좋은데 습해서 낮에는 더워",
          "speaker_id": "1",
          "time": "20210829 08:59"
        },
        {
          "id": "MDRW2100022127.1.6",
          "form": "비가 적당히오고 가을이 와야 할텐데",
          "original_form": "비가 적당히오고 가을이 와야 할텐데",
          "speaker_id": "1",
          "time": "20210829 09:00"
        },
        {
          "id": "MDRW2100022127.1.7",
          "form": "미세먼지도 한동안 없으니 너무좋고",
          "original_form": "미세먼지도 한동안 없으니 너무좋고",
          "speaker_id": "1",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100022127.1.8",
          "form": "옷 긴팔도 챙겨가야겠네",
          "original_form": "옷 긴팔도 챙겨가야겠네",
          "speaker_id": "2",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100022127.1.9",
          "form": "아침저녁 날씨가 선선하잖아. 바다라 바람도 강할테고",
          "original_form": "아침저녁 날씨가 선선하잖아. 바다라 바람도 강할테고",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100022127.1.10",
          "form": "파란하늘 본지 오래된것같네",
          "original_form": "파란하늘 본지 오래된것같네",
          "speaker_id": "1",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022127.1.11",
          "form": "여행가서 파란하늘 보면 좋겠다.",
          "original_form": "여행가서 파란하늘 보면 좋겠다.",
          "speaker_id": "2",
          "time": "20210829 15:17"
        },
        {
          "id": "MDRW2100022127.1.12",
          "form": "파란하늘 배경으로 사진 찍어본지가 언제였는지",
          "original_form": "파란하늘 배경으로 사진 찍어본지가 언제였는지",
          "speaker_id": "2",
          "time": "20210829 15:17"
        },
        {
          "id": "MDRW2100022127.1.13",
          "form": "더울때는 파란하늘도 쳐다보기도 싫더니.",
          "original_form": "더울때는 파란하늘도 쳐다보기도 싫더니.",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022127.1.14",
          "form": "파란가을하늘이 곧 오겠지",
          "original_form": "파란가을하늘이 곧 오겠지",
          "speaker_id": "1",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100022127.1.15",
          "form": "요즘 계절이 바뀌니 입을 옷이 또 없네",
          "original_form": "요즘 계절이 바뀌니 입을 옷이 또 없네",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022127.1.16",
          "form": "오늘은 흐린건지 미세먼지 농도가 높은건지 모르겠네.",
          "original_form": "오늘은 흐린건지 미세먼지 농도가 높은건지 모르겠네.",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022127.1.17",
          "form": "모처럼 비가 안와서 밖에서 운동해드리고왔어",
          "original_form": "모처럼 비가 안와서 밖에서 운동해드리고왔어",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100022127.1.18",
          "form": "태풍만 안오면 이번주는 잘 지나가겠어",
          "original_form": "태풍만 안오면 이번주는 잘 지나가겠어",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022127.1.19",
          "form": "날씨도 덥지않고 운동하기 딱 좋았네. 좋아하셨겠다.",
          "original_form": "날씨도 덥지않고 운동하기 딱 좋았네. 좋아하셨겠다.",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022127.1.20",
          "form": "이번여행에 숨겨진 관광포인트 발굴해봐.",
          "original_form": "이번여행에 숨겨진 관광포인트 발굴해봐.",
          "speaker_id": "2",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100022127.1.21",
          "form": "이번주 제주도 날씨는 목금토 반나절씩 비 예보 있다",
          "original_form": "이번주 제주도 날씨는 목금토 반나절씩 비 예보 있다",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100022127.1.22",
          "form": "온도는 대략 23도에서 30도까지네",
          "original_form": "온도는 대략 23도에서 30도까지네",
          "speaker_id": "2",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022127.1.23",
          "form": "죽전 오늘 날씨는 미세먼지 좋음이고 파란하늘 보인다",
          "original_form": "죽전 오늘 날씨는 미세먼지 좋음이고 파란하늘 보인다",
          "speaker_id": "2",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022127.1.24",
          "form": "일산도 날씨가 미세먼지없고 흐리지만 비는 안오네",
          "original_form": "일산도 날씨가 미세먼지없고 흐리지만 비는 안오네",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022127.1.25",
          "form": "나가서 걸으면 좀 더워",
          "original_form": "나가서 걸으면 좀 더워",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022127.1.26",
          "form": "걸으면 땀나긴 하지",
          "original_form": "걸으면 땀나긴 하지",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022127.1.27",
          "form": "여름이 이만큼 더웠으니 겨울은 얼마나 추울까 걱정이다",
          "original_form": "여름이 이만큼 더웠으니 겨울은 얼마나 추울까 걱정이다",
          "speaker_id": "1",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100022127.1.28",
          "form": "지난주 롯데몰 갔더니 여름옷 다 들어가고 가을옷이더라",
          "original_form": "지난주 롯데몰 갔더니 여름옷 다 들어가고 가을옷이더라",
          "speaker_id": "2",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100022127.1.29",
          "form": "곧 긴팔입어야겠지?",
          "original_form": "곧 긴팔입어야겠지?",
          "speaker_id": "1",
          "time": "20210829 21:41"
        }
      ]
