      "utterance": [
        {
          "id": "MDRW2100005088.1.1",
          "form": "봉사 좋아하세요?",
          "original_form": "봉사 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005088.1.2",
          "form": "코로나 전에는 자주 했어요",
          "original_form": "코로나 전에는 자주 했어요",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005088.1.3",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005088.1.4",
          "form": "저는 봉사활동은 고등학교 때 이후로 잘 안해봤어요 ㅎ",
          "original_form": "저는 봉사활동은 고등학교 때 이후로 잘 안해봤어요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005088.1.5",
          "form": "주로 어떤 봉사활동 하셨어요?",
          "original_form": "주로 어떤 봉사활동 하셨어요?",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005088.1.6",
          "form": "저는 장애인 복지관에서 장애인 활동보조하는 활동 했아여!",
          "original_form": "저는 장애인 복지관에서 장애인 활동보조하는 활동 했아여!",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005088.1.7",
          "form": "저도 노인 복지관에서 잠깐 했었어요",
          "original_form": "저도 노인 복지관에서 잠깐 했었어요",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005088.1.8",
          "form": "다른 것도 하신 거 있으신 가요?",
          "original_form": "다른 것도 하신 거 있으신 가요?",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005088.1.9",
          "form": "저 축제 보조 이런 것도 했었어요",
          "original_form": "저 축제 보조 이런 것도 했었어요",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005088.1.10",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005088.1.11",
          "form": "다른거는 헌혈이나 캠프 참여하는것들?",
          "original_form": "다른거는 헌혈이나 캠프 참여하는것들?",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005088.1.12",
          "form": "딱히 봉사활동 이력이 중요하지않았어서",
          "original_form": "딱히 봉사활동 이력이 중요하지않았어서",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005088.1.13",
          "form": "정말 시간만 채웠었어요 ㅋㅋ",
          "original_form": "정말 시간만 채웠었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005088.1.14",
          "form": "축제 보조 꽤 재밌었어요 요리 계속 얻어먹고ㅋㅋㅋㅋ",
          "original_form": "축제 보조 꽤 재밌었어요 요리 계속 얻어먹고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005088.1.15",
          "form": "그때 먹은 김치전이 진짜 맛있었어요",
          "original_form": "그때 먹은 김치전이 진짜 맛있었어요",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005088.1.16",
          "form": "오와",
          "original_form": "오와",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005088.1.17",
          "form": "재밌었을거같아요 ㅋㅋ",
          "original_form": "재밌었을거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005088.1.18",
          "form": "처음에는 시간 채우려고 간 거였는데",
          "original_form": "처음에는 시간 채우려고 간 거였는데",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005088.1.19",
          "form": "축제 같은 거는 4~5시간 정도라",
          "original_form": "축제 같은 거는 4~5시간 정도라",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005088.1.20",
          "form": "꽤 재밌게 하고 왔어요",
          "original_form": "꽤 재밌게 하고 왔어요",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005088.1.21",
          "form": "학교 자체에서 하는 봉사활동은 주로 뭐하셨어요?",
          "original_form": "학교 자체에서 하는 봉사활동은 주로 뭐하셨어요?",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005088.1.22",
          "form": "5시간 정도는 하교에서 채워줬던 거 같아서요",
          "original_form": "5시간 정도는 하교에서 채워줬던 거 같아서요",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005088.1.23",
          "form": "저는 학교에서 봉사활동을 거의 안채워줬어서",
          "original_form": "저는 학교에서 봉사활동을 거의 안채워줬어서",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005088.1.24",
          "form": "그래봐야 헌혈버스와서 헌혈하는거?",
          "original_form": "그래봐야 헌혈버스와서 헌혈하는거?",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005088.1.25",
          "form": "그외엔 없었던것같아요ㅜ",
          "original_form": "그외엔 없었던것같아요ㅜ",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005088.1.26",
          "form": "아 헌혈 버스도 왔었죠",
          "original_form": "아 헌혈 버스도 왔었죠",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005088.1.27",
          "form": "저는 빈혈이 좀 있어서 못 했는데",
          "original_form": "저는 빈혈이 좀 있어서 못 했는데",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005088.1.28",
          "form": "헌혈 자주 하시나요?",
          "original_form": "헌혈 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005088.1.29",
