      "utterance": [
        {
          "id": "MDRW2100032060.1.1",
          "form": "오늘도 헬스가세요? 날씨.어뚜세오?",
          "original_form": "오늘도 헬스가세요? 날씨.어뚜세오?",
          "speaker_id": "1",
          "time": "20210905 11:41"
        },
        {
          "id": "MDRW2100032060.1.2",
          "form": "오늘은 쉬는날이라서 집에 있는데",
          "original_form": "오늘은 쉬는날이라서 집에 있는데",
          "speaker_id": "2",
          "time": "20210905 14:00"
        },
        {
          "id": "MDRW2100032060.1.3",
          "form": "많이 덥지는 않아보이네용",
          "original_form": "많이 덥지는 않아보이네용",
          "speaker_id": "2",
          "time": "20210905 14:00"
        },
        {
          "id": "MDRW2100032060.1.4",
          "form": "거기는 어떤가요?",
          "original_form": "거기는 어떤가요?",
          "speaker_id": "2",
          "time": "20210905 14:00"
        },
        {
          "id": "MDRW2100032060.1.5",
          "form": "네네 저희도 시원하규 선선",
          "original_form": "네네 저희도 시원하규 선선",
          "speaker_id": "1",
          "time": "20210905 14:23"
        },
        {
          "id": "MDRW2100032060.1.6",
          "form": "해는 안났어요ㅠㅠ",
          "original_form": "해는 안났어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 14:24"
        },
        {
          "id": "MDRW2100032060.1.7",
          "form": "그런 날이 좋은거 같아요~~ㅎㅎ",
          "original_form": "그런 날이 좋은거 같아요~~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032060.1.8",
          "form": "해 나는 날이 좋으신가요?",
          "original_form": "해 나는 날이 좋으신가요?",
          "speaker_id": "2",
          "time": "20210905 14:56"
        },
        {
          "id": "MDRW2100032060.1.9",
          "form": "전 좀 흐린날이 더 좋아요 햇볕 싫어해요 ㅋㅋ",
          "original_form": "전 좀 흐린날이 더 좋아요 햇볕 싫어해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032060.1.10",
          "form": "저두요 덥기만하지ㅜㅠㅜㅜ",
          "original_form": "저두요 덥기만하지ㅜㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100032060.1.11",
          "form": "비도 조금씩 오네여",
          "original_form": "비도 조금씩 오네여",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032060.1.12",
          "form": "오늘요??",
          "original_form": "오늘요??",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032060.1.13",
          "form": "저 있는 곳은 안왔어요",
          "original_form": "저 있는 곳은 안왔어요",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032060.1.14",
          "form": "조금 흐리고",
          "original_form": "조금 흐리고",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032060.1.15",
          "form": "저희도 왔다가 안왔다가",
          "original_form": "저희도 왔다가 안왔다가",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032060.1.16",
          "form": "해 도 나고 쾌청하면 좋겠어요",
          "original_form": "해 도 나고 쾌청하면 좋겠어요",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100032060.1.17",
          "form": "그죠 지금 밖에 선선해서 너무 좋아요~!!",
          "original_form": "그죠 지금 밖에 선선해서 너무 좋아요~!!",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100032060.1.18",
          "form": "밤에 딱 좋네요",
          "original_form": "밤에 딱 좋네요",
