      "utterance": [
        {
          "id": "MDRW2100006692.1.1",
          "form": "전 시리얼이라도 먹으려고요",
          "original_form": "전 시리얼이라도 먹으려고요",
          "speaker_id": "2",
          "time": "20210908 06:50"
        },
        {
          "id": "MDRW2100006692.1.2",
          "form": "아 시리얼 좋네요",
          "original_form": "아 시리얼 좋네요",
          "speaker_id": "1",
          "time": "20210908 06:50"
        },
        {
          "id": "MDRW2100006692.1.3",
          "form": "인터넷으로 시키면 저렴하더라고요",
          "original_form": "인터넷으로 시키면 저렴하더라고요",
          "speaker_id": "2",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100006692.1.4",
          "form": "그냥 무념무상으로 먹습니다",
          "original_form": "그냥 무념무상으로 먹습니다",
          "speaker_id": "2",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100006692.1.5",
          "form": "그렇구나 저도 시리얼은 좋아해요",
          "original_form": "그렇구나 저도 시리얼은 좋아해요",
          "speaker_id": "1",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100006692.1.6",
          "form": "숙직이라 밤샜는데 입맛없어서 뭐 챙겨먹기는 싫은데",
          "original_form": "숙직이라 밤샜는데 입맛없어서 뭐 챙겨먹기는 싫은데",
          "speaker_id": "1",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100006692.1.7",
          "form": "일단 우유랑 먹는 건 다 맛있는 거 같아요",
          "original_form": "일단 우유랑 먹는 건 다 맛있는 거 같아요",
          "speaker_id": "2",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100006692.1.8",
          "form": "아 숙직이셨군요",
          "original_form": "아 숙직이셨군요",
          "speaker_id": "2",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100006692.1.9",
          "form": "시리얼은 편하게 차려먹기 좋네요",
          "original_form": "시리얼은 편하게 차려먹기 좋네요",
          "speaker_id": "1",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100006692.1.10",
          "form": "네 빨리 퇴근하고 싶네요",
          "original_form": "네 빨리 퇴근하고 싶네요",
          "speaker_id": "1",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100006692.1.11",
          "form": "배달음식은 자주 드시나요",
          "original_form": "배달음식은 자주 드시나요",
          "speaker_id": "1",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100006692.1.12",
          "form": "네 떡볶이 자주 시킵니다",
          "original_form": "네 떡볶이 자주 시킵니다",
          "speaker_id": "2",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100006692.1.13",
          "form": "저는 치킨",
          "original_form": "저는 치킨",
          "speaker_id": "1",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100006692.1.14",
          "form": "떡볶이 정말 좋아하시나보내요",
          "original_form": "떡볶이 정말 좋아하시나보내요",
          "speaker_id": "1",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100006692.1.15",
          "form": "오 저는 지코바 자주 먹어요",
          "original_form": "오 저는 지코바 자주 먹어요",
          "speaker_id": "2",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100006692.1.16",
          "form": "첫질문이 떡볶이였는데ㅎㅎ",
          "original_form": "첫질문이 떡볶이였는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100006692.1.17",
          "form": "이게 어플이 느려서요",
          "original_form": "이게 어플이 느려서요",
          "speaker_id": "2",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100006692.1.18",
          "form": "첫 줄만 먼저 떴네요ㅎㅎ",
          "original_form": "첫 줄만 먼저 떴네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100006692.1.19",
          "form": "그런데 배달음식 자주 먹으니 살이쪄서",
          "original_form": "그런데 배달음식 자주 먹으니 살이쪄서",
          "speaker_id": "1",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100006692.1.20",
          "form": "요즘은 샐러드먹고 식단 조절하고 있어요",
          "original_form": "요즘은 샐러드먹고 식단 조절하고 있어요",
          "speaker_id": "1",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100006692.1.21",
          "form": "맛있는거 원없이 먹고싶은데 어렵네요",
          "original_form": "맛있는거 원없이 먹고싶은데 어렵네요",
          "speaker_id": "1",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100006692.1.22",
          "form": "에이 건강이 최고죠",
          "original_form": "에이 건강이 최고죠",
          "speaker_id": "2",
          "time": "20210908 07:00"
        },
        {
          "id": "MDRW2100006692.1.23",
          "form": "바깥음식은 확실히 짜긴해요",
          "original_form": "바깥음식은 확실히 짜긴해요",
          "speaker_id": "2",
          "time": "20210908 07:00"
        }
