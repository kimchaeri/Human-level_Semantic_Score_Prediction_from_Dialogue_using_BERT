      "utterance": [
        {
          "id": "MDRW2100014183.1.1",
          "form": "너 항거 유관순 이야기 영화 봤어?",
          "original_form": "너 항거 유관순 이야기 영화 봤어?",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014183.1.2",
          "form": "아니 오 그거보려구?",
          "original_form": "아니 오 그거보려구?",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014183.1.3",
          "form": "저번에도 대여로 받아뒀다 못봤었는데\n지금도 대여 만료일 다돼감",
          "original_form": "저번에도 대여로 받아뒀다 못봤었는데\n지금도 대여 만료일 다돼감",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014183.1.4",
          "form": "봤으면 어떤가 물어 볼랬어ㅎ",
          "original_form": "봤으면 어떤가 물어 볼랬어ㅎ",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014183.1.5",
          "form": "나 그런 현실적인거 보기힘들더리ㅣ",
          "original_form": "나 그런 현실적인거 보기힘들더리ㅣ",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014183.1.6",
          "form": "항일영화 다른건 잘 보긴했는데",
          "original_form": "항일영화 다른건 잘 보긴했는데",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014183.1.7",
          "form": "저건 넘 맘이 아플듯",
          "original_form": "저건 넘 맘이 아플듯",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014183.1.8",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014183.1.9",
          "form": "나도 그래서 긍가",
          "original_form": "나도 그래서 긍가",
          "speaker_id": "2",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014183.1.10",
          "form": "암살은 잘 봤는데\n2번봄ㄱㅎ",
          "original_form": "암살은 잘 봤는데\n2번봄ㄱㅎ",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014183.1.11",
          "form": "오 암살 난 그거 티비에서 봤당",
          "original_form": "오 암살 난 그거 티비에서 봤당",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014183.1.12",
          "form": "티비에서 봐도 재밌드라",
          "original_form": "티비에서 봐도 재밌드라",
          "speaker_id": "1",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014183.1.13",
          "form": "티비서도 해줬구나",
          "original_form": "티비서도 해줬구나",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014183.1.14",
          "form": "요즘 영화관 가나?",
          "original_form": "요즘 영화관 가나?",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014183.1.15",
          "form": "ㅇㅇ나 모가디슈 보고옴!!",
          "original_form": "ㅇㅇ나 모가디슈 보고옴!!",
          "speaker_id": "1",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014183.1.16",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014183.1.17",
          "form": "재밌더나",
          "original_form": "재밌더나",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014183.1.18",
          "form": "어떤 내용인데?",
          "original_form": "어떤 내용인데?",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014183.1.19",
          "form": "지금아프간사태랑 비슷해 소말리아 모가디슈에 갑자기 내전이 터져서 거기를 남북이 연합해서 탈출한다는 내용",
          "original_form": "지금아프간사태랑 비슷해 소말리아 모가디슈에 갑자기 내전이 터져서 거기를 남북이 연합해서 탈출한다는 내용",
          "speaker_id": "1",
          "time": "20210825 19:30"
        },
        {
          "id": "MDRW2100014183.1.20",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014183.1.21",
          "form": "지금 아프간사태때문에 보고싶어진다",
          "original_form": "지금 아프간사태때문에 보고싶어진다",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014183.1.22",
          "form": "ㅇㅇ진짜 재밌어 특히 액션씬 최고야",
          "original_form": "ㅇㅇ진짜 재밌어 특히 액션씬 최고야",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014183.1.23",
          "form": "볼수있음 봐야겠네",
          "original_form": "볼수있음 봐야겠네",
          "speaker_id": "2",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014183.1.24",
          "form": "ㅇㅇ 저번에 영화 이벤트해서 삼천원 주고 봤음",
          "original_form": "ㅇㅇ 저번에 영화 이벤트해서 삼천원 주고 봤음",
          "speaker_id": "1",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014183.1.25",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014183.1.26",
          "form": "들었던듯도하네",
          "original_form": "들었던듯도하네",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014183.1.27",
          "form": "좋앟겠다ㅎ",
          "original_form": "좋앟겠다ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014183.1.28",
          "form": "ㅇㅇ 재밌엉 근데 이건 확실히 영화관가서 봐야겠드라",
          "original_form": "ㅇㅇ 재밌엉 근데 이건 확실히 영화관가서 봐야겠드라",
          "speaker_id": "1",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014183.1.29",
          "form": "액션이라서?",
          "original_form": "액션이라서?",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014183.1.30",
          "form": "ㅇㅇ그리고 스케일도 크거든",
          "original_form": "ㅇㅇ그리고 스케일도 크거든",
          "speaker_id": "1",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014183.1.31",
          "form": "돈냄새 나기도 하고",
          "original_form": "돈냄새 나기도 하고",
          "speaker_id": "1",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014183.1.32",
          "form": "엇 지금은 내렸을거같은데",
          "original_form": "엇 지금은 내렸을거같은데",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014183.1.33",
          "form": "아직 하더라 지금 상영순위 3위더라구",
          "original_form": "아직 하더라 지금 상영순위 3위더라구",
          "speaker_id": "1",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014183.1.34",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014183.1.35",
          "form": "요즘 영화비 비싸던데",
          "original_form": "요즘 영화비 비싸던데",
          "speaker_id": "2",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014183.1.36",
          "form": "저렴하게볼수있나 알아봐야겠당",
          "original_form": "저렴하게볼수있나 알아봐야겠당",
          "speaker_id": "2",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014183.1.37",
