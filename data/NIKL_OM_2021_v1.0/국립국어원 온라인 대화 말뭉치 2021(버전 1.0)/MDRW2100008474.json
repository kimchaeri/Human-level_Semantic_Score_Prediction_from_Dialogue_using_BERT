      "utterance": [
        {
          "id": "MDRW2100008474.1.1",
          "form": "요즘 코로나 시국이라 너무 힘들다",
          "original_form": "요즘 코로나 시국이라 너무 힘들다",
          "speaker_id": "2",
          "time": "20210822 16:04"
        },
        {
          "id": "MDRW2100008474.1.2",
          "form": "맞아ㅠㅠㅠ",
          "original_form": "맞아ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008474.1.3",
          "form": "너 백신 맞았어?",
          "original_form": "너 백신 맞았어?",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008474.1.4",
          "form": "나는 예약만했어",
          "original_form": "나는 예약만했어",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008474.1.5",
          "form": "나도 예약만했어",
          "original_form": "나도 예약만했어",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008474.1.6",
          "form": "백신 후기보고",
          "original_form": "백신 후기보고",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008474.1.7",
          "form": "오돌오돌 떠는 중",
          "original_form": "오돌오돌 떠는 중",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008474.1.8",
          "form": "그래도 백신 맞아야지ㅠㅠ 후기 조금 무섭긴해",
          "original_form": "그래도 백신 맞아야지ㅠㅠ 후기 조금 무섭긴해",
          "speaker_id": "2",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008474.1.9",
          "form": "타이레놀 무조건 사놔야해ㅋㅋㅋㅋㅋ",
          "original_form": "타이레놀 무조건 사놔야해ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008474.1.10",
          "form": "맞아맞아 나도 집에 쟁여놨여",
          "original_form": "맞아맞아 나도 집에 쟁여놨여",
          "speaker_id": "2",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008474.1.11",
          "form": "코로나만큼 무서운 주사ㅠ",
          "original_form": "코로나만큼 무서운 주사ㅠ",
          "speaker_id": "1",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100008474.1.12",
          "form": "맞아..하지만 백신을 안맞을순 없으니 참구 맞아야지..ㅠㅠ!!",
          "original_form": "맞아..하지만 백신을 안맞을순 없으니 참구 맞아야지..ㅠㅠ!!",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008474.1.13",
          "form": "백신도 백신인데",
          "original_form": "백신도 백신인데",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008474.1.14",
          "form": "밖에 못 나가니까",
          "original_form": "밖에 못 나가니까",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008474.1.15",
          "form": "비타민D 떨어지는 느낌ㅠㅋㅋㅋ",
          "original_form": "비타민D 떨어지는 느낌ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008474.1.16",
          "form": "ㅠㅠ 나도 알바 갈때만 걷는것 같아..",
          "original_form": "ㅠㅠ 나도 알바 갈때만 걷는것 같아..",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008474.1.17",
          "form": "사람도 광합성이 필요해",
          "original_form": "사람도 광합성이 필요해",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008474.1.18",
          "form": "근데 진짜 코로나 따문에 마스크 쓰고 다니다보니",
          "original_form": "근데 진짜 코로나 따문에 마스크 쓰고 다니다보니",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008474.1.19",
          "form": "다른 잔병치레가 안걸리는건 좀 신기하더라",
          "original_form": "다른 잔병치레가 안걸리는건 좀 신기하더라",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008474.1.20",
          "form": "맞아 뭔가 걸려도 가볍게 지나가는 느낌?",
          "original_form": "맞아 뭔가 걸려도 가볍게 지나가는 느낌?",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008474.1.21",
          "form": "마스크의 중요성",
          "original_form": "마스크의 중요성",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008474.1.22",
          "form": "손도 자주 씻다보니 진짜 감기는 안걸리더라",
          "original_form": "손도 자주 씻다보니 진짜 감기는 안걸리더라",
          "speaker_id": "2",
          "time": "20210822 16:15"
        }
      ]
