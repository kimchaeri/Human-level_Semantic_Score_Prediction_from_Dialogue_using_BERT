      "utterance": [
        {
          "id": "MDRW2100017902.1.1",
          "form": "가고싶은 여행지 있으세요?",
          "original_form": "가고싶은 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017902.1.2",
          "form": "해외는 일본가구싶고",
          "original_form": "해외는 일본가구싶고",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017902.1.3",
          "form": "국내라면 제주도 다시 한번 가보고 싶네요 ㅎㅎ",
          "original_form": "국내라면 제주도 다시 한번 가보고 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017902.1.4",
          "form": "와 일본 굿굿",
          "original_form": "와 일본 굿굿",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017902.1.5",
          "form": "제주도 굿굿",
          "original_form": "제주도 굿굿",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017902.1.6",
          "form": "일본 가신 적 있으세요?",
          "original_form": "일본 가신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017902.1.7",
          "form": "일본은 못가봤어용 ㅠㅠ",
          "original_form": "일본은 못가봤어용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017902.1.8",
          "form": "계획은 했는데 하필 코로나..",
          "original_form": "계획은 했는데 하필 코로나..",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017902.1.9",
          "form": "아아 ㅠㅠ",
          "original_form": "아아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017902.1.10",
          "form": "코로나 ㅠㅠ",
          "original_form": "코로나 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017902.1.11",
          "form": "전 딱 한번 가봤어요",
          "original_form": "전 딱 한번 가봤어요",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017902.1.12",
          "form": "라면이 맛있었어요 ㅠㅠ",
          "original_form": "라면이 맛있었어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017902.1.13",
          "form": "와.. 부럽네용 ㅠㅠ",
          "original_form": "와.. 부럽네용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017902.1.14",
          "form": "출장이라 관광지는 따로 못갔어요 ㅎㅎ",
          "original_form": "출장이라 관광지는 따로 못갔어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017902.1.15",
          "form": "나중에 또 가려구요",
          "original_form": "나중에 또 가려구요",
          "speaker_id": "1",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017902.1.16",
          "form": "나중에 코로나 종식되거나 많이 줄어들면",
          "original_form": "나중에 코로나 종식되거나 많이 줄어들면",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017902.1.17",
          "form": "가볼려구용 저도",
          "original_form": "가볼려구용 저도",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017902.1.18",
          "form": "그때되면 저도!",
          "original_form": "그때되면 저도!",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017902.1.19",
          "form": "기다려봐요 얍얍",
          "original_form": "기다려봐요 얍얍",
          "speaker_id": "1",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017902.1.20",
          "form": "우선 가게 되묜",
          "original_form": "우선 가게 되묜",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017902.1.21",
          "form": "타코야끼를 제일 먼저..",
          "original_form": "타코야끼를 제일 먼저..",
          "speaker_id": "2",
          "time": "20210826 21:38"
        },
        {
          "id": "MDRW2100017902.1.22",
          "form": "하하 먹는건 진리!",
          "original_form": "하하 먹는건 진리!",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017902.1.23",
          "form": "한국에서도 자주 드세요?",
          "original_form": "한국에서도 자주 드세요?",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017902.1.24",
          "form": "현지 맛이 나려나...",
          "original_form": "현지 맛이 나려나...",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017902.1.25",
          "form": "넹 엄청 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "넹 엄청 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017902.1.26",
          "form": "현지맛인진 잘모르겠는데 엄청 맛있더라구요",
          "original_form": "현지맛인진 잘모르겠는데 엄청 맛있더라구요",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017902.1.27",
          "form": "ㅎㅎ 오 입맛에 맞아 다행이에요",
          "original_form": "ㅎㅎ 오 입맛에 맞아 다행이에요",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017902.1.28",
          "form": "name2님 저 눈이 넘 피곤해서",
          "original_form": "&name2&님 저 눈이 넘 피곤해서",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017902.1.29",
          "form": "이만 가볼게요~ 대화 감사했어요^^",
          "original_form": "이만 가볼게요~ 대화 감사했어요^^",
