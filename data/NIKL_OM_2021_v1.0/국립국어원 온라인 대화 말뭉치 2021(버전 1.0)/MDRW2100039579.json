        },
        {
          "id": "MDRW2100039579.1.2",
          "form": "네 두개요ㅎ",
          "original_form": "네 두개요ㅎ",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039579.1.3",
          "form": "오 좋은거요? ㅎㅎ",
          "original_form": "오 좋은거요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039579.1.4",
          "form": "베개랑 먹을거랑 강아지 사료랑.ㅎㅎ",
          "original_form": "베개랑 먹을거랑 강아지 사료랑.ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100039579.1.5",
          "form": "오ㅏㅎㅎ 강아지도 ㅋㅣ우시나봐요 저흰 아이 하나만 잘 키우기로 ㅎㅎ",
          "original_form": "오ㅏㅎㅎ 강아지도 ㅋㅣ우시나봐요 저흰 아이 하나만 잘 키우기로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:16"
        },
        {
          "id": "MDRW2100039579.1.6",
          "form": "아이 하나도 장난아니지요ㅎ",
          "original_form": "아이 하나도 장난아니지요ㅎ",
          "speaker_id": "1",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100039579.1.7",
          "form": "멘탈이 흔들려요ㅠ",
          "original_form": "멘탈이 흔들려요ㅠ",
          "speaker_id": "1",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100039579.1.8",
          "form": "맞아요 남편도 큰 아이같아서 ㅋㅋ 늘 달래고 뭐 사주며 키워요",
          "original_form": "맞아요 남편도 큰 아이같아서 ㅋㅋ 늘 달래고 뭐 사주며 키워요",
          "speaker_id": "2",
          "time": "20210904 15:42"
        },
        {
          "id": "MDRW2100039579.1.9",
          "form": "ㅋㅋㅋ 큰아들이네요",
          "original_form": "ㅋㅋㅋ 큰아들이네요",
          "speaker_id": "1",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100039579.1.10",
          "form": "그쵸 맞아요 늘 달래야 ㅋㅋ ㅠㅠ",
          "original_form": "그쵸 맞아요 늘 달래야 ㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100039579.1.11",
          "form": "육아 어렵지 않으세요?",
          "original_form": "육아 어렵지 않으세요?",
          "speaker_id": "1",
          "time": "20210904 19:21"
        },
        {
          "id": "MDRW2100039579.1.12",
          "form": "큰아들이 힘들지 작은 아들은 말도 잘 들어서 ㅎㅎ",
          "original_form": "큰아들이 힘들지 작은 아들은 말도 잘 들어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 19:30"
        },
        {
          "id": "MDRW2100039579.1.13",
          "form": "우와 부러워요",
          "original_form": "우와 부러워요",
          "speaker_id": "1",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100039579.1.14",
          "form": "전 아들이랑 맨날 싸워요ㅠ",
          "original_form": "전 아들이랑 맨날 싸워요ㅠ",
