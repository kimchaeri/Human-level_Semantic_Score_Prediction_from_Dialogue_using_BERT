      "utterance": [
        {
          "id": "MDRW2100039579.1.1",
          "form": "오늘도 택배 왔나요 ㅎㅎ",
          "original_form": "오늘도 택배 왔나요 ㅎㅎ",
          "speaker_id": "2",
