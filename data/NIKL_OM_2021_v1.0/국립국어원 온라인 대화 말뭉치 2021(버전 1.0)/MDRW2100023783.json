      "utterance": [
        {
          "id": "MDRW2100023783.1.1",
          "form": "뜨거워요",
          "original_form": "뜨거워요",
          "speaker_id": "2",
          "time": "20210830 17:29"
        },
        {
          "id": "MDRW2100023783.1.2",
          "form": "그치만 맛있네요",
          "original_form": "그치만 맛있네요",
          "speaker_id": "2",
          "time": "20210830 17:29"
        },
        {
          "id": "MDRW2100023783.1.3",
          "form": "만두이즈뭔들",
          "original_form": "만두이즈뭔들",
          "speaker_id": "2",
          "time": "20210830 17:29"
        },
        {
          "id": "MDRW2100023783.1.4",
          "form": "백종원은 만두도 잘 만드네요,,",
          "original_form": "백종원은 만두도 잘 만드네요,,",
          "speaker_id": "1",
          "time": "20210830 17:31"
        },
        {
          "id": "MDRW2100023783.1.5",
          "form": "저 빽다방에서 옥수크림 좋아했는데,,",
          "original_form": "저 빽다방에서 옥수크림 좋아했는데,,",
          "speaker_id": "1",
          "time": "20210830 17:31"
        },
        {
          "id": "MDRW2100023783.1.6",
          "form": "글쿤요",
          "original_form": "글쿤요",
          "speaker_id": "2",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023783.1.7",
          "form": "옥수수 + 아이스크림",
          "original_form": "옥수수 + 아이스크림",
          "speaker_id": "1",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023783.1.8",
          "form": "거긴 가성비가 좋죠",
          "original_form": "거긴 가성비가 좋죠",
          "speaker_id": "2",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023783.1.9",
          "form": "바닐라 아이스크림에",
          "original_form": "바닐라 아이스크림에",
          "speaker_id": "1",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023783.1.10",
          "form": "스위트콘 옥수수",
          "original_form": "스위트콘 옥수수",
          "speaker_id": "1",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023783.1.11",
          "form": "와 존맛이겠는데!",
          "original_form": "와 존맛이겠는데!",
          "speaker_id": "2",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100023783.1.12",
          "form": "단짠단짠의 조화예요~!",
          "original_form": "단짠단짠의 조화예요~!",
          "speaker_id": "1",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100023783.1.13",
          "form": "아직도 파나...?",
          "original_form": "아직도 파나...?",
          "speaker_id": "1",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100023783.1.14",
          "form": "팔았으면 좋겠네요",
          "original_form": "팔았으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100023783.1.15",
          "form": "오늘 가는길에 사먹을까...? ㅋㅋ",
          "original_form": "오늘 가는길에 사먹을까...? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100023783.1.16",
          "form": "가격도 괜찮았어요,, 천원대였나??",
          "original_form": "가격도 괜찮았어요,, 천원대였나??",
          "speaker_id": "1",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100023783.1.17",
          "form": "네네네네네",
          "original_form": "네네네네네",
          "speaker_id": "1",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100023783.1.18",
          "form": "진짜 먹어보세요,,, 제 소울푸드 중 하나예요",
          "original_form": "진짜 먹어보세요,,, 제 소울푸드 중 하나예요",
          "speaker_id": "1",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100023783.1.19",
          "form": "먹을거에요",
          "original_form": "먹을거에요",
          "speaker_id": "2",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100023783.1.20",
          "form": "그냥 옥수수는 별로지만 스위트콘은 사랑해요",
          "original_form": "그냥 옥수수는 별로지만 스위트콘은 사랑해요",
          "speaker_id": "2",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100023783.1.21",
          "form": "꺄~",
          "original_form": "꺄~",
          "speaker_id": "1",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100023783.1.22",
          "form": "옥수크림 영업 성공!!!!",
          "original_form": "옥수크림 영업 성공!!!!",
          "speaker_id": "1",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100023783.1.23",
          "form": "가격도 2500원 나쁘지 않아요!",
          "original_form": "가격도 2500원 나쁘지 않아요!",
          "speaker_id": "1",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100023783.1.24",
          "form": "거기도 요즘 9시에 닫나?",
          "original_form": "거기도 요즘 9시에 닫나?",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023783.1.25",
          "form": "그럼 오늘 못먹을지도 ㅠ",
          "original_form": "그럼 오늘 못먹을지도 ㅠ",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023783.1.26",
          "form": "찾아볼게욥!",
          "original_form": "찾아볼게욥!",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023783.1.27",
          "form": "동네마다 다른데",
          "original_form": "동네마다 다른데",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023783.1.28",
          "form": "밤 11시에 닫는 데도 있고",
          "original_form": "밤 11시에 닫는 데도 있고",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023783.1.29",
          "form": "저녁 8시에 닫는 데도 있고 그러네요???",
          "original_form": "저녁 8시에 닫는 데도 있고 그러네요???",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023783.1.30",
          "form": "그래요? 우리 동네 제가 찾아볼게요",
          "original_form": "그래요? 우리 동네 제가 찾아볼게요",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023783.1.31",
          "form": "네네~!",
          "original_form": "네네~!",
          "speaker_id": "1",
          "time": "20210830 17:47"
        }
      ]
    }
  ]
