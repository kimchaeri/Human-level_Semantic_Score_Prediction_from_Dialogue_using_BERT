      "utterance": [
        {
          "id": "MDRW2100022610.1.1",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210829 16:21"
        },
        {
          "id": "MDRW2100022610.1.2",
          "form": "저는 치킨이나 전을 좋아해요.",
          "original_form": "저는 치킨이나 전을 좋아해요.",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022610.1.3",
          "form": "날마다 다르긴 한데",
          "original_form": "날마다 다르긴 한데",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022610.1.4",
          "form": "오늘같이 비가 오는 날에는 파전이 생각나네요",
          "original_form": "오늘같이 비가 오는 날에는 파전이 생각나네요",
          "speaker_id": "2",
          "time": "20210829 16:22"
        },
        {
          "id": "MDRW2100022610.1.5",
          "form": "지금 비가 내리나요?",
          "original_form": "지금 비가 내리나요?",
          "speaker_id": "1",
          "time": "20210829 16:23"
        },
        {
          "id": "MDRW2100022610.1.6",
          "form": "여긴 대구인데 비가 왔다가 안왔다가",
          "original_form": "여긴 대구인데 비가 왔다가 안왔다가",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022610.1.7",
          "form": "엄청 반복하네요..",
          "original_form": "엄청 반복하네요..",
          "speaker_id": "2",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022610.1.8",
          "form": "여긴 해가 떠서요. 요즘 날씨가 참 변덕스럽죠..",
          "original_form": "여긴 해가 떠서요. 요즘 날씨가 참 변덕스럽죠..",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022610.1.9",
          "form": "저도 막걸리에 파전 좋아하는데",
          "original_form": "저도 막걸리에 파전 좋아하는데",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022610.1.10",
          "form": "비오는 날에 파전 먹고 싶네요",
          "original_form": "비오는 날에 파전 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022610.1.11",
          "form": "오징어 들어있는 해물파전이면 더 좋구요",
          "original_form": "오징어 들어있는 해물파전이면 더 좋구요",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100022610.1.12",
          "form": "해물파전에 막걸리 최고지요!!",
          "original_form": "해물파전에 막걸리 최고지요!!",
          "speaker_id": "2",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022610.1.13",
          "form": "요즘에 배달이 잘 되어 있어서",
          "original_form": "요즘에 배달이 잘 되어 있어서",
          "speaker_id": "2",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022610.1.14",
          "form": "먹고싶은거 다 시켜 먹을 수 있어서 너무 좋아요ㅎㅎ",
          "original_form": "먹고싶은거 다 시켜 먹을 수 있어서 너무 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022610.1.15",
          "form": "오늘 점심은 뭐 드셨나요?",
          "original_form": "오늘 점심은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022610.1.16",
          "form": "파전도 배달되나요?",
          "original_form": "파전도 배달되나요?",
          "speaker_id": "1",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022610.1.17",
          "form": "전 점심에 닭갈비 먹었습니다",
          "original_form": "전 점심에 닭갈비 먹었습니다",
          "speaker_id": "1",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100022610.1.18",
          "form": "춘천에서 배송해서 먹거든요",
          "original_form": "춘천에서 배송해서 먹거든요",
          "speaker_id": "1",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100022610.1.19",
          "form": "와 춘천에서 닭갈비 배송시켜 먹는다니..",
          "original_form": "와 춘천에서 닭갈비 배송시켜 먹는다니..",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022610.1.20",
          "form": "춘천 닭갈비 들어만 봤지 저도 먹어보고 싶네요",
          "original_form": "춘천 닭갈비 들어만 봤지 저도 먹어보고 싶네요",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022610.1.21",
          "form": "네 요즘은 온라인, 전화 주문 다 되어서 직접 안가도 맛있게 먹을 수 있더라고요",
          "original_form": "네 요즘은 온라인, 전화 주문 다 되어서 직접 안가도 맛있게 먹을 수 있더라고요",
          "speaker_id": "1",
          "time": "20210829 16:31"
        },
        {
          "id": "MDRW2100022610.1.22",
          "form": "정말 엄청 편해졌어요ㅎㅎ",
          "original_form": "정말 엄청 편해졌어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022610.1.23",
          "form": "전 항상 밥을 먹으면 후식을 먹는데",
          "original_form": "전 항상 밥을 먹으면 후식을 먹는데",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022610.1.24",
          "form": "예전에 배달안되던 것들도 집에서 척척 받아볼 수 있으니 편하네요",
          "original_form": "예전에 배달안되던 것들도 집에서 척척 받아볼 수 있으니 편하네요",
          "speaker_id": "1",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022610.1.25",
          "form": "이젠 배달을 안하면 살아남기 힘든 세상인거 같아요",
          "original_form": "이젠 배달을 안하면 살아남기 힘든 세상인거 같아요",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022610.1.26",
          "form": "삼겹살까지 구워서 배달하고",
          "original_form": "삼겹살까지 구워서 배달하고",
          "speaker_id": "2",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022610.1.27",
          "form": "배달 앱을 열면 항상 뭐먹을지 엄청 고민이에요ㅎㅅ",
          "original_form": "배달 앱을 열면 항상 뭐먹을지 엄청 고민이에요ㅎㅅ",
          "speaker_id": "2",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022610.1.28",
          "form": "맞아요~ 배달앱을 지우던지 해야지 오늘도 누르게 생겼네요 ㅎㅎ",
          "original_form": "맞아요~ 배달앱을 지우던지 해야지 오늘도 누르게 생겼네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:34"
        }
      ]
