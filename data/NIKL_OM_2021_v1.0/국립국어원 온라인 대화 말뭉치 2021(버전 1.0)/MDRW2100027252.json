      "utterance": [
        {
          "id": "MDRW2100027252.1.1",
          "form": "아 튼튼하시군요",
          "original_form": "아 튼튼하시군요",
          "speaker_id": "2",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100027252.1.2",
          "form": "헬스나 이런거 하세요?",
          "original_form": "헬스나 이런거 하세요?",
          "speaker_id": "2",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027252.1.3",
          "form": "튼튼해야 잘 먹쥬",
          "original_form": "튼튼해야 잘 먹쥬",
          "speaker_id": "1",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027252.1.4",
          "form": "ㅎㅎㅎ 평생 운동했습니다 숨쉬기를",
          "original_form": "ㅎㅎㅎ 평생 운동했습니다 숨쉬기를",
          "speaker_id": "1",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027252.1.5",
          "form": "코가 건강하시네요",
          "original_form": "코가 건강하시네요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027252.1.6",
          "form": "숨잘쉬는것도 복입니다",
          "original_form": "숨잘쉬는것도 복입니다",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027252.1.7",
          "form": "저는 비염이 약간있어서 살짝씩 코가 막힘니다",
          "original_form": "저는 비염이 약간있어서 살짝씩 코가 막힘니다",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027252.1.8",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027252.1.9",
          "form": "아앗 비염 저도 있어요",
          "original_form": "아앗 비염 저도 있어요",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027252.1.10",
          "form": "지금 코가 막혔나요?",
          "original_form": "지금 코가 막혔나요?",
          "speaker_id": "2",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027252.1.11",
          "form": "근데 너무 한평생 같이 살아서 자각못하고 살아요",
          "original_form": "근데 너무 한평생 같이 살아서 자각못하고 살아요",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027252.1.12",
          "form": "지금은 아뇨!",
          "original_form": "지금은 아뇨!",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027252.1.13",
          "form": "마스크 쓴 후로 덜 막히는듯?",
          "original_form": "마스크 쓴 후로 덜 막히는듯?",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027252.1.14",
          "form": "님은 건강체질인가요?",
          "original_form": "님은 건강체질인가요?",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100027252.1.15",
          "form": "음 건강하다고 착각하며 살았던것 같습니다",
          "original_form": "음 건강하다고 착각하며 살았던것 같습니다",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027252.1.16",
          "form": "주1회 축구하고",
          "original_form": "주1회 축구하고",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027252.1.17",
          "form": "수영하고 해서",
          "original_form": "수영하고 해서",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027252.1.18",
          "form": "괜찮았는데",
          "original_form": "괜찮았는데",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027252.1.19",
          "form": "아웃도어형이시군요ㄷㄷㄷ",
          "original_form": "아웃도어형이시군요ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027252.1.20",
          "form": "몸이 하나씩 아파요",
          "original_form": "몸이 하나씩 아파요",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027252.1.21",
          "form": "어릴때는",
          "original_form": "어릴때는",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027252.1.22",
          "form": "ㅜㅇㅜ 비염 말고도요?",
          "original_form": "ㅜㅇㅜ 비염 말고도요?",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027252.1.23",
          "form": "몸으로는 1등이엇는데",
          "original_form": "몸으로는 1등이엇는데",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027252.1.24",
          "form": "비염 살짝?ㅎㅎ",
          "original_form": "비염 살짝?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027252.1.25",
          "form": "아니ㅋ 가진자였잖아요 이분ㅋㅋ",
          "original_form": "아니ㅋ 가진자였잖아요 이분ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027252.1.26",
          "form": "어깨도 아프고",
          "original_form": "어깨도 아프고",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027252.1.27",
          "form": "골프도",
          "original_form": "골프도",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027252.1.28",
          "form": "풀스윙하고있엇는데 축구하다가 어깨 삐끗해서 못하고있슫니다",
          "original_form": "풀스윙하고있엇는데 축구하다가 어깨 삐끗해서 못하고있슫니다",
          "speaker_id": "2",
          "time": "20210902 10:54"
        }
      ]
    }
