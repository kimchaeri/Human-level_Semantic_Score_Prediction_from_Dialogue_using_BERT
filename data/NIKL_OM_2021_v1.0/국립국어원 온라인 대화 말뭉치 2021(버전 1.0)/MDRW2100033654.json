      "utterance": [
        {
          "id": "MDRW2100033654.1.1",
          "form": "거기 날씨 어던가요",
          "original_form": "거기 날씨 어던가요",
          "speaker_id": "1",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100033654.1.2",
          "form": "바람이 엄청 많이 불고 쌀쌀해요",
          "original_form": "바람이 엄청 많이 불고 쌀쌀해요",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100033654.1.3",
          "form": "name1 님 계신 곳은요?",
          "original_form": "&name1& 님 계신 곳은요?",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100033654.1.4",
          "form": "여기는 습기가 좀 가득해요!!",
          "original_form": "여기는 습기가 좀 가득해요!!",
          "speaker_id": "1",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100033654.1.5",
          "form": "아 여기도요",
          "original_form": "아 여기도요",
          "speaker_id": "2",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100033654.1.6",
          "form": "침대가 축축한 느낌 별로예요",
          "original_form": "침대가 축축한 느낌 별로예요",
          "speaker_id": "2",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100033654.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100033654.1.8",
          "form": "그래서 보일러를",
          "original_form": "그래서 보일러를",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100033654.1.9",
          "form": "좀 틀어야해요",
          "original_form": "좀 틀어야해요",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100033654.1.10",
          "form": "그쵸 그래도 목욕하고 나니까 좀 낫네요",
          "original_form": "그쵸 그래도 목욕하고 나니까 좀 낫네요",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100033654.1.11",
          "form": "이제 완전 가을 느낌이에요",
          "original_form": "이제 완전 가을 느낌이에요",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100033654.1.12",
          "form": "맞아요!!!!!",
          "original_form": "맞아요!!!!!",
          "speaker_id": "1",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100033654.1.13",
          "form": "name1님은 가을 좋아하세요?",
          "original_form": "&name1&님은 가을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100033654.1.14",
          "form": "네 ㅠㅠㅠㅠ 가을 좋아하죠",
          "original_form": "네 ㅠㅠㅠㅠ 가을 좋아하죠",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100033654.1.15",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100033654.1.16",
          "form": "저도 좋아요! 가을 분위기랑 단풍 너무 좋아요 낙엽 밟는 것도 좋고요",
          "original_form": "저도 좋아요! 가을 분위기랑 단풍 너무 좋아요 낙엽 밟는 것도 좋고요",
          "speaker_id": "2",
          "time": "20210824 22:09"
        },
        {
          "id": "MDRW2100033654.1.17",
          "form": "맞아요 은행 빼고는 ㅎ",
          "original_form": "맞아요 은행 빼고는 ㅎ",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100033654.1.18",
          "form": "앗 그쵸 은행은 싫어요",
          "original_form": "앗 그쵸 은행은 싫어요",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100033654.1.19",
          "form": "길거리에 은행열매는 없었으면 좋겠네요",
          "original_form": "길거리에 은행열매는 없었으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100033654.1.20",
          "form": "네 냄새가 어마무시해요ㅋㅋㅋㅋㅋㅋ",
