      "utterance": [
        {
          "id": "MDRW2100014902.1.1",
          "form": "제주도 가고 싶어요",
          "original_form": "제주도 가고 싶어요",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014902.1.2",
          "form": "잘 갔다 와",
          "original_form": "잘 갔다 와",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014902.1.3",
          "form": "아 대화를 할수있게 보내봐요",
          "original_form": "아 대화를 할수있게 보내봐요",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014902.1.4",
          "form": "갔다 와",
          "original_form": "갔다 와",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014902.1.5",
          "form": "배 타고 갈 거야 비행기 타고 갈 거야",
          "original_form": "배 타고 갈 거야 비행기 타고 갈 거야",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014902.1.6",
          "form": "비행기 타야죠 배타면 한참이라던데요",
          "original_form": "비행기 타야죠 배타면 한참이라던데요",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014902.1.7",
          "form": "누구랑 갈 거야",
          "original_form": "누구랑 갈 거야",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014902.1.8",
          "form": "엄마랑요",
          "original_form": "엄마랑요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014902.1.9",
          "form": "아저씨도 갈거자나요",
          "original_form": "아저씨도 갈거자나요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014902.1.10",
          "form": "엄마가 같이 간데",
          "original_form": "엄마가 같이 간데",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014902.1.11",
          "form": "엄마가 젤 가고시퍼 할 걸요",
          "original_form": "엄마가 젤 가고시퍼 할 걸요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014902.1.12",
          "form": "엄마는 똥싸야 해",
          "original_form": "엄마는 똥싸야 해",
          "speaker_id": "2",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014902.1.13",
          "form": "헐 진짜 짜증나ㅠㅜ",
          "original_form": "헐 진짜 짜증나ㅠㅜ",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014902.1.14",
          "form": "제주도가서도 전에 쌌어요?",
          "original_form": "제주도가서도 전에 쌌어요?",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014902.1.15",
          "form": "많이 쌌어",
          "original_form": "많이 쌌어",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014902.1.16",
          "form": "이런 넘 냄새나는 대화네요",
          "original_form": "이런 넘 냄새나는 대화네요",
          "speaker_id": "1",
          "time": "20210825 21:14"
        }
