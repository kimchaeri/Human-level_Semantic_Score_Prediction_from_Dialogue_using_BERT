      "utterance": [
        {
          "id": "MDRW2100029089.1.1",
          "form": "네 뭐 ㅌ첨엔 다들 한소리 햇죠 ㅎ",
          "original_form": "네 뭐 ㅌ첨엔 다들 한소리 햇죠 ㅎ",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029089.1.2",
          "form": "엄마도  그냥 쉬라고",
          "original_form": "엄마도  그냥 쉬라고",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029089.1.3",
          "form": "글져글져ㅎ",
          "original_form": "글져글져ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029089.1.4",
          "form": "ㅋㅋ 근데 뭐 하나 사드리니 요즘엔  응원해주심여ㅠㅋ",
          "original_form": "ㅋㅋ 근데 뭐 하나 사드리니 요즘엔  응원해주심여ㅠㅋ",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029089.1.5",
          "form": "오늘 어제 공유드린거 아내한테 전송했는데",
          "original_form": "오늘 어제 공유드린거 아내한테 전송했는데",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029089.1.6",
          "form": "했더라규요ㅎㅎ",
          "original_form": "했더라규요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029089.1.7",
          "form": "절때 뭐 안한다 했었는데ㅎ 이제 저 보고 하나봐요 ㅡㅎ",
          "original_form": "절때 뭐 안한다 했었는데ㅎ 이제 저 보고 하나봐요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100029089.1.8",
          "form": "어떤거여 ㅎㅎ",
          "original_form": "어떤거여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029089.1.9",
          "form": "신세계요ㅎㅎㅎ",
          "original_form": "신세계요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029089.1.10",
          "form": "아항 ㅋ",
          "original_form": "아항 ㅋ",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100029089.1.11",
          "form": "2600원에 샀다고 자랑하더라구요",
          "original_form": "2600원에 샀다고 자랑하더라구요",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029089.1.12",
          "form": "ㅎㅎㅎ 다행이네요",
          "original_form": "ㅎㅎㅎ 다행이네요",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029089.1.13",
          "form": "ㅎㅎ 에고에고 ㅡㅎ",
          "original_form": "ㅎㅎ 에고에고 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029089.1.14",
          "form": "언제까지 이래야하눈지ㅎㅎ",
          "original_form": "언제까지 이래야하눈지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029089.1.15",
          "form": "음 지나다보면 다시 좋아질수더 잇자나요",
          "original_form": "음 지나다보면 다시 좋아질수더 잇자나요",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029089.1.16",
          "form": "이혼한다고 출석일은다가오늗데",
          "original_form": "이혼한다고 출석일은다가오늗데",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029089.1.17",
          "form": "이번달이라ㅎ걱정이많네요ㅎ",
          "original_form": "이번달이라ㅎ걱정이많네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029089.1.18",
          "form": "음 마음이 바뀔수도 잇어여",
          "original_form": "음 마음이 바뀔수도 잇어여",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029089.1.19",
          "form": "뭐 아내가 바뀌어도 제가 바뀔지가",
          "original_form": "뭐 아내가 바뀌어도 제가 바뀔지가",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029089.1.20",
          "form": "영상보는거 보셨어여?",
          "original_form": "영상보는거 보셨어여?",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029089.1.21",
          "form": "네네ㅎ 그건 예전에 봤죠 히",
          "original_form": "네네ㅎ 그건 예전에 봤죠 히",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029089.1.22",
          "form": "그때 상담사랑 엄청난 대화를했네요",
          "original_form": "그때 상담사랑 엄청난 대화를했네요",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029089.1.23",
          "form": "보통 그거보면  취소하는경우가 많닿더라구요",
          "original_form": "보통 그거보면  취소하는경우가 많닿더라구요",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029089.1.24",
          "form": "펑펑 운다든데",
          "original_form": "펑펑 운다든데",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100029089.1.25",
          "form": "그거보고 별로 반응없었어요ㅎㄹ",
