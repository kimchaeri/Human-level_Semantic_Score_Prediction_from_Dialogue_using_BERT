        {
          "id": "MDRW2100002762.1.1",
          "form": "애들 줄넘기 대신 훌라우프 하는 중",
          "original_form": "애들 줄넘기 대신 훌라우프 하는 중",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002762.1.2",
          "form": "ㅎㅎ 잘 못하잖아",
          "original_form": "ㅎㅎ 잘 못하잖아",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002762.1.3",
          "form": "그래도 저번보다는 나아",
          "original_form": "그래도 저번보다는 나아",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002762.1.4",
          "form": "몇 개나 하는데?",
          "original_form": "몇 개나 하는데?",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002762.1.5",
          "form": "한번에 10번은 넘는듯 ㅋㅋ",
          "original_form": "한번에 10번은 넘는듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002762.1.6",
          "form": "오 많이 늘었네 ㅎㅎ",
          "original_form": "오 많이 늘었네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002762.1.7",
          "form": "이제 신랑이 우리집에서 훌라우프 제일 못하네",
          "original_form": "이제 신랑이 우리집에서 훌라우프 제일 못하네",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002762.1.8",
          "form": "ㅋㅋㅋ 아무리 해도 안돼",
          "original_form": "ㅋㅋㅋ 아무리 해도 안돼",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002762.1.9",
          "form": "ㅎㅎ 뭐 중요한 건 아니니까",
          "original_form": "ㅎㅎ 뭐 중요한 건 아니니까",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002762.1.10",
          "form": "ㅎㅎ 다른 운동 열심히 하면 되지",
          "original_form": "ㅎㅎ 다른 운동 열심히 하면 되지",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002762.1.11",
          "form": "맞아 굳이 안되는 거 할 필요는 없지",
          "original_form": "맞아 굳이 안되는 거 할 필요는 없지",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002762.1.12",
          "form": "그래도 애들은 계속 도전하네 잘 못해도",
          "original_form": "그래도 애들은 계속 도전하네 잘 못해도",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002762.1.13",
          "form": "ㅇㅇ 애들한테도 배울 점이 많아 ㅎㅎ",
          "original_form": "ㅇㅇ 애들한테도 배울 점이 많아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
